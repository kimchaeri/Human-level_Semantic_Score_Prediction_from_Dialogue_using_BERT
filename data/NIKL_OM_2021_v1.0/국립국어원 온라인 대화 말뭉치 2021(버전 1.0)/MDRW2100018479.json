      "utterance": [
        {
          "id": "MDRW2100018479.1.1",
          "form": "그러면 서구청이랑",
          "original_form": "그러면 서구청이랑",
          "speaker_id": "1",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018479.1.2",
          "form": "남구 대명동에",
          "original_form": "남구 대명동에",
          "speaker_id": "1",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018479.1.3",
          "form": "총 두개가 새로 생기는거네요",
          "original_form": "총 두개가 새로 생기는거네요",
          "speaker_id": "1",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.4",
          "form": "사서직 티오를 좀... 행복회로 돌려봐야겠어요!^^",
          "original_form": "사서직 티오를 좀... 행복회로 돌려봐야겠어요!^^",
          "speaker_id": "1",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.6",
          "form": "오래걸릴거같은디",
          "original_form": "오래걸릴거같은디",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.7",
          "form": "근데 제가 일한 곳은",
          "original_form": "근데 제가 일한 곳은",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.8",
          "form": "임기제공무원?",
          "original_form": "임기제공무원?",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.9",
          "form": "그걸로 붙으신 분이라서",
          "original_form": "그걸로 붙으신 분이라서",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018479.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아ㅠㅜ 안돼...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아ㅠㅜ 안돼...",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.11",
          "form": "다시 시험보던가 해야한다고 그러더라구여",
          "original_form": "다시 시험보던가 해야한다고 그러더라구여",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.12",
          "form": "임기제공무원이요? 그런 것도 있나요?",
          "original_form": "임기제공무원이요? 그런 것도 있나요?",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.13",
          "form": "얼핏 얘기는 들어본거같은데",
          "original_form": "얼핏 얘기는 들어본거같은데",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.14",
          "form": "저도 처음 들었는데",
          "original_form": "저도 처음 들었는데",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.15",
          "form": "사서직에도 그런게 있나보네요",
          "original_form": "사서직에도 그런게 있나보네요",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.16",
          "form": "요건이 뭐냐 물었더니",
          "original_form": "요건이 뭐냐 물었더니",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.17",
          "form": "면접만 보고 붙었다 하시더라구여",
          "original_form": "면접만 보고 붙었다 하시더라구여",
          "speaker_id": "2",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.18",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018479.1.19",
          "form": "약간 기간제 공무원이라",
          "original_form": "약간 기간제 공무원이라",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.20",
          "form": "근데 공시안치고 그런거면",
          "original_form": "근데 공시안치고 그런거면",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.21",
          "form": "보면될거에요",
          "original_form": "보면될거에요",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.22",
          "form": "정규직은 아닌가보네요",
          "original_form": "정규직은 아닌가보네요",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.23",
          "form": "아하아하 기간제!!",
          "original_form": "아하아하 기간제!!",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.24",
          "form": "그렇군뇨ㅠㅠㅠ",
          "original_form": "그렇군뇨ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.25",
          "form": "대신에 기간끝나고",
          "original_form": "대신에 기간끝나고",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.26",
          "form": "근데 사복기관도 그렇겠지만",
          "original_form": "근데 사복기관도 그렇겠지만",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.27",
          "form": "도서관도 진짜 공무원 대신",
          "original_form": "도서관도 진짜 공무원 대신",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.28",
          "form": "외부 인력? 그런사람들 계약직으로 잘 넣는거같아요",
          "original_form": "외부 인력? 그런사람들 계약직으로 잘 넣는거같아요",
          "speaker_id": "1",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.29",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.30",
          "form": "저는 사서직은 전부",
          "original_form": "저는 사서직은 전부",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018479.1.31",
          "form": "대구 중앙 도서관은 외부 기관에서 고용한 분들",
          "original_form": "대구 중앙 도서관은 외부 기관에서 고용한 분들",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.32",
          "form": "공시패스한 공무원인줄알았는데",
          "original_form": "공시패스한 공무원인줄알았는데",
          "speaker_id": "2",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.33",
          "form": "1년씩 계약직 형태로? 재고용해서",
          "original_form": "1년씩 계약직 형태로? 재고용해서",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.34",
          "form": "계속 쓰고 그러더라구요 약았어요 공무원이나 뽑지",
          "original_form": "계속 쓰고 그러더라구요 약았어요 공무원이나 뽑지",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.35",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.36",
          "form": "그렇다고 하더라구여",
          "original_form": "그렇다고 하더라구여",
          "speaker_id": "2",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.37",
          "form": "근데 도서관에 가보면",
          "original_form": "근데 도서관에 가보면",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.38",
          "form": "은근 사서직 말고 일반행정분들도 계세요",
          "original_form": "은근 사서직 말고 일반행정분들도 계세요",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.39",
          "form": "일반행정 분들은 여기저기 다 가시나봐요 ㅋㅋ",
          "original_form": "일반행정 분들은 여기저기 다 가시나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.40",
          "form": "ㅋㅋㅋ 그렇죠",
          "original_form": "ㅋㅋㅋ 그렇죠",
          "speaker_id": "2",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018479.1.41",
          "form": "제가 일한 도서관은",
          "original_form": "제가 일한 도서관은",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.42",
          "form": "작은데라서",
          "original_form": "작은데라서",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.43",
          "form": "관장님 한분이랑",
          "original_form": "관장님 한분이랑",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.44",
          "form": "행정직 한분 계셨거든요",
          "original_form": "행정직 한분 계셨거든요",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.45",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.46",
          "form": "근로생들이랑 공익 관리한다고",
          "original_form": "근로생들이랑 공익 관리한다고",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.47",
          "form": "딱 두명이 다였어요??",
          "original_form": "딱 두명이 다였어요??",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.48",
          "form": "이야기 많이했었는데",
          "original_form": "이야기 많이했었는데",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.49",
          "form": "관장이 정말 말만 관장이네요 ㅋㅋㅋ",
          "original_form": "관장이 정말 말만 관장이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.50",
          "form": "거기에 사서분 두명",
          "original_form": "거기에 사서분 두명",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.51",
          "form": "아하아하~~",
          "original_form": "아하아하~~",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.52",
          "form": "동네 작은 도서관이었습니다 ㅋㅋ",
          "original_form": "동네 작은 도서관이었습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018479.1.53",
          "form": "대신에 거의매일 문화강좌있고",
          "original_form": "대신에 거의매일 문화강좌있고",
          "speaker_id": "2",
          "time": "20210827 02:44"
        },
        {
          "id": "MDRW2100018479.1.54",
          "form": "근로생들이 은근 힘쓸일이 많았어요",
          "original_form": "근로생들이 은근 힘쓸일이 많았어요",
          "speaker_id": "2",
          "time": "20210827 02:44"
        },
        {
          "id": "MDRW2100018479.1.55",
          "form": "맞아요 직원분들이 적으면",
          "original_form": "맞아요 직원분들이 적으면",
          "speaker_id": "1",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018479.1.56",
          "form": "근로생이 할 일이 많아지더라구요",
          "original_form": "근로생이 할 일이 많아지더라구요",
          "speaker_id": "1",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018479.1.57",
          "form": "서가정리부터 시작해서",
          "original_form": "서가정리부터 시작해서",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018479.1.58",
          "form": "장서점검이라도 하면 정말 끔찍했어요 ㅋㅋㅋㅋ",
          "original_form": "장서점검이라도 하면 정말 끔찍했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018479.1.59",
          "form": "아무래도 그런 힘쓰는 일은 근로가 다 하니까...ㅠㅠㅠㅠ",
          "original_form": "아무래도 그런 힘쓰는 일은 근로가 다 하니까...ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018479.1.60",
          "form": "3월부터 6월까지는",
          "original_form": "3월부터 6월까지는",
          "speaker_id": "2",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018479.1.61",
          "form": "근로생 저 혼자였어서 ㅋㅋ",
          "original_form": "근로생 저 혼자였어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018479.1.62",
          "form": "하필 제가올때",
          "original_form": "하필 제가올때",
          "speaker_id": "2",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018479.1.63",
          "form": "보존서고 정리한다고",
          "original_form": "보존서고 정리한다고",
          "speaker_id": "2",
          "time": "20210827 02:47"
        },
