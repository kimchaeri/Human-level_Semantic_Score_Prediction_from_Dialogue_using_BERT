        {
          "id": "MDRW2100019309.1.1",
          "form": "최근에 뭐 구매했나요?",
          "original_form": "최근에 뭐 구매했나요?",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019309.1.2",
          "form": "자켓 구매했어요",
          "original_form": "자켓 구매했어요",
          "speaker_id": "2",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019309.1.3",
          "form": "오오 저도 가음 자켓 고민중인데",
          "original_form": "오오 저도 가음 자켓 고민중인데",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019309.1.4",
          "form": "아하 ㅎㅎㅎ 그러시구나",
          "original_form": "아하 ㅎㅎㅎ 그러시구나",
          "speaker_id": "2",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.5",
          "form": "어디걸로 구매햇나요?",
          "original_form": "어디걸로 구매햇나요?",
          "speaker_id": "1",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.6",
          "form": "저는 타미힐피거에서요",
          "original_form": "저는 타미힐피거에서요",
          "speaker_id": "2",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.7",
          "form": "오 저 타미힐피거 좋아해요",
          "original_form": "오 저 타미힐피거 좋아해요",
          "speaker_id": "2",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.8",
          "form": "오오 저도요 좋아해요",
          "original_form": "오오 저도요 좋아해요",
          "speaker_id": "1",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.10",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019309.1.11",
          "form": "보니까 타미진이었네요",
          "original_form": "보니까 타미진이었네요",
          "speaker_id": "2",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019309.1.12",
          "form": "ㅋㅋㅋㅋㅋ넴ㅋㅋㅋㅋ 무슨색인가요?",
          "original_form": "ㅋㅋㅋㅋㅋ넴ㅋㅋㅋㅋ 무슨색인가요?",
          "speaker_id": "1",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019309.1.13",
          "form": "베이지로 구매햇어요",
          "original_form": "베이지로 구매햇어요",
          "speaker_id": "2",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019309.1.14",
          "form": "저두 베이지자켓 구매고민중인데 ㅜㅜ 가격때문에 고민이....",
          "original_form": "저두 베이지자켓 구매고민중인데 ㅜㅜ 가격때문에 고민이....",
          "speaker_id": "1",
          "time": "20210827 17:18"
        },
        {
          "id": "MDRW2100019309.1.15",
          "form": "얼마고민중이신가요?",
          "original_form": "얼마고민중이신가요?",
          "speaker_id": "2",
          "time": "20210827 17:18"
        },
        {
