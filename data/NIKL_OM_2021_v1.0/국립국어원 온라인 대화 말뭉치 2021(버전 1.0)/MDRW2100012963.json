      "utterance": [
        {
          "id": "MDRW2100012963.1.1",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012963.1.2",
          "form": "아니요ㅜ 바빠서 못먹었어요",
          "original_form": "아니요ㅜ 바빠서 못먹었어요",
          "speaker_id": "2",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012963.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012963.1.4",
          "form": "아니요 저도 아직 못 먹었어요",
          "original_form": "아니요 저도 아직 못 먹었어요",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012963.1.5",
          "form": "음... 점심메뉴 생각하신 거 있으신가요",
          "original_form": "음... 점심메뉴 생각하신 거 있으신가요",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012963.1.6",
          "form": "전 빵 먹으려구요",
          "original_form": "전 빵 먹으려구요",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012963.1.7",
          "form": "빵 어떤 빵 드시나요",
          "original_form": "빵 어떤 빵 드시나요",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012963.1.8",
          "form": "크로크무슈 였나 파바에서 맛있어보이는빵 집어왔어요",
          "original_form": "크로크무슈 였나 파바에서 맛있어보이는빵 집어왔어요",
          "speaker_id": "2",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012963.1.9",
          "form": "파리바게트 빵 완전 맛있어요",
          "original_form": "파리바게트 빵 완전 맛있어요",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012963.1.10",
          "form": "크로크무슈도 먹어본 적은 없지만 맛있을 것 같아요",
          "original_form": "크로크무슈도 먹어본 적은 없지만 맛있을 것 같아요",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012963.1.11",
          "form": "맞아요 아침에 갔더니 갓나온 빵이 다 맛있어보이더라구요",
          "original_form": "맞아요 아침에 갔더니 갓나온 빵이 다 맛있어보이더라구요",
          "speaker_id": "2",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012963.1.12",
          "form": "저도 첨 먹어봐요",
          "original_form": "저도 첨 먹어봐요",
          "speaker_id": "2",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012963.1.13",
          "form": "갓나온 빵만큼 맛있는 건 없는 거 같아요",
          "original_form": "갓나온 빵만큼 맛있는 건 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012963.1.14",
          "form": "정말 잘 골라 오셨네요",
          "original_form": "정말 잘 골라 오셨네요",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012963.1.15",
          "form": "같이 마실 음료도 고르셨나요",
          "original_form": "같이 마실 음료도 고르셨나요",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012963.1.16",
          "form": "아뇨 그냥 바빠서 못골랐네요",
          "original_form": "아뇨 그냥 바빠서 못골랐네요",
          "speaker_id": "2",
          "time": "20210825 09:36"
        },
        {
          "id": "MDRW2100012963.1.17",
          "form": "name1님은 점심 뭐 드세요?",
          "original_form": "&name1&님은 점심 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210825 09:36"
        },
        {
          "id": "MDRW2100012963.1.18",
          "form": "아앗 그러셨군요",
          "original_form": "아앗 그러셨군요",
          "speaker_id": "1",
          "time": "20210825 09:36"
        },
        {
          "id": "MDRW2100012963.1.19",
          "form": "저는 점심에 짜장면 먹을까 생각 중이에요",
          "original_form": "저는 점심에 짜장면 먹을까 생각 중이에요",
          "speaker_id": "1",
          "time": "20210825 09:36"
        },
        {
          "id": "MDRW2100012963.1.20",
          "form": "오 짜장면 좋죠",
          "original_form": "오 짜장면 좋죠",
          "speaker_id": "2",
          "time": "20210825 09:36"
        },
        {
          "id": "MDRW2100012963.1.21",
          "form": "먹을때 짬뽕보다 짜장면을 더 좋아하시나요",
          "original_form": "먹을때 짬뽕보다 짜장면을 더 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 09:37"
        },
        {
          "id": "MDRW2100012963.1.22",
          "form": "저는 짜장면이 더 좋아요",
          "original_form": "저는 짜장면이 더 좋아요",
          "speaker_id": "1",
          "time": "20210825 09:37"
        }
      ]
