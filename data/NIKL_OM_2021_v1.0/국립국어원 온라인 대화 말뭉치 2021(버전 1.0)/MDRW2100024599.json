      "utterance": [
        {
          "id": "MDRW2100024599.1.1",
          "form": "그 꼬맹이?",
          "original_form": "그 꼬맹이?",
          "speaker_id": "2",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100024599.1.2",
          "form": "ㅇㅇ함 집에 다시 갔더니",
          "original_form": "ㅇㅇ함 집에 다시 갔더니",
          "speaker_id": "2",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100024599.1.3",
          "form": "수정골수 또 달래서",
          "original_form": "수정골수 또 달래서",
          "speaker_id": "2",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100024599.1.4",
          "form": "또 줌",
          "original_form": "또 줌",
          "speaker_id": "2",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100024599.1.5",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.6",
          "form": "내가 전에 말한",
          "original_form": "내가 전에 말한",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.7",
          "form": "뱀뼈머리 옆에",
          "original_form": "뱀뼈머리 옆에",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.8",
          "form": "아ㅇㅇ",
          "original_form": "아ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.9",
          "form": "감실이랑 npc있는갓",
          "original_form": "감실이랑 npc있는갓",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.10",
          "form": "세번기도하면",
          "original_form": "세번기도하면",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.11",
          "form": "보상 주는거",
          "original_form": "보상 주는거",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100024599.1.12",
          "form": "또 기도 가능하길래",
          "original_form": "또 기도 가능하길래",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024599.1.13",
          "form": "또 해봄",
          "original_form": "또 해봄",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024599.1.14",
          "form": "울림풀 달라는애도",
          "original_form": "울림풀 달라는애도",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024599.1.15",
          "form": "또 줄수잇길래",
          "original_form": "또 줄수잇길래",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024599.1.16",
          "form": "또 줘봐서",
          "original_form": "또 줘봐서",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024599.1.17",
          "form": "울림풀 채집지 부족해.....",
          "original_form": "울림풀 채집지 부족해.....",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100024599.1.18",
          "form": "ㅇㅎ 그거 기도 세번 하면",
          "original_form": "ㅇㅎ 그거 기도 세번 하면",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024599.1.19",
          "form": "다음날 상자주니까",
          "original_form": "다음날 상자주니까",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024599.1.20",
          "form": "4일동안 열심히 해...",
          "original_form": "4일동안 열심히 해...",
          "speaker_id": "1",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024599.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그랰",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그랰",
          "speaker_id": "2",
          "time": "20210831 16:13"
        },
        {
          "id": "MDRW2100024599.1.22",
          "form": "라이덴 플레이 홍보영상 올라왔는데",
          "original_form": "라이덴 플레이 홍보영상 올라왔는데",
          "speaker_id": "1",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024599.1.23",
          "form": "누가 번개의 신 아니랄까봐",
          "original_form": "누가 번개의 신 아니랄까봐",
          "speaker_id": "1",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024599.1.24",
          "form": "번쩍번쩍 난리도 아니네",
          "original_form": "번쩍번쩍 난리도 아니네",
          "speaker_id": "1",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024599.1.25",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024599.1.26",
          "form": "ㅎㅏ나도 5성....",
          "original_form": "ㅎㅏ나도 5성....",
          "speaker_id": "2",
          "time": "20210831 16:15"
        },
        {
          "id": "MDRW2100024599.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100024599.1.28",
          "form": "그치만 이번 뽑기 안돌릴거잖아",
          "original_form": "그치만 이번 뽑기 안돌릴거잖아",
          "speaker_id": "1",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100024599.1.29",
          "form": "그치만",
          "original_form": "그치만",
          "speaker_id": "2",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100024599.1.30",
          "form": "안돌려됴 가지고는 싶어....",
          "original_form": "안돌려됴 가지고는 싶어....",
          "speaker_id": "2",
          "time": "20210831 16:19"
        },
        {
          "id": "MDRW2100024599.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100024599.1.32",
          "form": "지금 몇연 돌렸어?",
          "original_form": "지금 몇연 돌렸어?",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100024599.1.33",
          "form": "한 10연이나 20연만 돌려보ㅓ",
          "original_form": "한 10연이나 20연만 돌려보ㅓ",
          "speaker_id": "1",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100024599.1.34",
          "form": "아직 70연",
          "original_form": "아직 70연",
          "speaker_id": "2",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100024599.1.35",
          "form": "안대",
          "original_form": "안대",
          "speaker_id": "2",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100024599.1.36",
          "form": "나오면",
          "original_form": "나오면",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.37",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.38",
          "form": "천장이 멀지 않았구나",
          "original_form": "천장이 멀지 않았구나",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.39",
          "form": "혹시 2.2에서 소 복각오면",
          "original_form": "혹시 2.2에서 소 복각오면",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.40",
          "form": "2천장을",
          "original_form": "2천장을",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.41",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.42",
          "form": "못찍잔아.....",
          "original_form": "못찍잔아.....",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.43",
          "form": "그럴 일은 없어보이긴 하는데...",
          "original_form": "그럴 일은 없어보이긴 하는데...",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.44",
          "form": "행복회로 태워야지",
          "original_form": "행복회로 태워야지",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.45",
          "form": "너무 불타서 계정 던지는거 아니냐구....",
          "original_form": "너무 불타서 계정 던지는거 아니냐구....",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100024599.1.46",
          "form": "이번에 안오면",
          "original_form": "이번에 안오면",
