      "utterance": [
        {
          "id": "MDRW2100034108.1.1",
          "form": "디즈니 ost모음 요새 많이 듣는데 너무 좋은거 같아오",
          "original_form": "디즈니 ost모음 요새 많이 듣는데 너무 좋은거 같아오",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034108.1.2",
          "form": "아 생각도 못했어요",
          "original_form": "아 생각도 못했어요",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034108.1.3",
          "form": "진짜 좋은곡 많은데",
          "original_form": "진짜 좋은곡 많은데",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034108.1.4",
          "form": "하나씩만 들었네요 ㅜㅜ",
          "original_form": "하나씩만 들었네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034108.1.5",
          "form": "정말 좋은거 많이 아시네요 ㅎㅎ",
          "original_form": "정말 좋은거 많이 아시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034108.1.6",
          "form": "ㅋㅋㅋㅋㅋ제가 뭐 많이 알았었나요!",
          "original_form": "ㅋㅋㅋㅋㅋ제가 뭐 많이 알았었나요!",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100034108.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ일할때 노동요로 딱이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ일할때 노동요로 딱이에요",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100034108.1.8",
          "form": "노동요 맞에요 ㅋㅋ",
          "original_form": "노동요 맞에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100034108.1.9",
          "form": "그냥 일하면 흥도 안나고 심심하고 저도 음악 틀어요",
          "original_form": "그냥 일하면 흥도 안나고 심심하고 저도 음악 틀어요",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100034108.1.10",
          "form": "ㅋㅋ길고긴",
          "original_form": "ㅋㅋ길고긴",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100034108.1.11",
          "form": "막장의 끝이 다가 오고 있네요",
          "original_form": "막장의 끝이 다가 오고 있네요",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100034108.1.12",
          "form": "펜트하우스 얼마 안남았어요",
          "original_form": "펜트하우스 얼마 안남았어요",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034108.1.13",
          "form": "오늘 금요일 보려고요",
          "original_form": "오늘 금요일 보려고요",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034108.1.14",
          "form": "저번주 충격이라 하차 하려했지만 ㅜㅜ",
          "original_form": "저번주 충격이라 하차 하려했지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034108.1.15",
          "form": "그냥 보려고요 얼마 안남아서",
          "original_form": "그냥 보려고요 얼마 안남아서",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034108.1.16",
          "form": "ㅋㅋ저 금방 다른분 음악 이야기랑 겹쳐서 ㅋㅋ헷살렸어요",
          "original_form": "ㅋㅋ저 금방 다른분 음악 이야기랑 겹쳐서 ㅋㅋ헷살렸어요",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034108.1.17",
          "form": "죄송해요 ㅜㅜ",
          "original_form": "죄송해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034108.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ아잌ㅋㅋㅋㅋㅋ무한 수다의 방에 아때요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아잌ㅋㅋㅋㅋㅋ무한 수다의 방에 아때요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:09"
        },
        {
          "id": "MDRW2100034108.1.19",
          "form": "급 펜하ㅋㅋㅋㅋ얼마 안남은거 안보면 뭔가 괜히 찝찝해요 보는게 낫죠",
          "original_form": "급 펜하ㅋㅋㅋㅋ얼마 안남은거 안보면 뭔가 괜히 찝찝해요 보는게 낫죠",
          "speaker_id": "2",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100034108.1.20",
          "form": "네 근데 어제 ㅋㅋ그큰 헤라 펠리스 폭파 ㅜㅜ충격의 도가니",
          "original_form": "네 근데 어제 ㅋㅋ그큰 헤라 펠리스 폭파 ㅜㅜ충격의 도가니",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100034108.1.21",
          "form": "허무했네요 ㅋㅋ",
          "original_form": "허무했네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100034108.1.22",
          "form": "에?ㅋㅋㅋㅋ안봤는뎈ㅋㅋ그런 웃긴일이ㅋㅋㅋㅋㅋ",
          "original_form": "에?ㅋㅋㅋㅋ안봤는뎈ㅋㅋ그런 웃긴일이ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034108.1.23",
          "form": "네 뭔 우리나라에 개인이 폭탄을",
          "original_form": "네 뭔 우리나라에 개인이 폭탄을",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034108.1.24",
          "form": "고층건물 터지게 살수 있나요 ㅜㅜ",
          "original_form": "고층건물 터지게 살수 있나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034108.1.25",
          "form": "참 너무 갔어요",
          "original_form": "참 너무 갔어요",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034108.1.26",
          "form": "거기는 총에 뭐ㅋㅋㅋ우리나라에서 안되는 것들 잔뜩 갔다놨잖아욬ㅋㅋㅋ",
          "original_form": "거기는 총에 뭐ㅋㅋㅋ우리나라에서 안되는 것들 잔뜩 갔다놨잖아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034108.1.27",
          "form": "네 ㅋㅋ아 정말 어제 대단했어요",
          "original_form": "네 ㅋㅋ아 정말 어제 대단했어요",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100034108.1.28",
          "form": "주단태 조커처럼 입도 옆으로 칼자국 내고",
          "original_form": "주단태 조커처럼 입도 옆으로 칼자국 내고",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100034108.1.29",
          "form": "스스로요? 아니면 다른 사람이?",
          "original_form": "스스로요? 아니면 다른 사람이?",
          "speaker_id": "2",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100034108.1.30",
          "form": "그걸 안보여 줬어요",
          "original_form": "그걸 안보여 줬어요",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034108.1.31",
          "form": "뭐지 했네요",
          "original_form": "뭐지 했네요",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034108.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ이상한거 숨기네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이상한거 숨기네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034108.1.33",
          "form": "천서진도 살아돌아왔다면서요?",
          "original_form": "천서진도 살아돌아왔다면서요?",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034108.1.34",
          "form": "몇명을 살려내는거야 도대체...",
          "original_form": "몇명을 살려내는거야 도대체...",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034108.1.35",
          "form": "네 멀쩡히요 ㅡㅡ",
          "original_form": "네 멀쩡히요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100034108.1.36",
          "form": "죽지도 않아요",
          "original_form": "죽지도 않아요",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.37",
          "form": "엄청 잔인하게",
          "original_form": "엄청 잔인하게",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.38",
          "form": "샹들리에 떨어졌다구",
          "original_form": "샹들리에 떨어졌다구",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.39",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.40",
          "form": "절대 불가능이라 했는데ㅜㅜㅋㅋㅋㅋㅋ",
          "original_form": "절대 불가능이라 했는데ㅜㅜㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.41",
          "form": "진짜 좀 심했죠 ㅋㅋ",
          "original_form": "진짜 좀 심했죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.42",
          "form": "저는 안봤지만ㅋㅋㅋ듣기만 해도 이해가 안돼욬ㅋ",
          "original_form": "저는 안봤지만ㅋㅋㅋ듣기만 해도 이해가 안돼욬ㅋ",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100034108.1.43",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210828 11:23"
        }
      ]
