      "utterance": [
        {
          "id": "MDRW2100017478.1.1",
          "form": "영화 자주 보세요?",
          "original_form": "영화 자주 보세요?",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017478.1.2",
          "form": "네 영화좋아해요!",
          "original_form": "네 영화좋아해요!",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017478.1.3",
          "form": "최근에 영화 마지막으로 본 게 언제에요?",
          "original_form": "최근에 영화 마지막으로 본 게 언제에요?",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017478.1.4",
          "form": "저는 블랙위도우 제일 최근에 봤어요!",
          "original_form": "저는 블랙위도우 제일 최근에 봤어요!",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017478.1.5",
          "form": "블랙위도우 어땠어요?",
          "original_form": "블랙위도우 어땠어요?",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017478.1.6",
          "form": "블랙위도우 액션도 좋고 진짜 재미있었어욤",
          "original_form": "블랙위도우 액션도 좋고 진짜 재미있었어욤",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017478.1.7",
          "form": "저도 재밌었는데 중간에 살짝 지루했어요 ㅜㅜ",
          "original_form": "저도 재밌었는데 중간에 살짝 지루했어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017478.1.8",
          "form": "크루엘라도 보셨어요?",
          "original_form": "크루엘라도 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017478.1.9",
          "form": "아하 그러셨구나! 아쉽네요 저는 진짜 재미있게 봤는데!",
          "original_form": "아하 그러셨구나! 아쉽네요 저는 진짜 재미있게 봤는데!",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017478.1.10",
          "form": "크루엘라는 안봤어요!",
          "original_form": "크루엘라는 안봤어요!",
          "speaker_id": "2",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100017478.1.11",
          "form": "아하 ㅜㅜ혹시 기다리는 영화 있어요?",
          "original_form": "아하 ㅜㅜ혹시 기다리는 영화 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017478.1.12",
          "form": "아직 개봉 안한거라도 ㅎㅎ",
          "original_form": "아직 개봉 안한거라도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017478.1.13",
          "form": "저는 스파이더맨 노웨이홈 엄청 기대중이에요!",
          "original_form": "저는 스파이더맨 노웨이홈 엄청 기대중이에요!",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017478.1.14",
          "form": "헐 그거 곧 나와요?",
          "original_form": "헐 그거 곧 나와요?",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017478.1.15",
          "form": "스파이더맨 파프럼홈도 재밌게 봤는데 ㅜㅜ",
          "original_form": "스파이더맨 파프럼홈도 재밌게 봤는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017478.1.16",
          "form": "12월에 개봉에요! 기다리기가 너무 힘드네요ㅠㅠ",
          "original_form": "12월에 개봉에요! 기다리기가 너무 힘드네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017478.1.17",
          "form": "스파이더맨을 제가 제일 좋아하거든요!",
          "original_form": "스파이더맨을 제가 제일 좋아하거든요!",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017478.1.18",
          "form": "코로나 때문에 개봉 미루고 그러진 않겠죠?ㅠㅠ",
          "original_form": "코로나 때문에 개봉 미루고 그러진 않겠죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017478.1.19",
          "form": "그러게요 제발 연기안됐으면ㅠㅠ",
          "original_form": "그러게요 제발 연기안됐으면ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017478.1.20",
          "form": "영화관은 주로 어디 선호해요?",
          "original_form": "영화관은 주로 어디 선호해요?",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017478.1.21",
          "form": "영화관은 딱히 가리지는 않아욤!",
          "original_form": "영화관은 딱히 가리지는 않아욤!",
          "speaker_id": "2",
          "time": "20210826 20:19"
        }
      ]
    }
