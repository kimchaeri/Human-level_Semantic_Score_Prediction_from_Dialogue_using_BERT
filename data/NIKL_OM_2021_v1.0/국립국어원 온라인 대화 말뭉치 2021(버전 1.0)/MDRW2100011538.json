      "utterance": [
        {
          "id": "MDRW2100011538.1.1",
          "form": "여기는 비가와요",
          "original_form": "여기는 비가와요",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011538.1.2",
          "form": "여기도 아까는 흐리기만 했는데 비오네요",
          "original_form": "여기도 아까는 흐리기만 했는데 비오네요",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011538.1.3",
          "form": "태풍 온다던데 그래서 그런가봐요",
          "original_form": "태풍 온다던데 그래서 그런가봐요",
          "speaker_id": "2",
          "time": "20210824 13:57"
        },
        {
          "id": "MDRW2100011538.1.4",
          "form": "벌써 추워지네요 감기 조심하세요",
          "original_form": "벌써 추워지네요 감기 조심하세요",
          "speaker_id": "1",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100011538.1.5",
          "form": "네!\r\n그러니까요 밤에는 창문여니까 춥더라구요",
          "original_form": "네!\r\n그러니까요 밤에는 창문여니까 춥더라구요",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011538.1.6",
          "form": "이제 좀있으면 바로 겨울오겠어요",
          "original_form": "이제 좀있으면 바로 겨울오겠어요",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011538.1.7",
          "form": "벌써요?ㅋㅋ",
          "original_form": "벌써요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011538.1.8",
          "form": "여름이 그립네요",
          "original_form": "여름이 그립네요",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011538.1.9",
          "form": "그래도 저는 여름이 더 싫어요ㅋㅋ",
          "original_form": "그래도 저는 여름이 더 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100011538.1.10",
          "form": "저는 춥고 건조한게 너무싫어서",
          "original_form": "저는 춥고 건조한게 너무싫어서",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011538.1.11",
          "form": "차라리 여름이나아요",
          "original_form": "차라리 여름이나아요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011538.1.12",
          "form": "마자요 저는 제가 여름 안좋아해서 다른 사람들도 그런줄알았는데\r\n여름 좋아하시는분들도 많더라구요",
          "original_form": "마자요 저는 제가 여름 안좋아해서 다른 사람들도 그런줄알았는데\r\n여름 좋아하시는분들도 많더라구요",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100011538.1.13",
          "form": "어떤계절 좋아하세요?",
          "original_form": "어떤계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100011538.1.14",
          "form": "저는 봄가을이요!\r\n전 더위도 잘타고 추위도 잘타서 중간이 제일좋아요ㅠ",
          "original_form": "저는 봄가을이요!\r\n전 더위도 잘타고 추위도 잘타서 중간이 제일좋아요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011538.1.15",
          "form": "근데 봄가을이 점점 짧아져서ㅠ",
          "original_form": "근데 봄가을이 점점 짧아져서ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011538.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011538.1.17",
          "form": "체감상",
          "original_form": "체감상",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011538.1.18",
          "form": "한주정도?",
          "original_form": "한주정도?",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011538.1.19",
          "form": "거의 한줌 ㅋㅋ",
          "original_form": "거의 한줌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011538.1.20",
          "form": "마자요 진짜 느낄새도 없이 지나가요ㅠ",
          "original_form": "마자요 진짜 느낄새도 없이 지나가요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100011538.1.21",
          "form": "지구 온난화 걱정 돼요",
          "original_form": "지구 온난화 걱정 돼요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        }
      ]
