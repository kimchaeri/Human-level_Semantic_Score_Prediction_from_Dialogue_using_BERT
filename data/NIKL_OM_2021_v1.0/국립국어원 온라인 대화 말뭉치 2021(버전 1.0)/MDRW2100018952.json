      "utterance": [
        {
          "id": "MDRW2100018952.1.1",
          "form": "당근하세요?",
          "original_form": "당근하세요?",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018952.1.2",
          "form": "네 저 당근 애용해요ㅎㅎㅎ",
          "original_form": "네 저 당근 애용해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018952.1.3",
          "form": "name2님도 당근 하시나요?",
          "original_form": "&name2&님도 당근 하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018952.1.4",
          "form": "사시나요 파시나요",
          "original_form": "사시나요 파시나요",
          "speaker_id": "2",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018952.1.5",
          "form": "전 주로사는쪽 ㅎ",
          "original_form": "전 주로사는쪽 ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018952.1.6",
          "form": "전 둘다 해요ㅋㅋㅋ",
          "original_form": "전 둘다 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018952.1.7",
          "form": "고양이 물품 주로 사고 생필품을 판답니다",
          "original_form": "고양이 물품 주로 사고 생필품을 판답니다",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018952.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018952.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018952.1.10",
          "form": "자기살을베서 고양이 먹이는 느낌이네요",
          "original_form": "자기살을베서 고양이 먹이는 느낌이네요",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018952.1.11",
          "form": "저는 최근에 고양이 사료사서 길고양이 영접할때마다 나눠줘요",
          "original_form": "저는 최근에 고양이 사료사서 길고양이 영접할때마다 나눠줘요",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018952.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 적절한 비유...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 적절한 비유...",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018952.1.13",
          "form": "와 좋아요 저도 첨에는 길고양이 케어하다가 입양했어요 ㅠㅠ",
          "original_form": "와 좋아요 저도 첨에는 길고양이 케어하다가 입양했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018952.1.14",
          "form": "저는 최근에 고양이 발톱깎이 샀어요",
          "original_form": "저는 최근에 고양이 발톱깎이 샀어요",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018952.1.15",
          "form": "엄머ㅜㅜ",
          "original_form": "엄머ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018952.1.16",
          "form": "길에서 집사로 간택되셨군요",
          "original_form": "길에서 집사로 간택되셨군요",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018952.1.17",
          "form": "실용적인거 사셨네요",
          "original_form": "실용적인거 사셨네요",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018952.1.18",
          "form": "네네ㅋㅋㅋ 요즘엔 화분도 보고 있어요",
          "original_form": "네네ㅋㅋㅋ 요즘엔 화분도 보고 있어요",
          "speaker_id": "1",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018952.1.19",
          "form": "name2님은 눈여겨보신 거 있으세요?",
          "original_form": "&name2&님은 눈여겨보신 거 있으세요?",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018952.1.20",
          "form": "오 화분은 고양이랑 맞는식물인지",
          "original_form": "오 화분은 고양이랑 맞는식물인지",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018952.1.21",
          "form": "찾아봐야한대요",
          "original_form": "찾아봐야한대요",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018952.1.22",
          "form": "저는 버티컬마우스 사고 지웠어요",
          "original_form": "저는 버티컬마우스 사고 지웠어요",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018952.1.23",
          "form": "맞아요 냥이들 까다롭 ㅠㅠ",
          "original_form": "맞아요 냥이들 까다롭 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018952.1.24",
          "form": "왜 지우셨나요....",
          "original_form": "왜 지우셨나요....",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018952.1.25",
          "form": "당근... 유용한데..",
          "original_form": "당근... 유용한데..",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018952.1.26",
          "form": "넘 유용해서",
          "original_form": "넘 유용해서",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018952.1.27",
          "form": "계속 보더라구요 그래서 지웟죠ㅋㅋ",
          "original_form": "계속 보더라구요 그래서 지웟죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018952.1.28",
          "form": "아.. 인정..",
          "original_form": "아.. 인정..",
          "speaker_id": "1",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018952.1.29",
          "form": "계속 찾아보게 되죠",
          "original_form": "계속 찾아보게 되죠",
          "speaker_id": "1",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018952.1.30",
          "form": "중독성있어요",
          "original_form": "중독성있어요",
          "speaker_id": "1",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018952.1.31",
          "form": "네 저는 필요한거 생기면 다시깔려구요",
          "original_form": "네 저는 필요한거 생기면 다시깔려구요",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018952.1.32",
          "form": "새로산 마우스두 잘쓰고있구요ㅎㅎ",
          "original_form": "새로산 마우스두 잘쓰고있구요ㅎㅎ",
