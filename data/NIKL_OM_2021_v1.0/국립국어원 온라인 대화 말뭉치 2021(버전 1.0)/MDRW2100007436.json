      "utterance": [
        {
          "id": "MDRW2100007436.1.1",
          "form": "00다방 이런 곳에서 맛있는데가 좀 있더라고요",
          "original_form": "00다방 이런 곳에서 맛있는데가 좀 있더라고요",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007436.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007436.1.3",
          "form": "쑥라떼 좋아하시나요",
          "original_form": "쑥라떼 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007436.1.4",
          "form": "완전좋아하죠",
          "original_form": "완전좋아하죠",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007436.1.5",
          "form": "쑥 흑임자 인절미 이런거",
          "original_form": "쑥 흑임자 인절미 이런거",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007436.1.6",
          "form": "맛이 없을 수 없는 조합",
          "original_form": "맛이 없을 수 없는 조합",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007436.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007436.1.8",
          "form": "맛잘알이시구나",
          "original_form": "맛잘알이시구나",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007436.1.9",
          "form": "떡도 좋아하세요?!",
          "original_form": "떡도 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007436.1.10",
          "form": "헐 너무좋아",
          "original_form": "헐 너무좋아",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007436.1.11",
          "form": "인절미 백설기 시루떡 바람떡",
          "original_form": "인절미 백설기 시루떡 바람떡",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007436.1.12",
          "form": "완전 미치죠",
          "original_form": "완전 미치죠",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007436.1.13",
          "form": "무슨 떡 좋아해요",
          "original_form": "무슨 떡 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007436.1.14",
          "form": "저는 잔기지떡",
          "original_form": "저는 잔기지떡",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007436.1.15",
          "form": "아시죠 비숑같이 생긴 떡",
          "original_form": "아시죠 비숑같이 생긴 떡",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007436.1.16",
          "form": "잔기지떡?",
          "original_form": "잔기지떡?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007436.1.17",
          "form": "어 처음들어보는데 찾아볼게요",
          "original_form": "어 처음들어보는데 찾아볼게요",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007436.1.18",
          "form": "비숑같이 생겼어요",
          "original_form": "비숑같이 생겼어요",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007436.1.19",
          "form": "아 기정떡이구나",
          "original_form": "아 기정떡이구나",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.20",
          "form": "하 인절미",
          "original_form": "하 인절미",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.21",
          "form": "앗 네네",
          "original_form": "앗 네네",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.22",
          "form": "이거 맛있어요 어제도 먹었어요",
          "original_form": "이거 맛있어요 어제도 먹었어요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.23",
          "form": "아니 그게 이름이 여러가지더라고요",
          "original_form": "아니 그게 이름이 여러가지더라고요",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.24",
          "form": "잔기지떡 진짜 사놓고",
          "original_form": "잔기지떡 진짜 사놓고",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.25",
          "form": "냉동해놨다가",
          "original_form": "냉동해놨다가",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.26",
          "form": "라떼랑 먹으면",
          "original_form": "라떼랑 먹으면",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.27",
          "form": "천국",
          "original_form": "천국",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.28",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007436.1.29",
