      "utterance": [
        {
          "id": "MDRW2100028066.1.1",
          "form": "일해요?",
          "original_form": "일해요?",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028066.1.2",
          "form": "예 일하는 중입니다",
          "original_form": "예 일하는 중입니다",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028066.1.3",
          "form": "물론 직장인의 미덕으로",
          "original_form": "물론 직장인의 미덕으로",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028066.1.4",
          "form": "이렇게 딴 일을 하고 있기는 하지만...",
          "original_form": "이렇게 딴 일을 하고 있기는 하지만...",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028066.1.5",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028066.1.6",
          "form": "훌륭하시군요",
          "original_form": "훌륭하시군요",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028066.1.7",
          "form": "그래도 곧 퇴근시간인지라",
          "original_form": "그래도 곧 퇴근시간인지라",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100028066.1.8",
          "form": "어쩔 수 없는거 아니겠나요",
          "original_form": "어쩔 수 없는거 아니겠나요",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100028066.1.9",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028066.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028066.1.11",
          "form": "그래도 내일이면 금요일이니",
          "original_form": "그래도 내일이면 금요일이니",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028066.1.12",
          "form": "하루만 더...",
          "original_form": "하루만 더...",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028066.1.13",
          "form": "힘들어여",
          "original_form": "힘들어여",
          "speaker_id": "1",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028066.1.14",
          "form": "유연 근무제 같은 거 하신다면",
          "original_form": "유연 근무제 같은 거 하신다면",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028066.1.15",
          "form": "내일의 나에게 맡기도",
          "original_form": "내일의 나에게 맡기도",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028066.1.16",
          "form": "맡기고",
          "original_form": "맡기고",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028066.1.17",
          "form": "반박자 빠른 퇴근을 하심이...",
          "original_form": "반박자 빠른 퇴근을 하심이...",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028066.1.18",
          "form": "안해주죠",
          "original_form": "안해주죠",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028066.1.19",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028066.1.20",
          "form": "6시 퇴근이신가요?",
          "original_form": "6시 퇴근이신가요?",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028066.1.21",
          "form": "9시요",
          "original_form": "9시요",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028066.1.22",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028066.1.23",
          "form": "회사에 식사는 주나요",
          "original_form": "회사에 식사는 주나요",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028066.1.24",
          "form": "아니 안 주면 안되겠지...",
          "original_form": "아니 안 주면 안되겠지...",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028066.1.25",
          "form": "안줘요",
          "original_form": "안줘요",
          "speaker_id": "1",
          "time": "20210902 18:09"
        }
      ]
