      "utterance": [
        {
          "id": "MDRW2100034330.1.1",
          "form": "진짜 직장이든 알바던 주위사람이 중요한거같앙ㅅ",
          "original_form": "진짜 직장이든 알바던 주위사람이 중요한거같앙ㅅ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034330.1.2",
          "form": "같아요",
          "original_form": "같아요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100034330.1.3",
          "form": "그니까요 \r\n동료랑  안맞으면 그만두고싶죠ㅠ",
          "original_form": "그니까요 \r\n동료랑  안맞으면 그만두고싶죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034330.1.4",
          "form": "인간관계가 제일 중요하더라구요",
          "original_form": "인간관계가 제일 중요하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034330.1.5",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100034330.1.6",
          "form": "특히 회사는 전 일보다 사람이 중요하다 생각해요 ㅜㅜ",
          "original_form": "특히 회사는 전 일보다 사람이 중요하다 생각해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.7",
          "form": "공감해요!",
          "original_form": "공감해요!",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.8",
          "form": "다같군요 ㅋㅋㅋ",
          "original_form": "다같군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.9",
          "form": "안맞는사람이랑은 일못해요 진짜",
          "original_form": "안맞는사람이랑은 일못해요 진짜",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.11",
          "form": "일은 익숙해져도 사람은 못 고쳐요",
          "original_form": "일은 익숙해져도 사람은 못 고쳐요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.12",
          "form": "저도 사람땜에 그만둔적있어서",
          "original_form": "저도 사람땜에 그만둔적있어서",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034330.1.13",
          "form": "못고치죠ㅠ진짜\r\n고치는거보다 제가 나가는게 빠르죠",
          "original_form": "못고치죠ㅠ진짜\r\n고치는거보다 제가 나가는게 빠르죠",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034330.1.14",
          "form": "맞아요 정말",
          "original_form": "맞아요 정말",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034330.1.15",
          "form": "전 전 회사에서 상사랑 안맞아서 관두려햇는데",
          "original_form": "전 전 회사에서 상사랑 안맞아서 관두려햇는데",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034330.1.16",
          "form": "상사가 바뀌어서 너무 편해졋어요 세상이 달라보이더라구요 ㅋㅋㅋㅋ",
          "original_form": "상사가 바뀌어서 너무 편해졋어요 세상이 달라보이더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100034330.1.17",
          "form": "상사랑  안맞으면 진짜 힘들죠",
          "original_form": "상사랑  안맞으면 진짜 힘들죠",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.18",
          "form": "그래도 다행이네요",
          "original_form": "그래도 다행이네요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.19",
          "form": "공감요 ㅜㅜ",
          "original_form": "공감요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.20",
          "form": "살맛나더라고요",
          "original_form": "살맛나더라고요",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.21",
          "form": "그니까요 상사도 잘맞는사람이면",
          "original_form": "그니까요 상사도 잘맞는사람이면",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.22",
          "form": "일도 더 편해지고",
          "original_form": "일도 더 편해지고",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.23",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100034330.1.24",
          "form": "너무 좋더라구요",
          "original_form": "너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
