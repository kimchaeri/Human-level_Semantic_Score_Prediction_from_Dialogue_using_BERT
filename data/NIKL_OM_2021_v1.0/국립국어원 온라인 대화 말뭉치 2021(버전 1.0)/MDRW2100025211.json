      "utterance": [
        {
          "id": "MDRW2100025211.1.1",
          "form": "챙겨보는 드라마나 예능 있으세요?",
          "original_form": "챙겨보는 드라마나 예능 있으세요?",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025211.1.2",
          "form": "저 드라마를 잘 챙겨보진 않아요!",
          "original_form": "저 드라마를 잘 챙겨보진 않아요!",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025211.1.3",
          "form": "그런데 그저께에",
          "original_form": "그런데 그저께에",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025211.1.4",
          "form": "왓챠에서",
          "original_form": "왓챠에서",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025211.1.5",
          "form": "왓챠 파티 침착맨 을 봤는데",
          "original_form": "왓챠 파티 침착맨 을 봤는데",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025211.1.6",
          "form": "너무 재밌었어요",
          "original_form": "너무 재밌었어요",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025211.1.7",
          "form": "아 왓챠 구독하세요?? 왓챠에서 주로 무슨 작품 보세요?",
          "original_form": "아 왓챠 구독하세요?? 왓챠에서 주로 무슨 작품 보세요?",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100025211.1.8",
          "form": "저는 영화 봐요!",
          "original_form": "저는 영화 봐요!",
          "speaker_id": "2",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025211.1.9",
          "form": "외국 영화를 좋아하는 편이라서요 ㅎㅎ",
          "original_form": "외국 영화를 좋아하는 편이라서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025211.1.10",
          "form": "왓챠에 영화 많더라구요! 저도 외국 영화 많이 봐요",
          "original_form": "왓챠에 영화 많더라구요! 저도 외국 영화 많이 봐요",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025211.1.11",
          "form": "최근에 재밌게 보신 영화 있나요?",
          "original_form": "최근에 재밌게 보신 영화 있나요?",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100025211.1.12",
          "form": "저는 사랑의 블랙홀이요!",
          "original_form": "저는 사랑의 블랙홀이요!",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025211.1.13",
          "form": "예전에 아는 분이 추천해주셨는데",
          "original_form": "예전에 아는 분이 추천해주셨는데",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025211.1.14",
          "form": "나쁘지 않아요",
          "original_form": "나쁘지 않아요",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025211.1.15",
          "form": "아 그거 로맨스 코미디죠? ㅎㅎ 본 적 있어요",
          "original_form": "아 그거 로맨스 코미디죠? ㅎㅎ 본 적 있어요",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100025211.1.16",
          "form": "루프물이라 인상 깊게 본 기억이 있네요",
          "original_form": "루프물이라 인상 깊게 본 기억이 있네요",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025211.1.17",
          "form": "와 정말요?",
          "original_form": "와 정말요?",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025211.1.18",
          "form": "맨날 groundhog day 라서 마못 계속 나오는 게 너무 귀여웠어요",
          "original_form": "맨날 groundhog day 라서 마못 계속 나오는 게 너무 귀여웠어요",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025211.1.19",
          "form": "기억에 남는 장면 있으신가요?",
          "original_form": "기억에 남는 장면 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100025211.1.20",
          "form": "전 리타한테 빰 맞는 거요ㅋㅋㅋ 계속 꼬시는 거 실패하는 게 웃겼어요",
          "original_form": "전 리타한테 빰 맞는 거요ㅋㅋㅋ 계속 꼬시는 거 실패하는 게 웃겼어요",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025211.1.21",
          "form": "결말도 기억에 남네요",
          "original_form": "결말도 기억에 남네요",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025211.1.22",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025211.1.23",
          "form": "리타한테 꼬시는데 리타가 호락호락하지 않은 부분이 진짜 재밌었던 거 같아요",
          "original_form": "리타한테 꼬시는데 리타가 호락호락하지 않은 부분이 진짜 재밌었던 거 같아요",
          "speaker_id": "2",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025211.1.24",
          "form": "맞아요ㅎㅎ 전 넷플도 구독하는데 왓챠랑 넷플에 있는 작품들이 달라서 둘 다 구독을 꾾을 수가 없네요ㅠㅠ",
          "original_form": "맞아요ㅎㅎ 전 넷플도 구독하는데 왓챠랑 넷플에 있는 작품들이 달라서 둘 다 구독을 꾾을 수가 없네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 01:25"
        },
        {
          "id": "MDRW2100025211.1.25",
          "form": "왓챠도 사실 구독해지하려고 했는데 해리포터 올라와서 계속 구독 중이에여",
          "original_form": "왓챠도 사실 구독해지하려고 했는데 해리포터 올라와서 계속 구독 중이에여",
          "speaker_id": "1",
          "time": "20210901 01:31"
        },
        {
          "id": "MDRW2100025211.1.26",
          "form": "오 저도 왓챠 해요!",
          "original_form": "오 저도 왓챠 해요!",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100025211.1.27",
          "form": "왓챠도 볼 작품 많더라구요 특히 영화요",
          "original_form": "왓챠도 볼 작품 많더라구요 특히 영화요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        }
      ]
