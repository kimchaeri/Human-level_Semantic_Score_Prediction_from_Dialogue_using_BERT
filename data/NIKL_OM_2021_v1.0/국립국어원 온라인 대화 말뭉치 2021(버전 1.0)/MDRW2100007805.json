      "utterance": [
        {
          "id": "MDRW2100007805.1.1",
          "form": "비가왔나요?",
          "original_form": "비가왔나요?",
          "speaker_id": "2",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007805.1.2",
          "form": "태풍오고있다하네요",
          "original_form": "태풍오고있다하네요",
          "speaker_id": "1",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007805.1.3",
          "form": "어제 비가 엄청많이쏟아졌어요",
          "original_form": "어제 비가 엄청많이쏟아졌어요",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.4",
          "form": "헐 어제까지 쏟아졌다 지금 그쳤는데",
          "original_form": "헐 어제까지 쏟아졌다 지금 그쳤는데",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.5",
          "form": "지금도비올것같아요",
          "original_form": "지금도비올것같아요",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.6",
          "form": "태풍이 온다구요?",
          "original_form": "태풍이 온다구요?",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.7",
          "form": "그런데 더운것보단 비오는게나아요",
          "original_form": "그런데 더운것보단 비오는게나아요",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.8",
          "form": "세상에 여름 다 갔는데 태풍이라니",
          "original_form": "세상에 여름 다 갔는데 태풍이라니",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.9",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007805.1.10",
          "form": "비오는 날 어떠세요",
          "original_form": "비오는 날 어떠세요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007805.1.11",
          "form": "저는 비를 좋아하는 편은 아니에요",
          "original_form": "저는 비를 좋아하는 편은 아니에요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007805.1.12",
          "form": "저는좋아해요",
          "original_form": "저는좋아해요",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007805.1.13",
          "form": "더운게더싫어서요",
          "original_form": "더운게더싫어서요",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007805.1.14",
          "form": "어떤점이  좋던가요",
          "original_form": "어떤점이  좋던가요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007805.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007805.1.16",
          "form": "그럼 눈은 어때요?",
          "original_form": "그럼 눈은 어때요?",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007805.1.17",
          "form": "눈은 좋아하시나요",
          "original_form": "눈은 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007805.1.18",
          "form": "눈은 내릴땐좋은데 빙판길되는게싫어서",
          "original_form": "눈은 내릴땐좋은데 빙판길되는게싫어서",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007805.1.19",
          "form": "공감합니다",
          "original_form": "공감합니다",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007805.1.20",
          "form": "넘어질거같은느낌이싫어요ㅜㅜ",
