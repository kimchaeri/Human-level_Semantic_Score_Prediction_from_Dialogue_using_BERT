      "utterance": [
        {
          "id": "MDRW2100044391.1.1",
          "form": "반려동물키우시나요",
          "original_form": "반려동물키우시나요",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100044391.1.2",
          "form": "강아지랑 고양이요",
          "original_form": "강아지랑 고양이요",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100044391.1.3",
          "form": "간식자주주시나요",
          "original_form": "간식자주주시나요",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100044391.1.4",
          "form": "고양이는 자주 주는데",
          "original_form": "고양이는 자주 주는데",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100044391.1.5",
          "form": "강아지는 어려서 조금만 줘요",
          "original_form": "강아지는 어려서 조금만 줘요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100044391.1.6",
          "form": "ㅎㅎ산책도가시나요",
          "original_form": "ㅎㅎ산책도가시나요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100044391.1.7",
          "form": "접종을 아직 다 못해서 못가요ㅠㅠ",
          "original_form": "접종을 아직 다 못해서 못가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100044391.1.8",
          "form": "ㅜㅜ아구",
          "original_form": "ㅜㅜ아구",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100044391.1.9",
          "form": "고양이도산책시켜줘야하나요?",
          "original_form": "고양이도산책시켜줘야하나요?",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100044391.1.10",
          "form": "아뇨 ㅋㅋ 고양이는",
          "original_form": "아뇨 ㅋㅋ 고양이는",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100044391.1.11",
          "form": "산책못시켜요유ㅠ",
          "original_form": "산책못시켜요유ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100044391.1.12",
          "form": "밖을 싫어해서",
          "original_form": "밖을 싫어해서",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100044391.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100044391.1.14",
          "form": "구석을좋아하나요",
          "original_form": "구석을좋아하나요",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100044391.1.15",
          "form": "넹 ㅋㅋㅋ 집안에서만 잘놀아요 그래서",
          "original_form": "넹 ㅋㅋㅋ 집안에서만 잘놀아요 그래서",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100044391.1.16",
          "form": "ㅎㅎㅎ냥이는혼자놀아요?",
          "original_form": "ㅎㅎㅎ냥이는혼자놀아요?",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100044391.1.17",
          "form": "제가 놀아줘야죰 ㅋㅋㅋ",
          "original_form": "제가 놀아줘야죰 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100044391.1.18",
          "form": "까다로워서",
          "original_form": "까다로워서",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100044391.1.19",
          "form": "놀아주기도 힘들어야...ㅋㅋ",
          "original_form": "놀아주기도 힘들어야...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100044391.1.20",
          "form": "ㅋㅋㅋㅇㅋ도도하군요",
          "original_form": "ㅋㅋㅋㅇㅋ도도하군요",
