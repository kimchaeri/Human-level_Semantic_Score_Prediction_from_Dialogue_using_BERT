        },
        {
          "id": "MDRW2100007312.1.2",
          "form": "카광 보냐?",
          "original_form": "카광 보냐?",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007312.1.3",
          "form": "이번 전소미 dumb dumb음원 너무 좋음",
          "original_form": "이번 전소미 dumb dumb음원 너무 좋음",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007312.1.4",
          "form": "오늘조선일보에 카광떴더라",
          "original_form": "오늘조선일보에 카광떴더라",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007312.1.5",
          "form": "카광은... 안보죠",
          "original_form": "카광은... 안보죠",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.6",
          "form": "그건 좀",
          "original_form": "그건 좀",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.7",
          "form": "뉴스메인에서봤어",
          "original_form": "뉴스메인에서봤어",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.8",
          "form": "사회에 반하는 채널이니까",
          "original_form": "사회에 반하는 채널이니까",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.9",
          "form": "너가추천해줬는데",
          "original_form": "너가추천해줬는데",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.10",
          "form": "다급하네",
          "original_form": "다급하네",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.11",
          "form": "얌전떨지마",
          "original_form": "얌전떨지마",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.12",
          "form": "내가언제",
          "original_form": "내가언제",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.13",
          "form": "너가 먼저 알려줬잖아",
          "original_form": "너가 먼저 알려줬잖아",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.14",
          "form": "이번에 60대랑 만난거 밨냐구",
          "original_form": "이번에 60대랑 만난거 밨냐구",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.15",
          "form": "나는만화",
          "original_form": "나는만화",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.16",
          "form": "너가 먼저 알려줬잖아",
          "original_form": "너가 먼저 알려줬잖아",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.17",
          "form": "너가 먼저!",
          "original_form": "너가 먼저!",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.18",
          "form": "유튜브는너가",
          "original_form": "유튜브는너가",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.19",
          "form": "60대랑만난거안봣고",
          "original_form": "60대랑만난거안봣고",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.20",
          "form": "카광영상이제그만시청",
          "original_form": "카광영상이제그만시청",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007312.1.21",
          "form": "음....ㅎㅎ",
          "original_form": "음....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007312.1.22",
          "form": "너또한...!",
          "original_form": "너또한...!",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007312.1.23",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007312.1.24",
          "form": "그거 솔직히 사안에 비해",
          "original_form": "그거 솔직히 사안에 비해",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007312.1.25",
          "form": "너무 조용히 스리슬쩍 지나가지않어...?",
          "original_form": "너무 조용히 스리슬쩍 지나가지않어...?",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007312.1.26",
          "form": "60대 할아버지가 초딩들 만나고 다닌건데...헬퍼 자처하면서",
          "original_form": "60대 할아버지가 초딩들 만나고 다닌건데...헬퍼 자처하면서",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.27",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.28",
          "form": "너무자세히",
          "original_form": "너무자세히",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.29",
          "form": "콘텐츠에대해설명하는듯",
          "original_form": "콘텐츠에대해설명하는듯",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.31",
          "form": "그러니까 이게 당이랑도 관련이있는데",
          "original_form": "그러니까 이게 당이랑도 관련이있는데",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.32",
          "form": "책으로 역기야 하겟어...",
          "original_form": "책으로 역기야 하겟어...",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.33",
          "form": "아 그니까",
          "original_form": "아 그니까",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.34",
          "form": "찐 그당 추종자 아니냐고",
          "original_form": "찐 그당 추종자 아니냐고",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.35",
          "form": "근데 점점 퍼지는것같기도?",
          "original_form": "근데 점점 퍼지는것같기도?",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.36",
          "form": "2대에 걸쳐 선거위원장 했으면...",
          "original_form": "2대에 걸쳐 선거위원장 했으면...",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.37",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007312.1.38",
          "form": "오늘자뉴스에난거보머ㅕㄴ",
          "original_form": "오늘자뉴스에난거보머ㅕㄴ",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007312.1.39",
          "form": "아 조선일보네",
          "original_form": "아 조선일보네",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007312.1.40",
          "form": "에",
          "original_form": "에",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007312.1.41",
          "form": "크게 났어?",
          "original_form": "크게 났어?",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007312.1.42",
          "form": "그냥 뉴스돌리다보면",
          "original_form": "그냥 뉴스돌리다보면",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007312.1.43",
          "form": "보일정도로",
          "original_form": "보일정도로",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007312.1.44",
          "form": "그 영상 캡쳐가",
          "original_form": "그 영상 캡쳐가",
          "speaker_id": "1",
          "time": "20210822 01:25"
        }
      ]
    }
