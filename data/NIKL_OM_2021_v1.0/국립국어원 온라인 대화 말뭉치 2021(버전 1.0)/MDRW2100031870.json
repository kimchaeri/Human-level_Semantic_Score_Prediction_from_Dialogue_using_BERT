      "utterance": [
        {
          "id": "MDRW2100031870.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210905 06:46"
        },
        {
          "id": "MDRW2100031870.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210905 06:46"
        },
        {
          "id": "MDRW2100031870.1.3",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210905 06:46"
        },
        {
          "id": "MDRW2100031870.1.4",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 15:02"
        },
        {
          "id": "MDRW2100031870.1.5",
          "form": "맑은가을하늘을 즐기고싶네요~",
          "original_form": "맑은가을하늘을 즐기고싶네요~",
          "speaker_id": "2",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100031870.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100031870.1.7",
          "form": "거기는 아직 흐린 건가요?",
          "original_form": "거기는 아직 흐린 건가요?",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100031870.1.8",
          "form": "여기는 주말 내내 맑은 가을하늘이어서 기분 좋게 보내고 있습니다",
          "original_form": "여기는 주말 내내 맑은 가을하늘이어서 기분 좋게 보내고 있습니다",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100031870.1.9",
          "form": "바람도 선선해졌어요",
          "original_form": "바람도 선선해졌어요",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100031870.1.10",
          "form": "산책하기 좋아요",
          "original_form": "산책하기 좋아요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100031870.1.11",
          "form": "오전엔 맑았는데 흐려졌네요 지금보니",
          "original_form": "오전엔 맑았는데 흐려졌네요 지금보니",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100031870.1.12",
          "form": "좀있으면 단풍구경가도 좋겠어요",
          "original_form": "좀있으면 단풍구경가도 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100031870.1.13",
          "form": "네 단풍은 정말 예쁘지요",
          "original_form": "네 단풍은 정말 예쁘지요",
          "speaker_id": "1",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100031870.1.14",
          "form": "해마다 보러 다녔었는데 작년부터 못다녀서 좀 아쉽네요",
          "original_form": "해마다 보러 다녔었는데 작년부터 못다녀서 좀 아쉽네요",
          "speaker_id": "1",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100031870.1.15",
          "form": "그러게말이에요",
          "original_form": "그러게말이에요",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100031870.1.16",
          "form": "맘놓고 어디다니질 못하니",
          "original_form": "맘놓고 어디다니질 못하니",
          "speaker_id": "2",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100031870.1.17",
          "form": "내년엔 상황이 좋아지길 빌어봅니다",
          "original_form": "내년엔 상황이 좋아지길 빌어봅니다",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100031870.1.18",
          "form": "가장 좋아하는 계절이 가을이신가요?",
          "original_form": "가장 좋아하는 계절이 가을이신가요?",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100031870.1.19",
          "form": "네 가을이 젤 좋아요",
          "original_form": "네 가을이 젤 좋아요",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100031870.1.20",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100031870.1.21",
          "form": "저는 사실 여름을 좋아했습니다 지금은 아니네요",
          "original_form": "저는 사실 여름을 좋아했습니다 지금은 아니네요",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100031870.1.22",
          "form": "이젠 바뀌었단 말씀인가요?",
          "original_form": "이젠 바뀌었단 말씀인가요?",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100031870.1.23",
          "form": "네 전 예전의 여름은 좋았는데 요새 기상이변으로 그때 그 느낌이 아니라서 요즘의 여름은 별로 좋지 않더군요",
          "original_form": "네 전 예전의 여름은 좋았는데 요새 기상이변으로 그때 그 느낌이 아니라서 요즘의 여름은 별로 좋지 않더군요",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100031870.1.24",
          "form": "혹시제답이 안갔나요?",
          "original_form": "혹시제답이 안갔나요?",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100031870.1.25",
          "form": "뭔가 보냈었다면 안온거네요",
          "original_form": "뭔가 보냈었다면 안온거네요",
          "speaker_id": "1",
          "time": "20210906 06:06"
        },
        {
          "id": "MDRW2100031870.1.26",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "1",
          "time": "20210906 06:12"
        },
        {
          "id": "MDRW2100031870.1.27",
          "form": "가끔 오류가 날 때가 있습니다",
          "original_form": "가끔 오류가 날 때가 있습니다",
          "speaker_id": "1",
          "time": "20210906 06:12"
        },
        {
          "id": "MDRW2100031870.1.28",
          "form": "아니면 별 말 아닌데 나쁜 말로 걸려지는 경우도 있지요",
          "original_form": "아니면 별 말 아닌데 나쁜 말로 걸려지는 경우도 있지요",
          "speaker_id": "1",
          "time": "20210906 06:13"
        },
        {
          "id": "MDRW2100031870.1.29",
          "form": "답이 안갈 때가 저도 종종 있었습니다",
          "original_form": "답이 안갈 때가 저도 종종 있었습니다",
          "speaker_id": "1",
          "time": "20210906 06:13"
        }
      ]
    }
  ]
