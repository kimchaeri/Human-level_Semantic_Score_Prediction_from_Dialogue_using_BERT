      "utterance": [
        {
          "id": "MDRW2100011946.1.1",
          "form": "여행가고싶네요~",
          "original_form": "여행가고싶네요~",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011946.1.2",
          "form": "비행기타규 슝슝~",
          "original_form": "비행기타규 슝슝~",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011946.1.3",
          "form": "자격증도 못따고 무슨 여행이고 ㅎㅎ",
          "original_form": "자격증도 못따고 무슨 여행이고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011946.1.4",
          "form": "ㅋㅋㅋㅋㅋ여행 자격증이라요??ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ여행 자격증이라요??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011946.1.5",
          "form": "아욱겨~",
          "original_form": "아욱겨~",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011946.1.6",
          "form": "아까 name3랑도 얘기했는데",
          "original_form": "아까 &name3&랑도 얘기했는데",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011946.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 우리 그때 코로나 없었을때 다 모여서 놀았제??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 우리 그때 코로나 없었을때 다 모여서 놀았제??",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.8",
          "form": "여행도 여행이고 이월드라도 맘편하게 가고싶네여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여행도 여행이고 이월드라도 맘편하게 가고싶네여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.9",
          "form": "텐트 펴고",
          "original_form": "텐트 펴고",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.10",
          "form": "구지에서",
          "original_form": "구지에서",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.11",
          "form": "그래 이월드 가고싶다",
          "original_form": "그래 이월드 가고싶다",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.12",
          "form": "아~!!그래 그때 잠깐이라도",
          "original_form": "아~!!그래 그때 잠깐이라도",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.13",
          "form": "재미있었는데 ㅋㅋㅋㅋ",
          "original_form": "재미있었는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011946.1.14",
          "form": "글치 재밌었지",
          "original_form": "글치 재밌었지",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011946.1.15",
          "form": "신랑은 캠핑 관심없나??",
          "original_form": "신랑은 캠핑 관심없나??",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011946.1.16",
          "form": "우예 또 그날은 5집 다 시간되서 ㅋㅋㅋ",
          "original_form": "우예 또 그날은 5집 다 시간되서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011946.1.17",
          "form": "우린..첫찌가 활동적이지 않아서",
          "original_form": "우린..첫찌가 활동적이지 않아서",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011946.1.18",
          "form": "텐트들고가면 30분 쯤 지나면 가자해요ㅠ",
          "original_form": "텐트들고가면 30분 쯤 지나면 가자해요ㅠ",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011946.1.19",
          "form": "아하 ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011946.1.20",
          "form": "우리원도 작년에는 벌레있고 더럽다고",
          "original_form": "우리원도 작년에는 벌레있고 더럽다고",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011946.1.21",
          "form": "캠핑가는거 싫다고",
          "original_form": "캠핑가는거 싫다고",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011946.1.22",
          "form": "펜션가자고 하더니",
          "original_form": "펜션가자고 하더니",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011946.1.23",
          "form": "올해는 name4랑 가고 지인들이랑 같이 가서 그런지",
          "original_form": "올해는 &name4&랑 가고 지인들이랑 같이 가서 그런지",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011946.1.24",
          "form": "재밌다고 하더라고",
          "original_form": "재밌다고 하더라고",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011946.1.25",
          "form": "글쵸~그 맘맞는친구를 잘 만나야되여~",
          "original_form": "글쵸~그 맘맞는친구를 잘 만나야되여~",
          "speaker_id": "2",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011946.1.26",
          "form": "아무것도 없어도 재미있게 놀수있는친구 ㅋㅋㅋ",
          "original_form": "아무것도 없어도 재미있게 놀수있는친구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011946.1.27",
          "form": "우린..둘찌랑은 어딜가도 싫아함..ㅠ",
          "original_form": "우린..둘찌랑은 어딜가도 싫아함..ㅠ",
          "speaker_id": "2",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011946.1.28",
          "form": "가슴이 아파여ㅠ",
          "original_form": "가슴이 아파여ㅠ",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011946.1.29",
          "form": "ㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋㄱ 그래 우리집투도 늘 징징이다",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋㄱ 그래 우리집투도 늘 징징이다",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011946.1.30",
          "form": "둘째라서 더 그런듯",
          "original_form": "둘째라서 더 그런듯",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011946.1.31",
          "form": "물놀이 가도 싫어하고",
          "original_form": "물놀이 가도 싫어하고",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011946.1.32",
          "form": "원투 다 물을 싫어함",
          "original_form": "원투 다 물을 싫어함",
          "speaker_id": "1",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011946.1.33",
          "form": "우린 물에는 환장요~",
          "original_form": "우린 물에는 환장요~",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011946.1.34",
          "form": "근데 계속 붙어서 놀어주라고~그래서 사촌언니네 두집이랑 늘 같이다녀요;;",
          "original_form": "근데 계속 붙어서 놀어주라고~그래서 사촌언니네 두집이랑 늘 같이다녀요;;",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011946.1.35",
          "form": "딸둘인 집 애들 다 컸는데 놀고 먹이고 씻기고 다해줘여 ㅋㅋㅋㅋㅋ",
          "original_form": "딸둘인 집 애들 다 컸는데 놀고 먹이고 씻기고 다해줘여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011946.1.36",
          "form": "대신 큰 언니네 아들, 딸 겁나별나고;;",
          "original_form": "대신 큰 언니네 아들, 딸 겁나별나고;;",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100011946.1.37",
          "form": "오 좋다잉",
          "original_form": "오 좋다잉",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011946.1.38",
          "form": "딸둘이 좋지",
          "original_form": "딸둘이 좋지",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011946.1.39",
          "form": "물 좋아하믄 갈데 많겠는데",
          "original_form": "물 좋아하믄 갈데 많겠는데",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011946.1.40",
          "form": "우린 수영장 있는 캠핑장 갔는데 \r\n물 좀 차가우니 바로 나옴",
          "original_form": "우린 수영장 있는 캠핑장 갔는데 \r\n물 좀 차가우니 바로 나옴",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011946.1.41",
          "form": "우리도 첫찌는 못놀고 둘찌는 차갑기나 말기나..물에서 안나와요..",
          "original_form": "우리도 첫찌는 못놀고 둘찌는 차갑기나 말기나..물에서 안나와요..",
          "speaker_id": "2",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011946.1.42",
          "form": "그람 또 따로 하나씩 봐야되고ㅠ",
          "original_form": "그람 또 따로 하나씩 봐야되고ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:55"
        }
      ]
