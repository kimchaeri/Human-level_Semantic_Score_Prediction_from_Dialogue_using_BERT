      "utterance": [
        {
          "id": "MDRW2100044588.1.1",
          "form": "아직이라며",
          "original_form": "아직이라며",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044588.1.2",
          "form": "이게 아직이라서 내린 결정이니",
          "original_form": "이게 아직이라서 내린 결정이니",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.3",
          "form": "온콘이랑 병행이라도 하니까 다행",
          "original_form": "온콘이랑 병행이라도 하니까 다행",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.4",
          "form": "한아직ㅋㅋㅋㅋㅋㅋㅋ...",
          "original_form": "한아직ㅋㅋㅋㅋㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.5",
          "form": "일본 빠돌이 두명이 만나가지고",
          "original_form": "일본 빠돌이 두명이 만나가지고",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.6",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.7",
          "form": "근데 첫공도 막공도 가고싶어",
          "original_form": "근데 첫공도 막공도 가고싶어",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.8",
          "form": "첫콘은 그 감격스러움을 느끼러",
          "original_form": "첫콘은 그 감격스러움을 느끼러",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.9",
          "form": "일본에 목매는놈들 싫어요",
          "original_form": "일본에 목매는놈들 싫어요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.10",
          "form": "막콘은 그 감정의소용돌이를 느끼러",
          "original_form": "막콘은 그 감정의소용돌이를 느끼러",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.11",
          "form": "갈수있으면",
          "original_form": "갈수있으면",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.12",
          "form": "무조건 가야지..",
          "original_form": "무조건 가야지..",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044588.1.13",
          "form": "방돼지 주거쓰면",
          "original_form": "방돼지 주거쓰면",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.14",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.15",
          "form": "온라인콘서트는",
          "original_form": "온라인콘서트는",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.16",
          "form": "어휴진짜",
          "original_form": "어휴진짜",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.17",
          "form": "적당한 가격으로 팔아라....",
          "original_form": "적당한 가격으로 팔아라....",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.18",
          "form": "현장의 반의 반도 못따라가",
          "original_form": "현장의 반의 반도 못따라가",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.19",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.20",
          "form": "오프콘은 12만원씩 해도",
          "original_form": "오프콘은 12만원씩 해도",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.21",
          "form": "잘만 다녔지만",
          "original_form": "잘만 다녔지만",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044588.1.22",
          "form": "내가 거기없음 무슨소용",
          "original_form": "내가 거기없음 무슨소용",
