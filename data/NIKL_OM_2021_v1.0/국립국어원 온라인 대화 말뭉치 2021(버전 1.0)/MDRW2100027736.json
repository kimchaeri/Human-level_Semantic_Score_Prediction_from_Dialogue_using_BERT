      "utterance": [
        {
          "id": "MDRW2100027736.1.1",
          "form": "1인분 만들기 은근 어렵죠....",
          "original_form": "1인분 만들기 은근 어렵죠....",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027736.1.2",
          "form": "특히 파스타는요 ㅋㅋ",
          "original_form": "특히 파스타는요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027736.1.3",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027736.1.4",
          "form": "그래서 항상 많이 먹게되더라구요",
          "original_form": "그래서 항상 많이 먹게되더라구요",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027736.1.5",
          "form": "어떤 파스타 좋아하세요?",
          "original_form": "어떤 파스타 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027736.1.6",
          "form": "전 알리오올리오를 잘하는데",
          "original_form": "전 알리오올리오를 잘하는데",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027736.1.7",
          "form": "좋아하는 건...토마토파스타입니다",
          "original_form": "좋아하는 건...토마토파스타입니다",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027736.1.8",
          "form": "아 좋아하는것과 잘하는게 같기 힘들죠",
          "original_form": "아 좋아하는것과 잘하는게 같기 힘들죠",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027736.1.9",
          "form": "전 로제 좋아해요",
          "original_form": "전 로제 좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027736.1.10",
          "form": "알리오올리오는 진짜 간단하게 먹기 좋은것같아요",
          "original_form": "알리오올리오는 진짜 간단하게 먹기 좋은것같아요",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027736.1.11",
          "form": "로제도 맛있죠! 알리오올리오에는 여러가지 많이 넣어먹기 좋아요",
          "original_form": "로제도 맛있죠! 알리오올리오에는 여러가지 많이 넣어먹기 좋아요",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027736.1.12",
          "form": "야채나....등등...버섯이나..",
          "original_form": "야채나....등등...버섯이나..",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027736.1.13",
          "form": "버섯 진짜 맛있죠",
          "original_form": "버섯 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027736.1.14",
          "form": "야채는 주로 뭘 넣어먹으세요?",
          "original_form": "야채는 주로 뭘 넣어먹으세요?",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027736.1.15",
          "form": "전 양파 자주 넣어먹어요",
          "original_form": "전 양파 자주 넣어먹어요",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027736.1.16",
          "form": "버섯도 왕창 넣어먹고 양파는 기본으로 넣는 느낌이에요",
          "original_form": "버섯도 왕창 넣어먹고 양파는 기본으로 넣는 느낌이에요",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027736.1.17",
          "form": "알리오올리오에 멸치액젓 살짝 넣으면 더 맛있어요!",
          "original_form": "알리오올리오에 멸치액젓 살짝 넣으면 더 맛있어요!",
          "speaker_id": "1",
          "time": "20210902 16:42"
        },
        {
          "id": "MDRW2100027736.1.18",
          "form": "멸치라니",
          "original_form": "멸치라니",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027736.1.19",
          "form": "생각도 못했어요",
          "original_form": "생각도 못했어요",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027736.1.20",
          "form": "나중에 있으면 시도해볼게요",
          "original_form": "나중에 있으면 시도해볼게요",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027736.1.21",
          "form": "와 완전 꿀팁 감사합니다",
          "original_form": "와 완전 꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027736.1.22",
          "form": "한숟가락 정도만 넣으면 감칠맛이 나요! 간도 되고요",
          "original_form": "한숟가락 정도만 넣으면 감칠맛이 나요! 간도 되고요",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100027736.1.23",
          "form": "저녁은 뭐 드실 거예요?",
          "original_form": "저녁은 뭐 드실 거예요?",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100027736.1.24",
          "form": "저녁 고민중이에요",
          "original_form": "저녁 고민중이에요",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027736.1.25",
          "form": "아마 샌드위치나 빵 먹게될것 같은데",
          "original_form": "아마 샌드위치나 빵 먹게될것 같은데",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027736.1.26",
          "form": "name1님은 뭐 드실예정인가요?",
          "original_form": "&name1&님은 뭐 드실예정인가요?",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027736.1.27",
          "form": "친구가 온대서 아마 불고기랑 피자 만들러 먹을 거 같아요!",
          "original_form": "친구가 온대서 아마 불고기랑 피자 만들러 먹을 거 같아요!",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027736.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027736.1.29",
          "form": "피자를 만들어 드시나요?",
          "original_form": "피자를 만들어 드시나요?",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027736.1.30",
          "form": "금손이시네요",
          "original_form": "금손이시네요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027736.1.31",
          "form": "친구랑 저녁파티 엄청 재밌을것같야요",
          "original_form": "친구랑 저녁파티 엄청 재밌을것같야요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027736.1.32",
          "form": "그냥 또띠아에 올리려고요 ㅋㅋㅋ",
          "original_form": "그냥 또띠아에 올리려고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100027736.1.33",
          "form": "저녁파티...는 아니지만 파티처럼 잼게 놀고 싶습다 ㅋㅋ",
          "original_form": "저녁파티...는 아니지만 파티처럼 잼게 놀고 싶습다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100027736.1.34",
          "form": "아 또띠아 불고기랑도 잘어울려요",
          "original_form": "아 또띠아 불고기랑도 잘어울려요",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100027736.1.35",
          "form": "재밌는 시간 보내세요!",
          "original_form": "재밌는 시간 보내세요!",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100027736.1.36",
          "form": "저녁 맛있게 드세요 !",
          "original_form": "저녁 맛있게 드세요 !",
          "speaker_id": "1",
          "time": "20210902 17:44"
        }
      ]
