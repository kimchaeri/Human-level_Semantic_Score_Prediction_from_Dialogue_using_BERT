      "utterance": [
        {
          "id": "MDRW2100045179.1.1",
          "form": "아아아앗 오징어도 들어가요?",
          "original_form": "아아아앗 오징어도 들어가요?",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045179.1.2",
          "form": "새우랑 잘어울리는",
          "original_form": "새우랑 잘어울리는",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045179.1.3",
          "form": "앗.. 새우랑 잘 어울리는..",
          "original_form": "앗.. 새우랑 잘 어울리는..",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045179.1.4",
          "form": "조개가 들어갔나요?",
          "original_form": "조개가 들어갔나요?",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045179.1.5",
          "form": "조개는 안넣었답니당",
          "original_form": "조개는 안넣었답니당",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045179.1.6",
          "form": "으음 으음 김치가 들어가나요?",
          "original_form": "으음 으음 김치가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045179.1.7",
          "form": "김치는 안들어갓어요",
          "original_form": "김치는 안들어갓어요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045179.1.8",
          "form": "소스를 섞었어요",
          "original_form": "소스를 섞었어요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045179.1.9",
          "form": "허억 소스라면",
          "original_form": "허억 소스라면",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045179.1.10",
          "form": "굴소스인가요!",
          "original_form": "굴소스인가요!",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045179.1.11",
          "form": "아닙니당 !! 빨간소스에요",
          "original_form": "아닙니당 !! 빨간소스에요",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045179.1.12",
          "form": "앗 그렇담 토마토소스?",
          "original_form": "앗 그렇담 토마토소스?",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045179.1.13",
          "form": "아닙니당 !! ㅋㅋ 새콤한 소스예요",
          "original_form": "아닙니당 !! ㅋㅋ 새콤한 소스예요",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045179.1.14",
          "form": "아 뭐지 뭐지",
          "original_form": "아 뭐지 뭐지",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045179.1.15",
          "form": "스위트 칠리소스?",
          "original_form": "스위트 칠리소스?",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045179.1.16",
          "form": "생각해보니까",
          "original_form": "생각해보니까",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045179.1.17",
          "form": "토마토소스가 맞네용 !! ㅋㅋㅋ",
          "original_form": "토마토소스가 맞네용 !! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045179.1.18",
          "form": "ㅋㅋㅌㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045179.1.19",
          "form": "좋아요! 도매도소스",
