        {
          "id": "MDRW2100003305.1.2",
          "form": "무교용~~~",
          "original_form": "무교용~~~",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003305.1.3",
          "form": "저도무교입니다",
          "original_form": "저도무교입니다",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003305.1.4",
          "form": "저는 종교는 잘모르겠어요",
          "original_form": "저는 종교는 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003305.1.5",
          "form": "배척은아닌데",
          "original_form": "배척은아닌데",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003305.1.6",
          "form": "믿음이 부족한가봐요",
          "original_form": "믿음이 부족한가봐요",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003305.1.7",
          "form": "음 저는 마음으로 믿어요",
          "original_form": "음 저는 마음으로 믿어요",
          "speaker_id": "2",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003305.1.8",
          "form": "가지는 않죠",
          "original_form": "가지는 않죠",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003305.1.9",
          "form": "ㄱㅋㅋㄱㄱ저도요저는",
          "original_form": "ㄱㅋㅋㄱㄱ저도요저는",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003305.1.10",
          "form": "제자신만믿어요",
          "original_form": "제자신만믿어요",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003305.1.11",
          "form": "전 가끔 기도는 해요 ㅎㅎ",
          "original_form": "전 가끔 기도는 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003305.1.12",
          "form": "저는 가끔 저도 못믿어요",
          "original_form": "저는 가끔 저도 못믿어요",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003305.1.13",
          "form": "ㅎㅎㅎ 왜요",
          "original_form": "ㅎㅎㅎ 왜요",
          "speaker_id": "2",
          "time": "20210820 22:25"
        },
        {
          "id": "MDRW2100003305.1.14",
          "form": "ㅋㅋㅋ도움이되더라구요",
          "original_form": "ㅋㅋㅋ도움이되더라구요",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003305.1.15",
          "form": "어떤 도움이요 ?",
          "original_form": "어떤 도움이요 ?",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003305.1.16",
          "form": "저도안믿는도움요",
          "original_form": "저도안믿는도움요",
          "speaker_id": "1",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003305.1.17",
          "form": "z앗 신기하네요 ㅎㅎ",
          "original_form": "z앗 신기하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003305.1.18",
          "form": "ㅋㅋㅋㅋ종교는 정치수단같아요",
          "original_form": "ㅋㅋㅋㅋ종교는 정치수단같아요",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003305.1.19",
          "form": "음 어떻게 보면 기업이기도 하구요",
          "original_form": "음 어떻게 보면 기업이기도 하구요",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
