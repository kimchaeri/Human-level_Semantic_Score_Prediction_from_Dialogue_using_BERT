      "utterance": [
        {
          "id": "MDRW2100015283.1.1",
          "form": "이시간만 되면 라면이 먹고 싶네요",
          "original_form": "이시간만 되면 라면이 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015283.1.2",
          "form": "저도요ㅠㅠ밥도 말구싶구...",
          "original_form": "저도요ㅠㅠ밥도 말구싶구...",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015283.1.3",
          "form": "낼 엄청 부을까바 참을려고요!!",
          "original_form": "낼 엄청 부을까바 참을려고요!!",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015283.1.4",
          "form": "네 저도 9시면 먹었을텐데ㅠㅠㅠ과자뜯으려구요",
          "original_form": "네 저도 9시면 먹었을텐데ㅠㅠㅠ과자뜯으려구요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015283.1.5",
          "form": "우아 무슨 과자 뜯으셧어용",
          "original_form": "우아 무슨 과자 뜯으셧어용",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015283.1.6",
          "form": "꼬북칩이요ㅎㅎ 좋아하는 과자있으세요?",
          "original_form": "꼬북칩이요ㅎㅎ 좋아하는 과자있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015283.1.7",
          "form": "오저도 꼬북칩 좋아해요 ㅋㅋㅋ",
          "original_form": "오저도 꼬북칩 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015283.1.8",
          "form": "저는 달달한게 좋아요!!",
          "original_form": "저는 달달한게 좋아요!!",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015283.1.9",
          "form": "저두요ㅎㅎ 초코과자ㅎㅎ",
          "original_form": "저두요ㅎㅎ 초코과자ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015283.1.10",
          "form": "지금 제앞에 과자가 있는데 저도 먹어야겟어요 ㅋㅋ",
          "original_form": "지금 제앞에 과자가 있는데 저도 먹어야겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015283.1.11",
          "form": "티비 보면서 먹을려고요",
          "original_form": "티비 보면서 먹을려고요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015283.1.12",
          "form": "과자 요즘 맛있는거 너무 많아요ㅋㅋ",
          "original_form": "과자 요즘 맛있는거 너무 많아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015283.1.13",
          "form": "맞아요!! 신메뉴도 많이 나왓어욬ㅋㅋ 이번에",
          "original_form": "맞아요!! 신메뉴도 많이 나왓어욬ㅋㅋ 이번에",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015283.1.14",
          "form": "민트초코맛이 유행인가바요",
          "original_form": "민트초코맛이 유행인가바요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015283.1.15",
          "form": "아 맞아요ㅎㅎ 재밌고 맛있어요ㅋㅋ",
          "original_form": "아 맞아요ㅎㅎ 재밌고 맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015283.1.16",
          "form": "민트초코 좋아하세요??",
