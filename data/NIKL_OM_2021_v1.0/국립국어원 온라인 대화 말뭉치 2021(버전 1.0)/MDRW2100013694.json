        {
          "id": "MDRW2100013694.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100013694.1.2",
          "form": "너 어릴때 꿈이 뭐였지",
          "original_form": "너 어릴때 꿈이 뭐였지",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100013694.1.3",
          "form": "파일럿",
          "original_form": "파일럿",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100013694.1.4",
          "form": "멋진꿈이네",
          "original_form": "멋진꿈이네",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100013694.1.5",
          "form": "이유는 안물어봐?",
          "original_form": "이유는 안물어봐?",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100013694.1.6",
          "form": "너야 어려서부터 여행을 좋아했으니",
          "original_form": "너야 어려서부터 여행을 좋아했으니",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100013694.1.7",
          "form": "맞아 비행기타는거 엄청 좋아했지",
          "original_form": "맞아 비행기타는거 엄청 좋아했지",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100013694.1.8",
          "form": "언제 꿈을 접었어",
          "original_form": "언제 꿈을 접었어",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100013694.1.9",
          "form": "한 중3쯤?",
          "original_form": "한 중3쯤?",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100013694.1.10",
          "form": "이유는?",
          "original_form": "이유는?",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100013694.1.11",
          "form": "키가 더이상 크질 않더라",
          "original_form": "키가 더이상 크질 않더라",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100013694.1.12",
          "form": "하긴.. 너키론 스튜어디스도힘들지",
          "original_form": "하긴.. 너키론 스튜어디스도힘들지",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100013694.1.13",
          "form": "눈은 좋은데 말이야",
          "original_form": "눈은 좋은데 말이야",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100013694.1.14",
          "form": "여전히 비행기타는건 좋아하니",
          "original_form": "여전히 비행기타는건 좋아하니",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100013694.1.15",
          "form": "예전만큼은아냐",
          "original_form": "예전만큼은아냐",
