      "utterance": [
        {
          "id": "MDRW2100009563.1.1",
          "form": "좋아요 우리 심즈에 대해 말해봐요",
          "original_form": "좋아요 우리 심즈에 대해 말해봐요",
          "speaker_id": "1",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009563.1.2",
          "form": "심즈의 어떤점이 매력인가요",
          "original_form": "심즈의 어떤점이 매력인가요",
          "speaker_id": "1",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009563.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.4",
          "form": "주제 전환이 빠르네요",
          "original_form": "주제 전환이 빠르네요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.5",
          "form": "마음에 들어요",
          "original_form": "마음에 들어요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.7",
          "form": "우선 경쟁이 없어요",
          "original_form": "우선 경쟁이 없어요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.8",
          "form": "경쟁없는 사회",
          "original_form": "경쟁없는 사회",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.9",
          "form": "그래서 평화로워요",
          "original_form": "그래서 평화로워요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009563.1.10",
          "form": "그것이 바로 Perfect",
          "original_form": "그것이 바로 Perfect",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.12",
          "form": "경쟁 싫어요ㅜㅜ",
          "original_form": "경쟁 싫어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.13",
          "form": "근데 너무 경쟁이없으면",
          "original_form": "근데 너무 경쟁이없으면",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.14",
          "form": "고등학교도 그래서 싫었어요",
          "original_form": "고등학교도 그래서 싫었어요",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.15",
          "form": "지루하지는 않나요",
          "original_form": "지루하지는 않나요",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.16",
          "form": "맞아 고등학교는 좀 과해요",
          "original_form": "맞아 고등학교는 좀 과해요",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.17",
          "form": "미친듯한 경쟁 심즈를 왜 좋아하는지 알것같아요",
          "original_form": "미친듯한 경쟁 심즈를 왜 좋아하는지 알것같아요",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009563.1.18",
          "form": "지치셨군요",
          "original_form": "지치셨군요",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009563.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009563.1.20",
          "form": "경쟁 좋아하시나요?",
          "original_form": "경쟁 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009563.1.21",
          "form": "심즈는 그리고 음",
          "original_form": "심즈는 그리고 음",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009563.1.22",
          "form": "애들이 귀엽고요",
          "original_form": "애들이 귀엽고요",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009563.1.23",
          "form": "아니요 저는 경쟁을 싫어합니다",
          "original_form": "아니요 저는 경쟁을 싫어합니다",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009563.1.24",
          "form": "막 살 수 있어요",
          "original_form": "막 살 수 있어요",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009563.1.25",
          "form": "학교를 안 가도",
          "original_form": "학교를 안 가도",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.26",
          "form": "물론 교장한테 전화 오지만",
          "original_form": "물론 교장한테 전화 오지만",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.27",
          "form": "버스도 사람많으면 안타고 20분을 앉아있어요 버스 보내느라고ㅋㅋ",
          "original_form": "버스도 사람많으면 안타고 20분을 앉아있어요 버스 보내느라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.28",
          "form": "인생이 망하지는 않더라고요",
          "original_form": "인생이 망하지는 않더라고요",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.29",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.30",
          "form": "거기도 교장이 있어요?",
          "original_form": "거기도 교장이 있어요?",
          "speaker_id": "1",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.31",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.32",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.34",
          "form": "평화가 좋아요",
          "original_form": "평화가 좋아요",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.35",
          "form": "생각보다 체계적이네",
          "original_form": "생각보다 체계적이네",
          "speaker_id": "1",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.36",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.37",
          "form": "싸워서 뭐해  좋은게 좋은거라고",
          "original_form": "싸워서 뭐해  좋은게 좋은거라고",
          "speaker_id": "1",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009563.1.38",
          "form": "와 마인드가 비슷한데요",
          "original_form": "와 마인드가 비슷한데요",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009563.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009563.1.40",
