        {
          "id": "MDRW2100021512.1.3",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021512.1.4",
          "form": "ㅎㅎ 열심히 해야죠",
          "original_form": "ㅎㅎ 열심히 해야죠",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021512.1.5",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021512.1.6",
          "form": "파이팅해서",
          "original_form": "파이팅해서",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100021512.1.7",
          "form": "얼른 살빼야죵",
          "original_form": "얼른 살빼야죵",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100021512.1.8",
          "form": "저도 홈트요",
          "original_form": "저도 홈트요",
          "speaker_id": "1",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021512.1.9",
          "form": "홈트 서로 파이팅해봐요^^",
          "original_form": "홈트 서로 파이팅해봐요^^",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021512.1.10",
          "form": "요가도 좋으니.해보시고요!",
          "original_form": "요가도 좋으니.해보시고요!",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021512.1.11",
          "form": "몸매관리 짱이네여",
          "original_form": "몸매관리 짱이네여",
          "speaker_id": "1",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021512.1.12",
          "form": "아직은 부족하죠 ㅠㅠ",
          "original_form": "아직은 부족하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021512.1.13",
          "form": "얼마나 하셨어요?",
          "original_form": "얼마나 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021512.1.14",
          "form": "오늘 한 2시간 가량한거같아요",
          "original_form": "오늘 한 2시간 가량한거같아요",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021512.1.15",
          "form": "대단한것 같아요",
          "original_form": "대단한것 같아요",
