      "utterance": [
        {
          "id": "MDRW2100019466.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019466.1.2",
          "form": "지금 연애중이세요?",
          "original_form": "지금 연애중이세요?",
          "speaker_id": "1",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019466.1.3",
          "form": "어머 아니요 ㅎㅎ 연애중이신가요",
          "original_form": "어머 아니요 ㅎㅎ 연애중이신가요",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019466.1.4",
          "form": "전 작년에 헤어졌어요......",
          "original_form": "전 작년에 헤어졌어요......",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019466.1.5",
          "form": "썸 있으신가요?",
          "original_form": "썸 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019466.1.6",
          "form": "ㅎㅎ 저도 작년에 헤어졌는데",
          "original_form": "ㅎㅎ 저도 작년에 헤어졌는데",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019466.1.7",
          "form": "소개팅 몇번하긴 했는데 지금은 또 안하고있어요",
          "original_form": "소개팅 몇번하긴 했는데 지금은 또 안하고있어요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019466.1.8",
          "form": "코로나라서 새로운 사람 만날 기회가 많이 없는거같아요 ㅎㅎ",
          "original_form": "코로나라서 새로운 사람 만날 기회가 많이 없는거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019466.1.9",
          "form": "그쵸 ㅜㅜ",
          "original_form": "그쵸 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019466.1.10",
          "form": "코시국에 데이트를 어떻게 해야될 지도 모르겠더라구요",
          "original_form": "코시국에 데이트를 어떻게 해야될 지도 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019466.1.11",
          "form": "저는 조심하고 싶은데 상대방은 만나고 싶어해서 힘들었어요",
          "original_form": "저는 조심하고 싶은데 상대방은 만나고 싶어해서 힘들었어요",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019466.1.12",
          "form": "맞아요 ㅎㅎ 데이트를 해도 다 거기서거기같고 그렇다고 사람많은데 가기도 부담스럽구요",
          "original_form": "맞아요 ㅎㅎ 데이트를 해도 다 거기서거기같고 그렇다고 사람많은데 가기도 부담스럽구요",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019466.1.13",
          "form": "전 예전에 모임에서 만났었는데 요즘은 오프라인 모임도 거의 못하니까요 ㅎㅎ",
          "original_form": "전 예전에 모임에서 만났었는데 요즘은 오프라인 모임도 거의 못하니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019466.1.14",
          "form": "맞죠 ㅎㅎ 그럼 요즘 연애하고 싶다는 생각 아직 드세요?",
          "original_form": "맞죠 ㅎㅎ 그럼 요즘 연애하고 싶다는 생각 아직 드세요?",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019466.1.15",
          "form": "또 혼자가 좀 길어지니까...혼자도 편하네요?ㅋㅋㅋㅋ",
          "original_form": "또 혼자가 좀 길어지니까...혼자도 편하네요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019466.1.16",
          "form": "제가 원래 집순이고 연락을 좀 귀찮아하는편이라 많이 싸웠었는데 ㅎㅎ 그래서 더 그런가봐오",
          "original_form": "제가 원래 집순이고 연락을 좀 귀찮아하는편이라 많이 싸웠었는데 ㅎㅎ 그래서 더 그런가봐오",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019466.1.17",
          "form": "ㅋㅋㅋ 저랑 비슷하시네요",
          "original_form": "ㅋㅋㅋ 저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019466.1.18",
          "form": "혼자 있으면 편한데 요즘 같이 친구들도 못만날 땐 외로움도 느끼고 그러네요 ㅜㅜ",
          "original_form": "혼자 있으면 편한데 요즘 같이 친구들도 못만날 땐 외로움도 느끼고 그러네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019466.1.19",
          "form": "맞아요 두개가 번갈아가면서 오더라구요 ㅋㅋㅋ",
          "original_form": "맞아요 두개가 번갈아가면서 오더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019466.1.20",
          "form": "이래놓고 연애하면 또 연애하겠지만 ㅋㅋ",
          "original_form": "이래놓고 연애하면 또 연애하겠지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019466.1.21",
          "form": "전 이래놓고 연애하면 다시 혼자가 그리울 것 같아요..",
          "original_form": "전 이래놓고 연애하면 다시 혼자가 그리울 것 같아요..",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019466.1.22",
          "form": "이것은 뫼비우스의 띠..",
          "original_form": "이것은 뫼비우스의 띠..",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019466.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋ 정확하십니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 ㅋㅋㅋ 정확하십니다",
          "speaker_id": "2",
