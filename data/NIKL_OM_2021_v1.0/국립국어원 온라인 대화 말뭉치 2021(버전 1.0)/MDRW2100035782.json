        {
          "id": "MDRW2100035782.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.2",
          "form": "여름이면 머리가 난리야",
          "original_form": "여름이면 머리가 난리야",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.3",
          "form": "부스스하지",
          "original_form": "부스스하지",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.4",
          "form": "난그게 너무 싫어",
          "original_form": "난그게 너무 싫어",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.5",
          "form": "나도 잔머리가 심한편이야",
          "original_form": "나도 잔머리가 심한편이야",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.6",
          "form": "어떻게 해결해",
          "original_form": "어떻게 해결해",
          "speaker_id": "1",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.7",
          "form": "매직을 하면 좀 나아",
          "original_form": "매직을 하면 좀 나아",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100035782.1.8",
          "form": "그거 비싸던데",
          "original_form": "그거 비싸던데",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.9",
          "form": "미용에 그정도 투자는 해야지",
          "original_form": "미용에 그정도 투자는 해야지",
          "speaker_id": "2",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.10",
          "form": "하면 얼마나 가?",
          "original_form": "하면 얼마나 가?",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.11",
          "form": "한 2달은 가는것같아",
          "original_form": "한 2달은 가는것같아",
          "speaker_id": "2",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.12",
          "form": "파마도 같이해?",
          "original_form": "파마도 같이해?",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.13",
          "form": "난 s컬 파마를 곁들였어",
          "original_form": "난 s컬 파마를 곁들였어",
          "speaker_id": "2",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.14",
          "form": "난 c컬하고싶어",
          "original_form": "난 c컬하고싶어",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100035782.1.15",
          "form": "그것도 될걸",
          "original_form": "그것도 될걸",
          "speaker_id": "2",
