      "utterance": [
        {
          "id": "MDRW2100015699.1.1",
          "form": "세상에서 없어선 안될 직업이 있다면 그건 뭘까요?",
          "original_form": "세상에서 없어선 안될 직업이 있다면 그건 뭘까요?",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015699.1.2",
          "form": "흠 글쎄요",
          "original_form": "흠 글쎄요",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015699.1.3",
          "form": "의사???",
          "original_form": "의사???",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015699.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015699.1.5",
          "form": "저는 교육자(교수, 교사 등)라고 생각합니다.",
          "original_form": "저는 교육자(교수, 교사 등)라고 생각합니다.",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015699.1.6",
          "form": "교육자도 맞죠",
          "original_form": "교육자도 맞죠",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015699.1.7",
          "form": "옛날부터 계속 존재해온 직업 ㅋㅋ",
          "original_form": "옛날부터 계속 존재해온 직업 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015699.1.8",
          "form": "계속 계승이 되니깐요..",
          "original_form": "계속 계승이 되니깐요..",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.9",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.10",
          "form": "요즘 같은 때엔 의사가 진짜 중요한 것 같아요",
          "original_form": "요즘 같은 때엔 의사가 진짜 중요한 것 같아요",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.11",
          "form": "name2 교육자라고 했죠?",
          "original_form": "&name2& 교육자라고 했죠?",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.12",
          "form": "의료진들 고생이 너무 많으셔서..",
          "original_form": "의료진들 고생이 너무 많으셔서..",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.13",
          "form": "성취감이 있나요?",
          "original_form": "성취감이 있나요?",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.14",
          "form": "어떤 성취감이요?",
          "original_form": "어떤 성취감이요?",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.15",
          "form": "가르치면서?!?!",
          "original_form": "가르치면서?!?!",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.16",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.17",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015699.1.19",
          "form": "어머 나쁜말 안썼는데 안보내지네",
          "original_form": "어머 나쁜말 안썼는데 안보내지네",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015699.1.20",
          "form": "얘는 정말 답이 없구나 싶었는데 그래도 개과천선 하는 거 보면 보람이 있죠",
          "original_form": "얘는 정말 답이 없구나 싶었는데 그래도 개과천선 하는 거 보면 보람이 있죠",
          "speaker_id": "2",
          "time": "20210826 00:26"
        },
        {
          "id": "MDRW2100015699.1.21",
          "form": "오 오 그런거진짜 뿌듯할거같아요",
          "original_form": "오 오 그런거진짜 뿌듯할거같아요",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015699.1.22",
          "form": "그치만 욕 먹는 직업인걸요",
          "original_form": "그치만 욕 먹는 직업인걸요",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015699.1.23",
          "form": "에이 누구한테요",
          "original_form": "에이 누구한테요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015699.1.24",
          "form": "사람들 인식이 그러잖아요 ㅜㅜ",
          "original_form": "사람들 인식이 그러잖아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015699.1.25",
          "form": "안그래요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "안그래요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015699.1.26",
          "form": "뭔가 사회적 지위는 높지만 그에 반해 인식은..",
          "original_form": "뭔가 사회적 지위는 높지만 그에 반해 인식은..",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015699.1.27",
          "form": "에이 사회적 지위가 인식이죠 ㅋㅋ",
          "original_form": "에이 사회적 지위가 인식이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015699.1.28",
          "form": "그런데에서 스트레스가 오는 것 같아요 ㅜㅜ",
          "original_form": "그런데에서 스트레스가 오는 것 같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015699.1.29",
          "form": "아 그렇군요 ㅠㅠㅠ",
          "original_form": "아 그렇군요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015699.1.30",
          "form": "그래도 워라밸 있어서 행보해요",
          "original_form": "그래도 워라밸 있어서 행보해요",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015699.1.31",
          "form": "행복해요",
          "original_form": "행복해요",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015699.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 워라벨 젤  중요하죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 워라벨 젤  중요하죠",
