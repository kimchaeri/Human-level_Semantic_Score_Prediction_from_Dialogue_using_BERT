      "utterance": [
        {
          "id": "MDRW2100041213.1.1",
          "form": "부모님도 자유여행 좋아하시고",
          "original_form": "부모님도 자유여행 좋아하시고",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041213.1.2",
          "form": "계획은 그럼 자매가 함께 짜신거죠??",
          "original_form": "계획은 그럼 자매가 함께 짜신거죠??",
          "speaker_id": "2",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041213.1.3",
          "form": "먹거나 다니는 것 좋아하셔서",
          "original_form": "먹거나 다니는 것 좋아하셔서",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041213.1.4",
          "form": "계획은 같이 짰고",
          "original_form": "계획은 같이 짰고",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041213.1.5",
          "form": "숙소 예약 같은 건 아버지가 하셨어요ㅎ",
          "original_form": "숙소 예약 같은 건 아버지가 하셨어요ㅎ",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041213.1.6",
          "form": "항공편도 아버지가 알아보시고",
          "original_form": "항공편도 아버지가 알아보시고",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041213.1.7",
          "form": "저희 부모님은 여행계하셔서 다 패키지로ㅋㅋ 두분이 다녀오셨어요",
          "original_form": "저희 부모님은 여행계하셔서 다 패키지로ㅋㅋ 두분이 다녀오셨어요",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041213.1.8",
          "form": "와!!! 최고!!!",
          "original_form": "와!!! 최고!!!",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041213.1.9",
          "form": "현지인처럼 지내는 것 좋아하셔서",
          "original_form": "현지인처럼 지내는 것 좋아하셔서",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041213.1.10",
          "form": "패키지는 오히려 싫으시다고ㅎㅎ",
          "original_form": "패키지는 오히려 싫으시다고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041213.1.11",
          "form": "한번 자유가시면 패키지 못간대요",
          "original_form": "한번 자유가시면 패키지 못간대요",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100041213.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100041213.1.13",
          "form": "근데 부모님과 자유여행 가려면 부모님이 잘 협조하시고 좀 진취적?이셔야 되는 듯해요",
          "original_form": "근데 부모님과 자유여행 가려면 부모님이 잘 협조하시고 좀 진취적?이셔야 되는 듯해요",
          "speaker_id": "1",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100041213.1.14",
          "form": "저는 욕심내지않아요 부모님 연세도 많으셔서ㅎㅎ",
          "original_form": "저는 욕심내지않아요 부모님 연세도 많으셔서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100041213.1.15",
          "form": "국내나 한번 시도해볼까싶고요",
          "original_form": "국내나 한번 시도해볼까싶고요",
          "speaker_id": "2",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100041213.1.16",
          "form": "국내 정도는 할 만하죠",
          "original_form": "국내 정도는 할 만하죠",
          "speaker_id": "1",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100041213.1.17",
          "form": "제 친구는 어머니랑 국내여행 다녀오고 다신 둘이서 안간다 했지만...",
          "original_form": "제 친구는 어머니랑 국내여행 다녀오고 다신 둘이서 안간다 했지만...",
          "speaker_id": "1",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100041213.1.18",
          "form": "사실 엄마가 반대할 가능성이 커요ㅋㅋ",
          "original_form": "사실 엄마가 반대할 가능성이 커요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100041213.1.19",
          "form": "아 어머니는 여행스타일이 다르신가요?",
          "original_form": "아 어머니는 여행스타일이 다르신가요?",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100041213.1.20",
          "form": "그냥 음식취향이라 그런 부분이 굉장히 보수적이세요 안먹어본 음식 절대 안드시는ㅋㅋ",
          "original_form": "그냥 음식취향이라 그런 부분이 굉장히 보수적이세요 안먹어본 음식 절대 안드시는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100041213.1.21",
          "form": "아 그럼 여행 계획 세우기 까다롭죠",
          "original_form": "아 그럼 여행 계획 세우기 까다롭죠",
          "speaker_id": "1",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100041213.1.22",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:34"
        }
      ]
    }
