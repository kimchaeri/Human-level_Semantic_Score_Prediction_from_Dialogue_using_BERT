      "utterance": [
        {
          "id": "MDRW2100011492.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011492.1.2",
          "form": "안녕하세요 대학생이신가요?",
          "original_form": "안녕하세요 대학생이신가요?",
          "speaker_id": "2",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011492.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011492.1.4",
          "form": "동아리해보셨나요?",
          "original_form": "동아리해보셨나요?",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011492.1.5",
          "form": "네 과학동아리햌ㅅ어요!",
          "original_form": "네 과학동아리햌ㅅ어요!",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011492.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011492.1.7",
          "form": "재밌었나요?",
          "original_form": "재밌었나요?",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011492.1.8",
          "form": "네 저는 재밌었어요 name1님은 동아리해보셨나요",
          "original_form": "네 저는 재밌었어요 &name1&님은 동아리해보셨나요",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011492.1.9",
          "form": "아니요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100011492.1.10",
          "form": "과학동아리에선",
          "original_form": "과학동아리에선",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100011492.1.11",
          "form": "뭘하나요?",
          "original_form": "뭘하나요?",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100011492.1.12",
          "form": "실험을합니다 !!",
          "original_form": "실험을합니다 !!",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011492.1.13",
          "form": "해보고싶은 동아리있으신가요?",
          "original_form": "해보고싶은 동아리있으신가요?",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011492.1.14",
          "form": "주로 무슨실험인가요?",
          "original_form": "주로 무슨실험인가요?",
          "speaker_id": "1",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100011492.1.15",
          "form": "전 등산이요",
          "original_form": "전 등산이요",
          "speaker_id": "1",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100011492.1.16",
          "form": "여러 실험을 했어요 건전지만들기도 하구요",
          "original_form": "여러 실험을 했어요 건전지만들기도 하구요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100011492.1.17",
          "form": "등산 재밌겠어요",
          "original_form": "등산 재밌겠어요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100011492.1.18",
          "form": "ㅋㅋㅋ 네네",
          "original_form": "ㅋㅋㅋ 네네",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100011492.1.19",
          "form": "코로나가 잠잠해진다면요",
          "original_form": "코로나가 잠잠해진다면요",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100011492.1.20",
          "form": "학업스트레스있으신가요?",
          "original_form": "학업스트레스있으신가요?",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100011492.1.21",
          "form": "학생이 아닙니다!",
          "original_form": "학생이 아닙니다!",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100011492.1.22",
          "form": "아하 그럼 전에 있으셨나요?",
          "original_form": "아하 그럼 전에 있으셨나요?",
          "speaker_id": "2",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100011492.1.23",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100011492.1.24",
          "form": "그닥 공부를안했죠",
          "original_form": "그닥 공부를안했죠",
