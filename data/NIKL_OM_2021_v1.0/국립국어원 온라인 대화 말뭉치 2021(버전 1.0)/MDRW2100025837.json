      "utterance": [
        {
          "id": "MDRW2100025837.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025837.1.2",
          "form": "처음 뵙겠습니다",
          "original_form": "처음 뵙겠습니다",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025837.1.3",
          "form": "오늘 날씨 어떠세요",
          "original_form": "오늘 날씨 어떠세요",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100025837.1.4",
          "form": "이상형이 어떻게 되나요?",
          "original_form": "이상형이 어떻게 되나요?",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025837.1.5",
          "form": "저는 이상형이 없어요 연애에도 관심이 없어요",
          "original_form": "저는 이상형이 없어요 연애에도 관심이 없어요",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025837.1.6",
          "form": "괜찮아요 요즘은 욜로족도 많아요",
          "original_form": "괜찮아요 요즘은 욜로족도 많아요",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025837.1.7",
          "form": "name1 언니는 어떤가요?",
          "original_form": "&name1& 언니는 어떤가요?",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025837.1.8",
          "form": "언니인지는 어떻게아세요",
          "original_form": "언니인지는 어떻게아세요",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025837.1.9",
          "form": "미팅은해본적 있나요",
          "original_form": "미팅은해본적 있나요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025837.1.10",
          "form": "전 소개팅할거에요",
          "original_form": "전 소개팅할거에요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025837.1.11",
          "form": "데이트 하러 이번주 나가고 싶어요",
          "original_form": "데이트 하러 이번주 나가고 싶어요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025837.1.12",
          "form": "제 결혼관과 맞는사람 만날래요",
          "original_form": "제 결혼관과 맞는사람 만날래요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025837.1.13",
          "form": "없어요 대학 다닐 때도 과 자체가 미팅을 하는 분위기가 아니었네요",
          "original_form": "없어요 대학 다닐 때도 과 자체가 미팅을 하는 분위기가 아니었네요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100025837.1.14",
          "form": "결혼관이 어떤가요?",
          "original_form": "결혼관이 어떤가요?",
          "speaker_id": "2",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025837.1.15",
          "form": "아..엄격한 대학이었군요 미팅도못할만큼",
          "original_form": "아..엄격한 대학이었군요 미팅도못할만큼",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025837.1.16",
          "form": "놀때놀고 쓸때 쓰는 사람이요",
          "original_form": "놀때놀고 쓸때 쓰는 사람이요",
          "speaker_id": "1",
          "time": "20210901 17:22"
        },
        {
          "id": "MDRW2100025837.1.17",
          "form": "씀씀이가 인색하지 않는 사람을 선호하시나봐요",
          "original_form": "씀씀이가 인색하지 않는 사람을 선호하시나봐요",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025837.1.18",
          "form": "본인도 손이 크신 편인가요?",
          "original_form": "본인도 손이 크신 편인가요?",
          "speaker_id": "2",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025837.1.19",
          "form": "아뇨 전 정말작은편이에요 정말 즐거웠어요 좋은대화 감사합니다:)",
          "original_form": "아뇨 전 정말작은편이에요 정말 즐거웠어요 좋은대화 감사합니다:)",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100025837.1.20",
          "form": "아 얘기를 더나눠봐요",
          "original_form": "아 얘기를 더나눠봐요",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025837.1.21",
          "form": "연애에 있어서는 어떤게 중요하다고 생각하나요",
          "original_form": "연애에 있어서는 어떤게 중요하다고 생각하나요",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025837.1.22",
          "form": "지금 연애를 하고 계세요?",
          "original_form": "지금 연애를 하고 계세요?",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025837.1.23",
          "form": "취향이 맞는 게 중요하지 않을까 해요 한 사람은 실외파인데 한 사람은 실내파면",
          "original_form": "취향이 맞는 게 중요하지 않을까 해요 한 사람은 실외파인데 한 사람은 실내파면",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025837.1.24",
          "form": "네 하고는 있답니다",
          "original_form": "네 하고는 있답니다",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025837.1.25",
          "form": "예정된 파국이 기다리겠죠",
          "original_form": "예정된 파국이 기다리겠죠",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025837.1.26",
          "form": "맞아요 연애관과",
          "original_form": "맞아요 연애관과",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.27",
          "form": "서로의취향도 진짜 중요해요",
          "original_form": "서로의취향도 진짜 중요해요",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.28",
          "form": "소개팅하고싶은 생각 있으신가요",
          "original_form": "소개팅하고싶은 생각 있으신가요",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.29",
          "form": "지금 연인과는 취향이 맞나요?",
          "original_form": "지금 연인과는 취향이 맞나요?",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.30",
          "form": "아니요 저는 욜로족을 할 거예요",
          "original_form": "아니요 저는 욜로족을 할 거예요",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.31",
          "form": "음 일정부분만요",
          "original_form": "음 일정부분만요",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.32",
          "form": "노는부분은 좀 맞아요",
          "original_form": "노는부분은 좀 맞아요",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025837.1.33",
          "form": "저는 혼자 있는 취향이라 같은 취향이랑 만나면 너는 네 집에서 나는 내집에서",
          "original_form": "저는 혼자 있는 취향이라 같은 취향이랑 만나면 너는 네 집에서 나는 내집에서",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025837.1.34",
          "form": "욜로 나중에는 외롭지 않겠어요?",
          "original_form": "욜로 나중에는 외롭지 않겠어요?",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025837.1.35",
          "form": "따로 지내겠네요",
          "original_form": "따로 지내겠네요",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025837.1.36",
          "form": "타지살이 할 때도 모두가 외롭다고 할때 혼자서 잘 지냈어요",
          "original_form": "타지살이 할 때도 모두가 외롭다고 할때 혼자서 잘 지냈어요",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025837.1.37",
          "form": "아 집순이시군요",
          "original_form": "아 집순이시군요",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025837.1.38",
          "form": "응원할게요 당신의 미래를",
          "original_form": "응원할게요 당신의 미래를",
          "speaker_id": "1",
          "time": "20210901 17:28"
        }
      ]
    }
