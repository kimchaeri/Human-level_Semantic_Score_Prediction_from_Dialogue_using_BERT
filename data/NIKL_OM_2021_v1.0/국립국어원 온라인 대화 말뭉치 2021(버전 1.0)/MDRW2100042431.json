      "utterance": [
        {
          "id": "MDRW2100042431.1.1",
          "form": "반려동물 있으신가요??",
          "original_form": "반려동물 있으신가요??",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042431.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042431.1.3",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042431.1.4",
          "form": "저도 따로 없어요 ㅠ",
          "original_form": "저도 따로 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042431.1.5",
          "form": "동물은 좋아하시나요??",
          "original_form": "동물은 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042431.1.6",
          "form": "네 동물은 좋아해요",
          "original_form": "네 동물은 좋아해요",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042431.1.7",
          "form": "랜선으로 반려동물 보는 것도 좋아하고",
          "original_form": "랜선으로 반려동물 보는 것도 좋아하고",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042431.1.8",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042431.1.9",
          "form": "고양이랑 강아지 보면 그렇게 힐링이 되는지 몰라요",
          "original_form": "고양이랑 강아지 보면 그렇게 힐링이 되는지 몰라요",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042431.1.10",
          "form": "네 그래서 요즘 랜선집사분들 많더라고요",
          "original_form": "네 그래서 요즘 랜선집사분들 많더라고요",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042431.1.11",
          "form": "귀여운 동물 영상을 보면 실제로 스트레스가 풀린데요  ㅋㅋㅋ",
          "original_form": "귀여운 동물 영상을 보면 실제로 스트레스가 풀린데요  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042431.1.12",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042431.1.13",
          "form": "정말 그런 것 같아요",
          "original_form": "정말 그런 것 같아요",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042431.1.14",
          "form": "나중에라도 함께하고 싶은 동물이 있으신가요?",
          "original_form": "나중에라도 함께하고 싶은 동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042431.1.15",
          "form": "음 구체적으로 생각해보진 않았는데",
          "original_form": "음 구체적으로 생각해보진 않았는데",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042431.1.16",
          "form": "나중엔 반려동물 한마리 있음 외롭지 않고 좋겠다 싶어요",
          "original_form": "나중엔 반려동물 한마리 있음 외롭지 않고 좋겠다 싶어요",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042431.1.17",
          "form": "개를 좋아하시나요 고양이를 좋아하시나요?",
          "original_form": "개를 좋아하시나요 고양이를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042431.1.18",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042431.1.19",
          "form": "저는 막 와서 앵기는 게 좋아서",
          "original_form": "저는 막 와서 앵기는 게 좋아서",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042431.1.20",
          "form": "강아지너무 사랑스러워요",
          "original_form": "강아지{emoji:♡}너무 사랑스러워요",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042431.1.21",
          "form": "네 사랑스럽죠 강아지",
          "original_form": "네 사랑스럽죠 강아지",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042431.1.22",
          "form": "인절미들 보면 진짜 심장에 해로운 것 같아요",
          "original_form": "인절미들 보면 진짜 심장에 해로운 것 같아요",
          "speaker_id": "1",
          "time": "20210906 22:29"
        }
