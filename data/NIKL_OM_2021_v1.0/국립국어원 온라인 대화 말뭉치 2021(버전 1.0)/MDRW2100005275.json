      "utterance": [
        {
          "id": "MDRW2100005275.1.1",
          "form": "싸이월드 많이 하셨나요?",
          "original_form": "싸이월드 많이 하셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005275.1.2",
          "form": "싸이월드 어릴 적에 좀 했었지...",
          "original_form": "싸이월드 어릴 적에 좀 했었지...",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005275.1.3",
          "form": "예전에 사진도 많이 올리고 일촌탐방? 그런것도 자주 했었는데",
          "original_form": "예전에 사진도 많이 올리고 일촌탐방? 그런것도 자주 했었는데",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005275.1.4",
          "form": "도토리로 bgm도 사고",
          "original_form": "도토리로 bgm도 사고",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005275.1.5",
          "form": "선물받고 ㅋㅋㅋㅋ",
          "original_form": "선물받고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005275.1.6",
          "form": "그 뭐지 싸이월드 대표가 그간 임금체불한 것 땜에 기사 뜬 것도 봤어...",
          "original_form": "그 뭐지 싸이월드 대표가 그간 임금체불한 것 땜에 기사 뜬 것도 봤어...",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005275.1.7",
          "form": "기사 보니까 성격 좋아보이시더라",
          "original_form": "기사 보니까 성격 좋아보이시더라",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005275.1.8",
          "form": "임금체불 관련해서 빠르게 인정하셨더라고",
          "original_form": "임금체불 관련해서 빠르게 인정하셨더라고",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005275.1.9",
          "form": "요즘 부활한다고 그때 유행했던 브금도 다시 리메이크로 나오고 있어요!",
          "original_form": "요즘 부활한다고 그때 유행했던 브금도 다시 리메이크로 나오고 있어요!",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005275.1.10",
          "form": "부활하는 건 이번에 첨들었네",
          "original_form": "부활하는 건 이번에 첨들었네",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005275.1.11",
          "form": "브금이 리메이크로 나오는건 뭐지?",
          "original_form": "브금이 리메이크로 나오는건 뭐지?",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005275.1.12",
          "form": "혹시 이번에 헤이즈가 비가오는날엔 리메이크한게 싸이월드 때문인가??",
          "original_form": "혹시 이번에 헤이즈가 비가오는날엔 리메이크한게 싸이월드 때문인가??",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005275.1.13",
          "form": "그 노래는 아니고 다른 노래들 있어요 소유도 부르고 정승환도 부르고~ 유튜브 싸이월드 계정도 생겼더라고요",
          "original_form": "그 노래는 아니고 다른 노래들 있어요 소유도 부르고 정승환도 부르고~ 유튜브 싸이월드 계정도 생겼더라고요",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005275.1.14",
          "form": "헐 유튜브 계정까지??",
          "original_form": "헐 유튜브 계정까지??",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005275.1.15",
          "form": "유튜브 계정엔 어떤 것들이 올라오는데...?",
          "original_form": "유튜브 계정엔 어떤 것들이 올라오는데...?",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005275.1.16",
          "form": "유튜브 할만한 것들이있나?",
          "original_form": "유튜브 할만한 것들이있나?",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005275.1.17",
          "form": "노래는 찾아봐야겠다~",
          "original_form": "노래는 찾아봐야겠다~",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005275.1.18",
          "form": "리메이크 된 노래가 어떤건데?",
          "original_form": "리메이크 된 노래가 어떤건데?",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005275.1.19",
          "form": "고칠게랑 와이 애인있어요 같은 옛날 노래들요",
          "original_form": "고칠게랑 와이 애인있어요 같은 옛날 노래들요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005275.1.20",
          "form": "고칠게가 특히 좋아요",
          "original_form": "고칠게가 특히 좋아요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005275.1.21",
          "form": "한번 들어보세요",
          "original_form": "한번 들어보세요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005275.1.22",
          "form": "아니 근데 와이라면 프리스타일 와이??",
          "original_form": "아니 근데 와이라면 프리스타일 와이??",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005275.1.23",
          "form": "리메이크가 됐다고???",
          "original_form": "리메이크가 됐다고???",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005275.1.24",
          "form": "들으러가야겠네",
          "original_form": "들으러가야겠네",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005275.1.25",
          "form": "네 맞아요 소유가 불렀어요",
          "original_form": "네 맞아요 소유가 불렀어요",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005275.1.26",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005275.1.27",
          "form": "헐 소유가 불렀구나 들어봐야겠다",
          "original_form": "헐 소유가 불렀구나 들어봐야겠다",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005275.1.28",
          "form": "싸이월드 새로 생기면서",
          "original_form": "싸이월드 새로 생기면서",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005275.1.29",
          "form": "바뀌는 점은 없어?",
          "original_form": "바뀌는 점은 없어?",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005275.1.30",
          "form": "싸이월드 없어졌을 때 사라진 그런 자료들도",
          "original_form": "싸이월드 없어졌을 때 사라진 그런 자료들도",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005275.1.31",
          "form": "다시 백업가능하게 해주면 좋겠다는 생각이 드네",
          "original_form": "다시 백업가능하게 해주면 좋겠다는 생각이 드네",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005275.1.32",
          "form": "오 검색해보니까 싸이월드 부활하면서 비지엠은 벅스가 담당하네",
          "original_form": "오 검색해보니까 싸이월드 부활하면서 비지엠은 벅스가 담당하네",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005275.1.33",
          "form": "이것도 뭔가 메타버스랑 관련있나",
          "original_form": "이것도 뭔가 메타버스랑 관련있나",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005275.1.34",
          "form": "저는 그때 흑역사를 많이 남겨둬서... 부활하는 거 좀 민망한데 그래도 하면 좋겠죠",
          "original_form": "저는 그때 흑역사를 많이 남겨둬서... 부활하는 거 좀 민망한데 그래도 하면 좋겠죠",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100005275.1.35",
          "form": "피식대학 메타버스가 유행하면서 다시 돌아온 영향도 없진 않을 것 같네요",
          "original_form": "피식대학 메타버스가 유행하면서 다시 돌아온 영향도 없진 않을 것 같네요",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100005275.1.36",
          "form": "피식대학때부터 뭔가 복고열풍 불면서",
          "original_form": "피식대학때부터 뭔가 복고열풍 불면서",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100005275.1.37",
          "form": "그런 영향이 확실히 있는 것 같기는 해요",
          "original_form": "그런 영향이 확실히 있는 것 같기는 해요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100005275.1.38",
          "form": "놀면뭐하니에서도 엠에스지 워너비가 흥했잖아요",
          "original_form": "놀면뭐하니에서도 엠에스지 워너비가 흥했잖아요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100005275.1.39",
          "form": "근데 솔직히 싸이월드라는 이미지가 갖는... 그런 느낌이 있어서 부활한다고 해서 흥할지 어쩔진 잘 모르겠어요",
          "original_form": "근데 솔직히 싸이월드라는 이미지가 갖는... 그런 느낌이 있어서 부활한다고 해서 흥할지 어쩔진 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100005275.1.40",
          "form": "그니까요 백업용 홈페이지 같은 느낌이라고 해야하나? 요즘 트렌드와는 맞지 않을 것 같네요",
          "original_form": "그니까요 백업용 홈페이지 같은 느낌이라고 해야하나? 요즘 트렌드와는 맞지 않을 것 같네요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100005275.1.41",
          "form": "그쵸... 블로그나 이런 사이트들도 사실 젊은 사람들은 잘 안쓰는 추세인데...",
          "original_form": "그쵸... 블로그나 이런 사이트들도 사실 젊은 사람들은 잘 안쓰는 추세인데...",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100005275.1.42",
          "form": "그래도 리메이크 된 곡들은 다 좋아서 마음에 드네요!",
          "original_form": "그래도 리메이크 된 곡들은 다 좋아서 마음에 드네요!",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100005275.1.43",
          "form": "리메이크 곡들은 꼭 들어봐야겠어요ㅋㅋㅋㅋ",
          "original_form": "리메이크 곡들은 꼭 들어봐야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100005275.1.44",
          "form": "그거 하나는 좋은 점인 것 같아요",
          "original_form": "그거 하나는 좋은 점인 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        }
      ]
    }
