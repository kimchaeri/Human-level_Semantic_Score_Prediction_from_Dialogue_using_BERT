      "utterance": [
        {
          "id": "MDRW2100025162.1.1",
          "form": "반려동물을 키워본적있니?",
          "original_form": "반려동물을 키워본적있니?",
          "speaker_id": "1",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100025162.1.2",
          "form": "나 어렸을 때 금붕어랑 고슴도치",
          "original_form": "나 어렸을 때 금붕어랑 고슴도치",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025162.1.3",
          "form": "이야 신기한거 키웠네",
          "original_form": "이야 신기한거 키웠네",
          "speaker_id": "1",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025162.1.4",
          "form": "고슴도치 이름은 뭐였니",
          "original_form": "고슴도치 이름은 뭐였니",
          "speaker_id": "1",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025162.1.5",
          "form": "이름이 없었어....",
          "original_form": "이름이 없었어....",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025162.1.6",
          "form": "남의 집에서 버리는 거 데려온 애라",
          "original_form": "남의 집에서 버리는 거 데려온 애라",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100025162.1.7",
          "form": "헐 너무 슬퍼",
          "original_form": "헐 너무 슬퍼",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025162.1.8",
          "form": "오래 못살았거든",
          "original_form": "오래 못살았거든",
          "speaker_id": "2",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025162.1.9",
          "form": "금붕어는 이름이 있니?",
          "original_form": "금붕어는 이름이 있니?",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025162.1.10",
          "form": "좀 성의없이 지어서 1호, 2호, 3호였어",
          "original_form": "좀 성의없이 지어서 1호, 2호, 3호였어",
          "speaker_id": "2",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025162.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025162.1.12",
          "form": "난 강아지 키웠어",
          "original_form": "난 강아지 키웠어",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.13",
          "form": "귀여웠겠다",
          "original_form": "귀여웠겠다",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.14",
          "form": "이름이 뭐였는데?",
          "original_form": "이름이 뭐였는데?",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.15",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.16",
          "form": "아주 엘레강스한 이름이구나",
          "original_form": "아주 엘레강스한 이름이구나",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.17",
          "form": "왕족출신이야",
          "original_form": "왕족출신이야",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.18",
          "form": "이름이 고급진데",
          "original_form": "이름이 고급진데",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.19",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.20",
          "form": "우리집 품격에 맞춰서 작명소에 데려가",
          "original_form": "우리집 품격에 맞춰서 작명소에 데려가",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.21",
          "form": "지은이름이야",
          "original_form": "지은이름이야",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.22",
          "form": "알아봐줘서 고마워",
          "original_form": "알아봐줘서 고마워",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025162.1.23",
          "form": "뭐야 진짜야 거짓말이야",
          "original_form": "뭐야 진짜야 거짓말이야",
          "speaker_id": "2",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025162.1.24",
          "form": "물론 거짓말이야",
          "original_form": "물론 거짓말이야",
          "speaker_id": "1",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025162.1.25",
          "form": "근데 내가 지었어",
          "original_form": "근데 내가 지었어",
          "speaker_id": "1",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025162.1.26",
          "form": "아니 뭐야",
          "original_form": "아니 뭐야",
          "speaker_id": "2",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025162.1.27",
          "form": "여자애였어",
          "original_form": "여자애였어",
          "speaker_id": "1",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025162.1.28",
          "form": "나 강아지에 환상이 있어서",
          "original_form": "나 강아지에 환상이 있어서",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.29",
          "form": "사람음식에 환장했지",
          "original_form": "사람음식에 환장했지",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.30",
          "form": "왕족출신이라 그런가보다",
          "original_form": "왕족출신이라 그런가보다",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.31",
          "form": "맞아 근데 내가 강아지용 소세지먹어봤는데",
          "original_form": "맞아 근데 내가 강아지용 소세지먹어봤는데",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.32",
          "form": "사람꺼랑 맛똑같던데",
          "original_form": "사람꺼랑 맛똑같던데",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.33",
          "form": "네 인권은 좀 챙겨라",
          "original_form": "네 인권은 좀 챙겨라",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.34",
          "form": "너도 나중에 한번 비교해봐",
          "original_form": "너도 나중에 한번 비교해봐",
          "speaker_id": "1",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.35",
          "form": "강아지 음식 뺏어먹지 말고",
          "original_form": "강아지 음식 뺏어먹지 말고",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.36",
          "form": "아니 안 그러고 싶어",
          "original_form": "아니 안 그러고 싶어",
          "speaker_id": "2",
          "time": "20210901 00:04"
        },
        {
          "id": "MDRW2100025162.1.37",
          "form": "울집 강아지가 서열 1위야",
          "original_form": "울집 강아지가 서열 1위야",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100025162.1.38",
          "form": "원래 다그런거겠지?",
          "original_form": "원래 다그런거겠지?",
          "speaker_id": "1",
          "time": "20210901 00:05"
        },
        {
          "id": "MDRW2100025162.1.39",
          "form": "동물 키우면 주인은 집사라잖아",
          "original_form": "동물 키우면 주인은 집사라잖아",
          "speaker_id": "2",
          "time": "20210901 00:05"
        }
      ]
    }
