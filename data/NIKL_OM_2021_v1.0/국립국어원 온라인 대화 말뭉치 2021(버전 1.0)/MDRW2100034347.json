      "utterance": [
        {
          "id": "MDRW2100034347.1.1",
          "form": "가을 좋아하세요?!!",
          "original_form": "가을 좋아하세요?!!",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.2",
          "form": "오늘 저희지역은 아직도 비오는데 name1님 \r\n네!!!!!!!!!",
          "original_form": "오늘 저희지역은 아직도 비오는데 &name1&님 \r\n네!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.3",
          "form": "엄청요!!!!!",
          "original_form": "엄청요!!!!!",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.4",
          "form": "오오 역시",
          "original_form": "오오 역시",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.5",
          "form": "지역도 비가 오나요!?!?!?",
          "original_form": "지역도 비가 오나요!?!?!?",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.7",
          "form": "아니욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.8",
          "form": "여긴 오늘 안왔어요ㅋㅋㅋㅋ",
          "original_form": "여긴 오늘 안왔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100034347.1.9",
          "form": "아 좋겠다 부러워요....",
          "original_form": "아 좋겠다 부러워요....",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100034347.1.10",
          "form": "ㅋㅋㅋㅋㅋ가을 너무 좋지 않나요!?!?!?!",
          "original_form": "ㅋㅋㅋㅋㅋ가을 너무 좋지 않나요!?!?!?!",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034347.1.11",
          "form": "최애 계절있으시죠?!?!",
          "original_form": "최애 계절있으시죠?!?!",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034347.1.12",
          "form": "네 저도 가을 젤 좋아해요!",
          "original_form": "네 저도 가을 젤 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034347.1.13",
          "form": "봄가을이 젤 조은데 선선한 가을이 더 좋아요 ㅋㅋㅋ옷도 이쁘구요",
          "original_form": "봄가을이 젤 조은데 선선한 가을이 더 좋아요 ㅋㅋㅋ옷도 이쁘구요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034347.1.14",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034347.1.15",
          "form": "가을 옷이 제일 예쁘죠!!!!!!",
          "original_form": "가을 옷이 제일 예쁘죠!!!!!!",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034347.1.16",
          "form": "그럼 가을 옷 구매하셨나요?",
          "original_form": "그럼 가을 옷 구매하셨나요?",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034347.1.17",
          "form": "저는 맨투맨이랑 트렌치 샀어요",
          "original_form": "저는 맨투맨이랑 트렌치 샀어요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034347.1.18",
          "form": "예쁜 옷 구매하셨군요,,,",
          "original_form": "예쁜 옷 구매하셨군요,,,",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034347.1.19",
          "form": "저는 후드집업을 샀답니다!",
          "original_form": "저는 후드집업을 샀답니다!",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034347.1.20",
          "form": "넵 ㅋㅋㅋㅋ",
          "original_form": "넵 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034347.1.21",
          "form": "장만하셨나여?",
          "original_form": "장만하셨나여?",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034347.1.22",
          "form": "네.. 후드집업을 장만했어요!ㅋㅋㅋㅋㅋㅋㅋㅋ 다른 옷도 사고싶어요,,",
          "original_form": "네.. 후드집업을 장만했어요!ㅋㅋㅋㅋㅋㅋㅋㅋ 다른 옷도 사고싶어요,,",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034347.1.23",
          "form": "후드집업 편하고 좋죠 ㅋㅋㅋ",
          "original_form": "후드집업 편하고 좋죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034347.1.24",
          "form": "요즘 시즌오프라 싸도라구요!!",
