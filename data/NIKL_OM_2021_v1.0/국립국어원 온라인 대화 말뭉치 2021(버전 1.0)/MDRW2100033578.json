      "utterance": [
        {
          "id": "MDRW2100033578.1.1",
          "form": "연애 몇년하셨나요",
          "original_form": "연애 몇년하셨나요",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100033578.1.2",
          "form": "한 4년 했죠",
          "original_form": "한 4년 했죠",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100033578.1.3",
          "form": "와 길게 하셨네요~",
          "original_form": "와 길게 하셨네요~",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100033578.1.4",
          "form": "네 그때 모든 사랑을 다 쏟았나봐요 ㅋㅋ",
          "original_form": "네 그때 모든 사랑을 다 쏟았나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100033578.1.5",
          "form": "연예는 짧은게 좋은거 같아요",
          "original_form": "연예는 짧은게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100033578.1.6",
          "form": "연애 오래하셨어요?",
          "original_form": "연애 오래하셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100033578.1.7",
          "form": "저는 1년반이요~",
          "original_form": "저는 1년반이요~",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100033578.1.8",
          "form": "딱좋네요",
          "original_form": "딱좋네요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100033578.1.9",
          "form": "그럼 질리지도 않고 서로 어느정도 알고",
          "original_form": "그럼 질리지도 않고 서로 어느정도 알고",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100033578.1.10",
          "form": "지금 남편분은 결혼전과 똑같이 잘해주세요?",
          "original_form": "지금 남편분은 결혼전과 똑같이 잘해주세요?",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100033578.1.11",
          "form": "흠..글쎄요..name2님은 어떠시나요 ㅋㅋ",
          "original_form": "흠..글쎄요..&name2&님은 어떠시나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100033578.1.12",
          "form": "별로요 성격이 ㅋㅋ불린줄 몰랐네요",
          "original_form": "별로요 성격이 ㅋㅋ불린줄 몰랐네요",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100033578.1.13",
          "form": "그땐",
          "original_form": "그땐",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100033578.1.14",
          "form": "전 미지근해서요 ㅋㅋ",
          "original_form": "전 미지근해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100033578.1.15",
          "form": "연애때 몰랐던걸 알게돼죠 결혼은 참 현실이라 똑같을수 없죠 ㅎㅎ",
          "original_form": "연애때 몰랐던걸 알게돼죠 결혼은 참 현실이라 똑같을수 없죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100033578.1.16",
          "form": "가만보면 다들 이상형과 결혼하진 않더라구요",
          "original_form": "가만보면 다들 이상형과 결혼하진 않더라구요",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100033578.1.17",
          "form": "아 불과 미지근인가요? 성향 차이가??",
          "original_form": "아 불과 미지근인가요? 성향 차이가??",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100033578.1.18",
          "form": "엄청 심하죠",
          "original_form": "엄청 심하죠",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100033578.1.19",
          "form": "제가 주로 참아요 ㅡㅡ",
          "original_form": "제가 주로 참아요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033578.1.20",
          "form": "전 이상형이 스윗한 사람 이였는데",
          "original_form": "전 이상형이 스윗한 사람 이였는데",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033578.1.21",
          "form": "그렇군요 저희는 남편이 얼음같달까요 ..ㅋ",
          "original_form": "그렇군요 저희는 남편이 얼음같달까요 ..ㅋ",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033578.1.22",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033578.1.23",
          "form": "결혼은 ㅋㅋ다른 사람과",
          "original_form": "결혼은 ㅋㅋ다른 사람과",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033578.1.24",
          "form": "오 얼음 차갑나요 냉철하고",
          "original_form": "오 얼음 차갑나요 냉철하고",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100033578.1.25",
          "form": "말에 얼음 송곳 있나요 ㅡㅡ",
          "original_form": "말에 얼음 송곳 있나요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100033578.1.26",
          "form": "저희남편은 너무 논리적이기도 해요",
          "original_form": "저희남편은 너무 논리적이기도 해요",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100033578.1.27",
          "form": "이제보니 성격이 넘 극과 극인데요 ㅋㅋ",
          "original_form": "이제보니 성격이 넘 극과 극인데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100033578.1.28",
          "form": "맞아요 논리적..이성적...ㅋ",
          "original_form": "맞아요 논리적..이성적...ㅋ",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100033578.1.29",
          "form": "네 아 피곤해요 ㅋㅋ",
          "original_form": "네 아 피곤해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:26"
        }
      ]
