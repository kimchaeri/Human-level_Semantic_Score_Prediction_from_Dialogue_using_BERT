      "utterance": [
        {
          "id": "MDRW2100043584.1.1",
          "form": "요즘 날씨가 좋으니 나들이 가고 싶어져요",
          "original_form": "요즘 날씨가 좋으니 나들이 가고 싶어져요",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043584.1.2",
          "form": "비..오지않나요?",
          "original_form": "비..오지않나요?",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043584.1.3",
          "form": "name1님은 화창한 곳에 사시나봐요 ㅎㅎ",
          "original_form": "&name1&님은 화창한 곳에 사시나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043584.1.4",
          "form": "아 오늘은 비와요ㅜ",
          "original_form": "아 오늘은 비와요ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043584.1.5",
          "form": "지난 주말에 날씨가 좋아서ㅎㅎ",
          "original_form": "지난 주말에 날씨가 좋아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043584.1.6",
          "form": "아.. 주말에 ㅋㅋㅋㅋ는 확실히 좋았어요 ㅎㅎ",
          "original_form": "아.. 주말에 ㅋㅋㅋㅋ는 확실히 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043584.1.7",
          "form": "어디 놀러가고싶으세요?",
          "original_form": "어디 놀러가고싶으세요?",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043584.1.8",
          "form": "그냥 근교라도 가고싶어요",
          "original_form": "그냥 근교라도 가고싶어요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043584.1.9",
          "form": "여름 휴가도 안가고 집콕했더니",
          "original_form": "여름 휴가도 안가고 집콕했더니",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043584.1.10",
          "form": "이번 주말이라도 ㅎㅎ",
          "original_form": "이번 주말이라도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043584.1.11",
          "form": "좀 떠나고 싶네요",
          "original_form": "좀 떠나고 싶네요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043584.1.12",
          "form": "한번 가보세요 ㅎㅎ",
          "original_form": "한번 가보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043584.1.13",
          "form": "아 그래야겠어요",
          "original_form": "아 그래야겠어요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043584.1.14",
          "form": "주말에 날 좋으면 가까운 공원이라도",
          "original_form": "주말에 날 좋으면 가까운 공원이라도",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043584.1.15",
          "form": "맞아요 ㅋㅋ 그정도는 기분 전환할수있죠!",
          "original_form": "맞아요 ㅋㅋ 그정도는 기분 전환할수있죠!",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043584.1.16",
          "form": "진짜 잠깐 나들이 다녀와야 겠어요",
          "original_form": "진짜 잠깐 나들이 다녀와야 겠어요",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043584.1.17",
          "form": "ㅎㅎ 좋아요!",
          "original_form": "ㅎㅎ 좋아요!",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043584.1.18",
          "form": "저도 코에 바람 넣고싶네요 ㅎㅎ",
          "original_form": "저도 코에 바람 넣고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043584.1.19",
          "form": "이번주에는 비가 안왔으면 좋겠어요!",
          "original_form": "이번주에는 비가 안왔으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043584.1.20",
          "form": "여행아닌 여행이라두 갈수있게 ㅎㅎ",
          "original_form": "여행아닌 여행이라두 갈수있게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043584.1.21",
          "form": "네 날씨가 좋아야 여행도 가죠",
          "original_form": "네 날씨가 좋아야 여행도 가죠",
