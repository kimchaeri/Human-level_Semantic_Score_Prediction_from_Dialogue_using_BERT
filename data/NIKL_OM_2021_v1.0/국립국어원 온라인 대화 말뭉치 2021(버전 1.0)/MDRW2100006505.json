      "utterance": [
        {
          "id": "MDRW2100006505.1.1",
          "form": "플립 구매하신진 얼마나 되신거죠",
          "original_form": "플립 구매하신진 얼마나 되신거죠",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100006505.1.2",
          "form": "이주쯤?? 된것같네요",
          "original_form": "이주쯤?? 된것같네요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100006505.1.3",
          "form": "2주 사용하신 소감은요?",
          "original_form": "2주 사용하신 소감은요?",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100006505.1.4",
          "form": "접히는 부분이 눈에 들어오긴하는데 크게 거슬리지는 않구요",
          "original_form": "접히는 부분이 눈에 들어오긴하는데 크게 거슬리지는 않구요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006505.1.5",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100006505.1.6",
          "form": "일단 예뻐요",
          "original_form": "일단 예뻐요",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006505.1.7",
          "form": "그래서 만족스러워욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래서 만족스러워욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006505.1.8",
          "form": "가로가 좁고 세로로만 너무길고... 한손으로 접고펴기 어렵고...",
          "original_form": "가로가 좁고 세로로만 너무길고... 한손으로 접고펴기 어렵고...",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100006505.1.9",
          "form": "아쉬운점들이 잇는데도",
          "original_form": "아쉬운점들이 잇는데도",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100006505.1.10",
          "form": "케이스도 사셨나요",
          "original_form": "케이스도 사셨나요",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006505.1.11",
          "form": "사전예약 사은품을 일단 골라서 신청을햇는데",
          "original_form": "사전예약 사은품을 일단 골라서 신청을햇는데",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006505.1.12",
          "form": "아직도 안왓어요",
          "original_form": "아직도 안왓어요",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006505.1.13",
          "form": "그럼 지금은.생폰?으로 쓰시는 건가요",
          "original_form": "그럼 지금은.생폰?으로 쓰시는 건가요",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006505.1.14",
          "form": "네 ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006505.1.15",
          "form": "쌩폰입니다",
          "original_form": "쌩폰입니다",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006505.1.16",
          "form": "모시고 다녀야겠네요 ㅎㅎ",
          "original_form": "모시고 다녀야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100006505.1.17",
          "form": "맞아요 두손으로 공손하게 들고다닙니다 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 두손으로 공손하게 들고다닙니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100006505.1.18",
          "form": "어디서 사셨어요?",
          "original_form": "어디서 사셨어요?",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100006505.1.19",
          "form": "저는 지마켓에서 삿어요",
          "original_form": "저는 지마켓에서 삿어요",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100006505.1.20",
          "form": "그러셨군요",
          "original_form": "그러셨군요",
          "speaker_id": "1",
          "time": "20210908 01:20"
        }
      ]
