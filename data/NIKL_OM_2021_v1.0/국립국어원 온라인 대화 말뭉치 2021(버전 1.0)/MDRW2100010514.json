      "utterance": [
        {
          "id": "MDRW2100010514.1.1",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010514.1.2",
          "form": "아뇨! 대학생이라 저는 하교하면서 많이 들었어요ㅎㅎ",
          "original_form": "아뇨! 대학생이라 저는 하교하면서 많이 들었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010514.1.3",
          "form": "name1 님은 어떤 일 하시나요?",
          "original_form": "&name1& 님은 어떤 일 하시나요?",
          "speaker_id": "2",
          "time": "20210823 19:08"
        },
        {
          "id": "MDRW2100010514.1.4",
          "form": "저는 치과다녀용 대학생이신가요?",
          "original_form": "저는 치과다녀용 대학생이신가요?",
          "speaker_id": "1",
          "time": "20210823 19:09"
        },
        {
          "id": "MDRW2100010514.1.5",
          "form": "네 알바로 과외하고 있어요!",
          "original_form": "네 알바로 과외하고 있어요!",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010514.1.6",
          "form": "치과 다니시는 군요!",
          "original_form": "치과 다니시는 군요!",
          "speaker_id": "2",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010514.1.7",
          "form": "오 어떤 과외요?",
          "original_form": "오 어떤 과외요?",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010514.1.8",
          "form": "네네 치위생사입니장",
          "original_form": "네네 치위생사입니장",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010514.1.9",
          "form": "너무 멋있어요!!!!",
          "original_form": "너무 멋있어요!!!!",
          "speaker_id": "2",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.10",
          "form": "아니예용 ..",
          "original_form": "아니예용 ..",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.11",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.12",
          "form": "초~중학교 수준으로 영어랑 수학 가르치고 있어요",
          "original_form": "초~중학교 수준으로 영어랑 수학 가르치고 있어요",
          "speaker_id": "2",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.13",
          "form": "어떤 과외하세여?",
          "original_form": "어떤 과외하세여?",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.15",
          "form": "더 멋진데요 ㅎㅎ",
          "original_form": "더 멋진데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010514.1.16",
          "form": "감사해요ㅎㅎ",
          "original_form": "감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010514.1.17",
          "form": "원래 교사가 꿈이었어서 재밌게 하고 있어요",
          "original_form": "원래 교사가 꿈이었어서 재밌게 하고 있어요",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010514.1.18",
          "form": "스트레스는 없나요?",
          "original_form": "스트레스는 없나요?",
          "speaker_id": "1",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010514.1.19",
          "form": "없진 않은데 다 그렇죠 뭐...ㅎㅎ",
          "original_form": "없진 않은데 다 그렇죠 뭐...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:15"
        },
        {
          "id": "MDRW2100010514.1.20",
          "form": "용돈을 제가 스스로 벌어야 해서",
          "original_form": "용돈을 제가 스스로 벌어야 해서",
          "speaker_id": "2",
