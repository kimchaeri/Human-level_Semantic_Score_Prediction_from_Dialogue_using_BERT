      "utterance": [
        {
          "id": "MDRW2100000895.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000895.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000895.1.3",
          "form": "키우시낭 ?",
          "original_form": "키우시낭 ?",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000895.1.4",
          "form": "나요?",
          "original_form": "나요?",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000895.1.5",
          "form": "전 강아지 키워요~",
          "original_form": "전 강아지 키워요~",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000895.1.6",
          "form": "저는 오래 키우던 댕댕이가 작년에 무지개다리를 건넜어요!",
          "original_form": "저는 오래 키우던 댕댕이가 작년에 무지개다리를 건넜어요!",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000895.1.7",
          "form": "그래서 아직 다시 키우고 있지는 않아요",
          "original_form": "그래서 아직 다시 키우고 있지는 않아요",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000895.1.8",
          "form": "ㅠㅠ 아이고",
          "original_form": "ㅠㅠ 아이고",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.9",
          "form": "너무 좋으시겠어요 댕댕이!",
          "original_form": "너무 좋으시겠어요 댕댕이!",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.10",
          "form": "댕댕 너무 좋죠..11살인데",
          "original_form": "댕댕 너무 좋죠..11살인데",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.11",
          "form": "첨 키우는 강아지예요",
          "original_form": "첨 키우는 강아지예요",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.12",
          "form": "아직 건강한데..",
          "original_form": "아직 건강한데..",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.13",
          "form": "언젠간 떠날거라고 생각하면 가끔 눈물이 나요",
          "original_form": "언젠간 떠날거라고 생각하면 가끔 눈물이 나요",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.14",
          "form": "잉 그 마음 너무 알겠어요",
          "original_form": "잉 그 마음 너무 알겠어요",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.15",
          "form": "무지개다리 건넌 댕댕이.. 보고싶으시겠다",
          "original_form": "무지개다리 건넌 댕댕이.. 보고싶으시겠다",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.16",
          "form": "괜찮으세요?",
          "original_form": "괜찮으세요?",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.17",
          "form": "우리 댕댕이는 19년 살았어요 저랑!",
          "original_form": "우리 댕댕이는 19년 살았어요 저랑!",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000895.1.18",
          "form": "매일 보고 싶고 생각나면 울고 그래요!",
          "original_form": "매일 보고 싶고 생각나면 울고 그래요!",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000895.1.19",
          "form": "가족들이랑도 얘기 많이 하고요",
          "original_form": "가족들이랑도 얘기 많이 하고요",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000895.1.20",
          "form": "장수했네요 댕댕이! 그만큼 더 그리우시겠어요..",
          "original_form": "장수했네요 댕댕이! 그만큼 더 그리우시겠어요..",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000895.1.21",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000895.1.22",
          "form": "저 지금도 눈물날려고하는데 주책이죠ㅠㅋㅋ",
          "original_form": "저 지금도 눈물날려고하는데 주책이죠ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000895.1.23",
          "form": "더 많이 사랑해주고 그럴걸.. 미안한 생각만 자꾸 들어요!",
          "original_form": "더 많이 사랑해주고 그럴걸.. 미안한 생각만 자꾸 들어요!",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000895.1.24",
          "form": "저도 얘기하니까 또 보고 싶네요!",
          "original_form": "저도 얘기하니까 또 보고 싶네요!",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000895.1.25",
          "form": "저도 덜 후회하기위해서 더 잘해줘야겠어요..",
          "original_form": "저도 덜 후회하기위해서 더 잘해줘야겠어요..",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000895.1.26",
          "form": "앞으로 다른 댕댕이 키울 생각은 없으세요?",
          "original_form": "앞으로 다른 댕댕이 키울 생각은 없으세요?",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000895.1.27",
          "form": "네! 많이 안아주고 사랑해주고",
          "original_form": "네! 많이 안아주고 사랑해주고",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000895.1.28",
          "form": "살짝 귀찮아했던 순간들도 너무 후회되고 그러더라고요",
          "original_form": "살짝 귀찮아했던 순간들도 너무 후회되고 그러더라고요",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000895.1.29",
          "form": "음..아직은 없는데",
          "original_form": "음..아직은 없는데",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000895.1.30",
          "form": "혹시나 연이 되는 댕댕이가 있다면 거절하지는 않을 생각이에요!",
          "original_form": "혹시나 연이 되는 댕댕이가 있다면 거절하지는 않을 생각이에요!",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000895.1.31",
          "form": "어떤 존재를",
          "original_form": "어떤 존재를",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000895.1.32",
          "form": "이만큼 사랑할 수 있다는 것 자체가 축복같아요",
          "original_form": "이만큼 사랑할 수 있다는 것 자체가 축복같아요",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000895.1.33",
          "form": "강아지라고해도.. 사람보다 더 사랑스러워요",
          "original_form": "강아지라고해도.. 사람보다 더 사랑스러워요",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000895.1.34",
          "form": "와..이 말이 정답!",
          "original_form": "와..이 말이 정답!",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000895.1.35",
          "form": "진짜 조건없이 사랑하는 존재",
          "original_form": "진짜 조건없이 사랑하는 존재",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000895.1.36",
          "form": "댕댕이 보낼 때 진짜 제가 대신 가고 싶다고 생각도 했어요",
          "original_form": "댕댕이 보낼 때 진짜 제가 대신 가고 싶다고 생각도 했어요",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000895.1.37",
          "form": "맞아요,, 우리 댕댕,,,,,,,,,,",
          "original_form": "맞아요,, 우리 댕댕,,,,,,,,,,",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000895.1.38",
          "form": "반려동물이 같이 사는 동물이란 뜻이잖아요",
          "original_form": "반려동물이 같이 사는 동물이란 뜻이잖아요",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000895.1.39",
          "form": "저한텐 그것보다.. 살아갈 수 있게 힘을 주는 존재라서..ㅠㅠ 정말 댕댕이때문에 출근하고.. 그러네요",
          "original_form": "저한텐 그것보다.. 살아갈 수 있게 힘을 주는 존재라서..ㅠㅠ 정말 댕댕이때문에 출근하고.. 그러네요",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000895.1.40",
          "form": "아우 맞아요 맞아",
          "original_form": "아우 맞아요 맞아",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000895.1.41",
          "form": "댕댕이랑 행복하려고 돈 버는 거지요 뭐",
          "original_form": "댕댕이랑 행복하려고 돈 버는 거지요 뭐",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000895.1.42",
          "form": "댕댕이 보내고 기부도 많이 했어요",
          "original_form": "댕댕이 보내고 기부도 많이 했어요",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000895.1.43",
          "form": "세상 모든 댕댕이들 행복해져라!!!",
          "original_form": "세상 모든 댕댕이들 행복해져라!!!",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000895.1.44",
          "form": "멋있으시다..!",
          "original_form": "멋있으시다..!",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000895.1.45",
          "form": "모든 강아지 고양이 동물들 다 행복하고",
          "original_form": "모든 강아지 고양이 동물들 다 행복하고",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.46",
          "form": "집사들도 행복했으면,,,",
          "original_form": "집사들도 행복했으면,,,",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.47",
          "form": "힝 맞아요",
          "original_form": "힝 맞아요",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.48",
          "form": "댕댕 알러지때문에 요즘 간식을 못주고있는데",
          "original_form": "댕댕 알러지때문에 요즘 간식을 못주고있는데",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.49",
          "form": "우울해보여서 저도 진빠지네요ㅠㅠ",
          "original_form": "우울해보여서 저도 진빠지네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.50",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.51",
          "form": "먹고 싶어하는데 못주는 마음...",
          "original_form": "먹고 싶어하는데 못주는 마음...",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.52",
          "form": "속상",
          "original_form": "속상",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.53",
          "form": "어떤 거에 알레르기 반응이 있는 거예요?",
          "original_form": "어떤 거에 알레르기 반응이 있는 거예요?",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000895.1.54",
          "form": "밀가루.. 계란.. 여기까진 괜찮은데",
          "original_form": "밀가루.. 계란.. 여기까진 괜찮은데",
