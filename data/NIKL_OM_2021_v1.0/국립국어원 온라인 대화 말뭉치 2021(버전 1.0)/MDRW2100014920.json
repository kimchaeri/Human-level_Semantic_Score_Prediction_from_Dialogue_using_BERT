      "utterance": [
        {
          "id": "MDRW2100014920.1.1",
          "form": "이상형이 있으신가요?",
          "original_form": "이상형이 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014920.1.2",
          "form": "음 딱히 없어요",
          "original_form": "음 딱히 없어요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014920.1.3",
          "form": "좀 성격이 부드러운 사람 요",
          "original_form": "좀 성격이 부드러운 사람 요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014920.1.4",
          "form": "ㅋㅋㅋㅋ성격 중요하죠",
          "original_form": "ㅋㅋㅋㅋ성격 중요하죠",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014920.1.5",
          "form": "저도 성격이 제일 중요한것같아요",
          "original_form": "저도 성격이 제일 중요한것같아요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014920.1.6",
          "form": "네 이상형이 있어요?",
          "original_form": "네 이상형이 있어요?",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014920.1.7",
          "form": "공감 잘해주는 사람!",
          "original_form": "공감 잘해주는 사람!",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014920.1.8",
          "form": "네 성격이 불같으면 힘들어요",
          "original_form": "네 성격이 불같으면 힘들어요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014920.1.9",
          "form": "아 그렇죠 공감잘해주는 사람좋죠",
          "original_form": "아 그렇죠 공감잘해주는 사람좋죠",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014920.1.10",
          "form": "맞아요 ㅠㅠ 서로 이해하고 배려할 줄 아는사람 최고...",
          "original_form": "맞아요 ㅠㅠ 서로 이해하고 배려할 줄 아는사람 최고...",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014920.1.11",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014920.1.12",
          "form": "살아보면 같이살면서 다른생각으로 안통하면 힘들어요",
          "original_form": "살아보면 같이살면서 다른생각으로 안통하면 힘들어요",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014920.1.13",
          "form": "음 외모는? 이상형 있으세요",
          "original_form": "음 외모는? 이상형 있으세요",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014920.1.14",
          "form": "순둥순둥하게 생긴 사람이 좋은것같아요 ㅋㅋㅋㅋ",
          "original_form": "순둥순둥하게 생긴 사람이 좋은것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014920.1.15",
          "form": "외모로 이상형 있으세요?",
          "original_form": "외모로 이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014920.1.16",
          "form": "아 그러세요 ㅎㅎ",
          "original_form": "아 그러세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014920.1.17",
          "form": "이상형 이라기보단",
          "original_form": "이상형 이라기보단",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014920.1.18",
          "form": "갑자기 이동욱이 ㅋㅋ",
          "original_form": "갑자기 이동욱이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014920.1.19",
          "form": "생각났어요",
          "original_form": "생각났어요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014920.1.20",
          "form": "이건 그냥 팍 떠오른 이미지요",
          "original_form": "이건 그냥 팍 떠오른 이미지요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014920.1.21",
          "form": "막이런 얼굴좋지는 없었던거 같아요 ㅎㅎ",
          "original_form": "막이런 얼굴좋지는 없었던거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014920.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이동욱도 잘생겼죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이동욱도 잘생겼죠",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014920.1.23",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014920.1.24",
          "form": "그냥 뮤직비디오가 떠올랐어요",
          "original_form": "그냥 뮤직비디오가 떠올랐어요",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014920.1.25",
          "form": "어떤 뮤직비디오요?",
          "original_form": "어떤 뮤직비디오요?",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014920.1.26",
          "form": "김동률 리플레이요",
          "original_form": "김동률 리플레이요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014920.1.27",
          "form": "본적이 없어서 궁금하네요 ㅋㅋㅋ이동욱이 어떻게 나왔는지 나중에 봐야겠어요",
          "original_form": "본적이 없어서 궁금하네요 ㅋㅋㅋ이동욱이 어떻게 나왔는지 나중에 봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014920.1.28",
          "form": "아 목소리도 중요한듯!",
          "original_form": "아 목소리도 중요한듯!",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014920.1.29",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014920.1.30",
          "form": "강렬해요",
          "original_form": "강렬해요",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014920.1.31",
          "form": "아 목소리",
          "original_form": "아 목소리",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014920.1.32",
          "form": "그렇네요 따뜻하고 잔잔한 ㅎㅎ",
          "original_form": "그렇네요 따뜻하고 잔잔한 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014920.1.33",
          "form": "네 개인적으로는 낮은목소리 굉장히 좋아해서 ㅎㅎ",
          "original_form": "네 개인적으로는 낮은목소리 굉장히 좋아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014920.1.34",
          "form": "아 다들 그런가봐요 ㅎㅎ",
          "original_form": "아 다들 그런가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014920.1.35",
          "form": "높은톤은 안정감이 없죠",
          "original_form": "높은톤은 안정감이 없죠",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014920.1.36",
          "form": "네 뭔가 낮은목소리를 들으면 차분해져서 ㅋㅋㅋ",
          "original_form": "네 뭔가 낮은목소리를 들으면 차분해져서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014920.1.37",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014920.1.38",
          "form": "그런분은 화도 안낼것 같아요 목소리 들으면",
          "original_form": "그런분은 화도 안낼것 같아요 목소리 들으면",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014920.1.39",
          "form": "맞아요 그런 느낌이 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 그런 느낌이 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014920.1.40",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        }
      ]
