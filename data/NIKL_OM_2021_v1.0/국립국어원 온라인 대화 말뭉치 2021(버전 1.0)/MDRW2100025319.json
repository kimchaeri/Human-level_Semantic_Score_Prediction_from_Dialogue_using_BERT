      "utterance": [
        {
          "id": "MDRW2100025319.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210901 09:19"
        },
        {
          "id": "MDRW2100025319.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100025319.1.3",
          "form": "동물 키우세요??",
          "original_form": "동물 키우세요??",
          "speaker_id": "2",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100025319.1.4",
          "form": "저도 아직은",
          "original_form": "저도 아직은",
          "speaker_id": "1",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100025319.1.5",
          "form": "강아지랑 고양이중에 어떤 동물 더 좋아하세요?",
          "original_form": "강아지랑 고양이중에 어떤 동물 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100025319.1.6",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100025319.1.7",
          "form": "저도 강아지 좋아해요!",
          "original_form": "저도 강아지 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 09:22"
        },
        {
          "id": "MDRW2100025319.1.8",
          "form": "어떤 종 좋아하세요?",
          "original_form": "어떤 종 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 09:23"
        },
        {
          "id": "MDRW2100025319.1.9",
          "form": "종은 잘 몰라서",
          "original_form": "종은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210901 09:26"
        },
        {
          "id": "MDRW2100025319.1.10",
          "form": "그렇군요.",
          "original_form": "그렇군요.",
          "speaker_id": "2",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100025319.1.11",
          "form": "털 알러지는 없으세요?",
          "original_form": "털 알러지는 없으세요?",
          "speaker_id": "2",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100025319.1.12",
          "form": "그런건 없는듯요",
          "original_form": "그런건 없는듯요",
          "speaker_id": "1",
          "time": "20210901 09:28"
        },
        {
          "id": "MDRW2100025319.1.13",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025319.1.14",
          "form": "반려동물 키우고 싶어도 알레르기 때문에 못키우시는 분들 많은것같아요",
          "original_form": "반려동물 키우고 싶어도 알레르기 때문에 못키우시는 분들 많은것같아요",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025319.1.15",
          "form": "털빠지는게 문재죠",
          "original_form": "털빠지는게 문재죠",
          "speaker_id": "1",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025319.1.16",
          "form": "키우는 집은 돌돌이 필수인듯요",
          "original_form": "키우는 집은 돌돌이 필수인듯요",
          "speaker_id": "2",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025319.1.17",
          "form": "돌돌이 좋죠",
          "original_form": "돌돌이 좋죠",
          "speaker_id": "1",
          "time": "20210901 09:33"
        }
      ]
    }
