      "utterance": [
        {
          "id": "MDRW2100032999.1.1",
          "form": "와 취두부 드셔보셨나요? 대만에도 파는군요",
          "original_form": "와 취두부 드셔보셨나요? 대만에도 파는군요",
          "speaker_id": "2",
          "time": "20210906 13:00"
        },
        {
          "id": "MDRW2100032999.1.2",
          "form": "아니요 냄새 심하던데요ㅠ",
          "original_form": "아니요 냄새 심하던데요ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:16"
        },
        {
          "id": "MDRW2100032999.1.3",
          "form": "오ㅠㅜ 맞아요ㅠㅠ 저도 도전못할듯",
          "original_form": "오ㅠㅜ 맞아요ㅠㅠ 저도 도전못할듯",
          "speaker_id": "2",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100032999.1.4",
          "form": "맛있는게 많은데 굳이 먹고 싶지 않아요 ㅋㅋ",
          "original_form": "맛있는게 많은데 굳이 먹고 싶지 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100032999.1.5",
          "form": "네 ㅋㅋ 다른거 충분해요",
          "original_form": "네 ㅋㅋ 다른거 충분해요",
          "speaker_id": "1",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100032999.1.6",
          "form": "그쵸 대만 너무 조아",
          "original_form": "그쵸 대만 너무 조아",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100032999.1.7",
          "form": "후쿠오카도 참 좋았어요",
          "original_form": "후쿠오카도 참 좋았어요",
          "speaker_id": "2",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100032999.1.8",
          "form": "후쿠오카 안가봤는데ㅠㅠ",
          "original_form": "후쿠오카 안가봤는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100032999.1.9",
          "form": "좋나요",
          "original_form": "좋나요",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100032999.1.10",
          "form": "네 너무 좋아요",
          "original_form": "네 너무 좋아요",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100032999.1.11",
          "form": "특히 어떤 점이?",
          "original_form": "특히 어떤 점이?",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100032999.1.12",
          "form": "료칸이 좋았고 온천도 좋았어요",
          "original_form": "료칸이 좋았고 온천도 좋았어요",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100032999.1.13",
          "form": "길도 좀 시골같이 한적하고 맛난거도 많고",
          "original_form": "길도 좀 시골같이 한적하고 맛난거도 많고",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100032999.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100032999.1.15",
          "form": "한적한 맛이 또 있죠",
          "original_form": "한적한 맛이 또 있죠",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100032999.1.16",
          "form": "그툐ㅠㅠ 모츠나베 맛나요",
          "original_form": "그툐ㅠㅠ 모츠나베 맛나요",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100032999.1.17",
          "form": "모모치 해변에서 파는 굴 구이듀 진짜 잊지못해",
          "original_form": "모모치 해변에서 파는 굴 구이듀 진짜 잊지못해",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100032999.1.18",
          "form": "오 모츠나베가 뭐죠",
          "original_form": "오 모츠나베가 뭐죠",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100032999.1.19",
          "form": "음 곱창 전골? 이에요",
          "original_form": "음 곱창 전골? 이에요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        }
      ]
