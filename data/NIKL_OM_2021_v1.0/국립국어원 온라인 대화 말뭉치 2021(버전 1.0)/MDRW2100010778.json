      "utterance": [
        {
          "id": "MDRW2100010778.1.1",
          "form": "게임 하시는거 있으세요?",
          "original_form": "게임 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010778.1.2",
          "form": "저 쿠키런하다가 접었어요ㅠㅠ 하시는 겜있으세요?",
          "original_form": "저 쿠키런하다가 접었어요ㅠㅠ 하시는 겜있으세요?",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010778.1.3",
          "form": "저는 꿈의집 시작했는데 가끔 심심풀이로 좋더라구요",
          "original_form": "저는 꿈의집 시작했는데 가끔 심심풀이로 좋더라구요",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010778.1.4",
          "form": "아 맞아요 그겜 재밋죠 중독성있구ㅎㅎ",
          "original_form": "아 맞아요 그겜 재밋죠 중독성있구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:55"
        },
        {
          "id": "MDRW2100010778.1.5",
          "form": "ㅋㅋㅋㅋ네네 사실 전 여러가지 건드려보긴하는데 꾸준히 하는 게임은 별로 없네용",
          "original_form": "ㅋㅋㅋㅋ네네 사실 전 여러가지 건드려보긴하는데 꾸준히 하는 게임은 별로 없네용",
          "speaker_id": "1",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100010778.1.6",
          "form": "아 저도 그래요ㅎㅎ 게임에 꾸준하지않은건 좋은건지ㅎㅎ",
          "original_form": "아 저도 그래요ㅎㅎ 게임에 꾸준하지않은건 좋은건지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010778.1.7",
          "form": "ㄱㅋㅋㄱㅋㅋㄱㅋ그러게요 그래도 저도 쿠키런은 오래했는데 쿠킹덤은 금방 질려서 금방 삭제해버렸어요",
          "original_form": "ㄱㅋㅋㄱㅋㅋㄱㅋ그러게요 그래도 저도 쿠키런은 오래했는데 쿠킹덤은 금방 질려서 금방 삭제해버렸어요",
          "speaker_id": "1",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010778.1.8",
          "form": "저도 하다가 재미는 있는데 현질안하면 못따라가서ㅜㅜ",
          "original_form": "저도 하다가 재미는 있는데 현질안하면 못따라가서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010778.1.9",
          "form": "그쵸 요즘 게임은 현질유도가 넘 심한것 같아요ㅠㅠ",
          "original_form": "그쵸 요즘 게임은 현질유도가 넘 심한것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010778.1.10",
          "form": "재밌게 하셨던 게임 있으세요?",
          "original_form": "재밌게 하셨던 게임 있으세요?",
          "speaker_id": "1",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010778.1.11",
          "form": "예전에 카트랑 크아ㅎㅎㅎ재밌게 했어요",
          "original_form": "예전에 카트랑 크아ㅎㅎㅎ재밌게 했어요",
          "speaker_id": "2",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010778.1.12",
          "form": "앜ㄲㅋㅋㅋ완전 추억겜이네요",
          "original_form": "앜ㄲㅋㅋㅋ완전 추억겜이네요",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010778.1.13",
          "form": "완전 재밌게했죠",
          "original_form": "완전 재밌게했죠",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010778.1.14",
          "form": "전 메이플도 했어욯ㅎ",
          "original_form": "전 메이플도 했어욯ㅎ",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010778.1.15",
          "form": "와 메이플ㅋㅋㅋ어렸을때 했었는데ㅎㅎ",
          "original_form": "와 메이플ㅋㅋㅋ어렸을때 했었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010778.1.16",
          "form": "ㅋㅋㅋㅋ방학때만 되면 캐릭터 하나씩 새로 키웠는데",
          "original_form": "ㅋㅋㅋㅋ방학때만 되면 캐릭터 하나씩 새로 키웠는데",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100010778.1.17",
          "form": "추억이네요ㅎㅎ",
          "original_form": "추억이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100010778.1.18",
          "form": "맞아요 일부러 방학때 내는거같구ㅎㅎ",
          "original_form": "맞아요 일부러 방학때 내는거같구ㅎㅎ",
