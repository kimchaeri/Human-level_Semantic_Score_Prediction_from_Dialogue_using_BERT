      "utterance": [
        {
          "id": "MDRW2100003787.1.1",
          "form": "우리 강아지들 다 어디 갔지",
          "original_form": "우리 강아지들 다 어디 갔지",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003787.1.2",
          "form": "그러게 어디갔지 ㅋ 안보이네",
          "original_form": "그러게 어디갔지 ㅋ 안보이네",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003787.1.3",
          "form": "그러게 안 보인다 숨었나",
          "original_form": "그러게 안 보인다 숨었나",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003787.1.4",
          "form": "비와서 숨었나보다",
          "original_form": "비와서 숨었나보다",
          "speaker_id": "1",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003787.1.5",
          "form": "그래 우리 애들 빗소리 싫어한다",
          "original_form": "그래 우리 애들 빗소리 싫어한다",
          "speaker_id": "2",
          "time": "20210821 12:06"
        },
        {
          "id": "MDRW2100003787.1.6",
          "form": "비오면 산책을 못가서 싫어하는가보네",
          "original_form": "비오면 산책을 못가서 싫어하는가보네",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.7",
          "form": "그런가 보다",
          "original_form": "그런가 보다",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.8",
          "form": "애들 스트레스 받겠다",
          "original_form": "애들 스트레스 받겠다",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.9",
          "form": "이따 간식 좀 줘라",
          "original_form": "이따 간식 좀 줘라",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.10",
          "form": "알겠어용ㅋㅋ",
          "original_form": "알겠어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.11",
          "form": "먹는게 낙이다",
          "original_form": "먹는게 낙이다",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.12",
          "form": "그래 ㅋㅋ 사람이랑 같다",
          "original_form": "그래 ㅋㅋ 사람이랑 같다",
          "speaker_id": "1",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.13",
          "form": "간식 다 안 떨어져 가나",
          "original_form": "간식 다 안 떨어져 가나",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003787.1.14",
          "form": "옹 아직 많이 남아있어용",
          "original_form": "옹 아직 많이 남아있어용",
          "speaker_id": "1",
          "time": "20210821 12:08"
        },
        {
