      "utterance": [
        {
          "id": "MDRW2100037632.1.1",
          "form": "그거 어떠셨나요?",
          "original_form": "그거 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100037632.1.2",
          "form": "모짜렐라인더버거요??",
          "original_form": "모짜렐라인더버거요??",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037632.1.3",
          "form": "네 !",
          "original_form": "네 !",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037632.1.4",
          "form": "배불러요",
          "original_form": "배불러요",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037632.1.5",
          "form": "아하 ㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037632.1.6",
          "form": "그거 패티 들었어요?",
          "original_form": "그거 패티 들었어요?",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037632.1.7",
          "form": "흠 기억이안나요 검색해보세요",
          "original_form": "흠 기억이안나요 검색해보세요",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037632.1.8",
          "form": "뭐가 두개든거같아요",
          "original_form": "뭐가 두개든거같아요",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037632.1.9",
          "form": "아 치즌가봐요 ㅋㄱㄲ",
          "original_form": "아 치즌가봐요 ㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037632.1.10",
          "form": "버거킹도 좋아하세요?",
          "original_form": "버거킹도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037632.1.11",
          "form": "네 버거킹도 맛있어요",
          "original_form": "네 버거킹도 맛있어요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037632.1.12",
          "form": "젤 배부른듯 ㅋㅋ",
          "original_form": "젤 배부른듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037632.1.13",
          "form": "저두요 ㅋㅋ 최애버거 뭐세요?",
          "original_form": "저두요 ㅋㅋ 최애버거 뭐세요?",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037632.1.14",
          "form": "와퍼요 뭘 자꾸 만들어내지 않으면 좋겠어요 ㅋㅋ",
          "original_form": "와퍼요 뭘 자꾸 만들어내지 않으면 좋겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037632.1.15",
          "form": "ㅋㅋㅋㅋ신메뉴 엄청 나오죠",
          "original_form": "ㅋㅋㅋㅋ신메뉴 엄청 나오죠",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037632.1.16",
          "form": "전 통새우 좋아해요",
          "original_form": "전 통새우 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037632.1.17",
          "form": "아 요즘 안 가서 몰랐어요 통새우 저도 먹어보고 싶은데요",
          "original_form": "아 요즘 안 가서 몰랐어요 통새우 저도 먹어보고 싶은데요",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037632.1.18",
          "form": "통새우 나온지 좀 오래 됐어요!",
          "original_form": "통새우 나온지 좀 오래 됐어요!",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037632.1.19",
          "form": "맛있습니다 ㅎㅎ",
          "original_form": "맛있습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037632.1.20",
          "form": "통새우 나온지 몇년됐어요 !",
          "original_form": "통새우 나온지 몇년됐어요 !",
          "speaker_id": "1",
          "time": "20210901 14:07"
        }
