          "speaker_id": "2",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003117.1.2",
          "form": "난 여름 젤좋음",
          "original_form": "난 여름 젤좋음",
          "speaker_id": "2",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100003117.1.3",
          "form": "헐 난 여름이 제일 싫어ㅎ",
          "original_form": "헐 난 여름이 제일 싫어ㅎ",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003117.1.4",
          "form": "왜 여름이 좋아?",
          "original_form": "왜 여름이 좋아?",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003117.1.5",
          "form": "물놀이 좋아해서",
          "original_form": "물놀이 좋아해서",
          "speaker_id": "2",
          "time": "20210820 18:56"
        },
        {
          "id": "MDRW2100003117.1.6",
          "form": "가을되면 가을탐",
          "original_form": "가을되면 가을탐",
          "speaker_id": "2",
          "time": "20210820 18:56"
        },
        {
          "id": "MDRW2100003117.1.7",
          "form": "물놀이 좋지",
          "original_form": "물놀이 좋지",
          "speaker_id": "1",
          "time": "20210820 18:58"
        },
        {
          "id": "MDRW2100003117.1.8",
          "form": "작년부터 물놀이를 못 가서 넘 아쉽다",
          "original_form": "작년부터 물놀이를 못 가서 넘 아쉽다",
          "speaker_id": "1",
          "time": "20210820 18:58"
        },
        {
          "id": "MDRW2100003117.1.9",
          "form": "난 겨울엔 추우면 껴입으면 되는데 여름은 뭘 할 수가 없어서 싫어ㅜ",
          "original_form": "난 겨울엔 추우면 껴입으면 되는데 여름은 뭘 할 수가 없어서 싫어ㅜ",
          "speaker_id": "1",
          "time": "20210820 18:58"
        },
        {
          "id": "MDRW2100003117.1.10",
          "form": "난 추위를 너무타서",
          "original_form": "난 추위를 너무타서",
          "speaker_id": "2",
          "time": "20210820 19:04"
        },
        {
          "id": "MDRW2100003117.1.11",
          "form": "겨울엔 놀이할것도없음",
          "original_form": "겨울엔 놀이할것도없음",
          "speaker_id": "2",
          "time": "20210820 19:04"
        },
        {
          "id": "MDRW2100003117.1.12",
          "form": "아쉽네ㅜㅠ",
          "original_form": "아쉽네ㅜㅠ",
          "speaker_id": "1",
          "time": "20210820 19:05"
        },
        {
          "id": "MDRW2100003117.1.13",
          "form": "난 고구마랑 귤 처럼 겨울 간식도 너무 좋아",
          "original_form": "난 고구마랑 귤 처럼 겨울 간식도 너무 좋아",
          "speaker_id": "1",
          "time": "20210820 19:05"
        },
        {
          "id": "MDRW2100003117.1.14",
          "form": "ㅋㅋ밤고구마? 호박고구마?",
          "original_form": "ㅋㅋ밤고구마? 호박고구마?",
          "speaker_id": "2",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003117.1.15",
          "form": "난 완전 밤고구마!",
          "original_form": "난 완전 밤고구마!",
          "speaker_id": "1",
          "time": "20210820 19:28"
        },
        {
          "id": "MDRW2100003117.1.16",
          "form": "오 나두",
          "original_form": "오 나두",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003117.1.17",
          "form": "우와 밤고구마 좋아하는 사람 별로 없던데",
          "original_form": "우와 밤고구마 좋아하는 사람 별로 없던데",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003117.1.18",
          "form": "반갑다ㅎㅎ",
          "original_form": "반갑다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003117.1.19",
          "form": "날씨가 선풍기를 키면 춥고 끄면 덥네",
          "original_form": "날씨가 선풍기를 키면 춥고 끄면 덥네",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003117.1.20",
          "form": "밤에 선선하더라",
          "original_form": "밤에 선선하더라",
          "speaker_id": "2",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003117.1.21",
          "form": "맞아 그래서 이불도 살짝 더 두꺼운 걸로 바꿨는데 딱 좋아",
          "original_form": "맞아 그래서 이불도 살짝 더 두꺼운 걸로 바꿨는데 딱 좋아",
          "speaker_id": "1",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003117.1.22",
          "form": "올해도 여름은 끝이나네 ㅜ",
          "original_form": "올해도 여름은 끝이나네 ㅜ",
          "speaker_id": "2",
          "time": "20210820 21:17"
        }
      ]
