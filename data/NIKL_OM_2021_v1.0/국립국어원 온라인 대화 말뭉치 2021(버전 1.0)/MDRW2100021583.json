      "utterance": [
        {
          "id": "MDRW2100021583.1.1",
          "form": "집안은 화목한 편인가요?",
          "original_form": "집안은 화목한 편인가요?",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100021583.1.2",
          "form": "제 생각엔? 그런편같아요",
          "original_form": "제 생각엔? 그런편같아요",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021583.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021583.1.4",
          "form": "저희 집은 조금 다툼이 잦은 편이세요",
          "original_form": "저희 집은 조금 다툼이 잦은 편이세요",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021583.1.5",
          "form": "저희도 다툼은ㅎㅎ",
          "original_form": "저희도 다툼은ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021583.1.6",
          "form": "정치적으로나 결혼관 돈문제 사유는 다양하죠ㅜㅋㅋ",
          "original_form": "정치적으로나 결혼관 돈문제 사유는 다양하죠ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021583.1.7",
          "form": "오 ㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021583.1.8",
          "form": "근데 특히 돈 문제 때문에 자주 다투는 것 같아요",
          "original_form": "근데 특히 돈 문제 때문에 자주 다투는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021583.1.9",
          "form": "그니까요 넉넉하다면 그냥 넘길 수 있을텐데 그게 안되니까ㅜㅠ",
          "original_form": "그니까요 넉넉하다면 그냥 넘길 수 있을텐데 그게 안되니까ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021583.1.10",
          "form": "님네 가족도 빚이 있으신가요?",
          "original_form": "님네 가족도 빚이 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021583.1.11",
          "form": "빚은 있죠ㅎㅎ",
          "original_form": "빚은 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021583.1.12",
          "form": "아이구ㅜ 역시 빚 없는 집은 드문가봐요",
          "original_form": "아이구ㅜ 역시 빚 없는 집은 드문가봐요",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021583.1.13",
          "form": "저희 가족도 아파트 사느라 빚 내셨대요",
          "original_form": "저희 가족도 아파트 사느라 빚 내셨대요",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021583.1.14",
          "form": "집값 많이 올랐잖아요!",
          "original_form": "집값 많이 올랐잖아요!",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021583.1.15",
          "form": "투자죠ㅜㅋㅋ",
          "original_form": "투자죠ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021583.1.16",
          "form": "ㅋㅋㅋㅋ안타깝게도 저희집은 별로 안올랐어요..",
          "original_form": "ㅋㅋㅋㅋ안타깝게도 저희집은 별로 안올랐어요..",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021583.1.17",
          "form": "재테크나 투자에 관심 없으신 분들이셔서..",
          "original_form": "재테크나 투자에 관심 없으신 분들이셔서..",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021583.1.18",
          "form": "그래도 실거주면 괜찮아요",
          "original_form": "그래도 실거주면 괜찮아요",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021583.1.19",
          "form": "ㅋㅋㅋ아 실거주 목적으로 사신거에요...",
          "original_form": "ㅋㅋㅋ아 실거주 목적으로 사신거에요...",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021583.1.20",
          "form": "한 20년 정도 살았던 것 같네요",
          "original_form": "한 20년 정도 살았던 것 같네요",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021583.1.21",
          "form": "그럼 조만간 재건축하지않을까요?",
          "original_form": "그럼 조만간 재건축하지않을까요?",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100021583.1.22",
          "form": "전 주택사는데 30년가까이 살았어요ㅎㅎ",
          "original_form": "전 주택사는데 30년가까이 살았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021583.1.23",
          "form": "와우 지금도 주택 살아요?",
          "original_form": "와우 지금도 주택 살아요?",
          "speaker_id": "1",
          "time": "20210828 21:10"
        }
      ]
