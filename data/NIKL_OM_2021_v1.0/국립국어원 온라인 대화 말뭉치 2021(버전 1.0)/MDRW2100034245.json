      "utterance": [
        {
          "id": "MDRW2100034245.1.1",
          "form": "다 좋긴했는데",
          "original_form": "다 좋긴했는데",
          "speaker_id": "2",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100034245.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100034245.1.3",
          "form": "데이식스는 팬인 친구따라 간거였는대도",
          "original_form": "데이식스는 팬인 친구따라 간거였는대도",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.4",
          "form": "너무 좋더라구요",
          "original_form": "너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.5",
          "form": "입덕한 상태도 아니었는데",
          "original_form": "입덕한 상태도 아니었는데",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.6",
          "form": "와그래요?",
          "original_form": "와그래요?",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.7",
          "form": "데이식스면 밴드공연이죠?",
          "original_form": "데이식스면 밴드공연이죠?",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.8",
          "form": "막 얼굴에서 빛이 나더라구요ㅋㅋㅋ",
          "original_form": "막 얼굴에서 빛이 나더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.9",
          "form": "네 밴드요ㅋㅋ",
          "original_form": "네 밴드요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.10",
          "form": "오호 ㅋㅋㅋㅋ",
          "original_form": "오호 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.11",
          "form": "밴드는 그리고보니 본적이 없네요 저는",
          "original_form": "밴드는 그리고보니 본적이 없네요 저는",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034245.1.12",
          "form": "밴드는 많지가 않아서 그럴수있죠",
          "original_form": "밴드는 많지가 않아서 그럴수있죠",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.13",
          "form": "그런가봐요 ㅋㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.14",
          "form": "옆에 팬분이랑도 얘기하고",
          "original_form": "옆에 팬분이랑도 얘기하고",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.15",
          "form": "와우 ㅋㅋㅋ",
          "original_form": "와우 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.16",
          "form": "생각보다 재밌었어요",
          "original_form": "생각보다 재밌었어요",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.17",
          "form": "두루두루 얘기하며 보면 더 재밌죠",
          "original_form": "두루두루 얘기하며 보면 더 재밌죠",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.18",
          "form": "친구랑 떨어져앉아서",
          "original_form": "친구랑 떨어져앉아서",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.19",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.20",
          "form": "좀 그렇겠다했는데",
          "original_form": "좀 그렇겠다했는데",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034245.1.21",
          "form": "팬분이 말걸어주셔서",
          "original_form": "팬분이 말걸어주셔서",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034245.1.22",
          "form": "재밌게 놀다왔어요",
          "original_form": "재밌게 놀다왔어요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034245.1.23",
          "form": "말걸어주면 저는 너무 고맙더라구요 ㅋㅋㅋ",
          "original_form": "말걸어주면 저는 너무 고맙더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:58"
        }
      ]
