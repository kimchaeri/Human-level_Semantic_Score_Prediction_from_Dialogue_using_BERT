      "utterance": [
        {
          "id": "MDRW2100044949.1.1",
          "form": "요즘 보는 프로그램 있으신가요?",
          "original_form": "요즘 보는 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044949.1.2",
          "form": "저는 유퀴즈요",
          "original_form": "저는 유퀴즈요",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044949.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044949.1.4",
          "form": "저는 유퀴즈랑 환승연애 보고있습니다!",
          "original_form": "저는 유퀴즈랑 환승연애 보고있습니다!",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044949.1.5",
          "form": "환승연애 보셨나요?",
          "original_form": "환승연애 보셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044949.1.6",
          "form": "클립은 몇번 봤어요",
          "original_form": "클립은 몇번 봤어요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044949.1.7",
          "form": "오 재밌더라구요",
          "original_form": "오 재밌더라구요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044949.1.8",
          "form": "유퀴즈 양궁선수 나온거 보셨나요?",
          "original_form": "유퀴즈 양궁선수 나온거 보셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044949.1.9",
          "form": "아니요 재밌나요?",
          "original_form": "아니요 재밌나요?",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044949.1.10",
          "form": "네 재밌게 봤어요",
          "original_form": "네 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100044949.1.11",
          "form": "도쿄 올림픽 보셨나요?",
          "original_form": "도쿄 올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100044949.1.12",
          "form": "네 봤어요",
          "original_form": "네 봤어요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100044949.1.13",
          "form": "뒷 얘기들이 많아서 유퀴즈 재밌더라구요!",
          "original_form": "뒷 얘기들이 많아서 유퀴즈 재밌더라구요!",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100044949.1.14",
          "form": "그러니까요 뭔가 쉽게 볼 수 있어서 좋아요",
          "original_form": "그러니까요 뭔가 쉽게 볼 수 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100044949.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100044949.1.16",
          "form": "영화도 자주 보시나요?",
          "original_form": "영화도 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100044949.1.17",
          "form": "제가 영화를 잘 안봐요",
          "original_form": "제가 영화를 잘 안봐요",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100044949.1.18",
          "form": "오 그럼 기억에 남는 영화 있으신가요?",
