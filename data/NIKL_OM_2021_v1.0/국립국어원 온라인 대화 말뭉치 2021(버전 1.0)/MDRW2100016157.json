      "utterance": [
        {
          "id": "MDRW2100016157.1.1",
          "form": "최근 잘샀다 생각하시는 꿀템있으세요?ㅎ",
          "original_form": "최근 잘샀다 생각하시는 꿀템있으세요?ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016157.1.2",
          "form": "최근에 잘샀다라..음 최근에는 쇼핑한건 없고",
          "original_form": "최근에 잘샀다라..음 최근에는 쇼핑한건 없고",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016157.1.3",
          "form": "어.. 돌돌이..?",
          "original_form": "어.. 돌돌이..?",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016157.1.4",
          "form": "그 바닥미는거쇼",
          "original_form": "그 바닥미는거쇼",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016157.1.5",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016157.1.6",
          "form": "다이소에서 사신건가요? ㅋㅋ",
          "original_form": "다이소에서 사신건가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016157.1.7",
          "form": "아니요 다이소꺼는 너무 스레기라..",
          "original_form": "아니요 다이소꺼는 너무 스레기라..",
          "speaker_id": "2",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016157.1.8",
          "form": "저는 음 아 신일 선풍기 어제 배달왔어요",
          "original_form": "저는 음 아 신일 선풍기 어제 배달왔어요",
          "speaker_id": "1",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016157.1.9",
          "form": "그럼 어디꺼가 좋나요? ㅎㅎ",
          "original_form": "그럼 어디꺼가 좋나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016157.1.10",
          "form": "인터넷에서 그 어디꺼지 그랩코꺼샀어요",
          "original_form": "인터넷에서 그 어디꺼지 그랩코꺼샀어요",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016157.1.11",
          "form": "원래 고로고로라고 일제꺼썼는데 비싸서 ㅎ",
          "original_form": "원래 고로고로라고 일제꺼썼는데 비싸서 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016157.1.12",
          "form": "name1은 최근 구매하신거 있으세요?",
          "original_form": "&name1&은 최근 구매하신거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016157.1.13",
          "form": "아 선풍기사셨다고 하셨구나",
          "original_form": "아 선풍기사셨다고 하셨구나",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016157.1.14",
          "form": "어떠세요?  저도 선풍기하나 사야하는데",
          "original_form": "어떠세요?  저도 선풍기하나 사야하는데",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016157.1.15",
          "form": "철이 좀 지나긴했죠 ㅋㅋ",
          "original_form": "철이 좀 지나긴했죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016157.1.16",
          "form": "이제 곧 가을이라 ㅋㅋ",
          "original_form": "이제 곧 가을이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016157.1.17",
          "form": "근데 잘산거같아욥",
          "original_form": "근데 잘산거같아욥",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016157.1.18",
          "form": "그래도 간간히 좀 더워서",
          "original_form": "그래도 간간히 좀 더워서",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016157.1.19",
          "form": "아직은 필요한거같아요",
          "original_form": "아직은 필요한거같아요",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016157.1.20",
          "form": "내년여름 생각하면 잘 사신거에요",
          "original_form": "내년여름 생각하면 잘 사신거에요",
          "speaker_id": "2",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016157.1.21",
          "form": "선풍기의 어떤점이 마음에들었나요",
          "original_form": "선풍기의 어떤점이 마음에들었나요",
          "speaker_id": "2",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016157.1.22",
          "form": "음 가격?",
          "original_form": "음 가격?",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016157.1.23",
          "form": "94000원인데",
          "original_form": "94000원인데",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016157.1.24",
          "form": "82000에 산점? ㅋㅋㅋ",
          "original_form": "82000에 산점? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016157.1.25",
          "form": "값이 꽤 나가네요",
          "original_form": "값이 꽤 나가네요",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016157.1.26",
          "form": "오 합리적인 가격에 구매하셨어요",
          "original_form": "오 합리적인 가격에 구매하셨어요",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016157.1.27",
          "form": "모터는 잘 돌아가나요",
          "original_form": "모터는 잘 돌아가나요",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016157.1.28",
          "form": "원래 어느 정도 하는지 시세를 모르겠어서요",
          "original_form": "원래 어느 정도 하는지 시세를 모르겠어서요",
          "speaker_id": "1",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016157.1.29",
          "form": "그냥 사셨구나 잘 사신거면 된거죠",
          "original_form": "그냥 사셨구나 잘 사신거면 된거죠",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016157.1.30",
          "form": "만족하시면 된거아니겠습니까ㅎ",
          "original_form": "만족하시면 된거아니겠습니까ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100016157.1.31",
          "form": "무소음이라고 하더라고요",
          "original_form": "무소음이라고 하더라고요",
          "speaker_id": "1",
          "time": "20210826 10:43"
        },
        {
          "id": "MDRW2100016157.1.32",
          "form": "장점이크네요",
          "original_form": "장점이크네요",
