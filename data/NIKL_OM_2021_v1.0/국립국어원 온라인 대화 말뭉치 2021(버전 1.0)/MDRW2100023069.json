      "utterance": [
        {
          "id": "MDRW2100023069.1.1",
          "form": "응 별로 안좋아해",
          "original_form": "응 별로 안좋아해",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023069.1.2",
          "form": "홍차도 안마셔?",
          "original_form": "홍차도 안마셔?",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100023069.1.3",
          "form": "홍차는 가끔 마셔",
          "original_form": "홍차는 가끔 마셔",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023069.1.4",
          "form": "아 그래? 한 번도 못 본 거 같은데",
          "original_form": "아 그래? 한 번도 못 본 거 같은데",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023069.1.5",
          "form": "예전에  몇 번 먹었던거 같은데",
          "original_form": "예전에  몇 번 먹었던거 같은데",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023069.1.6",
          "form": "얼그레이 디저트도 좋아해 그럼?",
          "original_form": "얼그레이 디저트도 좋아해 그럼?",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023069.1.7",
          "form": "예전에 몇 번 먹었던거 같은데",
          "original_form": "예전에 몇 번 먹었던거 같은데",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023069.1.8",
          "form": "의외다 난 싫어하는 줄 알았어",
          "original_form": "의외다 난 싫어하는 줄 알았어",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023069.1.9",
          "form": "가끔 먹어",
          "original_form": "가끔 먹어",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023069.1.10",
          "form": "다음에 먹으러 가자",
          "original_form": "다음에 먹으러 가자",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023069.1.11",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023069.1.12",
          "form": "카페같은 곳",
          "original_form": "카페같은 곳",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023069.1.13",
          "form": "오 거기 맛있는 데야?",
          "original_form": "오 거기 맛있는 데야?",
