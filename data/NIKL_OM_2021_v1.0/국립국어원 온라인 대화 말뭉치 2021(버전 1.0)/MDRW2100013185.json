      "utterance": [
        {
          "id": "MDRW2100013185.1.1",
          "form": "건강을 위해 어떤걸 하시나요?",
          "original_form": "건강을 위해 어떤걸 하시나요?",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013185.1.2",
          "form": "저는 요즘 운동하고 있어요~!",
          "original_form": "저는 요즘 운동하고 있어요~!",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013185.1.3",
          "form": "다이어트를 하시는군요 운동 힘들텐데 멋있으세요",
          "original_form": "다이어트를 하시는군요 운동 힘들텐데 멋있으세요",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013185.1.4",
          "form": "아 다이어트는 아니고 체력 증진 목적이요",
          "original_form": "아 다이어트는 아니고 체력 증진 목적이요",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013185.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013185.1.6",
          "form": "저는 운동 귀찮아해서 집에서 푸쉬업만 하고있어요",
          "original_form": "저는 운동 귀찮아해서 집에서 푸쉬업만 하고있어요",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013185.1.7",
          "form": "요즘 무기력해서 운동은 해야겠고..  푸쉬업 시작했어요",
          "original_form": "요즘 무기력해서 운동은 해야겠고..  푸쉬업 시작했어요",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013185.1.8",
          "form": "어떤 운동하시는데요?",
          "original_form": "어떤 운동하시는데요?",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013185.1.9",
          "form": "저는 복싱해요",
          "original_form": "저는 복싱해요",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013185.1.10",
          "form": "복싱장 가면 늘 푸쉬업하는데 반갑네요",
          "original_form": "복싱장 가면 늘 푸쉬업하는데 반갑네요",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013185.1.11",
          "form": "푸쉬업바 사용하시나요?",
          "original_form": "푸쉬업바 사용하시나요?",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013185.1.12",
          "form": "손목 다칠까봐 그냥 해요 기구사용하는건 안좋은거같아요",
          "original_form": "손목 다칠까봐 그냥 해요 기구사용하는건 안좋은거같아요",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013185.1.13",
          "form": "아 오히려 바닥에서 하는게 더 무리가지 않나요?",
          "original_form": "아 오히려 바닥에서 하는게 더 무리가지 않나요?",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013185.1.14",
          "form": "저는 그래서 집안에 하나 들일까 고민중이었는데",
          "original_form": "저는 그래서 집안에 하나 들일까 고민중이었는데",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013185.1.15",
          "form": "손목보호대를 사용하죠 무리하게 안됩니다",
          "original_form": "손목보호대를 사용하죠 무리하게 안됩니다",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013185.1.16",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013185.1.17",
          "form": "몸조심해야죠",
          "original_form": "몸조심해야죠",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013185.1.18",
          "form": "맨몸운동을 위주로 하는데  운동 추천좀 해주셍6",
          "original_form": "맨몸운동을 위주로 하는데  운동 추천좀 해주셍6",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013185.1.19",
          "form": "어떤 운동을 해야 체력이 좋아질까요?",
          "original_form": "어떤 운동을 해야 체력이 좋아질까요?",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013185.1.20",
          "form": "복싱하세요 체력 많이 늡니다 ㅋㅋ",
          "original_form": "복싱하세요 체력 많이 늡니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013185.1.21",
          "form": "해봤는데 1주일하고 ㅋㅋ 접은거같습니다 저랑 안맞아요",
          "original_form": "해봤는데 1주일하고 ㅋㅋ 접은거같습니다 저랑 안맞아요",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013185.1.22",
          "form": "맨몸운동은 저는 버피할 때 제일 힘들던데요",
