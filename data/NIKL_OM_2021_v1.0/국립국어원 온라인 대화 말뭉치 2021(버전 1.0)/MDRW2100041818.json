      "utterance": [
        {
          "id": "MDRW2100041818.1.1",
          "form": "어제까지 좋았던 날씨가 흐려졌네요",
          "original_form": "어제까지 좋았던 날씨가 흐려졌네요",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041818.1.2",
          "form": "거기 날씨는 어떤가요?",
          "original_form": "거기 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041818.1.3",
          "form": "여기도 오늘 하루종일 가랑비가 내려요 \r\n날씨도 좀 서늘해진 것 같아요",
          "original_form": "여기도 오늘 하루종일 가랑비가 내려요 \r\n날씨도 좀 서늘해진 것 같아요",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100041818.1.4",
          "form": "풋사랑님 계신 곳도 비가 오나요?",
          "original_form": "풋사랑님 계신 곳도 비가 오나요?",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041818.1.5",
          "form": "이제 비가오기 시작하네요",
          "original_form": "이제 비가오기 시작하네요",
          "speaker_id": "1",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100041818.1.6",
          "form": "가을 장마가 끝난줄알았는데 비가 또 오네요",
          "original_form": "가을 장마가 끝난줄알았는데 비가 또 오네요",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100041818.1.7",
          "form": "그러게요 가을에 유독 비가 많이 내리는 것 같아요 ㅠㅠ",
          "original_form": "그러게요 가을에 유독 비가 많이 내리는 것 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100041818.1.8",
          "form": "바람도 꽤 불어서 추워요 ㅠㅠ",
          "original_form": "바람도 꽤 불어서 추워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100041818.1.9",
          "form": "이 비가 끝나면 좀더 쌀쌀해질것같죠?",
          "original_form": "이 비가 끝나면 좀더 쌀쌀해질것같죠?",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100041818.1.10",
          "form": "맞아요 금방 겨울이 올 것 같아요~",
          "original_form": "맞아요 금방 겨울이 올 것 같아요~",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041818.1.11",
          "form": "풋사랑님은 어느계절 좋아하세요?",
          "original_form": "풋사랑님은 어느계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041818.1.12",
          "form": "저는 가을이 좋아요",
          "original_form": "저는 가을이 좋아요",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100041818.1.13",
          "form": "올해 여름이 너무 싫어졌어요",
          "original_form": "올해 여름이 너무 싫어졌어요",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100041818.1.14",
          "form": "맞아요 올여름 짧게짧게 엄청 더웠었어요 ㅠㅠㅠ",
          "original_form": "맞아요 올여름 짧게짧게 엄청 더웠었어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100041818.1.15",
          "form": "가을 좋은데 갈수록 짧아져서 아쉬워요 ㅠㅠ 지금도 비가 자꾸와서 가을을 오롯이 느낄 수 없어 아쉬워요",
          "original_form": "가을 좋은데 갈수록 짧아져서 아쉬워요 ㅠㅠ 지금도 비가 자꾸와서 가을을 오롯이 느낄 수 없어 아쉬워요",
          "speaker_id": "2",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100041818.1.16",
          "form": "파란하늘도 잠깐이엇어요",
          "original_form": "파란하늘도 잠깐이엇어요",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100041818.1.17",
          "form": "맞아요 파란하늘 보였을때 너무 좋았는데 지금은 너무 우중충해서 기분도 그런 것 같아요 ㅠㅠ",
          "original_form": "맞아요 파란하늘 보였을때 너무 좋았는데 지금은 너무 우중충해서 기분도 그런 것 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100041818.1.18",
          "form": "습해서 그런지 기분도 처지네요",
          "original_form": "습해서 그런지 기분도 처지네요",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100041818.1.19",
          "form": "맞아요 오늘 유독 습하더라구요 ㅠㅠ\r\n날씨가 흐리고 습하면 기분이 덩달아 안좋아지는건 어쩔 수 없나봐요 ㅠㅠㅠ",
          "original_form": "맞아요 오늘 유독 습하더라구요 ㅠㅠ\r\n날씨가 흐리고 습하면 기분이 덩달아 안좋아지는건 어쩔 수 없나봐요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100041818.1.20",
          "form": "이럴때 맛있는거 먹고 기분전환이 필요할 것 같아요 ㅎㅎ",
          "original_form": "이럴때 맛있는거 먹고 기분전환이 필요할 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100041818.1.21",
          "form": "내일은 비가 그만왔으면좋겠어요",
          "original_form": "내일은 비가 그만왔으면좋겠어요",
