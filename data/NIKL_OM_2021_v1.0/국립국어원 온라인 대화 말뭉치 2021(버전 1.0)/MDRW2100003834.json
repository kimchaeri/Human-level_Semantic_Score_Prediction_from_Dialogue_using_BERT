      "utterance": [
        {
          "id": "MDRW2100003834.1.1",
          "form": "요즘 날씨가 선선하니 좋네요",
          "original_form": "요즘 날씨가 선선하니 좋네요",
          "speaker_id": "1",
          "time": "20210821 12:42"
        },
        {
          "id": "MDRW2100003834.1.2",
          "form": "맞아요~ 이번주가 가을 장마 기간이여서 그런지 더 좋은것같아요~~",
          "original_form": "맞아요~ 이번주가 가을 장마 기간이여서 그런지 더 좋은것같아요~~",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100003834.1.3",
          "form": "어쩐지 비가 억수로 내리던데",
          "original_form": "어쩐지 비가 억수로 내리던데",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100003834.1.4",
          "form": "가을 장마였네요",
          "original_form": "가을 장마였네요",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100003834.1.5",
          "form": "네~ 다시 비가 멈추었는데 햇빛이 쨍하네요~@!",
          "original_form": "네~ 다시 비가 멈추었는데 햇빛이 쨍하네요~@!",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100003834.1.6",
          "form": "새벽엔 진짜 하늘에 구멍난 줄 알았어요",
          "original_form": "새벽엔 진짜 하늘에 구멍난 줄 알았어요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100003834.1.7",
          "form": "흐 맞아요ㅠ 진짜 비가 이번달 들어서 제일 심하게 온것같더라구요ㅠ",
          "original_form": "흐 맞아요ㅠ 진짜 비가 이번달 들어서 제일 심하게 온것같더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100003834.1.8",
          "form": "지금은 비온것도 모를정도로 그쳤네요~!",
          "original_form": "지금은 비온것도 모를정도로 그쳤네요~!",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100003834.1.9",
          "form": "다음주내내 온다네요",
          "original_form": "다음주내내 온다네요",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100003834.1.10",
          "form": "이 장마가 끝나면 금새 추워지겠어요",
          "original_form": "이 장마가 끝나면 금새 추워지겠어요",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100003834.1.11",
          "form": "맞아요~ 가을은 정말 짧아서 여름 다음 바로 겨율같아요~",
          "original_form": "맞아요~ 가을은 정말 짧아서 여름 다음 바로 겨율같아요~",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100003834.1.12",
          "form": "가을이 선선하고 쾌청해서 제일 좋은데",
          "original_form": "가을이 선선하고 쾌청해서 제일 좋은데",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100003834.1.13",
          "form": "올 겨울도 추울것같아요",
          "original_form": "올 겨울도 추울것같아요",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100003834.1.14",
          "form": "맞아여ㅠㅠ 겨울대비하려니",
          "original_form": "맞아여ㅠㅠ 겨울대비하려니",
          "speaker_id": "2",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100003834.1.15",
          "form": "벌써 춥네요ㅠ 요즘도 가끔은 춥더라구요!",
          "original_form": "벌써 춥네요ㅠ 요즘도 가끔은 춥더라구요!",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100003834.1.16",
          "form": "그치만 가을이 선선하고 습기없어 좋아용ㅎㅎ",
          "original_form": "그치만 가을이 선선하고 습기없어 좋아용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100003834.1.17",
          "form": "여름보다는 겨울이 좋아요 가을은 더 좋구요",
          "original_form": "여름보다는 겨울이 좋아요 가을은 더 좋구요",
          "speaker_id": "1",
          "time": "20210823 11:13"
        },
        {
          "id": "MDRW2100003834.1.18",
          "form": "요즘은 날씨가 제멋대로예요",
          "original_form": "요즘은 날씨가 제멋대로예요",
          "speaker_id": "1",
          "time": "20210823 11:14"
        },
        {
          "id": "MDRW2100003834.1.19",
          "form": "가장 좋아하는 계절이 가을이시겠어용~~",
          "original_form": "가장 좋아하는 계절이 가을이시겠어용~~",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100003834.1.20",
          "form": "그쵸ㅠ 비왔다 살짝 더웠다ㅠ",
          "original_form": "그쵸ㅠ 비왔다 살짝 더웠다ㅠ",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100003834.1.21",
          "form": "옷차림도 애매하더르구요ㅠ",
          "original_form": "옷차림도 애매하더르구요ㅠ",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100003834.1.22",
          "form": "네 가을 좋아해요",
          "original_form": "네 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100003834.1.23",
          "form": "곧 태풍 온다네요",
          "original_form": "곧 태풍 온다네요",
          "speaker_id": "1",
          "time": "20210823 19:36"
        }
      ]
    }
