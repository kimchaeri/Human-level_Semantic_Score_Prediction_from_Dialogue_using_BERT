      "utterance": [
        {
          "id": "MDRW2100034209.1.1",
          "form": "연극도 요새 유투브로해주던데요",
          "original_form": "연극도 요새 유투브로해주던데요",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100034209.1.2",
          "form": "오페라의유령하고 캣츠요",
          "original_form": "오페라의유령하고 캣츠요",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100034209.1.3",
          "form": "와 한번 찾아봐아겠어요!!",
          "original_form": "와 한번 찾아봐아겠어요!!",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100034209.1.4",
          "form": "기간오픈이었던거 같아요",
          "original_form": "기간오픈이었던거 같아요",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100034209.1.5",
          "form": "함찾아보세요",
          "original_form": "함찾아보세요",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100034209.1.6",
          "form": "감사해요ㅋㅋㅋ",
          "original_form": "감사해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034209.1.7",
          "form": "좋은정보 얻어갑니다ㅎㅎ",
          "original_form": "좋은정보 얻어갑니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034209.1.8",
          "form": "저는 창극을 찾아봐서 패왕별희랑 적벽도 하더라구요",
          "original_form": "저는 창극을 찾아봐서 패왕별희랑 적벽도 하더라구요",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034209.1.9",
          "form": "원래 극장가서 봤던 극인데 쩔수없이 온라인으로 봤는데 좀 심심했던거같아요ㅜㅜ",
          "original_form": "원래 극장가서 봤던 극인데 쩔수없이 온라인으로 봤는데 좀 심심했던거같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100034209.1.10",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034209.1.11",
          "form": "스피커로 듣는거랑",
          "original_form": "스피커로 듣는거랑",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034209.1.12",
          "form": "현장이랑은 너무너무 다르더라구요ㅜ",
          "original_form": "현장이랑은 너무너무 다르더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034209.1.13",
          "form": "왜 돈 주고 보는지 알겠더라구요ㅎ",
          "original_form": "왜 돈 주고 보는지 알겠더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034209.1.14",
          "form": "현장소리도좋고 또 배우들도 관객이 있어야 힘이난대요",
          "original_form": "현장소리도좋고 또 배우들도 관객이 있어야 힘이난대요",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100034209.1.15",
          "form": "역시ㅋㅋㅋ",
          "original_form": "역시ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100034209.1.16",
          "form": "괜히 응원상이란게 있는게 아닌가봐요ㅋㅋ",
          "original_form": "괜히 응원상이란게 있는게 아닌가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100034209.1.17",
          "form": "응원상  오오",
          "original_form": "응원상  오오",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034209.1.18",
          "form": "로키호러쇼 보셨나요 뮤지컬",
          "original_form": "로키호러쇼 보셨나요 뮤지컬",
          "speaker_id": "1",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100034209.1.19",
          "form": "관객들 일어나서 같이 춤춰요",
          "original_form": "관객들 일어나서 같이 춤춰요",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100034209.1.20",
          "form": "아뇨ㅜ",
          "original_form": "아뇨ㅜ",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100034209.1.21",
          "form": "이시국엔 못 하겠죠ㅜ",
          "original_form": "이시국엔 못 하겠죠ㅜ",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100034209.1.22",
          "form": "헤드윅도 관객들이랑 호흡하는게 많다고 하던데ㅜ",
          "original_form": "헤드윅도 관객들이랑 호흡하는게 많다고 하던데ㅜ",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100034209.1.23",
          "form": "저 로키호러쇼 춤 열심히 연습해가서 ㅋㅋ 얼굴에 철판깔고 췄는데ㅋㅋ배우분들힘내라구ㅋㅋ다 추억이네요 헤드윅도 그런프로그램이 있군요",
          "original_form": "저 로키호러쇼 춤 열심히 연습해가서 ㅋㅋ 얼굴에 철판깔고 췄는데ㅋㅋ배우분들힘내라구ㅋㅋ다 추억이네요 헤드윅도 그런프로그램이 있군요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100034209.1.24",
          "form": "와 신기합니다!!!",
          "original_form": "와 신기합니다!!!",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
