      "utterance": [
        {
          "id": "MDRW2100020437.1.1",
          "form": "두살 차이나요",
          "original_form": "두살 차이나요",
          "speaker_id": "2",
          "time": "20210828 05:39"
        },
        {
          "id": "MDRW2100020437.1.2",
          "form": "오호 두살차이 딱 좋죠",
          "original_form": "오호 두살차이 딱 좋죠",
          "speaker_id": "1",
          "time": "20210828 05:39"
        },
        {
          "id": "MDRW2100020437.1.3",
          "form": "저도 구남친 두살 연상이었어요!",
          "original_form": "저도 구남친 두살 연상이었어요!",
          "speaker_id": "1",
          "time": "20210828 05:39"
        },
        {
          "id": "MDRW2100020437.1.4",
          "form": "으왕 헤어지셨군요",
          "original_form": "으왕 헤어지셨군요",
          "speaker_id": "2",
          "time": "20210828 05:39"
        },
        {
          "id": "MDRW2100020437.1.5",
          "form": "근데 남친 새로 생길때마다 뭔가 업그레이드 되더라구요",
          "original_form": "근데 남친 새로 생길때마다 뭔가 업그레이드 되더라구요",
          "speaker_id": "2",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.6",
          "form": "네네ㅠㅠ 집착하고 그런거 싫어하는데",
          "original_form": "네네ㅠㅠ 집착하고 그런거 싫어하는데",
          "speaker_id": "1",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.7",
          "form": "보는 안목이 점점 생겨서 그런가",
          "original_form": "보는 안목이 점점 생겨서 그런가",
          "speaker_id": "2",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.8",
          "form": "집착하려고 들어서 찼어요ㅋㅋㅋㅋㅋ",
          "original_form": "집착하려고 들어서 찼어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.9",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.10",
          "form": "ㅋㅋㅋㅋ잘하셨네요",
          "original_form": "ㅋㅋㅋㅋ잘하셨네요",
          "speaker_id": "2",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.11",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.12",
          "form": "그래서 더 연애텀이 길어지는것같아요",
          "original_form": "그래서 더 연애텀이 길어지는것같아요",
          "speaker_id": "1",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.13",
          "form": "이제 더 좋은 인연 만날거에요",
          "original_form": "이제 더 좋은 인연 만날거에요",
          "speaker_id": "2",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.14",
          "form": "거르고 거르느라ㅋㅋ",
          "original_form": "거르고 거르느라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.15",
          "form": "맞아요ㅋㅋㅋ 아예 안하면 안했지",
          "original_form": "맞아요ㅋㅋㅋ 아예 안하면 안했지",
          "speaker_id": "2",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.16",
          "form": "이상한 사람 만나면 안돼요 ㅋㅋㅋ",
          "original_form": "이상한 사람 만나면 안돼요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:40"
        },
        {
          "id": "MDRW2100020437.1.17",
          "form": "그쵸 요즘 이상한 사람 너무 많고",
          "original_form": "그쵸 요즘 이상한 사람 너무 많고",
          "speaker_id": "1",
          "time": "20210828 05:41"
        },
        {
          "id": "MDRW2100020437.1.18",
          "form": "흉악한 기사도 많이 뜨구요ㅠ",
          "original_form": "흉악한 기사도 많이 뜨구요ㅠ",
          "speaker_id": "1",
          "time": "20210828 05:41"
        },
        {
          "id": "MDRW2100020437.1.19",
          "form": "그니까요 ㅠㅠ",
          "original_form": "그니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 05:41"
        },
        {
          "id": "MDRW2100020437.1.20",
          "form": "최근에두 그 살인 ㅠㅠ",
          "original_form": "최근에두 그 살인 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 05:41"
        },
        {
          "id": "MDRW2100020437.1.21",
          "form": "아 맞아요ㅠㅠ 저도",
          "original_form": "아 맞아요ㅠㅠ 저도",
          "speaker_id": "1",
          "time": "20210828 05:41"
        },
        {
          "id": "MDRW2100020437.1.22",
          "form": "청원하고 왔어요..",
          "original_form": "청원하고 왔어요..",
          "speaker_id": "1",
          "time": "20210828 05:41"
        }
      ]
