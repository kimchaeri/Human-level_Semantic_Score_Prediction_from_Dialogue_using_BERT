      "utterance": [
        {
          "id": "MDRW2100027558.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027558.1.2",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027558.1.3",
          "form": "점심 드셨나용",
          "original_form": "점심 드셨나용",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027558.1.4",
          "form": "저는 아점 빵 먹었어요",
          "original_form": "저는 아점 빵 먹었어요",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027558.1.5",
          "form": "name2은 뭐 드셨나요??",
          "original_form": "&name2&은 뭐 드셨나요??",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027558.1.6",
          "form": "저는 다이어트중이라",
          "original_form": "저는 다이어트중이라",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027558.1.7",
          "form": "닭슴살이요 ㅋㅋㅋㅋ",
          "original_form": "닭슴살이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027558.1.8",
          "form": "저는 하림밖에 안먹어봤어요",
          "original_form": "저는 하림밖에 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027558.1.9",
          "form": "하림꺼는 안먹어봤는대 어때요ㅡ",
          "original_form": "하림꺼는 안먹어봤는대 어때요ㅡ",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027558.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027558.1.11",
          "form": "아하ㅋㅋㅋㅋㅋ 닭다리파인데 하림 닭슴살 나름 부드럽고 좋아요",
          "original_form": "아하ㅋㅋㅋㅋㅋ 닭다리파인데 하림 닭슴살 나름 부드럽고 좋아요",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027558.1.12",
          "form": "막 시즈닝 붙어있는거 추천받아서 먹어봤는데 괜찮더라구요",
          "original_form": "막 시즈닝 붙어있는거 추천받아서 먹어봤는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027558.1.13",
          "form": "오 다음에 한번 시켜봐야갰내요",
          "original_form": "오 다음에 한번 시켜봐야갰내요",
          "speaker_id": "2",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100027558.1.14",
          "form": "하림은 가격대가 좀 있어서 도전을 ㅎ..",
          "original_form": "하림은 가격대가 좀 있어서 도전을 ㅎ..",
          "speaker_id": "2",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100027558.1.15",
          "form": "오 그런가요 쫌쫌따리로 시켜서 몰랐어요ㅋㅋㅋㅜㅜ",
          "original_form": "오 그런가요 쫌쫌따리로 시켜서 몰랐어요ㅋㅋㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100027558.1.16",
          "form": "근데 요즘 어디든 닭슴살 잘 나오더라구요",
          "original_form": "근데 요즘 어디든 닭슴살 잘 나오더라구요",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100027558.1.17",
          "form": "맞아요 맛있개 잘나와요",
          "original_form": "맞아요 맛있개 잘나와요",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100027558.1.18",
          "form": "그래서.. 좀 낫죠",
          "original_form": "그래서.. 좀 낫죠",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100027558.1.19",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100027558.1.20",
          "form": "다른 다이어트 식품은 뭐 드세요?",
          "original_form": "다른 다이어트 식품은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100027558.1.21",
          "form": "곤약바 좋아해요!",
          "original_form": "곤약바 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100027558.1.22",
          "form": "젤리도 가끔먹고",
          "original_form": "젤리도 가끔먹고",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100027558.1.23",
          "form": "오 맛있어보여요",
          "original_form": "오 맛있어보여요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100027558.1.24",
          "form": "저도 곤약젤리 좋아해요 어디가 제일 맛있어요?",
          "original_form": "저도 곤약젤리 좋아해요 어디가 제일 맛있어요?",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100027558.1.25",
          "form": "곤약젤리는 먹은지 얼마안돼서 잘 모르갰는데",
          "original_form": "곤약젤리는 먹은지 얼마안돼서 잘 모르갰는데",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100027558.1.26",
          "form": "추천하시는거 있어요?",
          "original_form": "추천하시는거 있어요?",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100027558.1.27",
          "form": "저는 빙그레 곤약젤리 맛있더라구요 칼로리 11이긴 한데 그만큼 더 맛있어요ㅋㅋㅋ",
          "original_form": "저는 빙그레 곤약젤리 맛있더라구요 칼로리 11이긴 한데 그만큼 더 맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100027558.1.28",
          "form": "빙그레꺼 맛있어요 11칼로린데 그만큼 더 달더라구요..ㅎㅎ",
          "original_form": "빙그레꺼 맛있어요 11칼로린데 그만큼 더 달더라구요..ㅎㅎ",
