      "utterance": [
        {
          "id": "MDRW2100046420.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211021 00:22"
        },
        {
          "id": "MDRW2100046420.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20211020 13:49"
        },
        {
          "id": "MDRW2100046420.1.3",
          "form": "점심식사하셨나요?",
          "original_form": "점심식사하셨나요?",
          "speaker_id": "2",
          "time": "20211020 13:49"
        },
        {
          "id": "MDRW2100046420.1.4",
          "form": "네 먹었습니다",
          "original_form": "네 먹었습니다",
          "speaker_id": "1",
          "time": "20211020 13:50"
        },
        {
          "id": "MDRW2100046420.1.5",
          "form": "뭐드셨어요~?",
          "original_form": "뭐드셨어요~?",
          "speaker_id": "2",
          "time": "20211020 13:50"
        },
        {
          "id": "MDRW2100046420.1.6",
          "form": "초밥먹었어요 식사 하셨어요?",
          "original_form": "초밥먹었어요 식사 하셨어요?",
          "speaker_id": "1",
          "time": "20211020 13:50"
        },
        {
          "id": "MDRW2100046420.1.7",
          "form": "네 저는 집에서 사골국 먹었어요!",
          "original_form": "네 저는 집에서 사골국 먹었어요!",
          "speaker_id": "2",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046420.1.8",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "2",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046420.1.9",
          "form": "초밥을 더 좋아해요",
          "original_form": "초밥을 더 좋아해요",
          "speaker_id": "1",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046420.1.10",
          "form": "회보다 초밥이요?!",
          "original_form": "회보다 초밥이요?!",
          "speaker_id": "2",
          "time": "20211020 14:38"
        },
        {
          "id": "MDRW2100046420.1.11",
          "form": "어떤 초밥이 최애 초밥인가요?",
          "original_form": "어떤 초밥이 최애 초밥인가요?",
          "speaker_id": "2",
          "time": "20211020 14:38"
        },
        {
          "id": "MDRW2100046420.1.12",
          "form": "참치요 ㅎㅎ",
          "original_form": "참치요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 14:54"
        },
        {
          "id": "MDRW2100046420.1.13",
          "form": "엇 참치는 회가 더 맛있는데...ㅎㅎ",
          "original_form": "엇 참치는 회가 더 맛있는데...ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 15:30"
        },
        {
          "id": "MDRW2100046420.1.14",
          "form": "은행골 가보셨어요?",
          "original_form": "은행골 가보셨어요?",
          "speaker_id": "2",
          "time": "20211020 15:30"
        },
        {
          "id": "MDRW2100046420.1.15",
          "form": "참치는 많이 먹으면 느끼하잖아요 ㅎㅎ 은행골은 태어나서 처음 들어보네요",
          "original_form": "참치는 많이 먹으면 느끼하잖아요 ㅎㅎ 은행골은 태어나서 처음 들어보네요",
          "speaker_id": "1",
          "time": "20211020 15:31"
        },
        {
          "id": "MDRW2100046420.1.16",
          "form": "헉 은행골이 참치 초밥으로 엄청 유명해요!",
          "original_form": "헉 은행골이 참치 초밥으로 엄청 유명해요!",
          "speaker_id": "2",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046420.1.17",
          "form": "꼭 가보세요!",
          "original_form": "꼭 가보세요!",
          "speaker_id": "2",
          "time": "20211020 15:32"
        },
        {
          "id": "MDRW2100046420.1.18",
          "form": "아하 감사합니다 나중에 가봐야겠네요",
          "original_form": "아하 감사합니다 나중에 가봐야겠네요",
          "speaker_id": "1",
          "time": "20211020 15:32"
        }
      ]
