      "utterance": [
        {
          "id": "MDRW2100037649.1.1",
          "form": "오, 준비운동으로 딱 좋네요",
          "original_form": "오, 준비운동으로 딱 좋네요",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037649.1.2",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037649.1.3",
          "form": "평일에는 운동 안하시나요??!",
          "original_form": "평일에는 운동 안하시나요??!",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037649.1.4",
          "form": "평일에는 출퇴근 걷는게 운동입니다 ㅎㅎ",
          "original_form": "평일에는 출퇴근 걷는게 운동입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037649.1.5",
          "form": "아 역시 걷기가 짱이죠 ㅋㅋ",
          "original_form": "아 역시 걷기가 짱이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037649.1.6",
          "form": "몇분이요??!",
          "original_form": "몇분이요??!",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037649.1.7",
          "form": "그래도 다합하면 30분은 걷지 않을까요?",
          "original_form": "그래도 다합하면 30분은 걷지 않을까요?",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100037649.1.8",
          "form": "출근길은 셔틀타는데 퇴근길은 그냥 걸으니까",
          "original_form": "출근길은 셔틀타는데 퇴근길은 그냥 걸으니까",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037649.1.9",
          "form": "넉넉하게 40분?",
          "original_form": "넉넉하게 40분?",
          "speaker_id": "2",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037649.1.10",
          "form": "오 꽤 걸으시네요 !",
          "original_form": "오 꽤 걸으시네요 !",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100037649.1.11",
          "form": "또 일하는 사이사이마다 걷자나여 ㅋㅋㅋ",
          "original_form": "또 일하는 사이사이마다 걷자나여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100037649.1.12",
          "form": "화장실을 간다던지",
          "original_form": "화장실을 간다던지",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100037649.1.13",
          "form": "그렇긴 한데 그래도 1분 이하 걷는게 운동이 될까요?",
          "original_form": "그렇긴 한데 그래도 1분 이하 걷는게 운동이 될까요?",
          "speaker_id": "2",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100037649.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100037649.1.15",
          "form": "아 ㅋㅋㄱㅋㅋ",
          "original_form": "아 ㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100037649.1.16",
          "form": "안걷는거보단.. 낫잖아요... !",
          "original_form": "안걷는거보단.. 낫잖아요... !",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100037649.1.17",
          "form": "그러네요,",
          "original_form": "그러네요,",
          "speaker_id": "2",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100037649.1.18",
          "form": "화장실 자주 가야겠다 ㅋㅋㅋㅋ",
          "original_form": "화장실 자주 가야겠다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100037649.1.19",
          "form": "전 물 자주 마셔서 자주 갑니다 ㅋㅋㅋㅋ",
          "original_form": "전 물 자주 마셔서 자주 갑니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100037649.1.20",
          "form": "아 ㅋㅋㅋㄱㄱ 그리고 물 많이 마시면 그자체로 건강에 좋죠",
          "original_form": "아 ㅋㅋㅋㄱㄱ 그리고 물 많이 마시면 그자체로 건강에 좋죠",
          "speaker_id": "2",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037649.1.21",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037649.1.22",
          "form": "예전엔 진짜 안마셨는데",
          "original_form": "예전엔 진짜 안마셨는데",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037649.1.23",
          "form": "운동시작하고 나서 마셔요 ㅋㅋ",
          "original_form": "운동시작하고 나서 마셔요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100037649.1.24",
          "form": "저는 음수량이 그렇게 는 것 같지는 않지만요",
          "original_form": "저는 음수량이 그렇게 는 것 같지는 않지만요",
          "speaker_id": "2",
          "time": "20210901 14:22"
        },
        {
          "id": "MDRW2100037649.1.25",
          "form": "의식적으로 마시려고 노력은 해요",
          "original_form": "의식적으로 마시려고 노력은 해요",
          "speaker_id": "2",
          "time": "20210901 14:23"
        }
      ]
    }
  ]
