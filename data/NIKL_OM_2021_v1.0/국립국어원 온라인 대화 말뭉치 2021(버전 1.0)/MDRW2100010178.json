      "utterance": [
        {
          "id": "MDRW2100010178.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010178.1.2",
          "form": "그냥 간단한 게임만 좋아해요ㅎㅎ",
          "original_form": "그냥 간단한 게임만 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100010178.1.3",
          "form": "어떤거 하세요?",
          "original_form": "어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100010178.1.4",
          "form": "쿠키런하다가 접구 타일게임같은거해요ㅎㅎ",
          "original_form": "쿠키런하다가 접구 타일게임같은거해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:15"
        },
        {
          "id": "MDRW2100010178.1.5",
          "form": "아 핸드폰게임 하시는 거구나",
          "original_form": "아 핸드폰게임 하시는 거구나",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010178.1.6",
          "form": "저는 가끔 컴퓨터로 게임하곤 해요",
          "original_form": "저는 가끔 컴퓨터로 게임하곤 해요",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010178.1.7",
          "form": "아 네 무슨게임하세요?",
          "original_form": "아 네 무슨게임하세요?",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010178.1.8",
          "form": "가끔 롤도 하고 카트라이더 해요! ㅋㅋㅋ",
          "original_form": "가끔 롤도 하고 카트라이더 해요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010178.1.9",
          "form": "카트라이더ㅎㅎ 어렸을때 많이 했었는데",
          "original_form": "카트라이더ㅎㅎ 어렸을때 많이 했었는데",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010178.1.10",
          "form": "가볍게 하기 좋은 거 같아요",
          "original_form": "가볍게 하기 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010178.1.11",
          "form": "크아는 이제 못하겠더라구요 ㅠㅠ",
          "original_form": "크아는 이제 못하겠더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010178.1.12",
          "form": "초반은 재밌는데 너무 빨리 질려요ㅎㅎ",
          "original_form": "초반은 재밌는데 너무 빨리 질려요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010178.1.13",
          "form": "그쵸 저도 그렇게 생각해요",
          "original_form": "그쵸 저도 그렇게 생각해요",
          "speaker_id": "2",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010178.1.14",
          "form": "요즘 리니지 모바일 광고 엄청 하더라고요 해봤는데 제 취향은 아닌 거 같아요 ㅠㅠ",
          "original_form": "요즘 리니지 모바일 광고 엄청 하더라고요 해봤는데 제 취향은 아닌 거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010178.1.15",
          "form": "저는 그런 시간 많이쓰는 게임은 오래 못잡고 있겠어서ㅎㅎ",
          "original_form": "저는 그런 시간 많이쓰는 게임은 오래 못잡고 있겠어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010178.1.16",
          "form": "저두요 가볍게 기분전환하는 게임이 좋더라구요 ㅎㅎ",
          "original_form": "저두요 가볍게 기분전환하는 게임이 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:19"
        }
      ]
