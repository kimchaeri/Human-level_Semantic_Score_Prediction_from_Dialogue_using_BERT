      "utterance": [
        {
          "id": "MDRW2100022715.1.1",
          "form": "반려동물 해요?",
          "original_form": "반려동물 해요?",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022715.1.2",
          "form": "네 고양이 두마리 키워요",
          "original_form": "네 고양이 두마리 키워요",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022715.1.3",
          "form": "치즈냥인가요?",
          "original_form": "치즈냥인가요?",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.4",
          "form": "반려동물있으세요?",
          "original_form": "반려동물있으세요?",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.5",
          "form": "치즈랑 오레오같은 애 있어요",
          "original_form": "치즈랑 오레오같은 애 있어요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.6",
          "form": "없습니다 저는",
          "original_form": "없습니다 저는",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.7",
          "form": "사진을 못보내는게 아쉽네요",
          "original_form": "사진을 못보내는게 아쉽네요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.8",
          "form": "어머 치즈오레오네요 깜찍",
          "original_form": "어머 치즈오레오네요 깜찍",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.9",
          "form": "이름이 뭔가요",
          "original_form": "이름이 뭔가요",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.10",
          "form": "비밀인데",
          "original_form": "비밀인데",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.11",
          "form": "name3랑 name4에요",
          "original_form": "&name3&랑 &name4&에요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022715.1.12",
          "form": "너무 깜찍하네요",
          "original_form": "너무 깜찍하네요",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100022715.1.13",
          "form": "근데 반려하먼 필요한 게 너무 많아요",
          "original_form": "근데 반려하먼 필요한 게 너무 많아요",
          "speaker_id": "2",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100022715.1.14",
          "form": "간식 용품 사료,,",
          "original_form": "간식 용품 사료,,",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022715.1.15",
          "form": "애기들건강,,",
          "original_form": "애기들건강,,",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022715.1.16",
          "form": "사료 값 많이 드나요?",
          "original_form": "사료 값 많이 드나요?",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022715.1.17",
          "form": "네 입맛이 고급이셔서 비싼 사료 사먹입니다..",
          "original_form": "네 입맛이 고급이셔서 비싼 사료 사먹입니다..",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022715.1.18",
          "form": "화장실도 한달에 한번 모래갈이 해줘야하는데 비용이 만만치않아요",
          "original_form": "화장실도 한달에 한번 모래갈이 해줘야하는데 비용이 만만치않아요",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022715.1.19",
          "form": "최근에 반려동물나오는 티비프로그램",
          "original_form": "최근에 반려동물나오는 티비프로그램",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022715.1.20",
          "form": "시작했던데 논란이 많더라고요",
          "original_form": "시작했던데 논란이 많더라고요",
          "speaker_id": "2",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022715.1.21",
          "form": "한달에 한번이나 갈아야하는군요",
          "original_form": "한달에 한번이나 갈아야하는군요",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022715.1.22",
          "form": "아 유기",
          "original_form": "아 유기",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022715.1.23",
          "form": "애들을 쉽게 데려와서 쉽게 버리는게 사람인지 모르겠어요",
          "original_form": "애들을 쉽게 데려와서 쉽게 버리는게 사람인지 모르겠어요",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022715.1.24",
          "form": "천사같은 애들을",
          "original_form": "천사같은 애들을",
          "speaker_id": "1",
          "time": "20210829 17:21"
        }
      ]
