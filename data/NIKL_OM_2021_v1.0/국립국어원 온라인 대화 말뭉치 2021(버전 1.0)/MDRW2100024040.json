      "utterance": [
        {
          "id": "MDRW2100024040.1.1",
          "form": "곰표맥주드셔보셨어요?",
          "original_form": "곰표맥주드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024040.1.2",
          "form": "name1님 저 집에 말표 막걸리 있어요",
          "original_form": "&name1&님 저 집에 말표 막걸리 있어요",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024040.1.3",
          "form": "네 먹어봤어요",
          "original_form": "네 먹어봤어요",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024040.1.4",
          "form": "name1님 드셔보심?",
          "original_form": "&name1&님 드셔보심?",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024040.1.5",
          "form": "말표요?",
          "original_form": "말표요?",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024040.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024040.1.7",
          "form": "그런것도  있나요?",
          "original_form": "그런것도  있나요?",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024040.1.8",
          "form": "말맛인가요?",
          "original_form": "말맛인가요?",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024040.1.9",
          "form": "검은콩 들어갔대요",
          "original_form": "검은콩 들어갔대요",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024040.1.10",
          "form": "음 설마요",
          "original_form": "음 설마요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024040.1.11",
          "form": "근데 말 드셔보셨어요?",
          "original_form": "근데 말 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024040.1.12",
          "form": "미숫가루들어간 맛이에요",
          "original_form": "미숫가루들어간 맛이에요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024040.1.13",
          "form": "설마요",
          "original_form": "설마요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024040.1.14",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024040.1.15",
          "form": "그럼이름이  흑 두표맥주가 맞지않나요?",
          "original_form": "그럼이름이  흑 두표맥주가 맞지않나요?",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024040.1.16",
          "form": "왜 말표일까요",
          "original_form": "왜 말표일까요",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024040.1.17",
          "form": "말표라고 써놨네요 양조장사장님께 문의하세요ㅋㅋ",
          "original_form": "말표라고 써놨네요 양조장사장님께 문의하세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024040.1.18",
          "form": "흠 사장님이랑 친분이 없어서",
          "original_form": "흠 사장님이랑 친분이 없어서",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024040.1.19",
          "form": "곰표맥주맛있으셨나요 name1님",
          "original_form": "곰표맥주맛있으셨나요 &name1&님",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024040.1.20",
          "form": "네 달달해요",
          "original_form": "네 달달해요",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024040.1.21",
          "form": "밀가루맛은 아니에요",
          "original_form": "밀가루맛은 아니에요",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024040.1.22",
          "form": "오",
          "original_form": "오",
