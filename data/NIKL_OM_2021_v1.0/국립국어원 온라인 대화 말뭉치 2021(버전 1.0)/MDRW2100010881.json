      "utterance": [
        {
          "id": "MDRW2100010881.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010881.1.2",
          "form": "오 베라각 스벅각하고있엉ㆍ",
          "original_form": "오 베라각 스벅각하고있엉ㆍ",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010881.1.3",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010881.1.4",
          "form": "그게 뭔가요??",
          "original_form": "그게 뭔가요??",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100010881.1.5",
          "form": "음 다운받아서 한번해보시는거 강추요",
          "original_form": "음 다운받아서 한번해보시는거 강추요",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010881.1.6",
          "form": "그냥간단하게 터치만하면",
          "original_form": "그냥간단하게 터치만하면",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010881.1.7",
          "form": "매일 150콩 200스푼 이렇게주고",
          "original_form": "매일 150콩 200스푼 이렇게주고",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010881.1.8",
          "form": "현금이랑 같아서",
          "original_form": "현금이랑 같아서",
          "speaker_id": "2",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010881.1.9",
          "form": "나중에스벅이나 베라로 바꿔먹을수있어요",
          "original_form": "나중에스벅이나 베라로 바꿔먹을수있어요",
          "speaker_id": "2",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010881.1.10",
          "form": "진짜요???",
          "original_form": "진짜요???",
          "speaker_id": "1",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010881.1.11",
          "form": "앱인거죠?",
          "original_form": "앱인거죠?",
          "speaker_id": "1",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010881.1.12",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010881.1.13",
          "form": "150콩이 150원이라서 ㅋ한달하면 무조건 스벅 아아 톨 한잔은 마셔요",
          "original_form": "150콩이 150원이라서 ㅋ한달하면 무조건 스벅 아아 톨 한잔은 마셔요",
          "speaker_id": "2",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010881.1.14",
          "form": "해봐야 오분이면 다하는데 ㅋㅋ",
          "original_form": "해봐야 오분이면 다하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010881.1.15",
          "form": "뭐라고 검색하면 되나요?",
          "original_form": "뭐라고 검색하면 되나요?",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010881.1.16",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010881.1.17",
          "form": "베라각",
          "original_form": "베라각",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010881.1.18",
          "form": "스벅각",
          "original_form": "스벅각",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010881.1.19",
          "form": "출근길",
          "original_form": "출근길",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010881.1.20",
          "form": "다 원리는같아요",
          "original_form": "다 원리는같아요",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010881.1.21",
          "form": "부르는 이름만다름 ㅋㅋ스벅각은 콩 베라각은 스푼 출근길은 도토리",
          "original_form": "부르는 이름만다름 ㅋㅋ스벅각은 콩 베라각은 스푼 출근길은 도토리",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010881.1.22",
          "form": "다 다른 앱인데 결국 같은 게임인거죠?",
          "original_form": "다 다른 앱인데 결국 같은 게임인거죠?",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010881.1.23",
          "form": "네네 심지어 베라각 스벅각은 개발자도 같아요",
          "original_form": "네네 심지어 베라각 스벅각은 개발자도 같아요",
          "speaker_id": "2",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010881.1.24",
          "form": "게임이라보기엔 넘 단순하긴하지만",
          "original_form": "게임이라보기엔 넘 단순하긴하지만",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100010881.1.25",
          "form": "도토리는 뭔가요?",
          "original_form": "도토리는 뭔가요?",
          "speaker_id": "1",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100010881.1.26",
          "form": "음 베라각에서는 매일  모으는 포인트를 스푼이라고하고요",
          "original_form": "음 베라각에서는 매일  모으는 포인트를 스푼이라고하고요",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010881.1.27",
          "form": "스벅각에서는 콩이라고학ㆍ",
          "original_form": "스벅각에서는 콩이라고학ㆍ",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010881.1.28",
          "form": "하고",
          "original_form": "하고",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010881.1.29",
          "form": "출근길에서는 도토리라고해요",
          "original_form": "출근길에서는 도토리라고해요",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010881.1.30",
          "form": "1포인트=1스푼=1콩=1도토리=1원",
          "original_form": "1포인트=1스푼=1콩=1도토리=1원",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010881.1.31",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010881.1.32",
          "form": "출근길에서는 뭘 바꿀수 있나요?",
          "original_form": "출근길에서는 뭘 바꿀수 있나요?",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010881.1.33",
          "form": "편의점 문상 네페 스벅아아",
          "original_form": "편의점 문상 네페 스벅아아",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010881.1.34",
          "form": "구글 등등요",
          "original_form": "구글 등등요",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010881.1.35",
          "form": "전 문상 목표에욤",
          "original_form": "전 문상 목표에욤",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010881.1.36",
          "form": "문상 만원요??",
          "original_form": "문상 만원요??",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010881.1.37",
          "form": "넵넵 매일그냥 한백원모이니까",
          "original_form": "넵넵 매일그냥 한백원모이니까",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010881.1.38",
          "form": "시나브로 지금 칠천원이에요",
          "original_form": "시나브로 지금 칠천원이에요",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010881.1.39",
          "form": "이건 뭔가 게임  한스푼의 앱테크들이긴하네요 ㅋㅋ",
          "original_form": "이건 뭔가 게임  한스푼의 앱테크들이긴하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:22"
        }
