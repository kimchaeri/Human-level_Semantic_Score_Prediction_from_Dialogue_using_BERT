      "utterance": [
        {
          "id": "MDRW2100035096.1.1",
          "form": "음료는 뭐좋아하세요",
          "original_form": "음료는 뭐좋아하세요",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.2",
          "form": "어머 ㅋㅋㅋ 저 민초파에요",
          "original_form": "어머 ㅋㅋㅋ 저 민초파에요",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.3",
          "form": "저 민초 좋아해요!",
          "original_form": "저 민초 좋아해요!",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.4",
          "form": "저두요ㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.5",
          "form": "근데막 엄청난 정도는아니고",
          "original_form": "근데막 엄청난 정도는아니고",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.6",
          "form": "이거 마시면 입 텁텁하지도 않고 상쾌하고 맛있어요",
          "original_form": "이거 마시면 입 텁텁하지도 않고 상쾌하고 맛있어요",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.7",
          "form": "달달하기도하고",
          "original_form": "달달하기도하고",
          "speaker_id": "1",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.8",
          "form": "혹시 던킨 민초쿠폰아세요??",
          "original_form": "혹시 던킨 민초쿠폰아세요??",
          "speaker_id": "2",
          "time": "20210829 02:34"
        },
        {
          "id": "MDRW2100035096.1.9",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.10",
          "form": "저 받았는데",
          "original_form": "저 받았는데",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.11",
          "form": "지난주인가?",
          "original_form": "지난주인가?",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.12",
          "form": "쿠폰을 뿌려서 해피오더에소 100원",
          "original_form": "쿠폰을 뿌려서 해피오더에소 100원",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.13",
          "form": "근데 저희 매장 해피오더에는 안뜨더라구요ㅠㅠ",
          "original_form": "근데 저희 매장 해피오더에는 안뜨더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.14",
          "form": "전 2번성공해서 ㅋㅋ2번마셔봤네요!",
          "original_form": "전 2번성공해서 ㅋㅋ2번마셔봤네요!",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.15",
          "form": "사장님이 닫으셨나봐요ㅠㅠ",
          "original_form": "사장님이 닫으셨나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.16",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.17",
          "form": "ㅠㅠ에공 아꿉네요ㅠ",
          "original_form": "ㅠㅠ에공 아꿉네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.18",
          "form": "이게 약간 민트폼이 점바점인것같더라구요",
          "original_form": "이게 약간 민트폼이 점바점인것같더라구요",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.19",
          "form": "지난번에 마신건 밍밍했는데",
          "original_form": "지난번에 마신건 밍밍했는데",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100035096.1.20",
          "form": "다른지점에서 제돈주고 사먹은건",
          "original_form": "다른지점에서 제돈주고 사먹은건",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035096.1.21",
          "form": "전 2번다 같은매장이였지만  만든이 달랐는데 둘다 괜찮저라구요!",
          "original_form": "전 2번다 같은매장이였지만  만든이 달랐는데 둘다 괜찮저라구요!",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035096.1.22",
          "form": "맛있고 진했어용",
          "original_form": "맛있고 진했어용",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035096.1.23",
          "form": "기술이 필요하군요 ㅋㅋㅋ",
          "original_form": "기술이 필요하군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100035096.1.24",
          "form": "요즘 그릭복숭아 유행하던데",
          "original_form": "요즘 그릭복숭아 유행하던데",
          "speaker_id": "1",
          "time": "20210829 02:58"
        }
      ]
    }
