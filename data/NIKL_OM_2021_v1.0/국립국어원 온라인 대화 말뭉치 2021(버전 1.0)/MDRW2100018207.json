      "utterance": [
        {
          "id": "MDRW2100018207.1.1",
          "form": "요새 머리 염색은 안해? ㅋㅋ",
          "original_form": "요새 머리 염색은 안해? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018207.1.2",
          "form": "다시 예전처엄",
          "original_form": "다시 예전처엄",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018207.1.3",
          "form": "머리털이",
          "original_form": "머리털이",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018207.1.4",
          "form": "돼지털이 되는게 두려워요 ㅋㅋㅋ",
          "original_form": "돼지털이 되는게 두려워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018207.1.5",
          "form": "ㅋㅋ 그럼 지금은\r\n머리 무슨 색이세요,",
          "original_form": "ㅋㅋ 그럼 지금은\r\n머리 무슨 색이세요,",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018207.1.6",
          "form": "검정ㅇ죠 ㅋㅋㅋㅋ",
          "original_form": "검정ㅇ죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018207.1.7",
          "form": "완저누싹 한번 자르고",
          "original_form": "완저누싹 한번 자르고",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018207.1.8",
          "form": "열심히 길랐어염",
          "original_form": "열심히 길랐어염",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018207.1.9",
          "form": "와우! 그랬는데 허리까지 길렀다니",
          "original_form": "와우! 그랬는데 허리까지 길렀다니",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018207.1.10",
          "form": "나는 마지막펌이 주하 낳으러\r\n가기전에 단발로 자르면서 한거니까",
          "original_form": "나는 마지막펌이 주하 낳으러\r\n가기전에 단발로 자르면서 한거니까",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018207.1.11",
          "form": "벌써 2년도 넘었네ㅋㅋㅋㅋ",
          "original_form": "벌써 2년도 넘었네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100018207.1.12",
          "form": "헉 2년 ㅋㅋ",
          "original_form": "헉 2년 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018207.1.13",
          "form": "근대뭐...묭실은",
          "original_form": "근대뭐...묭실은",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018207.1.14",
          "form": "생각읎으면 잘안가게도되공",
          "original_form": "생각읎으면 잘안가게도되공",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018207.1.15",
          "form": "언닌바뿌니깡",
          "original_form": "언닌바뿌니깡",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018207.1.16",
          "form": "코로나 때문에도 못가고",
          "original_form": "코로나 때문에도 못가고",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018207.1.17",
          "form": "수유하니까 그 약때문에\r\n펌이나 염색을 못했었음ㅋㅋ",
          "original_form": "수유하니까 그 약때문에\r\n펌이나 염색을 못했었음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018207.1.18",
          "form": "아 그런것도있구만",
          "original_form": "아 그런것도있구만",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018207.1.19",
          "form": "뭐암튼",
          "original_form": "뭐암튼",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018207.1.20",
          "form": "여러모로 신경쓸게많구만",
          "original_form": "여러모로 신경쓸게많구만",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018207.1.21",
          "form": "나도 이제 수유 끝났으니\r\n미용실 가서 머리도 좀 해야디",
          "original_form": "나도 이제 수유 끝났으니\r\n미용실 가서 머리도 좀 해야디",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018207.1.22",
          "form": "왁싱도 해보고 싶은데\r\n그 고통이 무서움ㅋㅋ",
          "original_form": "왁싱도 해보고 싶은데\r\n그 고통이 무서움ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018207.1.23",
          "form": "...아",
          "original_form": "...아",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018207.1.24",
          "form": "왁싱 ㅋㅋㅋㅋ",
          "original_form": "왁싱 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018207.1.25",
          "form": "진짴ㅋㅋ 겨털 하나뽑는것도",
          "original_form": "진짴ㅋㅋ 겨털 하나뽑는것도",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018207.1.26",
          "form": "개아픈대",
          "original_form": "개아픈대",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018207.1.27",
          "form": "무슨머리할려공?",
          "original_form": "무슨머리할려공?",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018207.1.28",
          "form": "모르겠음ㅋㅋ\r\n요즘 트랜드도 모르고ㅋㅋ",
          "original_form": "모르겠음ㅋㅋ\r\n요즘 트랜드도 모르고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:39"
        }
      ]
    }
