        {
          "id": "MDRW2100025944.1.1",
          "form": "요즘 뭐산거 있어?",
          "original_form": "요즘 뭐산거 있어?",
          "speaker_id": "2",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100025944.1.2",
          "form": "나엄청많아ㅋㅋ",
          "original_form": "나엄청많아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025944.1.3",
          "form": "악ㅋㅋㅋ뭘그리삼?",
          "original_form": "악ㅋㅋㅋ뭘그리삼?",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025944.1.4",
          "form": "나이사가거든ㅋㅋ준비하는거때뭇에",
          "original_form": "나이사가거든ㅋㅋ준비하는거때뭇에",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025944.1.5",
          "form": "오ㅋㅋㅋ뭐뭐사려고?",
          "original_form": "오ㅋㅋㅋ뭐뭐사려고?",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025944.1.6",
          "form": "궁금해",
          "original_form": "궁금해",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025944.1.7",
          "form": "나가전가구 다 사ㅋㄱㅋㅋ",
          "original_form": "나가전가구 다 사ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025944.1.8",
          "form": "대단하다ㅋㅋㅋ물건알아보는것도일일듯",
          "original_form": "대단하다ㅋㅋㅋ물건알아보는것도일일듯",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025944.1.9",
          "form": "힘들어ㅋㅋ쇼핑이이리힝들줄이야ㅋㅋㅋ",
          "original_form": "힘들어ㅋㅋ쇼핑이이리힝들줄이야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025944.1.10",
          "form": "그래도집채우는재미는있겠다",
          "original_form": "그래도집채우는재미는있겠다",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025944.1.11",
          "form": "ㅋㅋㅋㄱ가전은뭐사?",
          "original_form": "ㅋㅋㅋㄱ가전은뭐사?",
