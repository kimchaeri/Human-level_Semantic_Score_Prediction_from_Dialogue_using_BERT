      "utterance": [
        {
          "id": "MDRW2100001896.1.1",
          "form": "꼭 가보고 싶은 여행지는?",
          "original_form": "꼭 가보고 싶은 여행지는?",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001896.1.2",
          "form": "음.. 일본?!",
          "original_form": "음.. 일본?!",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001896.1.3",
          "form": "한번 가보고 싶어요",
          "original_form": "한번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001896.1.4",
          "form": "원래 고3 수능 끝나구",
          "original_form": "원래 고3 수능 끝나구",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001896.1.5",
          "form": "갈려했는데",
          "original_form": "갈려했는데",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001896.1.6",
          "form": "그시국이라고 일본을 못갔어요..",
          "original_form": "그시국이라고 일본을 못갔어요..",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001896.1.7",
          "form": "일본에서도 어느 도시를 젤 가보고 싶으세요?",
          "original_form": "일본에서도 어느 도시를 젤 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001896.1.8",
          "form": "저는 성인 된지 좀 돼서...",
          "original_form": "저는 성인 된지 좀 돼서...",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001896.1.9",
          "form": "일본 한 4번 가봣거든요",
          "original_form": "일본 한 4번 가봣거든요",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001896.1.10",
          "form": "도시마다 다르더라구요",
          "original_form": "도시마다 다르더라구요",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001896.1.11",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001896.1.12",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001896.1.13",
          "form": "저 추천해주세요",
          "original_form": "저 추천해주세요",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001896.1.14",
          "form": "저는 친구가 일본 대학 노리는 친구 있어서",
          "original_form": "저는 친구가 일본 대학 노리는 친구 있어서",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001896.1.15",
          "form": "그 친구 내년에 가면 놀러갈려구요",
          "original_form": "그 친구 내년에 가면 놀러갈려구요",
          "speaker_id": "2",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001896.1.16",
          "form": "여름 vs 겨울 산vs 바다 중에 뭘 더 좋아하세요?",
          "original_form": "여름 vs 겨울 산vs 바다 중에 뭘 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001896.1.17",
          "form": "오 친구분 멋지다",
          "original_form": "오 친구분 멋지다",
          "speaker_id": "1",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001896.1.18",
          "form": "전 겨울! 바다!",
          "original_form": "전 겨울! 바다!",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001896.1.19",
          "form": "겨울! 산?!",
          "original_form": "겨울! 산?!",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001896.1.20",
          "form": "산 바다 둘다 좋아요!!",
          "original_form": "산 바다 둘다 좋아요!!",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001896.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001896.1.22",
          "form": "겨울 좋아하면 삿포로 좋아요!!",
          "original_form": "겨울 좋아하면 삿포로 좋아요!!",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001896.1.23",
          "form": "오키나와는 바다가 예쁘고 약간 아시아 그리스 산토리니 느낌",
          "original_form": "오키나와는 바다가 예쁘고 약간 아시아 그리스 산토리니 느낌",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001896.1.24",
          "form": "친구는 어느 도시로 가요?",
          "original_form": "친구는 어느 도시로 가요?",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001896.1.25",
          "form": "아직..",
          "original_form": "아직..",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001896.1.26",
          "form": "모르겠어요.. 시험 준비중이라",
          "original_form": "모르겠어요.. 시험 준비중이라",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001896.1.27",
          "form": "오키나와.. 대박 멋지네요",
          "original_form": "오키나와.. 대박 멋지네요",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001896.1.28",
          "form": "저는 해산물도 짱 좋아해서",
          "original_form": "저는 해산물도 짱 좋아해서",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001896.1.29",
          "form": "섬나라 재밌을거 같아요!",
          "original_form": "섬나라 재밌을거 같아요!",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001896.1.30",
          "form": "그럼 누님은.. 어떤 나라 가고 싶어요?",
          "original_form": "그럼 누님은.. 어떤 나라 가고 싶어요?",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001896.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 코로나 전에 유럽을 못가봐서 그게 제일 아쉽네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 코로나 전에 유럽을 못가봐서 그게 제일 아쉽네요",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001896.1.32",
          "form": "에펠탑앞에서 바게트 한번 먹어줬어야 하는건데",
          "original_form": "에펠탑앞에서 바게트 한번 먹어줬어야 하는건데",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001896.1.33",
          "form": "유럽중엔 가보고 싶은 나라 있어요?",
          "original_form": "유럽중엔 가보고 싶은 나라 있어요?",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001896.1.34",
          "form": "런던 한번 가보고 싶어요!",
          "original_form": "런던 한번 가보고 싶어요!",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001896.1.35",
          "form": "그 긴 털모자 쓰고 있는 런던 근위병 보고 싶어요",
          "original_form": "그 긴 털모자 쓰고 있는 런던 근위병 보고 싶어요",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001896.1.36",
          "form": "ㅋㅋㅋㅎㅋ",
          "original_form": "ㅋㅋㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001896.1.37",
          "form": "에펠탑 앞에서 바게트 ㅋㅋㅋㅋ",
          "original_form": "에펠탑 앞에서 바게트 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001896.1.38",
          "form": "오 맞아요 그거 멋지더라구요 런던도 볼게 많아서 재밌을 것 같아요",
          "original_form": "오 맞아요 그거 멋지더라구요 런던도 볼게 많아서 재밌을 것 같아요",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001896.1.39",
          "form": "지금 사는 곳에 관광명소있나요?",
          "original_form": "지금 사는 곳에 관광명소있나요?",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001896.1.40",
          "form": "광주.. 뭐 있나요?",
          "original_form": "광주.. 뭐 있나요?",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001896.1.41",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001896.1.42",
          "form": "무등산?",
          "original_form": "무등산?",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001896.1.43",
