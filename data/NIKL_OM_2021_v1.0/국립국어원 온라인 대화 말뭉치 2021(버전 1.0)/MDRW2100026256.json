      "utterance": [
        {
          "id": "MDRW2100026256.1.1",
          "form": "저녁 먹었어??",
          "original_form": "저녁 먹었어??",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026256.1.2",
          "form": "응! 나 칼국수 먹었지",
          "original_form": "응! 나 칼국수 먹었지",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026256.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026256.1.4",
          "form": "칼국수 맛있었겠다!",
          "original_form": "칼국수 맛있었겠다!",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026256.1.5",
          "form": "나도 먹었어!",
          "original_form": "나도 먹었어!",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026256.1.6",
          "form": "난 저녁 카레 해먹었어~",
          "original_form": "난 저녁 카레 해먹었어~",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026256.1.7",
          "form": "우와 나 낼 카레하려고 장봐왔는데 ㅋㅋㅋㅋ",
          "original_form": "우와 나 낼 카레하려고 장봐왔는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026256.1.8",
          "form": "너무 신기해 ㅋㅋㅋㅋ",
          "original_form": "너무 신기해 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026256.1.9",
          "form": "헐 신기하다ㅋㅋㅋㅋㅋ",
          "original_form": "헐 신기하다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026256.1.10",
          "form": "카레 너무 맛있어!",
          "original_form": "카레 너무 맛있어!",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026256.1.11",
          "form": "카레여왕꺼 맛있더라구!",
          "original_form": "카레여왕꺼 맛있더라구!",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026256.1.12",
          "form": "오 그래? 나는 오뚜기 백세카레 샀다",
          "original_form": "오 그래? 나는 오뚜기 백세카레 샀다",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026256.1.13",
          "form": "나중에 그것두 사볼게!",
          "original_form": "나중에 그것두 사볼게!",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026256.1.14",
          "form": "그건 맛이 어때?",
          "original_form": "그건 맛이 어때?",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026256.1.15",
          "form": "오뚜기보다 좀 단편이야!",
          "original_form": "오뚜기보다 좀 단편이야!",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026256.1.16",
          "form": "맛이 다양한데 내 입맛엔 구운양파가 맛있었어ㅎㅎ",
          "original_form": "맛이 다양한데 내 입맛엔 구운양파가 맛있었어ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026256.1.17",
          "form": "요리 좋아해??",
          "original_form": "요리 좋아해??",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026256.1.18",
          "form": "우와 구운양파 맛나겠다 ㅋㅋㅋ",
          "original_form": "우와 구운양파 맛나겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026256.1.19",
          "form": "나 그럭저럭? 내가 혼자 있을 땐 요리 잘 안해",
          "original_form": "나 그럭저럭? 내가 혼자 있을 땐 요리 잘 안해",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026256.1.20",
          "form": "누가 있어야 좀 해 ㅋㅋ",
          "original_form": "누가 있어야 좀 해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026256.1.21",
          "form": "요리하는거 좋은데 너무 귀찮아...",
          "original_form": "요리하는거 좋은데 너무 귀찮아...",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026256.1.22",
          "form": "혼자있을땐 그럼 주로 시켜먹어??",
          "original_form": "혼자있을땐 그럼 주로 시켜먹어??",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026256.1.23",
          "form": "응응 혼자는 사실 뭐 해먹기 귀찮아서 대충 먹어",
          "original_form": "응응 혼자는 사실 뭐 해먹기 귀찮아서 대충 먹어",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026256.1.24",
          "form": "시켜먹기도 하공?",
          "original_form": "시켜먹기도 하공?",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026256.1.25",
          "form": "요리는 진짜 손이 많이가ㅠㅠ",
          "original_form": "요리는 진짜 손이 많이가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026256.1.26",
          "form": "맞아 국 하나 끓이는데도 손이 엄청 가더라구!",
          "original_form": "맞아 국 하나 끓이는데도 손이 엄청 가더라구!",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026256.1.27",
          "form": "근데 난 요리도 요리인데",
          "original_form": "근데 난 요리도 요리인데",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026256.1.28",
          "form": "요리하면서 나온 설거지하는게 힘들더라ㅎㅎㅎ",
          "original_form": "요리하면서 나온 설거지하는게 힘들더라ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026256.1.29",
          "form": "아 맞앙",
          "original_form": "아 맞앙",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026256.1.30",
          "form": "설거지만 아니었어도 반은 먹고 들어가ㅠㅠ",
          "original_form": "설거지만 아니었어도 반은 먹고 들어가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026256.1.31",
          "form": "식세기 사고 싶당...",
          "original_form": "식세기 사고 싶당...",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026256.1.32",
          "form": "맞아 요즘 유튜브나 광고 보면 사고싶어...",
          "original_form": "맞아 요즘 유튜브나 광고 보면 사고싶어...",
          "speaker_id": "1",
          "time": "20210901 22:48"
        }
      ]
    }
