      "utterance": [
        {
          "id": "MDRW2100044775.1.1",
          "form": "자기가그만둔 것처럼",
          "original_form": "자기가그만둔 것처럼",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044775.1.2",
          "form": "코메디찍냐구",
          "original_form": "코메디찍냐구",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044775.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044775.1.4",
          "form": "쪽팔림이란걸 아는 인간이",
          "original_form": "쪽팔림이란걸 아는 인간이",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.5",
          "form": "여태껏 그러고 살았어...?",
          "original_form": "여태껏 그러고 살았어...?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ인두겁을 쓰고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ인두겁을 쓰고",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.7",
          "form": "못할짓 많이했지",
          "original_form": "못할짓 많이했지",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.8",
          "form": "아니 100번중 하나맞는ㄷ",
          "original_form": "아니 100번중 하나맞는ㄷ",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.9",
          "form": "그거가자고",
          "original_form": "그거가자고",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.10",
          "form": "눈먼신봉자들 양성해놓고",
          "original_form": "눈먼신봉자들 양성해놓고",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.11",
          "form": "남의 스캔들 파헤치며 추잡스럽게 돈벌었으면서",
          "original_form": "남의 스캔들 파헤치며 추잡스럽게 돈벌었으면서",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.12",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044775.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.14",
          "form": "진짜 웃기지말라그래",
          "original_form": "진짜 웃기지말라그래",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.15",
          "form": "유튜브 렉카들 다 망했으면",
          "original_form": "유튜브 렉카들 다 망했으면",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.16",
          "form": "한예슬이 호빠를 가든 말든 무슨상관인데요",
          "original_form": "한예슬이 호빠를 가든 말든 무슨상관인데요",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.17",
          "form": "왜그러고 살아 진짜",
          "original_form": "왜그러고 살아 진짜",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.18",
          "form": "그러든말든 안물안궁",
          "original_form": "그러든말든 안물안궁",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.19",
          "form": "그걸 물고늘어지면서",
          "original_form": "그걸 물고늘어지면서",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.20",
          "form": "어떻게든 끌어내리려고",
          "original_form": "어떻게든 끌어내리려고",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.21",
          "form": "박수홍 얘기안했나여?",
          "original_form": "박수홍 얘기안했나여?",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044775.1.22",
          "form": "박수홍 전여친이 데이트폭력 당했다고",
          "original_form": "박수홍 전여친이 데이트폭력 당했다고",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.23",
          "form": "???????",
          "original_form": "???????",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.24",
          "form": "박수홍한테",
          "original_form": "박수홍한테",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.25",
          "form": "근데 하는말이",
          "original_form": "근데 하는말이",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.26",
          "form": "자기를 공주처럼 대해줬는데",
          "original_form": "자기를 공주처럼 대해줬는데",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.27",
          "form": "형에대한 이야기를 하기시작했대",
          "original_form": "형에대한 이야기를 하기시작했대",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.28",
          "form": "?????????",
          "original_form": "?????????",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.29",
          "form": "말하는 박수홍 눈빛이 너무 무서워서",
          "original_form": "말하는 박수홍 눈빛이 너무 무서워서",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.30",
          "form": "ㄱㄱ 당한거 같았대",
          "original_form": "ㄱㄱ 당한거 같았대",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044775.1.31",
          "form": "거기에 형까지 끼어들였어?",
          "original_form": "거기에 형까지 끼어들였어?",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
