        {
          "id": "MDRW2100035778.1.1",
          "form": "안녕라새요",
          "original_form": "안녕라새요",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100035778.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100035778.1.3",
          "form": "혹시 향수에 관심 많으세여?",
          "original_form": "혹시 향수에 관심 많으세여?",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100035778.1.4",
          "form": "아니욥",
          "original_form": "아니욥",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100035778.1.5",
          "form": "왜죱",
          "original_form": "왜죱",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100035778.1.6",
          "form": "집에 향수 별로없나욥",
          "original_form": "집에 향수 별로없나욥",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100035778.1.7",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100035778.1.8",
          "form": "그러쿤여 저희집에는 향수가 너무 많아서\r\n요즘 향수뿌리는데 재미들렷러료",
          "original_form": "그러쿤여 저희집에는 향수가 너무 많아서\r\n요즘 향수뿌리는데 재미들렷러료",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100035778.1.9",
          "form": "저좀 주세영",
          "original_form": "저좀 주세영",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100035778.1.10",
          "form": "동생이 저한테 이상한냄새가난대서 뿌리게되엇어요",
          "original_form": "동생이 저한테 이상한냄새가난대서 뿌리게되엇어요",
          "speaker_id": "1",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100035778.1.11",
          "form": "잘 안씻으세여?",
          "original_form": "잘 안씻으세여?",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100035778.1.12",
          "form": "열심히 씻어요",
          "original_form": "열심히 씻어요",
          "speaker_id": "1",
          "time": "20210830 01:46"
        },
        {
          "id": "MDRW2100035778.1.13",
          "form": "하루 한번요?",
          "original_form": "하루 한번요?",
          "speaker_id": "2",
          "time": "20210830 01:47"
        },
        {
          "id": "MDRW2100035778.1.14",
          "form": "요즘엔 덜하긴하는데",
          "original_form": "요즘엔 덜하긴하는데",
