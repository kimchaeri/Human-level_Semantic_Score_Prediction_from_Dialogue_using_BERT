      "utterance": [
        {
          "id": "MDRW2100042116.1.1",
          "form": "연애와 최준에 이어",
          "original_form": "연애와 최준에 이어",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042116.1.2",
          "form": "오늘 로퍼 신고 갔는데",
          "original_form": "오늘 로퍼 신고 갔는데",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042116.1.3",
          "form": "넘 이쁘고 편했어요 잘샀어",
          "original_form": "넘 이쁘고 편했어요 잘샀어",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042116.1.4",
          "form": "오 로퍼 굿초이스네용",
          "original_form": "오 로퍼 굿초이스네용",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042116.1.5",
          "form": "좋아요 하나 잘 사면 이렇게 즐김 되는데",
          "original_form": "좋아요 하나 잘 사면 이렇게 즐김 되는데",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042116.1.6",
          "form": "또 사려고 해요 ㅋㅋㅋㅋㅋ",
          "original_form": "또 사려고 해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042116.1.7",
          "form": "잘못사도 만회하려규 또 사고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잘못사도 만회하려규 또 사고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042116.1.8",
          "form": "저 돈 언제 모아요?",
          "original_form": "저 돈 언제 모아요?",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042116.1.9",
          "form": "로퍼발은안아파용?",
          "original_form": "로퍼발은안아파용?",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042116.1.10",
          "form": "양말신어용?",
          "original_form": "양말신어용?",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042116.1.11",
          "form": "버선 신었어요",
          "original_form": "버선 신었어요",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042116.1.12",
          "form": "발 하나도 안아팔어요 잘샀어요",
          "original_form": "발 하나도 안아팔어요 잘샀어요",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042116.1.13",
          "form": "아하버선ㅋㅋㄱㅋ",
          "original_form": "아하버선ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042116.1.14",
          "form": "버선하니 한복에신는그거생각나는데ㅋㄱㅈㅋㅈㄱ",
          "original_form": "버선하니 한복에신는그거생각나는데ㅋㄱㅈㅋㅈㄱ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042116.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 맞아요 그렁거 신고 갔어요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 맞아요 그렁거 신고 갔어요",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042116.1.16",
          "form": "저 만나주실거에요?",
          "original_form": "저 만나주실거에요?",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042116.1.17",
          "form": "ㄱㄱㅈㅋㅈㄱㅈㄱㅈㄱ악ㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅊㅋ",
          "original_form": "ㄱㄱㅈㅋㅈㄱㅈㄱㅈㄱ악ㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅊㅋ",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042116.1.18",
          "form": "신발벗는식당안가면..? 이러구막ㅋㅈㅋ",
          "original_form": "신발벗는식당안가면..? 이러구막ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042116.1.19",
          "form": "앜ㅋㅋㅋㄴ 왜요 신발 벗어도 같이 먹어주세요",
          "original_form": "앜ㅋㅋㅋㄴ 왜요 신발 벗어도 같이 먹어주세요",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100042116.1.20",
          "form": "ㅋㅋㄲㅋㅋㅋㅋㄱ악ㅋㅋㄱ알았어요 버선궁금하네요",
