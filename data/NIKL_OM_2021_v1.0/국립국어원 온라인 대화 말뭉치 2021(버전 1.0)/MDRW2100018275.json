      "utterance": [
        {
          "id": "MDRW2100018275.1.1",
          "form": "그렇죠 저 오늘 충격적인 이야기 들었어요",
          "original_form": "그렇죠 저 오늘 충격적인 이야기 들었어요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018275.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018275.1.3",
          "form": "매일 운동하기를 좋아하는 분이 하루아침에 백혈병 걸려서 돌아가셨대요 ㅠㅠ",
          "original_form": "매일 운동하기를 좋아하는 분이 하루아침에 백혈병 걸려서 돌아가셨대요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018275.1.4",
          "form": "헉... 아시는 분이신가요?",
          "original_form": "헉... 아시는 분이신가요?",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018275.1.5",
          "form": "네 친척이에요 ㅜㅜ",
          "original_form": "네 친척이에요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018275.1.6",
          "form": "그래서 오늘 욜로라는 단어가 떠오르더군요",
          "original_form": "그래서 오늘 욜로라는 단어가 떠오르더군요",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018275.1.7",
          "form": "앗ㅜㅜ 건강하고 병은 정말 다르네요..",
          "original_form": "앗ㅜㅜ 건강하고 병은 정말 다르네요..",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018275.1.8",
          "form": "충격이 크시겠어요",
          "original_form": "충격이 크시겠어요",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018275.1.9",
          "form": "그러게요.. 님은 건강한 편이세요?",
          "original_form": "그러게요.. 님은 건강한 편이세요?",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018275.1.10",
          "form": "네 체중 나가는거에비해 건강하더라구요",
          "original_form": "네 체중 나가는거에비해 건강하더라구요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018275.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018275.1.12",
          "form": "전 많이 건강해졌어요.",
          "original_form": "전 많이 건강해졌어요.",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018275.1.13",
          "form": "원래 혈당수치랑 콜레스테롤수치 혈압수치 등이 다 불안정했거든요",
          "original_form": "원래 혈당수치랑 콜레스테롤수치 혈압수치 등이 다 불안정했거든요",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018275.1.14",
          "form": "열심히 노력하신거 같아요",
          "original_form": "열심히 노력하신거 같아요",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018275.1.15",
          "form": "이제 근량만 늘리면 될 것 같아요 ㅜㅜ",
          "original_form": "이제 근량만 늘리면 될 것 같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018275.1.16",
          "form": "팔굽혀펴기도 너무 힘들어요",
          "original_form": "팔굽혀펴기도 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018275.1.17",
          "form": "홈트만 하시는건가요??",
          "original_form": "홈트만 하시는건가요??",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018275.1.18",
          "form": "네네 헬스장은 뭔가 돈이 아깝고 눈치도 보여요",
          "original_form": "네네 헬스장은 뭔가 돈이 아깝고 눈치도 보여요",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018275.1.19",
          "form": "그리고 코로나문제도 있어서요 ㅠㅠ",
          "original_form": "그리고 코로나문제도 있어서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018275.1.20",
          "form": "그쵸ㅠ 코로나라 쉭지않죠ㅠ",
          "original_form": "그쵸ㅠ 코로나라 쉭지않죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 00:14"
        }
