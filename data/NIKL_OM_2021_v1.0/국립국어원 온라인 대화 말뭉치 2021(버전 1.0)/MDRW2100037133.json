      "utterance": [
        {
          "id": "MDRW2100037133.1.1",
          "form": "저는 일드 미드 영드 같은거 많이봐요",
          "original_form": "저는 일드 미드 영드 같은거 많이봐요",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100037133.1.2",
          "form": "근데 전에 동생한테 그 드라마 이야기 들었었는데",
          "original_form": "근데 전에 동생한테 그 드라마 이야기 들었었는데",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100037133.1.3",
          "form": "그 임성한 작가 드라마 ㅋ",
          "original_form": "그 임성한 작가 드라마 ㅋ",
          "speaker_id": "2",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100037133.1.4",
          "form": "그분이 누구죠?ㅎ",
          "original_form": "그분이 누구죠?ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037133.1.5",
          "form": "그 보석비빔밥 인어아가씨 드라마 쓰신분요 ㅋㅋ",
          "original_form": "그 보석비빔밥 인어아가씨 드라마 쓰신분요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037133.1.6",
          "form": "아...그분이요",
          "original_form": "아...그분이요",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037133.1.7",
          "form": "결혼작가이혼작곡 인가 그 드라마 보셨어요? ㅎㅎ",
          "original_form": "결혼작가이혼작곡 인가 그 드라마 보셨어요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 07:41"
        },
        {
          "id": "MDRW2100037133.1.8",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037133.1.9",
          "form": "그건 새로하는건가요?",
          "original_form": "그건 새로하는건가요?",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037133.1.10",
          "form": "아마 완결났을거에요",
          "original_form": "아마 완결났을거에요",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037133.1.11",
          "form": "제동생이 다시보기로 보더라고요",
          "original_form": "제동생이 다시보기로 보더라고요",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037133.1.12",
          "form": "아 끝난거군요",
          "original_form": "아 끝난거군요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037133.1.13",
          "form": "잼나나요?",
          "original_form": "잼나나요?",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037133.1.14",
          "form": "저는 요새 미생 다시해주길래 보는데",
          "original_form": "저는 요새 미생 다시해주길래 보는데",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037133.1.15",
          "form": "다시봐도 잼나네요",
          "original_form": "다시봐도 잼나네요",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037133.1.16",
          "form": "제동생이 자가격리를당했었거든요 타지에혼자사는데",
          "original_form": "제동생이 자가격리를당했었거든요 타지에혼자사는데",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037133.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037133.1.18",
          "form": "그때 너무할거없고 지겨워서 갑자기 막연락자주하는거에요",
          "original_form": "그때 너무할거없고 지겨워서 갑자기 막연락자주하는거에요",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037133.1.19",
          "form": "어쩌다가",
          "original_form": "어쩌다가",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037133.1.20",
          "form": "그냥 만난친구가 코로나 양성나와서 ㅋㅋ",
          "original_form": "그냥 만난친구가 코로나 양성나와서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037133.1.21",
          "form": "근데 나라에서 하라고한게아니고요",
          "original_form": "근데 나라에서 하라고한게아니고요",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037133.1.22",
          "form": "그럼요?",
          "original_form": "그럼요?",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037133.1.23",
          "form": "예를들어 나라에서 3일동선파악해서 자가격리시키면",
          "original_form": "예를들어 나라에서 3일동선파악해서 자가격리시키면",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037133.1.24",
          "form": "제동생은 5일전에 만난그런상황이었어요",
          "original_form": "제동생은 5일전에 만난그런상황이었어요",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037133.1.25",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037133.1.26",
          "form": "애매하네요",
          "original_form": "애매하네요",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100037133.1.27",
          "form": "저는 아이들 좀 재우고 올케요 ㅎ",
          "original_form": "저는 아이들 좀 재우고 올케요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:11"
        }
      ]
    }
