      "utterance": [
        {
          "id": "MDRW2100031311.1.1",
          "form": "우리 사야할 목록 정하자!",
          "original_form": "우리 사야할 목록 정하자!",
          "speaker_id": "1",
          "time": "20210904 20:20"
        },
        {
          "id": "MDRW2100031311.1.2",
          "form": "난 운동화 사야돼",
          "original_form": "난 운동화 사야돼",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100031311.1.3",
          "form": "방수로 살 거지?",
          "original_form": "방수로 살 거지?",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031311.1.4",
          "form": "방수도 좋긴 한데 일단 발에 맞는 게 중요해",
          "original_form": "방수도 좋긴 한데 일단 발에 맞는 게 중요해",
          "speaker_id": "2",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100031311.1.5",
          "form": "언제 몰에 가서 신어 봐야지 아님 종합매장 가서",
          "original_form": "언제 몰에 가서 신어 봐야지 아님 종합매장 가서",
          "speaker_id": "1",
          "time": "20210905 09:25"
        },
        {
          "id": "MDRW2100031311.1.6",
          "form": "방수커버도 사야하는데 다이소에는 잘 안 파는 듯.",
          "original_form": "방수커버도 사야하는데 다이소에는 잘 안 파는 듯.",
          "speaker_id": "2",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100031311.1.7",
          "form": "소도시라 그래ㅠㅠ 아님 재고 물어보거나 발주 신청해야할듯",
          "original_form": "소도시라 그래ㅠㅠ 아님 재고 물어보거나 발주 신청해야할듯",
          "speaker_id": "1",
          "time": "20210905 09:30"
        },
        {
          "id": "MDRW2100031311.1.8",
          "form": "그래야할 것같네 ㅠㅠ",
          "original_form": "그래야할 것같네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 09:33"
        },
        {
          "id": "MDRW2100031311.1.9",
          "form": "다른 브랜드는 생각해 봤어? 스케쳐스 말고 아식스",
          "original_form": "다른 브랜드는 생각해 봤어? 스케쳐스 말고 아식스",
          "speaker_id": "1",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100031311.1.10",
          "form": "거기도 방수기능 있어",
          "original_form": "거기도 방수기능 있어",
          "speaker_id": "1",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100031311.1.11",
          "form": "응.아식스도 좀 조사했어",
          "original_form": "응.아식스도 좀 조사했어",
          "speaker_id": "2",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100031311.1.12",
          "form": "오 롯데몰레 아식스 있나 모르겠네",
          "original_form": "오 롯데몰레 아식스 있나 모르겠네",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031311.1.13",
          "form": "신어보는게 젤 좋잖아",
          "original_form": "신어보는게 젤 좋잖아",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031311.1.14",
          "form": "롯데몰하고 이마트에 있어",
          "original_form": "롯데몰하고 이마트에 있어",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100031311.1.15",
          "form": "오 그럼 쇼핑할 때 하면 되겠네",
          "original_form": "오 그럼 쇼핑할 때 하면 되겠네",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031311.1.16",
          "form": "딴 거 뭐 사지? 영양제??",
          "original_form": "딴 거 뭐 사지? 영양제??",
          "speaker_id": "1",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100031311.1.17",
          "form": "영양제 다 먹었다고 안 했어? 그럼 사야지",
          "original_form": "영양제 다 먹었다고 안 했어? 그럼 사야지",
          "speaker_id": "2",
          "time": "20210905 09:44"
        }
      ]
