      "utterance": [
        {
          "id": "MDRW2100041375.1.1",
          "form": "고등어굽는방법문의요",
          "original_form": "고등어굽는방법문의요",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041375.1.2",
          "form": "그냥 3~4분간 앞뒤로 구워주면 됩니다",
          "original_form": "그냥 3~4분간 앞뒤로 구워주면 됩니다",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041375.1.3",
          "form": "제가 산거말이에요 ㅋㅋ",
          "original_form": "제가 산거말이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041375.1.4",
          "form": "이미 한번 훈제인지 익힌 거에요",
          "original_form": "이미 한번 훈제인지 익힌 거에요",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041375.1.5",
          "form": "중불 ? 강불 ? 기름은?",
          "original_form": "중불 ? 강불 ? 기름은?",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041375.1.6",
          "form": "중불이고 기름 노노",
          "original_form": "중불이고 기름 노노",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041375.1.7",
          "form": "고등어 본연의 기름이 나옴",
          "original_form": "고등어 본연의 기름이 나옴",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041375.1.8",
          "form": "대박... 궁금해요",
          "original_form": "대박... 궁금해요",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041375.1.9",
          "form": "(조리법에 써있는 그대로 ㅋㅋㅋ",
          "original_form": "(조리법에 써있는 그대로 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041375.1.10",
          "form": "냄새도 없고 맛나용",
          "original_form": "냄새도 없고 맛나용",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100041375.1.11",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100041375.1.12",
          "form": "얼마요",
          "original_form": "얼마요",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100041375.1.13",
          "form": "3천 얼마였던 거 같은데요 프레시지",
          "original_form": "3천 얼마였던 거 같은데요 프레시지",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100041375.1.14",
          "form": "그쵸 저희 엄마한테 인정을 받았답니다 그럼 믿고 먹어도 됩니다",
          "original_form": "그쵸 저희 엄마한테 인정을 받았답니다 그럼 믿고 먹어도 됩니다",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041375.1.15",
          "form": "ㅋㅋㅋㅋname2님맘픽은 인정이죠",
          "original_form": "ㅋㅋㅋㅋ&name2&님맘픽은 인정이죠",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041375.1.16",
          "form": "3마리에 9640원이었네여",
          "original_form": "3마리에 9640원이었네여",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041375.1.17",
          "form": "지금보니까 1마리에 3500이에요 담에 행사하면 삽시당!!",
          "original_form": "지금보니까 1마리에 3500이에요 담에 행사하면 삽시당!!",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041375.1.18",
          "form": "에어프라이어에 해도된대요",
          "original_form": "에어프라이어에 해도된대요",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041375.1.19",
          "form": "귀찬치만 팬에굽는게 맛은 잇는뎅",
          "original_form": "귀찬치만 팬에굽는게 맛은 잇는뎅",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041375.1.20",
          "form": "맞아요!!! 근데 집안에 생선냄새 날거같긴해요",
          "original_form": "맞아요!!! 근데 집안에 생선냄새 날거같긴해요",
          "speaker_id": "2",
          "time": "20210905 23:37"
        },
        {
          "id": "MDRW2100041375.1.21",
          "form": "그종...아파트살면 환  기돼서 나은대",
          "original_form": "그종...아파트살면 환  기돼서 나은대",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100041375.1.22",
          "form": "흠 저 예전 집이요 밑에층에서 환풍기돌리먄 저희집으로 냄새가 올라왔어요",
          "original_form": "흠 저 예전 집이요 밑에층에서 환풍기돌리먄 저희집으로 냄새가 올라왔어요",
          "speaker_id": "2",
          "time": "20210905 23:48"
        },
        {
          "id": "MDRW2100041375.1.23",
          "form": "화남요",
          "original_form": "화남요",
          "speaker_id": "2",
          "time": "20210905 23:48"
        },
        {
          "id": "MDRW2100041375.1.24",
          "form": "헉.... 아니 어케설계가됏길개",
          "original_form": "헉.... 아니 어케설계가됏길개",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100041375.1.25",
          "form": "몰라요 정말 ㅠㅠ 근데 또 저희집 환풍기는 고장이 나버려서 문 다 여는 수 밖에 없었어요",
