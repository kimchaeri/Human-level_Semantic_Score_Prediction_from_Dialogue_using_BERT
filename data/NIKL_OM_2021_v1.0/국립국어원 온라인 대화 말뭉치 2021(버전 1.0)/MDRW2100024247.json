      "utterance": [
        {
          "id": "MDRW2100024247.1.1",
          "form": "내일 점심 메뉴 정하셨어요?",
          "original_form": "내일 점심 메뉴 정하셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024247.1.2",
          "form": "아뇨 내일이 되어봐야 알것 같아요!",
          "original_form": "아뇨 내일이 되어봐야 알것 같아요!",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024247.1.3",
          "form": "대신 아침 씨리얼 먹으려고요",
          "original_form": "대신 아침 씨리얼 먹으려고요",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024247.1.4",
          "form": "헐 너무 좋다",
          "original_form": "헐 너무 좋다",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024247.1.5",
          "form": "저번에도 시리얼 먹지 않았어요?",
          "original_form": "저번에도 시리얼 먹지 않았어요?",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024247.1.6",
          "form": "네 ㅋㅋ 사놓으니 계속 먹게되네요 간편하고 맛있어서",
          "original_form": "네 ㅋㅋ 사놓으니 계속 먹게되네요 간편하고 맛있어서",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024247.1.7",
          "form": "낼 아침 드시나요?",
          "original_form": "낼 아침 드시나요?",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100024247.1.8",
          "form": "저는 내일 아침에 먹을 시간 없을 거 같아요",
          "original_form": "저는 내일 아침에 먹을 시간 없을 거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024247.1.9",
          "form": "근데 시리얼은 어떤 거 사셨어요 오레오?",
          "original_form": "근데 시리얼은 어떤 거 사셨어요 오레오?",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024247.1.10",
          "form": "아몬드후레이크요! 전 이게 젤 무난하고 괜찮더라구요",
          "original_form": "아몬드후레이크요! 전 이게 젤 무난하고 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024247.1.11",
          "form": "시리얼 좋아하시는거 있으신가요?",
          "original_form": "시리얼 좋아하시는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024247.1.12",
          "form": "엌ㅋㅋ 그거 설탕 안들었죠?",
          "original_form": "엌ㅋㅋ 그거 설탕 안들었죠?",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024247.1.13",
          "form": "저 그냥 무난하게 콘푸라이트 좋아해요",
          "original_form": "저 그냥 무난하게 콘푸라이트 좋아해요",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024247.1.14",
          "form": "설탕들었어용!",
          "original_form": "설탕들었어용!",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024247.1.15",
          "form": "저 어렸을 때 그거 한 번 먹고 안먹었는데 ㅜㅜ",
          "original_form": "저 어렸을 때 그거 한 번 먹고 안먹었는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024247.1.16",
          "form": "헐 그렇군요! 전 아몬드가 있어서 더 맛있는거 같아요 ㅋㅋ",
          "original_form": "헐 그렇군요! 전 아몬드가 있어서 더 맛있는거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024247.1.17",
          "form": "점심메뉴 정하신건 있나요?",
          "original_form": "점심메뉴 정하신건 있나요?",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024247.1.18",
          "form": "아 네네 저 정했어요",
          "original_form": "아 네네 저 정했어요",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024247.1.19",
          "form": "낼 점심 하려고 어제 미리 재료 준비했어요 크크",
          "original_form": "낼 점심 하려고 어제 미리 재료 준비했어요 크크",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024247.1.20",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024247.1.21",
          "form": "저 도시락 싸가려구요",
          "original_form": "저 도시락 싸가려구요",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024247.1.22",
          "form": "도시락이요? 와 부지런하시다",
          "original_form": "도시락이요? 와 부지런하시다",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024247.1.23",
          "form": "그 바게트에 소스 살짝 발라서",
          "original_form": "그 바게트에 소스 살짝 발라서",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024247.1.24",
          "form": "그 위에 연어랑 삶은계란 올려서 만들려구요",
          "original_form": "그 위에 연어랑 삶은계란 올려서 만들려구요",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024247.1.25",
          "form": "무슨 이름 있을텐데 잘 모르겠어요 ㅜ",
          "original_form": "무슨 이름 있을텐데 잘 모르겠어요 ㅜ",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024247.1.26",
          "form": "헐 말만들어도 맛있네요 ㅋㅋ",
          "original_form": "헐 말만들어도 맛있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024247.1.27",
          "form": "그쵸!! 마늘빵이였으면 더 맛있었을거에요",
          "original_form": "그쵸!! 마늘빵이였으면 더 맛있었을거에요",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024247.1.28",
          "form": "ㅋㅋㅋ피자스쿨도 먹고 싶네요",
          "original_form": "ㅋㅋㅋ피자스쿨도 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210830 23:52"
        }
      ]
