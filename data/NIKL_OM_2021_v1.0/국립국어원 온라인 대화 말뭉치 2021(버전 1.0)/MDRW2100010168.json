      "utterance": [
        {
          "id": "MDRW2100010168.1.1",
          "form": "점심 뭐 먹었어?",
          "original_form": "점심 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010168.1.2",
          "form": "초밥이랑 우동 먹었어 너 뭐 먹었어",
          "original_form": "초밥이랑 우동 먹었어 너 뭐 먹었어",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010168.1.3",
          "form": "나는 김치찌개랑 밥 먹었어",
          "original_form": "나는 김치찌개랑 밥 먹었어",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.4",
          "form": "초코우유도 마시고",
          "original_form": "초코우유도 마시고",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.5",
          "form": "맛있는 거 먹었네",
          "original_form": "맛있는 거 먹었네",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.6",
          "form": "김치찌개는 돼지 김치찌개 였어 참치 김치찌개 였어",
          "original_form": "김치찌개는 돼지 김치찌개 였어 참치 김치찌개 였어",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.7",
          "form": "돼지 김치찌개였어",
          "original_form": "돼지 김치찌개였어",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.8",
          "form": "넌 어떤 걸 더 좋아해?",
          "original_form": "넌 어떤 걸 더 좋아해?",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.9",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010168.1.10",
          "form": "나는 돼지김치찌개 좋아해 너도 그래 ?",
          "original_form": "나는 돼지김치찌개 좋아해 너도 그래 ?",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010168.1.11",
          "form": "응 참치도 좋지만 돼지김치찌개 제일 좋아해",
          "original_form": "응 참치도 좋지만 돼지김치찌개 제일 좋아해",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010168.1.12",
          "form": "너 좋아하는 음식은 뭐야?",
          "original_form": "너 좋아하는 음식은 뭐야?",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010168.1.13",
          "form": "난 과메기 찜 핫도그 좋아해",
          "original_form": "난 과메기 찜 핫도그 좋아해",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.14",
          "form": "꽈배기집 핫도그야",
          "original_form": "꽈배기집 핫도그야",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.15",
          "form": "과메기는 싫어해",
          "original_form": "과메기는 싫어해",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.16",
          "form": "가게에 주종목이 아닌 걸 좋아하네",
          "original_form": "가게에 주종목이 아닌 걸 좋아하네",
          "speaker_id": "1",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.17",
          "form": "개웃기다",
          "original_form": "개웃기다",
          "speaker_id": "1",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.19",
          "form": "과메기집 핫도그인 줄 알았어",
          "original_form": "과메기집 핫도그인 줄 알았어",
          "speaker_id": "1",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010168.1.20",
          "form": "아니야 그 집은 핫도그도 주종목이",
          "original_form": "아니야 그 집은 핫도그도 주종목이",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010168.1.21",
          "form": "주종목이야",
          "original_form": "주종목이야",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010168.1.22",
          "form": "치즈 핫도그 제일 잘 나간다고",
          "original_form": "치즈 핫도그 제일 잘 나간다고",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010168.1.23",
          "form": "핫도그 좋아하니",
          "original_form": "핫도그 좋아하니",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010168.1.24",
          "form": "ㄱㅋㅋㅋㅋㅋㅋ응 꽈배기집이면 그렇겠지",
          "original_form": "ㄱㅋㅋㅋㅋㅋㅋ응 꽈배기집이면 그렇겠지",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010168.1.25",
          "form": "응 핫도그 좋아해ㅋㅋ",
          "original_form": "응 핫도그 좋아해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010168.1.26",
          "form": "어떤 핫도그 좋아하니",
          "original_form": "어떤 핫도그 좋아하니",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010168.1.27",
          "form": "나 그냥 일반! 다른 건 이것저것 먹어보진 않았어",
          "original_form": "나 그냥 일반! 다른 건 이것저것 먹어보진 않았어",
          "speaker_id": "1",
          "time": "20210823 15:07"
        }
      ]
    }
  ]
