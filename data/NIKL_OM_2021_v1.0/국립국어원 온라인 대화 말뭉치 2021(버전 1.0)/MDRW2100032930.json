      "utterance": [
        {
          "id": "MDRW2100032930.1.1",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032930.1.2",
          "form": "오늘 동사무소 다녀와야해서",
          "original_form": "오늘 동사무소 다녀와야해서",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032930.1.3",
          "form": "아점 먹었어요:)",
          "original_form": "아점 먹었어요:)",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032930.1.4",
          "form": "점심시간이시죠?",
          "original_form": "점심시간이시죠?",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032930.1.5",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032930.1.6",
          "form": "점심먹고 사무실에서 널부러져있네요",
          "original_form": "점심먹고 사무실에서 널부러져있네요",
          "speaker_id": "2",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032930.1.7",
          "form": "오늘 메뉴는 뭔가요 ㅋ",
          "original_form": "오늘 메뉴는 뭔가요 ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100032930.1.8",
          "form": "황태콩나물국밥",
          "original_form": "황태콩나물국밥",
          "speaker_id": "2",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100032930.1.9",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210906 12:18"
        },
        {
          "id": "MDRW2100032930.1.10",
          "form": "아재메뉴ㅋㅋㅋㅋㅋ",
          "original_form": "아재메뉴ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100032930.1.11",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100032930.1.12",
          "form": "또 절 자극하시는군요",
          "original_form": "또 절 자극하시는군요",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100032930.1.13",
          "form": "ㅋㅋㅋ저 국밥 종류 좋아하는데",
          "original_form": "ㅋㅋㅋ저 국밥 종류 좋아하는데",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100032930.1.14",
          "form": "콩나물국밥은 제 취향이 아님 ㅜㅜ",
          "original_form": "콩나물국밥은 제 취향이 아님 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100032930.1.15",
          "form": "드시면서 크아~ 시원하다",
          "original_form": "드시면서 크아~ 시원하다",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100032930.1.16",
          "form": "이러셨죠? ㅋㅋㅋㅋ",
          "original_form": "이러셨죠? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100032930.1.17",
          "form": "제여친도 콩나물국밥 시러하는데",
          "original_form": "제여친도 콩나물국밥 시러하는데",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100032930.1.18",
          "form": "근데 시원하다 안했거든요?",
          "original_form": "근데 시원하다 안했거든요?",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100032930.1.19",
          "form": "완전 아재로 몰아가시네",
          "original_form": "완전 아재로 몰아가시네",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100032930.1.20",
          "form": "ㅋㅋ국밥은 시원하다~ 하면서\n먹어야쥬 ㅋㅋㅋ",
          "original_form": "ㅋㅋ국밥은 시원하다~ 하면서\n먹어야쥬 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100032930.1.21",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100032930.1.22",
          "form": "저 아재아니거든요?",
          "original_form": "저 아재아니거든요?",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100032930.1.23",
          "form": "아뇨... 국밥먹을땐\n그래줘야 맛있는데...",
          "original_form": "아뇨... 국밥먹을땐\n그래줘야 맛있는데...",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100032930.1.24",
          "form": "전 맛있게 먹어요",
          "original_form": "전 맛있게 먹어요",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100032930.1.25",
          "form": "그렇게 아재스럽게 안먹죠",
          "original_form": "그렇게 아재스럽게 안먹죠",
          "speaker_id": "2",
          "time": "20210906 20:12"
        }
      ]
    }
