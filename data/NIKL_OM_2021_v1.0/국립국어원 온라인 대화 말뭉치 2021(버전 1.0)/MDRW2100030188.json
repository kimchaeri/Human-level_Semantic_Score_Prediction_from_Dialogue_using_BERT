        {
          "id": "MDRW2100030188.1.1",
          "form": "지금",
          "original_form": "지금",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.2",
          "form": "치킨먹고있어요",
          "original_form": "치킨먹고있어요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.3",
          "form": "저녁인가요?",
          "original_form": "저녁인가요?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.4",
          "form": "네 77켄터키라고 아시낭ㅂ",
          "original_form": "네 77켄터키라고 아시낭ㅂ",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.5",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.6",
          "form": "늦게 드시네요",
          "original_form": "늦게 드시네요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.7",
          "form": "알죠 양 푸짐하죠",
          "original_form": "알죠 양 푸짐하죠",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.8",
          "form": "일을 늦게마쳐서요",
          "original_form": "일을 늦게마쳐서요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.9",
          "form": "감자도 푸짐합니다",
          "original_form": "감자도 푸짐합니다",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.10",
          "form": "배고팠겠네요",
          "original_form": "배고팠겠네요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.11",
          "form": "가족이 많아서 1마리 반",
          "original_form": "가족이 많아서 1마리 반",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030188.1.12",
          "form": "시켰어요",
          "original_form": "시켰어요",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.13",
          "form": "늦게먹으면 살쪄요",
          "original_form": "늦게먹으면 살쪄요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.14",
          "form": "맛있으면0칼로리",
          "original_form": "맛있으면0칼로리",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.15",
          "form": "1인1닭이니 1.5명인가요",
          "original_form": "1인1닭이니 1.5명인가요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.16",
          "form": "앗 그런가요",
          "original_form": "앗 그런가요",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.17",
          "form": "순살입니다",
          "original_form": "순살입니다",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.18",
          "form": "후라이드인가요",
          "original_form": "후라이드인가요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.19",
          "form": "프라이드 양념 간장",
          "original_form": "프라이드 양념 간장",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.20",
          "form": "양념이군요",
          "original_form": "양념이군요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030188.1.21",
          "form": "3가지맛이지요",
          "original_form": "3가지맛이지요",
