      "utterance": [
        {
          "id": "MDRW2100002571.1.1",
          "form": "너 대학 다녀?",
          "original_form": "너 대학 다녀?",
          "speaker_id": "1",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002571.1.2",
          "form": "대학 졸업했지",
          "original_form": "대학 졸업했지",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002571.1.3",
          "form": "오 어디 학과 나왔어?",
          "original_form": "오 어디 학과 나왔어?",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002571.1.4",
          "form": "물리치료과 나왔지~",
          "original_form": "물리치료과 나왔지~",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002571.1.5",
          "form": "난 대학을 안다녀서 모르는데 엠티 같은건 어땠어?",
          "original_form": "난 대학을 안다녀서 모르는데 엠티 같은건 어땠어?",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002571.1.6",
          "form": "엠티를 나도 1학년밖에 안해봐서 잘 모르겠긴한데",
          "original_form": "엠티를 나도 1학년밖에 안해봐서 잘 모르겠긴한데",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002571.1.7",
          "form": "우선 말해주자면",
          "original_form": "우선 말해주자면",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002571.1.8",
          "form": "술파티야 그냥",
          "original_form": "술파티야 그냥",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002571.1.9",
          "form": "뭐 장기자랑해서 주는 상품이 고량주라",
          "original_form": "뭐 장기자랑해서 주는 상품이 고량주라",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002571.1.10",
          "form": "진짜 술로 시작해서 술로 끝나는거같아",
          "original_form": "진짜 술로 시작해서 술로 끝나는거같아",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002571.1.11",
          "form": "그게 엠티야 술파티야..",
          "original_form": "그게 엠티야 술파티야..",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002571.1.12",
          "form": "나머지학년은 왜 참여 안한거야?",
          "original_form": "나머지학년은 왜 참여 안한거야?",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002571.1.13",
          "form": "술파티라서?",
          "original_form": "술파티라서?",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002571.1.14",
          "form": "2, 3학년때는",
          "original_form": "2, 3학년때는",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002571.1.15",
          "form": "내가 1학년때 3학년 졸업반 선배님들 사고땜에",
          "original_form": "내가 1학년때 3학년 졸업반 선배님들 사고땜에",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002571.1.16",
          "form": "못했지",
          "original_form": "못했지",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002571.1.17",
          "form": "1학년땐 전학년이 다하긴했어",
          "original_form": "1학년땐 전학년이 다하긴했어",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002571.1.18",
          "form": "다만 그 사고땜에 그 이후로 엠티없어지긴했어",
          "original_form": "다만 그 사고땜에 그 이후로 엠티없어지긴했어",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002571.1.19",
          "form": "뭔 사고가 터진거면 별수없지",
          "original_form": "뭔 사고가 터진거면 별수없지",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002571.1.20",
          "form": "동아리 같은건 했었어?",
          "original_form": "동아리 같은건 했었어?",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002571.1.21",
          "form": "대학 동아리는 안했고",
          "original_form": "대학 동아리는 안했고",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002571.1.22",
          "form": "고등학교땐 좀 많이하긴했지",
          "original_form": "고등학교땐 좀 많이하긴했지",
          "speaker_id": "2",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002571.1.23",
          "form": "다른 사람들 말 들어보면 과제때문에 스트레스라던데",
          "original_form": "다른 사람들 말 들어보면 과제때문에 스트레스라던데",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002571.1.24",
          "form": "넌 어땠어?",
          "original_form": "넌 어땠어?",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002571.1.25",
          "form": "난 의료계열인지라",
          "original_form": "난 의료계열인지라",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002571.1.26",
          "form": "과제 스케일이 너무 힘들었지",
          "original_form": "과제 스케일이 너무 힘들었지",
          "speaker_id": "2",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002571.1.27",
          "form": "도서관에서 전문서적 찾아야 나오는 단어를 조사해오고",
          "original_form": "도서관에서 전문서적 찾아야 나오는 단어를 조사해오고",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002571.1.28",
          "form": "물리치료 기구에서 나오는 물리학적 현상이나 그런거 조사한다던지",
          "original_form": "물리치료 기구에서 나오는 물리학적 현상이나 그런거 조사한다던지",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002571.1.29",
          "form": "엄청 많았어",
          "original_form": "엄청 많았어",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002571.1.30",
          "form": "역시 전문 분야라서 그런가 말만 들어도 복잡해보인다",
          "original_form": "역시 전문 분야라서 그런가 말만 들어도 복잡해보인다",
          "speaker_id": "1",
          "time": "20210820 13:49"
        }
      ]
