      "utterance": [
        {
          "id": "MDRW2100014701.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014701.1.2",
          "form": "스시 배민시켜서 먹엇어요",
          "original_form": "스시 배민시켜서 먹엇어요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014701.1.3",
          "form": "식사하셧나요?",
          "original_form": "식사하셧나요?",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014701.1.4",
          "form": "제가 이구역 푸드파이터 입니다",
          "original_form": "제가 이구역 푸드파이터 입니다",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014701.1.5",
          "form": "오ㅎ",
          "original_form": "오ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014701.1.6",
          "form": "전 콩국수 먹었어요",
          "original_form": "전 콩국수 먹었어요",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014701.1.7",
          "form": "콩물 좋아해서 콩물로 한대접 마신거같아요ㅋ",
          "original_form": "콩물 좋아해서 콩물로 한대접 마신거같아요ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014701.1.8",
          "form": "스시는 어땠어요?",
          "original_form": "스시는 어땠어요?",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014701.1.9",
          "form": "신선하던가요?",
          "original_form": "신선하던가요?",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014701.1.10",
          "form": "안그래도 친한 동생이 스시 먹고 포장해온다던데 \r\n이따 저도 스시 먹을거예요ㅋ",
          "original_form": "안그래도 친한 동생이 스시 먹고 포장해온다던데 \r\n이따 저도 스시 먹을거예요ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014701.1.11",
          "form": "이야 콩국수 저 환장해요ㅠㅠ",
          "original_form": "이야 콩국수 저 환장해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014701.1.12",
          "form": "스시는 쏘쏘였어요ㅠㅠ",
          "original_form": "스시는 쏘쏘였어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014701.1.13",
          "form": "맛났으면 좋았을텐데ㅎ\r\n다음엔 맛집으로 알아봐봐요ㅋㅋ",
          "original_form": "맛났으면 좋았을텐데ㅎ\r\n다음엔 맛집으로 알아봐봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014701.1.14",
          "form": "요즘 코시국이라 배달이 엄청 활성화되었더라구요",
          "original_form": "요즘 코시국이라 배달이 엄청 활성화되었더라구요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014701.1.15",
          "form": "내일 저녁메뉴 벌써 정햇어요",
          "original_form": "내일 저녁메뉴 벌써 정햇어요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014701.1.16",
          "form": "부.대.찌.개",
          "original_form": "부.대.찌.개",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014701.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014701.1.18",
          "form": "맛집이라 계속 먹던 가게인데 \r\n누룽지백숙도 배달된다해서 깜짝 놀랐지뭐예요",
          "original_form": "맛집이라 계속 먹던 가게인데 \r\n누룽지백숙도 배달된다해서 깜짝 놀랐지뭐예요",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014701.1.19",
          "form": "ㅋㅋㅋ\r\n전 보쌈ㅋ",
          "original_form": "ㅋㅋㅋ\r\n전 보쌈ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014701.1.20",
          "form": "요리 해먹는것도 좋지만 역시 뭐니뭐니해도 타인이 해준 밥이 좋아요ㅎ",
          "original_form": "요리 해먹는것도 좋지만 역시 뭐니뭐니해도 타인이 해준 밥이 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014701.1.21",
          "form": "우와ㅜㅜ",
          "original_form": "우와ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014701.1.22",
          "form": "누룽지백숙",
          "original_form": "누룽지백숙",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014701.1.23",
          "form": "미쳤ㅜ",
          "original_form": "미쳤ㅜ",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014701.1.24",
          "form": "전 요리 살림에 잼병이라서",
          "original_form": "전 요리 살림에 잼병이라서",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014701.1.25",
          "form": "배달음식 편의점 환장해요 ㅋㅋ",
          "original_form": "배달음식 편의점 환장해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014701.1.26",
          "form": "다행히 먹는거에비해서 건강한편이에욤",
          "original_form": "다행히 먹는거에비해서 건강한편이에욤",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014701.1.27",
          "form": "라면이 주식 ㅎㅎ",
          "original_form": "라면이 주식 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014701.1.28",
          "form": "ㅋㅋ요즘 간변식도 그렇고 배달도 그렇고 요리 못해도 하등 불편함 없을거같아요 오히려 더 맛나게 더 잘먹을수있는거 같아요ㅎ",
          "original_form": "ㅋㅋ요즘 간변식도 그렇고 배달도 그렇고 요리 못해도 하등 불편함 없을거같아요 오히려 더 맛나게 더 잘먹을수있는거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014701.1.29",
          "form": "라면 종류만해도 엄청나잖아요ㅋ",
          "original_form": "라면 종류만해도 엄청나잖아요ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014701.1.30",
          "form": "365일 한끼씩 새로운 메뉴로 먹어도 다 못먹어볼거같은데요ㅎ\r\n라면 말하니 순두부라면 먹고싶어지네요",
          "original_form": "365일 한끼씩 새로운 메뉴로 먹어도 다 못먹어볼거같은데요ㅎ\r\n라면 말하니 순두부라면 먹고싶어지네요",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014701.1.31",
          "form": "한때 순두부라면 유행이던데 드셔보셨어요?",
          "original_form": "한때 순두부라면 유행이던데 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014701.1.32",
          "form": "ㅋㅋㅋ열라면",
          "original_form": "ㅋㅋㅋ열라면",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014701.1.33",
          "form": "당연히먹엇슴다",
          "original_form": "당연히먹엇슴다",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014701.1.34",
          "form": "약속된맛이에요",
          "original_form": "약속된맛이에요",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014701.1.35",
          "form": "맞아여ㅋㅋㅋ",
          "original_form": "맞아여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014701.1.36",
          "form": "저는 순후추라면",
          "original_form": "저는 순후추라면",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.37",
          "form": "궁금해서",
          "original_form": "궁금해서",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.38",
          "form": "먹어봐야지 생각중이에요",
          "original_form": "먹어봐야지 생각중이에요",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.39",
          "form": "후추 상상 안가요",
          "original_form": "후추 상상 안가요",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.40",
          "form": "사리곰탕느낌",
          "original_form": "사리곰탕느낌",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.41",
          "form": "나더라구요",
          "original_form": "나더라구요",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.42",
          "form": "혹시 계란후라이에 후추 뿌려 드셔보셨어요?",
          "original_form": "혹시 계란후라이에 후추 뿌려 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.43",
          "form": "아니요ㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014701.1.44",
          "form": "전 뭔가 거부감? 안익숙해서 안돼던데 먹어본 사람이 극찬했어요",
          "original_form": "전 뭔가 거부감? 안익숙해서 안돼던데 먹어본 사람이 극찬했어요",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.45",
          "form": "저는 후라이 케찹 조합도",
          "original_form": "저는 후라이 케찹 조합도",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.46",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.47",
          "form": "낯설었어요",
          "original_form": "낯설었어요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.48",
          "form": "후추...시도해봐야겟네요",
          "original_form": "후추...시도해봐야겟네요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.49",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.50",
          "form": "전 후라이에 소금",
          "original_form": "전 후라이에 소금",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014701.1.51",
          "form": "근데 이거 대화제출 자동으로 되는거아닌가용?",
          "original_form": "근데 이거 대화제출 자동으로 되는거아닌가용?",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014701.1.52",
          "form": "깔끔하게 그냥 구울때 소금 살짝이 좋아요",
          "original_form": "깔끔하게 그냥 구울때 소금 살짝이 좋아요",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014701.1.53",
          "form": "소금이 베스트 ㅎㅎ",
          "original_form": "소금이 베스트 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014701.1.54",
          "form": "저 이거 오늘첨이라서",
          "original_form": "저 이거 오늘첨이라서",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014701.1.55",
          "form": "잘몰라요",
          "original_form": "잘몰라요",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014701.1.56",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
