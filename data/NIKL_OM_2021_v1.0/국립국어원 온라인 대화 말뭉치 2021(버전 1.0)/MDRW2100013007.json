      "utterance": [
        {
          "id": "MDRW2100013007.1.1",
          "form": "요즘 마라탕 자주 먹어요",
          "original_form": "요즘 마라탕 자주 먹어요",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013007.1.2",
          "form": "마라탕 드셔보셨나요?",
          "original_form": "마라탕 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013007.1.3",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013007.1.4",
          "form": "전 요새 떡볶이를 자주 먹어요",
          "original_form": "전 요새 떡볶이를 자주 먹어요",
          "speaker_id": "1",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100013007.1.5",
          "form": "떡볶이 맛이죠!",
          "original_form": "떡볶이 맛이죠!",
          "speaker_id": "2",
          "time": "20210825 10:25"
        },
        {
          "id": "MDRW2100013007.1.6",
          "form": "어디서 시켜드세요?",
          "original_form": "어디서 시켜드세요?",
          "speaker_id": "2",
          "time": "20210825 10:25"
        },
        {
          "id": "MDRW2100013007.1.7",
          "form": "신전이나 걸작떡볶이요",
          "original_form": "신전이나 걸작떡볶이요",
          "speaker_id": "1",
          "time": "20210825 10:29"
        },
        {
          "id": "MDRW2100013007.1.8",
          "form": "신전 중독적이죠",
          "original_form": "신전 중독적이죠",
          "speaker_id": "2",
          "time": "20210825 10:29"
        },
        {
          "id": "MDRW2100013007.1.9",
          "form": "신전에서 주로 뭐 시켜드세요?",
          "original_form": "신전에서 주로 뭐 시켜드세요?",
          "speaker_id": "2",
          "time": "20210825 10:30"
        },
        {
          "id": "MDRW2100013007.1.10",
          "form": "치즈떡볶이요",
          "original_form": "치즈떡볶이요",
          "speaker_id": "1",
          "time": "20210825 10:30"
        },
        {
          "id": "MDRW2100013007.1.11",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 10:31"
        },
        {
          "id": "MDRW2100013007.1.12",
          "form": "잘먹는 건 아닌데 좋아해요!",
          "original_form": "잘먹는 건 아닌데 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 11:22"
        },
        {
          "id": "MDRW2100013007.1.13",
          "form": "저두요! 기분 안좋을땐 꼭 먹게되요",
          "original_form": "저두요! 기분 안좋을땐 꼭 먹게되요",
          "speaker_id": "1",
          "time": "20210825 11:24"
        },
        {
          "id": "MDRW2100013007.1.14",
          "form": "맞아요ㅠㅠ 스트레스 받으면 꼭 생각나요",
          "original_form": "맞아요ㅠㅠ 스트레스 받으면 꼭 생각나요",
          "speaker_id": "2",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013007.1.15",
          "form": "걸작 떡볶이는 어떤가요? 한번도 안 먹어봐서요",
          "original_form": "걸작 떡볶이는 어떤가요? 한번도 안 먹어봐서요",
          "speaker_id": "2",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100013007.1.16",
          "form": "달달하게 매워서 좋아요.",
          "original_form": "달달하게 매워서 좋아요.",
          "speaker_id": "1",
          "time": "20210825 11:38"
        },
        {
          "id": "MDRW2100013007.1.17",
          "form": "좋아하는 브랜드 있으세요?",
          "original_form": "좋아하는 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013007.1.18",
          "form": "저는 응급실 많이 먹어요",
          "original_form": "저는 응급실 많이 먹어요",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013007.1.19",
          "form": "치즈가 너무 맛있어요ㅠㅠ",
          "original_form": "치즈가 너무 맛있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100013007.1.20",
          "form": "그렇구나. 한번 시켜 먹어봐야겠어요",
          "original_form": "그렇구나. 한번 시켜 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 12:14"
        },
        {
          "id": "MDRW2100013007.1.21",
          "form": "신전 드실 때 사이드 메뉴도 같이 드시나요?",
          "original_form": "신전 드실 때 사이드 메뉴도 같이 드시나요?",
          "speaker_id": "2",
          "time": "20210825 12:14"
        },
        {
          "id": "MDRW2100013007.1.22",
          "form": "튀김 시켜요. 신전 김밥은 독특하더라구요",
          "original_form": "튀김 시켜요. 신전 김밥은 독특하더라구요",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013007.1.23",
          "form": "신전에서 처음 먹었는데 어묵 튀김이 매력적이더라고요",
          "original_form": "신전에서 처음 먹었는데 어묵 튀김이 매력적이더라고요",
          "speaker_id": "2",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013007.1.24",
          "form": "신전 떡볶이랑 조합이 좋아요",
          "original_form": "신전 떡볶이랑 조합이 좋아요",
          "speaker_id": "2",
          "time": "20210825 12:23"
        }
      ]
    }
  ]
