      "utterance": [
        {
          "id": "MDRW2100023357.1.1",
          "form": "ㅎㅎ 어짜피 이혼하면 다갚을꺼라 상관은없는데ㅎ",
          "original_form": "ㅎㅎ 어짜피 이혼하면 다갚을꺼라 상관은없는데ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023357.1.2",
          "form": "가정일이니 이쪽이 맞겍죠?",
          "original_form": "가정일이니 이쪽이 맞겍죠?",
          "speaker_id": "1",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.3",
          "form": "그래도 해도 너무하더라구요ㅎ 무슨제가 많은돈은 쓰는것도아니고ㅎ",
          "original_form": "그래도 해도 너무하더라구요ㅎ 무슨제가 많은돈은 쓰는것도아니고ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.4",
          "form": "맞죠맞죠ㅎ",
          "original_form": "맞죠맞죠ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.5",
          "form": "제가봐도 전 아직 철이없는듯ㅎ",
          "original_form": "제가봐도 전 아직 철이없는듯ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.6",
          "form": "음  지금은 누가돈관리 해요?",
          "original_form": "음  지금은 누가돈관리 해요?",
          "speaker_id": "1",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.7",
          "form": "문제가 생겼으면 그거에대해",
          "original_form": "문제가 생겼으면 그거에대해",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.8",
          "form": "이젠 제가 가져왔어요 ㅡㅎ",
          "original_form": "이젠 제가 가져왔어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023357.1.9",
          "form": "그럴수밖에없는게ㄹ 어짜피 이혼할꺼라ㅎ",
          "original_form": "그럴수밖에없는게ㄹ 어짜피 이혼할꺼라ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.10",
          "form": "아 글쵸",
          "original_form": "아 글쵸",
          "speaker_id": "1",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.11",
          "form": "이제 제대로 관리해야죠ㅎ",
          "original_form": "이제 제대로 관리해야죠ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.12",
          "form": "대출금  이런거없는데 죽자인거죠",
          "original_form": "대출금  이런거없는데 죽자인거죠",
          "speaker_id": "1",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.13",
          "form": "적자",
          "original_form": "적자",
          "speaker_id": "1",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.14",
          "form": "정말 빚지는거싫어했는데ㅠ",
          "original_form": "정말 빚지는거싫어했는데ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.15",
          "form": "적자인건아닌데ㅎ",
          "original_form": "적자인건아닌데ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023357.1.16",
          "form": "어짜피 나중에 갚으면되는거라ㅎ",
          "original_form": "어짜피 나중에 갚으면되는거라ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:45"
        },
        {
          "id": "MDRW2100023357.1.17",
          "form": "암튼 경제적으로 너무 힘드네요ㅎㅎ",
          "original_form": "암튼 경제적으로 너무 힘드네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:45"
        },
        {
          "id": "MDRW2100023357.1.18",
          "form": "이제좀 정신 차려야죠ㅎ",
          "original_form": "이제좀 정신 차려야죠ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:45"
        },
        {
          "id": "MDRW2100023357.1.19",
          "form": "근데 식비 아이한테 넘 많이 들어가긴 하는거 같아요",
          "original_form": "근데 식비 아이한테 넘 많이 들어가긴 하는거 같아요",
          "speaker_id": "1",
          "time": "20210830 00:45"
        },
        {
          "id": "MDRW2100023357.1.20",
          "form": "저도 그렇게 생각해요ㅠ",
          "original_form": "저도 그렇게 생각해요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:46"
        },
        {
          "id": "MDRW2100023357.1.21",
          "form": "요거 조절을 잘 하셔야ㅠ할거같은",
          "original_form": "요거 조절을 잘 하셔야ㅠ할거같은",
          "speaker_id": "1",
          "time": "20210830 00:46"
        },
        {
          "id": "MDRW2100023357.1.22",
          "form": "솔직히 시켜먿어서 그런거같애요",
          "original_form": "솔직히 시켜먿어서 그런거같애요",
          "speaker_id": "2",
          "time": "20210830 00:46"
        }
      ]
