      "utterance": [
        {
          "id": "MDRW2100025262.1.1",
          "form": "산초도못드시겠네요?",
          "original_form": "산초도못드시겠네요?",
          "speaker_id": "1",
          "time": "20210901 03:33"
        },
        {
          "id": "MDRW2100025262.1.2",
          "form": "그게 뭐지요",
          "original_form": "그게 뭐지요",
          "speaker_id": "2",
          "time": "20210901 03:34"
        },
        {
          "id": "MDRW2100025262.1.3",
          "form": "윗지방분이시군요! 산초를 모르시다니",
          "original_form": "윗지방분이시군요! 산초를 모르시다니",
          "speaker_id": "1",
          "time": "20210901 03:34"
        },
        {
          "id": "MDRW2100025262.1.4",
          "form": "추어탕 안드시나요..? 어탕이나..?",
          "original_form": "추어탕 안드시나요..? 어탕이나..?",
          "speaker_id": "1",
          "time": "20210901 03:34"
        },
        {
          "id": "MDRW2100025262.1.5",
          "form": "저 서울 촌놈이라서....",
          "original_form": "저 서울 촌놈이라서....",
          "speaker_id": "2",
          "time": "20210901 03:34"
        },
        {
          "id": "MDRW2100025262.1.6",
          "form": "추어탕 먹어요",
          "original_form": "추어탕 먹어요",
          "speaker_id": "2",
          "time": "20210901 03:34"
        },
        {
          "id": "MDRW2100025262.1.7",
          "form": "맛있죠 그건",
          "original_form": "맛있죠 그건",
          "speaker_id": "2",
          "time": "20210901 03:34"
        },
        {
          "id": "MDRW2100025262.1.8",
          "form": "추어탕에 산초 안넣나요??",
          "original_form": "추어탕에 산초 안넣나요??",
          "speaker_id": "1",
          "time": "20210901 03:35"
        },
        {
          "id": "MDRW2100025262.1.9",
          "form": "서울은 아예 안넣어요??",
          "original_form": "서울은 아예 안넣어요??",
          "speaker_id": "1",
          "time": "20210901 03:35"
        },
        {
          "id": "MDRW2100025262.1.10",
          "form": "그거 살면서 처음 듣는 단어인데",
          "original_form": "그거 살면서 처음 듣는 단어인데",
          "speaker_id": "2",
          "time": "20210901 03:35"
        },
        {
          "id": "MDRW2100025262.1.11",
          "form": "산초는 축구선수 제이미 산초밖에",
          "original_form": "산초는 축구선수 제이미 산초밖에",
          "speaker_id": "2",
          "time": "20210901 03:36"
        },
        {
          "id": "MDRW2100025262.1.12",
          "form": "몰랐습니다 5분 전까지",
          "original_form": "몰랐습니다 5분 전까지",
          "speaker_id": "2",
          "time": "20210901 03:36"
        },
        {
          "id": "MDRW2100025262.1.13",
          "form": "추어탕이나 어탕에 산초가루라고 싸한 가루 넣어먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "추어탕이나 어탕에 산초가루라고 싸한 가루 넣어먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:36"
        },
        {
          "id": "MDRW2100025262.1.14",
          "form": "마라탕에 알싸한맛이 산초에요 ㅋㅋㅋ 아마 못드실듯하네요...",
          "original_form": "마라탕에 알싸한맛이 산초에요 ㅋㅋㅋ 아마 못드실듯하네요...",
          "speaker_id": "1",
          "time": "20210901 03:36"
        },
        {
          "id": "MDRW2100025262.1.15",
          "form": "저는 싸린이 하겠습니다....",
          "original_form": "저는 싸린이 하겠습니다....",
          "speaker_id": "2",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.16",
          "form": "아랫지방은 산초를 다양하게 먹습니다 이 작은 나라에 음식문화가 이마이 다르군요",
          "original_form": "아랫지방은 산초를 다양하게 먹습니다 이 작은 나라에 음식문화가 이마이 다르군요",
          "speaker_id": "1",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.17",
          "form": "이마이 어디지",
          "original_form": "이마이 어디지",
          "speaker_id": "2",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.18",
          "form": "산초로 지도 담그고 김치도 합니다",
          "original_form": "산초로 지도 담그고 김치도 합니다",
          "speaker_id": "1",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.19",
          "form": "경상도군요",
          "original_form": "경상도군요",
          "speaker_id": "2",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.20",
          "form": "맞나",
          "original_form": "맞나",
          "speaker_id": "2",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.21",
          "form": "맞추셨군요 ㅋㅋㅋ",
          "original_form": "맞추셨군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:37"
        },
        {
          "id": "MDRW2100025262.1.22",
          "form": "지가 뭐죠",
          "original_form": "지가 뭐죠",
          "speaker_id": "2",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.23",
          "form": "지?",
          "original_form": "지?",
          "speaker_id": "2",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.24",
          "form": "김지?",
          "original_form": "김지?",
          "speaker_id": "2",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.25",
          "form": "어.. 장아찌?",
          "original_form": "어.. 장아찌?",
          "speaker_id": "1",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.26",
          "form": "간장같은걸로 그 담그는거 있잖아요 ㅋㅋㅋ",
          "original_form": "간장같은걸로 그 담그는거 있잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.27",
          "form": "오 그게 지군요",
          "original_form": "오 그게 지군요",
          "speaker_id": "2",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.28",
          "form": "양파나 마늘이런거",
          "original_form": "양파나 마늘이런거",
          "speaker_id": "1",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025262.1.30",
          "form": "신기한 음식 공부",
          "original_form": "신기한 음식 공부",
          "speaker_id": "2",
