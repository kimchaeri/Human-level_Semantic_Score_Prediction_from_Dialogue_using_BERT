      "utterance": [
        {
          "id": "MDRW2100017598.1.1",
          "form": "지금 학생이세요??",
          "original_form": "지금 학생이세요??",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017598.1.2",
          "form": "네 재학중입니다",
          "original_form": "네 재학중입니다",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017598.1.3",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017598.1.4",
          "form": "저도 학생이에요",
          "original_form": "저도 학생이에요",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017598.1.5",
          "form": "오 이번학기 대면으로하시나요?",
          "original_form": "오 이번학기 대면으로하시나요?",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017598.1.6",
          "form": "아뇨ㅋㅋ 블렌딩수업이랑 비대면수업 있었는데 저는 다 비대면으로 했어요ㅋㅋ",
          "original_form": "아뇨ㅋㅋ 블렌딩수업이랑 비대면수업 있었는데 저는 다 비대면으로 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017598.1.7",
          "form": "오 좋은결정이시네요!",
          "original_form": "오 좋은결정이시네요!",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017598.1.8",
          "form": "막학기라 학교나가기 싫더라구요ㅋㅋ",
          "original_form": "막학기라 학교나가기 싫더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017598.1.9",
          "form": "name2은 대면수업 하세요??",
          "original_form": "&name2&은 대면수업 하세요??",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017598.1.10",
          "form": "저두 비대면이에요 ㅎㅎ",
          "original_form": "저두 비대면이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017598.1.11",
          "form": "확실히 비대면이 더 편하긴 해요ㅎㅎ",
          "original_form": "확실히 비대면이 더 편하긴 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017598.1.12",
          "form": "맞아요 수월하기도하구!",
          "original_form": "맞아요 수월하기도하구!",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017598.1.13",
          "form": "가끔은 오히려좋아욬ㅋㅋ",
          "original_form": "가끔은 오히려좋아욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017598.1.14",
          "form": "맞아요 학교까지 가는 것도 힘들어서..",
          "original_form": "맞아요 학교까지 가는 것도 힘들어서..",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017598.1.15",
          "form": "학교 가는 데 얼마나 걸리셨어요??",
          "original_form": "학교 가는 데 얼마나 걸리셨어요??",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017598.1.16",
          "form": "아 저는 해외유학생이라 좀 상황이 다른거같긴해요~ 거기서 기숙사써요 ㅎㅎ",
          "original_form": "아 저는 해외유학생이라 좀 상황이 다른거같긴해요~ 거기서 기숙사써요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017598.1.17",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
