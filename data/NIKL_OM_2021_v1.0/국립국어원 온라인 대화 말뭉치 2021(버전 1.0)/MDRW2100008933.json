      "utterance": [
        {
          "id": "MDRW2100008933.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008933.1.2",
          "form": "네 부타동 먹었어요",
          "original_form": "네 부타동 먹었어요",
          "speaker_id": "2",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008933.1.3",
          "form": "저녁 드셧나요?",
          "original_form": "저녁 드셧나요?",
          "speaker_id": "2",
          "time": "20210822 19:51"
        },
        {
          "id": "MDRW2100008933.1.4",
          "form": "김밥 먹고 왔어요",
          "original_form": "김밥 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008933.1.5",
          "form": "어떤 김밥 좋아하시나요?",
          "original_form": "어떤 김밥 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008933.1.6",
          "form": "저는 기본 김밥에 어묵 넣은거 좋아해요",
          "original_form": "저는 기본 김밥에 어묵 넣은거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 19:56"
        },
        {
          "id": "MDRW2100008933.1.7",
          "form": "저는 참치김밥 좋아해요",
          "original_form": "저는 참치김밥 좋아해요",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008933.1.8",
          "form": "어떤 음식 제일 좋아하시나요?",
          "original_form": "어떤 음식 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008933.1.9",
          "form": "라면이요ㅋㅋ 국민음식",
          "original_form": "라면이요ㅋㅋ 국민음식",
          "speaker_id": "1",
          "time": "20210822 19:57"
        },
        {
          "id": "MDRW2100008933.1.10",
          "form": "어떤 라면 좋아하세요??",
          "original_form": "어떤 라면 좋아하세요??",
          "speaker_id": "2",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008933.1.11",
          "form": "저는 진라면이랑 진짬뽕 좋아해요",
          "original_form": "저는 진라면이랑 진짬뽕 좋아해요",
          "speaker_id": "2",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008933.1.12",
          "form": "저는 신라면이랑 불닭이요",
          "original_form": "저는 신라면이랑 불닭이요",
          "speaker_id": "1",
          "time": "20210822 20:02"
        },
        {
          "id": "MDRW2100008933.1.13",
          "form": "매운거 잘드시나봐요",
          "original_form": "매운거 잘드시나봐요",
          "speaker_id": "2",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008933.1.14",
          "form": "저는 까르보불닭이 최대예요",
          "original_form": "저는 까르보불닭이 최대예요",
          "speaker_id": "2",
          "time": "20210822 20:04"
        },
        {
          "id": "MDRW2100008933.1.15",
          "form": "저도 잘먹는 편은 아닌데 울면서도 불닭은 먹어요",
          "original_form": "저도 잘먹는 편은 아닌데 울면서도 불닭은 먹어요",
          "speaker_id": "1",
          "time": "20210822 20:05"
        },
        {
          "id": "MDRW2100008933.1.16",
          "form": "배달음식 자주 시켜드시나요?",
          "original_form": "배달음식 자주 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210822 20:07"
        },
        {
          "id": "MDRW2100008933.1.17",
          "form": "잘 안먹어요",
          "original_form": "잘 안먹어요",
          "speaker_id": "1",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100008933.1.18",
          "form": "대신 레토르트 사서 해먹어요",
          "original_form": "대신 레토르트 사서 해먹어요",
