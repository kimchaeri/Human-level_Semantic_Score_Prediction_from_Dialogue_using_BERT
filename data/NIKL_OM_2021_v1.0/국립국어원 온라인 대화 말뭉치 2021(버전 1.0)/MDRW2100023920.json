      "utterance": [
        {
          "id": "MDRW2100023920.1.1",
          "form": "ㅋㅋ 아 그러네요",
          "original_form": "ㅋㅋ 아 그러네요",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.2",
          "form": "제 멸치 맛있습니다",
          "original_form": "제 멸치 맛있습니다",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.3",
          "form": "부러우시죠",
          "original_form": "부러우시죠",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.4",
          "form": "아 전 아까 하이오커피에서",
          "original_form": "아 전 아까 하이오커피에서",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.5",
          "form": "바다소금라떼 사왔는데",
          "original_form": "바다소금라떼 사왔는데",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.6",
          "form": "진짜 맛있어요",
          "original_form": "진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.7",
          "form": "부러우시죠?",
          "original_form": "부러우시죠?",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100023920.1.8",
          "form": "어케그런",
          "original_form": "어케그런",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100023920.1.9",
          "form": "라떼 생크림에",
          "original_form": "라떼 생크림에",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100023920.1.10",
          "form": "소금이 들어가서",
          "original_form": "소금이 들어가서",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100023920.1.11",
          "form": "이시간에 라떼를드시면 안되니까 저도 나눠마셔드리죠",
          "original_form": "이시간에 라떼를드시면 안되니까 저도 나눠마셔드리죠",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100023920.1.12",
          "form": "단짠",
          "original_form": "단짠",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100023920.1.13",
          "form": "단짠의 정석",
          "original_form": "단짠의 정석",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100023920.1.14",
          "form": "영도 핫플 사신다고 자랑하시는겁니까",
          "original_form": "영도 핫플 사신다고 자랑하시는겁니까",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.15",
          "form": "체인카페니까",
          "original_form": "체인카페니까",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.16",
          "form": "어디서 구하신겁니까",
          "original_form": "어디서 구하신겁니까",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.17",
          "form": "시간되시면 근처에  있나 검색해보세요",
          "original_form": "시간되시면 근처에  있나 검색해보세요",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.18",
          "form": "하이오커피",
          "original_form": "하이오커피",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.19",
          "form": "없을듯..드니드니타워에는...",
          "original_form": "없을듯..드니드니타워에는...",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.20",
          "form": "없어..ㅜ",
          "original_form": "없어..ㅜ",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.21",
          "form": "바다소금라떼",
          "original_form": "바다소금라떼",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100023920.1.22",
          "form": "여긴 한강녹조라떼",
          "original_form": "여긴 한강녹조라떼",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023920.1.23",
          "form": "이런거뿐일듯",
          "original_form": "이런거뿐일듯",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023920.1.24",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100023920.1.25",
          "form": "혹시 담에 부산오시게 되면 꼭 마시러가요",
          "original_form": "혹시 담에 부산오시게 되면 꼭 마시러가요",
          "speaker_id": "2",
          "time": "20210830 21:43"
        }
