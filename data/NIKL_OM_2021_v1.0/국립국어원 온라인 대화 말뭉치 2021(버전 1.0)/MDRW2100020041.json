      "utterance": [
        {
          "id": "MDRW2100020041.1.1",
          "form": "10시에요?",
          "original_form": "10시에요?",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020041.1.2",
          "form": "암튼 여자가 올라왔는데",
          "original_form": "암튼 여자가 올라왔는데",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020041.1.3",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020041.1.4",
          "form": "10시번 잘시간 아닌가요?",
          "original_form": "10시번 잘시간 아닌가요?",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020041.1.5",
          "form": "10시가 잘시간인가요?ㅎ",
          "original_form": "10시가 잘시간인가요?ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020041.1.6",
          "form": "애기가?ㅎㅎ",
          "original_form": "애기가?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020041.1.7",
          "form": "그때 낮잠을 잘때라ㅎ 좀늦게잤어요ㅎㅎ",
          "original_form": "그때 낮잠을 잘때라ㅎ 좀늦게잤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020041.1.8",
          "form": "네네  혹시 애기들디 뛰엇나 싶어서 이야기 해본건데",
          "original_form": "네네  혹시 애기들디 뛰엇나 싶어서 이야기 해본건데",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020041.1.9",
          "form": "애기한명이고ㅎㅎ 그때 책읽는데 얼마나 떠들었겟어요ㅎ",
          "original_form": "애기한명이고ㅎㅎ 그때 책읽는데 얼마나 떠들었겟어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.10",
          "form": "아하 그럼   오해 받을수 있는 상황 되는건가요",
          "original_form": "아하 그럼   오해 받을수 있는 상황 되는건가요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.11",
          "form": "읔 아래집이 넘 예민 한거 같아요",
          "original_form": "읔 아래집이 넘 예민 한거 같아요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.12",
          "form": "제가 피해를 줬을수도있는데",
          "original_form": "제가 피해를 줬을수도있는데",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.13",
          "form": "너무 예민하게 반응했던것같애요ㅎ",
          "original_form": "너무 예민하게 반응했던것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.14",
          "form": "나오라고 하더라구요ㅎ 그때 마스크도 안쓰고올라와서는",
          "original_form": "나오라고 하더라구요ㅎ 그때 마스크도 안쓰고올라와서는",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.15",
          "form": "아마 술취한것같더라구요ㅎ",
          "original_form": "아마 술취한것같더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.16",
          "form": "헐 무대뽀",
          "original_form": "헐 무대뽀",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020041.1.17",
          "form": "그래서 내일 이야기하시죠 했죠ㅎ",
          "original_form": "그래서 내일 이야기하시죠 했죠ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.18",
          "form": "올라오는더 자체가 불법인데",
          "original_form": "올라오는더 자체가 불법인데",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.19",
          "form": "제가 이야기한것도 아니고 아내가 이야기해ㅛ는데",
          "original_form": "제가 이야기한것도 아니고 아내가 이야기해ㅛ는데",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.20",
          "form": "나중에 흥분하더니",
          "original_form": "나중에 흥분하더니",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.21",
          "form": "칼들고올라와ㅛ어요ㅎㅎ",
          "original_form": "칼들고올라와ㅛ어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.22",
          "form": "헐....",
          "original_form": "헐....",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.23",
          "form": "아주그냥 막장에ㅎㅎ",
          "original_form": "아주그냥 막장에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.24",
          "form": "살다살다 그런사람들 처음봐요ㅎ",
          "original_form": "살다살다 그런사람들 처음봐요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.25",
          "form": "신고감 아닌가여...",
          "original_form": "신고감 아닌가여...",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020041.1.26",
          "form": "온라인커뮤니티에서는 있을 수 없는일이죠ㅎ",
          "original_form": "온라인커뮤니티에서는 있을 수 없는일이죠ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.27",
          "form": "그거때문데 딴데 이사간거에요?",
          "original_form": "그거때문데 딴데 이사간거에요?",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.28",
          "form": "당연하죠ㅎㅎ 신고했죠ㅎ",
          "original_form": "당연하죠ㅎㅎ 신고했죠ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.29",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.30",
          "form": "신고하고 영상확보해놓고",
          "original_form": "신고하고 영상확보해놓고",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.31",
          "form": "진짜 이런일이 있네요 ㄷㄷㄷ",
          "original_form": "진짜 이런일이 있네요 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.32",
          "form": "이사온거에요ㅎ",
          "original_form": "이사온거에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.33",
          "form": "알아보니 밑에집에 애가 3이더라구요",
          "original_form": "알아보니 밑에집에 애가 3이더라구요",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020041.1.34",
          "form": "우잉",
          "original_form": "우잉",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.35",
          "form": "선처 해주고 그냥 우리가 이사왔어요",
          "original_form": "선처 해주고 그냥 우리가 이사왔어요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.36",
          "form": "저도 아이 아빠라",
          "original_form": "저도 아이 아빠라",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.37",
          "form": "정신이 아프신분인듯하네요",
          "original_form": "정신이 아프신분인듯하네요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.38",
          "form": "이해해줘야지",
          "original_form": "이해해줘야지",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.39",
          "form": "전과가 있었나봐요",
          "original_form": "전과가 있었나봐요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.40",
          "form": "헐!!",
          "original_form": "헐!!",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.41",
          "form": "원래부터가 그런사람들이었어요ㅎㄹ",
          "original_form": "원래부터가 그런사람들이었어요ㅎㄹ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.42",
          "form": "전 회사를 가야하지만",
          "original_form": "전 회사를 가야하지만",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.43",
          "form": "무섭네요",
          "original_form": "무섭네요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.44",
          "form": "아내랑 아이는 무섭죠",
          "original_form": "아내랑 아이는 무섭죠",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020041.1.45",
          "form": "그래서 이사를 갔어요ㅎㅎ",
          "original_form": "그래서 이사를 갔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020041.1.46",
          "form": "그런 이웃 만날까 무섭",
          "original_form": "그런 이웃 만날까 무섭",
          "speaker_id": "1",
          "time": "20210827 23:30"
        }
      ]
