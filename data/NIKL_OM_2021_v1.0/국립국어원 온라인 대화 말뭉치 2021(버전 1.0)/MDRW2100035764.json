        {
          "id": "MDRW2100035764.1.1",
          "form": "스벅가면 자허블...ㅎㅎ",
          "original_form": "스벅가면 자허블...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.2",
          "form": "우왕 이디야가 근처에 생겼는데",
          "original_form": "우왕 이디야가 근처에 생겼는데",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.3",
          "form": "아 자허블이 맛있긴하죠ㅎㅎ",
          "original_form": "아 자허블이 맛있긴하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.4",
          "form": "주중에 도전이요!",
          "original_form": "주중에 도전이요!",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.5",
          "form": "여기꺼 찐하고 맛있었어요!",
          "original_form": "여기꺼 찐하고 맛있었어요!",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.6",
          "form": "밀크티도 좋아하시나용?",
          "original_form": "밀크티도 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.7",
          "form": "추천감사해요 ㅋㅋ",
          "original_form": "추천감사해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.8",
          "form": "버블티?",
          "original_form": "버블티?",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.9",
          "form": "네네 저 버블티도 좋아해요 ㅎㅎ",
          "original_form": "네네 저 버블티도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035764.1.10",
          "form": "오오 저랑 입맛이 비슷하신것같아요!",
          "original_form": "오오 저랑 입맛이 비슷하신것같아요!",
          "speaker_id": "1",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035764.1.11",
          "form": "공차 타로티 좋아해요ㅎㅎ",
          "original_form": "공차 타로티 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035764.1.12",
          "form": "저 지금 다 메모해놨어요 ㅋㅋ",
          "original_form": "저 지금 다 메모해놨어요 ㅋㅋ",
