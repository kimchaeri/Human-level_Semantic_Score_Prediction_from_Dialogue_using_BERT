      "utterance": [
        {
          "id": "MDRW2100012148.1.1",
          "form": "알바 친구들이랑하면 즐거워요",
          "original_form": "알바 친구들이랑하면 즐거워요",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012148.1.2",
          "form": "저도 어렸을때 친구랑 같이 일했는데",
          "original_form": "저도 어렸을때 친구랑 같이 일했는데",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100012148.1.3",
          "form": "맞아요 그때 피자집에서 또래애들이랑 알바 같이하니까 넘재밌었어요",
          "original_form": "맞아요 그때 피자집에서 또래애들이랑 알바 같이하니까 넘재밌었어요",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100012148.1.4",
          "form": "어떤거하셨어요",
          "original_form": "어떤거하셨어요",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100012148.1.5",
          "form": "저는 스크린골프요! 거기가 골프레슨이랑 같이하는곳인데",
          "original_form": "저는 스크린골프요! 거기가 골프레슨이랑 같이하는곳인데",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100012148.1.6",
          "form": "어쩌다가 친구랑 같이 구해서 일했어요",
          "original_form": "어쩌다가 친구랑 같이 구해서 일했어요",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100012148.1.7",
          "form": "간식이 많아서 매번 주워먹었네요!",
          "original_form": "간식이 많아서 매번 주워먹었네요!",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100012148.1.8",
          "form": "미피 일하면 밥은 어떻게 되는거예요?",
          "original_form": "미피 일하면 밥은 어떻게 되는거예요?",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100012148.1.9",
          "form": "와 스크린골프장 궁금했었는데",
          "original_form": "와 스크린골프장 궁금했었는데",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012148.1.10",
          "form": "밥은 보통 피자 원하는거말하면 만들어주고 저희가 만들어먹을때도있고요 ㅎㅎ",
          "original_form": "밥은 보통 피자 원하는거말하면 만들어주고 저희가 만들어먹을때도있고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012148.1.11",
          "form": "우와... 최고예요",
          "original_form": "우와... 최고예요",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012148.1.12",
          "form": "거기있는메뉴들많이먹고 가끔은 근처김밥집시켜먹었어요",
          "original_form": "거기있는메뉴들많이먹고 가끔은 근처김밥집시켜먹었어요",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012148.1.13",
          "form": "그럼 샐러드바도 같이 가져다 드실수 있는거예요?",
          "original_form": "그럼 샐러드바도 같이 가져다 드실수 있는거예요?",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012148.1.14",
          "form": "그정도의 여유는없었어요ㅜ",
          "original_form": "그정도의 여유는없었어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012148.1.15",
          "form": "아ㅠㅠ바빠서 그건 안되었군요",
          "original_form": "아ㅠㅠ바빠서 그건 안되었군요",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012148.1.16",
          "form": "따로 식사시간이있는게아니라 시간빌때 한명씩들어가서 허겁지겁먹고나와가지구여",
          "original_form": "따로 식사시간이있는게아니라 시간빌때 한명씩들어가서 허겁지겁먹고나와가지구여",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012148.1.17",
          "form": "네ㅜ",
          "original_form": "네ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012148.1.18",
          "form": "스크린골프는 별거없었어요 손님들오면 신발이랑 장갑주고",
          "original_form": "스크린골프는 별거없었어요 손님들오면 신발이랑 장갑주고",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012148.1.19",
          "form": "대신 그때일하면서 자주 먹으러갔었어요 쉬는날",
          "original_form": "대신 그때일하면서 자주 먹으러갔었어요 쉬는날",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012148.1.20",
          "form": "원하는골프장? 틀어주는건데",
          "original_form": "원하는골프장? 틀어주는건데",
          "speaker_id": "2",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012148.1.21",
          "form": "골프에대해서도 알아야하나요?",
          "original_form": "골프에대해서도 알아야하나요?",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012148.1.22",
          "form": "골프에대해서도 잘알아야되나요?",
          "original_form": "골프에대해서도 잘알아야되나요?",
          "speaker_id": "1",
          "time": "20210824 19:44"
        }
      ]
    }
  ]
