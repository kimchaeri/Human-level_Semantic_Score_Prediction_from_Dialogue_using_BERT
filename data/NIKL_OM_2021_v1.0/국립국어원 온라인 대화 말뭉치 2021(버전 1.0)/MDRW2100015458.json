      "utterance": [
        {
          "id": "MDRW2100015458.1.1",
          "form": "음 베놈이랑 어벤져스?",
          "original_form": "음 베놈이랑 어벤져스?",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015458.1.2",
          "form": "두개가 같은 세계관은 아닌 거지?",
          "original_form": "두개가 같은 세계관은 아닌 거지?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015458.1.3",
          "form": "내가 알기에는 다른걸로 알고있어",
          "original_form": "내가 알기에는 다른걸로 알고있어",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015458.1.4",
          "form": "나는 은근히 그거 구별하는 것도 어렵더라",
          "original_form": "나는 은근히 그거 구별하는 것도 어렵더라",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015458.1.5",
          "form": "첨 보면 그럴 수 있지",
          "original_form": "첨 보면 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015458.1.6",
          "form": "그리고 일단 주인공 급 인물들이 너무 많아",
          "original_form": "그리고 일단 주인공 급 인물들이 너무 많아",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015458.1.7",
          "form": "맞아 그래서 보는 듯",
          "original_form": "맞아 그래서 보는 듯",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015458.1.8",
          "form": "아 그게 또 재미 포인트 구나",
          "original_form": "아 그게 또 재미 포인트 구나",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015458.1.9",
          "form": "응 그렇지",
          "original_form": "응 그렇지",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015458.1.10",
          "form": "다 기억이 나?",
          "original_form": "다 기억이 나?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015458.1.11",
          "form": "음 대충?보면서 이해함",
          "original_form": "음 대충?보면서 이해함",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015458.1.12",
          "form": "그냥 나와서 보면 아는 거구나",
          "original_form": "그냥 나와서 보면 아는 거구나",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015458.1.13",
          "form": "좋아하는 영화있어?",
          "original_form": "좋아하는 영화있어?",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015458.1.14",
          "form": "글쎄 막 기억나는 건 없는데 그래도 좀 힐링 되는 걸 좋아하는 것 같아",
          "original_form": "글쎄 막 기억나는 건 없는데 그래도 좀 힐링 되는 걸 좋아하는 것 같아",
          "speaker_id": "1",
