      "utterance": [
        {
          "id": "MDRW2100002765.1.1",
          "form": "날씨 좋은데 여행은 안가시나요?",
          "original_form": "날씨 좋은데 여행은 안가시나요?",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002765.1.2",
          "form": "내일은 가까운 곳이라도 나들이 갈 생각이에요",
          "original_form": "내일은 가까운 곳이라도 나들이 갈 생각이에요",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002765.1.3",
          "form": "비만 안오면 나들이 좋겠어요 어디로요?",
          "original_form": "비만 안오면 나들이 좋겠어요 어디로요?",
          "speaker_id": "1",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002765.1.4",
          "form": "아 ㅎ 그냥 가까운 부여나 공주요",
          "original_form": "아 ㅎ 그냥 가까운 부여나 공주요",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002765.1.5",
          "form": "아~그쪽이 가까우신가봐요",
          "original_form": "아~그쪽이 가까우신가봐요",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002765.1.6",
          "form": "네 ㅎ 사실 가고픈 곳은 문경새재 가서 걷고싶은데 거긴 멀어서요",
          "original_form": "네 ㅎ 사실 가고픈 곳은 문경새재 가서 걷고싶은데 거긴 멀어서요",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002765.1.7",
          "form": "아~~그쪽은 한번도 안가봤어요",
          "original_form": "아~~그쪽은 한번도 안가봤어요",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002765.1.8",
          "form": "여름휴가는 가셨어요? 저는 그냥 당일치기로 충주갔다왔어요",
          "original_form": "여름휴가는 가셨어요? 저는 그냥 당일치기로 충주갔다왔어요",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002765.1.9",
          "form": "네~저는 가까운 호텔로 호캉스요",
          "original_form": "네~저는 가까운 호텔로 호캉스요",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002765.1.10",
          "form": "으~ 최고의 휴가였네요^^",
          "original_form": "으~ 최고의 휴가였네요^^",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002765.1.11",
          "form": "거의 먹고 자고 먹고 자고 ㅎㅎ",
          "original_form": "거의 먹고 자고 먹고 자고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
