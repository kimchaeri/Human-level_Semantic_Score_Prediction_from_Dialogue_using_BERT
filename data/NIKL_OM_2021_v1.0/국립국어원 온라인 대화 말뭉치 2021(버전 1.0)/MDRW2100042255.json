      "utterance": [
        {
          "id": "MDRW2100042255.1.1",
          "form": "과일 좋아하시나요?",
          "original_form": "과일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042255.1.2",
          "form": "네   요즘 샤인머스켓 나오는데",
          "original_form": "네   요즘 샤인머스켓 나오는데",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042255.1.3",
          "form": "어제주문했어용",
          "original_form": "어제주문했어용",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042255.1.4",
          "form": "오 샤인머스켓 맛있죠",
          "original_form": "오 샤인머스켓 맛있죠",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042255.1.5",
          "form": "진짜 맛이 특이하더라구요 망고맛이 나요",
          "original_form": "진짜 맛이 특이하더라구요 망고맛이 나요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042255.1.6",
          "form": "비싸서글칭ㅠ맛이 굿",
          "original_form": "비싸서글칭ㅠ맛이 굿",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042255.1.7",
          "form": "맞아요 가격이 사악하지만 맛있기에 사먹습니다..",
          "original_form": "맞아요 가격이 사악하지만 맛있기에 사먹습니다..",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042255.1.8",
          "form": "맞아요ㅠ근데비싼데 안달면 너무화나여",
          "original_form": "맞아요ㅠ근데비싼데 안달면 너무화나여",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042255.1.9",
          "form": "진짜루요 샤인머스켓 맛안나고 그냥 청포도맛 나는 것도 있더라구요",
          "original_form": "진짜루요 샤인머스켓 맛안나고 그냥 청포도맛 나는 것도 있더라구요",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042255.1.10",
          "form": "ㅠㅠ씅나여",
          "original_form": "ㅠㅠ씅나여",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042255.1.11",
          "form": "이게무슨맛인고 싶더라구여ㅠㅠ",
          "original_form": "이게무슨맛인고 싶더라구여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042255.1.12",
          "form": "그니까요 과일 비싸게 주고샀는데 맛없을때가 제일 화나더라구요!!",
          "original_form": "그니까요 과일 비싸게 주고샀는데 맛없을때가 제일 화나더라구요!!",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042255.1.13",
          "form": "그랬으면 좋겠네요 ㅋㅋ",
          "original_form": "그랬으면 좋겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100042255.1.14",
          "form": "어떤과일 젤 좋아하세요?",
          "original_form": "어떤과일 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 17:30"
        },
        {
          "id": "MDRW2100042255.1.15",
          "form": "음 딱복욬",
          "original_form": "음 딱복욬",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042255.1.16",
          "form": "오 딱복파이시군요 저는 물복파였는데 딱볶도 요즘 참 맛있더라구요..",
          "original_form": "오 딱복파이시군요 저는 물복파였는데 딱볶도 요즘 참 맛있더라구요..",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100042255.1.17",
          "form": "아직 딱복을 못먹었어여ㅠㅠ복숭아 왜이리비싸죠ㅠ",
          "original_form": "아직 딱복을 못먹었어여ㅠㅠ복숭아 왜이리비싸죠ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100042255.1.18",
          "form": "요즘 과일값 넘 비싸지 않나요 ..ㅠㅠㅠ",
          "original_form": "요즘 과일값 넘 비싸지 않나요 ..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100042255.1.19",
          "form": "그나마 어제 핫딜로 샤인머스켓 사서 오늘왔는데 못먹었네요ㅜ바빠서ㅋㅈㅋ",
          "original_form": "그나마 어제 핫딜로 샤인머스켓 사서 오늘왔는데 못먹었네요ㅜ바빠서ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210907 23:11"
        }
      ]
