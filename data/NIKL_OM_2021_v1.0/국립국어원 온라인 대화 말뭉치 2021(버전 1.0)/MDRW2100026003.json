      "utterance": [
        {
          "id": "MDRW2100026003.1.1",
          "form": "안녕하세요.ㅎㅎ",
          "original_form": "안녕하세요.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100026003.1.2",
          "form": "안녕하세요 온라인 커뮤니티 하시나요?",
          "original_form": "안녕하세요 온라인 커뮤니티 하시나요?",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100026003.1.3",
          "form": "저는 더쿠만 가입했어요. 아실라나?",
          "original_form": "저는 더쿠만 가입했어요. 아실라나?",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026003.1.4",
          "form": "아 더쿠 구글링하다 봤어요",
          "original_form": "아 더쿠 구글링하다 봤어요",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100026003.1.5",
          "form": "저두요.ㅋㅋ 커뮤니티 하는거 있으세요?",
          "original_form": "저두요.ㅋㅋ 커뮤니티 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100026003.1.6",
          "form": "커뮤니티는 따로 안하고 페북만해요",
          "original_form": "커뮤니티는 따로 안하고 페북만해요",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100026003.1.7",
          "form": "저는 인스타만 조금ㅋㅋ",
          "original_form": "저는 인스타만 조금ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100026003.1.8",
          "form": "다들 화려한거 올리는 경쟁붙은거같아요",
          "original_form": "다들 화려한거 올리는 경쟁붙은거같아요",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100026003.1.9",
          "form": "인스타는 확실히.그러더라구요.",
          "original_form": "인스타는 확실히.그러더라구요.",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026003.1.10",
          "form": "트위터는 불행경쟁이고",
          "original_form": "트위터는 불행경쟁이고",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026003.1.11",
          "form": "인스타는 뭐더라",
          "original_form": "인스타는 뭐더라",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026003.1.12",
          "form": "그런 말도있더라구요",
          "original_form": "그런 말도있더라구요",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026003.1.13",
          "form": "진짜 그런거 같네요. 트위터는 불만. 불행 얘기만하구ㅋㅋㅋ",
          "original_form": "진짜 그런거 같네요. 트위터는 불만. 불행 얘기만하구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026003.1.14",
          "form": "네 그렇다고 하더라구요",
          "original_form": "네 그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026003.1.15",
          "form": "트위터는 잘안해서 모르긴한데...",
          "original_form": "트위터는 잘안해서 모르긴한데...",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026003.1.16",
          "form": "저누인스타는 보기만 하는데. 친구 하는거 보면",
          "original_form": "저누인스타는 보기만 하는데. 친구 하는거 보면",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100026003.1.17",
          "form": "이 한장을 올리기위해 엄청 노력하는 느낌ㅋㅋ",
          "original_form": "이 한장을 올리기위해 엄청 노력하는 느낌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100026003.1.18",
          "form": "아 그죠 전 페북하는데 거기 셀카올리는거도",
          "original_form": "아 그죠 전 페북하는데 거기 셀카올리는거도",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100026003.1.19",
          "form": "막 포샵하고 그런단 말 듣고 놀랐어요",
          "original_form": "막 포샵하고 그런단 말 듣고 놀랐어요",
          "speaker_id": "2",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100026003.1.20",
          "form": "인스타 사진은 실물이랑 넘달라요.ㅋㅋㅋ",
          "original_form": "인스타 사진은 실물이랑 넘달라요.ㅋㅋㅋ",
          "speaker_id": "1",
