      "utterance": [
        {
          "id": "MDRW2100021182.1.1",
          "form": "좋아하는 카페 있으신가요",
          "original_form": "좋아하는 카페 있으신가요",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021182.1.2",
          "form": "주변의 작은 개인카페 탐방을 좋아해요",
          "original_form": "주변의 작은 개인카페 탐방을 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021182.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100021182.1.4",
          "form": "아 저는 디저트류따라 좋아하는 카페가 갈려요",
          "original_form": "아 저는 디저트류따라 좋아하는 카페가 갈려요",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021182.1.5",
          "form": "가령 프레즐은 탐탐이요",
          "original_form": "가령 프레즐은 탐탐이요",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021182.1.6",
          "form": "오 탐탐이 프레즐 맛집이었군요",
          "original_form": "오 탐탐이 프레즐 맛집이었군요",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021182.1.7",
          "form": "몰랐어요",
          "original_form": "몰랐어요",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021182.1.8",
          "form": "일반 프레즐과 다르게 이탈리아 프레즐을 팔아요",
          "original_form": "일반 프레즐과 다르게 이탈리아 프레즐을 팔아요",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021182.1.9",
          "form": "이탈리아 프레즐? 그게 뭔가요",
          "original_form": "이탈리아 프레즐? 그게 뭔가요",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021182.1.10",
          "form": "피자같은 맛나는 거예요",
          "original_form": "피자같은 맛나는 거예요",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021182.1.11",
          "form": "빵속에 치즈 토마토소스 넣고",
          "original_form": "빵속에 치즈 토마토소스 넣고",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021182.1.12",
          "form": "완전히 피자 조합이네요 정말",
          "original_form": "완전히 피자 조합이네요 정말",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021182.1.13",
          "form": "네  피자빵보다 훨씬 나아요. 전 페퍼로니 프레즐 좋아해요",
          "original_form": "네  피자빵보다 훨씬 나아요. 전 페퍼로니 프레즐 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021182.1.14",
          "form": "마침 집근처에 탐탐이 있네요",
          "original_form": "마침 집근처에 탐탐이 있네요",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021182.1.15",
          "form": "사먹어봐야겠어요",
          "original_form": "사먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021182.1.16",
          "form": "네 디저트보다 식사에 가까워요",
          "original_form": "네 디저트보다 식사에 가까워요",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021182.1.17",
          "form": "배부를때 시키지 마세요",
          "original_form": "배부를때 시키지 마세요",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021182.1.18",
          "form": "명심할게요!",
          "original_form": "명심할게요!",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021182.1.19",
          "form": "아 탐탐이 음료는 그저 그래요ㅜ",
          "original_form": "아 탐탐이 음료는 그저 그래요ㅜ",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021182.1.20",
          "form": "그냥 아메리카노 드셔요",
          "original_form": "그냥 아메리카노 드셔요",
          "speaker_id": "2",
          "time": "20210828 16:42"
        }
