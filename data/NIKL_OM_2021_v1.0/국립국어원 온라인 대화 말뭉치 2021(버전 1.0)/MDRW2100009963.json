      "utterance": [
        {
          "id": "MDRW2100009963.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009963.1.2",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "2",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009963.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009963.1.4",
          "form": "전 요즘 로제떡볶이에 빠져있어요~",
          "original_form": "전 요즘 로제떡볶이에 빠져있어요~",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009963.1.5",
          "form": "어머 어느 브랜드꺼 제일 좋아하세요?",
          "original_form": "어머 어느 브랜드꺼 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009963.1.6",
          "form": "전 배떡이 가장 좋아여! 순한맛으로ㅎㅎ",
          "original_form": "전 배떡이 가장 좋아여! 순한맛으로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100009963.1.7",
          "form": "배떡 순한맛은 한 번도 안 먹어 봤는데 안느끼한가요?",
          "original_form": "배떡 순한맛은 한 번도 안 먹어 봤는데 안느끼한가요?",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100009963.1.8",
          "form": "네 전 살짝 매웠어요ㅋㅋㅋ같이 먹던 친구는 무슨 맛으로 먹냐 하긴 했지만요",
          "original_form": "네 전 살짝 매웠어요ㅋㅋㅋ같이 먹던 친구는 무슨 맛으로 먹냐 하긴 했지만요",
          "speaker_id": "1",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100009963.1.9",
          "form": "느끼하지 않다니 다행이네요",
          "original_form": "느끼하지 않다니 다행이네요",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100009963.1.10",
          "form": "느끼하다 느끼실 수도 있어요! 전 그런 맛을 좋아해서..",
          "original_form": "느끼하다 느끼실 수도 있어요! 전 그런 맛을 좋아해서..",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100009963.1.11",
          "form": "요즘 너무 초등학교 앞 떡볶이가 땡겨요",
          "original_form": "요즘 너무 초등학교 앞 떡볶이가 땡겨요",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100009963.1.12",
          "form": "맞아요 뭔가 자극적이지 않고 달달한 떡볶이가 땡길 때가 있죠",
          "original_form": "맞아요 뭔가 자극적이지 않고 달달한 떡볶이가 땡길 때가 있죠",
          "speaker_id": "2",
          "time": "20210823 21:02"
        },
        {
          "id": "MDRW2100009963.1.13",
          "form": "맞아요 슬러쉬랑 같이 먹어줘야해요ㅋㅋ",
          "original_form": "맞아요 슬러쉬랑 같이 먹어줘야해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:01"
        },
        {
          "id": "MDRW2100009963.1.14",
          "form": "그리고 오뎅국물까지!!",
          "original_form": "그리고 오뎅국물까지!!",
          "speaker_id": "2",
          "time": "20210824 10:56"
        },
        {
          "id": "MDRW2100009963.1.15",
          "form": "먹을 줄 아시네요ㅎㅎㅎ",
          "original_form": "먹을 줄 아시네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100009963.1.16",
          "form": "최근에 튀긴 떡볶이 먹어봤는데 얘도 되게 맛있어요!",
          "original_form": "최근에 튀긴 떡볶이 먹어봤는데 얘도 되게 맛있어요!",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100009963.1.17",
          "form": "튀긴 떡볶이는 뭔가요?",
          "original_form": "튀긴 떡볶이는 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100009963.1.18",
          "form": "떡꼬치 떡처럼 튀기고 떡볶이 소스랑 먹는거예요",
          "original_form": "떡꼬치 떡처럼 튀기고 떡볶이 소스랑 먹는거예요",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100009963.1.19",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210826 21:41"
        }
      ]
