      "utterance": [
        {
          "id": "MDRW2100022258.1.1",
          "form": "name1님은",
          "original_form": "&name1&님은",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022258.1.2",
          "form": "어떨거같아요",
          "original_form": "어떨거같아요",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022258.1.3",
          "form": "저는 사실..  살아나는것보다..",
          "original_form": "저는 사실..  살아나는것보다..",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100022258.1.4",
          "form": "수련씨가 책을봤는지가 궁금해요..",
          "original_form": "수련씨가 책을봤는지가 궁금해요..",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100022258.1.5",
          "form": "10조",
          "original_form": "10조",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.6",
          "form": "들어있는 그 책이요?ㅋㅋㅋ",
          "original_form": "들어있는 그 책이요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.7",
          "form": "네.",
          "original_form": "네.",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.8",
          "form": "도대체 왜 안보는거지",
          "original_form": "도대체 왜 안보는거지",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.9",
          "form": "아마 읽었겠죠?",
          "original_form": "아마 읽었겠죠?",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.10",
          "form": "10조인데..!",
          "original_form": "10조인데..!",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.11",
          "form": "나중에 회상씬으로 나오지 않을까 싶어요",
          "original_form": "나중에 회상씬으로 나오지 않을까 싶어요",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.12",
          "form": "아..  그러려나요",
          "original_form": "아..  그러려나요",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022258.1.13",
          "form": "저 매주 그거 신경 쓰고있었거든요ㅋㅋㅋ",
          "original_form": "저 매주 그거 신경 쓰고있었거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022258.1.14",
          "form": "그 진짜 주단태가",
          "original_form": "그 진짜 주단태가",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022258.1.15",
          "form": "방에서 확인하고 갔었잖아요?",
          "original_form": "방에서 확인하고 갔었잖아요?",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022258.1.16",
          "form": "그 후로 눈치채고 열어보지 않았을까....",
          "original_form": "그 후로 눈치채고 열어보지 않았을까....",
          "speaker_id": "1",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022258.1.17",
          "form": "아.. 그랬으려나요",
          "original_form": "아.. 그랬으려나요",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022258.1.18",
          "form": "그나저나 주단태 얼굴요 ㅋㅋㅋ",
          "original_form": "그나저나 주단태 얼굴요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022258.1.19",
          "form": "지가 무슨 조커인가.. 그거 보고 너무 어이가없어서",
          "original_form": "지가 무슨 조커인가.. 그거 보고 너무 어이가없어서",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022258.1.20",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100022258.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100022258.1.22",
          "form": "그장면 왜 안보여주는지",
          "original_form": "그장면 왜 안보여주는지",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022258.1.23",
          "form": "어떤거요",
          "original_form": "어떤거요",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022258.1.24",
          "form": "시간없어서 편집한걸까요?",
          "original_form": "시간없어서 편집한걸까요?",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022258.1.25",
          "form": "주단태얼굴이요",
          "original_form": "주단태얼굴이요",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022258.1.26",
          "form": "아 왜 꼬메게되었는가",
          "original_form": "아 왜 꼬메게되었는가",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022258.1.27",
          "form": "이거요?",
          "original_form": "이거요?",
          "speaker_id": "2",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100022258.1.28",
          "form": "경도 화상입으면서 부상입어서 그런거아니었나요",
          "original_form": "경도 화상입으면서 부상입어서 그런거아니었나요",
          "speaker_id": "2",
          "time": "20210829 15:00"
        }
      ]
