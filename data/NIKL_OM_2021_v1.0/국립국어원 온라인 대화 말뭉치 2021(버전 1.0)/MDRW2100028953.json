      "utterance": [
        {
          "id": "MDRW2100028953.1.1",
          "form": "name3 오셨네요ㅎㅎㅎ",
          "original_form": "&name3& 오셨네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028953.1.2",
          "form": "애기 키우는게 쉽지가않네요ㅎㅎ",
          "original_form": "애기 키우는게 쉽지가않네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028953.1.3",
          "form": "저희는 거의 한달에 18만원? 아마 더들꺼애요",
          "original_form": "저희는 거의 한달에 18만원? 아마 더들꺼애요",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028953.1.4",
          "form": "어떸 부분이요",
          "original_form": "어떸 부분이요",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028953.1.5",
          "form": "아 영양제요?",
          "original_form": "아 영양제요?",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028953.1.6",
          "form": "영양제요ㅎㅎㅎㅎ",
          "original_form": "영양제요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028953.1.7",
          "form": "아마 더들꺼에요ㅎㅎ",
          "original_form": "아마 더들꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028953.1.8",
          "form": "아앙 저는 아직 쟁여둔게 ㅎ",
          "original_form": "아앙 저는 아직 쟁여둔게 ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028953.1.9",
          "form": "대충 계산해봐도ㅎ 그정도ㅎ",
          "original_form": "대충 계산해봐도ㅎ 그정도ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028953.1.10",
          "form": "전 락피도 적립금떼도 쟁여놔서 ㅎ",
          "original_form": "전 락피도 적립금떼도 쟁여놔서 ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028953.1.11",
          "form": "이염ㅠㅠ",
          "original_form": "이염ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028953.1.12",
          "form": "전 name1에 비하면ㅠㅠ 아직은 배울께 많네요ㅠ",
          "original_form": "전 &name1&에 비하면ㅠㅠ 아직은 배울께 많네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028953.1.13",
          "form": "어린이꺼 뜨면  공유해드릴게요 ㄹㄹ",
          "original_form": "어린이꺼 뜨면  공유해드릴게요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028953.1.14",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028953.1.15",
          "form": "애기꺼에대한 결정권한이 없어서ㅎㅎ",
          "original_form": "애기꺼에대한 결정권한이 없어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028953.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028953.1.17",
          "form": "아내가 다해서 그냥ㅎ 믿고 맡겨요",
          "original_form": "아내가 다해서 그냥ㅎ 믿고 맡겨요",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028953.1.18",
          "form": "제품 어디꺼 주로 먹여여",
          "original_form": "제품 어디꺼 주로 먹여여",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028953.1.19",
          "form": "미네랄이구나ㅎㅎ 미네랄은 네이쳐팜",
          "original_form": "미네랄이구나ㅎㅎ 미네랄은 네이쳐팜",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028953.1.20",
          "form": "비타민도 네이쳐팜ㅎ",
          "original_form": "비타민도 네이쳐팜ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028953.1.21",
          "form": "락토비타",
          "original_form": "락토비타",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028953.1.22",
          "form": "일동 비타민d",
          "original_form": "일동 비타민d",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028953.1.23",
          "form": "계속 다니던 약국이있어서ㅎ 하나씩 더 챙겨주긴하는데ㅎ 그래도 비싸요ㅎㅎ",
          "original_form": "계속 다니던 약국이있어서ㅎ 하나씩 더 챙겨주긴하는데ㅎ 그래도 비싸요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028953.1.24",
          "form": "아함 이게 다 선호하는 브랜드가 잇어서",
          "original_form": "아함 이게 다 선호하는 브랜드가 잇어서",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028953.1.25",
          "form": "별로 안먹이네요ㅎ",
          "original_form": "별로 안먹이네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028953.1.26",
          "form": "근데 맨날 먹는거보면 많이먹는것같은 느낌이ㅎ",
          "original_form": "근데 맨날 먹는거보면 많이먹는것같은 느낌이ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028953.1.27",
          "form": "그래도 애들께 비싸긴한거 같어요",
          "original_form": "그래도 애들께 비싸긴한거 같어요",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028953.1.28",
          "form": "ㅎㅎ 맞아요ㅎ",
          "original_form": "ㅎㅎ 맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028953.1.29",
          "form": "애들껀 비싸더라구요ㅎ",
          "original_form": "애들껀 비싸더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028953.1.30",
          "form": "이래저래 돈들어갈때가 많은건 사실이에요ㅎㅎ",
          "original_form": "이래저래 돈들어갈때가 많은건 사실이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028953.1.31",
