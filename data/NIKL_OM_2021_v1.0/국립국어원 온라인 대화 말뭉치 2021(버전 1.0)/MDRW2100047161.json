      "utterance": [
        {
          "id": "MDRW2100047161.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100047161.1.2",
          "form": "키크고 잘생긴사람이요",
          "original_form": "키크고 잘생긴사람이요",
          "speaker_id": "2",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100047161.1.3",
          "form": "이상형은요?",
          "original_form": "이상형은요?",
          "speaker_id": "2",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047161.1.4",
          "form": "저는 잘웃는사람 좋아해요 착한사람",
          "original_form": "저는 잘웃는사람 좋아해요 착한사람",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047161.1.5",
          "form": "키는 큰게 역시 좋지요",
          "original_form": "키는 큰게 역시 좋지요",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100047161.1.6",
          "form": "결혼관은 어떠신가요",
          "original_form": "결혼관은 어떠신가요",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100047161.1.7",
          "form": "결혼은 아직 생각이없어요 그럴 상황도 안되는거같고요 언제 결혼하고싶으세요",
          "original_form": "결혼은 아직 생각이없어요 그럴 상황도 안되는거같고요 언제 결혼하고싶으세요",
          "speaker_id": "2",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100047161.1.8",
          "form": "돈만 많으면 혼자사는게 짱인거 같아요",
          "original_form": "돈만 많으면 혼자사는게 짱인거 같아요",
          "speaker_id": "1",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100047161.1.9",
          "form": "요즘 이상한 사람이 많아서",
          "original_form": "요즘 이상한 사람이 많아서",
          "speaker_id": "1",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100047161.1.10",
          "form": "결혼도 무서워요",
          "original_form": "결혼도 무서워요",
          "speaker_id": "1",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100047161.1.11",
          "form": "맞아요 그럼 최고죠",
          "original_form": "맞아요 그럼 최고죠",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100047161.1.12",
          "form": "돈이 없어서 문제네요  ㅠ",
          "original_form": "돈이 없어서 문제네요  ㅠ",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100047161.1.13",
          "form": "연예인중에 이상형 있나요",
          "original_form": "연예인중에 이상형 있나요",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100047161.1.14",
          "form": "맞아요 결혼하려면 집도그렇고 결혼식비용도 그렇고 만만치않게들테니 문제네요",
          "original_form": "맞아요 결혼하려면 집도그렇고 결혼식비용도 그렇고 만만치않게들테니 문제네요",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100047161.1.15",
          "form": "송강 배우 잘생겼더라구요",
          "original_form": "송강 배우 잘생겼더라구요",
          "speaker_id": "2",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100047161.1.16",
          "form": "오 어깨 진짜 넓던데요",
          "original_form": "오 어깨 진짜 넓던데요",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100047161.1.17",
          "form": "저는 박보검이요",
          "original_form": "저는 박보검이요",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100047161.1.18",
          "form": "최근 티비에 나왔더라구요",
          "original_form": "최근 티비에 나왔더라구요",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100047161.1.19",
          "form": "그러게요 저도 봤는데 잘생겼더라구요",
          "original_form": "그러게요 저도 봤는데 잘생겼더라구요",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100047161.1.20",
          "form": "세상에는 왜이렇게 잘생긴사람이 많죠",
          "original_form": "세상에는 왜이렇게 잘생긴사람이 많죠",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100047161.1.21",
          "form": "근데 제꺼는 없네요",
          "original_form": "근데 제꺼는 없네요",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100047161.1.22",
          "form": "ㅋㅋ그러게요 다른세계사람들인가봐요",
          "original_form": "ㅋㅋ그러게요 다른세계사람들인가봐요",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100047161.1.23",
          "form": "부럽네요 그런 사람이랑 결혼하는 사람들은 누군지",
          "original_form": "부럽네요 그런 사람이랑 결혼하는 사람들은 누군지",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100047161.1.24",
          "form": "그러게요 다 지인들 통해서 만나는것같던데 궁금해지네요",
          "original_form": "그러게요 다 지인들 통해서 만나는것같던데 궁금해지네요",
          "speaker_id": "2",
          "time": "20210901 15:53"
        }
      ]
    }
