      "utterance": [
        {
          "id": "MDRW2100019486.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019486.1.2",
          "form": "마지막 학기를 앞둔 대학생이에요",
          "original_form": "마지막 학기를 앞둔 대학생이에요",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019486.1.3",
          "form": "앗 전 졸업했어요",
          "original_form": "앗 전 졸업했어요",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019486.1.4",
          "form": "기억에 남는 강의 있어요?",
          "original_form": "기억에 남는 강의 있어요?",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019486.1.5",
          "form": "문화유산답사라는 수업이 기억나요",
          "original_form": "문화유산답사라는 수업이 기억나요",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019486.1.6",
          "form": "아 답사 수업 재밌죠 답사 실제로도 갔어요?",
          "original_form": "아 답사 수업 재밌죠 답사 실제로도 갔어요?",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019486.1.7",
          "form": "네 ㅎㅎㅎ막 문화유산 답사 같이 가니까 고등학생이 된 기분이었어요",
          "original_form": "네 ㅎㅎㅎ막 문화유산 답사 같이 가니까 고등학생이 된 기분이었어요",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019486.1.8",
          "form": "ㅋㅋㅋ 현장학습 같았나봐요",
          "original_form": "ㅋㅋㅋ 현장학습 같았나봐요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019486.1.9",
          "form": "어디 갔었어요?",
          "original_form": "어디 갔었어요?",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019486.1.10",
          "form": "경복궁을 갔었어요 ㅎㅎㅎㅎ",
          "original_form": "경복궁을 갔었어요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019486.1.11",
          "form": "아 저 궁투어 좋아해요",
          "original_form": "아 저 궁투어 좋아해요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019486.1.12",
          "form": "궁 좋죠 힐링되고",
          "original_form": "궁 좋죠 힐링되고",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019486.1.13",
          "form": "저는 답사 수업때 현충원 갔어요 ㅋㅋ",
          "original_form": "저는 답사 수업때 현충원 갔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019486.1.14",
          "form": "장소만 들으면 정말 재미없네요ㅋㅋ",
          "original_form": "장소만 들으면 정말 재미없네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019486.1.15",
          "form": "오 답사 수업 다들 많이 하는군요",
          "original_form": "오 답사 수업 다들 많이 하는군요",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019486.1.16",
          "form": "그런가봐요 인기강의라 들었던 것 같기도 해요ㅋㅋ",
          "original_form": "그런가봐요 인기강의라 들었던 것 같기도 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019486.1.17",
          "form": "제가 들은 건 공간의 의미를 찾는 그런 수업이었어요ㅋㅋ",
          "original_form": "제가 들은 건 공간의 의미를 찾는 그런 수업이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019486.1.18",
          "form": "ㅋㅋㅋㅋㅋ재미있으셨나요",
          "original_form": "ㅋㅋㅋㅋㅋ재미있으셨나요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019486.1.19",
          "form": "ㅋㅋ 아무래도 장소가 장소라 배산임수 풍광이 멋졌던 기억이 있네요ㅋㅋ",
          "original_form": "ㅋㅋ 아무래도 장소가 장소라 배산임수 풍광이 멋졌던 기억이 있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:56"
        }
      ]
