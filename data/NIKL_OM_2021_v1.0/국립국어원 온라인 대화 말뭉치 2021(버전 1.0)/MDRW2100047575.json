      "utterance": [
        {
          "id": "MDRW2100047575.1.1",
          "form": "저 구글 상품권 며칠뒤 끝인거",
          "original_form": "저 구글 상품권 며칠뒤 끝인거",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047575.1.2",
          "form": "확다 썼어요 ㅜㅜ",
          "original_form": "확다 썼어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047575.1.3",
          "form": "겨우 기간 맞췄어요",
          "original_form": "겨우 기간 맞췄어요",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047575.1.4",
          "form": "오늘 뭐 사신거 있으세요 ?",
          "original_form": "오늘 뭐 사신거 있으세요 ?",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047575.1.5",
          "form": "와 기간맞춰서 다행이네요",
          "original_form": "와 기간맞춰서 다행이네요",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047575.1.6",
          "form": "오늘 산건 없어요 기다리는 건 잇고 ㅋㅋㅋ",
          "original_form": "오늘 산건 없어요 기다리는 건 잇고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047575.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047575.1.8",
          "form": "뭐 기다리세요?",
          "original_form": "뭐 기다리세요?",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047575.1.9",
          "form": "구글 담엔 안쓰고 꼭 팔게요",
          "original_form": "구글 담엔 안쓰고 꼭 팔게요",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047575.1.10",
          "form": "쓸때 없는거 샀어요 ㅜㅜ",
          "original_form": "쓸때 없는거 샀어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047575.1.11",
          "form": "마음의 양식은 늘겠죠 ㅎㅎ",
          "original_form": "마음의 양식은 늘겠죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047575.1.12",
          "form": "전 마스크 특대형 사려했더니 다품절",
          "original_form": "전 마스크 특대형 사려했더니 다품절",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047575.1.13",
          "form": "이게 뭔일린지",
          "original_form": "이게 뭔일린지",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047575.1.14",
          "form": "못샀어요",
          "original_form": "못샀어요",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047575.1.15",
          "form": "품절 ㅠㅠ슬프네요",
          "original_form": "품절 ㅠㅠ슬프네요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047575.1.16",
          "form": "전 폰 기다리고 잇어요",
          "original_form": "전 폰 기다리고 잇어요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047575.1.17",
          "form": "네 에어데이즈요",
          "original_form": "네 에어데이즈요",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047575.1.18",
          "form": "요새 대형이 없어요",
          "original_form": "요새 대형이 없어요",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047575.1.19",
          "form": "다 어디간거죠?",
          "original_form": "다 어디간거죠?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047575.1.20",
          "form": "뭐사서 기다리세요?",
          "original_form": "뭐사서 기다리세요?",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100047575.1.21",
          "form": "전 고기산게 계속 안오네요",
          "original_form": "전 고기산게 계속 안오네요",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047575.1.22",
          "form": "일주일 된거 같은데 ㅋㅋ",
          "original_form": "일주일 된거 같은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047575.1.23",
          "form": "핸드폰을 사서 기다리고잇어요 ㅋㅋㅋㅋ",
          "original_form": "핸드폰을 사서 기다리고잇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100047575.1.24",
          "form": "아니 고기가 왜 일주일동안이나...!!",
          "original_form": "아니 고기가 왜 일주일동안이나...!!",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100047575.1.25",
          "form": "잡아서 온데요 ㅋㅋ",
          "original_form": "잡아서 온데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047575.1.26",
          "form": "농담같죠",
          "original_form": "농담같죠",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047575.1.27",
          "form": "오 사전예약",
          "original_form": "오 사전예약",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047575.1.28",
          "form": "하신거예욮",
          "original_form": "하신거예욮",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047575.1.29",
          "form": "와 좋으시겠다",
          "original_form": "와 좋으시겠다",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047575.1.30",
          "form": "네 사전예약 막차탔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 사전예약 막차탔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047575.1.31",
          "form": "어머 사전예약이 선물도 많고 싸대요",
          "original_form": "어머 사전예약이 선물도 많고 싸대요",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047575.1.32",
          "form": "선물 뭐줘요?",
          "original_form": "선물 뭐줘요?",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047575.1.33",
          "form": "언제 온대요 너무 기다려 지겠다 ㅎㅎ",
          "original_form": "언제 온대요 너무 기다려 지겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047575.1.34",
          "form": "저도 막 사고 싶네요",
          "original_form": "저도 막 사고 싶네요",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100047575.1.35",
          "form": "버즈2랑 정품케이스를 준대요",
          "original_form": "버즈2랑 정품케이스를 준대요",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100047575.1.36",
          "form": "악세사리랑",
          "original_form": "악세사리랑",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047575.1.37",
          "form": "오 버즈 좋은데요",
          "original_form": "오 버즈 좋은데요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047575.1.38",
          "form": "푸짐하네요",
          "original_form": "푸짐하네요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047575.1.39",
          "form": "전 버즈 사고싶어요",
          "original_form": "전 버즈 사고싶어요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047575.1.40",
          "form": "선물이 그정도는 되야죠 ㅎㅎ",
          "original_form": "선물이 그정도는 되야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047575.1.41",
          "form": "얼른왓으면 좋겟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "얼른왓으면 좋겟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047575.1.42",
          "form": "그럼요 빨리 와야죠 넘기다려 지죠 ㅎㅎ",
          "original_form": "그럼요 빨리 와야죠 넘기다려 지죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047575.1.43",
          "form": "막차타는바람에 배송 한참 늦을것같아요 ㅋㅋㅋㅋ",
          "original_form": "막차타는바람에 배송 한참 늦을것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047575.1.44",
          "form": "고기는 참나 언제와도 되지만 폰은 빨리와야죠",
          "original_form": "고기는 참나 언제와도 되지만 폰은 빨리와야죠",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047575.1.45",
          "form": "에이 어째요",
          "original_form": "에이 어째요",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047575.1.46",
          "form": "9월에 오나요?",
          "original_form": "9월에 오나요?",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047575.1.47",
          "form": "네 9월초에온대요",
          "original_form": "네 9월초에온대요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047575.1.48",
          "form": "예정으로는 ㅋㅋㅋㅋ",
          "original_form": "예정으로는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047575.1.49",
          "form": "고기도 중요하죠!!",
          "original_form": "고기도 중요하죠!!",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047575.1.50",
          "form": "식품인데 ㅋㅋㅋㅋㅋ",
          "original_form": "식품인데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047575.1.51",
          "form": "아 그럼 5일전 오면 좋겠어요",
          "original_form": "아 그럼 5일전 오면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047575.1.52",
          "form": "ㅋㅋ잡아서 오니",
          "original_form": "ㅋㅋ잡아서 오니",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047575.1.53",
          "form": "신선하게 오지 않을까요",
          "original_form": "신선하게 오지 않을까요",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047575.1.54",
          "form": "농담같죠 잡아 온다니",
          "original_form": "농담같죠 잡아 온다니",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100047575.1.55",
          "form": "고기를 안사고 기다리니",
          "original_form": "고기를 안사고 기다리니",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047575.1.56",
          "form": "집에 반찬 할게 없어요ㅜㅜ",
          "original_form": "집에 반찬 할게 없어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047575.1.57",
          "form": "낼은 그냥가서 좀 사와야 겠어요",
          "original_form": "낼은 그냥가서 좀 사와야 겠어요",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047575.1.58",
          "form": "오면 그것도 또 먹겠죠 뭐 ㅎㅎ",
          "original_form": "오면 그것도 또 먹겠죠 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        }
      ]
