      "utterance": [
        {
          "id": "MDRW2100004015.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004015.1.2",
          "form": "안녕하세요 ? 거기도 아침에 비왔었나요 ?",
          "original_form": "안녕하세요 ? 거기도 아침에 비왔었나요 ?",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004015.1.3",
          "form": "여기는 지금도 비가많이와요~",
          "original_form": "여기는 지금도 비가많이와요~",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004015.1.4",
          "form": "여긴 비가 많이 오드니 지금은 해가 떳어요",
          "original_form": "여긴 비가 많이 오드니 지금은 해가 떳어요",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004015.1.5",
          "form": "햇살이 엄청 쨍쨍해요",
          "original_form": "햇살이 엄청 쨍쨍해요",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004015.1.6",
          "form": "헉 정말요?",
          "original_form": "헉 정말요?",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004015.1.7",
          "form": "어디사세요?",
          "original_form": "어디사세요?",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004015.1.8",
          "form": "네 여기는 경기도 구리 ?",
          "original_form": "네 여기는 경기도 구리 ?",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004015.1.9",
          "form": "아 위쪽이시구나~ 여기는 경남인데",
          "original_form": "아 위쪽이시구나~ 여기는 경남인데",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004015.1.10",
          "form": "하루종일 비가 올거같네요..",
          "original_form": "하루종일 비가 올거같네요..",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004015.1.11",
          "form": "산책나가고 싶었는데~",
          "original_form": "산책나가고 싶었는데~",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004015.1.12",
          "form": "비오는날 산책도 운치 있지 않나요 ?",
          "original_form": "비오는날 산책도 운치 있지 않나요 ?",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004015.1.13",
          "form": "그건 그래요 ㅎㅎ",
          "original_form": "그건 그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004015.1.14",
          "form": "근데 보슬비가 아니라.. 장대비가 내리고있어서",
          "original_form": "근데 보슬비가 아니라.. 장대비가 내리고있어서",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004015.1.15",
          "form": "그냥 집에 있으려구요",
          "original_form": "그냥 집에 있으려구요",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004015.1.16",
          "form": "아 장대비라면... 나가지 않는게 좋겠네요",
          "original_form": "아 장대비라면... 나가지 않는게 좋겠네요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004015.1.17",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004015.1.18",
          "form": "내일도 비가 온다고 하든데",
          "original_form": "내일도 비가 온다고 하든데",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004015.1.19",
          "form": "비가 한바탕 내리고나면 가을이 오려나 싶네요!!",
          "original_form": "비가 한바탕 내리고나면 가을이 오려나 싶네요!!",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004015.1.20",
          "form": " P2님은 어느 계절을 좋아하세요?",
          "original_form": " P2님은 어느 계절을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004015.1.21",
          "form": "저는 겨울을 좋아해요 .",
          "original_form": "저는 겨울을 좋아해요 .",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004015.1.22",
          "form": "어느계절을 좋아하세요 ?",
          "original_form": "어느계절을 좋아하세요 ?",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004015.1.23",
          "form": "앗~ 저는 여름을 좋아해요 ㅋㅋ",
          "original_form": "앗~ 저는 여름을 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004015.1.24",
          "form": "추위를 많이 타고 더위를 안타서 ㅎㅎ",
          "original_form": "추위를 많이 타고 더위를 안타서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004015.1.25",
          "form": "겨울 좋아하시는 이유가 뭔가요?",
          "original_form": "겨울 좋아하시는 이유가 뭔가요?",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004015.1.26",
          "form": "여름은 더우면 옷에 한계가있잖아요",
          "original_form": "여름은 더우면 옷에 한계가있잖아요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004015.1.27",
          "form": "겨울은 껴입든가 핫팩등 다양한 대처 용품들이 있어서 겨울이 더 좋아요",
          "original_form": "겨울은 껴입든가 핫팩등 다양한 대처 용품들이 있어서 겨울이 더 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004015.1.28",
          "form": "아~ 맞아요",
          "original_form": "아~ 맞아요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        }
      ]
    }
