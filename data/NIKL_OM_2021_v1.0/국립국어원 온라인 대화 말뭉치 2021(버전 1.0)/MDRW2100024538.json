      "utterance": [
        {
          "id": "MDRW2100024538.1.1",
          "form": "잘했네 책읽아웃 재밌지",
          "original_form": "잘했네 책읽아웃 재밌지",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024538.1.2",
          "form": "진행자 둘 다 책 좋아하는 사람들이고 패널이나 게스트도 좋아 ㅋㅋ",
          "original_form": "진행자 둘 다 책 좋아하는 사람들이고 패널이나 게스트도 좋아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100024538.1.3",
          "form": "아 언니는 이미 듣고있었구나",
          "original_form": "아 언니는 이미 듣고있었구나",
          "speaker_id": "1",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100024538.1.4",
          "form": "절멸이란 책 읽고 애들이랑 활동 계획하기도 괜찮을 것 같아서",
          "original_form": "절멸이란 책 읽고 애들이랑 활동 계획하기도 괜찮을 것 같아서",
          "speaker_id": "1",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100024538.1.5",
          "form": "한번 읽어보려고",
          "original_form": "한번 읽어보려고",
          "speaker_id": "1",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100024538.1.6",
          "form": "난 듣다말다 듣다말다 해",
          "original_form": "난 듣다말다 듣다말다 해",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024538.1.7",
          "form": "수업준비에 도움되면 좋겠구먼",
          "original_form": "수업준비에 도움되면 좋겠구먼",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024538.1.8",
          "form": "나도 아직 하나 들었어ㅋㅋ",
          "original_form": "나도 아직 하나 들었어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100024538.1.9",
          "form": "책소개하는 유튜브나 팟캐스트 들으면 책 다 사고싶어져 ㅋㅋ",
          "original_form": "책소개하는 유튜브나 팟캐스트 들으면 책 다 사고싶어져 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100024538.1.10",
          "form": "응 하지만 일단 도서관에 신청해서 대출하는 걸로~",
          "original_form": "응 하지만 일단 도서관에 신청해서 대출하는 걸로~",
          "speaker_id": "1",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100024538.1.11",
          "form": "그치 다 읽는 것도 해당이야~",
          "original_form": "그치 다 읽는 것도 해당이야~",
          "speaker_id": "2",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100024538.1.12",
          "form": "오늘도 상호대차 신청한 책 하나 칙칙폭폭에 왔대서 받아가려고",
          "original_form": "오늘도 상호대차 신청한 책 하나 칙칙폭폭에 왔대서 받아가려고",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100024538.1.13",
          "form": "아하 그래서 겸사겸사 역으로 간다 한거군",
          "original_form": "아하 그래서 겸사겸사 역으로 간다 한거군",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024538.1.14",
          "form": "잘 대출해 오세요",
          "original_form": "잘 대출해 오세요",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100024538.1.15",
          "form": "응 도서관도 들를 겸 겸사겸사",
          "original_form": "응 도서관도 들를 겸 겸사겸사",
          "speaker_id": "1",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100024538.1.16",
          "form": "칙칙폭폭 도서관들 위치가 너무 좋지",
          "original_form": "칙칙폭폭 도서관들 위치가 너무 좋지",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024538.1.17",
          "form": "다른 지역에도 확대됐으면",
          "original_form": "다른 지역에도 확대됐으면",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024538.1.18",
          "form": "응 부천시가 도서관 정책이 진짜 잘돼이서",
          "original_form": "응 부천시가 도서관 정책이 진짜 잘돼이서",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024538.1.19",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024538.1.20",
          "form": "그르게말여",
          "original_form": "그르게말여",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024538.1.21",
          "form": "지하철역 작은도서관에 상호대차 2권 도착했대",
          "original_form": "지하철역 작은도서관에 상호대차 2권 도착했대",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100024538.1.22",
          "form": "내일 반납할 책도 2권인디",
          "original_form": "내일 반납할 책도 2권인디",
          "speaker_id": "1",
