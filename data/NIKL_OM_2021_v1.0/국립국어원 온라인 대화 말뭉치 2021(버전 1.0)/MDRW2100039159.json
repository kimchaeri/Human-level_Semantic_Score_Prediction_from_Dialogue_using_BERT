        {
          "id": "MDRW2100039159.1.2",
          "form": "반품ㅋ",
          "original_form": "반품ㅋ",
          "speaker_id": "1",
          "time": "20210903 11:56"
        },
        {
          "id": "MDRW2100039159.1.3",
          "form": "아직도 진행 중ㅠ",
          "original_form": "아직도 진행 중ㅠ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039159.1.4",
          "form": "다음 주까지 쭉~~ㅠㅠ",
          "original_form": "다음 주까지 쭉~~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100039159.1.5",
          "form": "아이구 장난아니구나 ㅠㅠ",
          "original_form": "아이구 장난아니구나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039159.1.6",
          "form": "다음주에 놀러가도 되나",
          "original_form": "다음주에 놀러가도 되나",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039159.1.7",
          "form": "그래 다음 주는 안 되겠다ㅋㅋㅋ",
          "original_form": "그래 다음 주는 안 되겠다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039159.1.8",
          "form": "봉리에서 모임 하시오ㅋㅋㅋ",
          "original_form": "봉리에서 모임 하시오ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:21"
        },
        {
          "id": "MDRW2100039159.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러니깐 바빠가 되겟나 싶더라잉",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러니깐 바빠가 되겟나 싶더라잉",
          "speaker_id": "1",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100039159.1.10",
          "form": "풍경채에서 보자캐야지 푸하하하",
          "original_form": "풍경채에서 보자캐야지 푸하하하",
          "speaker_id": "1",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100039159.1.11",
          "form": "커피 맛있게 하던데 ㅋㅋ 언니 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "커피 맛있게 하던데 ㅋㅋ 언니 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100039159.1.12",
          "form": "그래 풍경채 커피 맛있는데ㅋㅋㅋ",
          "original_form": "그래 풍경채 커피 맛있는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039159.1.13",
          "form": "커피 맛있게 타면 안돼ㅋㅋ",
          "original_form": "커피 맛있게 타면 안돼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039159.1.14",
          "form": "계속 생각나ㅋㅋ",
          "original_form": "계속 생각나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039159.1.15",
          "form": "ㅋㅋㅋ맞어 ㅋㅋㅋ 커피는 풍경채랑 우미린언니가 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞어 ㅋㅋㅋ 커피는 풍경채랑 우미린언니가 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039159.1.16",
          "form": "맛나게 타주는듯 ㅋㅋ ㅋ",
          "original_form": "맛나게 타주는듯 ㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039159.1.17",
          "form": "미린 언니 커피 못 먹은 지",
          "original_form": "미린 언니 커피 못 먹은 지",
          "speaker_id": "2",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039159.1.18",
          "form": "일 년이 돼 가는 거 같네ㅋㅋㅋ",
          "original_form": "일 년이 돼 가는 거 같네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100039159.1.19",
          "form": "그러니깐은 ㅋㅋ",
          "original_form": "그러니깐은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039159.1.20",
          "form": "만나는것도 힘들다 ㅋ",
          "original_form": "만나는것도 힘들다 ㅋ",
          "speaker_id": "1",
          "time": "20210903 14:33"
        },
        {
          "id": "MDRW2100039159.1.21",
