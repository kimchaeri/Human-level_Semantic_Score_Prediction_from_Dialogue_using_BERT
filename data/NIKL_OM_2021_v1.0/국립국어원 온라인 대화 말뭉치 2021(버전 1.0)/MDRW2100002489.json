      "utterance": [
        {
          "id": "MDRW2100002489.1.1",
          "form": "안뇽하세요",
          "original_form": "안뇽하세요",
          "speaker_id": "1",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002489.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002489.1.3",
          "form": "연애하고 계신가요??",
          "original_form": "연애하고 계신가요??",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002489.1.4",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002489.1.5",
          "form": "ㅎㅎ저두",
          "original_form": "ㅎㅎ저두",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.6",
          "form": "최근에 헤어져서",
          "original_form": "최근에 헤어져서",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.7",
          "form": "아...언제헤어지셨어요..?",
          "original_form": "아...언제헤어지셨어요..?",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.8",
          "form": "연애를 안하구있네요",
          "original_form": "연애를 안하구있네요",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.9",
          "form": "이런..",
          "original_form": "이런..",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.10",
          "form": "소개팅같은건 안하시나요",
          "original_form": "소개팅같은건 안하시나요",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.11",
          "form": "소개팅은 뭔가",
          "original_form": "소개팅은 뭔가",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.12",
          "form": "하라면 하는데",
          "original_form": "하라면 하는데",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.13",
          "form": "제대로 된 느낌이 아니라고 해야되나",
          "original_form": "제대로 된 느낌이 아니라고 해야되나",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.14",
          "form": "저는 좀",
          "original_form": "저는 좀",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.15",
          "form": "자연스러움을 추구하다보니",
          "original_form": "자연스러움을 추구하다보니",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.16",
          "form": "소개팅같은건 좀 꺼려지게 되네요 ㅋㅋ",
          "original_form": "소개팅같은건 좀 꺼려지게 되네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002489.1.17",
          "form": "자만추 이시구나",
          "original_form": "자만추 이시구나",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002489.1.18",
          "form": "네 그쳐 ㅎㅎ",
          "original_form": "네 그쳐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002489.1.19",
          "form": "지금 학생이신가요??",
          "original_form": "지금 학생이신가요??",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002489.1.20",
          "form": "대학교는 졸업했고",
          "original_form": "대학교는 졸업했고",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002489.1.21",
          "form": "이제 군대 해결 후에 취업해야죠",
          "original_form": "이제 군대 해결 후에 취업해야죠",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002489.1.22",
          "form": "아 아직 미필이시구나",
          "original_form": "아 아직 미필이시구나",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.23",
          "form": "공익이라 학교는 다 다니고 가고싶어서",
          "original_form": "공익이라 학교는 다 다니고 가고싶어서",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.24",
          "form": "학교 다 다닌 뒤에",
          "original_form": "학교 다 다닌 뒤에",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.25",
          "form": "공익 대기중이에요~",
          "original_form": "공익 대기중이에요~",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.26",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.27",
          "form": "그럼 지금은",
          "original_form": "그럼 지금은",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.28",
          "form": "알바같은거라도 하고 계신가요",
          "original_form": "알바같은거라도 하고 계신가요",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002489.1.29",
          "form": "지금은 알바도 하고",
          "original_form": "지금은 알바도 하고",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.30",
          "form": "이거저거 하면서 지내고 있습니다",
          "original_form": "이거저거 하면서 지내고 있습니다",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.31",
          "form": "솔직히 과가 의료계열이였던지라 놀지를 못해서",
          "original_form": "솔직히 과가 의료계열이였던지라 놀지를 못해서",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.32",
          "form": "알바보단 노는거 위주로 ㅎㅎㅋㅋㅋ",
          "original_form": "알바보단 노는거 위주로 ㅎㅎㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.33",
          "form": "거의 하고있어요",
          "original_form": "거의 하고있어요",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.34",
          "form": "물론 코로나때문ㅇ0",
          "original_form": "물론 코로나때문ㅇ0",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.35",
          "form": "ㅋㅋㅋ이것저것 하고 계신다면",
          "original_form": "ㅋㅋㅋ이것저것 하고 계신다면",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.36",
          "form": "여행이라던지 그런건 못하고있고",
          "original_form": "여행이라던지 그런건 못하고있고",
          "speaker_id": "2",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.37",
          "form": "그래도 사람만날일은 많겠네용",
          "original_form": "그래도 사람만날일은 많겠네용",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002489.1.38",
          "form": "네 그쵸",
          "original_form": "네 그쵸",
          "speaker_id": "2",
          "time": "20210820 12:52"
        }
      ]
    }
