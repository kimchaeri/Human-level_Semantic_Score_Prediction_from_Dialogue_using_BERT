      "utterance": [
        {
          "id": "MDRW2100036069.1.1",
          "form": "name2님은 집안일중에 제일 하기 싫은게 뭔가요?",
          "original_form": "&name2&님은 집안일중에 제일 하기 싫은게 뭔가요?",
          "speaker_id": "1",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100036069.1.2",
          "form": "저 아무래도",
          "original_form": "저 아무래도",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100036069.1.3",
          "form": "음쓰 버리는거용ㅋㅋ",
          "original_form": "음쓰 버리는거용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100036069.1.4",
          "form": "아항.. 그건 제 담당이 아니라 .. 제 담당이면 싫을거 같네요..ㅎㅎ",
          "original_form": "아항.. 그건 제 담당이 아니라 .. 제 담당이면 싫을거 같네요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100036069.1.5",
          "form": "전 세탁하는 일이 좀 힘들어요...ㅎㅎ",
          "original_form": "전 세탁하는 일이 좀 힘들어요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100036069.1.6",
          "form": "ㅋㅋㅋ 세탁하는거용?",
          "original_form": "ㅋㅋㅋ 세탁하는거용?",
          "speaker_id": "2",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100036069.1.7",
          "form": "망에 넣을거 분리하고",
          "original_form": "망에 넣을거 분리하고",
          "speaker_id": "2",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100036069.1.8",
          "form": "색깔 분리하고 널고 개고",
          "original_form": "색깔 분리하고 널고 개고",
          "speaker_id": "2",
          "time": "20210830 15:40"
        },
        {
          "id": "MDRW2100036069.1.9",
          "form": "이런거용?",
          "original_form": "이런거용?",
          "speaker_id": "2",
          "time": "20210830 15:40"
        },
        {
          "id": "MDRW2100036069.1.10",
          "form": "그쵸.. 머리 써야 해요..",
          "original_form": "그쵸.. 머리 써야 해요..",
          "speaker_id": "1",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100036069.1.11",
          "form": "이거 티비보면서 생각없이하면 금방해요!ㅎ",
          "original_form": "이거 티비보면서 생각없이하면 금방해요!ㅎ",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100036069.1.12",
          "form": "섬유별로 색깔별로.. 따로 세탁하고 세제도 다르게 써야하고",
          "original_form": "섬유별로 색깔별로.. 따로 세탁하고 세제도 다르게 써야하고",
          "speaker_id": "1",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100036069.1.13",
          "form": "널고 개고",
          "original_form": "널고 개고",
          "speaker_id": "1",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100036069.1.14",
          "form": "그 다음으로 싫은건 정리정돈이요",
          "original_form": "그 다음으로 싫은건 정리정돈이요",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.15",
          "form": "분류해서 뭘 버릴지 안버릴지 결정하는거요",
          "original_form": "분류해서 뭘 버릴지 안버릴지 결정하는거요",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.16",
          "form": "그냥 단순노동이 좋아요..ㅎㅎㅎ",
          "original_form": "그냥 단순노동이 좋아요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.17",
          "form": "아ㅋㅋㅋ 단순노동이라면",
          "original_form": "아ㅋㅋㅋ 단순노동이라면",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.18",
          "form": "청소기돌리고",
          "original_form": "청소기돌리고",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.19",
          "form": "설거지하고",
          "original_form": "설거지하고",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.20",
          "form": "저도 이런쪽을 더 좋아하긴해요ㅋㅋㅋ",
          "original_form": "저도 이런쪽을 더 좋아하긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:44"
        },
        {
          "id": "MDRW2100036069.1.21",
          "form": "네.. 그건 정말 암생각없이 할 수 있고.. 하고 나면 깨끗해져서 뿌듯함도 느껴요",
          "original_form": "네.. 그건 정말 암생각없이 할 수 있고.. 하고 나면 깨끗해져서 뿌듯함도 느껴요",
          "speaker_id": "1",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100036069.1.22",
          "form": "집안일하면 잡생각 안나니까",
          "original_form": "집안일하면 잡생각 안나니까",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036069.1.23",
          "form": "너무 머리가 복잡할때 하는데",
          "original_form": "너무 머리가 복잡할때 하는데",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036069.1.24",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036069.1.25",
          "form": "저는 머리복잡할 때 찌든 때를 벗겨요..",
          "original_form": "저는 머리복잡할 때 찌든 때를 벗겨요..",
          "speaker_id": "1",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100036069.1.26",
          "form": "언제 3시간동안 한적 있어요..ㅎㅎㅎ",
          "original_form": "언제 3시간동안 한적 있어요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100036069.1.27",
          "form": "어머나ㅋㅋㅋㅋ",
          "original_form": "어머나ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100036069.1.28",
          "form": "엄청 빤딱빤딱했겠는데요?ㅋㅋㅋ",
          "original_form": "엄청 빤딱빤딱했겠는데요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100036069.1.29",
          "form": "저도 얼마전에 가스레인지를 그렇게 닦았어요",
          "original_form": "저도 얼마전에 가스레인지를 그렇게 닦았어요",
          "speaker_id": "2",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100036069.1.30",
          "form": "정말 그런 단순노동이 머리를 식혀주더라구요..",
          "original_form": "정말 그런 단순노동이 머리를 식혀주더라구요..",
          "speaker_id": "1",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100036069.1.31",
          "form": "가스레인지 닦는 주기가 어떻게 되세요?",
          "original_form": "가스레인지 닦는 주기가 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210830 16:00"
        },
        {
          "id": "MDRW2100036069.1.32",
