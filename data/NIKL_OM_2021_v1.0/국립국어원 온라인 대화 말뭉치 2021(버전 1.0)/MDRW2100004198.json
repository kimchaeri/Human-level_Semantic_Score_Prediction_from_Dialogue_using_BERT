      "utterance": [
        {
          "id": "MDRW2100004198.1.1",
          "form": "우리 정말",
          "original_form": "우리 정말",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.2",
          "form": "학교에 대해서만 얘기해야하니",
          "original_form": "학교에 대해서만 얘기해야하니",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.3",
          "form": "넌 휴학에 대해 얘기해봐",
          "original_form": "넌 휴학에 대해 얘기해봐",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.4",
          "form": "그것도 학교야",
          "original_form": "그것도 학교야",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.5",
          "form": "나는 휴학을 했어",
          "original_form": "나는 휴학을 했어",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.6",
          "form": "ㅋㅋㅋㅋ왜 그렇게말해",
          "original_form": "ㅋㅋㅋㅋ왜 그렇게말해",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.7",
          "form": "지금 학교를 가지않고있다고 할수있지",
          "original_form": "지금 학교를 가지않고있다고 할수있지",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.8",
          "form": "수능 영어지문처럼",
          "original_form": "수능 영어지문처럼",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004198.1.9",
          "form": "말하네",
          "original_form": "말하네",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004198.1.10",
          "form": "휴학을 했기 때문이야",
          "original_form": "휴학을 했기 때문이야",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004198.1.11",
          "form": "짜증난다고 할 수 있지",
          "original_form": "짜증난다고 할 수 있지",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004198.1.12",
          "form": "휴학무새..",
          "original_form": "휴학무새..",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004198.1.13",
          "form": "나는 휴학무새!",
          "original_form": "나는 휴학무새!",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.14",
          "form": "휴후학",
          "original_form": "휴후학",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.15",
          "form": "너 사람 아니지",
          "original_form": "너 사람 아니지",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.16",
          "form": "심심이지",
          "original_form": "심심이지",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.17",
          "form": "휴-학",
          "original_form": "휴-학",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.18",
          "form": "저는 그런사람이 아니에요.",
          "original_form": "저는 그런사람이 아니에요.",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.19",
          "form": "그런..사람?",
          "original_form": "그런..사람?",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004198.1.20",
          "form": "사람 아니지 너",
          "original_form": "사람 아니지 너",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004198.1.21",
          "form": "저는 학생입니다",
          "original_form": "저는 학생입니다",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004198.1.22",
          "form": "심심이 몰라요 그런거",
          "original_form": "심심이 몰라요 그런거",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004198.1.23",
          "form": "그럼 동아리 했어?",
          "original_form": "그럼 동아리 했어?",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004198.1.24",
          "form": "학교다닐때",
          "original_form": "학교다닐때",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.25",
          "form": "아니 동아리 하나도 안했어",
          "original_form": "아니 동아리 하나도 안했어",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.26",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.27",
          "form": "미술동아리 들어갈 생각",
          "original_form": "미술동아리 들어갈 생각",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.28",
          "form": "없어??",
          "original_form": "없어??",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.29",
          "form": "귀찮아서",
          "original_form": "귀찮아서",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.30",
          "form": "없어!",
          "original_form": "없어!",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.31",
          "form": "지금도 생각없어?ㅇ",
          "original_form": "지금도 생각없어?ㅇ",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.32",
          "form": "생각없어!",
          "original_form": "생각없어!",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004198.1.33",
          "form": "크로키도 하고",
          "original_form": "크로키도 하고",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004198.1.34",
          "form": "재밌을텐데",
          "original_form": "재밌을텐데",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004198.1.35",
          "form": "참석 자율적이어도",
          "original_form": "참석 자율적이어도",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004198.1.36",
          "form": "귀찮아??",
          "original_form": "귀찮아??",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004198.1.37",
          "form": "참석 하는것 자체가 싫어!",
          "original_form": "참석 하는것 자체가 싫어!",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004198.1.38",
          "form": "그렇구나 아쉽다",
          "original_form": "그렇구나 아쉽다",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004198.1.39",
          "form": "난 동아리 좋아하거든",
          "original_form": "난 동아리 좋아하거든",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004198.1.40",
          "form": "사람들이랑 만나는거 싫어!",
          "original_form": "사람들이랑 만나는거 싫어!",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004198.1.41",
          "form": "언니는 동아리 좋아해?",
          "original_form": "언니는 동아리 좋아해?",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004198.1.42",
          "form": "왜 좋아해??",
          "original_form": "왜 좋아해??",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004198.1.43",
          "form": "나는 사람 만나는것도 좋고",
          "original_form": "나는 사람 만나는것도 좋고",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004198.1.44",
          "form": "이것저것 해보는거좋아해~~",
          "original_form": "이것저것 해보는거좋아해~~",
          "speaker_id": "1",
          "time": "20210821 15:40"
        }
      ]
    }
