      "utterance": [
        {
          "id": "MDRW2100035421.1.1",
          "form": "크로플 첨먹었는데맛있군용",
          "original_form": "크로플 첨먹었는데맛있군용",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100035421.1.2",
          "form": "와~ 나가서 먹으면 엄청 맛있을것 같아요",
          "original_form": "와~ 나가서 먹으면 엄청 맛있을것 같아요",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100035421.1.3",
          "form": "그 어려운이름의 스벅메뉴가 무슨맛이던가요",
          "original_form": "그 어려운이름의 스벅메뉴가 무슨맛이던가요",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100035421.1.4",
          "form": "그게 흑임자에요",
          "original_form": "그게 흑임자에요",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100035421.1.5",
          "form": "이름도 까묵었어",
          "original_form": "이름도 까묵었어",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.6",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.7",
          "form": "루프탑 그레이 라떼",
          "original_form": "루프탑 그레이 라떼",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.8",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.9",
          "form": "네 ㅋㅋ 흑임자 폼 들어간.ㅋㅋ 저도 두번만 먹었던 ㅠㅠ",
          "original_form": "네 ㅋㅋ 흑임자 폼 들어간.ㅋㅋ 저도 두번만 먹었던 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.10",
          "form": "흑임자라떼라고 안하고 ㅋㅋㅋㅋ",
          "original_form": "흑임자라떼라고 안하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.11",
          "form": "고급지게 지었져??",
          "original_form": "고급지게 지었져??",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.12",
          "form": "어이가없넠ㅋㅋㅋ왜 어려운말을",
          "original_form": "어이가없넠ㅋㅋㅋ왜 어려운말을",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.14",
          "form": "루프탑...ㅋㅋㅋㅋ 그레이...ㅋㅋㅋㅋ",
          "original_form": "루프탑...ㅋㅋㅋㅋ 그레이...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.15",
          "form": "뭐 틀린말은 아니죠??",
          "original_form": "뭐 틀린말은 아니죠??",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100035421.1.16",
          "form": "루프탑 맞잖아여 위에 ㅋㅋㅋ",
          "original_form": "루프탑 맞잖아여 위에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100035421.1.17",
          "form": "제가먹은건 루프탑 그린 라떼네요",
          "original_form": "제가먹은건 루프탑 그린 라떼네요",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100035421.1.18",
          "form": "말차니까ㅋㅋ ㄲ",
          "original_form": "말차니까ㅋㅋ ㄲ",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100035421.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 세상에나 ㅋㅋㅋ",
