        {
          "id": "MDRW2100018750.1.1",
          "form": "감삼다",
          "original_form": "감삼다",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.2",
          "form": "금식인데 먹어부럿네요",
          "original_form": "금식인데 먹어부럿네요",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.3",
          "form": "밥을 먹었다..?",
          "original_form": "밥을 먹었다..?",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.4",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.5",
          "form": "= 건강검진 할 생각이 읍다",
          "original_form": "= 건강검진 할 생각이 읍다",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.6",
          "form": "혈당수치 폭.발",
          "original_form": "혈당수치 폭.발",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.7",
          "form": "=말만 건강맨이다",
          "original_form": "=말만 건강맨이다",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.8",
          "form": "시티찍으면",
          "original_form": "시티찍으면",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018750.1.9",
          "form": "아아아악",
          "original_form": "아아아악",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.10",
          "form": "내장에 고등어 보일듯",
          "original_form": "내장에 고등어 보일듯",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.11",
          "form": "오.. 이 환자 멋진 식사를 했나본데요?",
          "original_form": "오.. 이 환자 멋진 식사를 했나본데요?",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.12",
          "form": "잘게 씹었거든요?!!",
          "original_form": "잘게 씹었거든요?!!",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.14",
          "form": "제가 두루미냐고요!",
          "original_form": "제가 두루미냐고요!",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.16",
          "form": "아주 잘게잘게 꼭꼭씹었나보오",
          "original_form": "아주 잘게잘게 꼭꼭씹었나보오",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.17",
          "form": "금식이라니까 너무 맛있게 드셨군요!",
          "original_form": "금식이라니까 너무 맛있게 드셨군요!",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.18",
          "form": "긍까요",
          "original_form": "긍까요",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.19",
          "form": "근데 상관없어요",
          "original_form": "근데 상관없어요",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.20",
          "form": "맘먹었더니 맛있네요",
          "original_form": "맘먹었더니 맛있네요",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018750.1.21",
          "form": "ㄹㅇ요?!",
          "original_form": "ㄹㅇ요?!",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018750.1.22",
          "form": "고등학교때 밥먹고온애들 걍 검사하던디",
          "original_form": "고등학교때 밥먹고온애들 걍 검사하던디",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018750.1.23",
          "form": "오케이 나도 그냥 간다.",
          "original_form": "오케이 나도 그냥 간다.",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018750.1.24",
          "form": "그거 감안해서 수치 기록하고 하더라구요",
          "original_form": "그거 감안해서 수치 기록하고 하더라구요",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018750.1.25",
