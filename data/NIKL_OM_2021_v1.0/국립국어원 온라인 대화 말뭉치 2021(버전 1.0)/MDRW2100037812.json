        {
          "id": "MDRW2100037812.1.1",
          "form": "운동 좋아하세요??",
          "original_form": "운동 좋아하세요??",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037812.1.2",
          "form": "숨쉬기운동해요",
          "original_form": "숨쉬기운동해요",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100037812.1.3",
          "form": "바빠욧",
          "original_form": "바빠욧",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100037812.1.4",
          "form": "ㅋㅋㅋ 아 너무 귀엽게ㅋㅋ 바빠욧ㅋㅋ 공감합니다ㅋㅋ",
          "original_form": "ㅋㅋㅋ 아 너무 귀엽게ㅋㅋ 바빠욧ㅋㅋ 공감합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037812.1.5",
          "form": "다이어트는??? ㅋㅋ",
          "original_form": "다이어트는??? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037812.1.6",
          "form": "다이어트 해야합니다ㅠㅠ",
          "original_form": "다이어트 해야합니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100037812.1.7",
          "form": "숙제못풀고잇어요",
          "original_form": "숙제못풀고잇어요",
          "speaker_id": "1",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100037812.1.8",
          "form": "평생숙제",
          "original_form": "평생숙제",
          "speaker_id": "1",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100037812.1.9",
          "form": "ㅋㅋ아니 너무 잔인해요 기간이 너무 길어요!!!",
          "original_form": "ㅋㅋ아니 너무 잔인해요 기간이 너무 길어요!!!",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100037812.1.10",
          "form": "적당히 살아도 건강하고 탄탄하면 좋을텐데요ㅜㅜ!",
          "original_form": "적당히 살아도 건강하고 탄탄하면 좋을텐데요ㅜㅜ!",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037812.1.11",
          "form": "글쳐ㅜㅜ",
          "original_form": "글쳐ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037812.1.12",
          "form": "영양제는 챙겨드시나용",
          "original_form": "영양제는 챙겨드시나용",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037812.1.13",
          "form": "넵ㅋㅋ 오늘도 영양제 배송됐어요ㅋㅋ",
          "original_form": "넵ㅋㅋ 오늘도 영양제 배송됐어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100037812.1.14",
          "form": "뭐 드세요?",
          "original_form": "뭐 드세요?",
          "speaker_id": "2",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100037812.1.15",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037812.1.16",
          "form": "저는 꾸준히챙겨먹는게 안되더라구용",
          "original_form": "저는 꾸준히챙겨먹는게 안되더라구용",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037812.1.17",
          "form": "유산균 먹다말다해용",
          "original_form": "유산균 먹다말다해용",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037812.1.18",
          "form": "전 이번에 두드러기로 고생해서 비타민디랑 유산균 먹어요ㅋㅋ",
          "original_form": "전 이번에 두드러기로 고생해서 비타민디랑 유산균 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037812.1.19",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037812.1.20",
          "form": "어디꺼드시나욤",
          "original_form": "어디꺼드시나욤",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037812.1.21",
          "form": "그냥 종근당꺼요ㅋㅋ",
          "original_form": "그냥 종근당꺼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037812.1.22",
          "form": "비타민은 종근당 유산균은 그때그때 할인하는거 먹어요",
          "original_form": "비타민은 종근당 유산균은 그때그때 할인하는거 먹어요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037812.1.23",
          "form": "아 비타민디도 여자들 먹으면 좋다고 들은거같아요",
          "original_form": "아 비타민디도 여자들 먹으면 좋다고 들은거같아요",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100037812.1.24",
          "form": "담에 쿠팡에서 시켜봐야겟어욤",
          "original_form": "담에 쿠팡에서 시켜봐야겟어욤",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100037812.1.25",
          "form": "네 저 생리전증후군 우울증으로 오는데 마그네슘섞인 비타민 먹음 좀 괜찮더라고요",
          "original_form": "네 저 생리전증후군 우울증으로 오는데 마그네슘섞인 비타민 먹음 좀 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100037812.1.26",
          "form": "오오 꿀팁 감사해용 !!!!!",
          "original_form": "오오 꿀팁 감사해용 !!!!!",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100037812.1.27",
          "form": "넵 건강 지키며 살아야죠ㅜㅋㅋ",
          "original_form": "넵 건강 지키며 살아야죠ㅜㅋㅋ",
          "speaker_id": "2",
