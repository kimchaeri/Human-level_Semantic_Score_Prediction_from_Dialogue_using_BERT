      "utterance": [
        {
          "id": "MDRW2100006108.1.1",
          "form": "최근에 읽으신 책이 있으신가요?",
          "original_form": "최근에 읽으신 책이 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006108.1.2",
          "form": "저 이번에 시집 사서",
          "original_form": "저 이번에 시집 사서",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006108.1.3",
          "form": "읽으려고 계획만 하고이ㅛ어요!!",
          "original_form": "읽으려고 계획만 하고이ㅛ어요!!",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006108.1.4",
          "form": "오오 시집!! 우아합니다 ㅎㅎ",
          "original_form": "오오 시집!! 우아합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006108.1.5",
          "form": "어떤 시집인가요?",
          "original_form": "어떤 시집인가요?",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006108.1.6",
          "form": "당신의 이름을  지어다가 며칠은 먹었다",
          "original_form": "당신의 이름을  지어다가 며칠은 먹었다",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006108.1.7",
          "form": "라는 제목의 시집이에요!",
          "original_form": "라는 제목의 시집이에요!",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006108.1.8",
          "form": "제목이 오묘하네요 ㅎㅎㅎ",
          "original_form": "제목이 오묘하네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006108.1.9",
          "form": "제목이 맘에 들어서 사게 됐어요!",
          "original_form": "제목이 맘에 들어서 사게 됐어요!",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006108.1.10",
          "form": "평소에 책 많이 읽으세요?",
          "original_form": "평소에 책 많이 읽으세요?",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006108.1.11",
          "form": "음 많이 읽으려고 노력하는데",
          "original_form": "음 많이 읽으려고 노력하는데",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006108.1.12",
          "form": "책도 아에 보기싫을땐 아예안보고",
          "original_form": "책도 아에 보기싫을땐 아예안보고",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006108.1.13",
          "form": "보고싶을때 왕창봐요",
          "original_form": "보고싶을때 왕창봐요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006108.1.14",
          "form": "저도 책 거의 안보는 스타일이에요",
          "original_form": "저도 책 거의 안보는 스타일이에요",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006108.1.15",
          "form": "ㅎㅎ 그래도 시집은 읽으시잖아요 ㅎㅎ",
          "original_form": "ㅎㅎ 그래도 시집은 읽으시잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006108.1.16",
          "form": "웹소설 이런것두 안읽으시나요?",
          "original_form": "웹소설 이런것두 안읽으시나요?",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006108.1.17",
          "form": "웹소설은 완전 좋아하죠..ㅎㅎ..",
          "original_form": "웹소설은 완전 좋아하죠..ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006108.1.18",
          "form": "웹소설 좋아하세요?",
          "original_form": "웹소설 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006108.1.19",
          "form": "네 ㅎㅎ 좋아하죠!",
          "original_form": "네 ㅎㅎ 좋아하죠!",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006108.1.20",
          "form": "재미있잖아요 ㅎㅎ",
          "original_form": "재미있잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006108.1.21",
          "form": "맞아요! 최애 웹소설 어던거세요?",
          "original_form": "맞아요! 최애 웹소설 어던거세요?",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006108.1.22",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100006108.1.23",
          "form": "이름찾고올께요",
          "original_form": "이름찾고올께요",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100006108.1.24",
          "form": "정말 많지만",
          "original_form": "정말 많지만",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006108.1.25",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006108.1.26",
          "form": "하나만 곱으면",
          "original_form": "하나만 곱으면",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006108.1.27",
          "form": "당신의 이해를 돕기 위하여",
          "original_form": "당신의 이해를 돕기 위하여",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006108.1.28",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006108.1.29",
          "form": "오 이거 알아요!!",
          "original_form": "오 이거 알아요!!",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006108.1.30",
          "form": "제 친구가 완전 미쳐서 봤었어요!!",
          "original_form": "제 친구가 완전 미쳐서 봤었어요!!",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006108.1.31",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.32",
          "form": "연재로",
          "original_form": "연재로",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.33",
          "form": "첫날부터 다질러서",
          "original_form": "첫날부터 다질러서",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.34",
          "form": "봤어요",
          "original_form": "봤어요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 이런..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 이런..",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.36",
          "form": "환장했습니다",
          "original_form": "환장했습니다",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.37",
          "form": "당이돕은",
          "original_form": "당이돕은",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006108.1.38",
          "form": "카카오페이진가요?",
          "original_form": "카카오페이진가요?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006108.1.39",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006108.1.40",
          "form": "독점입니다",
          "original_form": "독점입니다",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006108.1.41",
          "form": "네 독점요!",
          "original_form": "네 독점요!",
          "speaker_id": "1",
