      "utterance": [
        {
          "id": "MDRW2100028249.1.1",
          "form": "망했지 뭐...",
          "original_form": "망했지 뭐...",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028249.1.2",
          "form": "계속 샵백에서 찾아봤는데 뭔가 마음에 드는게 없었어",
          "original_form": "계속 샵백에서 찾아봤는데 뭔가 마음에 드는게 없었어",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028249.1.3",
          "form": "그 당시 40퍼 할인. 입금 받으려면 최소 1만원 필요.",
          "original_form": "그 당시 40퍼 할인. 입금 받으려면 최소 1만원 필요.",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028249.1.4",
          "form": "충족되는 가격은 최소 5만원에서 6만원짜리",
          "original_form": "충족되는 가격은 최소 5만원에서 6만원짜리",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028249.1.5",
          "form": "난 돈이 없다 설치지 말자.",
          "original_form": "난 돈이 없다 설치지 말자.",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028249.1.6",
          "form": "끄덕 엔딩",
          "original_form": "끄덕 엔딩",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028249.1.7",
          "form": "슬퍼지네...",
          "original_form": "슬퍼지네...",
          "speaker_id": "1",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028249.1.8",
          "form": "당신 굉장히 철저한 기준이 있구나",
          "original_form": "당신 굉장히 철저한 기준이 있구나",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028249.1.9",
          "form": "굉장히 가격적으로 괜찮으면서 외관도 내취향인것을 고르다가",
          "original_form": "굉장히 가격적으로 괜찮으면서 외관도 내취향인것을 고르다가",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028249.1.10",
          "form": "이랗게 살아야하나 국내쇼핑몰도 많은데",
          "original_form": "이랗게 살아야하나 국내쇼핑몰도 많은데",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028249.1.11",
          "form": "그러고 쓰러지고 맘",
          "original_form": "그러고 쓰러지고 맘",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028249.1.12",
          "form": "나도 그래서 원래 사려던 건...",
          "original_form": "나도 그래서 원래 사려던 건...",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028249.1.13",
          "form": "결국 잊어버리고",
          "original_form": "결국 잊어버리고",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028249.1.14",
          "form": "문구류만 잔뜩 샀어",
          "original_form": "문구류만 잔뜩 샀어",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028249.1.15",
          "form": "아 나 새 옷 산 것도 왔어",
          "original_form": "아 나 새 옷 산 것도 왔어",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028249.1.16",
          "form": "헐 어떤옷이야",
          "original_form": "헐 어떤옷이야",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028249.1.17",
          "form": "말했던 버건디~붉은 계통은 낙낙한 기모맨투맨이랑",
          "original_form": "말했던 버건디~붉은 계통은 낙낙한 기모맨투맨이랑",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028249.1.18",
          "form": "반바지",
          "original_form": "반바지",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028249.1.19",
          "form": "바지 생각보다 바스락재질이라서 좀비사태 일어나면 도망 못 치겠더라",
          "original_form": "바지 생각보다 바스락재질이라서 좀비사태 일어나면 도망 못 치겠더라",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028249.1.20",
          "form": "아 그거 왔구나",
          "original_form": "아 그거 왔구나",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028249.1.21",
          "form": "아 어이없네 설명ㅋㅋㅋ",
          "original_form": "아 어이없네 설명ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028249.1.22",
          "form": "아 내일이면 나도 아빠가 컴퓨터 산거 배달하러 와주겠다",
          "original_form": "아 내일이면 나도 아빠가 컴퓨터 산거 배달하러 와주겠다",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028249.1.23",
          "form": "근데 이러고 다운 재부팅 안나아지면 어케 되는거임?",
          "original_form": "근데 이러고 다운 재부팅 안나아지면 어케 되는거임?",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028249.1.24",
          "form": "새컴 왜산겨 되는거 아님?",
          "original_form": "새컴 왜산겨 되는거 아님?",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028249.1.25",
          "form": "(...)",
          "original_form": "(...)",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028249.1.26",
          "form": "...그건 그렇지",
          "original_form": "...그건 그렇지",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028249.1.27",
          "form": "근데 오늘 저 조합 입어봤거든",
          "original_form": "근데 오늘 저 조합 입어봤거든",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028249.1.28",
          "form": "역시 귀여움",
          "original_form": "역시 귀여움",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028249.1.29",
          "form": "안에 입을 큰 흰셔츠도 사야겠어",
          "original_form": "안에 입을 큰 흰셔츠도 사야겠어",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028249.1.30",
          "form": "이것봐 뭐 하나 사면",
          "original_form": "이것봐 뭐 하나 사면",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028249.1.31",
          "form": "계속 산다니깐",
          "original_form": "계속 산다니깐",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028249.1.32",
          "form": "같이 입을 이것도...저것도...",
          "original_form": "같이 입을 이것도...저것도...",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028249.1.33",
          "form": "근데 입어보니까 진짜 흰셔츠 입으면 너무 귀여울 것 같아",
          "original_form": "근데 입어보니까 진짜 흰셔츠 입으면 너무 귀여울 것 같아",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028249.1.34",
          "form": "그럼 하나 장만하자",
          "original_form": "그럼 하나 장만하자",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028249.1.35",
          "form": "같이 부평 지하상가도 돌고싶다",
          "original_form": "같이 부평 지하상가도 돌고싶다",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028249.1.36",
          "form": "잉",
          "original_form": "잉",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028249.1.37",
          "form": "나도ㅠㅠ",
          "original_form": "나도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100028249.1.38",
          "form": "인천에 있을때 백번해야하는데",
          "original_form": "인천에 있을때 백번해야하는데",
          "speaker_id": "1",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100028249.1.39",
          "form": "왜 안하고 놀았찌... 아쉽다",
          "original_form": "왜 안하고 놀았찌... 아쉽다",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028249.1.40",
          "form": "얼른 인천올라와잉",
          "original_form": "얼른 인천올라와잉",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028249.1.41",
          "form": "가면 지하상가 털어야지",
          "original_form": "가면 지하상가 털어야지",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028249.1.42",
          "form": "돈 많이 벌어야겠어",
          "original_form": "돈 많이 벌어야겠어",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028249.1.43",
          "form": "지하상가 돌때 나는 스케이트 타야겠어 걸어다니면 발아파",
          "original_form": "지하상가 돌때 나는 스케이트 타야겠어 걸어다니면 발아파",
          "speaker_id": "2",
          "time": "20210902 19:48"
        }
      ]
    }
  ]
