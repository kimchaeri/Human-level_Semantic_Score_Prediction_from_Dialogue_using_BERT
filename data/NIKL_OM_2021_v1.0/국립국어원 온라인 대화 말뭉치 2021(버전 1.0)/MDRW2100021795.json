      "utterance": [
        {
          "id": "MDRW2100021795.1.1",
          "form": "쇼핑 좋아하셔요?",
          "original_form": "쇼핑 좋아하셔요?",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021795.1.2",
          "form": "쇼핑 좋아해요 ㅎㅎ",
          "original_form": "쇼핑 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021795.1.3",
          "form": "저 할인중독이거든요",
          "original_form": "저 할인중독이거든요",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021795.1.4",
          "form": "저두요ㅋㅋㅋ 할인만 하면 사는거같아요",
          "original_form": "저두요ㅋㅋㅋ 할인만 하면 사는거같아요",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021795.1.5",
          "form": "보통 어떤거 구매하세요?",
          "original_form": "보통 어떤거 구매하세요?",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021795.1.6",
          "form": "저 할인률 높은거 사여..",
          "original_form": "저 할인률 높은거 사여..",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021795.1.7",
          "form": "옷도 할인하는걸로 사세요??",
          "original_form": "옷도 할인하는걸로 사세요??",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021795.1.8",
          "form": "그럼요 ㅎㅎ",
          "original_form": "그럼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021795.1.9",
          "form": "쇼핑몰에서 하프삭스도 샀어요!",
          "original_form": "쇼핑몰에서 하프삭스도 샀어요!",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021795.1.10",
          "form": "오 ! 어디브랜드요?",
          "original_form": "오 ! 어디브랜드요?",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021795.1.11",
          "form": "일공공칠이였나?",
          "original_form": "일공공칠이였나?",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021795.1.12",
          "form": "무신사에서 파는거에요",
          "original_form": "무신사에서 파는거에요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021795.1.13",
          "form": "아아 무신사 저도 애용해요 ㅋㅋ",
          "original_form": "아아 무신사 저도 애용해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021795.1.14",
          "form": "세일하는 제품만 ㅋㅋㅋ",
          "original_form": "세일하는 제품만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021795.1.15",
          "form": "ㅋㅋㅋ 세일이 짱이에요",
          "original_form": "ㅋㅋㅋ 세일이 짱이에요",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021795.1.16",
          "form": "그리고 무신사 정품만 팔아서 안심돼요",
          "original_form": "그리고 무신사 정품만 팔아서 안심돼요",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021795.1.17",
          "form": "맞아요 ! 근데 그 병행수입 제품은 조심하셔요",
          "original_form": "맞아요 ! 근데 그 병행수입 제품은 조심하셔요",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100021795.1.18",
          "form": "예전에 병행수입 해온 정품이랬는데 짭이여서",
          "original_form": "예전에 병행수입 해온 정품이랬는데 짭이여서",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100021795.1.19",
          "form": "전체 환불해준적 있대요 무신사에서 !",
          "original_form": "전체 환불해준적 있대요 무신사에서 !",
          "speaker_id": "2",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021795.1.20",
          "form": "어어 기억 났어요 ㅎㅎㄹ",
          "original_form": "어어 기억 났어요 ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021795.1.21",
          "form": "그래도 타 쇼핑몰에 비하면 정품 거의 100퍼인듯 해요",
          "original_form": "그래도 타 쇼핑몰에 비하면 정품 거의 100퍼인듯 해요",
          "speaker_id": "1",
          "time": "20210828 23:16"
        }
      ]
