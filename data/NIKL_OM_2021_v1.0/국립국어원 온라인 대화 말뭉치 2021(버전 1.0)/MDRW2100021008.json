      "utterance": [
        {
          "id": "MDRW2100021008.1.1",
          "form": "저는 필링기요",
          "original_form": "저는 필링기요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100021008.1.2",
          "form": "사신거 있으세요?",
          "original_form": "사신거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100021008.1.3",
          "form": "피부관리용인가요?",
          "original_form": "피부관리용인가요?",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100021008.1.4",
          "form": "저는 옷이요 ㅋㅋㅋ",
          "original_form": "저는 옷이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100021008.1.5",
          "form": "네 피부관리용이요",
          "original_form": "네 피부관리용이요",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100021008.1.6",
          "form": "옷 어떤거 사셨어요?",
          "original_form": "옷 어떤거 사셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100021008.1.7",
          "form": "오 효과있나요",
          "original_form": "오 효과있나요",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100021008.1.8",
          "form": "저는 트렌치코트랑 맨투맨 이요",
          "original_form": "저는 트렌치코트랑 맨투맨 이요",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100021008.1.9",
          "form": "기름기는 좀 덜해요 ㅎ",
          "original_form": "기름기는 좀 덜해요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100021008.1.10",
          "form": "오 가을대비 쇼핑하셨네요",
          "original_form": "오 가을대비 쇼핑하셨네요",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100021008.1.11",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100021008.1.12",
          "form": "가을은 근데 넘 짧아서 ㅠ 슬퍼요",
          "original_form": "가을은 근데 넘 짧아서 ㅠ 슬퍼요",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100021008.1.13",
          "form": "아 맞아요ㅜㅜㅜ",
          "original_form": "아 맞아요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100021008.1.14",
          "form": "그래도 봄에 같이입으니까ㅜ",
          "original_form": "그래도 봄에 같이입으니까ㅜ",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100021008.1.15",
          "form": "괜찮은거같아요",
          "original_form": "괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100021008.1.16",
          "form": "그건 그래요 ㅋㅋㅋ",
          "original_form": "그건 그래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100021008.1.17",
          "form": "요새 시즌오프로 다 싸더라구요",
          "original_form": "요새 시즌오프로 다 싸더라구요",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100021008.1.18",
          "form": "인터넷 쇼핑하신건가요?",
          "original_form": "인터넷 쇼핑하신건가요?",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100021008.1.19",
          "form": "네 저는 거의",
          "original_form": "네 저는 거의",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100021008.1.20",
          "form": "인터넷으로만 사는거같아요",
          "original_form": "인터넷으로만 사는거같아요",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100021008.1.21",
          "form": "인쇼가 안전하긴하죠 싸고",
          "original_form": "인쇼가 안전하긴하죠 싸고",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100021008.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100021008.1.23",
          "form": "비교하기도 더 쉽궁6",
