        },
        {
          "id": "MDRW2100016447.1.3",
          "form": "영화볼거?",
          "original_form": "영화볼거?",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016447.1.4",
          "form": "우리 게임만 할 거 아니였음?",
          "original_form": "우리 게임만 할 거 아니였음?",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016447.1.5",
          "form": "오 보자",
          "original_form": "오 보자",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016447.1.6",
          "form": "ㅋㅋㅋㅋㅋ맞다",
          "original_form": "ㅋㅋㅋㅋㅋ맞다",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016447.1.7",
          "form": "들다하지",
          "original_form": "들다하지",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016447.1.8",
          "form": "아 그럼 되네",
          "original_form": "아 그럼 되네",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016447.1.9",
          "form": "ㅇㅇㅇㅇ뭐볼까",
          "original_form": "ㅇㅇㅇㅇ뭐볼까",
          "speaker_id": "1",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016447.1.10",
          "form": "로맨스만아니면상관없는데",
          "original_form": "로맨스만아니면상관없는데",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016447.1.11",
          "form": "피도 안되잖아",
          "original_form": "피도 안되잖아",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016447.1.12",
          "form": "잔인한 것도 안되고",
          "original_form": "잔인한 것도 안되고",
