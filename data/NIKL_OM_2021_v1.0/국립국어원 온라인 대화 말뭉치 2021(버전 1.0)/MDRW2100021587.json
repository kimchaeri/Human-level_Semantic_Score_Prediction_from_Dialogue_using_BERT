      "utterance": [
        {
          "id": "MDRW2100021587.1.1",
          "form": "그 둘이 어울려..?",
          "original_form": "그 둘이 어울려..?",
          "speaker_id": "2",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100021587.1.2",
          "form": "처음 듣는 조합",
          "original_form": "처음 듣는 조합",
          "speaker_id": "2",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100021587.1.3",
          "form": "약간 매운탕같은 느낌인가",
          "original_form": "약간 매운탕같은 느낌인가",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021587.1.4",
          "form": "그냥 막 많이 먹었어",
          "original_form": "그냥 막 많이 먹었어",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021587.1.5",
          "form": "신나게",
          "original_form": "신나게",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021587.1.6",
          "form": "다이어트는 내일 하하",
          "original_form": "다이어트는 내일 하하",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021587.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021587.1.8",
          "form": "많이 먹고 다 근육 만드는 겨",
          "original_form": "많이 먹고 다 근육 만드는 겨",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.9",
          "form": "나는 카레먹었어",
          "original_form": "나는 카레먹었어",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.10",
          "form": "근데 오뚜기 카레가",
          "original_form": "근데 오뚜기 카레가",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.11",
          "form": "카레계에서는 하급이라던데",
          "original_form": "카레계에서는 하급이라던데",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.12",
          "form": "3분카러?",
          "original_form": "3분카러?",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.13",
          "form": "아니 분말카레",
          "original_form": "아니 분말카레",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.14",
          "form": "카레?",
          "original_form": "카레?",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021587.1.16",
          "form": "진짜????",
          "original_form": "진짜????",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.17",
          "form": "3분카레도 그렇고",
          "original_form": "3분카레도 그렇고",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.18",
          "form": "분말카레도 그렇고",
          "original_form": "분말카레도 그렇고",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.19",
          "form": "엥 난 오뚜기만 먹는데",
          "original_form": "엥 난 오뚜기만 먹는데",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.20",
          "form": "카레여왕이런거",
          "original_form": "카레여왕이런거",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.21",
          "form": "카레마니아들은 오뚜기를 증오하던데",
          "original_form": "카레마니아들은 오뚜기를 증오하던데",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.22",
          "form": "안먹걸랑",
          "original_form": "안먹걸랑",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.23",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.24",
          "form": "마니아는 아니구나",
          "original_form": "마니아는 아니구나",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.25",
          "form": "이것도 취향 엄청 타나봐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이것도 취향 엄청 타나봐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021587.1.26",
          "form": "카레여왕은 왜 안 먹니",
          "original_form": "카레여왕은 왜 안 먹니",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.27",
          "form": "한 번도 안 먹어봄 난",
          "original_form": "한 번도 안 먹어봄 난",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.28",
          "form": "어떤 맛이야?",
          "original_form": "어떤 맛이야?",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.29",
          "form": "그냥 이름이 마음에 안들어",
          "original_form": "그냥 이름이 마음에 안들어",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.30",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.31",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.32",
          "form": "무슨 카레주제에",
          "original_form": "무슨 카레주제에",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.33",
          "form": "자웅동체하라그래",
          "original_form": "자웅동체하라그래",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021587.1.34",
          "form": "그래서 진입장벽좀",
          "original_form": "그래서 진입장벽좀",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.35",
          "form": "높아",
          "original_form": "높아",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.36",
          "form": "그냥 카레황제 하지",
          "original_form": "그냥 카레황제 하지",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.37",
          "form": "카레 어릹내부터",
          "original_form": "카레 어릹내부터",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.38",
          "form": "그게 더 훌륭해보이는데",
          "original_form": "그게 더 훌륭해보이는데",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.39",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.40",
          "form": "오뚜기먹었더니",
          "original_form": "오뚜기먹었더니",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.41",
          "form": "그맛이 익숙해",
          "original_form": "그맛이 익숙해",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.42",
          "form": "나도 오뚜기 그저 잘 먹는데",
          "original_form": "나도 오뚜기 그저 잘 먹는데",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021587.1.43",
          "form": "나는 내가 인도카레를 먹어본 적이 없어서 그런 줄 알았어",
          "original_form": "나는 내가 인도카레를 먹어본 적이 없어서 그런 줄 알았어",
          "speaker_id": "2",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021587.1.44",
          "form": "한국인은 오뚜기지",
          "original_form": "한국인은 오뚜기지",
          "speaker_id": "1",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021587.1.45",
          "form": "ㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021587.1.46",
          "form": "어릴때부터 길들여졌어",
          "original_form": "어릴때부터 길들여졌어",
          "speaker_id": "2",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021587.1.47",
          "form": "맞아 넌 카레먹을때",
          "original_form": "맞아 넌 카레먹을때",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.48",
          "form": "난 갓 끓였을 때",
          "original_form": "난 갓 끓였을 때",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.49",
          "form": "김치먹냐",
          "original_form": "김치먹냐",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.50",
          "form": "포슬포슬 감자가좋아",
          "original_form": "포슬포슬 감자가좋아",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.51",
          "form": "아니 난 김치 그닥 안 먹어",
          "original_form": "아니 난 김치 그닥 안 먹어",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.52",
          "form": "난 오래 끓인거",
          "original_form": "난 오래 끓인거",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.53",
          "form": "계숙 푹 끓인거",
          "original_form": "계숙 푹 끓인거",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.54",
          "form": "나도 김치는 별로드라",
          "original_form": "나도 김치는 별로드라",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.55",
          "form": "오래 끓였을 때는 양파가 좋더라",
          "original_form": "오래 끓였을 때는 양파가 좋더라",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021587.1.56",
          "form": "단무지는 더 이해안돼",
          "original_form": "단무지는 더 이해안돼",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.57",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ약간 짜장의 아류처럼 먹네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ약간 짜장의 아류처럼 먹네",
          "speaker_id": "2",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.58",
          "form": "단무지 먹는 사람도 있구나",
          "original_form": "단무지 먹는 사람도 있구나",
          "speaker_id": "2",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.59",
          "form": "난 카레 먹을 때",
          "original_form": "난 카레 먹을 때",
          "speaker_id": "2",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.60",
          "form": "김치냐 단무지냐는",
          "original_form": "김치냐 단무지냐는",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.61",
          "form": "승부가 안돼",
          "original_form": "승부가 안돼",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.62",
          "form": "아무것도 곁들이지 않고 먹어",
          "original_form": "아무것도 곁들이지 않고 먹어",
          "speaker_id": "2",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021587.1.63",
          "form": "너도 단품으로 먹냐",
          "original_form": "너도 단품으로 먹냐",
          "speaker_id": "2",
          "time": "20210828 20:49"
        },
