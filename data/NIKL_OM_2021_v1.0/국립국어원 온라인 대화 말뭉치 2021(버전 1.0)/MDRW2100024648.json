      "utterance": [
        {
          "id": "MDRW2100024648.1.1",
          "form": "여행좋아하세요",
          "original_form": "여행좋아하세요",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024648.1.2",
          "form": "아뇨 전 집순이에요..ㅋㅋ",
          "original_form": "아뇨 전 집순이에요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024648.1.3",
          "form": "여행 자주 다니세요?",
          "original_form": "여행 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024648.1.4",
          "form": "저도 집순이인데  1년에한번씩 여행병이 들어서 ㅋㅋㅋㅋ코로나전엔 여행은근다녔어요",
          "original_form": "저도 집순이인데  1년에한번씩 여행병이 들어서 ㅋㅋㅋㅋ코로나전엔 여행은근다녔어요",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024648.1.5",
          "form": "오 다녀왔던 곳중에 베스트랑 워스트 알려주세요",
          "original_form": "오 다녀왔던 곳중에 베스트랑 워스트 알려주세요",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024648.1.6",
          "form": "그게 같은데 둘다 우유니요 ㅋㅋㅋㅋㅋ",
          "original_form": "그게 같은데 둘다 우유니요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024648.1.7",
          "form": "오 그 사막인가!? 엄청 유명한 곳이잖아요",
          "original_form": "오 그 사막인가!? 엄청 유명한 곳이잖아요",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024648.1.8",
          "form": "잘 모르지만.. 정확히 어디에요 나라가?",
          "original_form": "잘 모르지만.. 정확히 어디에요 나라가?",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024648.1.9",
          "form": "남미의 볼리비아에요!",
          "original_form": "남미의 볼리비아에요!",
          "speaker_id": "1",
          "time": "20210831 16:38"
        },
        {
          "id": "MDRW2100024648.1.10",
          "form": "소금사막에 물이차서 하늘이 비치는 곳인데",
          "original_form": "소금사막에 물이차서 하늘이 비치는 곳인데",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024648.1.11",
          "form": "천국인줄여 ㅋㅋㅋㅋㅋ",
          "original_form": "천국인줄여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100024648.1.12",
          "form": "사진 그대로인가봐요! 사진으로 보면 완전 합성처럼 이쁘더라구요",
          "original_form": "사진 그대로인가봐요! 사진으로 보면 완전 합성처럼 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024648.1.13",
          "form": "근데 왜 워스트로도 뽑으신거에요?ㅋㅋㅋㅋ",
          "original_form": "근데 왜 워스트로도 뽑으신거에요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024648.1.14",
          "form": "가는게 넘힘들고..먹거리젤중요한데 거기서 먹은음식먹고 배탈나서 고생했었어요",
          "original_form": "가는게 넘힘들고..먹거리젤중요한데 거기서 먹은음식먹고 배탈나서 고생했었어요",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024648.1.15",
          "form": "아이고.. 남미 치안은 어때요??",
          "original_form": "아이고.. 남미 치안은 어때요??",
          "speaker_id": "2",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024648.1.16",
          "form": "예...치안도..정말...",
          "original_form": "예...치안도..정말...",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024648.1.17",
          "form": "우유니자체는 시골마을이라 나쁘지않은데요",
          "original_form": "우유니자체는 시골마을이라 나쁘지않은데요",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024648.1.18",
          "form": "남미전체로보면 후덜덜했어요",
          "original_form": "남미전체로보면 후덜덜했어요",
          "speaker_id": "1",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024648.1.19",
          "form": "ㅠㅡㅠ 밤에는 역시 숙소에만..?",
          "original_form": "ㅠㅡㅠ 밤에는 역시 숙소에만..?",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024648.1.20",
          "form": "네 한발짝도안나가구여",
          "original_form": "네 한발짝도안나가구여",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024648.1.21",
          "form": "숙소에도 페루에선 밑에 철창 감옥처럼 잠가놔요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "숙소에도 페루에선 밑에 철창 감옥처럼 잠가놔요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024648.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 진짜 위험하구나.. 여러명이서 가신거겠죠?",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 진짜 위험하구나.. 여러명이서 가신거겠죠?",
          "speaker_id": "2",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024648.1.23",
          "form": "막 버스강도들었다는소리 같은장소 며칠전에 강도만났다는소리 많이들었어요",
          "original_form": "막 버스강도들었다는소리 같은장소 며칠전에 강도만났다는소리 많이들었어요",
          "speaker_id": "1",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024648.1.24",
          "form": "가방도 꼭 안고다니고 그정도에요?",
          "original_form": "가방도 꼭 안고다니고 그정도에요?",
          "speaker_id": "2",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024648.1.25",
          "form": "전혼자갔었는데",
          "original_form": "전혼자갔었는데",
          "speaker_id": "1",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024648.1.26",
          "form": "여행지에서 동행구해서 다녔어요",
          "original_form": "여행지에서 동행구해서 다녔어요",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024648.1.27",
          "form": "헉 ㄷㄷㄷ 짱이다 완전 이뇨ㅏ",
          "original_form": "헉 ㄷㄷㄷ 짱이다 완전 이뇨ㅏ",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024648.1.28",
          "form": "어디든 한국인들은있거든여 ㅋㅋㅋ홀수무리에꼽사리끼면돼요",
          "original_form": "어디든 한국인들은있거든여 ㅋㅋㅋ홀수무리에꼽사리끼면돼요",
          "speaker_id": "1",
          "time": "20210831 17:51"
        }
      ]
