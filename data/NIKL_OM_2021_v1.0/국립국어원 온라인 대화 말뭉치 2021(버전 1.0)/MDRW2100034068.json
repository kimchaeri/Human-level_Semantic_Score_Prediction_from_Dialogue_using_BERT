        },
        {
          "id": "MDRW2100034068.1.2",
          "form": "무슨 재료들어간 음료임?",
          "original_form": "무슨 재료들어간 음료임?",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100034068.1.3",
          "form": "뭔가요새 계속 우울한데",
          "original_form": "뭔가요새 계속 우울한데",
          "speaker_id": "2",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100034068.1.4",
          "form": "브랜드임 백종원커피",
          "original_form": "브랜드임 백종원커피",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.5",
          "form": "프랜차이즈",
          "original_form": "프랜차이즈",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.6",
          "form": "백다방?",
          "original_form": "백다방?",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.7",
          "form": "비와서그러나",
          "original_form": "비와서그러나",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.8",
          "form": "달달한거 먹으면 일시적으로 기분 좋아짐",
          "original_form": "달달한거 먹으면 일시적으로 기분 좋아짐",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.10",
          "form": "일시적인게 마약작용과 머가다름니까 하고 나도 먹고프네",
          "original_form": "일시적인게 마약작용과 머가다름니까 하고 나도 먹고프네",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.11",
          "form": "인생의 낙?",
          "original_form": "인생의 낙?",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034068.1.12",
          "form": "근데 솔직히 먹고나면 후회함 ㅋ",
          "original_form": "근데 솔직히 먹고나면 후회함 ㅋ",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100034068.1.13",
          "form": "ㅋㅋ그것도 마약중독같다야",
          "original_form": "ㅋㅋ그것도 마약중독같다야",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100034068.1.14",
          "form": "이러고 나도 무금ㅋㅋ",
          "original_form": "이러고 나도 무금ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100034068.1.15",
          "form": "솔직히 비싸기도함",
          "original_form": "솔직히 비싸기도함",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100034068.1.16",
          "form": "카페서 사먹는음료들",
          "original_form": "카페서 사먹는음료들",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100034068.1.17",
          "form": "음 글체",
