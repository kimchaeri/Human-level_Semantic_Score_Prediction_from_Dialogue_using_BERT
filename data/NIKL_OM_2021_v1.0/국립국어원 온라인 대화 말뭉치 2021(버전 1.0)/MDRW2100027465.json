        {
          "id": "MDRW2100027465.1.1",
          "form": "결혼은 해야할까",
          "original_form": "결혼은 해야할까",
          "speaker_id": "1",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027465.1.2",
          "form": "결혼 안해도돼",
          "original_form": "결혼 안해도돼",
          "speaker_id": "2",
          "time": "20210902 12:50"
        },
        {
          "id": "MDRW2100027465.1.3",
          "form": "연애는 해야할까",
          "original_form": "연애는 해야할까",
          "speaker_id": "1",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027465.1.4",
          "form": "연애는 다르지",
          "original_form": "연애는 다르지",
          "speaker_id": "2",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027465.1.5",
          "form": "삶의 활력이 되지",
          "original_form": "삶의 활력이 되지",
          "speaker_id": "2",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027465.1.6",
          "form": "좋은사람 만나야해",
          "original_form": "좋은사람 만나야해",
          "speaker_id": "1",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027465.1.7",
          "form": "연애 근황어때",
          "original_form": "연애 근황어때",
          "speaker_id": "1",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027465.1.8",
          "form": "어려워",
          "original_form": "어려워",
          "speaker_id": "2",
          "time": "20210902 12:51"
        },
        {
          "id": "MDRW2100027465.1.9",
          "form": "나아닌 누군가를 이해하고 받아들이기 쉽지않지",
          "original_form": "나아닌 누군가를 이해하고 받아들이기 쉽지않지",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.10",
          "form": "맞춘다는게 쉽지않아",
          "original_form": "맞춘다는게 쉽지않아",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.11",
          "form": "그래도 연애는 좋아",
          "original_form": "그래도 연애는 좋아",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.12",
          "form": "결혼도 장점이 많아",
          "original_form": "결혼도 장점이 많아",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.13",
          "form": "친구같고 좋지",
          "original_form": "친구같고 좋지",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.14",
          "form": "동반자니까",
          "original_form": "동반자니까",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.15",
          "form": "고민도 나누고",
          "original_form": "고민도 나누고",
          "speaker_id": "1",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027465.1.16",
          "form": "함께 기뻐하고",
          "original_form": "함께 기뻐하고",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027465.1.17",
          "form": "같이 늙어가는게 큰 축복이야",
          "original_form": "같이 늙어가는게 큰 축복이야",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027465.1.18",
