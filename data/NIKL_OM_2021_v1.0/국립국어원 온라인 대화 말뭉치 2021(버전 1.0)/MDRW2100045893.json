        {
          "id": "MDRW2100045893.1.1",
          "form": "북유럽ㅁ가봤니",
          "original_form": "북유럽ㅁ가봤니",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045893.1.2",
          "form": "덴마크?",
          "original_form": "덴마크?",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.3",
          "form": "스웨덴도 있고",
          "original_form": "스웨덴도 있고",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.4",
          "form": "덴마크 가봤어",
          "original_form": "덴마크 가봤어",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.5",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.6",
          "form": "레고의 나라야",
          "original_form": "레고의 나라야",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.7",
          "form": "온곳이 레곤가?",
          "original_form": "온곳이 레곤가?",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.8",
          "form": "그리고 핫도그의 고장이지",
          "original_form": "그리고 핫도그의 고장이지",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.9",
          "form": "진짜? 몰랐어",
          "original_form": "진짜? 몰랐어",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100045893.1.10",
          "form": "스테프핫도그 이런거  다 덴마크꺼잖아",
          "original_form": "스테프핫도그 이런거  다 덴마크꺼잖아",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045893.1.11",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045893.1.12",
          "form": "나도 1개먹어봤어",
          "original_form": "나도 1개먹어봤어",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045893.1.13",
          "form": "뭐감종 달라?",
          "original_form": "뭐감종 달라?",
