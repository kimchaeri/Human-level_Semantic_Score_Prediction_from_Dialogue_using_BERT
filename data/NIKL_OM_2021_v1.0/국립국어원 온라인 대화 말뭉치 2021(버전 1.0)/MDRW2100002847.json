      "utterance": [
        {
          "id": "MDRW2100002847.1.1",
          "form": "전시회 좋아하시나요?",
          "original_form": "전시회 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002847.1.2",
          "form": "별로 경험이 없어요~",
          "original_form": "별로 경험이 없어요~",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002847.1.3",
          "form": "공연은요?",
          "original_form": "공연은요?",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002847.1.4",
          "form": "연극 본적있어요",
          "original_form": "연극 본적있어요",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002847.1.5",
          "form": "어떤 연극 보셨나요?",
          "original_form": "어떤 연극 보셨나요?",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002847.1.6",
          "form": "대학로에서 하는거요",
          "original_form": "대학로에서 하는거요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002847.1.7",
          "form": "저도 대학로에서 연극본적있는데..",
          "original_form": "저도 대학로에서 연극본적있는데..",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002847.1.8",
          "form": "너무 재미없었던것만 기억나네요ㅠㅠ",
          "original_form": "너무 재미없었던것만 기억나네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002847.1.9",
          "form": "제목도 까먹었어요",
          "original_form": "제목도 까먹었어요",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002847.1.10",
          "form": "아 ㅋㅋ저도 제목이 생각이 안나요",
          "original_form": "아 ㅋㅋ저도 제목이 생각이 안나요",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002847.1.11",
          "form": "최근 본 영화는 모가디슈봤어요",
          "original_form": "최근 본 영화는 모가디슈봤어요",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002847.1.12",
          "form": "재밌던가요?? 모가디슈 후기 좋아보이던데",
          "original_form": "재밌던가요?? 모가디슈 후기 좋아보이던데",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002847.1.13",
          "form": "꼭 보세요~ 저는 정말 잘 봤어요",
          "original_form": "꼭 보세요~ 저는 정말 잘 봤어요",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002847.1.14",
          "form": "ㅎ한번 봐야겠네요 ㅎㅎ 전 마지막으로 본 영화가 블랙위도우예요",
          "original_form": "ㅎ한번 봐야겠네요 ㅎㅎ 전 마지막으로 본 영화가 블랙위도우예요",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100002847.1.15",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100002847.1.16",
          "form": "앗 ㅎ 못봤어요~ 블랙위도우 재밌어요?",
          "original_form": "앗 ㅎ 못봤어요~ 블랙위도우 재밌어요?",
          "speaker_id": "2",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100002847.1.17",
          "form": "진짜 재밌어요 ㅋㅋㅋ 마블 보셨으면.. 꼭 보시길..ㅠㅠ",
          "original_form": "진짜 재밌어요 ㅋㅋㅋ 마블 보셨으면.. 꼭 보시길..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 19:35"
        },
        {
          "id": "MDRW2100002847.1.18",
          "form": "오 감사합니다 꼭 보겠습니다",
          "original_form": "오 감사합니다 꼭 보겠습니다",
