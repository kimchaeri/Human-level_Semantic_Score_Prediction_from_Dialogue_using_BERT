      "utterance": [
        {
          "id": "MDRW2100023036.1.1",
          "form": "저녁 머 드셨나요?",
          "original_form": "저녁 머 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023036.1.2",
          "form": "저녁 가자미구이랑 된장찌개 먹었어요!",
          "original_form": "저녁 가자미구이랑 된장찌개 먹었어요!",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023036.1.3",
          "form": "어떤거 드셨나용",
          "original_form": "어떤거 드셨나용",
          "speaker_id": "1",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023036.1.4",
          "form": "고등어 구이 먹었어요",
          "original_form": "고등어 구이 먹었어요",
          "speaker_id": "2",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100023036.1.5",
          "form": "생선구이가 참 맛있어요 ㅎㅎ",
          "original_form": "생선구이가 참 맛있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023036.1.6",
          "form": "냄새 처리가 어려워서 그렇지 생선 구이 넘 맛있어요",
          "original_form": "냄새 처리가 어려워서 그렇지 생선 구이 넘 맛있어요",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023036.1.7",
          "form": "그쵸 ㅠㅠ 맞아요.. 그래서 가끔은 반조리생선구이 사먹는데 편하더라구요..!",
          "original_form": "그쵸 ㅠㅠ 맞아요.. 그래서 가끔은 반조리생선구이 사먹는데 편하더라구요..!",
          "speaker_id": "1",
          "time": "20210830 11:14"
        },
        {
          "id": "MDRW2100023036.1.8",
          "form": "저도 그런거 한 번 먹어봤는데 양이 넘 작아서 ㅎㅎ",
          "original_form": "저도 그런거 한 번 먹어봤는데 양이 넘 작아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100023036.1.9",
          "form": "그래서 두개데워 먹을때도 있죠 ..ㅋㅋ 양이 조금 적긴해요",
          "original_form": "그래서 두개데워 먹을때도 있죠 ..ㅋㅋ 양이 조금 적긴해요",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100023036.1.10",
          "form": "ㅋㅋㅋ저만 그렇게 느낀게 아니었군요",
          "original_form": "ㅋㅋㅋ저만 그렇게 느낀게 아니었군요",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100023036.1.11",
          "form": "그게 살이 발라져있어서 그런가 진짜 작긴작더라구요ㅋㅋ",
          "original_form": "그게 살이 발라져있어서 그런가 진짜 작긴작더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100023036.1.12",
          "form": "곧 점심시간인데 뭐 드세요?",
          "original_form": "곧 점심시간인데 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023036.1.13",
          "form": "샌드위치 먹을까 고민중인데 아직 정해지진않았어요",
          "original_form": "샌드위치 먹을까 고민중인데 아직 정해지진않았어요",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023036.1.14",
          "form": "어떤거 드실예정이세요?",
          "original_form": "어떤거 드실예정이세요?",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023036.1.15",
          "form": "저는 그냥 구내식당에서 주는 거요 ㅎㅎ 메뉴도 몰라요",
          "original_form": "저는 그냥 구내식당에서 주는 거요 ㅎㅎ 메뉴도 몰라요",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023036.1.16",
          "form": "구내식당이용하시는군요ㅎㅎ 밥은 괜찮게 나오는 편인가요?",
          "original_form": "구내식당이용하시는군요ㅎㅎ 밥은 괜찮게 나오는 편인가요?",
          "speaker_id": "1",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100023036.1.17",
          "form": "아니오ㅋㅋㅋㅋㅋ고등학교 때 먹었던 것보다 별로에요ㅠㅠ",
          "original_form": "아니오ㅋㅋㅋㅋㅋ고등학교 때 먹었던 것보다 별로에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023036.1.18",
          "form": "아앗 .. 그렇군요ㅠㅠ 밥이 맛있어야 힘이 나는데 말이죠",
          "original_form": "아앗 .. 그렇군요ㅠㅠ 밥이 맛있어야 힘이 나는데 말이죠",
          "speaker_id": "1",
          "time": "20210830 12:06"
        },
        {
          "id": "MDRW2100023036.1.19",
          "form": "그러게요ㅠㅠ 그치만 단체 급식은 어쩔 수 없나봐요",
          "original_form": "그러게요ㅠㅠ 그치만 단체 급식은 어쩔 수 없나봐요",
          "speaker_id": "2",
          "time": "20210830 12:09"
        },
        {
          "id": "MDRW2100023036.1.20",
          "form": "그러게말이에요 ㅠㅠ",
          "original_form": "그러게말이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:34"
        }
      ]
    }
