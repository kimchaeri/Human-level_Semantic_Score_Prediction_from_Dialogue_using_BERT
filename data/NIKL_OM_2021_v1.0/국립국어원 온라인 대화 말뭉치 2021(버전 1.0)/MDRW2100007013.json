      "utterance": [
        {
          "id": "MDRW2100007013.1.1",
          "form": "ㅋㅋㅋ다 먹구살자구 하는거 아니겠어요...ㅎ",
          "original_form": "ㅋㅋㅋ다 먹구살자구 하는거 아니겠어요...ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007013.1.2",
          "form": "돈들이면 오마카세도 좋아해요",
          "original_form": "돈들이면 오마카세도 좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007013.1.3",
          "form": "저는 못가봤어요 ㅠㅠㅠ",
          "original_form": "저는 못가봤어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007013.1.4",
          "form": "결혼하고 애낳고 찌들어 사느라",
          "original_form": "결혼하고 애낳고 찌들어 사느라",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007013.1.5",
          "form": "여긴 지방이거든요 싼건 5만원대도 있어요!!",
          "original_form": "여긴 지방이거든요 싼건 5만원대도 있어요!!",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007013.1.6",
          "form": "나중에 아이분이랑 데이트해보세요ㅎ",
          "original_form": "나중에 아이분이랑 데이트해보세요ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.7",
          "form": "예전에는 그런곳도 많이 없었던것 같은데",
          "original_form": "예전에는 그런곳도 많이 없었던것 같은데",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.8",
          "form": "최근들어 많이 보이더라구요",
          "original_form": "최근들어 많이 보이더라구요",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.9",
          "form": "약간 유행인거같긴해요ㅋㅋ",
          "original_form": "약간 유행인거같긴해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.10",
          "form": "일본에서 수출한...?",
          "original_form": "일본에서 수출한...?",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.11",
          "form": "근데 한우오마카세가 인기더라구요 스시보다",
          "original_form": "근데 한우오마카세가 인기더라구요 스시보다",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.12",
          "form": "와 한우는 뭔들",
          "original_form": "와 한우는 뭔들",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007013.1.13",
          "form": "모퉁이우라고 인당 25만원하는곳있는데",
          "original_form": "모퉁이우라고 인당 25만원하는곳있는데",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007013.1.14",
          "form": "출세해서 꼭 가보고싶습니다",
          "original_form": "출세해서 꼭 가보고싶습니다",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007013.1.15",
          "form": "와 진짜 비싸네요 이생에 저는 못가볼것 같아요ㅠㅠ",
          "original_form": "와 진짜 비싸네요 이생에 저는 못가볼것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007013.1.16",
          "form": "너무 비싸네용 ㅠㅠ",
          "original_form": "너무 비싸네용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007013.1.17",
          "form": "저두 로또 당첨되면ㅎㅎ",
          "original_form": "저두 로또 당첨되면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007013.1.18",
          "form": "근데 치킨한마리로도 충분히행복할수잇죠",
          "original_form": "근데 치킨한마리로도 충분히행복할수잇죠",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007013.1.19",
          "form": "맞아요 두마리 치킨 안먹고",
          "original_form": "맞아요 두마리 치킨 안먹고",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007013.1.20",
          "form": "교촌 푸라닭 먹어도 행복하죠 ㅎㅎ 나름의 출세",
          "original_form": "교촌 푸라닭 먹어도 행복하죠 ㅎㅎ 나름의 출세",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007013.1.21",
          "form": "호식이치킨ㅋㅋㅋ거기두잇나요",
          "original_form": "호식이치킨ㅋㅋㅋ거기두잇나요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007013.1.22",
          "form": "여기가 본고장 일거에요",
          "original_form": "여기가 본고장 일거에요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007013.1.23",
          "form": "교촌은 출세의 상징이죠",
          "original_form": "교촌은 출세의 상징이죠",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100007013.1.24",
          "form": "치킨계의 도미노?",
          "original_form": "치킨계의 도미노?",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007013.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 교촌 레드콤보 딱",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 교촌 레드콤보 딱",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007013.1.26",
          "form": "어 저돈데 호식이치킨 사장분 제친구옆집이엇대요ㅋㅋㅋ",
          "original_form": "어 저돈데 호식이치킨 사장분 제친구옆집이엇대요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007013.1.27",
          "form": "아ㅠ근데저는 또 후라이드만...",
          "original_form": "아ㅠ근데저는 또 후라이드만...",
          "speaker_id": "2",
