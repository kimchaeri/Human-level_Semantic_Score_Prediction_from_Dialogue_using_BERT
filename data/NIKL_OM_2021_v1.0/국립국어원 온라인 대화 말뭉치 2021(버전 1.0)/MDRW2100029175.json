      "utterance": [
        {
          "id": "MDRW2100029175.1.1",
          "form": "이제 개학",
          "original_form": "이제 개학",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029175.1.2",
          "form": "늦게마침",
          "original_form": "늦게마침",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029175.1.3",
          "form": "시험 안치는 학교도 있음",
          "original_form": "시험 안치는 학교도 있음",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029175.1.4",
          "form": "대학교 가요",
          "original_form": "대학교 가요",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029175.1.5",
          "form": "초딩들 방학 길어서 부럳",
          "original_form": "초딩들 방학 길어서 부럳",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.6",
          "form": "하나도 모르겠던데요",
          "original_form": "하나도 모르겠던데요",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.7",
          "form": "부럽",
          "original_form": "부럽",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.8",
          "form": "초딩으로 다시 돌아가고 싶다",
          "original_form": "초딩으로 다시 돌아가고 싶다",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.9",
          "form": "한 달 동안 잠만 자고 싶다",
          "original_form": "한 달 동안 잠만 자고 싶다",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.10",
          "form": "내 사촌동생은",
          "original_form": "내 사촌동생은",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.11",
          "form": "2주뒤에 시험침",
          "original_form": "2주뒤에 시험침",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.12",
          "form": "오늘 회원은",
          "original_form": "오늘 회원은",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.13",
          "form": "중간은 안치고",
          "original_form": "중간은 안치고",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.14",
          "form": "초등학교 때 문구점에서 팔던 물만두 먹고 싶어요",
          "original_form": "초등학교 때 문구점에서 팔던 물만두 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.15",
          "form": "기말을 전과목으로 친다던데",
          "original_form": "기말을 전과목으로 친다던데",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.16",
          "form": "중학생이에요",
          "original_form": "중학생이에요",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.17",
          "form": "3일빠세게",
          "original_form": "3일빠세게",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029175.1.18",
          "form": "중삼 은 보통 많이 그렇게 해요",
          "original_form": "중삼 은 보통 많이 그렇게 해요",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029175.1.19",
          "form": "난 학교에서 도시락",
          "original_form": "난 학교에서 도시락",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029175.1.20",
          "form": "수업시간에 까먹은거",
          "original_form": "수업시간에 까먹은거",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029175.1.21",
          "form": "생각남",
          "original_form": "생각남",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029175.1.22",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029175.1.23",
