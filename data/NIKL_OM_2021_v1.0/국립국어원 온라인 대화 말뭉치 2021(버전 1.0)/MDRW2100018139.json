      "utterance": [
        {
          "id": "MDRW2100018139.1.1",
          "form": "넷플 보시나여",
          "original_form": "넷플 보시나여",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018139.1.2",
          "form": "넷플 구독을 지금 고민하고 있어요",
          "original_form": "넷플 구독을 지금 고민하고 있어요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018139.1.3",
          "form": "넷플 구독 하고 계신가요",
          "original_form": "넷플 구독 하고 계신가요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018139.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018139.1.5",
          "form": "넵 하고있어여",
          "original_form": "넵 하고있어여",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018139.1.6",
          "form": "자주 보시나요? 지금 제가 보고 싶은 드라마는 있는데 그것때문에 결제하긴 망설여지더라구요",
          "original_form": "자주 보시나요? 지금 제가 보고 싶은 드라마는 있는데 그것때문에 결제하긴 망설여지더라구요",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018139.1.7",
          "form": "넵 엄청 유용해요",
          "original_form": "넵 엄청 유용해요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018139.1.8",
          "form": "주로 어떤 걸 보시나요?",
          "original_form": "주로 어떤 걸 보시나요?",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018139.1.9",
          "form": "킹덤이요!",
          "original_form": "킹덤이요!",
          "speaker_id": "1",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100018139.1.10",
          "form": "아 맞아요 킹덤도 넷플릭스에서 볼 수 있는 거였죠",
          "original_form": "아 맞아요 킹덤도 넷플릭스에서 볼 수 있는 거였죠",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100018139.1.11",
          "form": "킹덤도 궁금하더라고요 아직 한번도 안봐봤거든요",
          "original_form": "킹덤도 궁금하더라고요 아직 한번도 안봐봤거든요",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100018139.1.12",
          "form": "진짜 재밌어요!",
          "original_form": "진짜 재밌어요!",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100018139.1.13",
          "form": "넷플릭스 구독해야겠네요 ㅋㅋㅋ",
          "original_form": "넷플릭스 구독해야겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100018139.1.14",
          "form": "주로 어느 시간대에 많이 보세요?",
          "original_form": "주로 어느 시간대에 많이 보세요?",
          "speaker_id": "2",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100018139.1.15",
          "form": "저녁시간대요!",
          "original_form": "저녁시간대요!",
          "speaker_id": "1",
          "time": "20210828 15:39"
        },
        {
          "id": "MDRW2100018139.1.16",
          "form": "좋네요!!",
          "original_form": "좋네요!!",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100018139.1.17",
          "form": "킹덤꼭보세요",
          "original_form": "킹덤꼭보세요",
          "speaker_id": "1",
