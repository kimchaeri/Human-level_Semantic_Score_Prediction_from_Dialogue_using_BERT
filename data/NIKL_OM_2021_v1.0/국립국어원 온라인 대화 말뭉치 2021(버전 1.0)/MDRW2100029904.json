      "utterance": [
        {
          "id": "MDRW2100029904.1.1",
          "form": "안녕하세요~ 직장다니시나요?",
          "original_form": "안녕하세요~ 직장다니시나요?",
          "speaker_id": "1",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029904.1.2",
          "form": "예 지금 일하는 중입니다",
          "original_form": "예 지금 일하는 중입니다",
          "speaker_id": "2",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029904.1.3",
          "form": "와우 어떤 쪽에 일하시나요?",
          "original_form": "와우 어떤 쪽에 일하시나요?",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029904.1.4",
          "form": "교육쪽이요",
          "original_form": "교육쪽이요",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029904.1.5",
          "form": "마침 엊그제가 9월 모의고사였던지라",
          "original_form": "마침 엊그제가 9월 모의고사였던지라",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029904.1.6",
          "form": "요즘 조금 바쁘네요",
          "original_form": "요즘 조금 바쁘네요",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029904.1.7",
          "form": "아 신기하네요~ 선생님이신가봐요~",
          "original_form": "아 신기하네요~ 선생님이신가봐요~",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029904.1.8",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029904.1.9",
          "form": "현장 쪽은 아니고",
          "original_form": "현장 쪽은 아니고",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029904.1.10",
          "form": "그 교재쪽이에요",
          "original_form": "그 교재쪽이에요",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029904.1.11",
          "form": "오 교과서 만드시는군요~? 스트레스 많이 받으세요??",
          "original_form": "오 교과서 만드시는군요~? 스트레스 많이 받으세요??",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029904.1.12",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029904.1.13",
          "form": "이쪽이 스트레스가 많은 업계인데",
          "original_form": "이쪽이 스트레스가 많은 업계인데",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029904.1.14",
          "form": "저는 덜 받네요",
          "original_form": "저는 덜 받네요",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029904.1.15",
          "form": "아 그래요? 주변에 없는 직종이라 신기해요",
          "original_form": "아 그래요? 주변에 없는 직종이라 신기해요",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029904.1.16",
          "form": "어떤일을 주로 하시는거예요? 문서?",
          "original_form": "어떤일을 주로 하시는거예요? 문서?",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029904.1.17",
          "form": "국어 비문학 지문 만들어요",
          "original_form": "국어 비문학 지문 만들어요",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029904.1.18",
          "form": "아 놀랍게도 맞춤법은 잘 못합니다",
          "original_form": "아 놀랍게도 맞춤법은 잘 못합니다",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029904.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029904.1.20",
          "form": "띄어쓰기는요?? 기본이죠??",
          "original_form": "띄어쓰기는요?? 기본이죠??",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029904.1.21",
          "form": "띄어쓰기가 어렵다던데",
          "original_form": "띄어쓰기가 어렵다던데",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029904.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029904.1.23",
          "form": "제일 나쁜 친구죠",
          "original_form": "제일 나쁜 친구죠",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029904.1.24",
          "form": "예컨대",
          "original_form": "예컨대",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029904.1.25",
          "form": "쌓아올리다 랑",
          "original_form": "쌓아올리다 랑",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029904.1.26",
          "form": "쌓아 올리다 랑",
          "original_form": "쌓아 올리다 랑",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029904.1.27",
          "form": "둘 다 가능한데 용례가 다르니",
          "original_form": "둘 다 가능한데 용례가 다르니",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029904.1.28",
          "form": "맞춤법 검사기도 못 믿어요",
          "original_form": "맞춤법 검사기도 못 믿어요",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029904.1.29",
          "form": "와우 뭔 차이인지 알려주실 수 있으세요?",
          "original_form": "와우 뭔 차이인지 알려주실 수 있으세요?",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029904.1.30",
          "form": "제 띄어쓰기도 맞나요?..ㅎ",
          "original_form": "제 띄어쓰기도 맞나요?..ㅎ",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
