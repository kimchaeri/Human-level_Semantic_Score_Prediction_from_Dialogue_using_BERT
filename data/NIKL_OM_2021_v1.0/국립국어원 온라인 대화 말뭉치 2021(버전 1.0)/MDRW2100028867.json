      "utterance": [
        {
          "id": "MDRW2100028867.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028867.1.2",
          "form": "책 읽어본 지가;;",
          "original_form": "책 읽어본 지가;;",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028867.1.3",
          "form": "애들 읽어주는 동화책이다 네요;;",
          "original_form": "애들 읽어주는 동화책이다 네요;;",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028867.1.4",
          "form": "자상한 부모님이시네요",
          "original_form": "자상한 부모님이시네요",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028867.1.5",
          "form": "매일 읽어주시나요?",
          "original_form": "매일 읽어주시나요?",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028867.1.6",
          "form": "읽어주려고 노력해요~",
          "original_form": "읽어주려고 노력해요~",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028867.1.7",
          "form": "요즘 코로나 때문에",
          "original_form": "요즘 코로나 때문에",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028867.1.8",
          "form": "바깥 활동도 제약이 많아서ㅠ",
          "original_form": "바깥 활동도 제약이 많아서ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028867.1.9",
          "form": "코로나땜에 더 그렇겠어요ㅜㅜㅜ",
          "original_form": "코로나땜에 더 그렇겠어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028867.1.10",
          "form": "요즘은 아가들 동화책이",
          "original_form": "요즘은 아가들 동화책이",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028867.1.11",
          "form": "너무 이쁘고",
          "original_form": "너무 이쁘고",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028867.1.12",
          "form": "아기자기 일러스트",
          "original_form": "아기자기 일러스트",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028867.1.13",
          "form": "내용도 깊이있고 다양하게",
          "original_form": "내용도 깊이있고 다양하게",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028867.1.14",
          "form": "잘 나오더라구요",
          "original_form": "잘 나오더라구요",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028867.1.15",
          "form": "맞아요 책으로 만나는 세상이",
          "original_form": "맞아요 책으로 만나는 세상이",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028867.1.16",
          "form": "지금은 거의 다라고 보면 돼요ㅠ",
          "original_form": "지금은 거의 다라고 보면 돼요ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028867.1.17",
          "form": "불쌍해요 애들ㅠ",
          "original_form": "불쌍해요 애들ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028867.1.18",
          "form": "ㅠ안타깝ㅠ",
          "original_form": "ㅠ안타깝ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028867.1.19",
          "form": "애들한테 미안하죠ㅠ",
          "original_form": "애들한테 미안하죠ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028867.1.20",
          "form": "재미있게 읽은 책 있으세요?",
          "original_form": "재미있게 읽은 책 있으세요?",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028867.1.21",
          "form": "저는 추리소설도 좋아하고 sf쪽도",
          "original_form": "저는 추리소설도 좋아하고 sf쪽도",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028867.1.22",
          "form": "좋아해서",
          "original_form": "좋아해서",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028867.1.23",
          "form": "이북대여해서  많이 읽는편인데",
          "original_form": "이북대여해서  많이 읽는편인데",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028867.1.24",
          "form": "나인폭스갬빗이랑 백번째여왕",
          "original_form": "나인폭스갬빗이랑 백번째여왕",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028867.1.25",
          "form": "재밌엇어요",
          "original_form": "재밌엇어요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028867.1.26",
          "form": "저도 시간 되면 찾아서 한번 볼게요~",
          "original_form": "저도 시간 되면 찾아서 한번 볼게요~",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028867.1.27",
          "form": "대화하면서 유행한 정보도 엄청 많이 얻어요",
          "original_form": "대화하면서 유행한 정보도 엄청 많이 얻어요",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028867.1.28",
          "form": "유용한",
          "original_form": "유용한",
          "speaker_id": "1",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028867.1.29",
          "form": "저도그래요",
          "original_form": "저도그래요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028867.1.30",
          "form": "이것도 나름 참 좋네요",
          "original_form": "이것도 나름 참 좋네요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        }
      ]
    }
  ]
