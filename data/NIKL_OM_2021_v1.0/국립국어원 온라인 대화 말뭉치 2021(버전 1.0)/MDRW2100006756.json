      "utterance": [
        {
          "id": "MDRW2100006756.1.1",
          "form": "어제 운동해서 알이 여기저기...",
          "original_form": "어제 운동해서 알이 여기저기...",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100006756.1.2",
          "form": "좋은 알이다!! 알도 많이 먹어",
          "original_form": "좋은 알이다!! 알도 많이 먹어",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100006756.1.3",
          "form": "ㅎㅎㅎ생태탕좋지",
          "original_form": "ㅎㅎㅎ생태탕좋지",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100006756.1.4",
          "form": "알 많이 고니 쪼끔",
          "original_form": "알 많이 고니 쪼끔",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100006756.1.5",
          "form": "ㅋㅋ 생태는 구하기 어려워ㅠ",
          "original_form": "ㅋㅋ 생태는 구하기 어려워ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006756.1.6",
          "form": "ㅎㅎ 운동하는데 직장상사가 너무 말랐다고",
          "original_form": "ㅎㅎ 운동하는데 직장상사가 너무 말랐다고",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006756.1.7",
          "form": "엄청 뭐라하시네",
          "original_form": "엄청 뭐라하시네",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006756.1.8",
          "form": "건강미가 사라졌다고",
          "original_form": "건강미가 사라졌다고",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006756.1.9",
          "form": "국밥이라도 사주며 잔소리하라고 해",
          "original_form": "국밥이라도 사주며 잔소리하라고 해",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006756.1.10",
          "form": "남편이 살쪘다고 구박하냐고",
          "original_form": "남편이 살쪘다고 구박하냐고",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006756.1.11",
          "form": "걱정된다는데",
          "original_form": "걱정된다는데",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006756.1.12",
          "form": "난 좋은데ㅜㅜ심한가",
          "original_form": "난 좋은데ㅜㅜ심한가",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006756.1.13",
          "form": "운동도 잘하는데",
          "original_form": "운동도 잘하는데",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006756.1.14",
          "form": "ㅋㅋ 아냐 안그래",
          "original_form": "ㅋㅋ 아냐 안그래",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006756.1.15",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006756.1.16",
          "form": "그치 다이어트 안하고 이제 밥 조절하며 운동하니까 ㅎㅎ",
          "original_form": "그치 다이어트 안하고 이제 밥 조절하며 운동하니까 ㅎㅎ",
