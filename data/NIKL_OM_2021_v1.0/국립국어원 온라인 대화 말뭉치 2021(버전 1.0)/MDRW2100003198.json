      "utterance": [
        {
          "id": "MDRW2100003198.1.1",
          "form": "나중에 배구보면서 맛있는 음식도 먹고싶네요 ㅎ",
          "original_form": "나중에 배구보면서 맛있는 음식도 먹고싶네요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100003198.1.2",
          "form": "앗 저도요",
          "original_form": "앗 저도요",
          "speaker_id": "2",
          "time": "20210820 20:26"
        },
        {
          "id": "MDRW2100003198.1.3",
          "form": "치맥 땡겨요..",
          "original_form": "치맥 땡겨요..",
          "speaker_id": "2",
          "time": "20210820 20:26"
        },
        {
          "id": "MDRW2100003198.1.4",
          "form": "오.. 술 좋아하시나요?",
          "original_form": "오.. 술 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 20:26"
        },
        {
          "id": "MDRW2100003198.1.5",
          "form": "음 그럭저럭 좋아하는 거 같아요",
          "original_form": "음 그럭저럭 좋아하는 거 같아요",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003198.1.6",
          "form": "집에서도 꽤 마셔요!",
          "original_form": "집에서도 꽤 마셔요!",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003198.1.7",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003198.1.8",
          "form": "오 대박 부러워요",
          "original_form": "오 대박 부러워요",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003198.1.9",
          "form": "전 술 좋아하고 싶은데 맛이 생각보다...",
          "original_form": "전 술 좋아하고 싶은데 맛이 생각보다...",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003198.1.10",
          "form": "아 ㅋㅋㅋㅋ 그쳐.... 그럼 과일소주 이런거는요?",
          "original_form": "아 ㅋㅋㅋㅋ 그쳐.... 그럼 과일소주 이런거는요?",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003198.1.11",
          "form": "먹어보긴 했어요 ㅎ",
          "original_form": "먹어보긴 했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003198.1.12",
          "form": "ㅋㅋㅋㅋ취향이 아니셨구냐..",
          "original_form": "ㅋㅋㅋㅋ취향이 아니셨구냐..",
          "speaker_id": "2",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003198.1.13",
          "form": "요새는 홍차랑 소주랑 타먹는 거 핫하다는데 드셔보셨나요?",
          "original_form": "요새는 홍차랑 소주랑 타먹는 거 핫하다는데 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003198.1.14",
          "form": "머드쉐이크 이런건 그냥 초코맛만나고 술같지않더라고요!!",
          "original_form": "머드쉐이크 이런건 그냥 초코맛만나고 술같지않더라고요!!",
          "speaker_id": "2",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003198.1.15",
          "form": "홀..그런게 핫해요..? 신기하다",
          "original_form": "홀..그런게 핫해요..? 신기하다",
          "speaker_id": "2",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003198.1.16",
          "form": "나중에 먹어봐야겠어요....",
          "original_form": "나중에 먹어봐야겠어요....",
          "speaker_id": "2",
          "time": "20210820 20:31"
        },
        {
          "id": "MDRW2100003198.1.17",
          "form": "혹시 민트초코 좋아하세요?? 민초 소주도 나왔던데요..!!",
          "original_form": "혹시 민트초코 좋아하세요?? 민초 소주도 나왔던데요..!!",
          "speaker_id": "2",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003198.1.18",
          "form": "네 티비에 나왔었거등요!!!",
          "original_form": "네 티비에 나왔었거등요!!!",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003198.1.19",
          "form": "너 민트초코 사랑합니다",
          "original_form": "너 민트초코 사랑합니다",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003198.1.20",
          "form": "근데 소주랑....민트라니...ㅎ",
          "original_form": "근데 소주랑....민트라니...ㅎ",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003198.1.21",
          "form": "오오오오오오 드셔보셨어요?!",
          "original_form": "오오오오오오 드셔보셨어요?!",
          "speaker_id": "2",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003198.1.22",
          "form": "근데 그거 초코맛이 더 느껴진다던데 좀 궁금하네요",
          "original_form": "근데 그거 초코맛이 더 느껴진다던데 좀 궁금하네요",
          "speaker_id": "2",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003198.1.23",
          "form": "그러게요 소주랑 민트라니......왜 그랬을까..",
          "original_form": "그러게요 소주랑 민트라니......왜 그랬을까..",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003198.1.24",
          "form": "아니요 아쉽게 먹어보진 못했는데",
          "original_form": "아니요 아쉽게 먹어보진 못했는데",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003198.1.25",
          "form": "뭔가 상상이 절대 안가는 맛 같아요ㅋㅋㅋㅋㅋ",
