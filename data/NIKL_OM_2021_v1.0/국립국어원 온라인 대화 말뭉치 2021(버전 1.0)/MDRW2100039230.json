        },
        {
          "id": "MDRW2100039230.1.2",
          "form": "저방금운동하겟다구",
          "original_form": "저방금운동하겟다구",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.3",
          "form": "줄넘기한다고설치다가",
          "original_form": "줄넘기한다고설치다가",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.4",
          "form": "발목삔거같아요",
          "original_form": "발목삔거같아요",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.5",
          "form": "ㄱㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㄱㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.6",
          "form": "세상이런바보가",
          "original_form": "세상이런바보가",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.7",
          "form": "헉ㅜㅜㅜㅜ",
          "original_form": "헉ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.8",
          "form": "갑자기 줄넘기욧?",
          "original_form": "갑자기 줄넘기욧?",
          "speaker_id": "1",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100039230.1.9",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100039230.1.10",
          "form": "네ㅋㅋㅋㅋㅋ아니나무에딱걸려있길래 급 이단뛰기하겠다고설치다가..",
          "original_form": "네ㅋㅋㅋㅋㅋ아니나무에딱걸려있길래 급 이단뛰기하겠다고설치다가..",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039230.1.11",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㄲㄱㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㄲㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039230.1.12",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100039230.1.13",
          "form": "한의원이낭근처병원가서",
          "original_form": "한의원이낭근처병원가서",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100039230.1.14",
          "form": "언능 물리치료받으세여ㅜㅜㅜㅜ",
          "original_form": "언능 물리치료받으세여ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039230.1.15",
          "form": "ㅋㅋㅋㅋ퇴근하고 파스붙여보죠일단...ㅋㅋㅋㄲ\r\n아너무바보짓이라헛웃음나네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ퇴근하고 파스붙여보죠일단...ㅋㅋㅋㄲ\r\n아너무바보짓이라헛웃음나네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100039230.1.16",
          "form": "저는제가병을만드는거같습니다..",
          "original_form": "저는제가병을만드는거같습니다..",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100039230.1.17",
          "form": "아ㅜㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039230.1.18",
          "form": "물리치료 혹은 한의원 가세요ㅠㅠ",
          "original_form": "물리치료 혹은 한의원 가세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039230.1.19",
          "form": "파스로 갠차늘까요ㅜㅜㅜ",
          "original_form": "파스로 갠차늘까요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039230.1.20",
          "form": "일단버티고가는타입이예요저는..ㄱㅋㅋㅋ허허... 바로병원가시는편이셔용?",
          "original_form": "일단버티고가는타입이예요저는..ㄱㅋㅋㅋ허허... 바로병원가시는편이셔용?",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039230.1.21",
          "form": "ㅋㅋㅋ저는 바로가는편",
          "original_form": "ㅋㅋㅋ저는 바로가는편",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039230.1.22",
          "form": "엄살쟁이에여",
          "original_form": "엄살쟁이에여",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039230.1.23",
          "form": "그게현명할수도ㅋㅋㅋ..\r\n약국에서파스사붙이고 불금보내고왔습니다혼자...",
          "original_form": "그게현명할수도ㅋㅋㅋ..\r\n약국에서파스사붙이고 불금보내고왔습니다혼자...",
