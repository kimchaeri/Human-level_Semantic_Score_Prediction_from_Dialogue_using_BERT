      "utterance": [
        {
          "id": "MDRW2100006646.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100006646.1.2",
          "form": "좋아하는데 코로나 때문에 너무 아쉬워요ㅠㅠ",
          "original_form": "좋아하는데 코로나 때문에 너무 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006646.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006646.1.4",
          "form": "저도 좋아하는데 최근엔 못 가봤네요ㅠ",
          "original_form": "저도 좋아하는데 최근엔 못 가봤네요ㅠ",
          "speaker_id": "1",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006646.1.5",
          "form": "기억에 남는곳 있으세요?",
          "original_form": "기억에 남는곳 있으세요?",
          "speaker_id": "1",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006646.1.6",
          "form": "저는 제주도요! 우도가 너무 예뻤어요",
          "original_form": "저는 제주도요! 우도가 너무 예뻤어요",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100006646.1.7",
          "form": "마지막 배 떠나고 사람이 빠지면 조금 무섭긴 했지만요ㅋㅋ",
          "original_form": "마지막 배 떠나고 사람이 빠지면 조금 무섭긴 했지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100006646.1.8",
          "form": "제주도 바다 예쁘죠 역시 여행은 바다인 것 같아요",
          "original_form": "제주도 바다 예쁘죠 역시 여행은 바다인 것 같아요",
          "speaker_id": "1",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100006646.1.9",
          "form": "맞아요 푸른색이 너무 예뻐요!ㅠㅠㅠ",
          "original_form": "맞아요 푸른색이 너무 예뻐요!ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100006646.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100006646.1.11",
          "form": "그리고 보기만해도 뭔가 기분이 탁 트이잖아요",
          "original_form": "그리고 보기만해도 뭔가 기분이 탁 트이잖아요",
          "speaker_id": "1",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100006646.1.12",
          "form": "제주하면 고기도 너무 맛있어요ㅠㅠ 연돈도 가보고싶었는데 아쉬워요",
          "original_form": "제주하면 고기도 너무 맛있어요ㅠㅠ 연돈도 가보고싶었는데 아쉬워요",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100006646.1.13",
          "form": "맞아요! 완전 해방감..,",
          "original_form": "맞아요! 완전 해방감..,",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100006646.1.14",
          "form": "연돈!! 저도 돈카츠 좋아해서 꼭 가보고싶은데",
          "original_form": "연돈!! 저도 돈카츠 좋아해서 꼭 가보고싶은데",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100006646.1.15",
          "form": "예약이 거의 불가능한 수준이라고 하더라고요 ㅋㅋㅋ",
          "original_form": "예약이 거의 불가능한 수준이라고 하더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100006646.1.16",
          "form": "그니까요ㅠㅠㅠ 전 손도 느려서 나아아중에나 가능할 것 같아요ㅋㅋㅠㅠ",
          "original_form": "그니까요ㅠㅠㅠ 전 손도 느려서 나아아중에나 가능할 것 같아요ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100006646.1.17",
          "form": "ㅠㅠ 언젠가 꼭 성공하시길바라요",
          "original_form": "ㅠㅠ 언젠가 꼭 성공하시길바라요",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100006646.1.18",
          "form": "해외는 안 가보셨나요?",
          "original_form": "해외는 안 가보셨나요?",
          "speaker_id": "1",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100006646.1.19",
          "form": "3년전인가? 홋카이도 다녀왔어요!",
          "original_form": "3년전인가? 홋카이도 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100006646.1.20",
          "form": "양고기가 유명하던데 너무 맛있더라구요",
          "original_form": "양고기가 유명하던데 너무 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100006646.1.21",
          "form": "오 양고기 한번도 안 먹어봐서 궁금하네요",
          "original_form": "오 양고기 한번도 안 먹어봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100006646.1.22",
          "form": "맛 있을 것 같아요",
          "original_form": "맛 있을 것 같아요",
          "speaker_id": "1",
          "time": "20210908 05:15"
        }
      ]
    }
  ]
