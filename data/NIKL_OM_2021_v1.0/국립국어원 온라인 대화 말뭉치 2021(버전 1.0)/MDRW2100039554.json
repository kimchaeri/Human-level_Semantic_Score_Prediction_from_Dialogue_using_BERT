      "utterance": [
        {
          "id": "MDRW2100039554.1.1",
          "form": "저녁머드세용",
          "original_form": "저녁머드세용",
          "speaker_id": "2",
          "time": "20210903 19:00"
        },
        {
          "id": "MDRW2100039554.1.2",
          "form": "지금 1차로 떡순튀 먹었고",
          "original_form": "지금 1차로 떡순튀 먹었고",
          "speaker_id": "1",
          "time": "20210903 19:58"
        },
        {
          "id": "MDRW2100039554.1.3",
          "form": "2차로 집에 가서 전복죽 먹을 예정ㅎㅎㅎ",
          "original_form": "2차로 집에 가서 전복죽 먹을 예정ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039554.1.4",
          "form": "이욜",
          "original_form": "이욜",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100039554.1.5",
          "form": "지금쯤은 다 드셧겟군여",
          "original_form": "지금쯤은 다 드셧겟군여",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100039554.1.6",
          "form": "참고로 저는 찜닭을 먹엇슾다",
          "original_form": "참고로 저는 찜닭을 먹엇슾다",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100039554.1.7",
          "form": "습니다",
          "original_form": "습니다",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100039554.1.8",
          "form": "예아 호로록 먹었쥬ㅎㅎ",
          "original_form": "예아 호로록 먹었쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039554.1.9",
          "form": "찜닭 is 뭔들이지만 간장인가요 고추장인가요 아님 다른거?",
          "original_form": "찜닭 is 뭔들이지만 간장인가요 고추장인가요 아님 다른거?",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039554.1.10",
          "form": "간장이엇습니당",
          "original_form": "간장이엇습니당",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039554.1.11",
          "form": "킹콩찜닭에서 시켜먹엇답니당",
          "original_form": "킹콩찜닭에서 시켜먹엇답니당",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039554.1.12",
          "form": "역시 찜닭은 간장이쥬!! 킹콩찜닭 맛있나유?",
          "original_form": "역시 찜닭은 간장이쥬!! 킹콩찜닭 맛있나유?",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039554.1.13",
          "form": "갠찬습니다 요동네에선 젤루 갠찬게 하는거같군용",
          "original_form": "갠찬습니다 요동네에선 젤루 갠찬게 하는거같군용",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039554.1.14",
          "form": "name1누님두 함 드셔보십셔",
          "original_form": "&name1&누님두 함 드셔보십셔",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039554.1.15",
          "form": "오오 담에 꼭 시켜볼게유!! 찜닭 먹을때 전 당면이 제일 맛있더라구요ㅎㅅㅎ",
          "original_form": "오오 담에 꼭 시켜볼게유!! 찜닭 먹을때 전 당면이 제일 맛있더라구요ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100039554.1.16",
          "form": "조아용 당면 마싯죵",
          "original_form": "조아용 당면 마싯죵",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039554.1.17",
          "form": "그런의미에서",
          "original_form": "그런의미에서",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039554.1.18",
          "form": "name1누님은 납작당면 둥근당면",
          "original_form": "&name1&누님은 납작당면 둥근당면",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039554.1.19",
          "form": "어떤게조으신가여",
          "original_form": "어떤게조으신가여",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100039554.1.20",
          "form": "둘 다 각자의 매력이 있어서 다 좋아해유 가족들은 둥근걸 선호해서 찜닭 먹을땐 둥근당면으로 시켜유",
          "original_form": "둘 다 각자의 매력이 있어서 다 좋아해유 가족들은 둥근걸 선호해서 찜닭 먹을땐 둥근당면으로 시켜유",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100039554.1.21",
          "form": "그렇군용",
          "original_form": "그렇군용",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100039554.1.22",
          "form": "저는 둥근당면이 더 좋아용",
          "original_form": "저는 둥근당면이 더 좋아용",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100039554.1.23",
          "form": "근데 둥근당면은 얇아서그런가",
          "original_form": "근데 둥근당면은 얇아서그런가",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100039554.1.24",
          "form": "불어서오면 슬퍼져요",
          "original_form": "불어서오면 슬퍼져요",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100039554.1.25",
          "form": "하...저두요...당면추가 했을때 국물 적어짐+불어서 옴 이 콤보는 참을 수가 없어유..",
          "original_form": "하...저두요...당면추가 했을때 국물 적어짐+불어서 옴 이 콤보는 참을 수가 없어유..",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100039554.1.26",
          "form": "ㅠㅠㅜ그래서 멀리서시키면이제...",
          "original_form": "ㅠㅠㅜ그래서 멀리서시키면이제...",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100039554.1.27",
          "form": "아쉬워도 납작당면을...",
          "original_form": "아쉬워도 납작당면을...",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100039554.1.28",
          "form": "가서먹으면 둥근당면을...",
          "original_form": "가서먹으면 둥근당면을...",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100039554.1.29",
          "form": "근데 요샌 납작당면하는데가 더 만은거같아영",
          "original_form": "근데 요샌 납작당면하는데가 더 만은거같아영",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100039554.1.30",
          "form": "맞아유 대부분 넙적이더라구여",
          "original_form": "맞아유 대부분 넙적이더라구여",
          "speaker_id": "1",
          "time": "20210904 01:19"
        },
        {
          "id": "MDRW2100039554.1.31",
          "form": "그래서 둥근당면으로 바꿀 수 있으면 바꿔서 주문합니다ㅎㅅㅎ",
          "original_form": "그래서 둥근당면으로 바꿀 수 있으면 바꿔서 주문합니다ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210904 01:19"
        }
      ]
    }
