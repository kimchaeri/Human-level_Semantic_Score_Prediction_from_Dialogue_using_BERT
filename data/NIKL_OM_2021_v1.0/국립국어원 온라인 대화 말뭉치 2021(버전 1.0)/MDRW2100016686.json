      "utterance": [
        {
          "id": "MDRW2100016686.1.1",
          "form": "최근에 구매하신거있나여?",
          "original_form": "최근에 구매하신거있나여?",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016686.1.2",
          "form": "최근에는 없네요",
          "original_form": "최근에는 없네요",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016686.1.3",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016686.1.4",
          "form": "저는 요즘 케이스티파이 디즈니 콜라보 케이스를 살까말까 고민중이에요",
          "original_form": "저는 요즘 케이스티파이 디즈니 콜라보 케이스를 살까말까 고민중이에요",
          "speaker_id": "1",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016686.1.5",
          "form": "어떤 캐릭터 케이스인가요?",
          "original_form": "어떤 캐릭터 케이스인가요?",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016686.1.6",
          "form": "인어공주에 나오는 노란색 물고기 아세요?",
          "original_form": "인어공주에 나오는 노란색 물고기 아세요?",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016686.1.7",
          "form": "기억이 날 듯 해요 걔 이름이 뭔지 아시나요??",
          "original_form": "기억이 날 듯 해요 걔 이름이 뭔지 아시나요??",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016686.1.8",
          "form": "전 캐스펀 줄 알았는데 되게 어려운 이름이더라구요",
          "original_form": "전 캐스펀 줄 알았는데 되게 어려운 이름이더라구요",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016686.1.9",
          "form": "가격은 얼마정도 하나요?",
          "original_form": "가격은 얼마정도 하나요?",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016686.1.10",
          "form": "55000원이요",
          "original_form": "55000원이요",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016686.1.11",
          "form": "케이스주제에 사악해서  고민중입니다",
          "original_form": "케이스주제에 사악해서  고민중입니다",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016686.1.12",
          "form": "고민하실만 하네요",
          "original_form": "고민하실만 하네요",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016686.1.13",
          "form": "케이스가 5만원이 넘다니",
          "original_form": "케이스가 5만원이 넘다니",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016686.1.14",
          "form": "그쵸 언젠가 질릴거같아서ㅜㅜ",
          "original_form": "그쵸 언젠가 질릴거같아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016686.1.15",
          "form": "맞아요 그리고 핸드폰이 바뀌면 케이스는 못쓰니까 더더욱 그렀죠",
          "original_form": "맞아요 그리고 핸드폰이 바뀌면 케이스는 못쓰니까 더더욱 그렀죠",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016686.1.16",
          "form": "결국 돌고돌아 투명으로 돌아가기도하고요ㄱㄲㅋ",
