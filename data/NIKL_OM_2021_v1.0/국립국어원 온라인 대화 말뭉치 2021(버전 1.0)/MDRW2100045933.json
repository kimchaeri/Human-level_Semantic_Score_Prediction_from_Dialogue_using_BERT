      "utterance": [
        {
          "id": "MDRW2100045933.1.1",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045933.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045933.1.3",
          "form": "잘못먹어요",
          "original_form": "잘못먹어요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045933.1.4",
          "form": "잘드세요?",
          "original_form": "잘드세요?",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045933.1.5",
          "form": "저도 못먹어요 ㅋㅋ가끔 매운게 땡기긴 하는데 ㅠㅜ",
          "original_form": "저도 못먹어요 ㅋㅋ가끔 매운게 땡기긴 하는데 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045933.1.6",
          "form": "땡기기도하세요?",
          "original_form": "땡기기도하세요?",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045933.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045933.1.8",
          "form": "네네 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045933.1.9",
          "form": "저도 한번씩 떡볶이가 땡겨요",
          "original_form": "저도 한번씩 떡볶이가 땡겨요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045933.1.10",
          "form": "근데 아직 짬뽕을 제메뉴로 시켜본적이 없어요",
          "original_form": "근데 아직 짬뽕을 제메뉴로 시켜본적이 없어요",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045933.1.11",
          "form": "매워서 못먹ㅡㄹ것같아서요",
          "original_form": "매워서 못먹ㅡㄹ것같아서요",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045933.1.12",
          "form": "오 짬뽕도 매우시구나 ㅠㅠㅠ",
          "original_form": "오 짬뽕도 매우시구나 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045933.1.13",
          "form": "단거는 좋아하시죠??",
          "original_form": "단거는 좋아하시죠??",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045933.1.14",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045933.1.15",
          "form": "매운거 못먹는대신 ㅋ",
          "original_form": "매운거 못먹는대신 ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045933.1.16",
          "form": "오 어떤거 즐겨드세요",
          "original_form": "오 어떤거 즐겨드세요",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045933.1.17",
          "form": "빵종류요 ㅎ",
          "original_form": "빵종류요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045933.1.18",
          "form": "오오 빵 어떤빵 좋아라세요",
          "original_form": "오오 빵 어떤빵 좋아라세요",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045933.1.19",
          "form": "저는 소보로 좋아해요",
          "original_form": "저는 소보로 좋아해요",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045933.1.20",
          "form": "성심당 튀소 드셔보셨어요?",
          "original_form": "성심당 튀소 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045933.1.21",
          "form": "오오 소보로",
          "original_form": "오오 소보로",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045933.1.22",
          "form": "들어본적은 잇어요!!",
          "original_form": "들어본적은 잇어요!!",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045933.1.23",
          "form": "거기도 맛나고 ㅎㅎ",
          "original_form": "거기도 맛나고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045933.1.24",
          "form": "요즘은 파바에 롤빵도 맛있더라구요",
          "original_form": "요즘은 파바에 롤빵도 맛있더라구요",
