      "utterance": [
        {
          "id": "MDRW2100020912.1.1",
          "form": "난 커피타임ㅋㅋ",
          "original_form": "난 커피타임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020912.1.2",
          "form": "애들은 뭐하노?",
          "original_form": "애들은 뭐하노?",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020912.1.3",
          "form": "첫째 두발자전거 연습시키려고 나가려구요ㅋ",
          "original_form": "첫째 두발자전거 연습시키려고 나가려구요ㅋ",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100020912.1.4",
          "form": "언니 커피타임좋네요ㅋ",
          "original_form": "언니 커피타임좋네요ㅋ",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100020912.1.5",
          "form": "나 커피타임끝ㅋ",
          "original_form": "나 커피타임끝ㅋ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100020912.1.6",
          "form": "이제 집안일하러가보까?ㅋㅋ",
          "original_form": "이제 집안일하러가보까?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100020912.1.7",
          "form": "건조기에서 빨래꺼내야되는듸ㅣㅈ귀찮",
          "original_form": "건조기에서 빨래꺼내야되는듸ㅣㅈ귀찮",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100020912.1.8",
          "form": "언니 저두요.ㅋㅋㅋ",
          "original_form": "언니 저두요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.9",
          "form": "첫째 자전거태우고 오늘할일은 일단 끝났네유",
          "original_form": "첫째 자전거태우고 오늘할일은 일단 끝났네유",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.10",
          "form": "같은마음이군",
          "original_form": "같은마음이군",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.11",
          "form": "저녁은 시댁넘어가서 먹으려구요ㅋ",
          "original_form": "저녁은 시댁넘어가서 먹으려구요ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.12",
          "form": "나도 지금애들은 게임중",
          "original_form": "나도 지금애들은 게임중",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.13",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.14",
          "form": "좋다좋다",
          "original_form": "좋다좋다",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100020912.1.15",
          "form": "네네ㅋㅋ 오늘 저녁도 이렇게 해결ㅋ",
          "original_form": "네네ㅋㅋ 오늘 저녁도 이렇게 해결ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100020912.1.16",
          "form": "아주 좋구만요",
          "original_form": "아주 좋구만요",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100020912.1.17",
          "form": "아름답네ㅎㅎ",
          "original_form": "아름답네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100020912.1.18",
          "form": "난도 오늘 치킨ㅋㅋㅋ",
          "original_form": "난도 오늘 치킨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100020912.1.19",
          "form": "ㅋㅋ 울신랑 안그래도 치킨먹고싶다했는데",
          "original_form": "ㅋㅋ 울신랑 안그래도 치킨먹고싶다했는데",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100020912.1.20",
          "form": "피자나라 치킨공주먹고싶대요",
          "original_form": "피자나라 치킨공주먹고싶대요",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100020912.1.21",
          "form": "그래서 집에있는 냉동피자랑 냉동치킨해줬더니 ㅋㅋ",
          "original_form": "그래서 집에있는 냉동피자랑 냉동치킨해줬더니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100020912.1.22",
          "form": "다음부터는 좀 시키자고",
          "original_form": "다음부터는 좀 시키자고",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100020912.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100020912.1.24",
          "form": "냉파 좋은데? 냉장고에 그게 있는게신기하노ㅋㅋ",
          "original_form": "냉파 좋은데? 냉장고에 그게 있는게신기하노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100020912.1.25",
          "form": "교촌먹자는데ㅡㅡ양이넘적다",
          "original_form": "교촌먹자는데ㅡㅡ양이넘적다",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100020912.1.26",
          "form": "우리가족 간식도안될양",
          "original_form": "우리가족 간식도안될양",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100020912.1.27",
          "form": "세일할때 사놨거든요ㅋㅋ",
          "original_form": "세일할때 사놨거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100020912.1.28",
          "form": "교촌적어요ㅜ",
          "original_form": "교촌적어요ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100020912.1.29",
          "form": "한마리로안됨ㅜ",
          "original_form": "한마리로안됨ㅜ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100020912.1.30",
          "form": "그자? 아ㅜ 재활용 쓰레기도 버려야되너ㆍㅡㅡ",
          "original_form": "그자? 아ㅜ 재활용 쓰레기도 버려야되너ㆍㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100020912.1.31",
          "form": "일많구만",
          "original_form": "일많구만",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100020912.1.32",
