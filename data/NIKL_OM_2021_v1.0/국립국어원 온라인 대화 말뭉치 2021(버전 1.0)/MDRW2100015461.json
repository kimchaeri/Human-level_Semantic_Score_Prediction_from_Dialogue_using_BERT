      "utterance": [
        {
          "id": "MDRW2100015461.1.1",
          "form": "배달음식 뭐 좋아하세요?",
          "original_form": "배달음식 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015461.1.2",
          "form": "치킨 족발요 ㅋㅋ",
          "original_form": "치킨 족발요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015461.1.3",
          "form": "뭐 좋아하세요",
          "original_form": "뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015461.1.4",
          "form": "치킨 족발도 좋아하고 피자도좋아해요",
          "original_form": "치킨 족발도 좋아하고 피자도좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015461.1.5",
          "form": "사실 웬만한 배달음식 다좋아합니다",
          "original_form": "사실 웬만한 배달음식 다좋아합니다",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015461.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015461.1.7",
          "form": "요새 모듬전 팠어요",
          "original_form": "요새 모듬전 팠어요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015461.1.8",
          "form": "와 종류도 많고 양이 많아서 반했어요",
          "original_form": "와 종류도 많고 양이 많아서 반했어요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015461.1.9",
          "form": "모듬전...!!",
          "original_form": "모듬전...!!",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015461.1.10",
          "form": "전 너무맛있죠 ㅠㅠ",
          "original_form": "전 너무맛있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015461.1.11",
          "form": "네 와 전 너무 좋아해요 ㅋㅋ",
          "original_form": "네 와 전 너무 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015461.1.12",
          "form": "어떤 전 제일 좋아하세요?",
          "original_form": "어떤 전 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015461.1.13",
          "form": "근데양도 가격도 좋아요",
          "original_form": "근데양도 가격도 좋아요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015461.1.14",
          "form": "전 감자전을 제일좋아하지만",
          "original_form": "전 감자전을 제일좋아하지만",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015461.1.15",
          "form": "김치전 깻잎전 해물파전도 너무좋아합니다",
          "original_form": "김치전 깻잎전 해물파전도 너무좋아합니다",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015461.1.16",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015461.1.17",
          "form": "감다전 싹다 갈아서요",
          "original_form": "감다전 싹다 갈아서요",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015461.1.18",
          "form": "감자전",
          "original_form": "감자전",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015461.1.19",
          "form": "넘맛나요 쫄깃하고",
          "original_form": "넘맛나요 쫄깃하고",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015461.1.20",
          "form": "감자전 진짜 너무 맛있음 ㅠㅠ",
          "original_form": "감자전 진짜 너무 맛있음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015461.1.21",
          "form": "새송이버섯 자잘하게 들어간것도 너무 맛잇어요 ㅠㅠ",
          "original_form": "새송이버섯 자잘하게 들어간것도 너무 맛잇어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015461.1.22",
          "form": "맞아요 헉",
          "original_form": "맞아요 헉",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015461.1.23",
          "form": "어쩌죠 먹고싶네요 ㅋㅋ",
          "original_form": "어쩌죠 먹고싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015461.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 침흘리는중....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 침흘리는중....",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015461.1.25",
          "form": "전은 엄마가 명절에 부치면",
          "original_form": "전은 엄마가 명절에 부치면",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015461.1.26",
          "form": "못먹게 하잖아요",
          "original_form": "못먹게 하잖아요",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015461.1.27",
          "form": "몰래 날름날름 먹어야죠",
          "original_form": "몰래 날름날름 먹어야죠",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.28",
          "form": "그때 막 집어 먹을때 뜨거운거",
          "original_form": "그때 막 집어 먹을때 뜨거운거",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.29",
          "form": "맛나요 ㅋㅋ",
          "original_form": "맛나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.30",
          "form": "등짝을 내어주고",
          "original_form": "등짝을 내어주고",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.31",
          "form": "어린시절 그립네요",
          "original_form": "어린시절 그립네요",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.32",
          "form": "구박받으면서 먹는 그맛",
          "original_form": "구박받으면서 먹는 그맛",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.33",
          "form": "부치지도 않으면서 그만먹어",
          "original_form": "부치지도 않으면서 그만먹어",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.34",
          "form": "동태전도 먹고싶네요",
          "original_form": "동태전도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015461.1.35",
          "form": "나쁜말이래요 기계가 ㅋㅋ",
          "original_form": "나쁜말이래요 기계가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.36",
          "form": "상에 올릴 음식 없다고 먹지말라고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "상에 올릴 음식 없다고 먹지말라고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.38",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.39",
          "form": "근데 상에 올릴거 남던데 ㅋㅋ",
          "original_form": "근데 상에 올릴거 남던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.40",
          "form": "꼭 그러세요 ㅎㅎ",
          "original_form": "꼭 그러세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.41",
          "form": "조만간 전 먹어야겟어요 안되겟어요",
          "original_form": "조만간 전 먹어야겟어요 안되겟어요",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015461.1.42",
          "form": "아 돌아가고싶네요",
          "original_form": "아 돌아가고싶네요",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015461.1.43",
          "form": "그러세요 ㅎㅎ",
          "original_form": "그러세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015461.1.44",
          "form": "너무먹고싶네요 ㅋㅋㅋㅋㅋ",
          "original_form": "너무먹고싶네요 ㅋㅋㅋㅋㅋ",
