      "utterance": [
        {
          "id": "MDRW2100016607.1.1",
          "form": "오늘 점심 식사 메뉴는 뭐였어요?",
          "original_form": "오늘 점심 식사 메뉴는 뭐였어요?",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016607.1.2",
          "form": "저 오늘 떡볶이 먹었어요 ㅎㅎ",
          "original_form": "저 오늘 떡볶이 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016607.1.3",
          "form": "점심 뭐드셨나요???",
          "original_form": "점심 뭐드셨나요???",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016607.1.4",
          "form": "저는 소고기 국 먹었어요.",
          "original_form": "저는 소고기 국 먹었어요.",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016607.1.5",
          "form": "좋아하는 배달 음식 있으세요?",
          "original_form": "좋아하는 배달 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016607.1.6",
          "form": "저는 치킨 종류 제일 좋아해요 !",
          "original_form": "저는 치킨 종류 제일 좋아해요 !",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016607.1.7",
          "form": "치킨 좋아하시나요?",
          "original_form": "치킨 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016607.1.8",
          "form": "네 치킨 좋아해요 특히 굽네치킨이요.",
          "original_form": "네 치킨 좋아해요 특히 굽네치킨이요.",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016607.1.9",
          "form": "저도 굽네 제일 좋아해요 !",
          "original_form": "저도 굽네 제일 좋아해요 !",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016607.1.10",
          "form": "정말요? 굽네 치킨이 기름이 별로 없어어 좋아해요.",
          "original_form": "정말요? 굽네 치킨이 기름이 별로 없어어 좋아해요.",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016607.1.11",
          "form": "어떤 카페 즐겨가세요?",
          "original_form": "어떤 카페 즐겨가세요?",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016607.1.12",
          "form": "저는 보통 메가커피나 컴포즈 가는거 같아요 ㅎㅎ",
          "original_form": "저는 보통 메가커피나 컴포즈 가는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016607.1.13",
          "form": "카페 좋아하시나요?",
          "original_form": "카페 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016607.1.14",
          "form": "저는 잘 가진 않아요. 요즘 음료가 너무 달아서..",
          "original_form": "저는 잘 가진 않아요. 요즘 음료가 너무 달아서..",
          "speaker_id": "1",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016607.1.15",
          "form": "아 저도 단거 안좋아해서 항상 카페라떼 마셔요 ㅎㅎ",
          "original_form": "아 저도 단거 안좋아해서 항상 카페라떼 마셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100016607.1.16",
          "form": "그 시럽을 싫어해서 ㅠㅠ",
          "original_form": "그 시럽을 싫어해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100016607.1.17",
          "form": "공교롭게 카페인도 안 받아서.. 요즘 세상에 참 슬프네요. ㅎㅎㅎ",
          "original_form": "공교롭게 카페인도 안 받아서.. 요즘 세상에 참 슬프네요. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016607.1.18",
          "form": "요즘 카페인 안받으시는 분들 많더라구요 ㅠㅠ",
          "original_form": "요즘 카페인 안받으시는 분들 많더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016607.1.19",
          "form": "그래서 디카페인 많이 드시던데 !",
          "original_form": "그래서 디카페인 많이 드시던데 !",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016607.1.20",
          "form": "맞아요. 그리고 차 종류도 속이 안정되고 좋아요.",
          "original_form": "맞아요. 그리고 차 종류도 속이 안정되고 좋아요.",
          "speaker_id": "1",
          "time": "20210826 14:15"
        }
      ]
