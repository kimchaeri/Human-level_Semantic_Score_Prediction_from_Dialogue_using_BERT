      "utterance": [
        {
          "id": "MDRW2100013911.1.1",
          "form": "너의모든순간 좋아하세요? ㅋㅋ",
          "original_form": "너의모든순간 좋아하세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013911.1.2",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013911.1.3",
          "form": "성시경 노래 중에 제일 좋아함 ㅠㅠㅠㅠ",
          "original_form": "성시경 노래 중에 제일 좋아함 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013911.1.4",
          "form": "ㅋㅋ노래 참 좋죠",
          "original_form": "ㅋㅋ노래 참 좋죠",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013911.1.5",
          "form": "또 좋아하는 곡 있으세요?",
          "original_form": "또 좋아하는 곡 있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013911.1.6",
          "form": "성시경 노래 중에서요?",
          "original_form": "성시경 노래 중에서요?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013911.1.7",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013911.1.8",
          "form": "그냥 name2이\r\n즐겨듣는 노래요:)",
          "original_form": "그냥 &name2&이\r\n즐겨듣는 노래요:)",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013911.1.9",
          "form": "아 요즘은 이무진 신호등이랑",
          "original_form": "아 요즘은 이무진 신호등이랑",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013911.1.10",
          "form": "그 빌보드 1위하는 거 저스틴비버 노래 들어용 홍홍",
          "original_form": "그 빌보드 1위하는 거 저스틴비버 노래 들어용 홍홍",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013911.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013911.1.12",
          "form": "저는 이무진님이 부른\r\n슬의생 ost요ㅋㅋ",
          "original_form": "저는 이무진님이 부른\r\n슬의생 ost요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013911.1.13",
          "form": "비와당신",
          "original_form": "비와당신",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013911.1.14",
          "form": "아 그것도 불렀어요?",
          "original_form": "아 그것도 불렀어요?",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013911.1.15",
          "form": "좋은가요?",
          "original_form": "좋은가요?",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013911.1.16",
          "form": "네:) 원래도 좋아했던 곡이고\r\n다른 가수들도 리메이크 했었는데\r\n이무진님이 부른게 젤 좋더라구요",
          "original_form": "네:) 원래도 좋아했던 곡이고\r\n다른 가수들도 리메이크 했었는데\r\n이무진님이 부른게 젤 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013911.1.17",
          "form": "들어봐야겠네요",
          "original_form": "들어봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013911.1.18",
          "form": "최신곡은 안 들으시나요?",
          "original_form": "최신곡은 안 들으시나요?",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013911.1.19",
          "form": "ㅋㅋ네ㅠ 요즘 노래 잘 몰라요",
          "original_form": "ㅋㅋ네ㅠ 요즘 노래 잘 몰라요",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013911.1.20",
          "form": "멜론 탑백 돌리세용",
          "original_form": "멜론 탑백 돌리세용",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013911.1.21",
          "form": "요즘 갬성이랑 안맞아서요ㅋㅋ",
          "original_form": "요즘 갬성이랑 안맞아서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013911.1.22",
          "form": "넷플 보시나요?",
