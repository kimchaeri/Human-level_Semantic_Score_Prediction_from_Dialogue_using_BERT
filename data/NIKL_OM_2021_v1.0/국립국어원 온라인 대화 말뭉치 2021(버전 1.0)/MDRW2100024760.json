      "utterance": [
        {
          "id": "MDRW2100024760.1.1",
          "form": "좋아하는 연예인 있으신가요?",
          "original_form": "좋아하는 연예인 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024760.1.2",
          "form": "엄기준이요ㅎㅎ",
          "original_form": "엄기준이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024760.1.3",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024760.1.4",
          "form": "학생때 사진보니까 아이돌이 던데요?",
          "original_form": "학생때 사진보니까 아이돌이 던데요?",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024760.1.5",
          "form": "너무 예쁘게  생겼어서ㅎㅎ지금도  잘생기셨지만ㅎ",
          "original_form": "너무 예쁘게  생겼어서ㅎㅎ지금도  잘생기셨지만ㅎ",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024760.1.6",
          "form": "지금도 잘생겼죠",
          "original_form": "지금도 잘생겼죠",
          "speaker_id": "2",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024760.1.7",
          "form": "전 티비보단 무대 모습을 더 좋아하는 편이에요",
          "original_form": "전 티비보단 무대 모습을 더 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024760.1.8",
          "form": "뮤지컬 좋아하시나봐요~!",
          "original_form": "뮤지컬 좋아하시나봐요~!",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024760.1.9",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024760.1.10",
          "form": "사실 뮤배치곤 노래가 살짝 아쉬우신데요...",
          "original_form": "사실 뮤배치곤 노래가 살짝 아쉬우신데요...",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024760.1.11",
          "form": "연기가 참 좋아요",
          "original_form": "연기가 참 좋아요",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024760.1.12",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024760.1.13",
          "form": "그래도 연기자가 그정도면 훌륭하죠",
          "original_form": "그래도 연기자가 그정도면 훌륭하죠",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024760.1.14",
          "form": "전 직관하지 못해서 아쉽네요ㅜ",
          "original_form": "전 직관하지 못해서 아쉽네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024760.1.15",
          "form": "무슨 공연 보셨어요?",
          "original_form": "무슨 공연 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024760.1.16",
          "form": "음... 처음 반한건 잭더리퍼 였구요",
          "original_form": "음... 처음 반한건 잭더리퍼 였구요",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024760.1.17",
          "form": "베르테르랑 삼총사도 좋아해요",
          "original_form": "베르테르랑 삼총사도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024760.1.18",
          "form": "뮤지컬은 정말 노래가 좋더라구요",
          "original_form": "뮤지컬은 정말 노래가 좋더라구요",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024760.1.19",
          "form": "가사도 쉽고ㅎㅎ",
          "original_form": "가사도 쉽고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024760.1.20",
          "form": "좋아하는 곡 있으신가요?",
          "original_form": "좋아하는 곡 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024760.1.21",
          "form": "뮤지컬 노래 중에선 드라큘라에서 나온 곡들을 좋아해요",
          "original_form": "뮤지컬 노래 중에선 드라큘라에서 나온 곡들을 좋아해요",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024760.1.22",
          "form": "가사 번역을 참 예쁘게 해서요",
          "original_form": "가사 번역을 참 예쁘게 해서요",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024760.1.23",
          "form": "번역이 정말 중요하더라구요",
          "original_form": "번역이 정말 중요하더라구요",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024760.1.24",
          "form": "빨래노래도 참 좋고ㅎㅎ",
          "original_form": "빨래노래도 참 좋고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024760.1.25",
          "form": "빨래 노래도 참 좋죠",
          "original_form": "빨래 노래도 참 좋죠",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024760.1.26",
          "form": "홍광호 빨래 참 재밌는 경험이었어요",
          "original_form": "홍광호 빨래 참 재밌는 경험이었어요",
          "speaker_id": "2",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100024760.1.27",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024760.1.28",
          "form": "참예뻐요 노래는 정말 예뻐요ㅎ",
          "original_form": "참예뻐요 노래는 정말 예뻐요ㅎ",
          "speaker_id": "1",
          "time": "20210831 18:26"
        }
      ]
    }
