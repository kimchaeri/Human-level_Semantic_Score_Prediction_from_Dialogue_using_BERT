        },
        {
          "id": "MDRW2100012669.1.2",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100012669.1.3",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100012669.1.4",
          "form": "요리 담당은 누군가요",
          "original_form": "요리 담당은 누군가요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100012669.1.5",
          "form": "와이프여",
          "original_form": "와이프여",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100012669.1.6",
          "form": "아하 남성분이셨구나",
          "original_form": "아하 남성분이셨구나",
          "speaker_id": "2",
