      "utterance": [
        {
          "id": "MDRW2100002487.1.1",
          "form": "내꺼? 옷 많은데 뭘",
          "original_form": "내꺼? 옷 많은데 뭘",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002487.1.2",
          "form": "포인트 많이 모여서 공짜로 한 벌 살 수 있을 득",
          "original_form": "포인트 많이 모여서 공짜로 한 벌 살 수 있을 득",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002487.1.3",
          "form": "오오 열심히 모았나보네",
          "original_form": "오오 열심히 모았나보네",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002487.1.4",
          "form": "할인쿠폰에 포인트까지 쓰면 거의 공짜",
          "original_form": "할인쿠폰에 포인트까지 쓰면 거의 공짜",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002487.1.5",
          "form": "싸구려 아니야? ㅎㅎ",
          "original_form": "싸구려 아니야? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002487.1.6",
          "form": "노노 메이커만 파는 곳이야",
          "original_form": "노노 메이커만 파는 곳이야",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002487.1.7",
          "form": "오 울 색시 알뜰하네",
          "original_form": "오 울 색시 알뜰하네",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002487.1.8",
          "form": "시원해지니 미리미리 티셔츠 구매하면 좋지",
          "original_form": "시원해지니 미리미리 티셔츠 구매하면 좋지",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002487.1.9",
          "form": "훌륭해요",
          "original_form": "훌륭해요",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002487.1.10",
          "form": "ㅎㅎ 난 훌륭한 여자",
          "original_form": "ㅎㅎ 난 훌륭한 여자",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002487.1.11",
          "form": "어떤 색으로 살까?",
          "original_form": "어떤 색으로 살까?",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002487.1.12",
          "form": "난 흰색이나 검정",
          "original_form": "난 흰색이나 검정",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002487.1.13",
          "form": "깔끔한 색이 좋아",
          "original_form": "깔끔한 색이 좋아",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002487.1.14",
          "form": "웅 알았어~ 사이즈 있나 보고",
          "original_form": "웅 알았어~ 사이즈 있나 보고",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
