      "utterance": [
        {
          "id": "MDRW2100002992.1.1",
          "form": "노브랜드꺼 말하는거니?",
          "original_form": "노브랜드꺼 말하는거니?",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002992.1.2",
          "form": "아 내 말은 방탄 버터쿠키였어",
          "original_form": "아 내 말은 방탄 버터쿠키였어",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002992.1.3",
          "form": "아까 배너광고로 bts 버터쿠키를 봤는데 조금 궁금하더라",
          "original_form": "아까 배너광고로 bts 버터쿠키를 봤는데 조금 궁금하더라",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002992.1.4",
          "form": "아하 들어만봤어",
          "original_form": "아하 들어만봤어",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002992.1.5",
          "form": "먹어볼 생각 있니?",
          "original_form": "먹어볼 생각 있니?",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002992.1.6",
          "form": "버터함유량이 높다던데.",
          "original_form": "버터함유량이 높다던데.",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.7",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.8",
          "form": "그래도 너무 비싼 거 같아",
          "original_form": "그래도 너무 비싼 거 같아",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.9",
          "form": "맛있다고 소문난건 다 먹어보고싶어",
          "original_form": "맛있다고 소문난건 다 먹어보고싶어",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.10",
          "form": "바이럴일까",
          "original_form": "바이럴일까",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.11",
          "form": "나도그래",
          "original_form": "나도그래",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.12",
          "form": "음식을 좋아하는구나",
          "original_form": "음식을 좋아하는구나",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.13",
          "form": "가격이 비싸?",
          "original_form": "가격이 비싸?",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.14",
          "form": "가격까지는 정확하게 몰라",
          "original_form": "가격까지는 정확하게 몰라",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.15",
          "form": "비싼 거 같더라.",
          "original_form": "비싼 거 같더라.",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.16",
          "form": "만원 넘는 고 같아",
          "original_form": "만원 넘는 고 같아",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002992.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ만원이면 비싸지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ만원이면 비싸지",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
