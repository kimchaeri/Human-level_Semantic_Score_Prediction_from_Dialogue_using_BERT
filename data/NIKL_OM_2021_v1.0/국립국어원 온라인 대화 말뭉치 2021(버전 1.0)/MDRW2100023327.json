      "utterance": [
        {
          "id": "MDRW2100023327.1.1",
          "form": "맥주 좋아하시나요?",
          "original_form": "맥주 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 00:08"
        },
        {
          "id": "MDRW2100023327.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100023327.1.3",
          "form": "어떤거 주로 드세요?",
          "original_form": "어떤거 주로 드세요?",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100023327.1.4",
          "form": "요즘은 그냥 생맥이요",
          "original_form": "요즘은 그냥 생맥이요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100023327.1.5",
          "form": "생맥이 제일 깔끔하고 시원하긴하죠ㅎ",
          "original_form": "생맥이 제일 깔끔하고 시원하긴하죠ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100023327.1.6",
          "form": "맞아요 그맛에 먹죠ㅎㅎ",
          "original_form": "맞아요 그맛에 먹죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100023327.1.7",
          "form": "전 과일향도 좋더라고요",
          "original_form": "전 과일향도 좋더라고요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100023327.1.8",
          "form": "써머스비같은?",
          "original_form": "써머스비같은?",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100023327.1.9",
          "form": "네 자몽도 좋고요ㅎㅎ호가든",
          "original_form": "네 자몽도 좋고요ㅎㅎ호가든",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100023327.1.10",
          "form": "호가든맥주는 맛별로 호불호가 나뉘더라고요",
          "original_form": "호가든맥주는 맛별로 호불호가 나뉘더라고요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100023327.1.11",
          "form": "호가든맥주 어떤거 드셔보셨나용",
          "original_form": "호가든맥주 어떤거 드셔보셨나용",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100023327.1.12",
          "form": "저 로제랑 무슨 허브향나는거요",
          "original_form": "저 로제랑 무슨 허브향나는거요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100023327.1.13",
          "form": "아 저도ㅎㅎ로제먹고 토한적있어서ㅠㅋ",
          "original_form": "아 저도ㅎㅎ로제먹고 토한적있어서ㅠㅋ",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100023327.1.14",
          "form": "에고 조심해요",
          "original_form": "에고 조심해요",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100023327.1.15",
          "form": "넹ㅎㅎ",
          "original_form": "넹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100023327.1.16",
          "form": "그럼 생맥은 어디서 드세요",
          "original_form": "그럼 생맥은 어디서 드세요",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100023327.1.17",
          "form": "할맥자주가요 거기 생맥주가 시원해서요",
          "original_form": "할맥자주가요 거기 생맥주가 시원해서요",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100023327.1.18",
          "form": "크 맛있겠네요",
          "original_form": "크 맛있겠네요",
          "speaker_id": "1",
          "time": "20210904 23:50"
        }
      ]
