      "utterance": [
        {
          "id": "MDRW2100000774.1.1",
          "form": "안녕하세요 여행좋아하세요?",
          "original_form": "안녕하세요 여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000774.1.2",
          "form": "안녕하세요 여행 좋아해요!!",
          "original_form": "안녕하세요 여행 좋아해요!!",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000774.1.3",
          "form": "국내 해외 다 좋아해요",
          "original_form": "국내 해외 다 좋아해요",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000774.1.4",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000774.1.5",
          "form": "네 저도 국내해외 다좋아해요!",
          "original_form": "네 저도 국내해외 다좋아해요!",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000774.1.6",
          "form": "최근에 다녀오신 여행지 잇으세요?",
          "original_form": "최근에 다녀오신 여행지 잇으세요?",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000774.1.7",
          "form": "최근에 부산이랑 청도 다녀왔어요!",
          "original_form": "최근에 부산이랑 청도 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000774.1.8",
          "form": "부산은 자주 가는 거 같은데, 청도는 처음가봤는데 좋더라고요",
          "original_form": "부산은 자주 가는 거 같은데, 청도는 처음가봤는데 좋더라고요",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000774.1.9",
          "form": "최근에 어디 다녀오셨어요??",
          "original_form": "최근에 어디 다녀오셨어요??",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000774.1.10",
          "form": "저는 밀양에 당일치기로 다녀왓어요",
          "original_form": "저는 밀양에 당일치기로 다녀왓어요",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000774.1.11",
          "form": "밀양 되게 좋더라구요",
          "original_form": "밀양 되게 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000774.1.12",
          "form": "오 밀양도 한번도 안가봤어요",
          "original_form": "오 밀양도 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000774.1.13",
          "form": "다음에 여행 계획 짤 때 밀양으로 찾아봐야겠어요",
          "original_form": "다음에 여행 계획 짤 때 밀양으로 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000774.1.14",
          "form": "전 청도 한번도 안가봤는데 어떤가요?",
          "original_form": "전 청도 한번도 안가봤는데 어떤가요?",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000774.1.15",
          "form": "자차없으면 불편하긴해요ㅜ",
          "original_form": "자차없으면 불편하긴해요ㅜ",
          "speaker_id": "2",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000774.1.16",
          "form": "근데 코로나시국에는 꽤 괜찮은 거 같아요",
          "original_form": "근데 코로나시국에는 꽤 괜찮은 거 같아요",
          "speaker_id": "2",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000774.1.17",
          "form": "카페 빼면 사람도 안북적거려서 좋아요!",
          "original_form": "카페 빼면 사람도 안북적거려서 좋아요!",
          "speaker_id": "2",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000774.1.18",
          "form": "name1님이 제일 좋아하시는 국내여행지는 어디예요?",
          "original_form": "&name1&님이 제일 좋아하시는 국내여행지는 어디예요?",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000774.1.19",
          "form": "속초랑 강릉이요",
          "original_form": "속초랑 강릉이요",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000774.1.20",
          "form": "넘 좋앗어요..ㅜㅜ 맛집도 많고 풍경도 좋고",
          "original_form": "넘 좋앗어요..ㅜㅜ 맛집도 많고 풍경도 좋고",
          "speaker_id": "1",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000774.1.21",
          "form": "청도 담에 시간내서 가봐야겟어요",
          "original_form": "청도 담에 시간내서 가봐야겟어요",
          "speaker_id": "1",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000774.1.22",
          "form": "헐 저도 강릉 가보고 싶은데 집에서 멀어서 엄두가 안나요...",
          "original_form": "헐 저도 강릉 가보고 싶은데 집에서 멀어서 엄두가 안나요...",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000774.1.23",
          "form": "저는 코로나 끝나고 시간되면 강릉 가보려고요",
          "original_form": "저는 코로나 끝나고 시간되면 강릉 가보려고요",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000774.1.24",
          "form": "예쁜 호텔이나 펜션 진짜 많던데 넘 좋아보여요..",
          "original_form": "예쁜 호텔이나 펜션 진짜 많던데 넘 좋아보여요..",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000774.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100000774.1.26",
          "form": "저는 코로나 끝나면 바로 해외갈려구요",
          "original_form": "저는 코로나 끝나면 바로 해외갈려구요",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100000774.1.27",
          "form": "방콕 너무 가보고 싶어요...ㅜㅜ",
          "original_form": "방콕 너무 가보고 싶어요...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100000774.1.28",
          "form": "헐 방콕 좋당",
          "original_form": "헐 방콕 좋당",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100000774.1.29",
          "form": "바로 태국가실거예요??",
          "original_form": "바로 태국가실거예요??",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100000774.1.30",
          "form": "네에 좀 잠잠해지면 첫해외여행으로 방콕갈려구요",
          "original_form": "네에 좀 잠잠해지면 첫해외여행으로 방콕갈려구요",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100000774.1.31",
          "form": "좀 가깝고 물가도 싸니깐?",
          "original_form": "좀 가깝고 물가도 싸니깐?",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100000774.1.32",
          "form": "좀 잠잠이 아니라 아예 종료되면!",
          "original_form": "좀 잠잠이 아니라 아예 종료되면!",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100000774.1.33",
