      "utterance": [
        {
          "id": "MDRW2100043027.1.1",
          "form": "저두..",
          "original_form": "저두..",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043027.1.2",
          "form": "집가고싶어요..일왜해",
          "original_form": "집가고싶어요..일왜해",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043027.1.3",
          "form": "비도오니까 기분나쁜대 다들 반차쓰자",
          "original_form": "비도오니까 기분나쁜대 다들 반차쓰자",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043027.1.4",
          "form": "졸려죽겠어",
          "original_form": "졸려죽겠어",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043027.1.5",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043027.1.6",
          "form": "진짜 졸려.. 퇴근하는길에 치킨시킬까..",
          "original_form": "진짜 졸려.. 퇴근하는길에 치킨시킬까..",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043027.1.7",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043027.1.8",
          "form": "샤브샤브는?",
          "original_form": "샤브샤브는?",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043027.1.9",
          "form": "그거능 오늘 안될거같아..ㅎ",
          "original_form": "그거능 오늘 안될거같아..ㅎ",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043027.1.10",
          "form": "뼈해장국어때",
          "original_form": "뼈해장국어때",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043027.1.11",
          "form": "좋지 스트레스엔 뼈해장국..",
          "original_form": "좋지 스트레스엔 뼈해장국..",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043027.1.12",
          "form": "소주한잔 딱",
          "original_form": "소주한잔 딱",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043027.1.13",
          "form": "근데 콜레스테롤땜에 안될듯",
          "original_form": "근데 콜레스테롤땜에 안될듯",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043027.1.14",
          "form": "일하기싫으니 먹을생각뿐",
          "original_form": "일하기싫으니 먹을생각뿐",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043027.1.15",
          "form": "이제슬슬 일해야하는데",
          "original_form": "이제슬슬 일해야하는데",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043027.1.16",
          "form": "그거도안조아?",
          "original_form": "그거도안조아?",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043027.1.17",
          "form": "거의웬만한 수치는탑이던데",
          "original_form": "거의웬만한 수치는탑이던데",
