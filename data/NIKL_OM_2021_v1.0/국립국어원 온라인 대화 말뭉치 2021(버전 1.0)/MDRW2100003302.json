      "utterance": [
        {
          "id": "MDRW2100003302.1.1",
          "form": "좋아하진 않고 먹고 살려고 해먹지ㅋㅋ",
          "original_form": "좋아하진 않고 먹고 살려고 해먹지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100003302.1.2",
          "form": "나는 요리 좋아해",
          "original_form": "나는 요리 좋아해",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100003302.1.3",
          "form": "우와 잘하는 음식 있어?",
          "original_form": "우와 잘하는 음식 있어?",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003302.1.4",
          "form": "튀김류 좀 잘하는 것 같아",
          "original_form": "튀김류 좀 잘하는 것 같아",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003302.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003302.1.6",
          "form": "바삭바삭 튀김",
          "original_form": "바삭바삭 튀김",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003302.1.7",
          "form": "맞아 바삭바삭 맛있어",
          "original_form": "맞아 바삭바삭 맛있어",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003302.1.8",
          "form": "튀김 먹고 싶다",
          "original_form": "튀김 먹고 싶다",
          "speaker_id": "2",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100003302.1.9",
          "form": "나도 ㅋㅋ",
          "original_form": "나도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100003302.1.10",
          "form": "나는 국은 잘 못 끓이는 거 같아",
          "original_form": "나는 국은 잘 못 끓이는 거 같아",
          "speaker_id": "1",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003302.1.11",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003302.1.12",
          "form": "간을 잘 못 맞추겠어",
          "original_form": "간을 잘 못 맞추겠어",
          "speaker_id": "1",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003302.1.13",
          "form": "맞아 간맞추기 어려워 ㅋㅋ 나는 무조건 레시피 그대로 넣어 ㅋㅋ",
          "original_form": "맞아 간맞추기 어려워 ㅋㅋ 나는 무조건 레시피 그대로 넣어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003302.1.14",
          "form": "맞아맞아 그래야해",
          "original_form": "맞아맞아 그래야해",
          "speaker_id": "1",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003302.1.15",
          "form": "내 맘대로 하면 신기한 맛이 나오더라고 ㅋㅋ",
          "original_form": "내 맘대로 하면 신기한 맛이 나오더라고 ㅋㅋ",
