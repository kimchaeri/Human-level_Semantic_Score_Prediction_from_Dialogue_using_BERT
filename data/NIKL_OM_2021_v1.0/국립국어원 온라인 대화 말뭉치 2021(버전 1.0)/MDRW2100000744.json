      "utterance": [
        {
          "id": "MDRW2100000744.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000744.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000744.1.3",
          "form": "가족 관계가 어떻게 되세요?",
          "original_form": "가족 관계가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000744.1.4",
          "form": "일단 저는 엄마 아빠 형 동생",
          "original_form": "일단 저는 엄마 아빠 형 동생",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000744.1.5",
          "form": "제가 둘째입니다!",
          "original_form": "제가 둘째입니다!",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000744.1.6",
          "form": "저도 형제만 있어요 전 첫째입니다",
          "original_form": "저도 형제만 있어요 전 첫째입니다",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000744.1.7",
          "form": "우와..",
          "original_form": "우와..",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000744.1.8",
          "form": "첫째 힘드시겠어요..",
          "original_form": "첫째 힘드시겠어요..",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.9",
          "form": "남성분이신가봐요",
          "original_form": "남성분이신가봐요",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.10",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.11",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.12",
          "form": "부담이 엄청 될텐데..",
          "original_form": "부담이 엄청 될텐데..",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.13",
          "form": "저 여자로 뜨나요?",
          "original_form": "저 여자로 뜨나요?",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.14",
          "form": "저희 형보면 그러더라고요..",
          "original_form": "저희 형보면 그러더라고요..",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000744.1.15",
          "form": "동생 한분 있나요?",
          "original_form": "동생 한분 있나요?",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000744.1.16",
          "form": "2명있어요",
          "original_form": "2명있어요",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000744.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000744.1.18",
          "form": "첫째랑 막네랑",
          "original_form": "첫째랑 막네랑",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000744.1.19",
          "form": "몇살차이나요?",
          "original_form": "몇살차이나요?",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000744.1.20",
          "form": "아무래도 사람이 많다보니 식비가 많이 드는거같앙ㅅ",
          "original_form": "아무래도 사람이 많다보니 식비가 많이 드는거같앙ㅅ",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000744.1.21",
          "form": "아요",
          "original_form": "아요",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000744.1.22",
          "form": "... 아 인정..",
          "original_form": "... 아 인정..",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000744.1.23",
          "form": "저희 집은 형이 자취하고 있어서 그나마 덜인데..",
          "original_form": "저희 집은 형이 자취하고 있어서 그나마 덜인데..",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000744.1.24",
          "form": "저희형이랑 제가 대학 다니고 있는데..",
          "original_form": "저희형이랑 제가 대학 다니고 있는데..",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000744.1.25",
          "form": "등록금이.. 엄청나죠..",
          "original_form": "등록금이.. 엄청나죠..",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000744.1.26",
          "form": "동생까지 오면은..",
          "original_form": "동생까지 오면은..",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000744.1.27",
          "form": "나중에 커서 효도해야죠.. ㅠㅠ",
          "original_form": "나중에 커서 효도해야죠.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 18:58"
        }
      ]
    }
