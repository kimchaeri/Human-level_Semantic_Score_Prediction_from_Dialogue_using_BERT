      "utterance": [
        {
          "id": "MDRW2100012852.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012852.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 10:08"
        },
        {
          "id": "MDRW2100012852.1.3",
          "form": "저는 무교예요",
          "original_form": "저는 무교예요",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100012852.1.4",
          "form": "저도 무교에요!",
          "original_form": "저도 무교에요!",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100012852.1.5",
          "form": "과거에 기독교 학교를 다니긴했었는데",
          "original_form": "과거에 기독교 학교를 다니긴했었는데",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100012852.1.6",
          "form": "무교입니다",
          "original_form": "무교입니다",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100012852.1.7",
          "form": "무교인 이유가 있으세요?",
          "original_form": "무교인 이유가 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100012852.1.8",
          "form": "저는 솔직히 말해서",
          "original_form": "저는 솔직히 말해서",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100012852.1.9",
          "form": "무신론자인지라",
          "original_form": "무신론자인지라",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100012852.1.10",
          "form": "신이없다고 생각하는 사람이에요",
          "original_form": "신이없다고 생각하는 사람이에요",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100012852.1.11",
          "form": "그래서 그런지",
          "original_form": "그래서 그런지",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100012852.1.12",
          "form": "종교를 믿거나 그러진 않죠",
          "original_form": "종교를 믿거나 그러진 않죠",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100012852.1.13",
          "form": "저는 저를 믿어서 무교예요",
          "original_form": "저는 저를 믿어서 무교예요",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100012852.1.14",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100012852.1.15",
          "form": "절 가본적있어요?",
          "original_form": "절 가본적있어요?",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100012852.1.16",
          "form": "절도 몇번 가보긴했는데",
          "original_form": "절도 몇번 가보긴했는데",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012852.1.17",
          "form": "그닥..",
          "original_form": "그닥..",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012852.1.18",
          "form": "어디 가봤어요?",
          "original_form": "어디 가봤어요?",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100012852.1.19",
          "form": "교회랑 절 몇번가봤어요",
          "original_form": "교회랑 절 몇번가봤어요",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100012852.1.20",
          "form": "교회는 학교에서 간혹 종교행사있을때 가봤고",
          "original_form": "교회는 학교에서 간혹 종교행사있을때 가봤고",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012852.1.21",
          "form": "절은 부모님이랑 몇번 가본기억이 있네요",
          "original_form": "절은 부모님이랑 몇번 가본기억이 있네요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012852.1.22",
          "form": "저는 봉은사 가봤어요",
          "original_form": "저는 봉은사 가봤어요",
          "speaker_id": "1",
          "time": "20210825 16:02"
        }
      ]
