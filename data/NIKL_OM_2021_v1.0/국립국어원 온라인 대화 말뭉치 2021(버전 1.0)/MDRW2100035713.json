      "utterance": [
        {
          "id": "MDRW2100035713.1.1",
          "form": "굿모닝\r\n아침 드셨어요?",
          "original_form": "굿모닝{emoji:♡}\r\n아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100035713.1.2",
          "form": "안녕하세요? 오늘도 아침은 건너뛰고 ㅜ 점심은 서브웨이 비엘티에 아보카도 추가에요!",
          "original_form": "안녕하세요? 오늘도 아침은 건너뛰고 ㅜ 점심은 서브웨이 비엘티에 아보카도 추가에요!",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100035713.1.3",
          "form": "아보카도 추가는 햄이나 좀 더 심플한 핸드위치에 하는게 좋겠어요",
          "original_form": "아보카도 추가는 햄이나 좀 더 심플한 핸드위치에 하는게 좋겠어요",
          "speaker_id": "1",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035713.1.4",
          "form": "베이컨과 아보카도는 그닥이네요",
          "original_form": "베이컨과 아보카도는 그닥이네요",
          "speaker_id": "1",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100035713.1.5",
          "form": "서브웨이 맛있겠네요 ㅋ",
          "original_form": "서브웨이 맛있겠네요 ㅋ",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035713.1.6",
          "form": "주말동안 밀트랩 준비 안하셨어요?",
          "original_form": "주말동안 밀트랩 준비 안하셨어요?",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035713.1.7",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035713.1.8",
          "form": "싸서 들고가서 데우기가..",
          "original_form": "싸서 들고가서 데우기가..",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035713.1.9",
          "form": "귀찮아서 만들어도 그건 집용이에요",
          "original_form": "귀찮아서 만들어도 그건 집용이에요",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035713.1.10",
          "form": "귀찮다기 보단 먹을 공간이 애매하달까",
          "original_form": "귀찮다기 보단 먹을 공간이 애매하달까",
          "speaker_id": "1",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100035713.1.11",
          "form": "밀트랩의 문제점이군요! ㅋ",
          "original_form": "밀트랩의 문제점이군요! ㅋ",
          "speaker_id": "2",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035713.1.12",
          "form": "도시락싸가두 먹을 공간이 좀 애매해욬",
          "original_form": "도시락싸가두 먹을 공간이 좀 애매해욬",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035713.1.13",
          "form": "ㅠㅜ 돈 안들고 맛있는 식사를 노력없이 하고 싶네요",
          "original_form": "ㅠㅜ 돈 안들고 맛있는 식사를 노력없이 하고 싶네요",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035713.1.14",
          "form": "묵고살기도 힘들어요",
          "original_form": "묵고살기도 힘들어요",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035713.1.15",
          "form": "name2님은 뭘 드셨나요?",
          "original_form": "&name2&님은 뭘 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035713.1.16",
          "form": "아 name1님 주부신줄\r\n알았는데 아니었나봐용ㅋ",
          "original_form": "아 &name1&님 주부신줄\r\n알았는데 아니었나봐용ㅋ",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035713.1.17",
          "form": "저는 아점으로 집밥 먹었어요:)",
          "original_form": "저는 아점으로 집밥 먹었어요:)",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035713.1.18",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100035713.1.19",
          "form": "집밥이 최고죠 아는 맛이고 건강에도 좋구요",
          "original_form": "집밥이 최고죠 아는 맛이고 건강에도 좋구요",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100035713.1.20",
          "form": "ㅋㅋ근데 자극적인거 먹고싶네요",
          "original_form": "ㅋㅋ근데 자극적인거 먹고싶네요",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035713.1.21",
          "form": "아는 건강한맛 말고 ㅋㅋ",
          "original_form": "아는 건강한맛 말고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035713.1.22",
          "form": "이것두 집밥의 장점이죠",
          "original_form": "이것두 집밥의 장점이죠",
          "speaker_id": "1",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100035713.1.23",
          "form": "건강해야 안건강한걸 땡겨할 수 있잖아요",
          "original_form": "건강해야 안건강한걸 땡겨할 수 있잖아요",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035713.1.24",
          "form": "집밥이 있어야 외식이 맛있을 수 있죠ㅋㅋㅋ",
          "original_form": "집밥이 있어야 외식이 맛있을 수 있죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100035713.1.25",
          "form": "역시 오늘도 name1님을 통해\r\n음식에 대해 배워갑니다 ㅋㅋ",
          "original_form": "역시 오늘도 &name1&님을 통해\r\n음식에 대해 배워갑니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100035713.1.26",
          "form": "이걸 하면서 전 좀 음식교회 목사가 되어서 음식 설교 하는것 같은 기분이 드는데요",
          "original_form": "이걸 하면서 전 좀 음식교회 목사가 되어서 음식 설교 하는것 같은 기분이 드는데요",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100035713.1.27",
          "form": "기분탓이 아니라 정말 그러니 좀 문제네요 ㅋㅋㅋ",
          "original_form": "기분탓이 아니라 정말 그러니 좀 문제네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100035713.1.28",
          "form": "ㅋㅋ좋아요:)\r\n아시는거 같이 나누고 하는잖아요\r\n음식에 관련된 정보들",
          "original_form": "ㅋㅋ좋아요:)\r\n아시는거 같이 나누고 하는잖아요{emoji:♡}\r\n음식에 관련된 정보들",
          "speaker_id": "2",
          "time": "20210830 16:47"
        }
      ]
