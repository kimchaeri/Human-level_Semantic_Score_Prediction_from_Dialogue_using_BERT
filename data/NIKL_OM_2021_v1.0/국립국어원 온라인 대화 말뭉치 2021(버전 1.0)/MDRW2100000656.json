        },
        {
          "id": "MDRW2100000656.1.3",
          "form": "name2색좋아하시나봐요",
          "original_form": "&name2&색좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000656.1.4",
          "form": "네, 딱 색이 생각나서 그걸로했어요",
          "original_form": "네, 딱 색이 생각나서 그걸로했어요",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000656.1.5",
          "form": "네ㅎ  아미신가 했어요",
          "original_form": "네ㅎ  아미신가 했어요",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000656.1.6",
          "form": "아, 방탄도좋아해요~",
          "original_form": "아, 방탄도좋아해요~",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000656.1.7",
          "form": "방탄 너무 멋있죠ㅎㅎ",
          "original_form": "방탄 너무 멋있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000656.1.8",
          "form": "그쵸~ 노래도 좋고요~",
          "original_form": "그쵸~ 노래도 좋고요~",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000656.1.9",
          "form": "빌보드에서 활약하는거보면 자랑스럽네요",
          "original_form": "빌보드에서 활약하는거보면 자랑스럽네요",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000656.1.10",
          "form": "맞아요~ UN연설때도그랬고요~",
          "original_form": "맞아요~ UN연설때도그랬고요~",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000656.1.11",
          "form": "네ㅎㅎ 반려동물 키우시나요?",
          "original_form": "네ㅎㅎ 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000656.1.12",
          "form": "아쉽지만 안키워요~",
          "original_form": "아쉽지만 안키워요~",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000656.1.13",
          "form": "네 저도 좋아하는데 알러지가있어서 키우진않고있어요",
          "original_form": "네 저도 좋아하는데 알러지가있어서 키우진않고있어요",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000656.1.14",
          "form": "아, 알러지 있으시구나ㅠ 전 어릴적부터 고양이키우고 싶었는데 커서 보니 책임감+연건이 안되서요~",
          "original_form": "아, 알러지 있으시구나ㅠ 전 어릴적부터 고양이키우고 싶었는데 커서 보니 책임감+연건이 안되서요~",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000656.1.15",
          "form": "맞아요 들이기전에 충분히 여건고려해보는게 중요한거같아요",
          "original_form": "맞아요 들이기전에 충분히 여건고려해보는게 중요한거같아요",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000656.1.16",
          "form": "그쵸 가족이 되는거니깐요ㅎ",
          "original_form": "그쵸 가족이 되는거니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000656.1.17",
          "form": "그렇네요. 가족맞죠^^",
          "original_form": "그렇네요. 가족맞죠^^",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100000656.1.18",
          "form": "어떤 반려동물 관심있으셨어요?",
          "original_form": "어떤 반려동물 관심있으셨어요?",
          "speaker_id": "2",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100000656.1.19",
          "form": "저는 강아지요^^",
          "original_form": "저는 강아지요^^",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100000656.1.20",
          "form": "아하, 좋아하는 강아지종있어요?",
          "original_form": "아하, 좋아하는 강아지종있어요?",
          "speaker_id": "2",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100000656.1.21",
          "form": "어렸을땐 말티즈 키우고싶었는데 요즘은 그냥다귀여워보여요",
          "original_form": "어렸을땐 말티즈 키우고싶었는데 요즘은 그냥다귀여워보여요",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000656.1.22",
          "form": "그쵸~산책하다보면 가끔 보면 강아지가 쳐다보거나 이뻐해달라고 다가오는거 보면 너무 귀여워요",
          "original_form": "그쵸~산책하다보면 가끔 보면 강아지가 쳐다보거나 이뻐해달라고 다가오는거 보면 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000656.1.23",
          "form": "맞아요ㅎ 키울자신은없지만 보면서 흐뭇해하고있네요ㅋ",
          "original_form": "맞아요ㅎ 키울자신은없지만 보면서 흐뭇해하고있네요ㅋ",
          "speaker_id": "1",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000656.1.24",
          "form": "그래서 유튜브로 동물채널도 보고 대리만족해요",
          "original_form": "그래서 유튜브로 동물채널도 보고 대리만족해요",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000656.1.25",
          "form": "아~유튜브에서도 볼수있겠네요ㅎ",
          "original_form": "아~유튜브에서도 볼수있겠네요ㅎ",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000656.1.26",
          "form": "유튜브 볼게 참 많죠",
          "original_form": "유튜브 볼게 참 많죠",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000656.1.27",
          "form": "그쵸~ 이젠 티비보다 유튜브를 더 많이 보게 되는것 같아요ㅎ",
          "original_form": "그쵸~ 이젠 티비보다 유튜브를 더 많이 보게 되는것 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000656.1.28",
          "form": "네 저도요ㅎ 호흡이 빠르니 지루하지않고 좋더라구요",
          "original_form": "네 저도요ㅎ 호흡이 빠르니 지루하지않고 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 20:10"
        },
        {
          "id": "MDRW2100000656.1.29",
          "form": "그렇죠~ 볼거리가 많아서 요즘 낙이네요",
          "original_form": "그렇죠~ 볼거리가 많아서 요즘 낙이네요",
          "speaker_id": "2",
          "time": "20210819 21:16"
        }
      ]
