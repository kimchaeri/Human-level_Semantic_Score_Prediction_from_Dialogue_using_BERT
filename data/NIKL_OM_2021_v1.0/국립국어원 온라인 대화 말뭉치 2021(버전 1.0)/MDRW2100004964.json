      "utterance": [
        {
          "id": "MDRW2100004964.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004964.1.2",
          "form": "혹시 자빱 이라고 게임유투버 아세요?",
          "original_form": "혹시 자빱 이라고 게임유투버 아세요?",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004964.1.3",
          "form": "넵 알아요 ㅎㅎ 가끔 보는데 재밌어요",
          "original_form": "넵 알아요 ㅎㅎ 가끔 보는데 재밌어요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004964.1.4",
          "form": "ㅎㅎ 너무 재밌더라구요",
          "original_form": "ㅎㅎ 너무 재밌더라구요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004964.1.5",
          "form": "게임은 어떤거 좋아하세요? ㅎㅎ",
          "original_form": "게임은 어떤거 좋아하세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004964.1.6",
          "form": "저 스타듀밸리 좋아해요!",
          "original_form": "저 스타듀밸리 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100004964.1.7",
          "form": "저도 스타듀밸리 하는데ㅎㅎ 몇년차세용? 저는 아직 2년차에요ㅋㅋㅋ",
          "original_form": "저도 스타듀밸리 하는데ㅎㅎ 몇년차세용? 저는 아직 2년차에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100004964.1.8",
          "form": "저는 처음키운거는",
          "original_form": "저는 처음키운거는",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100004964.1.9",
          "form": "5년차정도 된거같아요 ㅋㅋ",
          "original_form": "5년차정도 된거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100004964.1.10",
          "form": "와 대박 ㅋㅋㅋ 저는 2년차가는것도 힘들었는데..",
          "original_form": "와 대박 ㅋㅋㅋ 저는 2년차가는것도 힘들었는데..",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100004964.1.11",
          "form": "전맨날 광산만 가요ㅋㅋㅋ",
          "original_form": "전맨날 광산만 가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100004964.1.12",
          "form": "가서 버섯캐면 하루가 빨리가요",
          "original_form": "가서 버섯캐면 하루가 빨리가요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100004964.1.13",
          "form": "와ㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100004964.1.14",
          "form": "광산도 한창 빠지면",
          "original_form": "광산도 한창 빠지면",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100004964.1.15",
          "form": "너무 재밌어요 컨텐츠가 많아서 ㅋㅋ",
          "original_form": "너무 재밌어요 컨텐츠가 많아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100004964.1.16",
          "form": "혹시 결혼 하셨나요? 전 커플링은 못찾아서 못하고 있어요ㅜㅜ",
          "original_form": "혹시 결혼 하셨나요? 전 커플링은 못찾아서 못하고 있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 08:16"
        },
        {
          "id": "MDRW2100004964.1.17",
          "form": "저는 샘이랑 ㅎㅎ",
          "original_form": "저는 샘이랑 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100004964.1.18",
          "form": "마을청년들 거의 다 꼬셔서 살다가",
          "original_form": "마을청년들 거의 다 꼬셔서 살다가",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100004964.1.19",
          "form": "4년차쯤되니까 지루해서 한명골라서 결혼했어요 ㅋㅋ",
          "original_form": "4년차쯤되니까 지루해서 한명골라서 결혼했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100004964.1.20",
          "form": "재밌게 사셨네요ㅋㅋㅋ 저는 지금 셰인이랑 알렉스 공략중이에요ㅎㅎ",
          "original_form": "재밌게 사셨네요ㅋㅋㅋ 저는 지금 셰인이랑 알렉스 공략중이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100004964.1.21",
          "form": "커플링 어떻게 받는지 아세요?",
          "original_form": "커플링 어떻게 받는지 아세요?",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100004964.1.22",
          "form": "결혼반지라면",
          "original_form": "결혼반지라면",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100004964.1.23",
          "form": "비오는날에 바닷가에 한번 가보세요!",
          "original_form": "비오는날에 바닷가에 한번 가보세요!",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100004964.1.24",
          "form": "그 아저씨 말하는거죠?",
          "original_form": "그 아저씨 말하는거죠?",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100004964.1.25",
          "form": "가니깐 조건이 아직 안됐대욬ㅋㅋ",
          "original_form": "가니깐 조건이 아직 안됐대욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100004964.1.26",
          "form": "그냥 결혼안하고 살아야되나봐용ㅎㅎ",
          "original_form": "그냥 결혼안하고 살아야되나봐용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:47"
        }
      ]
    }
