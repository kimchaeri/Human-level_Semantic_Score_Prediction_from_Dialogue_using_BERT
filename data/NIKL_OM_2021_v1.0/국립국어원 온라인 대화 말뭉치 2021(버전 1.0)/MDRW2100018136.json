      "utterance": [
        {
          "id": "MDRW2100018136.1.1",
          "form": "최근에 뭘 사셨나요",
          "original_form": "최근에 뭘 사셨나요",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018136.1.2",
          "form": "크 크큭",
          "original_form": "크 크큭",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018136.1.3",
          "form": "전 어",
          "original_form": "전 어",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018136.1.4",
          "form": "뭐샀더라",
          "original_form": "뭐샀더라",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018136.1.5",
          "form": "모바일게임 캐릭터샀어요",
          "original_form": "모바일게임 캐릭터샀어요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018136.1.6",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018136.1.7",
          "form": "저는 갤워치요",
          "original_form": "저는 갤워치요",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018136.1.8",
          "form": "어제 왔어요 ㅋㅋㅋ",
          "original_form": "어제 왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018136.1.9",
          "form": "헐 자랑좀요",
          "original_form": "헐 자랑좀요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018136.1.10",
          "form": "저는 한게 캐릭터 5만5천원씩 확정가챠 샀었네요",
          "original_form": "저는 한게 캐릭터 5만5천원씩 확정가챠 샀었네요",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018136.1.11",
          "form": "오.. 가챠 들어봤어요",
          "original_form": "오.. 가챠 들어봤어요",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018136.1.12",
          "form": "피규어샵 같은거죠?",
          "original_form": "피규어샵 같은거죠?",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018136.1.13",
          "form": "가챠는 뽑기라서피규어샵 맞는ㆍ디ㅣ",
          "original_form": "가챠는 뽑기라서피규어샵 맞는ㆍ디ㅣ",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018136.1.14",
          "form": "저는 모바일 게임 플레이 캐릭터를",
          "original_form": "저는 모바일 게임 플레이 캐릭터를",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018136.1.15",
          "form": "돈주고 확률 100프로로 샀어요",
          "original_form": "돈주고 확률 100프로로 샀어요",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018136.1.16",
          "form": "아~~",
          "original_form": "아~~",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018136.1.17",
          "form": "이해완전했어요",
          "original_form": "이해완전했어요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018136.1.18",
          "form": "좋으셨겠네요 ㅋㅋㅋ",
          "original_form": "좋으셨겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018136.1.19",
          "form": "근데되게비싸네요ㅠㅠ",
          "original_form": "근데되게비싸네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018136.1.20",
          "form": "한국겜들 사행성 심하다하는거 다 랜덤뽑기(가챠)때문인데요",
          "original_form": "한국겜들 사행성 심하다하는거 다 랜덤뽑기(가챠)때문인데요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018136.1.21",
          "form": "캐릭터 만들때까지 소액주고 뽑는게 많은데 \r\n제가하는게임은 캐릭터는 게임내재화로 사는 캐릭터있고 과금해서 패키지로사는 캐릭터랑 출석보상 캐릭터가있어요",
          "original_form": "캐릭터 만들때까지 소액주고 뽑는게 많은데 \r\n제가하는게임은 캐릭터는 게임내재화로 사는 캐릭터있고 과금해서 패키지로사는 캐릭터랑 출석보상 캐릭터가있어요",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018136.1.22",
          "form": "오 여러 갈래가 있네요",
          "original_form": "오 여러 갈래가 있네요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018136.1.23",
          "form": "다행히",
          "original_form": "다행히",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018136.1.24",
          "form": "모 심한게임은 캐릭터 데려올때 머리 몸통 나눠서 그거 나올때까지 다 뽑아야해서 몇십만원도든다던데 그거에 비하면 5만5천원은 물건사는구나싶은",
          "original_form": "모 심한게임은 캐릭터 데려올때 머리 몸통 나눠서 그거 나올때까지 다 뽑아야해서 몇십만원도든다던데 그거에 비하면 5만5천원은 물건사는구나싶은",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018136.1.25",
          "form": "그치만 게임에 과금\r\n모바일게임 데이터에 찐돈쓰는게 현타오면 아까울 수 있어요",
          "original_form": "그치만 게임에 과금\r\n모바일게임 데이터에 찐돈쓰는게 현타오면 아까울 수 있어요",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018136.1.26",
          "form": "헐... 대박",
          "original_form": "헐... 대박",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018136.1.27",
          "form": "맞아요 생각이 다 다르니까",
          "original_form": "맞아요 생각이 다 다르니까",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018136.1.28",
          "form": "저는 게임을 안 해서 그런가봐요",
          "original_form": "저는 게임을 안 해서 그런가봐요",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018136.1.29",
          "form": "게임 덕질 이런거안하는 사람한테는",
          "original_form": "게임 덕질 이런거안하는 사람한테는",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018136.1.30",
          "form": "대신 저는 다른 데다가 돈 쓰니까 그게 그거 같아요 쌤쌤",
          "original_form": "대신 저는 다른 데다가 돈 쓰니까 그게 그거 같아요 쌤쌤",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018136.1.31",
          "form": "게임 서비스종료하면 허공으로날아가는데 아깝죠",
          "original_form": "게임 서비스종료하면 허공으로날아가는데 아깝죠",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018136.1.32",
          "form": "아...ㅠㅠ 서비스종료할 수도 있구나",
          "original_form": "아...ㅠㅠ 서비스종료할 수도 있구나",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018136.1.33",
          "form": "전 겜 질리면",
          "original_form": "전 겜 질리면",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018136.1.34",
          "form": "가끔 저도 아까워요 돈만있으면 사겠지만 5만5천원이면 치킨이 3마리",
          "original_form": "가끔 저도 아까워요 돈만있으면 사겠지만 5만5천원이면 치킨이 3마리",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018136.1.35",
          "form": "아까울것같긴하네요",
          "original_form": "아까울것같긴하네요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018136.1.36",
          "form": "겜회사가 캐릭터는 매력있게뽑아놓고 운영을 못하면",
          "original_form": "겜회사가 캐릭터는 매력있게뽑아놓고 운영을 못하면",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018136.1.37",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018136.1.38",
          "form": "다른거 산 건 있으세요?",
          "original_form": "다른거 산 건 있으세요?",
          "speaker_id": "1",
