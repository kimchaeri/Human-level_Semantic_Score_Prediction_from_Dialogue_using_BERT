        {
          "id": "MDRW2100029867.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029867.1.2",
          "form": "아뇨 전 치킨정도?",
          "original_form": "아뇨 전 치킨정도?",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029867.1.3",
          "form": "배달 뭐 드시나용",
          "original_form": "배달 뭐 드시나용",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029867.1.4",
          "form": "주로 치킨이요",
          "original_form": "주로 치킨이요",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029867.1.5",
          "form": "역시 치킨이 최고죠",
          "original_form": "역시 치킨이 최고죠",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029867.1.6",
          "form": "족발이나 보쌈 좋아하세요?",
          "original_form": "족발이나 보쌈 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029867.1.7",
          "form": "네 좋죠",
          "original_form": "네 좋죠",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029867.1.8",
          "form": "저도 보쌈 좋아해요",
          "original_form": "저도 보쌈 좋아해요",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029867.1.9",
          "form": "저는 샤브샤브도 좋아해요",
          "original_form": "저는 샤브샤브도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029867.1.10",
          "form": "좋아하는 음식 있으세용?",
          "original_form": "좋아하는 음식 있으세용?",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029867.1.11",
          "form": "전 치킨이요",
          "original_form": "전 치킨이요",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100029867.1.12",
          "form": "오 치킨 좋아시는구나",
          "original_form": "오 치킨 좋아시는구나",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029867.1.13",
          "form": "네 ㅋㅋ 맛있네요",
          "original_form": "네 ㅋㅋ 맛있네요",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029867.1.14",
          "form": "전 뿌링클 좋아해요",
          "original_form": "전 뿌링클 좋아해요",
          "speaker_id": "2",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100029867.1.15",
          "form": "것도 좋죠",
          "original_form": "것도 좋죠",
          "speaker_id": "1",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029867.1.16",
          "form": "후라이드랑 양념중에 뭐가 좋으세요?",
          "original_form": "후라이드랑 양념중에 뭐가 좋으세요?",
