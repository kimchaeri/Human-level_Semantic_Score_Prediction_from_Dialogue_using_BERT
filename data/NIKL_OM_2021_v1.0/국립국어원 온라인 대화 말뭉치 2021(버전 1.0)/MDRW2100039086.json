      "utterance": [
        {
          "id": "MDRW2100039086.1.1",
          "form": "좋아하는 연예인 있나요?",
          "original_form": "좋아하는 연예인 있나요?",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039086.1.2",
          "form": "저 투바투 좋아해요",
          "original_form": "저 투바투 좋아해요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039086.1.3",
          "form": "아이돌이죠? 제가 잘 몰라서요",
          "original_form": "아이돌이죠? 제가 잘 몰라서요",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039086.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100039086.1.5",
          "form": "name1님은 좋아하는 연예인 있으신가요",
          "original_form": "&name1&님은 좋아하는 연예인 있으신가요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100039086.1.6",
          "form": "해외 배우 좋아해요",
          "original_form": "해외 배우 좋아해요",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039086.1.7",
          "form": "그래서 영화랑 드라마 추석에 볼려고요",
          "original_form": "그래서 영화랑 드라마 추석에 볼려고요",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039086.1.8",
          "form": "어느 배우요? 저도 추석에 볼만한 영화 골라야하는데",
          "original_form": "어느 배우요? 저도 추석에 볼만한 영화 골라야하는데",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039086.1.9",
          "form": "추천해주세요",
          "original_form": "추천해주세요",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039086.1.10",
          "form": "벤반스라고 영국배우요",
          "original_form": "벤반스라고 영국배우요",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100039086.1.11",
          "form": "좋아하는 장르 있으세요?",
          "original_form": "좋아하는 장르 있으세요?",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100039086.1.12",
          "form": "호러만 아니면 다 봐요",
          "original_form": "호러만 아니면 다 봐요",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100039086.1.13",
          "form": "극장에서 보실거면 마블 샹치 추천이요",
          "original_form": "극장에서 보실거면 마블 샹치 추천이요",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100039086.1.14",
          "form": "아 샹치 개봉했겠군요",
          "original_form": "아 샹치 개봉했겠군요",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100039086.1.15",
          "form": "네 개봉했어요 마블 영화 재밌잖아요",
          "original_form": "네 개봉했어요 마블 영화 재밌잖아요",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100039086.1.16",
          "form": "맞아요 저번 블랙위도우까지 챙겨보고",
          "original_form": "맞아요 저번 블랙위도우까지 챙겨보고",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100039086.1.17",
          "form": "까먹었네요...ㅋㅋ",
          "original_form": "까먹었네요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100039086.1.18",
          "form": "마블 영화 잘 챙겨 보시는군요",
          "original_form": "마블 영화 잘 챙겨 보시는군요",
          "speaker_id": "1",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100039086.1.19",
          "form": "네 마블 영화는 다 봤어요",
