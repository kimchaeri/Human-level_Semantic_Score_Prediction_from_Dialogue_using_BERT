      "utterance": [
        {
          "id": "MDRW2100036739.1.1",
          "form": "난 카페..",
          "original_form": "난 카페..",
          "speaker_id": "2",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100036739.1.2",
          "form": "실강 들을 때 원래 집 말고 카페에서 들어?",
          "original_form": "실강 들을 때 원래 집 말고 카페에서 들어?",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036739.1.3",
          "form": "카페는 시끄럽지 않아?",
          "original_form": "카페는 시끄럽지 않아?",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100036739.1.4",
          "form": "점심 친구랑 먹으려고 나왔었지",
          "original_form": "점심 친구랑 먹으려고 나왔었지",
          "speaker_id": "2",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100036739.1.5",
          "form": "친구가 같이 점심 먹자길래",
          "original_form": "친구가 같이 점심 먹자길래",
          "speaker_id": "2",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100036739.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100036739.1.7",
          "form": "원래는 집에서 들어?",
          "original_form": "원래는 집에서 들어?",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100036739.1.8",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100036739.1.9",
          "form": "교양때 캠도 안켜서",
          "original_form": "교양때 캠도 안켜서",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100036739.1.10",
          "form": "잠옷입고 들을거다",
          "original_form": "잠옷입고 들을거다",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100036739.1.11",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100036739.1.12",
          "form": "오늘 수업은 어땠어?",
          "original_form": "오늘 수업은 어땠어?",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100036739.1.13",
          "form": "오티만 했지",
          "original_form": "오티만 했지",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036739.1.14",
          "form": "개이득",
          "original_form": "개이득",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036739.1.15",
          "form": "으아 부럽다",
          "original_form": "으아 부럽다",
          "speaker_id": "1",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036739.1.16",
          "form": "나도 내일 실시간 강의 하나 있어...",
          "original_form": "나도 내일 실시간 강의 하나 있어...",
          "speaker_id": "1",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036739.1.17",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036739.1.18",
          "form": "난 항상 실강...",
          "original_form": "난 항상 실강...",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100036739.1.19",
          "form": "대면만 아니면 다행...",
          "original_form": "대면만 아니면 다행...",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100036739.1.20",
          "form": "너희 이번학기도 계속 비대면 한대?",
          "original_form": "너희 이번학기도 계속 비대면 한대?",
          "speaker_id": "1",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100036739.1.21",
          "form": "아니..",
          "original_form": "아니..",
          "speaker_id": "2",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100036739.1.22",
          "form": "3주부터",
          "original_form": "3주부터",
          "speaker_id": "2",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100036739.1.23",
          "form": "병행",
          "original_form": "병행",
          "speaker_id": "2",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100036739.1.24",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100036739.1.25",
          "form": "그럼 대면이랑 비대멱 섞어서 하는 거야?",
          "original_form": "그럼 대면이랑 비대멱 섞어서 하는 거야?",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100036739.1.26",
          "form": "아니 선택",
          "original_form": "아니 선택",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100036739.1.27",
          "form": "얍",
