      "utterance": [
        {
          "id": "MDRW2100004889.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004889.1.2",
          "form": "아르바이트 해보신적잏나요?",
          "original_form": "아르바이트 해보신적잏나요?",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004889.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004889.1.4",
          "form": "내일도 알바 가요ㅠ",
          "original_form": "내일도 알바 가요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004889.1.5",
          "form": "헉 힘드시겠다",
          "original_form": "헉 힘드시겠다",
          "speaker_id": "1",
          "time": "20210821 19:07"
        },
        {
          "id": "MDRW2100004889.1.6",
          "form": "혹시 어떤 쪽 알바하시는지 알려주실 슈 있나요?",
          "original_form": "혹시 어떤 쪽 알바하시는지 알려주실 슈 있나요?",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004889.1.7",
          "form": "저는 서비스업 알바하고 있어요",
          "original_form": "저는 서비스업 알바하고 있어요",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004889.1.8",
          "form": "저는 공연장 티켓 발권하는 알바 해봤어요!",
          "original_form": "저는 공연장 티켓 발권하는 알바 해봤어요!",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004889.1.9",
          "form": "우와! 안 힘드셨나요?",
          "original_form": "우와! 안 힘드셨나요?",
          "speaker_id": "2",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004889.1.10",
          "form": "저는 오히려 업무시간 자체는 짧고",
          "original_form": "저는 오히려 업무시간 자체는 짧고",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004889.1.11",
          "form": "유동적으로 스케줄 조정도 가능해서 할만했던것같아요!",
          "original_form": "유동적으로 스케줄 조정도 가능해서 할만했던것같아요!",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004889.1.12",
          "form": "서비스업이면 고객상대하는게 되게 힘들 것같아요ㅜ",
          "original_form": "서비스업이면 고객상대하는게 되게 힘들 것같아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004889.1.13",
          "form": "네ㅠ 특히 코로나라서 마스크 쓰시는거",
          "original_form": "네ㅠ 특히 코로나라서 마스크 쓰시는거",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004889.1.14",
          "form": "주의드리는것도 신경 쓰이더라고요",
          "original_form": "주의드리는것도 신경 쓰이더라고요",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004889.1.15",
          "form": "티켓 발권 알바는 어떻게 구하셨나요?",
          "original_form": "티켓 발권 알바는 어떻게 구하셨나요?",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004889.1.16",
          "form": "그러게요ㅜ",
          "original_form": "그러게요ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004889.1.17",
          "form": "요즘 코로나인데 사람들이랑 계속 부딫혀야되니까 힘드시겠어요ㅜ",
          "original_form": "요즘 코로나인데 사람들이랑 계속 부딫혀야되니까 힘드시겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004889.1.18",
          "form": "티켓발권알바는 우연히 이 알바 하고있던 지인이 다른일로 일 그만두게되면서 자리 이어서 할 사람 구하길래 제가 하게됐어요!",
          "original_form": "티켓발권알바는 우연히 이 알바 하고있던 지인이 다른일로 일 그만두게되면서 자리 이어서 할 사람 구하길래 제가 하게됐어요!",
          "speaker_id": "1",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004889.1.19",
          "form": "오 그렇군요!",
          "original_form": "오 그렇군요!",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004889.1.20",
          "form": "저는 요즘 알바하면서 취준하고 있어요",
          "original_form": "저는 요즘 알바하면서 취준하고 있어요",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004889.1.21",
          "form": "요즘 취업 어렵더라고요...ㅎ",
          "original_form": "요즘 취업 어렵더라고요...ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004889.1.22",
          "form": "아이고ㅜ",
          "original_form": "아이고ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004889.1.23",
          "form": "그쵸 오히려 코로나때문에 더 어렵게된거같아요",
          "original_form": "그쵸 오히려 코로나때문에 더 어렵게된거같아요",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004889.1.24",
          "form": "스펙쌓을 기회도 줄고,,",
          "original_form": "스펙쌓을 기회도 줄고,,",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004889.1.25",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004889.1.26",
          "form": "인턴 자리도 경쟁률이 엄청나더라고요",
          "original_form": "인턴 자리도 경쟁률이 엄청나더라고요",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004889.1.27",
          "form": "취준 해보신적 있으신가요?",
          "original_form": "취준 해보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004889.1.28",
          "form": "저는 아직 대학교 재학중이라서 경험은 없는데",
          "original_form": "저는 아직 대학교 재학중이라서 경험은 없는데",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004889.1.29",
          "form": "점점 주변에서 이야기가 들랴오니까",
          "original_form": "점점 주변에서 이야기가 들랴오니까",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004889.1.30",
          "form": "걱정만 쌓이고있어요ㅜ",
          "original_form": "걱정만 쌓이고있어요ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004889.1.31",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004889.1.32",
          "form": "저는 졸업반이라",
          "original_form": "저는 졸업반이라",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004889.1.33",
          "form": "동기들이랑 만나면 매번 취준 얘기하는거 같네요ㅋㅋㅋ",
          "original_form": "동기들이랑 만나면 매번 취준 얘기하는거 같네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004889.1.34",
          "form": "아앗ㅠㅠ",
          "original_form": "아앗ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004889.1.35",
          "form": "정말 시간은 왜그렇게 빨리 가는지",
          "original_form": "정말 시간은 왜그렇게 빨리 가는지",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004889.1.36",
          "form": "엊그제 일학년인거같아ㅛ는데",
          "original_form": "엊그제 일학년인거같아ㅛ는데",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004889.1.37",
          "form": "좀있으면 졸업하고 취준때문에 허우적거리겠죠, ㅠㅠ",
          "original_form": "좀있으면 졸업하고 취준때문에 허우적거리겠죠, ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004889.1.38",
          "form": "ㅠㅠ화이팅입니다",
          "original_form": "ㅠㅠ화이팅입니다",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004889.1.39",
          "form": "지금보단 취업시장이 더 좋아지기를",
          "original_form": "지금보단 취업시장이 더 좋아지기를",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004889.1.40",
          "form": "바라요",
          "original_form": "바라요",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004889.1.41",
          "form": "name2님도 화이팅입니다ㅜ",
          "original_form": "&name2&님도 화이팅입니다ㅜ",
          "speaker_id": "1",
          "time": "20210821 19:39"
        }
      ]
