      "utterance": [
        {
          "id": "MDRW2100033133.1.1",
          "form": "지금도 키우고 있죠!",
          "original_form": "지금도 키우고 있죠!",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100033133.1.2",
          "form": "고양이 키우고 있어요^^",
          "original_form": "고양이 키우고 있어요^^",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100033133.1.3",
          "form": "집사님이시군요",
          "original_form": "집사님이시군요",
          "speaker_id": "1",
          "time": "20210906 17:15"
        },
        {
          "id": "MDRW2100033133.1.4",
          "form": "고양이 이름은 뭔가요?",
          "original_form": "고양이 이름은 뭔가요?",
          "speaker_id": "1",
          "time": "20210906 17:15"
        },
        {
          "id": "MDRW2100033133.1.5",
          "form": "쿠키요!",
          "original_form": "쿠키요!",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100033133.1.6",
          "form": "쿠키색이라 비슷해서인가요?",
          "original_form": "쿠키색이라 비슷해서인가요?",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100033133.1.7",
          "form": "네 ㅋㅋ 갈색도 있고 그래서용",
          "original_form": "네 ㅋㅋ 갈색도 있고 그래서용",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033133.1.8",
          "form": "예쁜 이름입니다",
          "original_form": "예쁜 이름입니다",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033133.1.9",
          "form": "오래키웠나요?",
          "original_form": "오래키웠나요?",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100033133.1.10",
          "form": "한 3년정도 키웠어요!",
          "original_form": "한 3년정도 키웠어요!",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033133.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033133.1.12",
          "form": "정이 많이 들었겠어요",
          "original_form": "정이 많이 들었겠어요",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100033133.1.13",
          "form": "요즘 흔히 말하는 개냥이 인가요?",
          "original_form": "요즘 흔히 말하는 개냥이 인가요?",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100033133.1.14",
          "form": "아뇨 ㅋㅋㅋ 개냥이는 아니구",
          "original_form": "아뇨 ㅋㅋㅋ 개냥이는 아니구",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033133.1.15",
          "form": "흔히 그냥 집사로 부리는 고양이입니다..ㅋㅋㅋ",
          "original_form": "흔히 그냥 집사로 부리는 고양이입니다..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100033133.1.16",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033133.1.17",
          "form": "저는 물고기 키웁니다",
          "original_form": "저는 물고기 키웁니다",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033133.1.18",
          "form": "구피라는 종이지요",
          "original_form": "구피라는 종이지요",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033133.1.19",
          "form": "저도 옛날에 구피종류 기른적 있긴해요!",
          "original_form": "저도 옛날에 구피종류 기른적 있긴해요!",
          "speaker_id": "2",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100033133.1.20",
          "form": "어머니랑 같이 길러서 기억에남네요 ㅎㅎ",
          "original_form": "어머니랑 같이 길러서 기억에남네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100033133.1.21",
          "form": "구피 길러보셨군요",
          "original_form": "구피 길러보셨군요",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100033133.1.22",
          "form": "예전에는 물고기를 키우는데 흥미가 없었지만",
          "original_form": "예전에는 물고기를 키우는데 흥미가 없었지만",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100033133.1.23",
          "form": "막상 어항에 키워보니 은근히 반려동물에 들어가겠다 싶습니다",
          "original_form": "막상 어항에 키워보니 은근히 반려동물에 들어가겠다 싶습니다",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100033133.1.24",
          "form": "정이 가요",
          "original_form": "정이 가요",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100033133.1.25",
          "form": "은근이 정이 가죠",
          "original_form": "은근이 정이 가죠",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100033133.1.26",
          "form": "먹을거 줄때 그 알고 가까이 오는거보면 ㅋㅋㅋ",
          "original_form": "먹을거 줄때 그 알고 가까이 오는거보면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100033133.1.27",
          "form": "아시는군요",
          "original_form": "아시는군요",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.28",
          "form": "먹이 줄때 헤엄쳐 오는데",
          "original_form": "먹이 줄때 헤엄쳐 오는데",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.29",
          "form": "꼭 꼬리치는 듯합니다",
          "original_form": "꼭 꼬리치는 듯합니다",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.30",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.31",
          "form": "강아지 꼬리 흔드는 것처럼",
          "original_form": "강아지 꼬리 흔드는 것처럼",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.32",
          "form": "반기는 느낌",
          "original_form": "반기는 느낌",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.33",
          "form": "그 반기는 듯한?",
          "original_form": "그 반기는 듯한?",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.34",
          "form": "느낌 주는게 뭔가 정이가요",
          "original_form": "느낌 주는게 뭔가 정이가요",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.35",
          "form": "물고기라 당연히 꼬리지느러미가 움직이는 건데도",
          "original_form": "물고기라 당연히 꼬리지느러미가 움직이는 건데도",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.36",
          "form": "꼭 강아지가 반기는 듯해서 정이갑니다",
          "original_form": "꼭 강아지가 반기는 듯해서 정이갑니다",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.37",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033133.1.38",
          "form": "지금 한 100마리 가까이 있답니다",
          "original_form": "지금 한 100마리 가까이 있답니다",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033133.1.39",
          "form": "처음엔 10마리였지요",
          "original_form": "처음엔 10마리였지요",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033133.1.40",
          "form": "오.. 많네요",
          "original_form": "오.. 많네요",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033133.1.41",
          "form": "구피가 번식력이 꽤 좋더군요",
          "original_form": "구피가 번식력이 꽤 좋더군요",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100033133.1.42",
          "form": "맞아요 번식력이 어마어마하긴해서..",
          "original_form": "맞아요 번식력이 어마어마하긴해서..",
          "speaker_id": "2",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100033133.1.43",
          "form": "그래서 주변에 나눠주기도 하는데",
          "original_form": "그래서 주변에 나눠주기도 하는데",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033133.1.44",
          "form": "그래도 꽤 수가 늡니다",
          "original_form": "그래도 꽤 수가 늡니다",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033133.1.45",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033133.1.46",
          "form": "고양이는 한마리 키우고 있는 거지요?",
          "original_form": "고양이는 한마리 키우고 있는 거지요?",
          "speaker_id": "1",
          "time": "20210906 17:48"
        }
      ]
