      "utterance": [
        {
          "id": "MDRW2100006620.1.1",
          "form": "강아지 키우고싶다",
          "original_form": "강아지 키우고싶다",
          "speaker_id": "1",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100006620.1.2",
          "form": "당장",
          "original_form": "당장",
          "speaker_id": "1",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100006620.1.3",
          "form": "너가 강이지야",
          "original_form": "너가 강이지야",
          "speaker_id": "2",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100006620.1.4",
          "form": "..?",
          "original_form": "..?",
          "speaker_id": "1",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100006620.1.5",
          "form": "강아지 입양",
          "original_form": "강아지 입양",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006620.1.6",
          "form": "쉽게 할수있는 곳이 있었으몀 좋겠어",
          "original_form": "쉽게 할수있는 곳이 있었으몀 좋겠어",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006620.1.7",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006620.1.8",
          "form": "요새 너무",
          "original_form": "요새 너무",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006620.1.9",
          "form": "생각이 드는게",
          "original_form": "생각이 드는게",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100006620.1.10",
          "form": "돈으로 사면 안되는거잖아 생명을",
          "original_form": "돈으로 사면 안되는거잖아 생명을",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006620.1.11",
          "form": "근데 입양하기 너무 힘들어 펫샵말고는",
          "original_form": "근데 입양하기 너무 힘들어 펫샵말고는",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006620.1.12",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006620.1.13",
          "form": "다른나라는 뭐가 많다던데",
          "original_form": "다른나라는 뭐가 많다던데",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006620.1.14",
          "form": "우린 아직 멀었다",
          "original_form": "우린 아직 멀었다",
          "speaker_id": "2",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100006620.1.15",
          "form": "정부가 제대로 법으로 제정해서 입양을 제대로 할수있게",
          "original_form": "정부가 제대로 법으로 제정해서 입양을 제대로 할수있게",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006620.1.16",
          "form": "했으면 좋겠당",
          "original_form": "했으면 좋겠당",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006620.1.17",
          "form": "그러면 강아지들도 행복할거야",
          "original_form": "그러면 강아지들도 행복할거야",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100006620.1.18",
          "form": "맞이",
          "original_form": "맞이",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.19",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.20",
          "form": "강아지 키우고싶다 진짜",
          "original_form": "강아지 키우고싶다 진짜",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.21",
          "form": "ㄱ근데 강아지를 키우기에는 아직 준비가 안됐어",
          "original_form": "ㄱ근데 강아지를 키우기에는 아직 준비가 안됐어",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.22",
          "form": "마음의 준비를 잘하고",
          "original_form": "마음의 준비를 잘하고",
          "speaker_id": "1",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.23",
          "form": "입양해와야하는데",
          "original_form": "입양해와야하는데",
          "speaker_id": "1",
          "time": "20210908 03:32"
