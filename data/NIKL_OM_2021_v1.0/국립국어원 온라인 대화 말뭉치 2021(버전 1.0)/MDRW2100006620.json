        {
          "id": "MDRW2100006620.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.25",
          "form": "강아지 입양해와서도 내가 다 키우지",
          "original_form": "강아지 입양해와서도 내가 다 키우지",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.26",
          "form": "4명 키우는거야",
          "original_form": "4명 키우는거야",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100006620.1.27",
          "form": "강아지 키울수있을만큼",
          "original_form": "강아지 키울수있을만큼",
          "speaker_id": "1",
          "time": "20210908 03:33"
        },
