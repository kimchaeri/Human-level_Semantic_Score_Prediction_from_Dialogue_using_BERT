      "utterance": [
        {
          "id": "MDRW2100037455.1.1",
          "form": "name1 님 점심 뭐 드시나요?",
          "original_form": "&name1& 님 점심 뭐 드시나요?",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100037455.1.2",
          "form": "저는 만두국 먹었어요ㅋㅋㅋㅋ",
          "original_form": "저는 만두국 먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037455.1.3",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037455.1.4",
          "form": "전 집에 있는 반찬 꺼내서 먹었어요",
          "original_form": "전 집에 있는 반찬 꺼내서 먹었어요",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100037455.1.5",
          "form": "어떤 반찬 드셨어요?",
          "original_form": "어떤 반찬 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100037455.1.6",
          "form": "가지볶음이랑 어묵볶음 그리고 이것저것 먹었어요ㅋㅋ",
          "original_form": "가지볶음이랑 어묵볶음 그리고 이것저것 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100037455.1.7",
          "form": "저녁도 똑같이 먹고 있어요",
          "original_form": "저녁도 똑같이 먹고 있어요",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100037455.1.8",
          "form": "오 어묵볶음 맛있겠다",
          "original_form": "오 어묵볶음 맛있겠다",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037455.1.9",
          "form": "가지 좋아하세요?",
          "original_form": "가지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037455.1.10",
          "form": "네ㅋㅋ 요즘 가지볶음에 꽂혔어요",
          "original_form": "네ㅋㅋ 요즘 가지볶음에 꽂혔어요",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037455.1.11",
          "form": "아 ㅋㅋㅋㅋ가지 좋아하시능구나",
          "original_form": "아 ㅋㅋㅋㅋ가지 좋아하시능구나",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037455.1.12",
          "form": "가지요리 생각보다 엄청 많더라구요!",
          "original_form": "가지요리 생각보다 엄청 많더라구요!",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037455.1.13",
          "form": "그쵸 가지파스타 이런 것도 있던데요",
          "original_form": "그쵸 가지파스타 이런 것도 있던데요",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100037455.1.14",
          "form": "네네 신기해요 ㅋㅋㅋㅋ저는 가지는 안좋아해서요...ㅋㅋㅋㅋ",
          "original_form": "네네 신기해요 ㅋㅋㅋㅋ저는 가지는 안좋아해서요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037455.1.15",
          "form": "ㅋㅋㅋ전 가지가 왜 이렇게 좋은지 모르겠어요 생가지도 먹어본 적 있는데 맛있더라고요",
          "original_form": "ㅋㅋㅋ전 가지가 왜 이렇게 좋은지 모르겠어요 생가지도 먹어본 적 있는데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037455.1.16",
          "form": "생가지두요??ㅋㅋㅋㅋ",
          "original_form": "생가지두요??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037455.1.17",
          "form": "진짜 취향에 맞으시나봐요 ㅋㅋㅋ",
          "original_form": "진짜 취향에 맞으시나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037455.1.18",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100037455.1.19",
          "form": "또 안 좋아하거나 안 먹는 거 있으세요?",
          "original_form": "또 안 좋아하거나 안 먹는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100037455.1.20",
          "form": "저는 비린거 못먹어요 ㅠㅠ",
          "original_form": "저는 비린거 못먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100037455.1.21",
          "form": "그럼 해산물 잘 못 드세요?",
          "original_form": "그럼 해산물 잘 못 드세요?",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100037455.1.22",
          "form": "네네 익힌건 좀 먹지만 날거잘 못먹어요!",
          "original_form": "네네 익힌건 좀 먹지만 날거잘 못먹어요!",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100037455.1.23",
          "form": "저도 날음식은 별로더라고요 이걸 굳이 왜 먹나 싶어요ㅋㅋ",
          "original_form": "저도 날음식은 별로더라고요 이걸 굳이 왜 먹나 싶어요ㅋㅋ",
          "speaker_id": "2",
