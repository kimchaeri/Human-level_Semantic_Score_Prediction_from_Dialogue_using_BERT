        {
          "id": "MDRW2100021019.1.6",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021019.1.7",
          "form": "그냥 티빙 결제하는거보다",
          "original_form": "그냥 티빙 결제하는거보다",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021019.1.8",
          "form": "싸요 ㅎㅎ",
          "original_form": "싸요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021019.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021019.1.10",
          "form": "꿀팁이네요!!",
          "original_form": "꿀팁이네요!!",
          "speaker_id": "1",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021019.1.11",
          "form": "그래서 티빙볼때는 멤버십 결제하고",
          "original_form": "그래서 티빙볼때는 멤버십 결제하고",
          "speaker_id": "2",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100021019.1.12",
          "form": "멤버십 혜택도 받고",
          "original_form": "멤버십 혜택도 받고",
          "speaker_id": "2",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100021019.1.13",
          "form": "일석이조예요",
          "original_form": "일석이조예요",
          "speaker_id": "2",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100021019.1.14",
          "form": "오 그러네요 ㅋㅋㅋㅋ",
          "original_form": "오 그러네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021019.1.15",
          "form": "그럼 매번 결제하세요?",
          "original_form": "그럼 매번 결제하세요?",
          "speaker_id": "1",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021019.1.16",
          "form": "아뇨 매번은 아니고",
          "original_form": "아뇨 매번은 아니고",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021019.1.17",
          "form": "보고싶은 프로그램 있을때나",
          "original_form": "보고싶은 프로그램 있을때나",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021019.1.18",
          "form": "아님 좀 비싼거 시킬때만요",
          "original_form": "아님 좀 비싼거 시킬때만요",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021019.1.19",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100021019.1.20",
          "form": "하긴 그래야 더 이득이죠",
          "original_form": "하긴 그래야 더 이득이죠",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100021019.1.21",
          "form": "name1님은 매번 결제하세요?",
          "original_form": "&name1&님은 매번 결제하세요?",
          "speaker_id": "2",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100021019.1.22",
          "form": "저는 결제는 안하구",
          "original_form": "저는 결제는 안하구",
