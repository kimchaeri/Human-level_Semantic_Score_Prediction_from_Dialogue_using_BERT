      "utterance": [
        {
          "id": "MDRW2100014740.1.1",
          "form": "사고싶은거 있나요?",
          "original_form": "사고싶은거 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014740.1.2",
          "form": "당근마켓 알아요?",
          "original_form": "당근마켓 알아요?",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014740.1.3",
          "form": "네 당근 알지요!",
          "original_form": "네 당근 알지요!",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014740.1.4",
          "form": "구경만하고 아직 거래는 안 해 봤어요!",
          "original_form": "구경만하고 아직 거래는 안 해 봤어요!",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014740.1.5",
          "form": "저도 구경만해봤어요  티비에서 당근당근하길래",
          "original_form": "저도 구경만해봤어요  티비에서 당근당근하길래",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014740.1.6",
          "form": "진짜 없는 게 없더라고요",
          "original_form": "진짜 없는 게 없더라고요",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014740.1.7",
          "form": "요즘은 중고도 괜찮아서!",
          "original_form": "요즘은 중고도 괜찮아서!",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014740.1.8",
          "form": "보면 혹해서  사고 싶어질까봐요 ㅋ",
          "original_form": "보면 혹해서  사고 싶어질까봐요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014740.1.9",
          "form": "백조라서...",
          "original_form": "백조라서...",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014740.1.10",
          "form": "왠만하면 알뜰히살아요",
          "original_form": "왠만하면 알뜰히살아요",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014740.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014740.1.12",
          "form": "다 사고 싶기는 하더라고요 정말!",
          "original_form": "다 사고 싶기는 하더라고요 정말!",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014740.1.13",
          "form": "운동기구에 혹한적은잇어요 ㅋ",
          "original_form": "운동기구에 혹한적은잇어요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014740.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014740.1.15",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014740.1.16",
          "form": "어떤 기구요???",
          "original_form": "어떤 기구요???",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014740.1.17",
          "form": "다이어트 해야지하면서 ㅋㅋ봣어요",
          "original_form": "다이어트 해야지하면서 ㅋㅋ봣어요",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014740.1.18",
          "form": "승마기계가신기하긴하더라구요",
          "original_form": "승마기계가신기하긴하더라구요",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014740.1.19",
          "form": "아님 싸이클",
          "original_form": "아님 싸이클",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014740.1.20",
          "form": "오!! 맞아요 그런 거 사람들이 중고로 많이 파는 것 같아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오!! 맞아요 그런 거 사람들이 중고로 많이 파는 것 같아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014740.1.21",
          "form": "저는 요즘 책장?",
          "original_form": "저는 요즘 책장?",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014740.1.22",
          "form": "수납이요",
          "original_form": "수납이요",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014740.1.23",
          "form": "책이많으시그나",
          "original_form": "책이많으시그나",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014740.1.24",
          "form": "틈새에 딱 맞는 책장이 없어서 찾고 있거든요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "틈새에 딱 맞는 책장이 없어서 찾고 있거든요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014740.1.25",
          "form": "잘찾아보면 있을거같은데요",
          "original_form": "잘찾아보면 있을거같은데요",
          "speaker_id": "1",
          "time": "20210825 20:56"
        }
      ]
    }
