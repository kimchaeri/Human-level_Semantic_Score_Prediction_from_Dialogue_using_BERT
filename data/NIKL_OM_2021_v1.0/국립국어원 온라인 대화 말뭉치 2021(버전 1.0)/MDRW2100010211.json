      "utterance": [
        {
          "id": "MDRW2100010211.1.1",
          "form": "여긴 광주인데 비가와요~",
          "original_form": "여긴 광주인데 비가와요~",
          "speaker_id": "2",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010211.1.2",
          "form": "거기날씬어떤가요?",
          "original_form": "거기날씬어떤가요?",
          "speaker_id": "2",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010211.1.3",
          "form": "여기도 비오고 있어요",
          "original_form": "여기도 비오고 있어요",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010211.1.4",
          "form": "태풍이 온다더라고요",
          "original_form": "태풍이 온다더라고요",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010211.1.5",
          "form": "남부지방은 더 많이 올거라던데",
          "original_form": "남부지방은 더 많이 올거라던데",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010211.1.6",
          "form": "ㅜㅠ 일기예보를 못봤네요",
          "original_form": "ㅜㅠ 일기예보를 못봤네요",
          "speaker_id": "2",
          "time": "20210823 15:40"
        },
        {
          "id": "MDRW2100010211.1.7",
          "form": "날씨가 애매해서 옷은 뭘 입어얄지모르겠네요?!~\r\n긴팔입었나요?",
          "original_form": "날씨가 애매해서 옷은 뭘 입어얄지모르겠네요?!~\r\n긴팔입었나요?",
          "speaker_id": "2",
          "time": "20210823 15:40"
        },
        {
          "id": "MDRW2100010211.1.8",
          "form": "썰렁하네요",
          "original_form": "썰렁하네요",
          "speaker_id": "1",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100010211.1.9",
          "form": "저는 반팔 입고 나왔는데",
          "original_form": "저는 반팔 입고 나왔는데",
          "speaker_id": "1",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100010211.1.10",
          "form": "오늘이 처서에요",
          "original_form": "오늘이 처서에요",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.11",
          "form": "여름도 다 갔네요~~^^",
          "original_form": "여름도 다 갔네요~~^^",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.12",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.13",
          "form": "이제 곧 가을이 오겠어요",
          "original_form": "이제 곧 가을이 오겠어요",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.14",
          "form": "담주부턴 긴팔입어도될것같아요",
          "original_form": "담주부턴 긴팔입어도될것같아요",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.15",
          "form": "이번에 태풍오고 가을장마 예보도 있던데",
          "original_form": "이번에 태풍오고 가을장마 예보도 있던데",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.16",
          "form": "곧 쌀쌀해질 듯해요",
          "original_form": "곧 쌀쌀해질 듯해요",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010211.1.17",
          "form": "이번주내내 비온다하더라구요",
          "original_form": "이번주내내 비온다하더라구요",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010211.1.18",
          "form": "네 그렇다고 하더라고요",
          "original_form": "네 그렇다고 하더라고요",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010211.1.19",
          "form": "미세먼지는 어느새 관심밖이네요ㅎㅎ",
          "original_form": "미세먼지는 어느새 관심밖이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010211.1.20",
          "form": "네 미세먼지는 봄에 특히 심한데",
          "original_form": "네 미세먼지는 봄에 특히 심한데",
          "speaker_id": "1",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010211.1.21",
          "form": "올해는 큰 소란 없이 지나간듯요",
          "original_form": "올해는 큰 소란 없이 지나간듯요",
