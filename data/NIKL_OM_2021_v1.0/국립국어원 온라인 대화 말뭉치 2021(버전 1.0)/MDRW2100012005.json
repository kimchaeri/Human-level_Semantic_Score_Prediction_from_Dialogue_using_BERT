      "utterance": [
        {
          "id": "MDRW2100012005.1.1",
          "form": "오늘 저녁뭐드실거에요?",
          "original_form": "오늘 저녁뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012005.1.2",
          "form": "전 김치전이요~~ㅎㅎ",
          "original_form": "전 김치전이요~~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012005.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 18:25"
        },
        {
          "id": "MDRW2100012005.1.4",
          "form": "오 지금날씨에 딱이네요",
          "original_form": "오 지금날씨에 딱이네요",
          "speaker_id": "2",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100012005.1.5",
          "form": "전 오늘 떡볶이를해먹을지 샐러드를먹을지 고민이에요ㅜ",
          "original_form": "전 오늘 떡볶이를해먹을지 샐러드를먹을지 고민이에요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100012005.1.6",
          "form": "ㅋㅋㅋ이제야봤네요ㅎㅎ 그래서 뭐드셨는지 궁금해요!",
          "original_form": "ㅋㅋㅋ이제야봤네요ㅎㅎ 그래서 뭐드셨는지 궁금해요!",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012005.1.7",
          "form": "저 포도먹고 피자먹었어요..떡볶이 하기가 귀찮아서 ㅋㅋㅋㅋㅋ",
          "original_form": "저 포도먹고 피자먹었어요..떡볶이 하기가 귀찮아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012005.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋ그쵸 그런날이 있어요~~",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ그쵸 그런날이 있어요~~",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012005.1.9",
          "form": "무슨피자드셨어요?",
          "original_form": "무슨피자드셨어요?",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012005.1.10",
          "form": "전 어제 도미노먹었는데!",
          "original_form": "전 어제 도미노먹었는데!",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100012005.1.11",
          "form": "저 포테이토피자요 ㅎㅎ",
          "original_form": "저 포테이토피자요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012005.1.12",
          "form": "도미노맛있죠 도미노뭐드셨어요",
          "original_form": "도미노맛있죠 도미노뭐드셨어요",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012005.1.13",
          "form": "ㅋㅋㅋㅋ전 직화스테이크피자요!\r\n포테이토피자는 사실 별거 없는것같은데 포슬포슬한 감자가 너무 맛있어서 종종 생각나요",
          "original_form": "ㅋㅋㅋㅋ전 직화스테이크피자요!\r\n포테이토피자는 사실 별거 없는것같은데 포슬포슬한 감자가 너무 맛있어서 종종 생각나요",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012005.1.14",
          "form": "스테이크도 넘맛있져ㅜ 전도미노예전에 치케샌넘좋아했었는데",
          "original_form": "스테이크도 넘맛있져ㅜ 전도미노예전에 치케샌넘좋아했었는데",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012005.1.15",
          "form": "그맛이제 안나더라구요ㅜ",
          "original_form": "그맛이제 안나더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012005.1.16",
          "form": "맞아요ㅠㅠ 요즘 뭔가 옛날에 인기있었던 프랜차이즈 피자들이 그맛이 안나요",
          "original_form": "맞아요ㅠㅠ 요즘 뭔가 옛날에 인기있었던 프랜차이즈 피자들이 그맛이 안나요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012005.1.17",
          "form": "그래서 요즘 반올림이나 알볼로 이런곳에서도 많이 시키게돼요",
          "original_form": "그래서 요즘 반올림이나 알볼로 이런곳에서도 많이 시키게돼요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012005.1.18",
          "form": "맞아요 가성비좋은 피자집들 많이생겨서요",
          "original_form": "맞아요 가성비좋은 피자집들 많이생겨서요",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012005.1.19",
          "form": "네!! 맛도 맛있고~괜찮은것같아요",
          "original_form": "네!! 맛도 맛있고~괜찮은것같아요",
