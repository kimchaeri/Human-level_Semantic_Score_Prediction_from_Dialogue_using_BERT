      "utterance": [
        {
          "id": "MDRW2100021128.1.1",
          "form": "저는 공차가 먹고싶어요....",
          "original_form": "저는 공차가 먹고싶어요....",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021128.1.2",
          "form": "제 소울푸드예요...",
          "original_form": "제 소울푸드예요...",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021128.1.3",
          "form": "넹 ㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:42"
        },
        {
          "id": "MDRW2100021128.1.4",
          "form": "아하 오 버블티좋아하시나봐여??",
          "original_form": "아하 오 버블티좋아하시나봐여??",
          "speaker_id": "1",
          "time": "20210829 06:43"
        },
        {
          "id": "MDRW2100021128.1.5",
          "form": "네 맞아요!!!",
          "original_form": "네 맞아요!!!",
          "speaker_id": "2",
          "time": "20210829 09:05"
        },
        {
          "id": "MDRW2100021128.1.6",
          "form": "씹히는거를 좋아해요 ㅎㅎ",
          "original_form": "씹히는거를 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 09:05"
        },
        {
          "id": "MDRW2100021128.1.7",
          "form": "그냥 각얼음만 들어있는 음료는 점점 싱거워지더라구요",
          "original_form": "그냥 각얼음만 들어있는 음료는 점점 싱거워지더라구요",
          "speaker_id": "2",
          "time": "20210829 09:05"
        },
        {
          "id": "MDRW2100021128.1.8",
          "form": "name1님은 버블티 좋아하시나요?",
          "original_form": "&name1&님은 버블티 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 09:06"
        },
        {
          "id": "MDRW2100021128.1.9",
          "form": "맞죠맞죠,, 전 안타깝게도 버블티 안좋아해요,, 타피오카펄먹으면 속이 울렁거려서 ㅠ",
          "original_form": "맞죠맞죠,, 전 안타깝게도 버블티 안좋아해요,, 타피오카펄먹으면 속이 울렁거려서 ㅠ",
          "speaker_id": "1",
          "time": "20210830 10:31"
        },
        {
          "id": "MDRW2100021128.1.10",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100021128.1.11",
          "form": "근데 일리 있는 현상이에요,,",
          "original_form": "근데 일리 있는 현상이에요,,",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100021128.1.12",
          "form": "이게 괴담인지는 모르겠는데",
          "original_form": "이게 괴담인지는 모르겠는데",
          "speaker_id": "2",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100021128.1.13",
          "form": "타피오카펄 많이 먹으면 위암 걸릴 수 있다고 하더라구요ㅠ",
          "original_form": "타피오카펄 많이 먹으면 위암 걸릴 수 있다고 하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100021128.1.14",
          "form": "허걱 위암이라니..ㄷㄷ",
          "original_form": "허걱 위암이라니..ㄷㄷ",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100021128.1.15",
          "form": "근데 알로에펄은 또 괜찮더라구요",
          "original_form": "근데 알로에펄은 또 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100021128.1.16",
          "form": "오오 알로에펄도 맛있죠",
          "original_form": "오오 알로에펄도 맛있죠",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100021128.1.17",
          "form": "저는 공차 화이트펄도 좋아해요!!!",
          "original_form": "저는 공차 화이트펄도 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100021128.1.18",
          "form": "가끔 타피오카 먹기 찝찝할 땐 화이트펄을 먹는답니다 ㅎㅎ",
          "original_form": "가끔 타피오카 먹기 찝찝할 땐 화이트펄을 먹는답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100021128.1.19",
          "form": "아하~ 입맛에맞기만하면 번갈아드시면 좋죠",
          "original_form": "아하~ 입맛에맞기만하면 번갈아드시면 좋죠",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100021128.1.20",
          "form": "보통 어떤메뉴시키시나요?",
          "original_form": "보통 어떤메뉴시키시나요?",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100021128.1.21",
          "form": "저는 공차 제주그린티 스무디가 제 소울푸드예요!!!",
          "original_form": "저는 공차 제주그린티 스무디가 제 소울푸드예요!!!",
          "speaker_id": "2",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100021128.1.22",
          "form": "여기에 밀크폼이 얹어져 나오는데",
          "original_form": "여기에 밀크폼이 얹어져 나오는데",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100021128.1.23",
          "form": "짭조름하면서... 정말 맛이 좋아요...!!!",
          "original_form": "짭조름하면서... 정말 맛이 좋아요...!!!",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100021128.1.24",
          "form": "오 녹차좋아하시는군요!!! 전 안타깝게도 녹차를 싫어해서 ㅠㅠ 아 저 치즈폼좋아해여~ㅋㅋㅋㅋ",
          "original_form": "오 녹차좋아하시는군요!!! 전 안타깝게도 녹차를 싫어해서 ㅠㅠ 아 저 치즈폼좋아해여~ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:08"
        },
        {
          "id": "MDRW2100021128.1.25",
          "form": "아아 녹차를 싫어하시는구나 8ㅅ8",
          "original_form": "아아 녹차를 싫어하시는구나 8ㅅ8",
          "speaker_id": "2",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100021128.1.26",
          "form": "치즈폼!!!!",
          "original_form": "치즈폼!!!!",
          "speaker_id": "2",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100021128.1.27",
          "form": "공차에 치즈폼도 있었군요",
          "original_form": "공차에 치즈폼도 있었군요",
          "speaker_id": "2",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100021128.1.28",
          "form": "저 왜 몰랐지,,, 분명히 제가 좋아할 맛일거예요",
          "original_form": "저 왜 몰랐지,,, 분명히 제가 좋아할 맛일거예요",
          "speaker_id": "2",
          "time": "20210830 11:09"
        },
        {
          "id": "MDRW2100021128.1.29",
          "form": "그럼 혹시 민초는 좋아하시나요???",
          "original_form": "그럼 혹시 민초는 좋아하시나요???",
          "speaker_id": "2",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100021128.1.30",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ넹ㅋㅋ 근데정식명칭이 저게맞나몰라요..",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ넹ㅋㅋ 근데정식명칭이 저게맞나몰라요..",
