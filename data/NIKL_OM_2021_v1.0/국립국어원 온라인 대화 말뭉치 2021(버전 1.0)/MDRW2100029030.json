      "utterance": [
        {
          "id": "MDRW2100029030.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029030.1.2",
          "form": "저 쿠키런만 해요",
          "original_form": "저 쿠키런만 해요",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100029030.1.3",
          "form": "전 브롤해요",
          "original_form": "전 브롤해요",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029030.1.4",
          "form": "브롤이 뭐예요...?",
          "original_form": "브롤이 뭐예요...?",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029030.1.5",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029030.1.6",
          "form": "아 제가 요즘 초등학생들은 뭐 하는질 잘 몰라서요ㅠ",
          "original_form": "아 제가 요즘 초등학생들은 뭐 하는질 잘 몰라서요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029030.1.7",
          "form": "뭐하는 게임이에요?",
          "original_form": "뭐하는 게임이에요?",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029030.1.8",
          "form": "총쏘는 단순해여",
          "original_form": "총쏘는 단순해여",
          "speaker_id": "1",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029030.1.9",
          "form": "아하 배그같은건가요?",
          "original_form": "아하 배그같은건가요?",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029030.1.10",
          "form": "것보더 쉬운",
          "original_form": "것보더 쉬운",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029030.1.11",
          "form": "오오 그렇군요 처음들어봤어요",
          "original_form": "오오 그렇군요 처음들어봤어요",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029030.1.12",
          "form": "제 사촌동생은 롤토체스? 롤투체스? 하더라구요",
          "original_form": "제 사촌동생은 롤토체스? 롤투체스? 하더라구요",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029030.1.13",
          "form": "먼지? ㅋㅋ",
          "original_form": "먼지? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029030.1.14",
          "form": "폰으로 하는 롤 같던데요",
          "original_form": "폰으로 하는 롤 같던데요",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
