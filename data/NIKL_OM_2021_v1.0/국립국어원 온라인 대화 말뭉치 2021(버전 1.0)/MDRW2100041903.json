      "utterance": [
        {
          "id": "MDRW2100041903.1.1",
          "form": "name3 엄마는 name4이 그거 카톡지우고",
          "original_form": "&name3& 엄마는 &name4&이 그거 카톡지우고",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.2",
          "form": "애 사이가 벌어지면",
          "original_form": "애 사이가 벌어지면",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.3",
          "form": "게임지운거에",
          "original_form": "게임지운거에",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.4",
          "form": "엄청 화난거 같더라고",
          "original_form": "엄청 화난거 같더라고",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.5",
          "form": "끝나는 거 같은ㅠ",
          "original_form": "끝나는 거 같은ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.6",
          "form": "근데 폰 만지면 뭐 어플 치울 수 있잖아요",
          "original_form": "근데 폰 만지면 뭐 어플 치울 수 있잖아요",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.7",
          "form": "언니가 화내는 스타일이 아니라 내한테는 아무말안하는데",
          "original_form": "언니가 화내는 스타일이 아니라 내한테는 아무말안하는데",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041903.1.8",
          "form": "name3 아빠가 신랑한테",
          "original_form": "&name3& 아빠가 신랑한테",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.9",
          "form": "일부러 물어 못하게 할라고 한 것도 아닌데",
          "original_form": "일부러 물어 못하게 할라고 한 것도 아닌데",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.10",
          "form": "화는 나겠지만",
          "original_form": "화는 나겠지만",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.11",
          "form": "응 근데 일부러 지운거라.....쩝....ㅋㅋㅋㅋㅋㄱ",
          "original_form": "응 근데 일부러 지운거라.....쩝....ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.12",
          "form": "그 정도로 갈 건 아닌 듯싶은데",
          "original_form": "그 정도로 갈 건 아닌 듯싶은데",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.13",
          "form": "우리가 계속 사과했지 뭐 미안하다고",
          "original_form": "우리가 계속 사과했지 뭐 미안하다고",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.14",
          "form": "그니까요 사과했잖아요~",
          "original_form": "그니까요 사과했잖아요~",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.15",
          "form": "엉 사과하고 했는데",
          "original_form": "엉 사과하고 했는데",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.16",
          "form": "이미 뭐 지운거 어떡하노~",
          "original_form": "이미 뭐 지운거 어떡하노~",
          "speaker_id": "1",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.17",
          "form": "다음날 또 name3가 우니까",
          "original_form": "다음날 또 &name3&가 우니까",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.18",
          "form": "name4이만 나쁜놈이지",
          "original_form": "&name4&이만 나쁜놈이지",
          "speaker_id": "2",
          "time": "20210906 17:24"
        },
        {
          "id": "MDRW2100041903.1.19",
          "form": "그 부모들은 자기 자식이 일방적으로 당한다 생각하는 듯",
          "original_form": "그 부모들은 자기 자식이 일방적으로 당한다 생각하는 듯",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041903.1.20",
          "form": "같이 놀 때 본 적이 잘 없잖아요",
          "original_form": "같이 놀 때 본 적이 잘 없잖아요",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041903.1.21",
          "form": "언니가 거의 옆에 있었고",
          "original_form": "언니가 거의 옆에 있었고",
          "speaker_id": "1",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041903.1.22",
          "form": "그래도 캠핑 가서 노는 거 보면",
          "original_form": "그래도 캠핑 가서 노는 거 보면",
