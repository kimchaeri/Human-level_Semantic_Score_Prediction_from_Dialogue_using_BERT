      "utterance": [
        {
          "id": "MDRW2100022278.1.1",
          "form": "감바스는어땠나요",
          "original_form": "감바스는어땠나요",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.2",
          "form": "감바스 맛있었어요!",
          "original_form": "감바스 맛있었어요!",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.3",
          "form": "근데 새우가 좀 작더라구요",
          "original_form": "근데 새우가 좀 작더라구요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.4",
          "form": "그냥 하셔도 되는데..",
          "original_form": "그냥 하셔도 되는데..",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.5",
          "form": "새우가 작구나",
          "original_form": "새우가 작구나",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.6",
          "form": "칵테일 새우로 감바스를 하다니",
          "original_form": "칵테일 새우로 감바스를 하다니",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.7",
          "form": "이건 좀 아니였어요",
          "original_form": "이건 좀 아니였어요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.8",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.9",
          "form": "새우의 대한 모독이다",
          "original_form": "새우의 대한 모독이다",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.10",
          "form": "좀 탱글탱글해야되지 않나요?ㅠㅠ",
          "original_form": "좀 탱글탱글해야되지 않나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.12",
          "form": "타이거새우는 넣어줘야죠",
          "original_form": "타이거새우는 넣어줘야죠",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.13",
          "form": "그 새우중에 가성비넘치는..",
          "original_form": "그 새우중에 가성비넘치는..",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.14",
          "form": "앗 타이거새우라니",
          "original_form": "앗 타이거새우라니",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022278.1.15",
          "form": "생각도 못했다",
          "original_form": "생각도 못했다",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.16",
          "form": "ㅠㅠ칵테일새우는 너무하잖아요",
          "original_form": "ㅠㅠ칵테일새우는 너무하잖아요",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.17",
          "form": "이건 볶음밥용 아닌가요?",
          "original_form": "이건 볶음밥용 아닌가요?",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.18",
          "form": "볶음밥용이라니요",
          "original_form": "볶음밥용이라니요",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.19",
          "form": "볶음밥에대한 모독입니다",
          "original_form": "볶음밥에대한 모독입니다",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.20",
          "form": "근데 제 친구는 감바스 처음 먹어봐서 만족하더라구요",
          "original_form": "근데 제 친구는 감바스 처음 먹어봐서 만족하더라구요",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.21",
          "form": "그건 나물무칠 때나 쓰는거에요",
          "original_form": "그건 나물무칠 때나 쓰는거에요",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.22",
          "form": "ㅋㅋㅋ마늘빵에 감바스 먹고싶네요",
          "original_form": "ㅋㅋㅋ마늘빵에 감바스 먹고싶네요",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.23",
          "form": "ㅋㅋㅋㅋㄱ 아시조 호박에 넣는 새우",
          "original_form": "ㅋㅋㅋㅋㄱ 아시조 호박에 넣는 새우",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022278.1.24",
          "form": "아 그거 알아요 ㅋㅋㅋ",
          "original_form": "아 그거 알아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:59"
        }
      ]
