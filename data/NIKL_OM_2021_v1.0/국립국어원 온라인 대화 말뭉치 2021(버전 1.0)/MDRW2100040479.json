      "utterance": [
        {
          "id": "MDRW2100040479.1.1",
          "form": "저는 어제 백신접종 하고나니 단 게 당겨서",
          "original_form": "저는 어제 백신접종 하고나니 단 게 당겨서",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100040479.1.2",
          "form": "오늘 아이스크림을 2개나 먹었어요",
          "original_form": "오늘 아이스크림을 2개나 먹었어요",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100040479.1.3",
          "form": "오홍 음식.당긴다하더니 사실인가봐요",
          "original_form": "오홍 음식.당긴다하더니 사실인가봐요",
          "speaker_id": "1",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100040479.1.4",
          "form": "네 식욕이 왕성해지네요",
          "original_form": "네 식욕이 왕성해지네요",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100040479.1.5",
          "form": "저도 곧 맞아서 두근두근",
          "original_form": "저도 곧 맞아서 두근두근",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100040479.1.6",
          "form": "접종 전에 돼지고기 먹음 덜 아프대서",
          "original_form": "접종 전에 돼지고기 먹음 덜 아프대서",
          "speaker_id": "2",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100040479.1.7",
          "form": "고기도 챙겨먹었어요",
          "original_form": "고기도 챙겨먹었어요",
          "speaker_id": "2",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100040479.1.8",
          "form": "오홍 메모메모  챙겨 먹어야지",
          "original_form": "오홍 메모메모  챙겨 먹어야지",
          "speaker_id": "1",
          "time": "20210904 21:41"
        },
        {
          "id": "MDRW2100040479.1.9",
          "form": "네 맞기 전에 삼겹살!",
          "original_form": "네 맞기 전에 삼겹살!",
          "speaker_id": "2",
          "time": "20210904 21:42"
        },
        {
          "id": "MDRW2100040479.1.10",
          "form": "오혼 삼겹 파티",
          "original_form": "오혼 삼겹 파티",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100040479.1.11",
          "form": "네 삼겹살파티",
          "original_form": "네 삼겹살파티",
          "speaker_id": "2",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100040479.1.12",
          "form": "다이어트 해야하는데",
          "original_form": "다이어트 해야하는데",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100040479.1.13",
          "form": "더 잘먹게 생겻네요 ㅋ",
          "original_form": "더 잘먹게 생겻네요 ㅋ",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100040479.1.14",
          "form": "백신접종 때는 잘 챙겨먹어야죠!!",
          "original_form": "백신접종 때는 잘 챙겨먹어야죠!!",
          "speaker_id": "2",
          "time": "20210904 21:45"
        },
        {
          "id": "MDRW2100040479.1.15",
          "form": "안 아플 수 있다면야!!",
          "original_form": "안 아플 수 있다면야!!",
          "speaker_id": "2",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100040479.1.16",
          "form": "그쵸 그래서 신나요 ㅎㄷㄷ 핑계거리가 생긴 ㅎㅎ",
          "original_form": "그쵸 그래서 신나요 ㅎㄷㄷ 핑계거리가 생긴 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:46"
        },
        {
          "id": "MDRW2100040479.1.17",
          "form": "ㅎㅎ핑계김에 식사 잘 챙겨드셔요",
