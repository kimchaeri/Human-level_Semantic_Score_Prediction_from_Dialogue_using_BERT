          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033962.1.3",
          "form": "마라탕에 뭐뭐 넣어 먹어?",
          "original_form": "마라탕에 뭐뭐 넣어 먹어?",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033962.1.4",
          "form": "나 건두부랑 숙주 꼭 추가해",
          "original_form": "나 건두부랑 숙주 꼭 추가해",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033962.1.5",
          "form": "나는 야채는 청경채",
          "original_form": "나는 야채는 청경채",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033962.1.6",
          "form": "청경채더 맛있지",
          "original_form": "청경채더 맛있지",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033962.1.7",
          "form": "고기는?",
          "original_form": "고기는?",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033962.1.8",
          "form": "소고기",
          "original_form": "소고기",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033962.1.9",
          "form": "근데 비싸서 요즘은 자주 안 추가 해",
          "original_form": "근데 비싸서 요즘은 자주 안 추가 해",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033962.1.10",
          "form": "나도 요즘에는 두부 많이 먹어",
          "original_form": "나도 요즘에는 두부 많이 먹어",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033962.1.11",
          "form": "옥수수면이랑 고구마떡도",
          "original_form": "옥수수면이랑 고구마떡도",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033962.1.12",
          "form": "나는 치즈떡!",
          "original_form": "나는 치즈떡!",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033962.1.13",
          "form": "치즈 떡도 좋아",
          "original_form": "치즈 떡도 좋아",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033962.1.14",
          "form": "나 햄도 좋아해",
          "original_form": "나 햄도 좋아해",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033962.1.15",
          "form": "맞아ㅜㅠ 목이버섯은 어때?",
          "original_form": "맞아ㅜㅠ 목이버섯은 어때?",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033962.1.16",
          "form": "난 취향 아니었어",
          "original_form": "난 취향 아니었어",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033962.1.17",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033962.1.18",
          "form": "진짜 맛있는데",
          "original_form": "진짜 맛있는데",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033962.1.19",
          "form": "그래도 백목이는 좀 괜찮았어",
          "original_form": "그래도 백목이는 좀 괜찮았어",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033962.1.20",
          "form": "다른 버섯 좋아하는 거 있어?",
          "original_form": "다른 버섯 좋아하는 거 있어?",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033962.1.21",
          "form": "새송이랑 팽이!",
          "original_form": "새송이랑 팽이!",
          "speaker_id": "1",
          "time": "20210825 17:07"
        }
      ]
