      "utterance": [
        {
          "id": "MDRW2100028697.1.1",
          "form": "해봤지 당연",
          "original_form": "해봤지 당연",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028697.1.2",
          "form": "구직 활동을 해 봐야 취직이 가능하지 참",
          "original_form": "구직 활동을 해 봐야 취직이 가능하지 참",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028697.1.3",
          "form": "구직 활동은 너므 자존감을 똘어뜨려",
          "original_form": "구직 활동은 너므 자존감을 똘어뜨려",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028697.1.4",
          "form": "떨어뜨려",
          "original_form": "떨어뜨려",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028697.1.5",
          "form": "맞아 면접이 제일 싫어",
          "original_form": "맞아 면접이 제일 싫어",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028697.1.6",
          "form": "낙하산이 부러워",
          "original_form": "낙하산이 부러워",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028697.1.7",
          "form": "나는 자기소개서가 싫어",
          "original_form": "나는 자기소개서가 싫어",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028697.1.8",
          "form": "나도 누가 대신 써줬으면",
          "original_form": "나도 누가 대신 써줬으면",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028697.1.9",
          "form": "너희 급여는 언제 오르니?",
          "original_form": "너희 급여는 언제 오르니?",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028697.1.10",
          "form": "내년 1월 협상할걸",
          "original_form": "내년 1월 협상할걸",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.11",
          "form": "많이 올라서 밥 사라",
          "original_form": "많이 올라서 밥 사라",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.12",
          "form": "늘 그랬듯이",
          "original_form": "늘 그랬듯이",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.13",
          "form": "그래 넌 언제 올라",
          "original_form": "그래 넌 언제 올라",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.14",
          "form": "연말상여 받아서 좋겠다",
          "original_form": "연말상여 받아서 좋겠다",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.15",
          "form": "우리도 내년",
          "original_form": "우리도 내년",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.16",
          "form": "협상이야 통보야?",
          "original_form": "협상이야 통보야?",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028697.1.17",
          "form": "상여금을 포함해도 니 월급보다 적어",
          "original_form": "상여금을 포함해도 니 월급보다 적어",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028697.1.18",
          "form": "내월급이 얼마인지 모르잖아",
          "original_form": "내월급이 얼마인지 모르잖아",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028697.1.19",
          "form": "우리는 협상 없어",
          "original_form": "우리는 협상 없어",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028697.1.20",
          "form": "대충 알아",
          "original_form": "대충 알아",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028697.1.21",
          "form": "너한테서 부자의 기운이 느껴져",
          "original_form": "너한테서 부자의 기운이 느껴져",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028697.1.22",
          "form": "협상 없으면 전직원 인상이 똑같아?",
          "original_form": "협상 없으면 전직원 인상이 똑같아?",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028697.1.23",
          "form": "그건 아닌 거 같아 개별적으로 더르게 인상되더라",
          "original_form": "그건 아닌 거 같아 개별적으로 더르게 인상되더라",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028697.1.24",
          "form": "연구소 직원 많이 올랐대",
          "original_form": "연구소 직원 많이 올랐대",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028697.1.25",
          "form": "너도 많이 오르면 좋겠다",
          "original_form": "너도 많이 오르면 좋겠다",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028697.1.26",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 21:29"
        }
      ]
    }
  ]
