      "utterance": [
        {
          "id": "MDRW2100041010.1.1",
          "form": "저녁드셨나요?ㅎㅎ",
          "original_form": "저녁드셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100041010.1.2",
          "form": "넹 ㅎㅎ 오늘은 삼교비 먹엇어요",
          "original_form": "넹 ㅎㅎ 오늘은 삼교비 먹엇어요",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100041010.1.3",
          "form": "와 맛있으셨겠네요",
          "original_form": "와 맛있으셨겠네요",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100041010.1.4",
          "form": "몸에 기름칠 좀 하셨겠는걸요?ㅎㅎ",
          "original_form": "몸에 기름칠 좀 하셨겠는걸요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100041010.1.5",
          "form": "네네 ㅋㅋ 낼부턴 다요트 흑흑",
          "original_form": "네네 ㅋㅋ 낼부턴 다요트 흑흑",
          "speaker_id": "1",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100041010.1.6",
          "form": "아아ㅠ",
          "original_form": "아아ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100041010.1.7",
          "form": "저도 늘 다요트 생각하는데",
          "original_form": "저도 늘 다요트 생각하는데",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100041010.1.8",
          "form": "이번 주말엔 백신접종 핑계로 잘먹었네요",
          "original_form": "이번 주말엔 백신접종 핑계로 잘먹었네요",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100041010.1.9",
          "form": "아 저도 ㅋㅋ 백식접종 ㅋㅋㅋ",
          "original_form": "아 저도 ㅋㅋ 백식접종 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100041010.1.10",
          "form": "화욜날인데 ㅋ",
          "original_form": "화욜날인데 ㅋ",
          "speaker_id": "1",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100041010.1.11",
          "form": "ㅎㅎ내일도 잘 드셔야겠네요",
          "original_form": "ㅎㅎ내일도 잘 드셔야겠네요",
