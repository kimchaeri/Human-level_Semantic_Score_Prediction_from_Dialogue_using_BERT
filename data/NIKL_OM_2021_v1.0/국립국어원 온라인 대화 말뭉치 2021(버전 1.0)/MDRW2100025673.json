      "utterance": [
        {
          "id": "MDRW2100025673.1.1",
          "form": "머가있죠 탸국음식은 ㅠㅠ",
          "original_form": "머가있죠 탸국음식은 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100025673.1.2",
          "form": "뿌빳퐁커리도 있고",
          "original_form": "뿌빳퐁커리도 있고",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025673.1.3",
          "form": "뚬얌꽁?!도 있고,,,",
          "original_form": "뚬얌꽁?!도 있고,,,",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025673.1.4",
          "form": "시큼한 음식이 많은 것 같아요 ㅎㅎㅎ",
          "original_form": "시큼한 음식이 많은 것 같아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025673.1.5",
          "form": "생소하네요.. 먹어본게없네요!ㅋㅋ",
          "original_form": "생소하네요.. 먹어본게없네요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100025673.1.6",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100025673.1.7",
          "form": "그럴 수 있어요!!!",
          "original_form": "그럴 수 있어요!!!",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100025673.1.8",
          "form": "그럼 name2님은 오늘 뭘 드셨나요???",
          "original_form": "그럼 &name2&님은 오늘 뭘 드셨나요???",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100025673.1.9",
          "form": "저오늘 떡볶이 먹었습니당",
          "original_form": "저오늘 떡볶이 먹었습니당",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100025673.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100025673.1.11",
          "form": "직접 해드셨나요?",
          "original_form": "직접 해드셨나요?",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100025673.1.12",
          "form": "아니요!!청년다방에서 친구랑 먹었어요!",
          "original_form": "아니요!!청년다방에서 친구랑 먹었어요!",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100025673.1.13",
          "form": "아시나용??",
          "original_form": "아시나용??",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100025673.1.14",
          "form": "우와!!!! 알죠알죠",
          "original_form": "우와!!!! 알죠알죠",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100025673.1.15",
          "form": "저 청년다방 떡볶이 진짜 좋아해요",
          "original_form": "저 청년다방 떡볶이 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025673.1.16",
          "form": "몇 번 못먹어봤지만...",
          "original_form": "몇 번 못먹어봤지만...",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025673.1.17",
          "form": "저도 오랜만에먹었어요 감튀가 맛있더라구요!",
          "original_form": "저도 오랜만에먹었어요 감튀가 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025673.1.18",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100025673.1.19",
          "form": "어떤 맛을 드셨나요?",
          "original_form": "어떤 맛을 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100025673.1.20",
          "form": "오늘은 치즈폭탄 치킨떡볶이릋먹었어요",
          "original_form": "오늘은 치즈폭탄 치킨떡볶이릋먹었어요",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100025673.1.21",
          "form": "우와 그런 맛도 있었군요",
          "original_form": "우와 그런 맛도 있었군요",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025673.1.22",
          "form": "저는 차돌박이만 먹어봤는데",
          "original_form": "저는 차돌박이만 먹어봤는데",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025673.1.23",
          "form": "정말 맛나더라구요!",
          "original_form": "정말 맛나더라구요!",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025673.1.24",
          "form": "차돌박이더 맛있어요",
          "original_form": "차돌박이더 맛있어요",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100025673.1.25",
          "form": "차돌은 자주먹어서 다른거 먹고싶어서 먹어봤는데 맛있더라구요",
          "original_form": "차돌은 자주먹어서 다른거 먹고싶어서 먹어봤는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 21:18"
        }
      ]
    }
