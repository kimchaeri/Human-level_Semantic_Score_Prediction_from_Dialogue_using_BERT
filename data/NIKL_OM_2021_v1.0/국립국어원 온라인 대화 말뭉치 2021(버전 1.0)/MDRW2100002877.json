      "utterance": [
        {
          "id": "MDRW2100002877.1.1",
          "form": "제 입맛에는 안 맞더라고요",
          "original_form": "제 입맛에는 안 맞더라고요",
          "speaker_id": "1",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002877.1.2",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002877.1.3",
          "form": "그럼 입맛에 맞았던 음식은 있었나요?",
          "original_form": "그럼 입맛에 맞았던 음식은 있었나요?",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002877.1.4",
          "form": "스테이크요 ㅎㅎㅎ",
          "original_form": "스테이크요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002877.1.5",
          "form": "고기는 항상 옳죠ㅎㅎ 어딜가나 맛있는 거 같아요",
          "original_form": "고기는 항상 옳죠ㅎㅎ 어딜가나 맛있는 거 같아요",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002877.1.6",
          "form": "디져트나 음료같은건 어땠어요?",
          "original_form": "디져트나 음료같은건 어땠어요?",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002877.1.7",
          "form": "제가 갔을때는 우기라서 과일이 좀 싱겁더라고요ㅠ",
          "original_form": "제가 갔을때는 우기라서 과일이 좀 싱겁더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002877.1.8",
          "form": "하필 우기에ㅠ",
          "original_form": "하필 우기에ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002877.1.9",
          "form": "비오면 싱거워지잖아요 원래 엄청단데",
          "original_form": "비오면 싱거워지잖아요 원래 엄청단데",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002877.1.10",
          "form": "좋아하는 열대과일 있으세요?",
          "original_form": "좋아하는 열대과일 있으세요?",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002877.1.11",
          "form": "저는 망고요!!ㅎㅎ 지원님은요?",
          "original_form": "저는 망고요!!ㅎㅎ 지원님은요?",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002877.1.12",
          "form": "헐 저도 망고 좋아해요",
          "original_form": "헐 저도 망고 좋아해요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002877.1.13",
          "form": "사실 망고를 그렇게 좋아하는 편은 아니었는데 생망고 먹어보고 생각이 바뀌었어요",
          "original_form": "사실 망고를 그렇게 좋아하는 편은 아니었는데 생망고 먹어보고 생각이 바뀌었어요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002877.1.14",
          "form": "맛있는 거 먹으니까 진짜 달고 과육도 살아있더라구요",
          "original_form": "맛있는 거 먹으니까 진짜 달고 과육도 살아있더라구요",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002877.1.15",
          "form": "네~~완전 맛있잖아요ㅠ",
          "original_form": "네~~완전 맛있잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002877.1.16",
          "form": "진짜 그 자리에서 바로 잘라주는 거 한 번 먹으면",
          "original_form": "진짜 그 자리에서 바로 잘라주는 거 한 번 먹으면",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002877.1.17",
          "form": "그 맛을 잊지못하겠더라구요",
          "original_form": "그 맛을 잊지못하겠더라구요",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002877.1.18",
          "form": "크~~~침고이네요",
          "original_form": "크~~~침고이네요",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002877.1.19",
          "form": "동남아 말고 한국 과일은 뭐 좋아하세오?",
          "original_form": "동남아 말고 한국 과일은 뭐 좋아하세오?",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002877.1.20",
          "form": "저는 딸기요!! ㅎ",
          "original_form": "저는 딸기요!! ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002877.1.21",
          "form": "name2님은요?",
