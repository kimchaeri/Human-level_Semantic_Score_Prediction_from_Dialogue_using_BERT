      "utterance": [
        {
          "id": "MDRW2100042095.1.1",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042095.1.2",
          "form": "저희도 바람차고 비가많이와요",
          "original_form": "저희도 바람차고 비가많이와요",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042095.1.3",
          "form": "하루종일비가~~",
          "original_form": "하루종일비가~~",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042095.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042095.1.5",
          "form": "저도 퇴근할 쯤 가랑비 내리더니",
          "original_form": "저도 퇴근할 쯤 가랑비 내리더니",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042095.1.6",
          "form": "집에 도착하고부터는 본격적으로 장대비가",
          "original_form": "집에 도착하고부터는 본격적으로 장대비가",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042095.1.7",
          "form": "아이공 집에도착해서 장대비라 다행이네요",
          "original_form": "아이공 집에도착해서 장대비라 다행이네요",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042095.1.8",
          "form": "네 작은 우산이라",
          "original_form": "네 작은 우산이라",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042095.1.9",
          "form": "이런 장대비였으면...",
          "original_form": "이런 장대비였으면...",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042095.1.10",
          "form": "정말다행이네요^^",
          "original_form": "정말다행이네요^^",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042095.1.11",
          "form": "여긴 내일도 하루종일 비소식이 예고..ㅜㅜ",
          "original_form": "여긴 내일도 하루종일 비소식이 예고..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042095.1.12",
          "form": "저희 지역도 내일 오전까지 온다더라고요",
          "original_form": "저희 지역도 내일 오전까지 온다더라고요",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042095.1.13",
          "form": "밤새 비 내리고 아침엔 개면 좋겠는데",
          "original_form": "밤새 비 내리고 아침엔 개면 좋겠는데",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042095.1.14",
          "form": "그쵸 저도 차라리 밤새 내리고",
          "original_form": "그쵸 저도 차라리 밤새 내리고",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042095.1.15",
          "form": "아침부터 개면 좋겠는데",
          "original_form": "아침부터 개면 좋겠는데",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042095.1.16",
          "form": "네 출근길에 비 오는 건 싫어서",
          "original_form": "네 출근길에 비 오는 건 싫어서",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042095.1.17",
          "form": "맞아요 정말싫죠ㅜㅜ",
          "original_form": "맞아요 정말싫죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042095.1.18",
          "form": "전 일은 안하지만 아이들 등원해야해서",
          "original_form": "전 일은 안하지만 아이들 등원해야해서",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
