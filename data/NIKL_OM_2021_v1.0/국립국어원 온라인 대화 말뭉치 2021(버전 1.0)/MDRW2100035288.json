      "utterance": [
        {
          "id": "MDRW2100035288.1.1",
          "form": "점심은 뭐드셨나요",
          "original_form": "점심은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035288.1.2",
          "form": "미역국 먹었어요~",
          "original_form": "미역국 먹었어요~",
          "speaker_id": "1",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100035288.1.3",
          "form": "전 아직인데 밥이랑 반찬꺼내먹어야겠어요ㅎㅎ",
          "original_form": "전 아직인데 밥이랑 반찬꺼내먹어야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035288.1.4",
          "form": "어여 드세요~ 시간이 늦었네용",
          "original_form": "어여 드세요~ 시간이 늦었네용",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035288.1.5",
          "form": "ㅎㅎ 빨래널고 할일이 많군요ㅋㅋ",
          "original_form": "ㅎㅎ 빨래널고 할일이 많군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100035288.1.6",
          "form": "주말이 더 바쁜것 같아요",
          "original_form": "주말이 더 바쁜것 같아요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035288.1.7",
          "form": "맨밥에 후딱 먹어야겠네요",
          "original_form": "맨밥에 후딱 먹어야겠네요",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100035288.1.8",
          "form": "에구 ㅠㅠ 어여드세용~",
          "original_form": "에구 ㅠㅠ 어여드세용~",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100035288.1.9",
          "form": "물말아먹고 차마시러나왔습니다",
          "original_form": "물말아먹고 차마시러나왔습니다",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100035288.1.10",
          "form": "아유 주말이 쫌밖에 안남았네요 ><",
          "original_form": "아유 주말이 쫌밖에 안남았네요 ><",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035288.1.11",
          "form": "이런 ㅋㅋㅋ 차는 어디로??ㅋㅋ",
          "original_form": "이런 ㅋㅋㅋ 차는 어디로??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035288.1.12",
          "form": "동네 카페에 말차라떼잘하는데 있다구해서",
          "original_form": "동네 카페에 말차라떼잘하는데 있다구해서",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035288.1.13",
          "form": "마시러왔어요",
          "original_form": "마시러왔어요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035288.1.14",
          "form": "오 말차라떼 ㅎㅎ 맛있겠어요 ~ 주말에는 카페나들이도 괜찮죠",
          "original_form": "오 말차라떼 ㅎㅎ 맛있겠어요 ~ 주말에는 카페나들이도 괜찮죠",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035288.1.15",
          "form": "저도 어제 스벅에서 한잔 픽업해서 먹었어요 신메뉴 ㅎㅎ",
          "original_form": "저도 어제 스벅에서 한잔 픽업해서 먹었어요 신메뉴 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100035288.1.16",
          "form": "오 신메뉴머에요?",
          "original_form": "오 신메뉴머에요?",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100035288.1.17",
          "form": "쿨피스맛 나는,...ㅋㅋㅋ 파인음료..이름은 기억이잘.. 그리고 아이스크림 들거간 뭐시기도 있었구요",
          "original_form": "쿨피스맛 나는,...ㅋㅋㅋ 파인음료..이름은 기억이잘.. 그리고 아이스크림 들거간 뭐시기도 있었구요",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100035288.1.18",
          "form": "아 여름신상이었나보네요",
          "original_form": "아 여름신상이었나보네요",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035288.1.19",
          "form": "저는 흑임자좋아하는데 이제 스벅에 안나와서 슬프네요ㅜㅜ",
          "original_form": "저는 흑임자좋아하는데 이제 스벅에 안나와서 슬프네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035288.1.20",
          "form": "루프탑그레이라떼...ㅠㅠㅠ 두번밖에 못먹었는데 흑",
          "original_form": "루프탑그레이라떼...ㅠㅠㅠ 두번밖에 못먹었는데 흑",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100035288.1.21",
          "form": "오 제가 안먹어본거네요",
          "original_form": "오 제가 안먹어본거네요",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100035288.1.22",
          "form": "어떤맛잇가요",
          "original_form": "어떤맛잇가요",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100035288.1.23",
          "form": "흑임자 든거 이름이 그거에요",
          "original_form": "흑임자 든거 이름이 그거에요",
