      "utterance": [
        {
          "id": "MDRW2100024921.1.1",
          "form": "생일선물로어디치킨기프트콘이무난할까?",
          "original_form": "생일선물로어디치킨기프트콘이무난할까?",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.2",
          "form": "음...교촌?",
          "original_form": "음...교촌?",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.3",
          "form": "교촌이무난하긴함ㅋㅋ",
          "original_form": "교촌이무난하긴함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.4",
          "form": "맞아ㅋㅋ대중적인치킨이긴해ㅋㅋㅋ",
          "original_form": "맞아ㅋㅋ대중적인치킨이긴해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.5",
          "form": "옛날통닭이그리울때가있어",
          "original_form": "옛날통닭이그리울때가있어",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.6",
          "form": "나도임ㅋㅋㅋ",
          "original_form": "나도임ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.7",
          "form": "그사라다?ㅅ",
          "original_form": "그사라다?ㅅ",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.8",
          "form": "샐러드?",
          "original_form": "샐러드?",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.9",
          "form": "그거ㅋㅋㅋ",
          "original_form": "그거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100024921.1.10",
          "form": "ㅋㅋ마요네즈케찹소스?ㅋㅋㅋ",
          "original_form": "ㅋㅋ마요네즈케찹소스?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024921.1.11",
          "form": "응ㅋㅋㅋㅋ그거ㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋㅋ그거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024921.1.12",
          "form": "거기에봉투에넣은무우ㅋㅋ",
          "original_form": "거기에봉투에넣은무우ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024921.1.13",
          "form": "오ㅋㅋ완전추억ㅋㅋㅋ",
          "original_form": "오ㅋㅋ완전추억ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100024921.1.14",
          "form": "맞아ㅋㅋ양념 후라이드이렇게 두종류였지ㅋㅋㅋ",
          "original_form": "맞아ㅋㅋ양념 후라이드이렇게 두종류였지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024921.1.15",
          "form": "다양하진않지맛ㅋㅋ진짜맛있었어ㅠㅠ",
          "original_form": "다양하진않지맛ㅋㅋ진짜맛있었어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024921.1.16",
          "form": "맞아맞아ㅋㅋㅋ추억의치킨",
          "original_form": "맞아맞아ㅋㅋㅋ추억의치킨",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024921.1.17",
          "form": "지금도처갓집은맛이그나마비슷한듯",
          "original_form": "지금도처갓집은맛이그나마비슷한듯",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100024921.1.18",
          "form": "치킨 집에서 만들어 먹어봤어?",
          "original_form": "치킨 집에서 만들어 먹어봤어?",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024921.1.19",
          "form": "아니ㅋㅋ나는 안해봄ㅋㅋ너는?",
          "original_form": "아니ㅋㅋ나는 안해봄ㅋㅋ너는?",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024921.1.20",
          "form": "나는몇번시도하다실패ㅋㅋㅋ",
          "original_form": "나는몇번시도하다실패ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024921.1.21",
          "form": "어렵더라고ㅋㅋ",
          "original_form": "어렵더라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024921.1.22",
          "form": "치킨은사먹는걸로ㅋㅋ",
          "original_form": "치킨은사먹는걸로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024921.1.23",
          "form": "주말에 배달시켜먹자ㅋㅋㅋ",
          "original_form": "주말에 배달시켜먹자ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:55"
        }
