      "utterance": [
        {
          "id": "MDRW2100017405.1.1",
          "form": "안녕하세요 중고거래 자주 하시나요?",
          "original_form": "안녕하세요 중고거래 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017405.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017405.1.3",
          "form": "자주는 아니지만 필요한게 생기면 검색해보긴해요",
          "original_form": "자주는 아니지만 필요한게 생기면 검색해보긴해요",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017405.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017405.1.5",
          "form": "저는 중고 거래로 득템 자주 해요ㅎㅎ",
          "original_form": "저는 중고 거래로 득템 자주 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017405.1.6",
          "form": "보통 직거래 위주로 하는 거 같아요",
          "original_form": "보통 직거래 위주로 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017405.1.7",
          "form": "오 최근엔 뭐 구매하셨어요?",
          "original_form": "오 최근엔 뭐 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017405.1.8",
          "form": "미개봉 화장품인데 반값도 안되는 가격으로 샀어요!ㅎㅎ",
          "original_form": "미개봉 화장품인데 반값도 안되는 가격으로 샀어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017405.1.9",
          "form": "가끔 덤도 주시더라구요ㅎㅎ",
          "original_form": "가끔 덤도 주시더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017405.1.10",
          "form": "우와 축하드려요!! 저는 두달전에 갤럭시탭 구매했어요 저도 직거래로! ㅋㅋㅋㅋㅋ",
          "original_form": "우와 축하드려요!! 저는 두달전에 갤럭시탭 구매했어요 저도 직거래로! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017405.1.11",
          "form": "예전에 중고나라에서 사기 당한적이 있어서",
          "original_form": "예전에 중고나라에서 사기 당한적이 있어서",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017405.1.12",
          "form": "직거래아니면 못하겠어요 ㅠ",
          "original_form": "직거래아니면 못하겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017405.1.13",
          "form": "직거래가 더 안전한 방법인것같아요!",
          "original_form": "직거래가 더 안전한 방법인것같아요!",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017405.1.14",
          "form": "그쵸 사기꾼들이 많아가지구..",
          "original_form": "그쵸 사기꾼들이 많아가지구..",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017405.1.15",
          "form": "요즘 조명이 사고 싶어서 검색중인데",
          "original_form": "요즘 조명이 사고 싶어서 검색중인데",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017405.1.16",
          "form": "잘 안나와요 ㅋㅋㅋ 인연이 아닌걸까요",
          "original_form": "잘 안나와요 ㅋㅋㅋ 인연이 아닌걸까요",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.17",
          "form": "저도 얼마 전에 조명 샀는데 신기하네요ㅋㅋㅋ",
          "original_form": "저도 얼마 전에 조명 샀는데 신기하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.19",
          "form": "신기해요 ㅋㅋㅋ",
          "original_form": "신기해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.20",
          "form": "와트가 너무 낮아서 그런 걸까요?",
          "original_form": "와트가 너무 낮아서 그런 걸까요?",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.21",
          "form": "그럴까요 ㅜ",
          "original_form": "그럴까요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.22",
          "form": "중고로 구매하신거에요?",
          "original_form": "중고로 구매하신거에요?",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017405.1.23",
          "form": "저는 어디 이벤트 당첨돼서 경품으로 받았어요ㅎㅎ",
          "original_form": "저는 어디 이벤트 당첨돼서 경품으로 받았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017405.1.24",
          "form": "전구는 LED 전구가 좋은 거 같아요",
          "original_form": "전구는 LED 전구가 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017405.1.25",
          "form": "우와 운이 좋으신가봐요",
          "original_form": "우와 운이 좋으신가봐요",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017405.1.26",
          "form": "맞아요 엘이디가 좋죠",
          "original_form": "맞아요 엘이디가 좋죠",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017405.1.27",
          "form": "어떤 조명이에요?",
          "original_form": "어떤 조명이에요?",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017405.1.28",
          "form": "침대 옆에 두는 조그만 스탠드형 조명이에요 이거 하나 바꾸니까 분위기가 달라지더라구요ㅋㅋ",
          "original_form": "침대 옆에 두는 조그만 스탠드형 조명이에요 이거 하나 바꾸니까 분위기가 달라지더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017405.1.29",
          "form": "저도 그런거 찾고 있는데 축하드려요!",
          "original_form": "저도 그런거 찾고 있는데 축하드려요!",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017405.1.30",
          "form": "확실히 조명이 분위기를 좌지우지하는것같아요",
          "original_form": "확실히 조명이 분위기를 좌지우지하는것같아요",
          "speaker_id": "2",
          "time": "20210826 19:24"
        }
