      "utterance": [
        {
          "id": "MDRW2100034771.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100034771.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034771.1.3",
          "form": "저녁 드셨어요??",
          "original_form": "저녁 드셨어요??",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034771.1.4",
          "form": "방금 먹고 왔오요 아 너무 배부른 하루네요",
          "original_form": "방금 먹고 왔오요 아 너무 배부른 하루네요",
          "speaker_id": "2",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034771.1.5",
          "form": "침대위를 벗어나질 않았더니 ㅋㅋㅋ",
          "original_form": "침대위를 벗어나질 않았더니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034771.1.6",
          "form": "ㅋㅋ저도 오늘 쉼없이 먹었더니 배가 부르네요",
          "original_form": "ㅋㅋ저도 오늘 쉼없이 먹었더니 배가 부르네요",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100034771.1.7",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100034771.1.8",
          "form": "그냥 계란후라이랑 김이랑 밥 먹었어요ㅋㅋㅋ",
          "original_form": "그냥 계란후라이랑 김이랑 밥 먹었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100034771.1.9",
          "form": "심플 이즈 베스트죠!",
          "original_form": "심플 이즈 베스트죠!",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100034771.1.10",
          "form": "맛없을 수 없는 조합이네요",
          "original_form": "맛없을 수 없는 조합이네요",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100034771.1.11",
          "form": "네ㅋㅋㅋ 내일 또 맛있는 걸 먹으러 가야하기 때문에",
          "original_form": "네ㅋㅋㅋ 내일 또 맛있는 걸 먹으러 가야하기 때문에",
          "speaker_id": "2",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100034771.1.12",
          "form": "크으 뭐드시러 가시나요?",
          "original_form": "크으 뭐드시러 가시나요?",
          "speaker_id": "1",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100034771.1.13",
          "form": "츠케멘 먹으러 갈 예정입니다",
          "original_form": "츠케멘 먹으러 갈 예정입니다",
          "speaker_id": "2",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100034771.1.14",
          "form": "우와.. 벌써 부러워욥",
          "original_form": "우와.. 벌써 부러워욥",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034771.1.15",
          "form": "처음에는 맛이 쎄다 느꼈는데 먹다보니 푹빠졌어요",
          "original_form": "처음에는 맛이 쎄다 느꼈는데 먹다보니 푹빠졌어요",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034771.1.16",
          "form": "전 아직 못먹어봐서 궁금해요 ㅋㅋㅋ",
          "original_form": "전 아직 못먹어봐서 궁금해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034771.1.17",
          "form": "일본가서 츠케멘 먹으려가 의사소통이 좀 잘못되어 걍 라멘먹었는데",
          "original_form": "일본가서 츠케멘 먹으려가 의사소통이 좀 잘못되어 걍 라멘먹었는데",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034771.1.18",
          "form": "약간 아쉬워서 ㅋㅋㅋ",
          "original_form": "약간 아쉬워서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034771.1.19",
          "form": "입맛에 맞으시면 좋겠어요",
          "original_form": "입맛에 맞으시면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034771.1.20",
          "form": "아앗! 그건 진짜 아쉽죠",
          "original_form": "아앗! 그건 진짜 아쉽죠",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034771.1.21",
          "form": "현지에서의 기회가!",
          "original_form": "현지에서의 기회가!",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034771.1.22",
          "form": "호불호가 좀 갈리는 맛인가요",
          "original_form": "호불호가 좀 갈리는 맛인가요",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034771.1.23",
          "form": "좀 갈리더라구요",
          "original_form": "좀 갈리더라구요",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034771.1.24",
          "form": "같이 간 친구는 안맞는다고 했었어요",
          "original_form": "같이 간 친구는 안맞는다고 했었어요",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034771.1.25",
          "form": "앗.. 안맞으면 안되는데...앗..",
          "original_form": "앗.. 안맞으면 안되는데...앗..",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100034771.1.26",
          "form": "앗 그러시면 안되는데",
          "original_form": "앗 그러시면 안되는데",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034771.1.27",
          "form": "맛집가시는거니 맛있을거에요!",
          "original_form": "맛집가시는거니 맛있을거에요!",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034771.1.28",
          "form": "네 맛집이라길래 지도에 저장해둔 곳이었는데",
          "original_form": "네 맛집이라길래 지도에 저장해둔 곳이었는데",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034771.1.29",
          "form": "드디어 찾아가기로 결심했거든요ㅋㅋㅋ",
          "original_form": "드디어 찾아가기로 결심했거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100034771.1.30",
          "form": "크으으 맛집이라니 너무 좋네요",
          "original_form": "크으으 맛집이라니 너무 좋네요",
          "speaker_id": "1",
          "time": "20210828 19:35"
        }
      ]
    }
