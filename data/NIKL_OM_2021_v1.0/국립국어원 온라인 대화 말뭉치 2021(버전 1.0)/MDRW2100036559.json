        {
          "id": "MDRW2100036559.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100036559.1.2",
          "form": "요 아 요즘 넷플에서 보는 드라마 생겼어",
          "original_form": "요 아 요즘 넷플에서 보는 드라마 생겼어",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.3",
          "form": "나도 요즘 넷플릭스 열심히 봐",
          "original_form": "나도 요즘 넷플릭스 열심히 봐",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.4",
          "form": "혹시 군대 드라마 보니",
          "original_form": "혹시 군대 드라마 보니",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.5",
          "form": "아 정해인 나온 디피 말해?",
          "original_form": "아 정해인 나온 디피 말해?",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.6",
          "form": "옷 아는구나 거 재밌어",
          "original_form": "옷 아는구나 거 재밌어",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.7",
          "form": "나도 앞부분 좀 봤어",
          "original_form": "나도 앞부분 좀 봤어",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.8",
          "form": "흥미진진하지 않니",
          "original_form": "흥미진진하지 않니",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.9",
          "form": "기대안했는데 재밌더라",
          "original_form": "기대안했는데 재밌더라",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036559.1.10",
          "form": "정혜인 옆에 있는 사람 귀엽더라",
          "original_form": "정혜인 옆에 있는 사람 귀엽더라",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036559.1.11",
          "form": "구교환 마저 귀엽더라",
          "original_form": "구교환 마저 귀엽더라",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036559.1.12",
          "form": "이름도 아는구나",
          "original_form": "이름도 아는구나",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036559.1.13",
          "form": "군대 드라마라서 재미없을 줄 알았는데 재밌더라",
          "original_form": "군대 드라마라서 재미없을 줄 알았는데 재밌더라",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036559.1.14",
          "form": "가혹행위 무서워",
          "original_form": "가혹행위 무서워",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036559.1.15",
          "form": "2014년 배경이래",
          "original_form": "2014년 배경이래",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036559.1.16",
          "form": "아 옛날이구나",
          "original_form": "아 옛날이구나",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100036559.1.17",
          "form": "빨리 결론까지 봐야겠다",
          "original_form": "빨리 결론까지 봐야겠다",
