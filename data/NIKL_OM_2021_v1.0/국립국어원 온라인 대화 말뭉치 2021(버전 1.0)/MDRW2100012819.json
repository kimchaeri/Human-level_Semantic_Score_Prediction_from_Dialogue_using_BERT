      "utterance": [
        {
          "id": "MDRW2100012819.1.1",
          "form": "너무좋아하죠ㅎ",
          "original_form": "너무좋아하죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012819.1.2",
          "form": "안그래도 다른분이랑 라면이야기하고 잇엌ㅅ어요ㅎ",
          "original_form": "안그래도 다른분이랑 라면이야기하고 잇엌ㅅ어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012819.1.3",
          "form": "비빔쪽이세요? 국물쪽이세요?",
          "original_form": "비빔쪽이세요? 국물쪽이세요?",
          "speaker_id": "2",
          "time": "20210825 01:58"
        },
        {
          "id": "MDRW2100012819.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:02"
        },
        {
          "id": "MDRW2100012819.1.5",
          "form": "당연 국물아닙니까!",
          "original_form": "당연 국물아닙니까!",
          "speaker_id": "1",
          "time": "20210825 02:02"
        },
        {
          "id": "MDRW2100012819.1.6",
          "form": "글쵸ㅎ 전 둘다ㄹㄹ",
          "original_form": "글쵸ㅎ 전 둘다ㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 02:03"
        },
        {
          "id": "MDRW2100012819.1.7",
          "form": "신라면 비빔면먹어보셨어요? ㅎ 완전 맛있ㅇ요ㅎ",
          "original_form": "신라면 비빔면먹어보셨어요? ㅎ 완전 맛있ㅇ요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012819.1.8",
          "form": "신라면비빔면이",
          "original_form": "신라면비빔면이",
          "speaker_id": "1",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012819.1.9",
          "form": "새로나왔나요?",
          "original_form": "새로나왔나요?",
          "speaker_id": "1",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012819.1.10",
          "form": "엇.. 모르셨군요ㅎ",
          "original_form": "엇.. 모르셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012819.1.11",
          "form": "엄청 맛있어요ㅎ",
          "original_form": "엄청 맛있어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012819.1.12",
          "form": "한번 드셔보세요ㅎ",
          "original_form": "한번 드셔보세요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:05"
        },
        {
          "id": "MDRW2100012819.1.13",
          "form": "맵지않나요?!",
          "original_form": "맵지않나요?!",
          "speaker_id": "1",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012819.1.14",
          "form": "맵지않아요ㅎㅎㅎ 맛있으니깐 드셔보세요ㄹㄹ",
          "original_form": "맵지않아요ㅎㅎㅎ 맛있으니깐 드셔보세요ㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100012819.1.15",
          "form": "라면은 진짜 맛있는듯요ㅎㅎ 비빔면도맛나고ㅎ",
          "original_form": "라면은 진짜 맛있는듯요ㅎㅎ 비빔면도맛나고ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100012819.1.16",
          "form": "맥주vs소주 어떠세요?",
          "original_form": "맥주vs소주 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100012819.1.17",
          "form": "전 안주에 따라 달라요ㅎㅎ",
          "original_form": "전 안주에 따라 달라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100012819.1.18",
          "form": "기름진건 맥주ㅎ 소주는 얼큰한걸로ㅎ",
          "original_form": "기름진건 맥주ㅎ 소주는 얼큰한걸로ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100012819.1.19",
          "form": "근데 솔직히 소주가 좋아요ㅎ",
          "original_form": "근데 솔직히 소주가 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100012819.1.20",
          "form": "ㅋㅋㅋ소주파시군요",
          "original_form": "ㅋㅋㅋ소주파시군요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100012819.1.21",
          "form": "저랑반대네요",
          "original_form": "저랑반대네요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100012819.1.22",
          "form": "ㅎㅎ 맥주도 좋아해요ㅎ",
          "original_form": "ㅎㅎ 맥주도 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100012819.1.23",
          "form": "맥주는 무슨브랜드 좋아하세요?",
          "original_form": "맥주는 무슨브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100012819.1.24",
          "form": "ㅋㅋㅋㅋ 사실저는요",
          "original_form": "ㅋㅋㅋㅋ 사실저는요",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100012819.1.25",
          "form": "술중에선 하이볼이 제일좋아요",
          "original_form": "술중에선 하이볼이 제일좋아요",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100012819.1.26",
          "form": "아아ㅎㅎㅎ",
          "original_form": "아아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100012819.1.27",
          "form": "하이볼 좋죠ㄹㅎ",
          "original_form": "하이볼 좋죠ㄹㅎ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100012819.1.28",
          "form": "맥주는 브랜드가 엄청 많은것 같애요ㅎㅎ",
          "original_form": "맥주는 브랜드가 엄청 많은것 같애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100012819.1.29",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100012819.1.30",
          "form": "님은뭐좋아하세요?",
          "original_form": "님은뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100012819.1.31",
          "form": "전 써머스비 좋아해요",
          "original_form": "전 써머스비 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100012819.1.32",
          "form": "전 하이네켄ㅎㅎㅎ",
          "original_form": "전 하이네켄ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100012819.1.33",
          "form": "오ㅎㅎ 좋죠 마셔봤어요ㅎㅎ",
          "original_form": "오ㅎㅎ 좋죠 마셔봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100012819.1.34",
          "form": "하이네켄 집에있는데ㅋㅋㅋ",
          "original_form": "하이네켄 집에있는데ㅋㅋㅋ",
