      "utterance": [
        {
          "id": "MDRW2100022046.1.1",
          "form": "혹시 소주 좋아하니",
          "original_form": "혹시 소주 좋아하니",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022046.1.2",
          "form": "자주 마시는 편 긴 해",
          "original_form": "자주 마시는 편 긴 해",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022046.1.3",
          "form": "편이긴 해",
          "original_form": "편이긴 해",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022046.1.4",
          "form": "브랜드가 워낙 다양하지",
          "original_form": "브랜드가 워낙 다양하지",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022046.1.5",
          "form": "맞아 그래서 항상 고민을 해",
          "original_form": "맞아 그래서 항상 고민을 해",
          "speaker_id": "1",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022046.1.6",
          "form": "나는 요즘 진로 만 먹어",
          "original_form": "나는 요즘 진로 만 먹어",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022046.1.7",
          "form": "그러고 보니 내 친구들도 다 그것만 먹더라",
          "original_form": "그러고 보니 내 친구들도 다 그것만 먹더라",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022046.1.8",
          "form": "아마 마케팅 때문일 거야",
          "original_form": "아마 마케팅 때문일 거야",
          "speaker_id": "2",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022046.1.9",
          "form": "콜라보레이션 마케팅 말하는 거지",
          "original_form": "콜라보레이션 마케팅 말하는 거지",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022046.1.10",
          "form": "맞아 그중에서도 진로 소주가 대표 지",
          "original_form": "맞아 그중에서도 진로 소주가 대표 지",
          "speaker_id": "2",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022046.1.11",
          "form": "두꺼비가 너무 귀여워",
          "original_form": "두꺼비가 너무 귀여워",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022046.1.12",
          "form": "브랜드 캐릭터가 귀엽다 보니 브랜드 이미지도 올라갔어",
          "original_form": "브랜드 캐릭터가 귀엽다 보니 브랜드 이미지도 올라갔어",
          "speaker_id": "2",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022046.1.13",
          "form": "시선을 확 끄는 매력이 있더라",
          "original_form": "시선을 확 끄는 매력이 있더라",
          "speaker_id": "1",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022046.1.14",
          "form": "맞아 마켓팅이 좋구나",
          "original_form": "맞아 마켓팅이 좋구나",
