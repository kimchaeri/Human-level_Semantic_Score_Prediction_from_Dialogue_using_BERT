      "utterance": [
        {
          "id": "MDRW2100031533.1.1",
          "form": "ㅎㅎ그렇죠 전공은살리기어렵죠",
          "original_form": "ㅎㅎ그렇죠 전공은살리기어렵죠",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100031533.1.2",
          "form": "저녁은맛있게드셨나요?",
          "original_form": "저녁은맛있게드셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100031533.1.3",
          "form": "저녁은 막국수 먹고",
          "original_form": "저녁은 막국수 먹고",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031533.1.4",
          "form": "지금 야식라면 또 먹는중..",
          "original_form": "지금 야식라면 또 먹는중..",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031533.1.5",
          "form": "신라면 볶음면 꽤 맵네여",
          "original_form": "신라면 볶음면 꽤 맵네여",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100031533.1.6",
          "form": "ㅎㅎㅎ면 맛있죠",
          "original_form": "ㅎㅎㅎ면 맛있죠",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100031533.1.7",
          "form": "저도저녁에감바스에파스타같이먹었어요",
          "original_form": "저도저녁에감바스에파스타같이먹었어요",
          "speaker_id": "1",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100031533.1.8",
          "form": "아 감바스 맛있었겠다",
          "original_form": "아 감바스 맛있었겠다",
          "speaker_id": "2",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100031533.1.9",
          "form": "아침은 챙겨드세요?",
          "original_form": "아침은 챙겨드세요?",
          "speaker_id": "2",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100031533.1.10",
          "form": "ㅎㅎㅎ맛있었어요",
          "original_form": "ㅎㅎㅎ맛있었어요",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031533.1.11",
          "form": "음 아침잠이 많아서ㅠ보통 못먹어요",
          "original_form": "음 아침잠이 많아서ㅠ보통 못먹어요",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031533.1.12",
          "form": "아침드시나요?",
          "original_form": "아침드시나요?",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031533.1.13",
          "form": "저는 거의 매일 야식을 먹어섵ㅋㅋㅋ 안 먹어요 속이 더부룩해서",
          "original_form": "저는 거의 매일 야식을 먹어섵ㅋㅋㅋ 안 먹어요 속이 더부룩해서",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031533.1.14",
          "form": "커피는 드시나요?",
          "original_form": "커피는 드시나요?",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031533.1.15",
          "form": "ㅎㅎㅎ좋아해요",
          "original_form": "ㅎㅎㅎ좋아해요",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031533.1.16",
          "form": "요새는줄이고있어요",
          "original_form": "요새는줄이고있어요",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031533.1.17",
          "form": "커피드시나요?",
          "original_form": "커피드시나요?",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031533.1.18",
          "form": "저는 카페인 들어간건 잘 안 먹어요 예민해서..ㅎ 초콜릿도 잘 안먹구",
          "original_form": "저는 카페인 들어간건 잘 안 먹어요 예민해서..ㅎ 초콜릿도 잘 안먹구",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031533.1.19",
          "form": "근데 진짜 한여름에 너무 더울때는 아아가 막 땡기더라구요",
          "original_form": "근데 진짜 한여름에 너무 더울때는 아아가 막 땡기더라구요",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031533.1.20",
          "form": "앗ㅠ힘드시겠어요",
          "original_form": "앗ㅠ힘드시겠어요",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100031533.1.21",
          "form": "저는하루한잔은아아마시는데",
          "original_form": "저는하루한잔은아아마시는데",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100031533.1.22",
          "form": "먹으면머리가맑아지는느낌이들더라고요",
          "original_form": "먹으면머리가맑아지는느낌이들더라고요",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100031533.1.23",
          "form": "그런분들 있더라구요 ㅋㅋㅋ 커피마시면 힐링포션마냥 막 기운이 난다고",
          "original_form": "그런분들 있더라구요 ㅋㅋㅋ 커피마시면 힐링포션마냥 막 기운이 난다고",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100031533.1.24",
          "form": "ㅎㅎ그리고 푸시식 식어요",
          "original_form": "ㅎㅎ그리고 푸시식 식어요",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100031533.1.25",
          "form": "미래의에너지를당겨오는느낌...?",
          "original_form": "미래의에너지를당겨오는느낌...?",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100031533.1.26",
          "form": "앗ㅋㅋㅋ 딱 필요할 때 잘 마셔야겠네요",
          "original_form": "앗ㅋㅋㅋ 딱 필요할 때 잘 마셔야겠네요",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100031533.1.27",
          "form": "ㅎㅎㅎ맞아요ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ맞아요ㅎㅎㅎ",
          "speaker_id": "1",
