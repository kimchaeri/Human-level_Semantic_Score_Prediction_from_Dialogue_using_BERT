      "utterance": [
        {
          "id": "MDRW2100035861.1.1",
          "form": "안녕하세요. 대화괜찮으세요?",
          "original_form": "안녕하세요. 대화괜찮으세요?",
          "speaker_id": "1",
          "time": "20210830 09:34"
        },
        {
          "id": "MDRW2100035861.1.2",
          "form": "네 괜찮습니다",
          "original_form": "네 괜찮습니다",
          "speaker_id": "2",
          "time": "20210830 09:36"
        },
        {
          "id": "MDRW2100035861.1.3",
          "form": "비가 계속오더니 오늘은 날씨 좋네요",
          "original_form": "비가 계속오더니 오늘은 날씨 좋네요",
          "speaker_id": "1",
          "time": "20210830 09:36"
        },
        {
          "id": "MDRW2100035861.1.4",
          "form": "그러네요 비가 와서 습도가 높았는데",
          "original_form": "그러네요 비가 와서 습도가 높았는데",
          "speaker_id": "2",
          "time": "20210830 09:37"
        },
        {
          "id": "MDRW2100035861.1.5",
          "form": "여기도 아직까지 끈적거려요",
          "original_form": "여기도 아직까지 끈적거려요",
          "speaker_id": "1",
          "time": "20210830 09:37"
        },
        {
          "id": "MDRW2100035861.1.6",
          "form": "원래의 쾌청한 가을 날씨로 돌아가면 좋겠어요",
          "original_form": "원래의 쾌청한 가을 날씨로 돌아가면 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 09:37"
        },
        {
          "id": "MDRW2100035861.1.7",
          "form": "그래도 바람이 선선하게부네요.",
          "original_form": "그래도 바람이 선선하게부네요.",
          "speaker_id": "1",
          "time": "20210830 09:38"
        },
        {
          "id": "MDRW2100035861.1.8",
          "form": "바람은 확실히 가을날씨죠",
          "original_form": "바람은 확실히 가을날씨죠",
          "speaker_id": "2",
          "time": "20210830 09:38"
        },
        {
          "id": "MDRW2100035861.1.9",
          "form": "어제 밤엔 춥더라구요",
          "original_form": "어제 밤엔 춥더라구요",
          "speaker_id": "1",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035861.1.10",
          "form": "새벽엔 기온이 많이 내려가는 것 같아요",
          "original_form": "새벽엔 기온이 많이 내려가는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035861.1.11",
          "form": "가을이 점점 짪아지네요.",
          "original_form": "가을이 점점 짪아지네요.",
          "speaker_id": "1",
          "time": "20210830 09:39"
        },
        {
          "id": "MDRW2100035861.1.12",
          "form": "여름 겨울만 남은 느낌이에요",
          "original_form": "여름 겨울만 남은 느낌이에요",
          "speaker_id": "2",
          "time": "20210830 09:40"
        },
        {
          "id": "MDRW2100035861.1.13",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:40"
        },
        {
          "id": "MDRW2100035861.1.14",
          "form": "가을도 금방 지나갈 것만 같아요..",
          "original_form": "가을도 금방 지나갈 것만 같아요..",
