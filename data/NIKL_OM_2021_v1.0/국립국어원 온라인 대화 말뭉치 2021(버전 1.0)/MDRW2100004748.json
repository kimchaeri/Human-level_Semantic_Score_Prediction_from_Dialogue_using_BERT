      "utterance": [
        {
          "id": "MDRW2100004748.1.1",
          "form": "안녕하세요 학생이신가요??",
          "original_form": "안녕하세요 학생이신가요??",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004748.1.2",
          "form": "직장인입니다",
          "original_form": "직장인입니다",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004748.1.3",
          "form": "요즘은 무슨동아리가 핫한가요",
          "original_form": "요즘은 무슨동아리가 핫한가요",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004748.1.4",
          "form": "글쎄요 ..사회나가면 동아리 할 수있는게",
          "original_form": "글쎄요 ..사회나가면 동아리 할 수있는게",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004748.1.5",
          "form": "요샌 그림동아리나 책 동아리가 많을까요?",
          "original_form": "요샌 그림동아리나 책 동아리가 많을까요?",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004748.1.6",
          "form": "저는 아직 대학생이라 과동아리만 하고있네요 ㅎㅎ",
          "original_form": "저는 아직 대학생이라 과동아리만 하고있네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004748.1.7",
          "form": "대학 동아리 물어본거예요",
          "original_form": "대학 동아리 물어본거예요",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004748.1.8",
          "form": "몇학번이세요?",
          "original_form": "몇학번이세요?",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004748.1.9",
          "form": "아 저 19학번이요 ㅎㅎ",
          "original_form": "아 저 19학번이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004748.1.10",
          "form": "저희 학교는 동아리가 많이 없어서",
          "original_form": "저희 학교는 동아리가 많이 없어서",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004748.1.11",
          "form": "재미가 없네요 ㅠㅠ..",
          "original_form": "재미가 없네요 ㅠㅠ..",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004748.1.12",
          "form": "막내가 18학번인데..ㅋㅋ 어리시네요",
          "original_form": "막내가 18학번인데..ㅋㅋ 어리시네요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004748.1.13",
          "form": "부러워요!!",
          "original_form": "부러워요!!",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004748.1.14",
          "form": "무슨과신가요",
          "original_form": "무슨과신가요",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004748.1.15",
          "form": "대학이 코로나때문에 재미없죠?",
          "original_form": "대학이 코로나때문에 재미없죠?",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004748.1.16",
          "form": "아고 ㅋㅋㅋ",
          "original_form": "아고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004748.1.17",
          "form": "전 디자인과요!",
          "original_form": "전 디자인과요!",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004748.1.18",
          "form": "대학생활 1년만 제대로 보내고",
          "original_form": "대학생활 1년만 제대로 보내고",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004748.1.19",
          "form": "나머지는 집에서만 보냈네요",
          "original_form": "나머지는 집에서만 보냈네요",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004748.1.20",
          "form": "대학생활 한 것 같지가 않네요 ㅠㅠ..저도 이런데",
          "original_form": "대학생활 한 것 같지가 않네요 ㅠㅠ..저도 이런데",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004748.1.21",
          "form": "20학번21학번들이 정말 불쌍해요..",
          "original_form": "20학번21학번들이 정말 불쌍해요..",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004748.1.22",
          "form": "지금3학년인가요!",
          "original_form": "지금3학년인가요!",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.23",
          "form": "취업걱정이겠어요",
          "original_form": "취업걱정이겠어요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.24",
          "form": "넵 맞아요 ㅎㅎ",
          "original_form": "넵 맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.25",
          "form": "취업 ㅠㅠ 암담해요",
          "original_form": "취업 ㅠㅠ 암담해요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.26",
          "form": "될지모르겠네요..",
          "original_form": "될지모르겠네요..",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.27",
          "form": "저는 컴공과였는데 지금은 또 다른직업 가지고있어요",
          "original_form": "저는 컴공과였는데 지금은 또 다른직업 가지고있어요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.28",
          "form": "컴퓨터쪽은 잘되면 아주잘되거나 아님 사무직이니ㅠ",
          "original_form": "컴퓨터쪽은 잘되면 아주잘되거나 아님 사무직이니ㅠ",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004748.1.29",
          "form": "온라인수업은 어때요?\r\n디자인이면 실습많을텐데",
          "original_form": "온라인수업은 어때요?\r\n디자인이면 실습많을텐데",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004748.1.30",
          "form": "그래도 막 나쁘진 않은거같아요",
          "original_form": "그래도 막 나쁘진 않은거같아요",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004748.1.31",
          "form": "진도가 느려서 좀 하고싶은걸 못할때도 많긴해요ㅠ",
          "original_form": "진도가 느려서 좀 하고싶은걸 못할때도 많긴해요ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004748.1.32",
          "form": "name1님이 실력이 좋으신가봐요~~",
          "original_form": "&name1&님이 실력이 좋으신가봐요~~",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004748.1.33",
          "form": "온라인시험이라 컨닝많이 할거같아요",
          "original_form": "온라인시험이라 컨닝많이 할거같아요",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004748.1.34",
          "form": "제 동생도 그것때문에 스트레스라하더라고요",
          "original_form": "제 동생도 그것때문에 스트레스라하더라고요",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004748.1.35",
          "form": "아 초반에 컨닝때문에 말이 많았죠 ㅋㅋㅋ",
          "original_form": "아 초반에 컨닝때문에 말이 많았죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004748.1.36",
          "form": "그래도 저희과는 거의 과제제출인게",
          "original_form": "그래도 저희과는 거의 과제제출인게",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004748.1.37",
          "form": "다행이네요 ㅠㅠ",
          "original_form": "다행이네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004748.1.38",
          "form": "시험보는과는 스트레스 많이 받겠어요 ..",
          "original_form": "시험보는과는 스트레스 많이 받겠어요 ..",
          "speaker_id": "1",
          "time": "20210821 18:48"
        },
        {
          "id": "MDRW2100004748.1.39",
          "form": "동생네는 문과라서ㅠ\r\n맨날 1등해서 등록금안냈는데",
          "original_form": "동생네는 문과라서ㅠ\r\n맨날 1등해서 등록금안냈는데",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004748.1.40",
          "form": "컨닝하는애들때문에 변별력이없다고",
          "original_form": "컨닝하는애들때문에 변별력이없다고",
          "speaker_id": "2",
          "time": "20210821 18:52"
        }
      ]
