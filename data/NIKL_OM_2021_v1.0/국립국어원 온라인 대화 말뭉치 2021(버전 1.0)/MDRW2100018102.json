      "utterance": [
        {
          "id": "MDRW2100018102.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018102.1.2",
          "form": "아뇨 친구랑 같이 살아서 키울 수가 없어요 ㅠ",
          "original_form": "아뇨 친구랑 같이 살아서 키울 수가 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018102.1.3",
          "form": "나중에 여건되면 고양이 키우고싶어요",
          "original_form": "나중에 여건되면 고양이 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018102.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018102.1.5",
          "form": "저는 고양이요!",
          "original_form": "저는 고양이요!",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018102.1.6",
          "form": "2마리요",
          "original_form": "2마리요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018102.1.7",
          "form": "와 귀엽겠어요!",
          "original_form": "와 귀엽겠어요!",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018102.1.8",
          "form": "사람 좋아하나요??",
          "original_form": "사람 좋아하나요??",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018102.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018102.1.10",
          "form": "좋아하지만 거리감은 늘 유지해요",
          "original_form": "좋아하지만 거리감은 늘 유지해요",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018102.1.11",
          "form": "가끔 주변에 보면 손에 상처가 가시지 않는 집사들이 있더라구요",
          "original_form": "가끔 주변에 보면 손에 상처가 가시지 않는 집사들이 있더라구요",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100018102.1.12",
          "form": "그러더라구요",
          "original_form": "그러더라구요",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018102.1.13",
          "form": "저희 애들은 물지 않아서 그래본적이 없어 약간 아쉽기도 해요ㅋㅋ",
          "original_form": "저희 애들은 물지 않아서 그래본적이 없어 약간 아쉽기도 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018102.1.14",
          "form": "헉 물지 않으면 좋은거죠 ㅋㅋㅋ",
          "original_form": "헉 물지 않으면 좋은거죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018102.1.15",
          "form": "키우면서 가장 예뻐보일땐 어떤때인가요?",
          "original_form": "키우면서 가장 예뻐보일땐 어떤때인가요?",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018102.1.16",
          "form": "그쵸ㅋㅋㅋ 그래도 왠지 부럽달까요",
          "original_form": "그쵸ㅋㅋㅋ 그래도 왠지 부럽달까요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018102.1.17",
          "form": "애기나 동물은 다 천사죠",
          "original_form": "애기나 동물은 다 천사죠",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018102.1.18",
          "form": "전 주위에서 살랑살랑 움직이면",
          "original_form": "전 주위에서 살랑살랑 움직이면",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018102.1.19",
          "form": "귀여움을 못참고 막 쓰다들고 껴안고 싶을거같아서 걱정이에요",
          "original_form": "귀여움을 못참고 막 쓰다들고 껴안고 싶을거같아서 걱정이에요",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018102.1.20",
          "form": "아 진짜요!",
          "original_form": "아 진짜요!",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018102.1.21",
          "form": "근디 그건 싫어하더라구요",
          "original_form": "근디 그건 싫어하더라구요",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018102.1.22",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018102.1.23",
          "form": "자기들이 내킬 때만 허락해주더라구요 ㅠ",
          "original_form": "자기들이 내킬 때만 허락해주더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018102.1.24",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018102.1.25",
          "form": "그게 매력이지만요",
          "original_form": "그게 매력이지만요",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018102.1.26",
          "form": "두마리 같이 있으면 자기들끼리 노나요? 아님 공간 분리하시나요?",
          "original_form": "두마리 같이 있으면 자기들끼리 노나요? 아님 공간 분리하시나요?",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018102.1.27",
          "form": "각자 자리가 있더라구요!",
          "original_form": "각자 자리가 있더라구요!",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018102.1.28",
          "form": "만들어주진 못했어요ㅜ",
          "original_form": "만들어주진 못했어요ㅜ",
