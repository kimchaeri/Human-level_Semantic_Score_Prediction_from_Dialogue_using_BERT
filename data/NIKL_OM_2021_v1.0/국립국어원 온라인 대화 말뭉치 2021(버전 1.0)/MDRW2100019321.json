      "utterance": [
        {
          "id": "MDRW2100019321.1.1",
          "form": "오늘 저녁은 뭐먹을까?",
          "original_form": "오늘 저녁은 뭐먹을까?",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100019321.1.2",
          "form": "고기 사와서 구워먹을까?",
          "original_form": "고기 사와서 구워먹을까?",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019321.1.3",
          "form": "아니다 그럼 설거지 할게 많아지겠네",
          "original_form": "아니다 그럼 설거지 할게 많아지겠네",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019321.1.4",
          "form": "그냥 집에 있는 걸로 대충 먹어야겠어",
          "original_form": "그냥 집에 있는 걸로 대충 먹어야겠어",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019321.1.5",
          "form": "설거지는 내가 하는데",
          "original_form": "설거지는 내가 하는데",
          "speaker_id": "2",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019321.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019321.1.7",
          "form": "육아는 참 힘들어",
          "original_form": "육아는 참 힘들어",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019321.1.8",
          "form": "아기랑 놀아주는게 쉬운 일이 아니야",
          "original_form": "아기랑 놀아주는게 쉬운 일이 아니야",
          "speaker_id": "1",
          "time": "20210827 17:38"
        },
        {
          "id": "MDRW2100019321.1.9",
          "form": "그렇지 어떻게 우리가 컸는지 몰라",
          "original_form": "그렇지 어떻게 우리가 컸는지 몰라",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.10",
          "form": "아기가 장난감을 좋아해서 다행이지",
          "original_form": "아기가 장난감을 좋아해서 다행이지",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.11",
          "form": "어린시절 기억도 안나는데",
          "original_form": "어린시절 기억도 안나는데",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.12",
          "form": "우리도 이렇게 컸울거야",
          "original_form": "우리도 이렇게 컸울거야",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.13",
          "form": "에듀테이블이랑 아기코끼리 코야",
          "original_form": "에듀테이블이랑 아기코끼리 코야",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.14",
          "form": "아기한테 책도 읽어주고 그래",
          "original_form": "아기한테 책도 읽어주고 그래",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.15",
          "form": "내일은 영유아검진 받으러 소아과 가야해",
          "original_form": "내일은 영유아검진 받으러 소아과 가야해",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.16",
          "form": "라이언 킹",
          "original_form": "라이언 킹",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.17",
          "form": "내일은 중요한 날이네",
          "original_form": "내일은 중요한 날이네",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.18",
          "form": "라이언 킹 책 읽어주면 좋아해",
          "original_form": "라이언 킹 책 읽어주면 좋아해",
          "speaker_id": "1",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100019321.1.19",
          "form": "요즘 아기가 발가락 빠는거에 재미들렸나봐",
          "original_form": "요즘 아기가 발가락 빠는거에 재미들렸나봐",
          "speaker_id": "1",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100019321.1.20",
          "form": "너무 귀여워",
          "original_form": "너무 귀여워",
          "speaker_id": "1",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100019321.1.21",
          "form": "귀여운 우리아들",
          "original_form": "귀여운 우리아들",
          "speaker_id": "2",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100019321.1.22",
          "form": "책도 많이읽고 에듀테이블도 열심히하고",
          "original_form": "책도 많이읽고 에듀테이블도 열심히하고",
          "speaker_id": "2",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100019321.1.23",
          "form": "덕분에 쉴 수 있어",
          "original_form": "덕분에 쉴 수 있어",
          "speaker_id": "2",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100019321.1.24",
          "form": "졸리다 자고싶다",
          "original_form": "졸리다 자고싶다",
          "speaker_id": "2",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100019321.1.25",
          "form": "집안일 좀 해놓고 자",
          "original_form": "집안일 좀 해놓고 자",
          "speaker_id": "1",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019321.1.26",
          "form": "헉 할게 남았지",
          "original_form": "헉 할게 남았지",
          "speaker_id": "2",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019321.1.27",
          "form": "뭐뭐 남아있더라",
          "original_form": "뭐뭐 남아있더라",
          "speaker_id": "2",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019321.1.28",
          "form": "설거지, 바닥 청소, 분리수거 쓰레기 버리기",
          "original_form": "설거지, 바닥 청소, 분리수거 쓰레기 버리기",
          "speaker_id": "1",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019321.1.29",
          "form": "집안일은 끝이없어",
          "original_form": "집안일은 끝이없어",
          "speaker_id": "2",
          "time": "20210827 17:41"
        },
        {
          "id": "MDRW2100019321.1.30",
          "form": "바닥청소눈 아까 했어",
          "original_form": "바닥청소눈 아까 했어",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019321.1.31",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019321.1.32",
          "form": "대단하지?",
          "original_form": "대단하지?",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019321.1.33",
          "form": "쓰레기는 저녁 먹고 버릴 예정이야",
          "original_form": "쓰레기는 저녁 먹고 버릴 예정이야",
          "speaker_id": "2",
          "time": "20210827 17:42"
        }
      ]
    }
