      "utterance": [
        {
          "id": "MDRW2100020189.1.1",
          "form": "세수도 매일하시구요? ㅋㅋㅋ",
          "original_form": "세수도 매일하시구요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020189.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ세수가 미용에 포함되는 건가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ세수가 미용에 포함되는 건가요",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020189.1.3",
          "form": "그럼요.. ㅋㅋㅋ 피부 미용 기본은 세안..",
          "original_form": "그럼요.. ㅋㅋㅋ 피부 미용 기본은 세안..",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020189.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ본격적으로 미용 세안 얘기 해볼까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ본격적으로 미용 세안 얘기 해볼까요",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020189.1.5",
          "form": "오일은 모쓰세여? ㅋㅋㅋ",
          "original_form": "오일은 모쓰세여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020189.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 마녀공장 클렌징오일 씁니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 마녀공장 클렌징오일 씁니다",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020189.1.7",
          "form": "마녀공장 갑자기 엄청 떳네요?",
          "original_form": "마녀공장 갑자기 엄청 떳네요?",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020189.1.8",
          "form": "그거 엄청 쓰는듯..",
          "original_form": "그거 엄청 쓰는듯..",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020189.1.9",
          "form": "전 5년쯤 된것같아요",
          "original_form": "전 5년쯤 된것같아요",
          "speaker_id": "2",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020189.1.10",
          "form": "마냐공장 원래 유명햇어여? 전 최근에 안 브랜드인ㄷ",
          "original_form": "마냐공장 원래 유명햇어여? 전 최근에 안 브랜드인ㄷ",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020189.1.11",
          "form": "데",
          "original_form": "데",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020189.1.12",
          "form": "전 몇년전에 갑자기 얼굴이 뒤집어져서",
          "original_form": "전 몇년전에 갑자기 얼굴이 뒤집어져서",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020189.1.13",
          "form": "성분이 순한 제품 찾다가 쓰기시작햇어요",
          "original_form": "성분이 순한 제품 찾다가 쓰기시작햇어요",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020189.1.14",
          "form": "아.. 전 클렌징 오일 안쓰다가.. \n파데로 바꾸면서.. 이게 제대로 안씻으면 은근 트러블이 많더아구요..",
          "original_form": "아.. 전 클렌징 오일 안쓰다가.. \n파데로 바꾸면서.. 이게 제대로 안씻으면 은근 트러블이 많더아구요..",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020189.1.15",
          "form": "맞아요 화장을 하면 이중세안이 필수인듯",
          "original_form": "맞아요 화장을 하면 이중세안이 필수인듯",
          "speaker_id": "2",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020189.1.16",
          "form": "팩은 자주해요?",
          "original_form": "팩은 자주해요?",
          "speaker_id": "1",
          "time": "20210828 00:26"
        },
        {
          "id": "MDRW2100020189.1.17",
          "form": "귀찮아서 잘 안해요 ㅋㅋㅋㅋ",
          "original_form": "귀찮아서 잘 안해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020189.1.18",
          "form": "자주 하세요?",
          "original_form": "자주 하세요?",
