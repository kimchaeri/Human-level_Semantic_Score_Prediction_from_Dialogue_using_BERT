      "utterance": [
        {
          "id": "MDRW2100000953.1.1",
          "form": "요새 슬기로운 의사생활 넘 재밌다",
          "original_form": "요새 슬기로운 의사생활 넘 재밌다",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000953.1.2",
          "form": "난 별로 재미없던데",
          "original_form": "난 별로 재미없던데",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000953.1.3",
          "form": "넌 억지감동 아니야",
          "original_form": "넌 억지감동 아니야",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000953.1.4",
          "form": "너무 억지 감동 인 듯",
          "original_form": "너무 억지 감동 인 듯",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000953.1.5",
          "form": "아닌뎅! 그냥 감동이지 ㅋㅋ 우리 엄마 감정이 매말랐네",
          "original_form": "아닌뎅! 그냥 감동이지 ㅋㅋ 우리 엄마 감정이 매말랐네",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000953.1.6",
          "form": "그래 재미 없는 건 아닌데",
          "original_form": "그래 재미 없는 건 아닌데",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000953.1.7",
          "form": "난 별로 안 끌린다",
          "original_form": "난 별로 안 끌린다",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000953.1.8",
          "form": "연기자들이 넘 연기를 잘한다",
          "original_form": "연기자들이 넘 연기를 잘한다",
          "speaker_id": "1",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000953.1.9",
          "form": "그래 다들 베테랑 연기자들 이니까",
          "original_form": "그래 다들 베테랑 연기자들 이니까",
          "speaker_id": "2",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000953.1.10",
          "form": "옹 ㅋㅋ 연기 보는 재미가 있다니까",
          "original_form": "옹 ㅋㅋ 연기 보는 재미가 있다니까",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000953.1.11",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000953.1.12",
          "form": "이따가 같이 보던지",
          "original_form": "이따가 같이 보던지",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000953.1.13",
          "form": "구랭 ㅎㅎ 같이 한 편 보자",
          "original_form": "구랭 ㅎㅎ 같이 한 편 보자",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000953.1.14",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000953.1.15",
          "form": "오키~ ㅎㅎ",
          "original_form": "오키~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100000953.1.16",
          "form": "한 편 한번 같이 보자",
          "original_form": "한 편 한번 같이 보자",
          "speaker_id": "2",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000953.1.17",
          "form": "욥욥 저번주꺼 재방송으로 보면될듯",
