        {
          "id": "MDRW2100045300.1.1",
          "form": "하나는 고등어 하나는 흰색이에요",
          "original_form": "하나는 고등어 하나는 흰색이에요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045300.1.2",
          "form": "형제인데 다른가요",
          "original_form": "형제인데 다른가요",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045300.1.3",
          "form": "그렇더라구요",
          "original_form": "그렇더라구요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045300.1.4",
          "form": "신기하고 귀엽다",
          "original_form": "신기하고 귀엽다",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045300.1.5",
          "form": "다들 안 닮았다고 해요",
          "original_form": "다들 안 닮았다고 해요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045300.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045300.1.7",
          "form": "그래도 형제인지 잘 놀아요",
          "original_form": "그래도 형제인지 잘 놀아요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045300.1.8",
          "form": "물지는 않나요",
          "original_form": "물지는 않나요",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045300.1.9",
          "form": "유튜브 냥이 처럼 물진 않더라고요",
          "original_form": "유튜브 냥이 처럼 물진 않더라고요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045300.1.10",
          "form": "다 아리랑이 같은 줄",
          "original_form": "다 아리랑이 같은 줄",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045300.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
