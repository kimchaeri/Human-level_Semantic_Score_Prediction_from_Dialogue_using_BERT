      "utterance": [
        {
          "id": "MDRW2100006607.1.1",
          "form": "너 영국도 갔다왔다고하지 않았나?",
          "original_form": "너 영국도 갔다왔다고하지 않았나?",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006607.1.2",
          "form": "영국 다녀왔지 name3한테 놀러감ㅋ",
          "original_form": "영국 다녀왔지 &name3&한테 놀러감ㅋ",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006607.1.3",
          "form": "name3가 영국에 살아?",
          "original_form": "&name3&가 영국에 살아?",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006607.1.4",
          "form": "아니 나 한국 들어오고 얼마 안돼서 name3도 영국으로 유학감ㅠ",
          "original_form": "아니 나 한국 들어오고 얼마 안돼서 &name3&도 영국으로 유학감ㅠ",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006607.1.5",
          "form": "와 너네 유학파구나",
          "original_form": "와 너네 유학파구나",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006607.1.6",
          "form": "어쩌다보니?ㅋㅋㅋ",
          "original_form": "어쩌다보니?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006607.1.7",
          "form": "영국은 어때? 소문대로 날씨가 우중충하고 그래?",
          "original_form": "영국은 어때? 소문대로 날씨가 우중충하고 그래?",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006607.1.8",
          "form": "내가 1월말인가 2월인가 한겨울에 갔었는데",
          "original_form": "내가 1월말인가 2월인가 한겨울에 갔었는데",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006607.1.9",
          "form": "그래? 그럼 비 보다는 눈이 엄청 오나?ㅋ",
          "original_form": "그래? 그럼 비 보다는 눈이 엄청 오나?ㅋ",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006607.1.10",
          "form": "아니 완전 따듯하더라고ㅋㅋ",
          "original_form": "아니 완전 따듯하더라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006607.1.11",
          "form": "한국보다 기온은 좀 높은가보네",
          "original_form": "한국보다 기온은 좀 높은가보네",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006607.1.12",
          "form": "그런듯? 내가 패딩 버릴뻔했자나 한번을 안입음ㅋ",
          "original_form": "그런듯? 내가 패딩 버릴뻔했자나 한번을 안입음ㅋ",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006607.1.13",
          "form": "그럼 뭐입고 다녔어?",
          "original_form": "그럼 뭐입고 다녔어?",
          "speaker_id": "2",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006607.1.14",
          "form": "그냥 티에 맨투맨입거나 니트 입는정도? 온화했어 비도 별로 안오고 해 많이 뜨노 완전 럭키!!",
