        {
          "id": "MDRW2100008535.1.1",
          "form": "일해여?",
          "original_form": "일해여?",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008535.1.2",
          "form": "저는 알바해요",
          "original_form": "저는 알바해요",
          "speaker_id": "2",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008535.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008535.1.4",
          "form": "편의점이요!",
          "original_form": "편의점이요!",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008535.1.5",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008535.1.6",
          "form": "야간아니에요?",
          "original_form": "야간아니에요?",
          "speaker_id": "1",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008535.1.7",
          "form": "저는 주간이요",
          "original_form": "저는 주간이요",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008535.1.8",
          "form": "야간은 힘들어서 안되겠더라고요ㅋㅋㅋ",
          "original_form": "야간은 힘들어서 안되겠더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008535.1.9",
          "form": "낮밤은 바꾼다고 안되요",
          "original_form": "낮밤은 바꾼다고 안되요",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008535.1.10",
          "form": "그쵸 해 떠있을때 움직여야해요",
          "original_form": "그쵸 해 떠있을때 움직여야해요",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008535.1.11",
          "form": "광합성도 하고",
          "original_form": "광합성도 하고",
          "speaker_id": "1",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008535.1.12",
          "form": "남들 움직일때 같이 출근하고",
          "original_form": "남들 움직일때 같이 출근하고",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008535.1.13",
          "form": "퇴근할때 같이 퇴근하고",
          "original_form": "퇴근할때 같이 퇴근하고",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008535.1.14",
          "form": "쉴때 쉬고",
          "original_form": "쉴때 쉬고",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008535.1.15",
          "form": "회사 다니시죠?",
          "original_form": "회사 다니시죠?",
