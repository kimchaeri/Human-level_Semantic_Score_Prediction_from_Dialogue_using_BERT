      "utterance": [
        {
          "id": "MDRW2100045807.1.1",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045807.1.2",
          "form": "네 요거트 먹었어요",
          "original_form": "네 요거트 먹었어요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045807.1.3",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100045807.1.4",
          "form": "오트밀 먹으려고요 지금 머리말려요..ㅋㅋㅋㅋㅋ",
          "original_form": "오트밀 먹으려고요 지금 머리말려요..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045807.1.5",
          "form": "오트밀 어떻게 해서 드세요? 처박아놨네요 ㅠㅠ",
          "original_form": "오트밀 어떻게 해서 드세요? 처박아놨네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045807.1.6",
          "form": "그릇에 오트밀 붓고 우유붓고 소금 약간 후츠 약간해서 전자렌지에 1분 30초 돌려서 먹어요!",
          "original_form": "그릇에 오트밀 붓고 우유붓고 소금 약간 후츠 약간해서 전자렌지에 1분 30초 돌려서 먹어요!",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045807.1.7",
          "form": "후추도 넣는군요 한번해봐야겠어요",
          "original_form": "후추도 넣는군요 한번해봐야겠어요",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045807.1.8",
          "form": "집에 쌓여있는거 빨리 먹어야히는데",
          "original_form": "집에 쌓여있는거 빨리 먹어야히는데",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045807.1.9",
          "form": "그냥 제 식대로 먹는거라 입에 안맞으실수듀 있어요..ㅎㅎ",
          "original_form": "그냥 제 식대로 먹는거라 입에 안맞으실수듀 있어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045807.1.10",
          "form": "저는 아몬드브리즈랑 오트밀만 넣고먹었는데 너무 노맛이어서",
          "original_form": "저는 아몬드브리즈랑 오트밀만 넣고먹었는데 너무 노맛이어서",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045807.1.11",
          "form": "후추소금한번넣어볼게요ㅎㅎ",
          "original_form": "후추소금한번넣어볼게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045807.1.12",
          "form": "저도 노맛이어서 넣은거거든요 확실히 조금 나을거에요 그럼 좋겠네요 ㅋㅋ",
          "original_form": "저도 노맛이어서 넣은거거든요 확실히 조금 나을거에요 그럼 좋겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045807.1.13",
          "form": "점심은 뭐드실건가용",
          "original_form": "점심은 뭐드실건가용",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045807.1.14",
          "form": "점심은 쭈꾸미볶음 먹으려구요 ㅎㅎ",
          "original_form": "점심은 쭈꾸미볶음 먹으려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100045807.1.15",
          "form": "뭐드실건가요?",
          "original_form": "뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100045807.1.16",
          "form": "저 라면 먹을까 고민중이에요",
          "original_form": "저 라면 먹을까 고민중이에요",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045807.1.17",
          "form": "며칠전부터 땡기는데 참았거든요",
          "original_form": "며칠전부터 땡기는데 참았거든요",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045807.1.18",
          "form": "어떻게 생각하세요",
          "original_form": "어떻게 생각하세요",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045807.1.19",
          "form": "땡기면먹어야죠",
          "original_form": "땡기면먹어야죠",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045807.1.20",
          "form": "라면은 의사가 절대안먹는 음식에 없더라구요",
          "original_form": "라면은 의사가 절대안먹는 음식에 없더라구요",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045807.1.21",
          "form": "무슨라면좋아하세요?",
          "original_form": "무슨라면좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045807.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그거 맏는사럼 있나요 ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그거 맏는사럼 있나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045807.1.23",
          "form": "다 좋아하는데 안성탕면이 집에 있어요",
          "original_form": "다 좋아하는데 안성탕면이 집에 있어요",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045807.1.24",
          "form": "전 열라면이 먹고 싶어욤",
          "original_form": "전 열라면이 먹고 싶어욤",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045807.1.25",
          "form": "ㅋㅋㅋ곱창이랑 소세지그런거",
          "original_form": "ㅋㅋㅋ곱창이랑 소세지그런거",
