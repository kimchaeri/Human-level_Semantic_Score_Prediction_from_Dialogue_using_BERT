      "utterance": [
        {
          "id": "MDRW2100041970.1.1",
          "form": "우동 좋아하세요?",
          "original_form": "우동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041970.1.2",
          "form": "우동 좋아하죠",
          "original_form": "우동 좋아하죠",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041970.1.3",
          "form": "저는 어묵 우동 좋아해요",
          "original_form": "저는 어묵 우동 좋아해요",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041970.1.4",
          "form": "저는 포장마차 우동이요 ㅎㅎ",
          "original_form": "저는 포장마차 우동이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041970.1.5",
          "form": "아 저도 포장마차 우동 좋아해요 약간 슴슴한 맛",
          "original_form": "아 저도 포장마차 우동 좋아해요 약간 슴슴한 맛",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041970.1.6",
          "form": "김가루 팍팍 뿌려서 먹어줘야해요",
          "original_form": "김가루 팍팍 뿌려서 먹어줘야해요",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041970.1.7",
          "form": "아 맛있겠다..",
          "original_form": "아 맛있겠다..",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041970.1.8",
          "form": "날이 점점 선선해지니까 국물이 땡기네요 ㅎ",
          "original_form": "날이 점점 선선해지니까 국물이 땡기네요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041970.1.9",
          "form": "저도요ㅠㅠ 여름엔 귀신같이 냉면이 땡기더니",
          "original_form": "저도요ㅠㅠ 여름엔 귀신같이 냉면이 땡기더니",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041970.1.10",
          "form": "ㅎ그러게요 튀김얹은 우동도 먹고싶네요",
          "original_form": "ㅎ그러게요 튀김얹은 우동도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041970.1.11",
          "form": "아 저 정말좋아해요 바삭한 튀김 우동국물에 적셔서",
          "original_form": "아 저 정말좋아해요 바삭한 튀김 우동국물에 적셔서",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041970.1.12",
          "form": "우동이랑 김밥도 참 잘어울리는데..",
          "original_form": "우동이랑 김밥도 참 잘어울리는데..",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041970.1.13",
          "form": "아 너무 맛있겠네요ㅠ",
          "original_form": "아 너무 맛있겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:10"
        }
      ]
