      "utterance": [
        {
          "id": "MDRW2100035663.1.1",
          "form": "에스파 디렉도 전부 직접한다면서요",
          "original_form": "에스파 디렉도 전부 직접한다면서요",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.2",
          "form": "영감탱 대단해...",
          "original_form": "영감탱 대단해...",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.3",
          "form": "아 맞아요 ㅋㅋ 막 헤어스타일도 보고 ...",
          "original_form": "아 맞아요 ㅋㅋ 막 헤어스타일도 보고 ...",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.4",
          "form": "영감과 유영진의 콜라보란..",
          "original_form": "영감과 유영진의 콜라보란..",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.5",
          "form": "샤이니 돈콜미도",
          "original_form": "샤이니 돈콜미도",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.6",
          "form": "할배가 골라줬다고...",
          "original_form": "할배가 골라줬다고...",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.7",
          "form": "와 그건 진짜 원래 보아 노래였잖아요",
          "original_form": "와 그건 진짜 원래 보아 노래였잖아요",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100035663.1.8",
          "form": "완전 할배 감 대박 ...",
          "original_form": "완전 할배 감 대박 ...",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.9",
          "form": "진짜 감 대박이요ㅋㅋㅋㅋㅋ 할배 오래살아...",
          "original_form": "진짜 감 대박이요ㅋㅋㅋㅋㅋ 할배 오래살아...",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.10",
          "form": "제 생각엔 스엠은",
          "original_form": "제 생각엔 스엠은",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.11",
          "form": "할배 없으면 .. 안됩니다",
          "original_form": "할배 없으면 .. 안됩니다",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.12",
          "form": "어케하면 그렇게 감이 안늙고 살 수 있는건지...",
          "original_form": "어케하면 그렇게 감이 안늙고 살 수 있는건지...",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.13",
          "form": "그니까요... 제왑은",
          "original_form": "그니까요... 제왑은",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.14",
          "form": "이제...반대인것같고...",
          "original_form": "이제...반대인것같고...",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035663.1.15",
          "form": "맞아요 ㅋㅋㅋ 제왑은 이제",
          "original_form": "맞아요 ㅋㅋㅋ 제왑은 이제",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.16",
          "form": "아 또 신인내잖아요",
          "original_form": "아 또 신인내잖아요",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.17",
          "form": "제왑 여그룹",
          "original_form": "제왑 여그룹",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.18",
          "form": "있는 있지나 잘하지...",
          "original_form": "있는 있지나 잘하지...",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.19",
          "form": "아니 그니까요",
          "original_form": "아니 그니까요",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.20",
          "form": "있지한테 이상한 마피아 주면서 ..",
          "original_form": "있지한테 이상한 마피아 주면서 ..",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.21",
          "form": "마피아 같은 노래 줘놓고",
          "original_form": "마피아 같은 노래 줘놓고",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035663.1.22",
          "form": "띄울 생각을 하는게 ..",
          "original_form": "띄울 생각을 하는게 ..",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035663.1.23",
          "form": "저같으면 탈퇴했어요",
          "original_form": "저같으면 탈퇴했어요",
