      "utterance": [
        {
          "id": "MDRW2100039345.1.1",
          "form": "안녕하세요? 오늘 날씨 어때요?",
          "original_form": "안녕하세요? 오늘 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039345.1.2",
          "form": "안녕하세요 오늘 날씨가 쨍쨍하네요",
          "original_form": "안녕하세요 오늘 날씨가 쨍쨍하네요",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.3",
          "form": "저는 쨍쨍한 날을 별로 안좋아해서",
          "original_form": "저는 쨍쨍한 날을 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.4",
          "form": "가을인데 아직은 여름같네요",
          "original_form": "가을인데 아직은 여름같네요",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.5",
          "form": "name1님은 쨩쨍한 날 좋아하시나요?",
          "original_form": "&name1&님은 쨩쨍한 날 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.6",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.7",
          "form": "뜨거웠어요",
          "original_form": "뜨거웠어요",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.8",
          "form": "아 저도 쨍쨍한 날 안 좋아해요",
          "original_form": "아 저도 쨍쨍한 날 안 좋아해요",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039345.1.9",
          "form": "오히려 비 오는 날 좋아해요",
          "original_form": "오히려 비 오는 날 좋아해요",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100039345.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100039345.1.11",
          "form": "아 저도요 비오는 날은 집에 있는거 좋아해요",
          "original_form": "아 저도요 비오는 날은 집에 있는거 좋아해요",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039345.1.12",
          "form": "구름 많은 날이 좋아요",
          "original_form": "구름 많은 날이 좋아요",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039345.1.13",
          "form": "안 덥게",
          "original_form": "안 덥게",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039345.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039345.1.15",
          "form": "오 저두요!",
          "original_form": "오 저두요!",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039345.1.16",
          "form": "햇빛 싫어하거든요",
          "original_form": "햇빛 싫어하거든요",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039345.1.17",
          "form": "아직 가을볕이 강하네요",
          "original_form": "아직 가을볕이 강하네요",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039345.1.18",
          "form": "그러니까요 아직 가을 같지도 않아요ㅠㅠ",
          "original_form": "그러니까요 아직 가을 같지도 않아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039345.1.19",
          "form": "빨리 선선해져야 할텐데",
          "original_form": "빨리 선선해져야 할텐데",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039345.1.20",
          "form": "전 가을이 제일 좋아하는 계절인데 휙 지나갈것 같아요",
          "original_form": "전 가을이 제일 좋아하는 계절인데 휙 지나갈것 같아요",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039345.1.21",
          "form": "어떤 계절 좋아하나요?",
          "original_form": "어떤 계절 좋아하나요?",
          "speaker_id": "1",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039345.1.22",
          "form": "마자요 저는 겨울이요",
          "original_form": "마자요 저는 겨울이요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039345.1.23",
          "form": "추운날이 좋아요",
          "original_form": "추운날이 좋아요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039345.1.24",
          "form": "따뜻한 봄에 태어났는데 추운날을 좋아해요",
          "original_form": "따뜻한 봄에 태어났는데 추운날을 좋아해요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039345.1.25",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100039345.1.26",
          "form": "전 여름 겨울 둘 중 고르라면 겨울요",
          "original_form": "전 여름 겨울 둘 중 고르라면 겨울요",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039345.1.27",
          "form": "여름은 진짜 기빨리는듯 쥐약이에요",
          "original_form": "여름은 진짜 기빨리는듯 쥐약이에요",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039345.1.28",
          "form": "저도욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039345.1.29",
          "form": "아 여름 인정....",
          "original_form": "아 여름 인정....",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039345.1.30",
          "form": "진짜 하루하루 축쳐져요",
          "original_form": "진짜 하루하루 축쳐져요",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039345.1.31",
          "form": "더울수록 힘도 안나고",
          "original_form": "더울수록 힘도 안나고",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100039345.1.32",
          "form": "완전 공감!",
          "original_form": "완전 공감!",
