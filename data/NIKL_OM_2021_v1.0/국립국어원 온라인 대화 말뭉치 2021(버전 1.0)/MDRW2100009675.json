      "utterance": [
        {
          "id": "MDRW2100009675.1.1",
          "form": "나는 피자가 제일 좋아~",
          "original_form": "나는 피자가 제일 좋아~",
          "speaker_id": "2",
          "time": "20210823 03:01"
        },
        {
          "id": "MDRW2100009675.1.2",
          "form": "너는 무슨 음식이 가장 좋아?",
          "original_form": "너는 무슨 음식이 가장 좋아?",
          "speaker_id": "2",
          "time": "20210823 03:01"
        },
        {
          "id": "MDRW2100009675.1.3",
          "form": "나는 뼈해장국을 좋아해~~",
          "original_form": "나는 뼈해장국을 좋아해~~",
          "speaker_id": "1",
          "time": "20210823 03:11"
        },
        {
          "id": "MDRW2100009675.1.4",
          "form": "뼈해장국 맛있지",
          "original_form": "뼈해장국 맛있지",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009675.1.5",
          "form": "크아 지금 먹고싶다",
          "original_form": "크아 지금 먹고싶다",
          "speaker_id": "2",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009675.1.6",
          "form": "그리고 빵은 파리바게트 카스테라 구마 좋아해",
          "original_form": "그리고 빵은 파리바게트 카스테라 구마 좋아해",
          "speaker_id": "1",
          "time": "20210824 01:00"
        },
        {
          "id": "MDRW2100009675.1.7",
          "form": "너는 무슨 피자 좋아해?",
          "original_form": "너는 무슨 피자 좋아해?",
          "speaker_id": "1",
          "time": "20210824 01:00"
        },
        {
          "id": "MDRW2100009675.1.8",
          "form": "나는 새우 올라간 피자 좋아해",
          "original_form": "나는 새우 올라간 피자 좋아해",
          "speaker_id": "2",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100009675.1.9",
          "form": "너는 새우 잘 먹어?",
          "original_form": "너는 새우 잘 먹어?",
          "speaker_id": "2",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100009675.1.10",
          "form": "나도 새우 좋아해 ㅎㅎ 파스타에 있는 새우 좋아해",
          "original_form": "나도 새우 좋아해 ㅎㅎ 파스타에 있는 새우 좋아해",
          "speaker_id": "1",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100009675.1.11",
          "form": "또 다른 건 뭐 좋아해?",
          "original_form": "또 다른 건 뭐 좋아해?",
          "speaker_id": "1",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100009675.1.12",
          "form": "포테이토 피자도 좋아해",
          "original_form": "포테이토 피자도 좋아해",
          "speaker_id": "2",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100009675.1.13",
          "form": "너는 포테이토 피자 먹어?",
          "original_form": "너는 포테이토 피자 먹어?",
          "speaker_id": "2",
          "time": "20210824 01:03"
        },
        {
          "id": "MDRW2100009675.1.14",
          "form": "오 나 그거 완전 좋아해 ㅎㅎ 불고기 피자 급이야!",
          "original_form": "오 나 그거 완전 좋아해 ㅎㅎ 불고기 피자 급이야!",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100009675.1.15",
          "form": "불고기 피자도 좋아하는구나!",
          "original_form": "불고기 피자도 좋아하는구나!",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100009675.1.16",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해{emoji:♡}",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100009675.1.17",
          "form": "하와이안 피자 좋아해?",
          "original_form": "하와이안 피자 좋아해?",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100009675.1.18",
          "form": "피자 좋아하는 사람들이 그거 좋아하더라고",
          "original_form": "피자 좋아하는 사람들이 그거 좋아하더라고",
          "speaker_id": "1",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.19",
          "form": "좋아하는 편이야",
          "original_form": "좋아하는 편이야",
          "speaker_id": "2",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.20",
          "form": "너는 안좋아해?",
          "original_form": "너는 안좋아해?",
          "speaker_id": "2",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.21",
          "form": "따뜻한 파인애플이라..",
          "original_form": "따뜻한 파인애플이라..",
          "speaker_id": "1",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.22",
          "form": "나는 아직 먹어보질 못했어",
          "original_form": "나는 아직 먹어보질 못했어",
          "speaker_id": "1",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.23",
          "form": "피자몰 가봤어?",
          "original_form": "피자몰 가봤어?",
          "speaker_id": "1",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.24",
          "form": "ㅋㅋ 하긴 호불호가 갈리지",
          "original_form": "ㅋㅋ 하긴 호불호가 갈리지",
          "speaker_id": "2",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100009675.1.25",
          "form": "아니 저번에 가려고 했는데",
          "original_form": "아니 저번에 가려고 했는데",
