      "utterance": [
        {
          "id": "MDRW2100033784.1.1",
          "form": "근데 대기업이라 ㅋㅋ",
          "original_form": "근데 대기업이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033784.1.2",
          "form": "그럴일은 없을듯",
          "original_form": "그럴일은 없을듯",
          "speaker_id": "2",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033784.1.3",
          "form": "응응ㅋㅋ",
          "original_form": "응응ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033784.1.4",
          "form": "맞어ㅋㅋ",
          "original_form": "맞어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033784.1.5",
          "form": "우리만 ㅠㅠ 또르르",
          "original_form": "우리만 ㅠㅠ 또르르",
          "speaker_id": "2",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033784.1.6",
          "form": "구속되었다는 기사봤니...",
          "original_form": "구속되었다는 기사봤니...",
          "speaker_id": "2",
          "time": "20210825 01:43"
        },
        {
          "id": "MDRW2100033784.1.7",
          "form": "참 카카오 눈치겜 했어??",
          "original_form": "참 카카오 눈치겜 했어??",
          "speaker_id": "2",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033784.1.8",
          "form": "응 한번했어ㅋㅋ",
          "original_form": "응 한번했어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033784.1.9",
          "form": "그래 구속됐다하더라",
          "original_form": "그래 구속됐다하더라",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033784.1.10",
          "form": "결국 못받는거아이가ㅋㅋ",
          "original_form": "결국 못받는거아이가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033784.1.11",
          "form": "다 짜놨겠지.",
          "original_form": "다 짜놨겠지.",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100033784.1.12",
          "form": "조금만 다녀오면 그돈은 우리꺼다캄써",
          "original_form": "조금만 다녀오면 그돈은 우리꺼다캄써",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100033784.1.13",
          "form": "진짜 사기꾼들 살기좋은 세상이다.",
          "original_form": "진짜 사기꾼들 살기좋은 세상이다.",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100033784.1.14",
          "form": "ㅠㅠㅠㅠㅠㅠ 오늘은?? 하루에 32번까지 되잖아",
          "original_form": "ㅠㅠㅠㅠㅠㅠ 오늘은?? 하루에 32번까지 되잖아",
          "speaker_id": "2",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100033784.1.15",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100033784.1.16",
          "form": "열심히 해야겠네",
          "original_form": "열심히 해야겠네",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100033784.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.18",
          "form": "1회 했다는거니?",
          "original_form": "1회 했다는거니?",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.19",
          "form": "설마.",
          "original_form": "설마.",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.20",
          "form": "응ㅋㅋㅋㅋ1회",
          "original_form": "응ㅋㅋㅋㅋ1회",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.21",
          "form": "어허. 오빠랑 둘이서 공유하기 각각 하고 30번 주고 받아 그럼 각각 32회",
          "original_form": "어허. 오빠랑 둘이서 공유하기 각각 하고 30번 주고 받아 그럼 각각 32회",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.22",
          "form": "응  알겠어.  좋구만ㅋㅋㅋㅋ",
          "original_form": "응  알겠어.  좋구만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.23",
          "form": "최소3200원인데 한번이상 걸려.",
          "original_form": "최소3200원인데 한번이상 걸려.",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033784.1.24",
          "form": "오오 좋다좋다",
          "original_form": "오오 좋다좋다",
          "speaker_id": "1",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033784.1.25",
          "form": "그걸로 물건 살수있어",
          "original_form": "그걸로 물건 살수있어",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100033784.1.26",
          "form": "그러니깐 아름답네 열심히 해야겠군",
          "original_form": "그러니깐 아름답네 열심히 해야겠군",
