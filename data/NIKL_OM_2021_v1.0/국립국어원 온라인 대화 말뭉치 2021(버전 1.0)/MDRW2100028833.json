      "utterance": [
        {
          "id": "MDRW2100028833.1.1",
          "form": "name1님은 운동하시나요?",
          "original_form": "&name1&님은 운동하시나요?",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028833.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028833.1.3",
          "form": "최근에 시작했어요",
          "original_form": "최근에 시작했어요",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028833.1.4",
          "form": "저는 해야지 해야지 생각만하고",
          "original_form": "저는 해야지 해야지 생각만하고",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028833.1.5",
          "form": "안하고 있어요",
          "original_form": "안하고 있어요",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028833.1.6",
          "form": "운동은 진짜 처음이 어려운 것 같아요",
          "original_form": "운동은 진짜 처음이 어려운 것 같아요",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028833.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028833.1.8",
          "form": "처음 스타트를 하는게",
          "original_form": "처음 스타트를 하는게",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028833.1.9",
          "form": "힘들더라구여",
          "original_form": "힘들더라구여",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028833.1.10",
          "form": "저도 지금 시작한지",
          "original_form": "저도 지금 시작한지",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028833.1.11",
          "form": "1달 조금 안되는데",
          "original_form": "1달 조금 안되는데",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028833.1.12",
          "form": "아직도 운동 시작할때는 밍기적거려요",
          "original_form": "아직도 운동 시작할때는 밍기적거려요",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028833.1.13",
          "form": "흑흑 사람은 다 똑같군요",
          "original_form": "흑흑 사람은 다 똑같군요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028833.1.14",
          "form": "그래도 막상 시작하면 금방 하니까",
          "original_form": "그래도 막상 시작하면 금방 하니까",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028833.1.15",
          "form": "name2님도 힘내세요!",
          "original_form": "&name2&님도 힘내세요!",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028833.1.16",
          "form": "감사합니다.",
          "original_form": "감사합니다.",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028833.1.17",
          "form": "저도 내일부터는 꼭!",
          "original_form": "저도 내일부터는 꼭!",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028833.1.18",
          "form": "ㅋㅋㅋㅋ그래놓고 내일에도 내일부터 꼭! 하시는거 아니죠?",
          "original_form": "ㅋㅋㅋㅋ그래놓고 내일에도 내일부터 꼭! 하시는거 아니죠?",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028833.1.19",
          "form": "ㅋㅋㅋㅋㅋ그럴수도 있어요",
          "original_form": "ㅋㅋㅋㅋㅋ그럴수도 있어요",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028833.1.20",
          "form": "제가 내일 꼭 물어볼거에요!",
          "original_form": "제가 내일 꼭 물어볼거에요!",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028833.1.21",
          "form": "ㅋㅋㅋㅋ알겠습니다",
          "original_form": "ㅋㅋㅋㅋ알겠습니다",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028833.1.22",
          "form": "꼭 해야겠네요",
          "original_form": "꼭 해야겠네요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        }
      ]
