      "utterance": [
        {
          "id": "MDRW2100012538.1.1",
          "form": "안녕하세요! name2은 드라마 좋아하세요?",
          "original_form": "안녕하세요! &name2&은 드라마 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012538.1.2",
          "form": "안녕하세요! 드라마 자주는 안보는데",
          "original_form": "안녕하세요! 드라마 자주는 안보는데",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012538.1.3",
          "form": "요즘 괴물, 로스쿨 봤어요",
          "original_form": "요즘 괴물, 로스쿨 봤어요",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012538.1.4",
          "form": "헐!! 저도 로스쿨 엄청 재밌게 봤어요!!",
          "original_form": "헐!! 저도 로스쿨 엄청 재밌게 봤어요!!",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012538.1.5",
          "form": "누굴 젤 좋아하셨어요 ㅎㅎ ?",
          "original_form": "누굴 젤 좋아하셨어요 ㅎㅎ ?",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012538.1.6",
          "form": "저 다 좋아요",
          "original_form": "저 다 좋아요",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012538.1.7",
          "form": "모든 인물들이 매력적이죠",
          "original_form": "모든 인물들이 매력적이죠",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012538.1.8",
          "form": "한명씩 사건 터지는게 재밌었어요ㅋㅋㅋ",
          "original_form": "한명씩 사건 터지는게 재밌었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012538.1.9",
          "form": "인간미?가 보인다고 해야하나",
          "original_form": "인간미?가 보인다고 해야하나",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012538.1.10",
          "form": "맞아요!!!!! 드라마가 특이하게 모든 인물을 중심으로 서사가 전개되더라고요!!",
          "original_form": "맞아요!!!!! 드라마가 특이하게 모든 인물을 중심으로 서사가 전개되더라고요!!",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.11",
          "form": "각자 욕망에 충실했다가 사건 해결해나가는 게 인상적이었어요",
          "original_form": "각자 욕망에 충실했다가 사건 해결해나가는 게 인상적이었어요",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.12",
          "form": "전 특히 그 김교수님인가",
          "original_form": "전 특히 그 김교수님인가",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.13",
          "form": "그 여자 교수님을 좋아했어요!!",
          "original_form": "그 여자 교수님을 좋아했어요!!",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.14",
          "form": "맞아요 김교수님 인상적이죠",
          "original_form": "맞아요 김교수님 인상적이죠",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.15",
          "form": "맞아요!!!! 다 이어져있는게 신기했던 사건..",
          "original_form": "맞아요!!!! 다 이어져있는게 신기했던 사건..",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.16",
          "form": "인기 교수님",
          "original_form": "인기 교수님",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012538.1.18",
          "form": "처음에 풀려난 범인",
          "original_form": "처음에 풀려난 범인",
