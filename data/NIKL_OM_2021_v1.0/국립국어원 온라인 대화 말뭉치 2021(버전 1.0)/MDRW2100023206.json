      "utterance": [
        {
          "id": "MDRW2100023206.1.1",
          "form": "갈때마다 꾹 참고있어요 ㅋㅋ",
          "original_form": "갈때마다 꾹 참고있어요 ㅋㅋ",
          "speaker_id": "2",
