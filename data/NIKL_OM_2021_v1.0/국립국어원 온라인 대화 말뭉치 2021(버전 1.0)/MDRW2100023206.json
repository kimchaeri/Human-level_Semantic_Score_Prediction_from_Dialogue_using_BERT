        },
        {
          "id": "MDRW2100023206.1.2",
          "form": "앗 name1은 재택근무 일자리에 잘 어울리시네요 프리랜서시니ㅎㅎ",
          "original_form": "앗 &name1&은 재택근무 일자리에 잘 어울리시네요 프리랜서시니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023206.1.3",
          "form": "근데 사실 재택근무 싫어한다눈",
          "original_form": "근데 사실 재택근무 싫어한다눈",
          "speaker_id": "1",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023206.1.4",
          "form": "집이 사무실이 되면..",
          "original_form": "집이 사무실이 되면..",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.5",
          "form": "휴식이 없어여",
          "original_form": "휴식이 없어여",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.6",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.7",
          "form": "헉....",
          "original_form": "헉....",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.8",
          "form": "생각못했는데",
          "original_form": "생각못했는데",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.9",
          "form": "그런 단점이 또 있을수있네요ㅠㅠ",
          "original_form": "그런 단점이 또 있을수있네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.10",
          "form": "사무실가서 에어컨쐬면서",
          "original_form": "사무실가서 에어컨쐬면서",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.11",
          "form": "과자랑 커피도먹고",
          "original_form": "과자랑 커피도먹고",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.12",
          "form": "수다도 떨면서",
          "original_form": "수다도 떨면서",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.13",
          "form": "그쵸그쵸 사무실 에어컨이랑 간식은",
          "original_form": "그쵸그쵸 사무실 에어컨이랑 간식은",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.14",
          "form": "무료니까 ㅋㅋㅋ",
          "original_form": "무료니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.15",
          "form": "일하면 좋은데여",
          "original_form": "일하면 좋은데여",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.16",
          "form": "근데 출퇴근 지옥철 안견뎌도 되니",
          "original_form": "근데 출퇴근 지옥철 안견뎌도 되니",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023206.1.17",
          "form": "그건 또 좋을 것 같아요!",
          "original_form": "그건 또 좋을 것 같아요!",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.18",
          "form": "근데 작가들처럼 혼자서 가만히",
          "original_form": "근데 작가들처럼 혼자서 가만히",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.19",
          "form": "신경질내면서 일해야되는 사람들은",
          "original_form": "신경질내면서 일해야되는 사람들은",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.20",
          "form": "집에서 일하는게 더 좋을 것 같네요",
          "original_form": "집에서 일하는게 더 좋을 것 같네요",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.21",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.22",
          "form": "그런 약간 예술가적인 기질이 있는 분은",
          "original_form": "그런 약간 예술가적인 기질이 있는 분은",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.23",
          "form": "다른 사람들이랑 부딪히면",
          "original_form": "다른 사람들이랑 부딪히면",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.24",
          "form": "서로간 스트레스니까....ㅎㅎㅎ",
          "original_form": "서로간 스트레스니까....ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.25",
          "form": "코로나가 심해지면서 재택근무도 많이 활성화된것같아요",
          "original_form": "코로나가 심해지면서 재택근무도 많이 활성화된것같아요",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.26",
          "form": "내향성 강한 인간분들",
          "original_form": "내향성 강한 인간분들",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023206.1.27",
          "form": "재택근무 일자리 개이득이죠",
          "original_form": "재택근무 일자리 개이득이죠",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023206.1.28",
          "form": "저는 외향적이어서",
          "original_form": "저는 외향적이어서",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023206.1.29",
          "form": "회의도 직접만나는게좋고요",
          "original_form": "회의도 직접만나는게좋고요",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023206.1.30",
          "form": "맞아요~~~ 코로나 심해지면서 원래 재택이 아니던 분들도",
          "original_form": "맞아요~~~ 코로나 심해지면서 원래 재택이 아니던 분들도",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023206.1.31",
          "form": "재택 하시고 그러더라구요 ㅎㅎ",
          "original_form": "재택 하시고 그러더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023206.1.32",
          "form": "저는 내향성이 강한데... 저는 재택으로 일을 하면 참 좋을 것 같아요ㅠㅠ",
          "original_form": "저는 내향성이 강한데... 저는 재택으로 일을 하면 참 좋을 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023206.1.33",
          "form": "사람들이랑 부대끼는게 스트레스여서요",
          "original_form": "사람들이랑 부대끼는게 스트레스여서요",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023206.1.34",
          "form": "name1은 원래는 회사를 다니셨나요?",
          "original_form": "&name1&은 원래는 회사를 다니셨나요?",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023206.1.35",
          "form": "네 작년에 일을 넘 많이해가지그",
          "original_form": "네 작년에 일을 넘 많이해가지그",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023206.1.36",
          "form": "번아웃 ㅋㅋㅋ",
          "original_form": "번아웃 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023206.1.37",
          "form": "친구회시라 일주일에 두번만 나가다가",
          "original_form": "친구회시라 일주일에 두번만 나가다가",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023206.1.38",
          "form": "회의만 참석하고 재택하는 형식으로 일했었어요",
          "original_form": "회의만 참석하고 재택하는 형식으로 일했었어요",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023206.1.39",
          "form": "10월까지는 아예 쉬기로 했어요",
          "original_form": "10월까지는 아예 쉬기로 했어요",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023206.1.40",
          "form": "헉.... 엄청 열심히 하셨나봐요....",
          "original_form": "헉.... 엄청 열심히 하셨나봐요....",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023206.1.41",
          "form": "쉴때는 쉬어주는게 필요한 것 같아요ㅠㅠㅠ",
          "original_form": "쉴때는 쉬어주는게 필요한 것 같아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023206.1.42",
          "form": "저희가... 50대 60대까진 일을 할텐데",
          "original_form": "저희가... 50대 60대까진 일을 할텐데",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023206.1.43",
          "form": "넘 힘들게 달리면 나중에 못버틸수도 잇으니까....",
          "original_form": "넘 힘들게 달리면 나중에 못버틸수도 잇으니까....",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023206.1.44",
          "form": "그런의미로 제주님 제주까지가신거 정말 대단하셔요 ㅎㅎㅎㅎ",
          "original_form": "그런의미로 제주님 제주까지가신거 정말 대단하셔요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023206.1.45",
          "form": "하하 엉덩이가 가벼워서",
          "original_form": "하하 엉덩이가 가벼워서",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023206.1.46",
          "form": "카페에서 일하거나",
          "original_form": "카페에서 일하거나",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023206.1.47",
          "form": "공유오피스빌려서 일하지",
          "original_form": "공유오피스빌려서 일하지",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023206.1.48",
          "form": "사실 재택근무는 생각보다 어려워요",
          "original_form": "사실 재택근무는 생각보다 어려워요",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023206.1.49",
          "form": "특히 출근개념이 없으니",
          "original_form": "특히 출근개념이 없으니",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023206.1.50",
          "form": "급한건이 있어야 제대로 일을 합니다",
          "original_form": "급한건이 있어야 제대로 일을 합니다",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023206.1.51",
          "form": "아 진짜요???",
          "original_form": "아 진짜요???",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023206.1.52",
          "form": "근데 생각해보니까 그렇네요ㅠㅠㅠ",
          "original_form": "근데 생각해보니까 그렇네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023206.1.53",
          "form": "저도 집에있다보면 집안일을 차일피일 미루게되던데",
          "original_form": "저도 집에있다보면 집안일을 차일피일 미루게되던데",
          "speaker_id": "2",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023206.1.54",
          "form": "재택근무도...그렇게 될 수도 있겠어요ㅠㅠㅠ",
          "original_form": "재택근무도...그렇게 될 수도 있겠어요ㅠㅠㅠ",
