      "utterance": [
        {
          "id": "MDRW2100016553.1.1",
          "form": "최근에 산 물품 있으세요?",
          "original_form": "최근에 산 물품 있으세요?",
          "speaker_id": "1",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100016553.1.2",
          "form": "식료품 같은거 사요 ㅎㅎ",
          "original_form": "식료품 같은거 사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016553.1.3",
          "form": "청소도구하고",
          "original_form": "청소도구하고",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016553.1.4",
          "form": "살림과 관련된걸 사셨군요ㅎㅎ",
          "original_form": "살림과 관련된걸 사셨군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016553.1.5",
          "form": "수집하는 취미는 없으신가요?",
          "original_form": "수집하는 취미는 없으신가요?",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016553.1.6",
          "form": "저 레고요 ㅎㅎ",
          "original_form": "저 레고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016553.1.7",
          "form": "장식장도 있어요 ㅎㅎ",
          "original_form": "장식장도 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016553.1.8",
          "form": "우와 많이 모으셨나요?",
          "original_form": "우와 많이 모으셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016553.1.9",
          "form": "네ㅋㅋ기차도 있고 우주선도 있고 건축물도 ㅎㅎ",
          "original_form": "네ㅋㅋ기차도 있고 우주선도 있고 건축물도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016553.1.10",
          "form": "우와 그거 조립하는데 많은 시간이 걸리던데",
          "original_form": "우와 그거 조립하는데 많은 시간이 걸리던데",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016553.1.11",
          "form": "정말 좋아하시나봐요",
          "original_form": "정말 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100016553.1.12",
          "form": "네 더 사고 \u0000싶은데 ㅋㅋ",
          "original_form": "네 더 사고 \u0000싶은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100016553.1.13",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100016553.1.14",
          "form": "저는 인형 모으는거 좋아해요ㅎㅎ",
          "original_form": "저는 인형 모으는거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100016553.1.15",
          "form": "인형 누가 사줘요?ㅎㅎ",
          "original_form": "인형 누가 사줘요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100016553.1.16",
          "form": "제가 사죠ㅋㅋㅋㅋㅋ",
