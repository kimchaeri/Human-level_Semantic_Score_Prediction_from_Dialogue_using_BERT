      "utterance": [
        {
          "id": "MDRW2100034166.1.1",
          "form": "슬라임에 푹 빠져있나요 ㅋㅋㅋㅋㅋ",
          "original_form": "슬라임에 푹 빠져있나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100034166.1.2",
          "form": "어떤 거 사셨어요?",
          "original_form": "어떤 거 사셨어요?",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100034166.1.3",
          "form": "아 이름이 넘 어려운디... 그 난사눈꽃....?",
          "original_form": "아 이름이 넘 어려운디... 그 난사눈꽃....?",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100034166.1.4",
          "form": "길잃은 토끼의 정원....?",
          "original_form": "길잃은 토끼의 정원....?",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100034166.1.5",
          "form": "또잉.. 찾아보고 올게요 ㅎㅎ",
          "original_form": "또잉.. 찾아보고 올게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100034166.1.6",
          "form": "찾을수가 없네요 ㅋㅋㅋㅋ",
          "original_form": "찾을수가 없네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100034166.1.7",
          "form": "되게 안에 비즈 많고 재밌을 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "되게 안에 비즈 많고 재밌을 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100034166.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 이름이 잘 생각으 안나네욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 이름이 잘 생각으 안나네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100034166.1.9",
          "form": "맞아요 비즈가 많아요!",
          "original_form": "맞아요 비즈가 많아요!",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100034166.1.10",
          "form": "그래서 뽀다닥소리 나더라구요",
          "original_form": "그래서 뽀다닥소리 나더라구요",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100034166.1.11",
          "form": "이걸 왜갖고놀지 했는데 재밌더구먼유....",
          "original_form": "이걸 왜갖고놀지 했는데 재밌더구먼유....",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100034166.1.12",
          "form": "이렇게 슬라임의 길로 들어서시는군요 ㅋㅋㅋㅋ",
          "original_form": "이렇게 슬라임의 길로 들어서시는군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100034166.1.13",
          "form": "전 한동안 보석비즈에 빠져있다가 진짜 거북이처럼 굽을 것 같아서 그만뒀어요 ㅋㅋㅋㅋ",
          "original_form": "전 한동안 보석비즈에 빠져있다가 진짜 거북이처럼 굽을 것 같아서 그만뒀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100034166.1.14",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 망했나요 저....",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 망했나요 저....",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100034166.1.15",
          "form": "거북목되나요...",
          "original_form": "거북목되나요...",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100034166.1.16",
          "form": "아뇨아뇨 보석비즈는 그.. 보석알갱이를 그림판에 붙여서 큰 그림을 완성하는거에요!",
          "original_form": "아뇨아뇨 보석비즈는 그.. 보석알갱이를 그림판에 붙여서 큰 그림을 완성하는거에요!",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034166.1.17",
          "form": "거북목 안되실 거에요 ㅋㅋㅋㅋㅋ",
          "original_form": "거북목 안되실 거에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034166.1.18",
          "form": "아!!! 뭔지 알아욧",
          "original_form": "아!!! 뭔지 알아욧",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034166.1.19",
          "form": "그거 이름이 보석비즈였군요",
          "original_form": "그거 이름이 보석비즈였군요",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034166.1.20",
          "form": "그것도 재밌어보이던데....",
          "original_form": "그것도 재밌어보이던데....",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034166.1.21",
          "form": "재밌어요! 시간 엄청 잘 가는데 목이랑 어깨가 너무 아파요 ㅋㅋㅋㅋㅋ",
          "original_form": "재밌어요! 시간 엄청 잘 가는데 목이랑 어깨가 너무 아파요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034166.1.22",
          "form": "슬라임 끝나고 보석비즈 시작하시는거 아니에요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "슬라임 끝나고 보석비즈 시작하시는거 아니에요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034166.1.23",
          "form": "약간 가능성 매우높아요",
          "original_form": "약간 가능성 매우높아요",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034166.1.24",
          "form": "당신 정말 디매킹부터 아주 유죄야....",
          "original_form": "당신 정말 디매킹부터 아주 유죄야....",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034166.1.25",
          "form": "이 죄를 다 어찌 갚지요 ㅋㅋㅋㅋㅋ",
          "original_form": "이 죄를 다 어찌 갚지요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034166.1.26",
          "form": "취미부자 만들어주게 생겼네요 ㅋㅋㅋㅋㅋ",
          "original_form": "취미부자 만들어주게 생겼네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100034166.1.27",
          "form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래도 취미가 있는거슨 좋지요,,,",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그래도 취미가 있는거슨 좋지요,,,",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034166.1.28",
          "form": "아 그럼 또 하나 알려줘야하는데",
          "original_form": "아 그럼 또 하나 알려줘야하는데",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100034166.1.29",
          "form": "이름이 기억이 안나요 ㅋㅋㅋㅋㅋ",
          "original_form": "이름이 기억이 안나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100034166.1.30",
          "form": "슬라임 다음 버전인데....",
          "original_form": "슬라임 다음 버전인데....",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100034166.1.31",
          "form": "속은 슬라임처럼 말랑한데 겉은 바삭한 껍질 같은게 있어서 빠드득 부수는 재미가 있대요 ㅋㅋㅋㅋ",
          "original_form": "속은 슬라임처럼 말랑한데 겉은 바삭한 껍질 같은게 있어서 빠드득 부수는 재미가 있대요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100034166.1.32",
          "form": "이름이 기억이 나야 또 취미 추가되는데... 초조하다..",
          "original_form": "이름이 기억이 나야 또 취미 추가되는데... 초조하다..",
          "speaker_id": "2",
