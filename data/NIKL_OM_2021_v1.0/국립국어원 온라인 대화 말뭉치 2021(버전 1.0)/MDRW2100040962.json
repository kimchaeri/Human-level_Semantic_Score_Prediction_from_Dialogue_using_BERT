        },
        {
          "id": "MDRW2100040962.1.3",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100040962.1.4",
          "form": "그게 끝이 아니야?",
          "original_form": "그게 끝이 아니야?",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100040962.1.5",
          "form": "섬도 하나 더 열린단 소리가",
          "original_form": "섬도 하나 더 열린단 소리가",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100040962.1.6",
          "form": "찌라시로 도는데??",
          "original_form": "찌라시로 도는데??",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100040962.1.7",
          "form": "하긴 세이라이섬",
          "original_form": "하긴 세이라이섬",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100040962.1.8",
          "form": "너무 혼자 툭 튀어나오긴 했어",
          "original_form": "너무 혼자 툭 튀어나오긴 했어",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040962.1.9",
          "form": "헐 토마 렙업 재료가",
          "original_form": "헐 토마 렙업 재료가",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040962.1.10",
          "form": "나 일단 시뇨라 잡으러감",
          "original_form": "나 일단 시뇨라 잡으러감",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040962.1.11",
          "form": "2.2에서 열리는 섬의 채집물이래",
          "original_form": "2.2에서 열리는 섬의 채집물이래",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040962.1.12",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100040962.1.13",
          "form": "잡고 한참 스토리 봐야할텐데",
          "original_form": "잡고 한참 스토리 봐야할텐데",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100040962.1.14",
          "form": "잘 보고와...",
          "original_form": "잘 보고와...",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100040962.1.15",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.16",
          "form": "이게",
          "original_form": "이게",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.17",
          "form": "스토리라고???",
          "original_form": "스토리라고???",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.18",
          "form": "이게??",
          "original_form": "이게??",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.19",
          "form": "이게???",
          "original_form": "이게???",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.20",
          "form": "나 약간",
          "original_form": "나 약간",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.21",
          "form": "어처구니가 없는데",
          "original_form": "어처구니가 없는데",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.22",
          "form": "나의",
          "original_form": "나의",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.23",
          "form": "걍 야에쇼 아니고?",
          "original_form": "걍 야에쇼 아니고?",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.24",
          "form": "이 황당함이",
          "original_form": "이 황당함이",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.25",
          "form": "이해가되지",
          "original_form": "이해가되지",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100040962.1.26",
          "form": "1부터 10까지",
          "original_form": "1부터 10까지",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100040962.1.27",
          "form": "문제가 아닌게 없어서",
          "original_form": "문제가 아닌게 없어서",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100040962.1.28",
          "form": "잠만",
          "original_form": "잠만",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100040962.1.29",
          "form": "내가 왜 글케 빨리 멘스 보고 나랑 얘기해달라고 한지 알겠지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내가 왜 글케 빨리 멘스 보고 나랑 얘기해달라고 한지 알겠지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
