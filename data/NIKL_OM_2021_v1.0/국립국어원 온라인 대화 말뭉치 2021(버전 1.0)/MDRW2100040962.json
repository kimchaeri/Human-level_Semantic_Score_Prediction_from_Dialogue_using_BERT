          "original_form": "죽었다고?????",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100040962.1.34",
          "form": "아니 왜???",
          "original_form": "아니 왜???",
          "speaker_id": "2",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100040962.1.35",
          "form": "왜 캐를 그렇게 써?",
          "original_form": "왜 캐를 그렇게 써?",
          "speaker_id": "2",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100040962.1.36",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040962.1.37",
          "form": "허무함",
          "original_form": "허무함",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040962.1.38",
          "form": "ㄹㅇ....",
          "original_form": "ㄹㅇ....",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040962.1.39",
          "form": "심지어 주인공이 성장해서 죽인 것도 아니고",
          "original_form": "심지어 주인공이 성장해서 죽인 것도 아니고",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040962.1.40",
          "form": "아니 그것도",
          "original_form": "아니 그것도",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.41",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.42",
          "form": "걍 라이덴이?",
          "original_form": "걍 라이덴이?",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.43",
          "form": "걍 졌다고 라이덴이 처단해버림 ㅋㅋㅋ",
          "original_form": "걍 졌다고 라이덴이 처단해버림 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.44",
          "form": "슥삭?",
          "original_form": "슥삭?",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.45",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.46",
          "form": "그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:25"
        },
        {
          "id": "MDRW2100040962.1.47",
          "form": "분명 주인공은",
          "original_form": "분명 주인공은",
