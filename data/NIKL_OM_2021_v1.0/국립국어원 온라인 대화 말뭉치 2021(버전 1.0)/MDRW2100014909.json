      "utterance": [
        {
          "id": "MDRW2100014909.1.1",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014909.1.2",
          "form": "서로 잘맞고 안싸우면 결혼이 좋죠ㄹ",
          "original_form": "서로 잘맞고 안싸우면 결혼이 좋죠ㄹ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014909.1.3",
          "form": "한번 해보셔야지 ㅎㅎㅎ",
          "original_form": "한번 해보셔야지 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014909.1.4",
          "form": "백날말해도 몰아요ㅎㅎ",
          "original_form": "백날말해도 몰아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014909.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014909.1.6",
          "form": "네 느낌이 안와요 ㅋㅋㅋ",
          "original_form": "네 느낌이 안와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014909.1.7",
          "form": "여자친구분이랑 어때요?",
          "original_form": "여자친구분이랑 어때요?",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014909.1.8",
          "form": "잘맞으신가요?",
          "original_form": "잘맞으신가요?",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014909.1.9",
          "form": "연애와 결혼은 다르대요",
          "original_form": "연애와 결혼은 다르대요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014909.1.10",
          "form": "잘맞는거 같은데",
          "original_form": "잘맞는거 같은데",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014909.1.11",
          "form": "결혼하면?",
          "original_form": "결혼하면?",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014909.1.12",
          "form": "과연",
          "original_form": "과연",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014909.1.13",
          "form": "처음에는 다 잘맞을꺼에요ㅎ",
          "original_form": "처음에는 다 잘맞을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014909.1.14",
          "form": "지금 얼마나되셨는대요?",
          "original_form": "지금 얼마나되셨는대요?",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014909.1.15",
          "form": "5년요",
          "original_form": "5년요",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014909.1.16",
          "form": "연애가  길긴한데",
          "original_form": "연애가  길긴한데",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014909.1.17",
          "form": "그러면 결혼해도 되겟는데요?",
          "original_form": "그러면 결혼해도 되겟는데요?",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014909.1.18",
          "form": "빨리하세요ㅎㅎ",
          "original_form": "빨리하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014909.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.20",
          "form": "저도 8년연애하다가 헤어졌는데... 너무 힘들었어요ㅎ",
          "original_form": "저도 8년연애하다가 헤어졌는데... 너무 힘들었어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.21",
          "form": "계속 같이 있을것 같았는데",
          "original_form": "계속 같이 있을것 같았는데",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.22",
          "form": "헐...8년",
          "original_form": "헐...8년",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.23",
          "form": "안되요 그게장",
          "original_form": "안되요 그게장",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.24",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.25",
          "form": "잘... ㅠ",
          "original_form": "잘... ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014909.1.26",
          "form": "그러니ㅎㅎ 좋은사람놓치기전에ㅎㅎ",
          "original_form": "그러니ㅎㅎ 좋은사람놓치기전에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014909.1.27",
          "form": "언능하세요ㅎㅎ",
          "original_form": "언능하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014909.1.28",
          "form": "네 언능할게요ㅋㅋ",
          "original_form": "네 언능할게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014909.1.29",
          "form": "ㅎㅎ 집안일 이야기가 나와서ㅎ",
          "original_form": "ㅎㅎ 집안일 이야기가 나와서ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100014909.1.30",
          "form": "그럼 친구분은 집안일 어디까지도와줘요?",
          "original_form": "그럼 친구분은 집안일 어디까지도와줘요?",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100014909.1.31",
          "form": "저도 많이 도와주는편인데",
          "original_form": "저도 많이 도와주는편인데",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100014909.1.32",
          "form": "너무 만족을 못하네요ㅠ",
          "original_form": "너무 만족을 못하네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        }
      ]
    }
