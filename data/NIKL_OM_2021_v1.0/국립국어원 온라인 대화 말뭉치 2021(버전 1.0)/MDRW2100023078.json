      "utterance": [
        {
          "id": "MDRW2100023078.1.1",
          "form": "매운 맛이 있나요?",
          "original_form": "매운 맛이 있나요?",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023078.1.2",
          "form": "매콤한 맛이 나나요?",
          "original_form": "매콤한 맛이 나나요?",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023078.1.3",
          "form": "과자 자체는 맵지 않아요!",
          "original_form": "과자 자체는 맵지 않아요!",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023078.1.4",
          "form": "양파가 들어가나요?",
          "original_form": "양파가 들어가나요?",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023078.1.5",
          "form": "과자를 찍어 먹는 소스에는 양파가 들어가요",
          "original_form": "과자를 찍어 먹는 소스에는 양파가 들어가요",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023078.1.6",
          "form": "오감자 사워크림맛?!",
          "original_form": "오감자 사워크림맛?!",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023078.1.7",
          "form": "틀~렸습니다~",
          "original_form": "틀~렸습니다~",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023078.1.8",
          "form": "오감자 새우깡 이런 과자는 아니에여",
          "original_form": "오감자 새우깡 이런 과자는 아니에여",
          "speaker_id": "2",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023078.1.9",
          "form": "크아아아악",
          "original_form": "크아아아악",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023078.1.10",
          "form": "감자 과자인가요?",
          "original_form": "감자 과자인가요?",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023078.1.11",
          "form": "감자 안들어갑니다!",
          "original_form": "감자 안들어갑니다!",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023078.1.12",
          "form": "한국 과자인가요?",
          "original_form": "한국 과자인가요?",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023078.1.13",
          "form": "한국에서도 만들긴 하지만",
          "original_form": "한국에서도 만들긴 하지만",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023078.1.14",
          "form": "기원이 한국은 아닙니다",
          "original_form": "기원이 한국은 아닙니다",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023078.1.15",
          "form": "멕시코가 기원인가요?",
          "original_form": "멕시코가 기원인가요?",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023078.1.16",
          "form": "허걱 맞아요",
          "original_form": "허걱 맞아요",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023078.1.17",
          "form": "ㅋㅋㅋㅋㅋ명예멕시코인 해일",
          "original_form": "ㅋㅋㅋㅋㅋ명예멕시코인 해일",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023078.1.18",
          "form": "나쵸?!",
          "original_form": "나쵸?!",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023078.1.19",
          "form": "나쵸 사워크림",
          "original_form": "나쵸 사워크림",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023078.1.20",
          "form": "헉 나쵸 딩동댕!",
          "original_form": "헉 나쵸 딩동댕!",
          "speaker_id": "2",
