      "utterance": [
        {
          "id": "MDRW2100038680.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100038680.1.2",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038680.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038680.1.4",
          "form": "키워본적도 없어요ㅠ",
          "original_form": "키워본적도 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100038680.1.5",
          "form": "키우고 싶은 친구는 있나요?",
          "original_form": "키우고 싶은 친구는 있나요?",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100038680.1.6",
          "form": "전 고양이요 ㅋㅋ",
          "original_form": "전 고양이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038680.1.7",
          "form": "근데 그냥 남의 반려동물 보는 걸로 만족하고 있어요",
          "original_form": "근데 그냥 남의 반려동물 보는 걸로 만족하고 있어요",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038680.1.8",
          "form": "키울 자신은 없거든요",
          "original_form": "키울 자신은 없거든요",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038680.1.9",
          "form": "저도 키울 자신이 없어서 못키우겠어요",
          "original_form": "저도 키울 자신이 없어서 못키우겠어요",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038680.1.10",
          "form": "관리가 빡빡하다고 하더군요",
          "original_form": "관리가 빡빡하다고 하더군요",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038680.1.11",
          "form": "관리두 그렇구...",
          "original_form": "관리두 그렇구...",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038680.1.12",
          "form": "한 생명을 죽을 때까지 책임 질 자신이 없어요ㅠ",
          "original_form": "한 생명을 죽을 때까지 책임 질 자신이 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038680.1.13",
          "form": "말을 할 수 있는 애도 아닌데ㅠ",
          "original_form": "말을 할 수 있는 애도 아닌데ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038680.1.14",
          "form": "맞아요 표현을 못한다는게 참 그렇죠",
          "original_form": "맞아요 표현을 못한다는게 참 그렇죠",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038680.1.15",
          "form": "바로바로 알수 있으면 좋을텐데",
          "original_form": "바로바로 알수 있으면 좋을텐데",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038680.1.16",
          "form": "그래서 그 강아지번역이 이런 물건도 나오고 하나봐요",
          "original_form": "그래서 그 강아지번역이 이런 물건도 나오고 하나봐요",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038680.1.17",
          "form": "친구도 강아지를 키우는데 얘가 아프거나 불편한 걸 말을 못 하는게",
          "original_form": "친구도 강아지를 키우는데 얘가 아프거나 불편한 걸 말을 못 하는게",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038680.1.18",
          "form": "걱정된자 하더라고요",
          "original_form": "걱정된자 하더라고요",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038680.1.19",
          "form": "맞아요 그럴때가 제일 불편하죠",
          "original_form": "맞아요 그럴때가 제일 불편하죠",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038680.1.20",
          "form": "하는수 없이 결국 정기검진 받는거 같아요 그럴땐",
          "original_form": "하는수 없이 결국 정기검진 받는거 같아요 그럴땐",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038680.1.21",
          "form": "근데 동물병원은 의료 보험도 안 되잖아요ㅠ",
          "original_form": "근데 동물병원은 의료 보험도 안 되잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038680.1.22",
          "form": "병원 검진비 장난 아니더라구요ㅠㅜ 약도 다 비급여라 비싸고ㅠㅠ",
          "original_form": "병원 검진비 장난 아니더라구요ㅠㅜ 약도 다 비급여라 비싸고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100038680.1.23",
          "form": "맞아요 동물병원비가 후덜덜하죠",
          "original_form": "맞아요 동물병원비가 후덜덜하죠",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100038680.1.24",
          "form": "그쪽은 보험 언제 생기나 몰라요",
          "original_form": "그쪽은 보험 언제 생기나 몰라요",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038680.1.25",
          "form": "보험이 있긴 한 것 같더라구요",
          "original_form": "보험이 있긴 한 것 같더라구요",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100038680.1.26",
          "form": "펫케어 서비스 뭐 그런 식으로 ㅋㅋ",
          "original_form": "펫케어 서비스 뭐 그런 식으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100038680.1.27",
          "form": "외국은 이런 보험이나 진료도 잘 돼 있다구 하던데 부러워요",
          "original_form": "외국은 이런 보험이나 진료도 잘 돼 있다구 하던데 부러워요",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100038680.1.28",
          "form": "키울려면 외국에 나가야하는군요",
          "original_form": "키울려면 외국에 나가야하는군요",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100038680.1.29",
          "form": "ㅋㅋㅋ우리나라는 아직 반려동물에 대한 인식 자체가 좀 미미하니까요",
          "original_form": "ㅋㅋㅋ우리나라는 아직 반려동물에 대한 인식 자체가 좀 미미하니까요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038680.1.30",
          "form": "그래도 요즘엔 좋아진거 같아요",
          "original_form": "그래도 요즘엔 좋아진거 같아요",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038680.1.31",
          "form": "애완동물이란 표현도 없어졌구요",
          "original_form": "애완동물이란 표현도 없어졌구요",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038680.1.32",
