      "utterance": [
        {
          "id": "MDRW2100039077.1.1",
          "form": "유튭 비싸요ㅠ",
          "original_form": "유튭 비싸요ㅠ",
          "speaker_id": "1",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100039077.1.2",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "1",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100039077.1.3",
          "form": "저는 아아 한잔 먹었어요 ㅜㅜ",
          "original_form": "저는 아아 한잔 먹었어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100039077.1.4",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100039077.1.5",
          "form": "저는 토마토 주스",
          "original_form": "저는 토마토 주스",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100039077.1.6",
          "form": "아아 저도 마시고싶어용",
          "original_form": "아아 저도 마시고싶어용",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100039077.1.7",
          "form": "직접 갈으셨어요?",
          "original_form": "직접 갈으셨어요?",
          "speaker_id": "2",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039077.1.8",
          "form": "어머니가 해주셨어용 ㅋㅋ",
          "original_form": "어머니가 해주셨어용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100039077.1.9",
          "form": "오 부러워용 ㅠㅠ",
          "original_form": "오 부러워용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:30"
        },
        {
          "id": "MDRW2100039077.1.10",
          "form": "오늘 아침은 그게 끝이죠? ㅎㅎ",
          "original_form": "오늘 아침은 그게 끝이죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100039077.1.11",
          "form": "네네ㅠㅠ 뭘 조금 더 챙겨먹어야하는데",
          "original_form": "네네ㅠㅠ 뭘 조금 더 챙겨먹어야하는데",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039077.1.12",
          "form": "빈속에 아아 드셨나요?ㅠㅠ",
          "original_form": "빈속에 아아 드셨나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039077.1.13",
          "form": "네 ㅠㅠ 맨날 빈속에 먹게돼요",
          "original_form": "네 ㅠㅠ 맨날 빈속에 먹게돼요",
          "speaker_id": "2",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039077.1.14",
          "form": "일어나기 바뻐서 ㅋㅋ",
          "original_form": "일어나기 바뻐서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039077.1.15",
          "form": "맞아요 저도 그래요ㅠㅠ",
          "original_form": "맞아요 저도 그래요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100039077.1.16",
          "form": "커피 수혈 빡",
          "original_form": "커피 수혈 빡",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100039077.1.17",
          "form": "저는 11시 30분즘 점심 먹을려구요",
          "original_form": "저는 11시 30분즘 점심 먹을려구요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100039077.1.18",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100039077.1.19",
          "form": "저는 아직 커피도 못마셔서 ㅋㅋ",
          "original_form": "저는 아직 커피도 못마셔서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100039077.1.20",
          "form": "커피마시고 점심 12시에 먹으려고용",
          "original_form": "커피마시고 점심 12시에 먹으려고용",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100039077.1.21",
          "form": "오 역시 아침 커피가 짱 ㅎㅎ",
          "original_form": "오 역시 아침 커피가 짱 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100039077.1.22",
          "form": "헉 저 유투브 프리미엄 결제 안하고 광고제거 찾았어요",
          "original_form": "헉 저 유투브 프리미엄 결제 안하고 광고제거 찾았어요",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100039077.1.23",
          "form": "오ㅏ 어케 찾우셨어요",
          "original_form": "오ㅏ 어케 찾우셨어요",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100039077.1.24",
          "form": "구글플레이에",
          "original_form": "구글플레이에",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100039077.1.25",
          "form": "유투브 밴드스?",
          "original_form": "유투브 밴드스?",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100039077.1.26",
          "form": "그어플이래요",
          "original_form": "그어플이래요",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100039077.1.27",
          "form": "밴스드네요 ㅋㅋ",
          "original_form": "밴스드네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100039077.1.28",
          "form": "프리미엄이랑 비슷한 기능하는거같아서 써볼까 하고있어요",
          "original_form": "프리미엄이랑 비슷한 기능하는거같아서 써볼까 하고있어요",
          "speaker_id": "2",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039077.1.29",
          "form": "어어 밴스드 그런게았구나",
          "original_form": "어어 밴스드 그런게았구나",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039077.1.30",
          "form": "걔는 유료가 아니에요?",
          "original_form": "걔는 유료가 아니에요?",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100039077.1.31",
          "form": "그런가봐요 이번 플립3 광고영상에도 깔려있었다는데",
          "original_form": "그런가봐요 이번 플립3 광고영상에도 깔려있었다는데",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100039077.1.32",
          "form": "불법이 아닌가봐요?",
          "original_form": "불법이 아닌가봐요?",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100039077.1.33",
          "form": "아 그래요?? 오 ...",
          "original_form": "아 그래요?? 오 ...",
          "speaker_id": "1",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039077.1.34",
          "form": "커피드셨나용",
          "original_form": "커피드셨나용",
          "speaker_id": "1",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039077.1.35",
          "form": "모카도 한잔 마셨네요",
          "original_form": "모카도 한잔 마셨네요",
          "speaker_id": "2",
          "time": "20210903 15:18"
        }
      ]
