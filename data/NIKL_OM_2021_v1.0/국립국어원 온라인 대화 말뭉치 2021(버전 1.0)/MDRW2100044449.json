        {
          "id": "MDRW2100044449.1.1",
          "form": "그리스식 소스인데",
          "original_form": "그리스식 소스인데",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044449.1.2",
          "form": "차치키",
          "original_form": "차치키",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044449.1.3",
          "form": "차치키!! 첨들어봐요 저는",
          "original_form": "차치키!! 첨들어봐요 저는",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044449.1.4",
          "form": "오이랑 요거트 올리브유 마늘다진거 레몬즙 후추 넣고",
          "original_form": "오이랑 요거트 올리브유 마늘다진거 레몬즙 후추 넣고",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044449.1.5",
          "form": "만들어요",
          "original_form": "만들어요",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044449.1.6",
          "form": "헐 대박 그러면 만들어서 빵이랑 먹나요?!",
          "original_form": "헐 대박 그러면 만들어서 빵이랑 먹나요?!",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044449.1.7",
          "form": "네 빵이랑 먹기도 하고",
          "original_form": "네 빵이랑 먹기도 하고",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044449.1.8",
          "form": "튀김음식과도 잘 어울려요",
          "original_form": "튀김음식과도 잘 어울려요",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044449.1.9",
          "form": "우와 ㅋㅋㅋㅋ저도 요거트 오면 도전해봐야겠어요",
          "original_form": "우와 ㅋㅋㅋㅋ저도 요거트 오면 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044449.1.10",
          "form": "오이를 빼면 어떨까요??",
          "original_form": "오이를 빼면 어떨까요??",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044449.1.11",
          "form": "오이가 메인인데",
          "original_form": "오이가 메인인데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044449.1.12",
          "form": "헐 오이가 메인이었나요ㅠㅠㅠㅠㅠㅠㅠㅠ힝",
          "original_form": "헐 오이가 메인이었나요ㅠㅠㅠㅠㅠㅠㅠㅠ힝",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044449.1.13",
          "form": "(오싫모)",
          "original_form": "(오싫모)",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044449.1.14",
          "form": "저두 오이 잘 안먹었는데",
          "original_form": "저두 오이 잘 안먹었는데",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044449.1.15",
          "form": "차치키 덕에 오이 많이 먹었어요",
          "original_form": "차치키 덕에 오이 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044449.1.16",
          "form": "오 ㅋㅋ그래도 이거랑 먹으면 괜찮나보군요!!",
          "original_form": "오 ㅋㅋ그래도 이거랑 먹으면 괜찮나보군요!!",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044449.1.17",
          "form": "저도그럼 도전...!!!",
          "original_form": "저도그럼 도전...!!!",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044449.1.18",
          "form": "네 일단 소량만 만들어보고 입맛에 맞음 대량생산해보셔요ㅎㅎ",
          "original_form": "네 일단 소량만 만들어보고 입맛에 맞음 대량생산해보셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044449.1.19",
          "form": "ㅋㅋ네 좋습니다 ㅋㅋㅋ",
          "original_form": "ㅋㅋ네 좋습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044449.1.20",
          "form": "저는 그릭요거트 사과랑 먹는거를 가장좋아해요",
          "original_form": "저는 그릭요거트 사과랑 먹는거를 가장좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044449.1.21",
          "form": "오 사과랑 그릭요거트도 좋네요",
          "original_form": "오 사과랑 그릭요거트도 좋네요",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044449.1.22",
          "form": "최근에 복숭아랑 그릭요거트로 만드는 디저트가 sns에서 핫하던데",
