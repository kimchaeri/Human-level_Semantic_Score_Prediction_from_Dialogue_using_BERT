      "utterance": [
        {
          "id": "MDRW2100016149.1.1",
          "form": "서천 가는거 어때요",
          "original_form": "서천 가는거 어때요",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016149.1.2",
          "form": "응 좋지",
          "original_form": "응 좋지",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016149.1.3",
          "form": "멀어서ㅜ걱정인네",
          "original_form": "멀어서ㅜ걱정인네",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016149.1.4",
          "form": "좀 더 커야하려나",
          "original_form": "좀 더 커야하려나",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016149.1.5",
          "form": "토욜 당일치기?",
          "original_form": "토욜 당일치기?",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016149.1.6",
          "form": "응 난 그게 나을 듯",
          "original_form": "응 난 그게 나을 듯",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016149.1.7",
          "form": "10시에 자서 5시면 7시간은 푹 자니까",
          "original_form": "10시에 자서 5시면 7시간은 푹 자니까",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016149.1.8",
          "form": "차에서 좀 못자도 되지 않을까",
          "original_form": "차에서 좀 못자도 되지 않을까",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100016149.1.9",
          "form": "그렇긴해 올라올때 막히려나",
          "original_form": "그렇긴해 올라올때 막히려나",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016149.1.10",
          "form": "진도에서 올라오는 것처럼..",
          "original_form": "진도에서 올라오는 것처럼..",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016149.1.11",
          "form": "여행에 대한 열망이 강해짐!",
          "original_form": "여행에 대한 열망이 강해짐!",
          "speaker_id": "1",
          "time": "20210827 10:15"
        },
        {
          "id": "MDRW2100016149.1.12",
          "form": "오늘도 잘 자자",
          "original_form": "오늘도 잘 자자",
          "speaker_id": "1",
          "time": "20210827 10:16"
        },
        {
          "id": "MDRW2100016149.1.13",
          "form": "응 잘자야 여행갈수있지",
          "original_form": "응 잘자야 여행갈수있지",
          "speaker_id": "2",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100016149.1.14",
          "form": "응 오늘 확실ㅎㅣ달라보여",
          "original_form": "응 오늘 확실ㅎㅣ달라보여",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100016149.1.15",
          "form": "당일치기 여행도 제법 괜찮네",
          "original_form": "당일치기 여행도 제법 괜찮네",
          "speaker_id": "2",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100016149.1.16",
          "form": "응 자주 다니자",
          "original_form": "응 자주 다니자",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100016149.1.17",
          "form": "응 강원도 자주 가볼망한것같아",
          "original_form": "응 강원도 자주 가볼망한것같아",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100016149.1.18",
          "form": "응 좋은 데가 많네",
          "original_form": "응 좋은 데가 많네",
          "speaker_id": "1",
          "time": "20210830 14:01"
        },
        {
          "id": "MDRW2100016149.1.19",
          "form": "평창 도 가고싶고",
          "original_form": "평창 도 가고싶고",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100016149.1.20",
          "form": "목장도 가고싶어",
          "original_form": "목장도 가고싶어",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100016149.1.21",
          "form": "이번주는 어디로 나들이갈까",
          "original_form": "이번주는 어디로 나들이갈까",
