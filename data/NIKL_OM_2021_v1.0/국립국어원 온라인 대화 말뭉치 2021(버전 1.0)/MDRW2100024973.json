      "utterance": [
        {
          "id": "MDRW2100024973.1.1",
          "form": "카페음료 뭐 제일 좋아하세요?",
          "original_form": "카페음료 뭐 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024973.1.2",
          "form": "저는 무조건 아메리카노이긴 한데 ㅋㅋㅋ 단게 땡기면 밀크티 마셔요",
          "original_form": "저는 무조건 아메리카노이긴 한데 ㅋㅋㅋ 단게 땡기면 밀크티 마셔요",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024973.1.3",
          "form": "얼그레이나 아쌈으로 된걸로..",
          "original_form": "얼그레이나 아쌈으로 된걸로..",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024973.1.4",
          "form": "허억 얼그레이도",
          "original_form": "허억 얼그레이도",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024973.1.5",
          "form": "카페인 커피보다 오지잖아요!",
          "original_form": "카페인 커피보다 오지잖아요!",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024973.1.6",
          "form": "근데 카페인보다도",
          "original_form": "근데 카페인보다도",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024973.1.7",
          "form": "홍차는 뭔가 단맛보단 쌉싸르한 맛이 더 강한 것 같아요",
          "original_form": "홍차는 뭔가 단맛보단 쌉싸르한 맛이 더 강한 것 같아요",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024973.1.8",
          "form": "저는 카페인에 내성이 좋은가봐요 ㅋㄱㄱ 먹고도 잘 자서..",
          "original_form": "저는 카페인에 내성이 좋은가봐요 ㅋㄱㄱ 먹고도 잘 자서..",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024973.1.9",
          "form": "네넹 맞아요",
          "original_form": "네넹 맞아요",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024973.1.10",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024973.1.11",
          "form": "그그그",
          "original_form": "그그그",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024973.1.12",
          "form": "되게 유명한",
          "original_form": "되게 유명한",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024973.1.13",
          "form": "홍차.... 뭐였지..",
          "original_form": "홍차.... 뭐였지..",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024973.1.14",
          "form": "냉침해서 먹으면 맛있다던데..",
          "original_form": "냉침해서 먹으면 맛있다던데..",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024973.1.15",
          "form": "으음 브랜드인가요...?",
          "original_form": "으음 브랜드인가요...?",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024973.1.16",
          "form": "네에.. 인터넷에도 파는..",
          "original_form": "네에.. 인터넷에도 파는..",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024973.1.17",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024973.1.18",
          "form": "도밍고요!!",
          "original_form": "도밍고요!!",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024973.1.19",
          "form": "로네펠트...? 트와이닝?",
          "original_form": "로네펠트...? 트와이닝?",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024973.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024973.1.21",
          "form": "그런 고급은 잘 모르겠어요..",
          "original_form": "그런 고급은 잘 모르겠어요..",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024973.1.22",
          "form": "들어는봤는데 그건 아직 안먹어봤어요 ㅋㅋㄱㅋㄱ",
          "original_form": "들어는봤는데 그건 아직 안먹어봤어요 ㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024973.1.23",
          "form": "로네펠트나 트와이닝은 저도 최근에 회사 거래처에서 선물받아서 첨먹어봤어요 ㅋㅋㅋ",
          "original_form": "로네펠트나 트와이닝은 저도 최근에 회사 거래처에서 선물받아서 첨먹어봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024973.1.24",
          "form": "저도원래 그냥 밀크티써있으면 먹는편이라",
          "original_form": "저도원래 그냥 밀크티써있으면 먹는편이라",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024973.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024973.1.26",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024973.1.27",
          "form": "일반홍차만 우려먹는건 잘 안먹어봐서 모르겠더라구요",
          "original_form": "일반홍차만 우려먹는건 잘 안먹어봐서 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024973.1.28",
          "form": "티젠 밀크티 파우더 사서먹고...",
          "original_form": "티젠 밀크티 파우더 사서먹고...",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024973.1.29",
          "form": "아 과일차 좋아하시면 쟈뎅에서 나온 아워티도 맛있어요",
          "original_form": "아 과일차 좋아하시면 쟈뎅에서 나온 아워티도 맛있어요",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024973.1.30",
          "form": "적당히달고 괜찮아요 말린 오렌지도 들어있고",
          "original_form": "적당히달고 괜찮아요 말린 오렌지도 들어있고",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024973.1.31",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024973.1.32",
          "form": "음료 종류 엄청 좋아하시나봐요",
          "original_form": "음료 종류 엄청 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210831 21:41"
        }
      ]
    }
