      "utterance": [
        {
          "id": "MDRW2100010191.1.1",
          "form": "name2님은 종교있으세요?",
          "original_form": "&name2&님은 종교있으세요?",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010191.1.2",
          "form": "천주교 다니긴했는데 이제 거의 무교에요ㅎㅎ",
          "original_form": "천주교 다니긴했는데 이제 거의 무교에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010191.1.3",
          "form": "저는 불교에요 천주교셨군요",
          "original_form": "저는 불교에요 천주교셨군요",
          "speaker_id": "1",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010191.1.4",
          "form": "크리스마스날 성당갔었는데",
          "original_form": "크리스마스날 성당갔었는데",
          "speaker_id": "1",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010191.1.5",
          "form": "영어를 많이 쓰더라구요",
          "original_form": "영어를 많이 쓰더라구요",
          "speaker_id": "1",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010191.1.6",
          "form": "아 그쵸 다 라틴어라ㅎㅎ 저도 절 자주가요",
          "original_form": "아 그쵸 다 라틴어라ㅎㅎ 저도 절 자주가요",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010191.1.7",
          "form": "절에서는 한자 많이쓰는데",
          "original_form": "절에서는 한자 많이쓰는데",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010191.1.8",
          "form": "영어 많이써서 신기했어요",
          "original_form": "영어 많이써서 신기했어요",
          "speaker_id": "1",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010191.1.9",
          "form": "맞아요ㅎㅎ저는 절 분위기 좋아해요",
          "original_form": "맞아요ㅎㅎ저는 절 분위기 좋아해요",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010191.1.10",
          "form": "아 한적하고 좋지요",
          "original_form": "아 한적하고 좋지요",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010191.1.11",
          "form": "성당에는 아름다운가게처럼 자선사업을 많이해서 좋아요",
          "original_form": "성당에는 아름다운가게처럼 자선사업을 많이해서 좋아요",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010191.1.12",
          "form": "복음자리잼도 많이먹어요",
          "original_form": "복음자리잼도 많이먹어요",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010191.1.13",
          "form": "맞아요 수도원애서 햄도 팔구 치즈도 팔구ㅋㅋ많이 그런구하시죠",
          "original_form": "맞아요 수도원애서 햄도 팔구 치즈도 팔구ㅋㅋ많이 그런구하시죠",
          "speaker_id": "2",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010191.1.14",
          "form": "아름다운가게 엄청 싸요 요샌 당근이 대세지만",
          "original_form": "아름다운가게 엄청 싸요 요샌 당근이 대세지만",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010191.1.15",
          "form": "아 맞아요 좋은일이기도 하구ㅎㅎ",
          "original_form": "아 맞아요 좋은일이기도 하구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010191.1.16",
          "form": "맥주도 수도원발명품아닌가요?",
          "original_form": "맥주도 수도원발명품아닌가요?",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010191.1.17",
          "form": "좋은문화에요 ㅋㅋㅋ",
          "original_form": "좋은문화에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010191.1.18",
          "form": "그것까진ㅎㅎ 와인은 그런걸로 아는데ㅋㅋ",
          "original_form": "그것까진ㅎㅎ 와인은 그런걸로 아는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100010191.1.19",
          "form": "아하! 좋은게 다나오네",
          "original_form": "아하! 좋은게 다나오네",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010191.1.20",
          "form": "그러게요ㅎㅎ",
          "original_form": "그러게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:52"
        }
      ]
