          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043465.1.4",
          "form": "제육이용 집에서",
          "original_form": "제육이용 집에서",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043465.1.5",
          "form": "와 맛있겠녜요",
          "original_form": "와 맛있겠녜요",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043465.1.6",
          "form": "넹 name1님은용",
          "original_form": "넹 &name1&님은용",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043465.1.7",
          "form": "전 가지를 베이컨으로 말아봣어여",
          "original_form": "전 가지를 베이컨으로 말아봣어여",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043465.1.8",
          "form": "우와 요리 잘하ㅅㅔ요?",
          "original_form": "우와 요리 잘하ㅅㅔ요?",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043465.1.9",
          "form": "아니요 못해서 간단한걸로요ㅋㅋ",
          "original_form": "아니요 못해서 간단한걸로요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043465.1.10",
          "form": "ㅋㅋㅋ 어려워보여요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 어려워보여요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043465.1.11",
          "form": "그렇게해서 구워요?",
          "original_form": "그렇게해서 구워요?",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043465.1.12",
          "form": "에프?팬 ?",
          "original_form": "에프?팬 ?",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043465.1.13",
          "form": "에어프라이어에 하면대요",
          "original_form": "에어프라이어에 하면대요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043465.1.14",
          "form": "맛있을거같아요",
          "original_form": "맛있을거같아요",
