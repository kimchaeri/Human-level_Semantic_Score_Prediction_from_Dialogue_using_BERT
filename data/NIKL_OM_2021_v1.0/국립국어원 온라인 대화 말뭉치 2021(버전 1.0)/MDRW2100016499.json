      "utterance": [
        {
          "id": "MDRW2100016499.1.1",
          "form": "점심드셧어요?",
          "original_form": "점심드셧어요?",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016499.1.2",
          "form": "아직이요! 님은요?",
          "original_form": "아직이요! 님은요?",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016499.1.3",
          "form": "도시락 먹었어요",
          "original_form": "도시락 먹었어요",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016499.1.4",
          "form": "언능 식사하세요 배고프겟어요",
          "original_form": "언능 식사하세요 배고프겟어요",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016499.1.5",
          "form": "도시락 직접만드셨나요?",
          "original_form": "도시락 직접만드셨나요?",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016499.1.6",
          "form": "직장에서 다같이 시켜먹는곳이 있어요",
          "original_form": "직장에서 다같이 시켜먹는곳이 있어요",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016499.1.7",
          "form": "맛은 쏘쏘",
          "original_form": "맛은 쏘쏘",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016499.1.8",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016499.1.9",
          "form": "출근하자마자 아메리카노 마셔요",
          "original_form": "출근하자마자 아메리카노 마셔요",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016499.1.10",
          "form": "아메리카노vs라떼",
          "original_form": "아메리카노vs라떼",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016499.1.11",
          "form": "닥 아메죠",
          "original_form": "닥 아메죠",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016499.1.12",
          "form": "커피조아하세요?",
          "original_form": "커피조아하세요?",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016499.1.13",
          "form": "못마세요ㅋㅋ",
          "original_form": "못마세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016499.1.14",
          "form": "디카페인만ㅎㅎ",
          "original_form": "디카페인만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016499.1.15",
          "form": "카페가면 주로 뭐드세요?",
          "original_form": "카페가면 주로 뭐드세요?",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016499.1.16",
          "form": "자몽음료요",
          "original_form": "자몽음료요",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016499.1.17",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016499.1.18",
          "form": "저는 무조건 아이스아메요",
          "original_form": "저는 무조건 아이스아메요",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016499.1.19",
          "form": "대신밤에는 잠못자서 걍 음료마셔요",
