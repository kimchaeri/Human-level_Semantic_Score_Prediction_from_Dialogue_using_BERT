      "utterance": [
        {
          "id": "MDRW2100015521.1.1",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015521.1.2",
          "form": "네 물건 구경하는 것도 재밌구요",
          "original_form": "네 물건 구경하는 것도 재밌구요",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015521.1.3",
          "form": "어떤품목이요?",
          "original_form": "어떤품목이요?",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100015521.1.4",
          "form": "보통 옷이나 신발 많이 보는것 같아요",
          "original_form": "보통 옷이나 신발 많이 보는것 같아요",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100015521.1.5",
          "form": "오 이제 가을옷~",
          "original_form": "오 이제 가을옷~",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100015521.1.6",
          "form": "맞아요 할인도 하니까 사고싶어요",
          "original_form": "맞아요 할인도 하니까 사고싶어요",
          "speaker_id": "2",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100015521.1.7",
          "form": "어디서 할인하나요?",
          "original_form": "어디서 할인하나요?",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100015521.1.8",
          "form": "대형쇼핑몰은 계속 하는거 같더라구요",
          "original_form": "대형쇼핑몰은 계속 하는거 같더라구요",
          "speaker_id": "2",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100015521.1.9",
          "form": "최근에 뭐 사셨나요",
          "original_form": "최근에 뭐 사셨나요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100015521.1.10",
          "form": "최근엔 핸드크림 정도 샀네요",
          "original_form": "최근엔 핸드크림 정도 샀네요",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100015521.1.11",
          "form": "금방 써버리더라구요",
          "original_form": "금방 써버리더라구요",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100015521.1.12",
          "form": "오 핸드크림 어디브랜드가좋나요",
          "original_form": "오 핸드크림 어디브랜드가좋나요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100015521.1.13",
          "form": "전 카밀 자주 써요",
          "original_form": "전 카밀 자주 써요",
