      "utterance": [
        {
          "id": "MDRW2100014963.1.1",
          "form": "갑자기 결혼강추분위기?",
          "original_form": "갑자기 결혼강추분위기?",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014963.1.2",
          "form": "오래사귀셨으니깐요ㅎ",
          "original_form": "오래사귀셨으니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014963.1.3",
          "form": "저는 속도위반이라 그런거고ㅎㅎ",
          "original_form": "저는 속도위반이라 그런거고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014963.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014963.1.5",
          "form": "name1님은 오래사귀셔서 누구보다 그분을 잘아실꺼라ㅎㅎ",
          "original_form": "&name1&님은 오래사귀셔서 누구보다 그분을 잘아실꺼라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014963.1.6",
          "form": "그만큼 사랑하셨던거",
          "original_form": "그만큼 사랑하셨던거",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014963.1.7",
          "form": "결혼하고 정착해야죠",
          "original_form": "결혼하고 정착해야죠",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014963.1.8",
          "form": "결혼은 좋아요ㅎ 정말",
          "original_form": "결혼은 좋아요ㅎ 정말",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014963.1.9",
          "form": "아내랑 사이가 안멀어진다면요ㅎ",
          "original_form": "아내랑 사이가 안멀어진다면요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014963.1.10",
          "form": "결혼전에 충분히파악하셨으면 됏죠ㅎㅎ",
          "original_form": "결혼전에 충분히파악하셨으면 됏죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014963.1.11",
          "form": "결혼 결정은 하신거에요?",
          "original_form": "결혼 결정은 하신거에요?",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014963.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014963.1.13",
          "form": "코로나4단계땜시 진행이 더뎌져서 그렇지",
          "original_form": "코로나4단계땜시 진행이 더뎌져서 그렇지",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014963.1.14",
          "form": "오ㅎㅎ 축하드려요ㅎ",
          "original_form": "오ㅎㅎ 축하드려요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.15",
          "form": "지금 뭐 되는게 없어서",
          "original_form": "지금 뭐 되는게 없어서",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.16",
          "form": "에고ㅠㅍ",
          "original_form": "에고ㅠㅍ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.17",
          "form": "힘드시겟어요ㅠ",
          "original_form": "힘드시겟어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.18",
          "form": "저희동생도 코로나때 결혼했는데... ㅠㅠ",
          "original_form": "저희동생도 코로나때 결혼했는데... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.19",
          "form": "저녁밥도 같이 못먹잖아요ㅜㅜ",
          "original_form": "저녁밥도 같이 못먹잖아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.20",
          "form": "그래도 그때는 사람들이 많이오고 밥도 같이먹어서 괜찬ㄹ앜ㅅ는제",
          "original_form": "그래도 그때는 사람들이 많이오고 밥도 같이먹어서 괜찬ㄹ앜ㅅ는제",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.21",
          "form": "이제는 4단계니ㅠ",
          "original_form": "이제는 4단계니ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014963.1.22",
          "form": "지금은 완전 아무것도 안되요ㅜㅜ",
          "original_form": "지금은 완전 아무것도 안되요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014963.1.23",
          "form": "에고ㅠㅠ",
          "original_form": "에고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014963.1.24",
          "form": "결혼도 축복이 있어야하는데ㅍ",
          "original_form": "결혼도 축복이 있어야하는데ㅍ",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014963.1.25",
          "form": "언제나 끝나려나ㅠ",
          "original_form": "언제나 끝나려나ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014963.1.26",
          "form": "그럼,같이 사시는거에요?",
          "original_form": "그럼,같이 사시는거에요?",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014963.1.27",
          "form": "그러니까 말이에요",
          "original_form": "그러니까 말이에요",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014963.1.28",
          "form": "아뇨 같이 살진 않아요",
          "original_form": "아뇨 같이 살진 않아요",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014963.1.29",
          "form": "식 안올리고 그냥 같이 살고싶은데 ㅋㅋㅋ",
          "original_form": "식 안올리고 그냥 같이 살고싶은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014963.1.30",
          "form": "아직은 아니시구나ㅎㅎ",
          "original_form": "아직은 아니시구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014963.1.31",
          "form": "무조건 맞춰주세요ㅎㅎ",
          "original_form": "무조건 맞춰주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014963.1.32",
          "form": "그래야 내 삶이 조금 편안해져요ㅎ 안그럼 계속 싸울껄요?",
          "original_form": "그래야 내 삶이 조금 편안해져요ㅎ 안그럼 계속 싸울껄요?",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014963.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014963.1.34",
          "form": "무섭군여",
          "original_form": "무섭군여",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014963.1.35",
          "form": "결혼이 삶의 일부분이라ㅎㅎ",
          "original_form": "결혼이 삶의 일부분이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014963.1.36",
          "form": "또 궁금한거없으세요? 상담료는 제출로 받을께요ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "또 궁금한거없으세요? 상담료는 제출로 받을께요ㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014963.1.37",
          "form": "제가 제일 무서운건",
          "original_form": "제가 제일 무서운건",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014963.1.38",
          "form": "준비하면서 겁나 싸운다는 얘기 많이 들어서",
          "original_form": "준비하면서 겁나 싸운다는 얘기 많이 들어서",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014963.1.39",
          "form": "제가 다 맞춰주고 싶지만",
          "original_form": "제가 다 맞춰주고 싶지만",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014963.1.40",
          "form": "못맞춰주면...?",
          "original_form": "못맞춰주면...?",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014963.1.41",
          "form": "여자가 다 하려고하는데",
          "original_form": "여자가 다 하려고하는데",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014963.1.42",
          "form": "내가 못해줄때가 있어요",
          "original_form": "내가 못해줄때가 있어요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014963.1.43",
          "form": "그럴때는 차분하게 설명해주셔야해요ㅎ",
          "original_form": "그럴때는 차분하게 설명해주셔야해요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014963.1.44",
          "form": "여자는 일생에 단한번뿐인 결혼을 아름답게 하고싶고 좋은 걸 하고싶은거애요ㅎ",
          "original_form": "여자는 일생에 단한번뿐인 결혼을 아름답게 하고싶고 좋은 걸 하고싶은거애요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014963.1.45",
          "form": "그 기분을 잘마쳐주시면 될듯요ㅎ",
          "original_form": "그 기분을 잘마쳐주시면 될듯요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014963.1.46",
          "form": "준비가 길면 안좋다던데",
          "original_form": "준비가 길면 안좋다던데",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014963.1.47",
          "form": "저희 연애 2년차때 진짜 미친듯이 싸웠거든요 ㅋㅋㅋ",
          "original_form": "저희 연애 2년차때 진짜 미친듯이 싸웠거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014963.1.48",
          "form": "그때 싸울거 다 싸운거 같은데",
          "original_form": "그때 싸울거 다 싸운거 같은데",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.49",
          "form": "또 싸우려나",
          "original_form": "또 싸우려나",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.50",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.51",
          "form": "더싸워요ㅎㅎㅎㅎㅎㅎ",
          "original_form": "더싸워요ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.52",
          "form": "아이때문에 싸우고",
          "original_form": "아이때문에 싸우고",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.53",
          "form": "임.신했을땨",
          "original_form": "임.신했을땨",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.54",
          "form": "많이싸워요...",
          "original_form": "많이싸워요...",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.55",
          "form": "아무래도 호로몬 변화가생겨서그런가봐요",
          "original_form": "아무래도 호로몬 변화가생겨서그런가봐요",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014963.1.56",
          "form": "결혼은 싸움의 연속이군여",
          "original_form": "결혼은 싸움의 연속이군여",
          "speaker_id": "1",
          "time": "20210825 21:39"
        },
        {
