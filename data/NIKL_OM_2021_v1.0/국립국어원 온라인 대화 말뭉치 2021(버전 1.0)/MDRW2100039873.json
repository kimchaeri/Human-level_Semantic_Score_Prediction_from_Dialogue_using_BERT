      "utterance": [
        {
          "id": "MDRW2100039873.1.1",
          "form": "응 진짜요오..",
          "original_form": "응 진짜요오..",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.2",
          "form": "난뭐반포기하긴했는듸ㅣㅣ",
          "original_form": "난뭐반포기하긴했는듸ㅣㅣ",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.3",
          "form": "반포기 ㅋㅋ",
          "original_form": "반포기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.4",
          "form": "진짜 큰맘먹고 헤어질까요",
          "original_form": "진짜 큰맘먹고 헤어질까요",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.5",
          "form": "친구들다보내면심심하긴히ㅡㄹ거같아요",
          "original_form": "친구들다보내면심심하긴히ㅡㄹ거같아요",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.6",
          "form": "쟤랑 헤어지면 뭔가 다른일이 잘될것같아요",
          "original_form": "쟤랑 헤어지면 뭔가 다른일이 잘될것같아요",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.7",
          "form": "후회안하겠어요?",
          "original_form": "후회안하겠어요?",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.8",
          "form": "그냥 내 앞길이 잘될 것만 같은 느낌",
          "original_form": "그냥 내 앞길이 잘될 것만 같은 느낌",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.9",
          "form": "잘생각해봐요",
          "original_form": "잘생각해봐요",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.10",
          "form": "후회 100퍼 하겠지만",
          "original_form": "후회 100퍼 하겠지만",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039873.1.11",
          "form": "아ㅠㅠㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.12",
          "form": "일단 말이나 꺼내볼까 헤어지자고ㅠㅠ",
          "original_form": "일단 말이나 꺼내볼까 헤어지자고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.13",
          "form": "또알수없는행운이찾이ㅡ올지도몰라그건그래",
          "original_form": "또알수없는행운이찾이ㅡ올지도몰라그건그래",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.14",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.15",
          "form": "헤어지자해도귓등으로듣자나",
          "original_form": "헤어지자해도귓등으로듣자나",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.16",
          "form": "어째야해..",
          "original_form": "어째야해..",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.17",
          "form": "알수없는 행운",
          "original_form": "알수없는 행운",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.18",
          "form": "그러게 ...",
          "original_form": "그러게 ...",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.19",
          "form": "헤어질까?ㅠㅠ",
          "original_form": "헤어질까?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039873.1.20",
          "form": "어렵다ㅠㅠ힝",
          "original_form": "어렵다ㅠㅠ힝",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039873.1.21",
          "form": "근데그건있어",
          "original_form": "근데그건있어",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039873.1.22",
          "form": "어려워",
          "original_form": "어려워",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039873.1.23",
          "form": "헤어져도name1이는좋은사람만날거라는그런확신",
          "original_form": "헤어져도&name1&이는좋은사람만날거라는그런확신",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039873.1.24",
          "form": "근데name1이맘이중요하지",
          "original_form": "근데&name1&이맘이중요하지",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039873.1.25",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039873.1.26",
          "form": "헐 헤어져도 난 좋은 사람 만날까?",
          "original_form": "헐 헤어져도 난 좋은 사람 만날까?",
