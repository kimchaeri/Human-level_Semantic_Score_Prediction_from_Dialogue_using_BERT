      "utterance": [
        {
          "id": "MDRW2100015263.1.1",
          "form": "노래는 어떤 장르 좋아해요?",
          "original_form": "노래는 어떤 장르 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015263.1.2",
          "form": "저는 팝송이랑 케이팝 좋아해요!!",
          "original_form": "저는 팝송이랑 케이팝 좋아해요!!",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015263.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015263.1.4",
          "form": "저도요! 저는 비티에스 팬분들껜 죄송하지만.. 그런 장르보다는 아이유나 버스커버스커 같은 게 더 취향이에요",
          "original_form": "저도요! 저는 비티에스 팬분들껜 죄송하지만.. 그런 장르보다는 아이유나 버스커버스커 같은 게 더 취향이에요",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.5",
          "form": "악동뮤지션도 좋더라구요!",
          "original_form": "악동뮤지션도 좋더라구요!",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.6",
          "form": "아 딱 취향이 보이네요",
          "original_form": "아 딱 취향이 보이네요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.7",
          "form": "근데 제 노래는 옛날에 갇혀있어요..",
          "original_form": "근데 제 노래는 옛날에 갇혀있어요..",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.8",
          "form": "저도 좋은거같애요 악뮤 짱 노래잘해요",
          "original_form": "저도 좋은거같애요 악뮤 짱 노래잘해요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.9",
          "form": "소녀시대.. 원더걸스.. 버즈.. 돌아와..",
          "original_form": "소녀시대.. 원더걸스.. 버즈.. 돌아와..",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.10",
          "form": "저도 듣던거만 들어요 거의 ㅋㅋㅋ",
          "original_form": "저도 듣던거만 들어요 거의 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015263.1.11",
          "form": "앜ㅋㅋㅋ완전 추억ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ완전 추억ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.12",
          "form": "원걸 좋아했죠",
          "original_form": "원걸 좋아했죠",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.13",
          "form": "ㅋㅋㅋ 저도 그래서 월 요금 안내고 엠피쓰리파일로 들어요",
          "original_form": "ㅋㅋㅋ 저도 그래서 월 요금 안내고 엠피쓰리파일로 들어요",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.14",
          "form": "아 진짜요?ㅋㅋㅋㅋㄲㅋㅋ대박",
          "original_form": "아 진짜요?ㅋㅋㅋㅋㄲㅋㅋ대박",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.15",
          "form": "대박 아날로그틱하시다",
          "original_form": "대박 아날로그틱하시다",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.16",
          "form": "네 ㅜㅜ 어차피 신곡을 잘 듣지 않으니..",
          "original_form": "네 ㅜㅜ 어차피 신곡을 잘 듣지 않으니..",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.17",
          "form": "듣던것만 들어서 ㅜㅜ",
          "original_form": "듣던것만 들어서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.18",
          "form": "그러면 그래도 돼죠 ㅋㅋㅋㅋ",
          "original_form": "그러면 그래도 돼죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015263.1.19",
          "form": "근데..",
          "original_form": "근데..",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015263.1.20",
          "form": "스타트업 오스티 곡은 .. 갖고싶어요.",
          "original_form": "스타트업 오스티 곡은 .. 갖고싶어요.",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015263.1.21",
          "form": "아 ㅋㅋㅋ꼭 들어야겠다 ㅋㅋ그것만 유투브로 들어요",
          "original_form": "아 ㅋㅋㅋ꼭 들어야겠다 ㅋㅋ그것만 유투브로 들어요",
