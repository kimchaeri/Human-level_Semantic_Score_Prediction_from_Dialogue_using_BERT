      "utterance": [
        {
          "id": "MDRW2100037475.1.1",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "1",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037475.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100037475.1.3",
          "form": "요리 즐기시나요?",
          "original_form": "요리 즐기시나요?",
          "speaker_id": "1",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100037475.1.4",
          "form": "네 좋아하는편이에요",
          "original_form": "네 좋아하는편이에요",
          "speaker_id": "2",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100037475.1.5",
          "form": "잘한다고 생각하는 요리 있나요?",
          "original_form": "잘한다고 생각하는 요리 있나요?",
          "speaker_id": "1",
          "time": "20210901 00:44"
        },
        {
          "id": "MDRW2100037475.1.6",
          "form": "아니요 특별히 잘한다 생각은 안 해요\r\n그냥 보통 정도라 생각하죠",
          "original_form": "아니요 특별히 잘한다 생각은 안 해요\r\n그냥 보통 정도라 생각하죠",
          "speaker_id": "2",
          "time": "20210901 00:45"
        },
        {
          "id": "MDRW2100037475.1.7",
          "form": "그렇군요ㅎ 한식요리 주로하시나요?",
          "original_form": "그렇군요ㅎ 한식요리 주로하시나요?",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100037475.1.8",
          "form": "네 한식요리도 하고 아니면 간단한 간식이라던가 한끼메뉴들도 종종해요",
          "original_form": "네 한식요리도 하고 아니면 간단한 간식이라던가 한끼메뉴들도 종종해요",
          "speaker_id": "2",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100037475.1.9",
          "form": "오 요리 정말 즐기시나보네요 전 요리는 양식쪽 종종하는 편이에요",
          "original_form": "오 요리 정말 즐기시나보네요 전 요리는 양식쪽 종종하는 편이에요",
          "speaker_id": "1",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100037475.1.10",
          "form": "오 양식요리들 맛있죠~",
          "original_form": "오 양식요리들 맛있죠~",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100037475.1.11",
          "form": "어떤 음식들 하세요?",
          "original_form": "어떤 음식들 하세요?",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100037475.1.12",
          "form": "전 스파게티 많이 해요 스파게티가 은근 쉽더라고요",
          "original_form": "전 스파게티 많이 해요 스파게티가 은근 쉽더라고요",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100037475.1.13",
          "form": "그쵸 스파게티 간단하게 할 수 있어서 자주 해먹는분들 많은거 같아요",
          "original_form": "그쵸 스파게티 간단하게 할 수 있어서 자주 해먹는분들 많은거 같아요",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037475.1.14",
          "form": "네 소스를 따로 구입할수도 있지만 알리오올리오 이런거도 정말 간편해서 좋아요",
          "original_form": "네 소스를 따로 구입할수도 있지만 알리오올리오 이런거도 정말 간편해서 좋아요",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100037475.1.15",
          "form": "그쵸 소스 구매없이도 가능하니까요",
          "original_form": "그쵸 소스 구매없이도 가능하니까요",
          "speaker_id": "2",
          "time": "20210901 01:03"
        }
      ]
