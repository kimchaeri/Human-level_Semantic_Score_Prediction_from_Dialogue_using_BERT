      "utterance": [
        {
          "id": "MDRW2100008894.1.1",
          "form": "저의 신체만큼 정신건강도 중요하니까요.",
          "original_form": "저의 신체만큼 정신건강도 중요하니까요.",
          "speaker_id": "2",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008894.1.2",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008894.1.3",
          "form": "사실 지난달 카드값보고 반성하긴 했어요",
          "original_form": "사실 지난달 카드값보고 반성하긴 했어요",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008894.1.4",
          "form": "외식비가 하늘높은줄 모르고 치솟아서..",
          "original_form": "외식비가 하늘높은줄 모르고 치솟아서..",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008894.1.5",
          "form": "(외식비=배달비)",
          "original_form": "(외식비=배달비)",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008894.1.6",
          "form": "헷 엥겔지수 최고치에요",
          "original_form": "헷 엥겔지수 최고치에요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008894.1.7",
          "form": "배달치킨vs배달피자",
          "original_form": "배달치킨vs배달피자",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008894.1.8",
          "form": "번외 떡볶이",
          "original_form": "번외 떡볶이",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008894.1.9",
          "form": "고민끝에 치킨 손을 들어봅니다",
          "original_form": "고민끝에 치킨 손을 들어봅니다",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008894.1.10",
          "form": "그리고 조용히 족발 패를 들어보는",
          "original_form": "그리고 조용히 족발 패를 들어보는",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008894.1.11",
          "form": "앗..와 배운분",
          "original_form": "앗..와 배운분",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008894.1.12",
          "form": "족발한테 미안하네요",
          "original_form": "족발한테 미안하네요",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008894.1.13",
          "form": "떡볶이는 맛있는데 식었다 다시먹는게 곤혹스러워요",
          "original_form": "떡볶이는 맛있는데 식었다 다시먹는게 곤혹스러워요",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008894.1.14",
          "form": "배달당시 상태로 살려내질 못하겠더라고요",
          "original_form": "배달당시 상태로 살려내질 못하겠더라고요",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008894.1.15",
          "form": "치킨 피자는 에프에 돌리고 족발은 뭐 차가워도 후후 맛있다인데",
          "original_form": "치킨 피자는 에프에 돌리고 족발은 뭐 차가워도 후후 맛있다인데",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008894.1.16",
          "form": "맞아여 떡ㅇㅣ 퍼지니까요",
          "original_form": "맞아여 떡ㅇㅣ 퍼지니까요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008894.1.17",
          "form": "밀떡파시군요",
          "original_form": "밀떡파시군요",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008894.1.18",
          "form": "저는 쌀떡파라 딱딱해져요 ㅋㅋ",
          "original_form": "저는 쌀떡파라 딱딱해져요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008894.1.19",
          "form": "ㅋㅋ아 그럼 힘들죠",
          "original_form": "ㅋㅋ아 그럼 힘들죠",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008894.1.20",
          "form": "야식은 즐기지 않으시나요?",
          "original_form": "야식은 즐기지 않으시나요?",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008894.1.21",
          "form": "저는 한동안 닭발-돼지껍데기-막창-닭껍질튀김의 굴레를 뱅글뱅글 돌았어요",
          "original_form": "저는 한동안 닭발-돼지껍데기-막창-닭껍질튀김의 굴레를 뱅글뱅글 돌았어요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008894.1.22",
          "form": "오 맛있겠어요",
          "original_form": "오 맛있겠어요",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008894.1.23",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008894.1.24",
          "form": "자취할때는 좋아했어요",
          "original_form": "자취할때는 좋아했어요",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008894.1.25",
          "form": "지갑엔 미안하지만..",
          "original_form": "지갑엔 미안하지만..",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008894.1.26",
          "form": "지금은 가족과 살아서ㅋㅋ",
          "original_form": "지금은 가족과 살아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008894.1.27",
          "form": "아하 ㅋㅋ 확실히 혼자 있으면 배달시키는 데 고삐가 풀리죠",
          "original_form": "아하 ㅋㅋ 확실히 혼자 있으면 배달시키는 데 고삐가 풀리죠",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008894.1.28",
          "form": "돼지껍데기 닭껍질 다 좋아요ㅋㅋ",
          "original_form": "돼지껍데기 닭껍질 다 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:35"
        }
      ]
    }
