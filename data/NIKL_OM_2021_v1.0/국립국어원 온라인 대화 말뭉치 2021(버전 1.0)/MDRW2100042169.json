      "utterance": [
        {
          "id": "MDRW2100042169.1.1",
          "form": "컴퓨터가 있는데도 시세를 찾아보는 거야?",
          "original_form": "컴퓨터가 있는데도 시세를 찾아보는 거야?",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042169.1.2",
          "form": "멋지다...",
          "original_form": "멋지다...",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042169.1.3",
          "form": "아니 걍... 할 것도 없고",
          "original_form": "아니 걍... 할 것도 없고",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042169.1.4",
          "form": "궁금하니까...",
          "original_form": "궁금하니까...",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042169.1.5",
          "form": "글쿠만...",
          "original_form": "글쿠만...",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042169.1.6",
          "form": "다른 사람들이 써둔 거 보는 거임",
          "original_form": "다른 사람들이 써둔 거 보는 거임",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042169.1.7",
          "form": "견적 못냄",
          "original_form": "견적 못냄",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042169.1.8",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042169.1.9",
          "form": "견적... 벌써 걱정",
          "original_form": "견적... 벌써 걱정",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042169.1.10",
          "form": "어따물어보지",
          "original_form": "어따물어보지",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042169.1.11",
          "form": "화이팅",
          "original_form": "화이팅",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042169.1.12",
          "form": "난 걍 맞추는 데서 검사 함만 돌리고 주문했는데",
          "original_form": "난 걍 맞추는 데서 검사 함만 돌리고 주문했는데",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042169.1.13",
          "form": "이젠 어디서 시켰는지도 기억 안남",
          "original_form": "이젠 어디서 시켰는지도 기억 안남",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042169.1.14",
          "form": "당연함 몇년전임",
          "original_form": "당연함 몇년전임",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042169.1.15",
          "form": "아무래도 그렇지",
          "original_form": "아무래도 그렇지",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042169.1.16",
          "form": "하... 요즘 주사위 사고 싶은데",
          "original_form": "하... 요즘 주사위 사고 싶은데",
          "speaker_id": "2",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100042169.1.17",
          "form": "쓸 일이 없음",
          "original_form": "쓸 일이 없음",
          "speaker_id": "2",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100042169.1.18",
          "form": "주사위... 집에 있는 것도 먼지 쌓였을듯",
          "original_form": "주사위... 집에 있는 것도 먼지 쌓였을듯",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042169.1.19",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042169.1.20",
          "form": "ㅠㅠ오프탁하고싶어",
          "original_form": "ㅠㅠ오프탁하고싶어",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042169.1.21",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042169.1.22",
          "form": "룰북도 새로운 거 많이 나오던데",
          "original_form": "룰북도 새로운 거 많이 나오던데",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042169.1.23",
          "form": "걍 외면하는 중",
          "original_form": "걍 외면하는 중",
