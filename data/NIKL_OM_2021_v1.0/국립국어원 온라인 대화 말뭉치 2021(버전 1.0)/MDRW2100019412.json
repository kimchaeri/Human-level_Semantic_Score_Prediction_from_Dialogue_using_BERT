      "utterance": [
        {
          "id": "MDRW2100019412.1.1",
          "form": "가족 관계는 어떻게 되세요?",
          "original_form": "가족 관계는 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019412.1.2",
          "form": "남동생하나 있어요",
          "original_form": "남동생하나 있어요",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019412.1.3",
          "form": "1남1녀군요",
          "original_form": "1남1녀군요",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019412.1.4",
          "form": "네 가족관계가 어떻게 되세요?",
          "original_form": "네 가족관계가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019412.1.5",
          "form": "저도 남동생 하나요ㅋㅋ",
          "original_form": "저도 남동생 하나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019412.1.6",
          "form": "지금은 결혼해서 독립했습니다",
          "original_form": "지금은 결혼해서 독립했습니다",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019412.1.7",
          "form": "결혼 하셨구나 늦게나마 축하드려요(?)",
          "original_form": "결혼 하셨구나 늦게나마 축하드려요(?)",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019412.1.8",
          "form": "제일 싫어하는 집안일이 뭐세요?",
          "original_form": "제일 싫어하는 집안일이 뭐세요?",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019412.1.9",
          "form": "음쓰 버리기요ㅋㅋ",
          "original_form": "음쓰 버리기요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019412.1.10",
          "form": "앜ㅋㅋㅋㅋㅋ음쓰 정리도 같이하시나요",
          "original_form": "앜ㅋㅋㅋㅋㅋ음쓰 정리도 같이하시나요",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019412.1.11",
          "form": "맞벌이니까 같이 해야....",
          "original_form": "맞벌이니까 같이 해야....",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019412.1.12",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019412.1.13",
          "form": "앗 음쓰버리실때 음쓰정리도 같이하시냔 뜻이였어요ㅋㅋㅋㅋ",
          "original_form": "앗 음쓰버리실때 음쓰정리도 같이하시냔 뜻이였어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019412.1.14",
          "form": "저는 음쓰 모으는게 너무 싫더라구요",
          "original_form": "저는 음쓰 모으는게 너무 싫더라구요",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019412.1.15",
          "form": "뒷정리도 싫구요ㅠ",
          "original_form": "뒷정리도 싫구요ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019412.1.16",
          "form": "아 ㅋㅋㅋ 넵",
          "original_form": "아 ㅋㅋㅋ 넵",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019412.1.17",
          "form": "냉장고도 탈탈 털죠",
          "original_form": "냉장고도 탈탈 털죠",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019412.1.18",
          "form": "해도해도 끝이 안나는게 집안일의 단점인거 같아요",
          "original_form": "해도해도 끝이 안나는게 집안일의 단점인거 같아요",
          "speaker_id": "1",
          "time": "20210827 23:36"
        }
      ]
