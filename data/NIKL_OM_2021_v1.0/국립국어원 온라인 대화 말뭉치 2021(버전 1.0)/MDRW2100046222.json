      "utterance": [
        {
          "id": "MDRW2100046222.1.1",
          "form": "배달 자주 안시키세여?",
          "original_form": "배달 자주 안시키세여?",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046222.1.2",
          "form": "넹ㅋㅋㅋㅋㅋㅋ 자주는 안시키는거 같아요",
          "original_form": "넹ㅋㅋㅋㅋㅋㅋ 자주는 안시키는거 같아요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046222.1.3",
          "form": "요리 자주하세요?",
          "original_form": "요리 자주하세요?",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046222.1.4",
          "form": "배달비 붙고 하는게 매번 시켜먹긴 부담돼서요 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "배달비 붙고 하는게 매번 시켜먹긴 부담돼서요 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046222.1.5",
          "form": "한 번에 많이 해놓고",
          "original_form": "한 번에 많이 해놓고",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046222.1.6",
          "form": "먹는거같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹는거같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046222.1.7",
          "form": "오.... 저희는 냉장고가",
          "original_form": "오.... 저희는 냉장고가",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046222.1.8",
          "form": "진짜 작고 성능이 구려서",
          "original_form": "진짜 작고 성능이 구려서",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046222.1.9",
          "form": "얼거나 상해요ㅜ",
          "original_form": "얼거나 상해요ㅜ",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046222.1.10",
          "form": "뭘 둘수가없어",
          "original_form": "뭘 둘수가없어",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046222.1.11",
          "form": "앗 ㅠㅠㅠㅠ",
          "original_form": "앗 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046222.1.12",
          "form": "그러면 힘들죠 ㅠㅠㅠㅠ...",
          "original_form": "그러면 힘들죠 ㅠㅠㅠㅠ...",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046222.1.13",
          "form": "냉장고는 진짜 다다익선인거 같아요...",
          "original_form": "냉장고는 진짜 다다익선인거 같아요...",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046222.1.14",
          "form": "저는 제가 배달어플 등급",
          "original_form": "저는 제가 배달어플 등급",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046222.1.15",
          "form": "최고 찍을줄 몰랐어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "최고 찍을줄 몰랐어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046222.1.16",
          "form": "하루에 두번시킬때도 많으니까",
          "original_form": "하루에 두번시킬때도 많으니까",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046222.1.17",
          "form": "와 ㅋㅋㅋㅋㅋ vip시구나 ㅠㅠㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋ vip시구나 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046222.1.18",
          "form": "근데 배달이 확실히 편하고 맛있긴해요 ㅠㅠ",
          "original_form": "근데 배달이 확실히 편하고 맛있긴해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046222.1.19",
          "form": "저희 건물에 배달오는거",
          "original_form": "저희 건물에 배달오는거",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046222.1.20",
          "form": "진짜 거의다 저희집이에여ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 거의다 저희집이에여ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046222.1.21",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046222.1.22",
          "form": "주로 어떤거 시켜드세요??",
          "original_form": "주로 어떤거 시켜드세요??",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046222.1.23",
          "form": "한식이 제일 많아요",
          "original_form": "한식이 제일 많아요",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046222.1.24",
          "form": "치킨도 많았는데 요즘은 질리더라고요ㅋㅋㅋㅋ",
          "original_form": "치킨도 많았는데 요즘은 질리더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046222.1.25",
          "form": "밥이 최고",
          "original_form": "밥이 최고",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046222.1.26",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046222.1.27",
          "form": "맞아요 ㅠㅠㅋㅋㅋㅋㅋ 밥이 최고죠...",
          "original_form": "맞아요 ㅠㅠㅋㅋㅋㅋㅋ 밥이 최고죠...",
          "speaker_id": "2",
