        {
          "id": "MDRW2100045440.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅂ무섭",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅂ무섭",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045440.1.2",
          "form": "가방에 무게도 있나 봐요",
          "original_form": "가방에 무게도 있나 봐요",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045440.1.3",
          "form": "무겁게 들면 느려져요",
          "original_form": "무겁게 들면 느려져요",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045440.1.4",
          "form": "차 운전도 되나요",
          "original_form": "차 운전도 되나요",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045440.1.5",
          "form": "그것도 열쇠가 필요해요",
          "original_form": "그것도 열쇠가 필요해요",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045440.1.6",
          "form": "본격적인 게임 이네요",
          "original_form": "본격적인 게임 이네요",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045440.1.7",
          "form": "기름도 필요하 에요",
          "original_form": "기름도 필요하 에요",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045440.1.8",
          "form": "주차장도 있다니",
          "original_form": "주차장도 있다니",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045440.1.9",
          "form": "주유소 말하시는 거죠",
          "original_form": "주유소 말하시는 거죠",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045440.1.10",
          "form": "아 맞아요 ㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045440.1.11",
          "form": "무기가 중요한 듯",
          "original_form": "무기가 중요한 듯",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045440.1.12",
          "form": "며칠 생존하실까요",
          "original_form": "며칠 생존하실까요",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045440.1.13",
          "form": "최대일수 궁금하네요",
          "original_form": "최대일수 궁금하네요",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045440.1.14",
