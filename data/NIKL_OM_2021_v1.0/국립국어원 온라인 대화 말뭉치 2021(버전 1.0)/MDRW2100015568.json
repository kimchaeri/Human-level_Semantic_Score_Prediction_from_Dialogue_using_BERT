      "utterance": [
        {
          "id": "MDRW2100015568.1.1",
          "form": "name2님은 여행도 좋아하시나요!!!!",
          "original_form": "&name2&님은 여행도 좋아하시나요!!!!",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015568.1.2",
          "form": "네에 해외는 많이 못가봤구",
          "original_form": "네에 해외는 많이 못가봤구",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015568.1.3",
          "form": "국내여행 조아해요!",
          "original_form": "국내여행 조아해요!",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015568.1.4",
          "form": "어디가 제일 좋으셨어요!?!?!",
          "original_form": "어디가 제일 좋으셨어요!?!?!",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015568.1.5",
          "form": "저는 제주도도 좋았구",
          "original_form": "저는 제주도도 좋았구",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015568.1.6",
          "form": "부산은 다시 가서 제대로 즐기고 싶어요ㅠㅠ",
          "original_form": "부산은 다시 가서 제대로 즐기고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015568.1.7",
          "form": "헐 부산!! 저도 좋아해요!! 특히 밤바다",
          "original_form": "헐 부산!! 저도 좋아해요!! 특히 밤바다",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015568.1.8",
          "form": "괜히 센치해요 ㅎ.ㅎ",
          "original_form": "괜히 센치해요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015568.1.9",
          "form": "맞아요ㅠㅠ저 바다도 정말 좋아요",
          "original_form": "맞아요ㅠㅠ저 바다도 정말 좋아요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015568.1.10",
          "form": "막상 바다 근처 살면 감흥없어하던데.. 전 바다근처에 안 살아봐서 로망있어요ㅋㅋㅋ",
          "original_form": "막상 바다 근처 살면 감흥없어하던데.. 전 바다근처에 안 살아봐서 로망있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015568.1.11",
          "form": "맞아요! 저도 완전 땅땅땅 사이에 살아서 물은 거의 못 보거든요",
          "original_form": "맞아요! 저도 완전 땅땅땅 사이에 살아서 물은 거의 못 보거든요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015568.1.12",
          "form": "그래서 물만 보면 강아지마냥 뛰어들어버려요 ㅎ.ㅎ",
          "original_form": "그래서 물만 보면 강아지마냥 뛰어들어버려요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015568.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 근데 물 너무 조아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 근데 물 너무 조아요",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015568.1.14",
          "form": "이번 여름엔 제대로 여행 못가서 넘 아쉬워요ㅠㅠ",
          "original_form": "이번 여름엔 제대로 여행 못가서 넘 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015568.1.15",
          "form": "그러니까요 저 바다말고 오션월드 같은 곳 가는 것도",
          "original_form": "그러니까요 저 바다말고 오션월드 같은 곳 가는 것도",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015568.1.16",
          "form": "엄청 좋아하거든요 ㅠ.ㅠ",
          "original_form": "엄청 좋아하거든요 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015568.1.17",
          "form": "다 마스크 쓰고는 못 가는 곳들..",
          "original_form": "다 마스크 쓰고는 못 가는 곳들..",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015568.1.18",
          "form": "어 맞아요ㅠㅠ",
          "original_form": "어 맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015568.1.19",
          "form": "안간지 진짜 오래됐네요",
          "original_form": "안간지 진짜 오래됐네요",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015568.1.20",
          "form": "저 놀이공원도 가고싶어요ㅠㅠ",
          "original_form": "저 놀이공원도 가고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015568.1.21",
          "form": "헐 저도요!! 저 롯데월드 완전 좋아래요",
          "original_form": "헐 저도요!! 저 롯데월드 완전 좋아래요",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015568.1.22",
          "form": "name2님 기구 잘 타세요~?",
          "original_form": "&name2&님 기구 잘 타세요~?",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015568.1.23",
          "form": "네 저 잘타요!",
          "original_form": "네 저 잘타요!",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015568.1.24",
          "form": "근데 자이로드롭은 한번도 안타뵜어욬ㅋㅋㅋㅋㅋ이상하게",
          "original_form": "근데 자이로드롭은 한번도 안타뵜어욬ㅋㅋㅋㅋㅋ이상하게",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015568.1.25",
          "form": "헉 전 자이로드롭 완전 좋아해여 ㅎ.ㅎ",
          "original_form": "헉 전 자이로드롭 완전 좋아해여 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015568.1.26",
          "form": "맨날 가면 적어도 10번 이상은 타요",
          "original_form": "맨날 가면 적어도 10번 이상은 타요",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015568.1.27",
          "form": "헐 제 친구들은 다 못타서 ㅠ ㅠ 저 혼자 맨날 자이로드롭 타고 오거덩요",
          "original_form": "헐 제 친구들은 다 못타서 ㅠ ㅠ 저 혼자 맨날 자이로드롭 타고 오거덩요",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015568.1.28",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015568.1.29",
          "form": "저 그래서 경주도 가보고 싶았어요",
          "original_form": "저 그래서 경주도 가보고 싶았어요",
