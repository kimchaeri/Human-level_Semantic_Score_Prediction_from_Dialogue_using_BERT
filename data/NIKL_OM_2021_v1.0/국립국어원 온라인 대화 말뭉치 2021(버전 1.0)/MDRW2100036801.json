        },
        {
          "id": "MDRW2100036801.1.2",
          "form": "물에 담군 양파랑",
          "original_form": "물에 담군 양파랑",
          "speaker_id": "1",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.3",
          "form": "양배추",
          "original_form": "양배추",
          "speaker_id": "1",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.4",
          "form": "지금 요리중이신가요?!",
          "original_form": "지금 요리중이신가요?!",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.5",
          "form": "일단 이정도만 넣어도",
          "original_form": "일단 이정도만 넣어도",
          "speaker_id": "1",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.6",
          "form": "그럴듯하죠",
          "original_form": "그럴듯하죠",
          "speaker_id": "1",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.7",
          "form": "아 그쵸 그쵸",
          "original_form": "아 그쵸 그쵸",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.8",
          "form": "앗 아니요!",
          "original_form": "앗 아니요!",
          "speaker_id": "1",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.9",
          "form": "앗ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036801.1.10",
          "form": "사실 주변에 소스 안쓴다하는 분 있는데",
          "original_form": "사실 주변에 소스 안쓴다하는 분 있는데",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036801.1.11",
          "form": "우와....고수이신가봐요",
          "original_form": "우와....고수이신가봐요",
          "speaker_id": "1",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036801.1.12",
          "form": "이런말하긴 그렇지만 그 좋은걸 왜 안쓰시지?! 싶어요",
          "original_form": "이런말하긴 그렇지만 그 좋은걸 왜 안쓰시지?! 싶어요",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036801.1.13",
          "form": "나가서 식사하시믄 다 그 소스인데말이죠",
          "original_form": "나가서 식사하시믄 다 그 소스인데말이죠",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036801.1.14",
          "form": "저는 msg 도 그래요...",
          "original_form": "저는 msg 도 그래요...",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.15",
          "form": "왜 안쓰시지...?",
          "original_form": "왜 안쓰시지...?",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.16",
          "form": "저두..",
          "original_form": "저두..",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.17",
          "form": "다시다 얼마나...",
          "original_form": "다시다 얼마나...",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.18",
          "form": "맛이 몇 배 이상 좋아지잖아요?",
          "original_form": "맛이 몇 배 이상 좋아지잖아요?",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.19",
          "form": "깊은맛이나는데...",
          "original_form": "깊은맛이나는데...",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.21",
          "form": "몸에 안좋은거 그거 옛말이자나여",
          "original_form": "몸에 안좋은거 그거 옛말이자나여",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.22",
          "form": "아.. 그니까여",
          "original_form": "아.. 그니까여",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036801.1.23",
          "form": "msg를 비싼돈받는 식당에서 쓰는건 좀그렇지만",
          "original_form": "msg를 비싼돈받는 식당에서 쓰는건 좀그렇지만",
          "speaker_id": "1",
          "time": "20210831 12:31"
        }
