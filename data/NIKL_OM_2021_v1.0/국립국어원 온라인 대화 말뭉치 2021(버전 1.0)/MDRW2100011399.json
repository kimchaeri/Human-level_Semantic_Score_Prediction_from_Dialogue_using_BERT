      "utterance": [
        {
          "id": "MDRW2100011399.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 12:03"
        },
        {
          "id": "MDRW2100011399.1.2",
          "form": "예 안녕하세요",
          "original_form": "예 안녕하세요",
          "speaker_id": "2",
          "time": "20210824 12:03"
        },
        {
          "id": "MDRW2100011399.1.3",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011399.1.4",
          "form": "아주 하지는 못하는데 좋아하는 편이에요",
          "original_form": "아주 하지는 못하는데 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011399.1.5",
          "form": "자주하지는 못하는데 좋아하는 편이에요",
          "original_form": "자주하지는 못하는데 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011399.1.6",
          "form": "게임 즐겨 하시나요",
          "original_form": "게임 즐겨 하시나요",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011399.1.7",
          "form": "요즘엔 잘안해요",
          "original_form": "요즘엔 잘안해요",
          "speaker_id": "1",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100011399.1.8",
          "form": "어떤게임 좋아하세요?",
          "original_form": "어떤게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100011399.1.9",
          "form": "크레이지 아케이드 해봤어요",
          "original_form": "크레이지 아케이드 해봤어요",
          "speaker_id": "2",
          "time": "20210825 03:12"
        },
        {
          "id": "MDRW2100011399.1.10",
          "form": "옥돔님은요?",
          "original_form": "옥돔님은요?",
          "speaker_id": "2",
          "time": "20210825 03:12"
        },
        {
          "id": "MDRW2100011399.1.11",
          "form": "저도 크아요",
          "original_form": "저도 크아요",
          "speaker_id": "1",
          "time": "20210825 03:12"
        },
        {
          "id": "MDRW2100011399.1.12",
          "form": "크아 너무 재밌져",
          "original_form": "크아 너무 재밌져",
          "speaker_id": "2",
          "time": "20210825 03:13"
        },
        {
          "id": "MDRW2100011399.1.13",
          "form": "초딩때 캐시 쓴 기억이있네요ㅋㅋ",
          "original_form": "초딩때 캐시 쓴 기억이있네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:13"
        },
        {
          "id": "MDRW2100011399.1.14",
          "form": "어떤 맵을 가장 좋아했나요",
          "original_form": "어떤 맵을 가장 좋아했나요",
          "speaker_id": "2",
          "time": "20210825 03:13"
        },
        {
          "id": "MDRW2100011399.1.15",
          "form": "빌리지요",
          "original_form": "빌리지요",
          "speaker_id": "1",
          "time": "20210825 03:13"
        },
        {
          "id": "MDRW2100011399.1.16",
          "form": "럭마아세요?",
          "original_form": "럭마아세요?",
          "speaker_id": "1",
          "time": "20210825 03:14"
        },
        {
          "id": "MDRW2100011399.1.17",
          "form": "빌리지 너무 좋죠 약간 아기자기하고 귀엽고",
          "original_form": "빌리지 너무 좋죠 약간 아기자기하고 귀엽고",
          "speaker_id": "2",
          "time": "20210825 03:14"
        },
        {
          "id": "MDRW2100011399.1.18",
          "form": "럭셔리마리드요?",
          "original_form": "럭셔리마리드요?",
          "speaker_id": "2",
          "time": "20210825 03:14"
        },
        {
          "id": "MDRW2100011399.1.19",
          "form": "당연히알죠",
          "original_form": "당연히알죠",
          "speaker_id": "2",
          "time": "20210825 03:14"
        },
        {
          "id": "MDRW2100011399.1.20",
          "form": "럭셔리 마리드에 샤샤샥 효과 넣는거 국룰이잖아요ㅎ",
          "original_form": "럭셔리 마리드에 샤샤샥 효과 넣는거 국룰이잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 03:15"
        },
        {
          "id": "MDRW2100011399.1.21",
          "form": "그당시 초딩들이 럭마 갖고 싶어서 혈안이었죠ㅋㅋ",
          "original_form": "그당시 초딩들이 럭마 갖고 싶어서 혈안이었죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:15"
        }
      ]
