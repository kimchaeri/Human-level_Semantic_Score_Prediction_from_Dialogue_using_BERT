      "utterance": [
        {
          "id": "MDRW2100024558.1.1",
          "form": "전시회도 좋아하시나요?",
          "original_form": "전시회도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 14:09"
        },
        {
          "id": "MDRW2100024558.1.2",
          "form": "유명한거나 끌리는건 종종 가요",
          "original_form": "유명한거나 끌리는건 종종 가요",
          "speaker_id": "1",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024558.1.3",
          "form": "아직 가진 못했지만",
          "original_form": "아직 가진 못했지만",
          "speaker_id": "1",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024558.1.4",
          "form": "뱅크시전도 예매는 해뒀어요",
          "original_form": "뱅크시전도 예매는 해뒀어요",
          "speaker_id": "1",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024558.1.5",
          "form": "그렇군요 뱅크시전은 어떤 전시인가요?",
          "original_form": "그렇군요 뱅크시전은 어떤 전시인가요?",
          "speaker_id": "2",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024558.1.6",
          "form": "뱅크시라고 좀 유명한 화가가 있는데",
          "original_form": "뱅크시라고 좀 유명한 화가가 있는데",
          "speaker_id": "1",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024558.1.7",
          "form": "그사람의 그림을 몇점 전시하는 거죠",
          "original_form": "그사람의 그림을 몇점 전시하는 거죠",
          "speaker_id": "1",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024558.1.8",
          "form": "뱅크시 들어본것같아요 저도 전시회 좋아하는데 지역이 문화생활과 멀어서 항상 아쉽더라구요",
          "original_form": "뱅크시 들어본것같아요 저도 전시회 좋아하는데 지역이 문화생활과 멀어서 항상 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100024558.1.9",
          "form": "요즘은 정해진 숫자만 입장하던데 좋은것같아요",
          "original_form": "요즘은 정해진 숫자만 입장하던데 좋은것같아요",
          "speaker_id": "2",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100024558.1.10",
          "form": "하긴... 문화인프라가 서울에 집중되어있죠",
          "original_form": "하긴... 문화인프라가 서울에 집중되어있죠",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100024558.1.11",
          "form": "볼때는 좋죠.",
          "original_form": "볼때는 좋죠.",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100024558.1.12",
          "form": "근데 예매가 힘들죠",
          "original_form": "근데 예매가 힘들죠",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100024558.1.13",
          "form": "국립중앙박물관에서 한 이건희 컬렉션 보셨어요?",
          "original_form": "국립중앙박물관에서 한 이건희 컬렉션 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100024558.1.14",
          "form": "그거 진짜 티켓팅 헬...",
          "original_form": "그거 진짜 티켓팅 헬...",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100024558.1.15",
          "form": "헉 저 그거 한번 해볼까 하다가 시도도 못해봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "헉 저 그거 한번 해볼까 하다가 시도도 못해봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100024558.1.16",
          "form": "정말 장난아니더라구요 세상에...그렇게 힘들줄이야 ㅠ 한번 보고싶어요 ㅠㅠ",
          "original_form": "정말 장난아니더라구요 세상에...그렇게 힘들줄이야 ㅠ 한번 보고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:36"
        },
        {
          "id": "MDRW2100024558.1.17",
          "form": "하... 저 보긴했거든요?",
          "original_form": "하... 저 보긴했거든요?",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024558.1.18",
          "form": "근데 아빠가 부러워하면서 하나 더 잡아달랬는데",
          "original_form": "근데 아빠가 부러워하면서 하나 더 잡아달랬는데",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024558.1.19",
          "form": "그거 완전 실패",
          "original_form": "그거 완전 실패",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024558.1.20",
          "form": "맨처음 한달치 풀렸을땐 그래도 평일은 잡을만 했거든요",
          "original_form": "맨처음 한달치 풀렸을땐 그래도 평일은 잡을만 했거든요",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024558.1.21",
          "form": "근데 이젠 한 5초컷인듯요",
          "original_form": "근데 이젠 한 5초컷인듯요",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024558.1.22",
          "form": "헉 그렇군요 ㅠㅠㅠ 이러다 시간 지나면 나중에라도 볼수있을까요? 정말 보고싶네요",
          "original_form": "헉 그렇군요 ㅠㅠㅠ 이러다 시간 지나면 나중에라도 볼수있을까요? 정말 보고싶네요",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024558.1.23",
          "form": "어떠셨어요? 기사로만 봤을때는 정말 좋을것같던데요",
          "original_form": "어떠셨어요? 기사로만 봤을때는 정말 좋을것같던데요",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024558.1.24",
          "form": "좋긴좋았어요",
          "original_form": "좋긴좋았어요",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024558.1.25",
          "form": "근데 약간 이건희를 욕하게 되는",
          "original_form": "근데 약간 이건희를 욕하게 되는",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100024558.1.26",
          "form": "유물 퀄이 막눈으로 봐도 국보, 보물급들이라",
          "original_form": "유물 퀄이 막눈으로 봐도 국보, 보물급들이라",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100024558.1.27",
          "form": "상속세 아녔으면 삼성에 있는지도 몰랐을거 아녜요... 후",
          "original_form": "상속세 아녔으면 삼성에 있는지도 몰랐을거 아녜요... 후",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100024558.1.28",
          "form": "와.....그죠 그거 다 그거죠 허허",
          "original_form": "와.....그죠 그거 다 그거죠 허허",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024558.1.29",
          "form": "저도 전시회 꼭 가보고싶네요 ㅠㅠㅠ",
          "original_form": "저도 전시회 꼭 가보고싶네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024558.1.30",
          "form": "이럴때 느껴요 정말 돈있는사람의 취미조차 남다르구나 싶어요 ㅎㅎ",
          "original_form": "이럴때 느껴요 정말 돈있는사람의 취미조차 남다르구나 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024558.1.31",
          "form": "과연 취미였을까요..?",
          "original_form": "과연 취미였을까요..?",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024558.1.32",
          "form": "전 비자금이나 돈세탁 목적이 아니었을까 해요",
          "original_form": "전 비자금이나 돈세탁 목적이 아니었을까 해요",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024558.1.33",
          "form": "돈세탁이라고 거의 다 생각하더라구요! 허허",
          "original_form": "돈세탁이라고 거의 다 생각하더라구요! 허허",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100024558.1.34",
          "form": "휴... 마음이 복잡해지긴하지만 볼 만한 전시였어요",
          "original_form": "휴... 마음이 복잡해지긴하지만 볼 만한 전시였어요",
          "speaker_id": "1",
          "time": "20210831 22:47"
        }
      ]
    }
