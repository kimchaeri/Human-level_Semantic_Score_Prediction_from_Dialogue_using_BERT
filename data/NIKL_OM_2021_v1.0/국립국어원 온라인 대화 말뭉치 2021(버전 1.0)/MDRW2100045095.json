        {
          "id": "MDRW2100045095.1.1",
          "form": "콘서트 좋아해여?",
          "original_form": "콘서트 좋아해여?",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045095.1.2",
          "form": "오 네 좋아해요",
          "original_form": "오 네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.3",
          "form": "그래서 예술과 창조성이란 수업도 듣게되었어요",
          "original_form": "그래서 예술과 창조성이란 수업도 듣게되었어요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.4",
          "form": "마지막 콘서트가 언제에ㅐ요?",
          "original_form": "마지막 콘서트가 언제에ㅐ요?",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.5",
          "form": "우와 예술과 창조성 그거 뮤지컬관련수업맞죠??",
          "original_form": "우와 예술과 창조성 그거 뮤지컬관련수업맞죠??",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.6",
          "form": "마지막이라하면..당신과 같이간 데이식스 콘서트요",
          "original_form": "마지막이라하면..당신과 같이간 데이식스 콘서트요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.7",
          "form": "오오 네 맞아요",
          "original_form": "오오 네 맞아요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.8",
          "form": "우와 영광이에요",
          "original_form": "우와 영광이에요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.9",
          "form": "가고싶은 콘서트있어요?",
          "original_form": "가고싶은 콘서트있어요?",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045095.1.10",
          "form": "저요? 더보이즈아세요? 진짜 가고싶어서 미치겠어요",
          "original_form": "저요? 더보이즈아세요? 진짜 가고싶어서 미치겠어요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045095.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진정하고 말해봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진정하고 말해봐요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045095.1.12",
          "form": "하 세상에 말입니다 오늘 하 핸드볼콘서트 대관했다고 세에상에",
          "original_form": "하 세상에 말입니다 오늘 하 핸드볼콘서트 대관했다고 세에상에",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045095.1.13",
          "form": "저는 공식가입도 안했그등요 ㄱㅡ",
          "original_form": "저는 공식가입도 안했그등요 ㄱㅡ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045095.1.14",
          "form": "하 ㅡㅡ 밀가루씨 공식하고서 티켓팅 받아야할텐데말이에요",
          "original_form": "하 ㅡㅡ 밀가루씨 공식하고서 티켓팅 받아야할텐데말이에요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045095.1.15",
          "form": "돈 내겠다는데 왜 말을 안듣지",
          "original_form": "돈 내겠다는데 왜 말을 안듣지",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045095.1.16",
          "form": "name2님은 마지막 콘서트가 언제세여",
          "original_form": "&name2&님은 마지막 콘서트가 언제세여",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045095.1.17",
          "form": "그니까여 저 돈 많지는 않은데",
          "original_form": "그니까여 저 돈 많지는 않은데",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045095.1.18",
          "form": "쓸 순 있단 말이죠",
          "original_form": "쓸 순 있단 말이죠",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045095.1.19",
          "form": "저도 아마 데식콘서트요 !!",
          "original_form": "저도 아마 데식콘서트요 !!",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045095.1.20",
          "form": "돈은 준비가 됐는데 세상이 준비가 안됐네요",
          "original_form": "돈은 준비가 됐는데 세상이 준비가 안됐네요",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045095.1.21",
          "form": "사실 돈도",
          "original_form": "사실 돈도",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045095.1.22",
          "form": "돈 준비된 척 오지시네요",
          "original_form": "돈 준비된 척 오지시네요",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045095.1.23",
          "form": "저희 서로 티켓팅 도와줘요",
          "original_form": "저희 서로 티켓팅 도와줘요",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045095.1.24",
          "form": "어때염",
          "original_form": "어때염",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045095.1.25",
          "form": "저 티켓팅 잘 못해욕",
          "original_form": "저 티켓팅 잘 못해욕",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045095.1.26",
          "form": "그래도 도와드릴게요",
          "original_form": "그래도 도와드릴게요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045095.1.27",
          "form": "도와주세요",
          "original_form": "도와주세요",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045095.1.28",
          "form": "용병르로 섭시다",
          "original_form": "용병르로 섭시다",
