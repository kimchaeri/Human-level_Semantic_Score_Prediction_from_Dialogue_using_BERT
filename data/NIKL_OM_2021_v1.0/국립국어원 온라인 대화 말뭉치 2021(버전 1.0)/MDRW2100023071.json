      "utterance": [
        {
          "id": "MDRW2100023071.1.1",
          "form": "ㅇㅇ......",
          "original_form": "ㅇㅇ......",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100023071.1.2",
          "form": "그때는 개그콘서트가 진짜로 재미가 있었는데",
          "original_form": "그때는 개그콘서트가 진짜로 재미가 있었는데",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.3",
          "form": "그치 ㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.4",
          "form": "그때 ㄹㅇ 최고의 프로그램이엇움",
          "original_form": "그때 ㄹㅇ 최고의 프로그램이엇움",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.5",
          "form": "근데 지금 다시보면 겁나정색하고 볼듯",
          "original_form": "근데 지금 다시보면 겁나정색하고 볼듯",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.6",
          "form": "?이게...재밌나.....?",
          "original_form": "?이게...재밌나.....?",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.7",
          "form": "그니까 ㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.8",
          "form": "이게....?",
          "original_form": "이게....?",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.9",
          "form": "이걸 지금 개그라고 하나...?",
          "original_form": "이걸 지금 개그라고 하나...?",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.10",
          "form": "ㅇㅇㅋㅋㅋ자기 이마를 때리면서 토크를 하는게",
          "original_form": "ㅇㅇㅋㅋㅋ자기 이마를 때리면서 토크를 하는게",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.11",
          "form": "어디가 웃기단거지...?",
          "original_form": "어디가 웃기단거지...?",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023071.1.12",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.13",
          "form": "못생겼다고 놀리는게 웃기냐..?",
          "original_form": "못생겼다고 놀리는게 웃기냐..?",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.14",
          "form": "상태 됨 ㅎ",
          "original_form": "상태 됨 ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.15",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ생각해보면",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ생각해보면",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.16",
          "form": "3분의 2는 외모조롱이고",
          "original_form": "3분의 2는 외모조롱이고",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.17",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.18",
          "form": "프로그램중 하나는 무조건 장애인 조롱이었음",
          "original_form": "프로그램중 하나는 무조건 장애인 조롱이었음",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.19",
          "form": "웃긴게 사실상",
          "original_form": "웃긴게 사실상",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.20",
          "form": "비웃는건데",
          "original_form": "비웃는건데",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023071.1.21",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.22",
          "form": "지금 남은 개그프로도",
          "original_form": "지금 남은 개그프로도",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.23",
          "form": "없어져야함",
          "original_form": "없어져야함",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.24",
          "form": "노잼...",
          "original_form": "노잼...",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.25",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.26",
          "form": "재미도없고 감동도없고",
          "original_form": "재미도없고 감동도없고",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.27",
          "form": "악습만 담습하고있음",
          "original_form": "악습만 담습하고있음",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.28",
          "form": "ㅇㅇ 가족들이 다같이 볼 수준도 아니고",
          "original_form": "ㅇㅇ 가족들이 다같이 볼 수준도 아니고",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023071.1.29",
          "form": "ㅇㅇ....개그맨이란 직업이",
          "original_form": "ㅇㅇ....개그맨이란 직업이",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.30",
          "form": "미래가 있을지 모르겠음",
          "original_form": "미래가 있을지 모르겠음",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.31",
          "form": "걍 개콘이랑 웃찾사 폐지될 때 묻어서 같이 나가지",
          "original_form": "걍 개콘이랑 웃찾사 폐지될 때 묻어서 같이 나가지",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.32",
          "form": "우리나라는 스탠딩 코미디 문화도 없고",
          "original_form": "우리나라는 스탠딩 코미디 문화도 없고",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.33",
          "form": "왜 꾸역꾸역 버티고 있는지 모를",
          "original_form": "왜 꾸역꾸역 버티고 있는지 모를",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.34",
          "form": "ㅇㅇ....ㄹㅇ",
          "original_form": "ㅇㅇ....ㄹㅇ",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.35",
          "form": "아무도 안봄",
          "original_form": "아무도 안봄",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.36",
          "form": "저열한것도 저열한데",
          "original_form": "저열한것도 저열한데",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.37",
          "form": "저열하면서 재미도없음",
          "original_form": "저열하면서 재미도없음",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023071.1.38",
          "form": "걍 재미가 없어",
          "original_form": "걍 재미가 없어",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.39",
          "form": "그리고 가끔 채널 돌리다 한줄씩 들리는데",
          "original_form": "그리고 가끔 채널 돌리다 한줄씩 들리는데",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.40",
          "form": "여전히 뚱뚱한 여자 조롱하고",
          "original_form": "여전히 뚱뚱한 여자 조롱하고",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.41",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.42",
          "form": "솔로천국 커플지옥 이딴거나",
          "original_form": "솔로천국 커플지옥 이딴거나",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.43",
          "form": "하고있음",
          "original_form": "하고있음",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.44",
          "form": "솔직히 이정도 시대 흐름도 반영 못할거면",
          "original_form": "솔직히 이정도 시대 흐름도 반영 못할거면",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.45",
          "form": "걍 사라지는게 맞암",
          "original_form": "걍 사라지는게 맞암",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023071.1.46",
          "form": "개그맨 공채도 안한지 꽤 되지 않았나",
          "original_form": "개그맨 공채도 안한지 꽤 되지 않았나",
          "speaker_id": "1",
          "time": "20210829 21:29"
        }
      ]
    }
