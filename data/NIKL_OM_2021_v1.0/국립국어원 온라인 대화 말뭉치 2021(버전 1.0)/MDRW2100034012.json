      "utterance": [
        {
          "id": "MDRW2100034012.1.1",
          "form": "알쓰시군요!",
          "original_form": "알쓰시군요!",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100034012.1.2",
          "form": "알콜은 가까이 안하는게 좋긴하죠",
          "original_form": "알콜은 가까이 안하는게 좋긴하죠",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100034012.1.3",
          "form": "저는 달리고 다음날 처절하게 살아가는 편이라",
          "original_form": "저는 달리고 다음날 처절하게 살아가는 편이라",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100034012.1.4",
          "form": "매번 후회해요",
          "original_form": "매번 후회해요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100034012.1.5",
          "form": "ㅋㅋㅋ 그래도 술잘먹는거",
          "original_form": "ㅋㅋㅋ 그래도 술잘먹는거",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100034012.1.6",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100034012.1.7",
          "form": "저는 술중엔 하이볼이 제일좋아서\r\n그거만좀 마니마셔요",
          "original_form": "저는 술중엔 하이볼이 제일좋아서\r\n그거만좀 마니마셔요",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100034012.1.8",
          "form": "오 하이볼",
          "original_form": "오 하이볼",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100034012.1.9",
          "form": "저랑 취향이 같으시네요",
          "original_form": "저랑 취향이 같으시네요",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100034012.1.10",
          "form": "하이볼 맛있어요",
          "original_form": "하이볼 맛있어요",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100034012.1.11",
          "form": "하이볼 처음 먹었을 때 맛있기만해서 놀랐던 경험있어요",
          "original_form": "하이볼 처음 먹었을 때 맛있기만해서 놀랐던 경험있어요",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100034012.1.12",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100034012.1.13",
          "form": "진짜개존맛아닌가요",
          "original_form": "진짜개존맛아닌가요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100034012.1.14",
          "form": "알쓰를 위한술같아요",
          "original_form": "알쓰를 위한술같아요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100034012.1.15",
          "form": "아 진짜 전 맛있기만한데",
          "original_form": "아 진짜 전 맛있기만한데",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100034012.1.16",
          "form": "제 주위엔 안맞는 사람들도 있더라구요",
          "original_form": "제 주위엔 안맞는 사람들도 있더라구요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100034012.1.17",
          "form": "술맛만 난다고",
          "original_form": "술맛만 난다고",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100034012.1.18",
          "form": "name1님도 위스키가 잘 맞는 사람일 수 있어요",
          "original_form": "&name1&님도 위스키가 잘 맞는 사람일 수 있어요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100034012.1.19",
          "form": "전 마가리타도 맛있더라구요",
          "original_form": "전 마가리타도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100034012.1.20",
          "form": "혹시 드셔보셨나요?",
          "original_form": "혹시 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100034012.1.21",
          "form": "마가리타",
          "original_form": "마가리타",
          "speaker_id": "1",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100034012.1.22",
          "form": "칵테일이죠?!!",
          "original_form": "칵테일이죠?!!",
          "speaker_id": "1",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100034012.1.23",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100034012.1.24",
          "form": "신짠의 정석",
          "original_form": "신짠의 정석",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100034012.1.25",
          "form": "소금 없이 먹어도 맛있더라구요",
          "original_form": "소금 없이 먹어도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100034012.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100034012.1.27",
          "form": "직접 타드셨나요?",
          "original_form": "직접 타드셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100034012.1.28",
          "form": "아뇨 그 완제품으로 나온 거 먹었어요",
          "original_form": "아뇨 그 완제품으로 나온 거 먹었어요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100034012.1.29",
          "form": "오 완제품이",
          "original_form": "오 완제품이",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100034012.1.30",
          "form": "있군요?????",
          "original_form": "있군요?????",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100034012.1.31",
          "form": "네 꽤 괜찮더라구요",
          "original_form": "네 꽤 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100034012.1.32",
          "form": "집에서 한잔씩 마시기 좋아요",
          "original_form": "집에서 한잔씩 마시기 좋아요",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100034012.1.33",
          "form": "어디서파나요?!",
          "original_form": "어디서파나요?!",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100034012.1.34",
          "form": "트레이더스에서 팔았던것 같아요",
          "original_form": "트레이더스에서 팔았던것 같아요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100034012.1.35",
          "form": "대용량이라 처음에 사기 꺼려졌는데",
          "original_form": "대용량이라 처음에 사기 꺼려졌는데",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100034012.1.36",
          "form": "다행히 맛은 괜찮더라구요",
          "original_form": "다행히 맛은 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        }
      ]
