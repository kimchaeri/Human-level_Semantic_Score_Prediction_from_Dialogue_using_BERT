      "utterance": [
        {
          "id": "MDRW2100010510.1.1",
          "form": "10분만 이따",
          "original_form": "10분만 이따",
          "speaker_id": "2",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010510.1.2",
          "form": "안돼 곧 저녁도먹아야돼",
          "original_form": "안돼 곧 저녁도먹아야돼",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010510.1.3",
          "form": "오늘저녁 뭔데요",
          "original_form": "오늘저녁 뭔데요",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010510.1.4",
          "form": "마파두부",
          "original_form": "마파두부",
          "speaker_id": "1",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010510.1.5",
          "form": "알겠어요 청소할게요",
          "original_form": "알겠어요 청소할게요",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010510.1.6",
          "form": "아니근데 아까 알콜뿌려서",
          "original_form": "아니근데 아까 알콜뿌려서",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010510.1.7",
          "form": "제가문도 닦았는데",
          "original_form": "제가문도 닦았는데",
          "speaker_id": "2",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010510.1.8",
          "form": "또 청소 제가해요?",
          "original_form": "또 청소 제가해요?",
          "speaker_id": "2",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010510.1.9",
          "form": "어쩔수없어",
          "original_form": "어쩔수없어",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010510.1.10",
          "form": "할사람이 없음",
          "original_form": "할사람이 없음",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010510.1.11",
          "form": "너가 저녁차릴래 그럼?",
          "original_form": "너가 저녁차릴래 그럼?",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010510.1.12",
          "form": "....아니",
          "original_form": "....아니",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010510.1.13",
          "form": "할게.......요",
          "original_form": "할게.......요",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010510.1.14",
          "form": "청소기안하고",
          "original_form": "청소기안하고",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010510.1.15",
          "form": "밀대로 할게",
          "original_form": "밀대로 할게",
          "speaker_id": "2",
          "time": "20210823 19:05"
        },
        {
          "id": "MDRW2100010510.1.16",
          "form": "왜 청소기로하지?",
          "original_form": "왜 청소기로하지?",
          "speaker_id": "1",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.17",
          "form": "힘들어?",
          "original_form": "힘들어?",
          "speaker_id": "1",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.18",
          "form": "어제 청소기했으니까 오늘은",
          "original_form": "어제 청소기했으니까 오늘은",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.19",
          "form": "청소포끼워서 할랭",
          "original_form": "청소포끼워서 할랭",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.20",
          "form": "알겠어 청소기돌리면 미세먼지나오니까",
          "original_form": "알겠어 청소기돌리면 미세먼지나오니까",
          "speaker_id": "1",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.21",
          "form": "밀대로해",
          "original_form": "밀대로해",
          "speaker_id": "1",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.22",
          "form": "야호",
          "original_form": "야호",
          "speaker_id": "2",
          "time": "20210823 19:06"
        },
        {
          "id": "MDRW2100010510.1.23",
          "form": "지금시작해",
          "original_form": "지금시작해",
          "speaker_id": "1",
          "time": "20210823 19:08"
        },
