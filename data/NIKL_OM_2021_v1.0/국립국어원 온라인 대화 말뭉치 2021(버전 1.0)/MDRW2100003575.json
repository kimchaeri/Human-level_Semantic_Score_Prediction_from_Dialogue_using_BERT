      "utterance": [
        {
          "id": "MDRW2100003575.1.1",
          "form": "공연 보는거 좋아하시나요?",
          "original_form": "공연 보는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 02:53"
        },
        {
          "id": "MDRW2100003575.1.2",
          "form": "네 좋아해요 연극 뮤지컬 영화....\r\n요새 코로나땜에 못가고있네요",
          "original_form": "네 좋아해요 연극 뮤지컬 영화....\r\n요새 코로나땜에 못가고있네요",
          "speaker_id": "2",
          "time": "20210821 08:22"
        },
        {
          "id": "MDRW2100003575.1.3",
          "form": "아 그렇죠ㅠㅠ",
          "original_form": "아 그렇죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003575.1.4",
          "form": "저도 못간지 오래됐네요",
          "original_form": "저도 못간지 오래됐네요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003575.1.5",
          "form": "요즘 모가디슈 핫하다던데 보고싶은데 못가고있어요",
          "original_form": "요즘 모가디슈 핫하다던데 보고싶은데 못가고있어요",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100003575.1.6",
          "form": "오 맞아요 재밌어 보이더라구요",
          "original_form": "오 맞아요 재밌어 보이더라구요",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100003575.1.7",
          "form": "싱크홀도 재밌어 보이던데",
          "original_form": "싱크홀도 재밌어 보이던데",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100003575.1.8",
          "form": "싱크홀 잼있다고 하시더라구요",
          "original_form": "싱크홀 잼있다고 하시더라구요",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100003575.1.9",
          "form": "코믹과 감동이 같이 있데요",
          "original_form": "코믹과 감동이 같이 있데요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100003575.1.10",
          "form": "정말요? 코로나 때문에 영화관 못 가는게 아쉽네요ㅠㅠ",
          "original_form": "정말요? 코로나 때문에 영화관 못 가는게 아쉽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100003575.1.11",
          "form": "저두요 ㅜㅠ",
          "original_form": "저두요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100003575.1.12",
          "form": "그냥 집에서 넷플릭스로 즐길수밖에요",
          "original_form": "그냥 집에서 넷플릭스로 즐길수밖에요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100003575.1.13",
          "form": "요즘은 넷플릭스 같은 서비스가 잘 되어 있어서 그나마 다행인 것 같아요",
          "original_form": "요즘은 넷플릭스 같은 서비스가 잘 되어 있어서 그나마 다행인 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100003575.1.14",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100003575.1.15",
          "form": "오랫만에 비밀의숲도 다시봤던거같아요",
          "original_form": "오랫만에 비밀의숲도 다시봤던거같아요",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100003575.1.16",
          "form": "오 저 그거 아직 안 봤어요!",
          "original_form": "오 저 그거 아직 안 봤어요!",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100003575.1.17",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100003575.1.18",
          "form": "전 이거 진짜 인생 드라마인데요",
          "original_form": "전 이거 진짜 인생 드라마인데요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100003575.1.19",
          "form": "헉 정말요?",
          "original_form": "헉 정말요?",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100003575.1.20",
          "form": "강추해요 추리좋아하시면요 ㅎ",
          "original_form": "강추해요 추리좋아하시면요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:06"
        }
      ]
