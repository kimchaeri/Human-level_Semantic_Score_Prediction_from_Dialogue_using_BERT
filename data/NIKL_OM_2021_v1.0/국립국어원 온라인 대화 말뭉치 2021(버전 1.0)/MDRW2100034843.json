      "utterance": [
        {
          "id": "MDRW2100034843.1.1",
          "form": "바랍니다 ㅎㅎ",
          "original_form": "바랍니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100034843.1.2",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100034843.1.3",
          "form": "네 좋아합니다 ㅎㅎ 좋아하세요?",
          "original_form": "네 좋아합니다 ㅎㅎ 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 18:26"
        },
        {
          "id": "MDRW2100034843.1.4",
          "form": "저도 좋아해요ㅎㅎ",
          "original_form": "저도 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:27"
        },
        {
          "id": "MDRW2100034843.1.5",
          "form": "막 오버워치같은 컴퓨터게임도 하셨어요 ㅎㅎ ?",
          "original_form": "막 오버워치같은 컴퓨터게임도 하셨어요 ㅎㅎ ?",
          "speaker_id": "1",
          "time": "20210829 18:31"
        },
        {
          "id": "MDRW2100034843.1.6",
          "form": "컨트롤이 별로라 컴퓨터게임은 마인크래프트만 했어요ㅎㅎ",
          "original_form": "컨트롤이 별로라 컴퓨터게임은 마인크래프트만 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100034843.1.7",
          "form": "아아 마인크래프트 유튭으로 자주봤죠 ㅎㅎ",
          "original_form": "아아 마인크래프트 유튭으로 자주봤죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100034843.1.8",
          "form": "건설게임 좋아하시나봐요!",
          "original_form": "건설게임 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100034843.1.9",
          "form": "약간 저 혼자 컨트롤이 별로여도 할 수 있는 게임이라서요ㅋㅋㅋ 주로 광물 캐고 놀았어요ㅋㅋ",
          "original_form": "약간 저 혼자 컨트롤이 별로여도 할 수 있는 게임이라서요ㅋㅋㅋ 주로 광물 캐고 놀았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100034843.1.10",
          "form": "아아 ㅎㅎ 혼자하기 좋은 게임인가요 ㅎㅎ ??",
          "original_form": "아아 ㅎㅎ 혼자하기 좋은 게임인가요 ㅎㅎ ??",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100034843.1.11",
          "form": "좀비?가 안 나오는 초보 모드를 하면 저만 있어서요ㅎㅎ",
          "original_form": "좀비?가 안 나오는 초보 모드를 하면 저만 있어서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100034843.1.12",
          "form": "아아 초보모드는 안나오는군요 !",
          "original_form": "아아 초보모드는 안나오는군요 !",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100034843.1.13",
          "form": "나오면 순삭당해요...",
          "original_form": "나오면 순삭당해요...",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100034843.1.14",
          "form": "ㅋㅋㅋㅋ좀비가 혹시 무섭게 나오나요???",
          "original_form": "ㅋㅋㅋㅋ좀비가 혹시 무섭게 나오나요???",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100034843.1.15",
          "form": "네...광석 캐다가 갑자기 나오면 깜짝 놀라요ㅠ",
          "original_form": "네...광석 캐다가 갑자기 나오면 깜짝 놀라요ㅠ",
          "speaker_id": "2",
