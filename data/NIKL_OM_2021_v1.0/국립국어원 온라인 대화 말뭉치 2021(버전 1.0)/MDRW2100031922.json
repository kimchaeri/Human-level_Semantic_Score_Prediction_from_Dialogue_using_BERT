      "utterance": [
        {
          "id": "MDRW2100031922.1.1",
          "form": "조리하는사람이",
          "original_form": "조리하는사람이",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031922.1.2",
          "form": "그 바이러스를 내뿜은 상태로 조리를하니...",
          "original_form": "그 바이러스를 내뿜은 상태로 조리를하니...",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031922.1.3",
          "form": "생각해보니 소름이네여ㄷㄷ",
          "original_form": "생각해보니 소름이네여ㄷㄷ",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031922.1.4",
          "form": "그쳐?",
          "original_form": "그쳐?",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031922.1.5",
          "form": "배달 음식 못먹겠다",
          "original_form": "배달 음식 못먹겠다",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031922.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031922.1.7",
          "form": "서울얘기긴해요",
          "original_form": "서울얘기긴해요",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.8",
          "form": "어디 나돌아다녔다가 접촉자 된적이 없으니",
          "original_form": "어디 나돌아다녔다가 접촉자 된적이 없으니",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.9",
          "form": "아직도 다 거짓말 같아요",
          "original_form": "아직도 다 거짓말 같아요",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.10",
          "form": "부산은 많이 나아지지 않았어요?",
          "original_form": "부산은 많이 나아지지 않았어요?",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.11",
          "form": "부산은 이제 3단계로 다시 내려가서",
          "original_form": "부산은 이제 3단계로 다시 내려가서",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.12",
          "form": "전면등교 할 수 있게 되었거든요",
          "original_form": "전면등교 할 수 있게 되었거든요",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.14",
          "form": "물론 당장 다음주는 급식 주문이 안되어서 2/3등교 하지만..",
          "original_form": "물론 당장 다음주는 급식 주문이 안되어서 2/3등교 하지만..",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031922.1.15",
          "form": "급식  주문해서 각자 교실에서 먹는건가요?",
          "original_form": "급식  주문해서 각자 교실에서 먹는건가요?",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031922.1.16",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031922.1.17",
          "form": "급.식.실",
          "original_form": "급.식.실",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031922.1.18",
          "form": "에서 조리해서",
          "original_form": "에서 조리해서",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031922.1.19",
          "form": "교실에서 먹어요",
          "original_form": "교실에서 먹어요",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031922.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031922.1.21",
          "form": "ㅋㅋㅋㅋ급.식.실 왜 나쁜말",
          "original_form": "ㅋㅋㅋㅋ급.식.실 왜 나쁜말",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031922.1.22",
          "form": "교실에서 먹으면 좀 낫네요",
          "original_form": "교실에서 먹으면 좀 낫네요",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031922.1.23",
          "form": "ㅋㅋㅋ교실도 사실 비위생적이긴 한데",
          "original_form": "ㅋㅋㅋ교실도 사실 비위생적이긴 한데",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031922.1.24",
          "form": "뭐가 나은지 알 수 없어요ㅋㅋㅋ",
          "original_form": "뭐가 나은지 알 수 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031922.1.25",
          "form": "이제 교실 급식 위험하다고",
          "original_form": "이제 교실 급식 위험하다고",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100031922.1.26",
          "form": "새로 식당 지어준다고 예산 내려올 예정이라",
          "original_form": "새로 식당 지어준다고 예산 내려올 예정이라",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100031922.1.27",
          "form": "코로나로부터는 교실이 더 안전하지 않나여?ㅋㅋ",
          "original_form": "코로나로부터는 교실이 더 안전하지 않나여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100031922.1.28",
          "form": "여친은 교실급식을 원하던데",
          "original_form": "여친은 교실급식을 원하던데",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100031922.1.29",
          "form": "아 거긴 초등인데도 교실 급식 안하나보네용",
          "original_form": "아 거긴 초등인데도 교실 급식 안하나보네용",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100031922.1.30",
          "form": "코로나땐 교실이 더 안전한거 같긴해요",
          "original_form": "코로나땐 교실이 더 안전한거 같긴해요",
          "speaker_id": "2",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031922.1.31",
          "form": "교장이 이상해요",
          "original_form": "교장이 이상해요",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031922.1.32",
          "form": "제가 들어봐도 이상함",
          "original_form": "제가 들어봐도 이상함",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031922.1.33",
          "form": "애들 교실급식하면 선생님들은 식사 어디서 해요?",
          "original_form": "애들 교실급식하면 선생님들은 식사 어디서 해요?",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031922.1.34",
          "form": "ㅋㅋㅋ초등은 교장 교감들이 참 이상한 분이 많은 거 같아요",
          "original_form": "ㅋㅋㅋ초등은 교장 교감들이 참 이상한 분이 많은 거 같아요",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031922.1.35",
          "form": "선생님들은 교사식당",
          "original_form": "선생님들은 교사식당",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031922.1.36",
          "form": "파워가 중고등보다 쎄서 그런거 같아요",
          "original_form": "파워가 중고등보다 쎄서 그런거 같아요",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031922.1.37",
          "form": "듣다보면 욕이 절로 나옵니다 ㅋㅋㅋ",
          "original_form": "듣다보면 욕이 절로 나옵니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031922.1.38",
          "form": "그렇죠 저 심심이에서 초등교사분도 만났는데",
          "original_form": "그렇죠 저 심심이에서 초등교사분도 만났는데",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031922.1.39",
          "form": "학교 욕 엄청 하시더라구요ㅋㅋㅋㅋ",
          "original_form": "학교 욕 엄청 하시더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031922.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031922.1.41",
