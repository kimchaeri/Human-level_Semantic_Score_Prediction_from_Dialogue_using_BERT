      "utterance": [
        {
          "id": "MDRW2100020648.1.1",
          "form": "그러게 말야~ 너무 좋아",
          "original_form": "그러게 말야~ 너무 좋아",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020648.1.2",
          "form": "근데 미세먼지 수치가 좀 높네",
          "original_form": "근데 미세먼지 수치가 좀 높네",
          "speaker_id": "1",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020648.1.3",
          "form": "요즘은 완벽한 날이 없구만 ㅠ",
          "original_form": "요즘은 완벽한 날이 없구만 ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020648.1.4",
          "form": "비오거나 습하거나 미세먼지 있거나 ㅋㅋㅋ",
          "original_form": "비오거나 습하거나 미세먼지 있거나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020648.1.5",
          "form": "ㅇㅇ 좀 쾌적하고 미세먼지도 없으면 좋겠어",
          "original_form": "ㅇㅇ 좀 쾌적하고 미세먼지도 없으면 좋겠어",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020648.1.6",
          "form": "ㅎㅎ 곧 오겠지",
          "original_form": "ㅎㅎ 곧 오겠지",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020648.1.7",
          "form": "창문은 신랑이 다 닫은거야?",
          "original_form": "창문은 신랑이 다 닫은거야?",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020648.1.8",
          "form": "ㅇㅇ 미세먼지 심하면 작은 아들 눈 가려울까봐",
          "original_form": "ㅇㅇ 미세먼지 심하면 작은 아들 눈 가려울까봐",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020648.1.9",
          "form": "잘했네~ 더우면 에어컨 켜는 게 낫겠다",
          "original_form": "잘했네~ 더우면 에어컨 켜는 게 낫겠다",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020648.1.10",
          "form": "글치~ 아들 눈 깜빡이는 것보단 ㅠ",
          "original_form": "글치~ 아들 눈 깜빡이는 것보단 ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020648.1.11",
          "form": "코로나도 싫지만 미세먼지 정말 싫다",
          "original_form": "코로나도 싫지만 미세먼지 정말 싫다",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020648.1.12",
          "form": "환경이 점점 안좋아지는 것 같아",
          "original_form": "환경이 점점 안좋아지는 것 같아",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020648.1.13",
          "form": "우리 꼬맹이들한테 미안한 마음 뿐이야 ㅠ",
          "original_form": "우리 꼬맹이들한테 미안한 마음 뿐이야 ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:49"
