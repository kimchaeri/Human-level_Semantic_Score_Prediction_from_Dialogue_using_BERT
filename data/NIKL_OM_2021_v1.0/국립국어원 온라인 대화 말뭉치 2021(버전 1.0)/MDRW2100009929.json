      "utterance": [
        {
          "id": "MDRW2100009929.1.1",
          "form": "점심은 맛있게 드셨나요?",
          "original_form": "점심은 맛있게 드셨나요?",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009929.1.2",
          "form": "네 낙지볶음밥 먹었어요",
          "original_form": "네 낙지볶음밥 먹었어요",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100009929.1.3",
          "form": "와 맛있으셨겠어요~!!",
          "original_form": "와 맛있으셨겠어요~!!",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100009929.1.4",
          "form": "네 매운데 맛있긴했어요 ㅎㅎ",
          "original_form": "네 매운데 맛있긴했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100009929.1.5",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100009929.1.6",
          "form": "아뇨 저 완전 맵찔이예요 ㅋㅋ",
          "original_form": "아뇨 저 완전 맵찔이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100009929.1.7",
          "form": "잘드시나요?",
          "original_form": "잘드시나요?",
          "speaker_id": "2",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100009929.1.8",
          "form": "저도요~ㅎㅎㅎ 매운거 완전 못 먹어요",
          "original_form": "저도요~ㅎㅎㅎ 매운거 완전 못 먹어요",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100009929.1.9",
          "form": "ㅋㅋ 예전엔 잘먹었는데 안먹어버릇하니 못먹겠더라고요",
          "original_form": "ㅋㅋ 예전엔 잘먹었는데 안먹어버릇하니 못먹겠더라고요",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009929.1.10",
          "form": "오~저도 그랬었는데~!!ㅎㅎ 예전에는 잘 먹었었어요~",
          "original_form": "오~저도 그랬었는데~!!ㅎㅎ 예전에는 잘 먹었었어요~",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009929.1.11",
          "form": "매운걸 안먹어 버릇하니 못먹겠더라고요 ㅎㅎ",
          "original_form": "매운걸 안먹어 버릇하니 못먹겠더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100009929.1.12",
          "form": "네~저도 나이 들수록 매운걸 못먹게 된 것 같아요",
          "original_form": "네~저도 나이 들수록 매운걸 못먹게 된 것 같아요",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100009929.1.13",
          "form": "맞아요 속에서 불나는 기분",
          "original_form": "맞아요 속에서 불나는 기분",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100009929.1.14",
          "form": "네~그리고 나중에 배가 아프더라고요",
          "original_form": "네~그리고 나중에 배가 아프더라고요",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100009929.1.15",
          "form": "그래서 약먹은적도 있어요",
