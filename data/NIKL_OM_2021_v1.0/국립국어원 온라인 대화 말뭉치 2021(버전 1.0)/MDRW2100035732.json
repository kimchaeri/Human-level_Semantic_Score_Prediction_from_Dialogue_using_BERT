      "utterance": [
        {
          "id": "MDRW2100035732.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100035732.1.2",
          "form": "넵 안냥하세요 여름 좋아하시나요",
          "original_form": "넵 안냥하세요 여름 좋아하시나요",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100035732.1.3",
          "form": "아녀 ㅋㅋㅋㅋ여름보단 가을을좋아해요",
          "original_form": "아녀 ㅋㅋㅋㅋ여름보단 가을을좋아해요",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035732.1.4",
          "form": "지금계절이 여름이다보니 ㅎㅎ",
          "original_form": "지금계절이 여름이다보니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035732.1.5",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100035732.1.6",
          "form": "ㅋㅋㅋㅋㅋ전 여름 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ전 여름 좋아해요",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100035732.1.7",
          "form": "여름밤 너무 기분좋고 설레는",
          "original_form": "여름밤 너무 기분좋고 설레는",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100035732.1.8",
          "form": "저도 여름의 바다는너무좋은데 넘습하고더워서ㅜㅜㅜ습하지만않았으면 최애날씨됐을거에요",
          "original_form": "저도 여름의 바다는너무좋은데 넘습하고더워서ㅜㅜㅜ습하지만않았으면 최애날씨됐을거에요",
          "speaker_id": "1",
          "time": "20210830 00:12"
        },
        {
          "id": "MDRW2100035732.1.9",
          "form": "마자요 습도가 진짜 최대의 적인 계절이에요 ㅠㅠ",
          "original_form": "마자요 습도가 진짜 최대의 적인 계절이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100035732.1.10",
          "form": "그래도 한국은 여름이 젤 긴거 같으니 여름을 좋아해야",
          "original_form": "그래도 한국은 여름이 젤 긴거 같으니 여름을 좋아해야",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100035732.1.11",
          "form": "오래도록 행복할거같아요ㅜㅜㅜㅋㅋㅋㅋㅋ",
          "original_form": "오래도록 행복할거같아요ㅜㅜㅜㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100035732.1.12",
          "form": "ㅜ마자요 습도만좀 낮아졌으면..",
          "original_form": "ㅜ마자요 습도만좀 낮아졌으면..",
          "speaker_id": "1",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100035732.1.13",
          "form": "요새 옷은어떻게입으세요?",
          "original_form": "요새 옷은어떻게입으세요?",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100035732.1.14",
          "form": "요즘 긴팔입어요 ㅋㅋㅋㅋ가을옷 좋아하는데",
          "original_form": "요즘 긴팔입어요 ㅋㅋㅋㅋ가을옷 좋아하는데",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035732.1.15",
          "form": "가을옷 길게 못입으니까 ㅠㅠㅠ",
          "original_form": "가을옷 길게 못입으니까 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035732.1.16",
          "form": "지금부터 입어요 ㅋㅋㅋㅋ",
          "original_form": "지금부터 입어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035732.1.17",
          "form": "ㅋㅋㅋㅋ오 대단하세여",
          "original_form": "ㅋㅋㅋㅋ오 대단하세여",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035732.1.18",
          "form": "지금 또 너무 더워서 전 가을옷언제꺼내야할지 타이밍만잡다가 겨울될거같아요",
          "original_form": "지금 또 너무 더워서 전 가을옷언제꺼내야할지 타이밍만잡다가 겨울될거같아요",
          "speaker_id": "1",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100035732.1.19",
          "form": "최대한 에어컨 밑에 있겠다는 의지가 있어요 ㅋㅋㅋㅋ",
          "original_form": "최대한 에어컨 밑에 있겠다는 의지가 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100035732.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ이번주는 넘더웠어요 습해서그런지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ이번주는 넘더웠어요 습해서그런지",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100035732.1.21",
          "form": "저번주는 에어컨이나 선풍기키면 넘추웠는데말이져",
          "original_form": "저번주는 에어컨이나 선풍기키면 넘추웠는데말이져",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100035732.1.22",
          "form": "그러게요 처서 지나고 가을오는구나 했는데 다시 리턴하네여",
          "original_form": "그러게요 처서 지나고 가을오는구나 했는데 다시 리턴하네여",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100035732.1.23",
          "form": "그니까여ㅜㅜㅜ가을옷좋아하는데 입을때는 진짜 잠깐밖에못입구여",
          "original_form": "그니까여ㅜㅜㅜ가을옷좋아하는데 입을때는 진짜 잠깐밖에못입구여",
          "speaker_id": "1",
          "time": "20210830 00:30"
        }
      ]
    }
