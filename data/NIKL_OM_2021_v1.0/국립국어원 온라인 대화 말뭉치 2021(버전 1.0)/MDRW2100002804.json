      "utterance": [
        {
          "id": "MDRW2100002804.1.1",
          "form": "뭘삿니",
          "original_form": "뭘삿니",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002804.1.2",
          "form": "북 트롤리를 샀어.",
          "original_form": "북 트롤리를 샀어.",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002804.1.3",
          "form": "꺅 결국에 구매했구나",
          "original_form": "꺅 결국에 구매했구나",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.4",
          "form": "내가 이방저방 이동하며 공부를 하기 때문에 그걸 계속 둘 곳이 필요했어",
          "original_form": "내가 이방저방 이동하며 공부를 하기 때문에 그걸 계속 둘 곳이 필요했어",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.5",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.6",
          "form": "근데 그건 조립형이니??",
          "original_form": "근데 그건 조립형이니??",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.7",
          "form": "중국산인데 좋은 제품이 왓으면 좋겔어",
          "original_form": "중국산인데 좋은 제품이 왓으면 좋겔어",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.8",
          "form": "응 하지만 문제없어.",
          "original_form": "응 하지만 문제없어.",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.9",
          "form": "나는 무려 행거도 조립해봤는걸.",
          "original_form": "나는 무려 행거도 조립해봤는걸.",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.10",
          "form": "대단해 경력자구나",
          "original_form": "대단해 경력자구나",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.11",
          "form": "빨리 배송이 왔으면 졸겠어.",
          "original_form": "빨리 배송이 왔으면 졸겠어.",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.12",
          "form": "나는 어제 그거샀어",
          "original_form": "나는 어제 그거샀어",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.13",
          "form": "니가 추천해준",
          "original_form": "니가 추천해준",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.14",
          "form": "브레타 변기에 붙이는거",
          "original_form": "브레타 변기에 붙이는거",
          "speaker_id": "2",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.15",
          "form": "왜냐몀. 내 방은. 쏘 메씨.",
          "original_form": "왜냐몀. 내 방은. 쏘 메씨.",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002804.1.16",
          "form": "오. 브레타 샀구나",
          "original_form": "오. 브레타 샀구나",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.17",
          "form": "그거 냄새 괜찮아",
          "original_form": "그거 냄새 괜찮아",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.18",
          "form": "벌써 샀구나.",
          "original_form": "벌써 샀구나.",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.19",
          "form": "그런 단오는 너무 어렵다",
          "original_form": "그런 단오는 너무 어렵다",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.20",
          "form": "단어",
          "original_form": "단어",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.21",
          "form": "레몬으로 샀어",
          "original_form": "레몬으로 샀어",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.22",
          "form": "너는 먹는 레몬은 싫어하는데",
          "original_form": "너는 먹는 레몬은 싫어하는데",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.23",
          "form": "맡는 레몬은 좋아하는구나",
          "original_form": "맡는 레몬은 좋아하는구나",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.24",
          "form": "아니 그게 제일쌌여",
          "original_form": "아니 그게 제일쌌여",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002804.1.25",
          "form": "아 그런 심오한 뜻이 있었구나",
          "original_form": "아 그런 심오한 뜻이 있었구나",
          "speaker_id": "1",
          "time": "20210820 15:42"
        }
      ]
