      "utterance": [
        {
          "id": "MDRW2100007371.1.1",
          "form": "대학에서 만난 사람들중에",
          "original_form": "대학에서 만난 사람들중에",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100007371.1.2",
          "form": "특이한 사람은 없엉?",
          "original_form": "특이한 사람은 없엉?",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100007371.1.3",
          "form": "있으면 썰풀어보쟈 ㅇㅅㅇ)/",
          "original_form": "있으면 썰풀어보쟈 ㅇㅅㅇ)/",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100007371.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100007371.1.5",
          "form": "우리과에서 좀 특이하다고 하면",
          "original_form": "우리과에서 좀 특이하다고 하면",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100007371.1.6",
          "form": "환승연애 하는 애가 하나 있었어! 동기중에 ㅋㅋㅋㅋ",
          "original_form": "환승연애 하는 애가 하나 있었어! 동기중에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100007371.1.7",
          "form": "헐 너네과 내에서??",
          "original_form": "헐 너네과 내에서??",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100007371.1.8",
          "form": "어딜가나 그런 사람 하나씩은 있나봐",
          "original_form": "어딜가나 그런 사람 하나씩은 있나봐",
          "speaker_id": "2",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100007371.1.9",
          "form": "우리도 있었어 ㅋㅋㅋㅋㅋ 너네쪽은 어땠는데?",
          "original_form": "우리도 있었어 ㅋㅋㅋㅋㅋ 너네쪽은 어땠는데?",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007371.1.10",
          "form": "웅응 과 내에서 환승연애 하더라고ㅋㅋㅋㅋ",
          "original_form": "웅응 과 내에서 환승연애 하더라고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007371.1.11",
          "form": "사귀던 남친이 군대간사이에",
          "original_form": "사귀던 남친이 군대간사이에",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007371.1.12",
          "form": "엠티가서 다른 남자를 새로 사귀게 됐어 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엠티가서 다른 남자를 새로 사귀게 됐어 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100007371.1.13",
          "form": "헐 너네 과 인원도 적은",
          "original_form": "헐 너네 과 인원도 적은",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100007371.1.14",
          "form": "소수과 아니었어???",
          "original_form": "소수과 아니었어???",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100007371.1.15",
          "form": "다 끼리끼리 알텐데도... 간도 크다",
          "original_form": "다 끼리끼리 알텐데도... 간도 크다",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100007371.1.16",
          "form": "ㅇㅇ우리 소수과인데",
          "original_form": "ㅇㅇ우리 소수과인데",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100007371.1.17",
          "form": "나중에 와서는 헤어졌다가 바로 만난척 하더라고",
          "original_form": "나중에 와서는 헤어졌다가 바로 만난척 하더라고",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100007371.1.18",
          "form": "아닌거 다 아는데...",
          "original_form": "아닌거 다 아는데...",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007371.1.19",
          "form": "와...대단하다 여러의미로",
          "original_form": "와...대단하다 여러의미로",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007371.1.20",
          "form": "우리과는 그런 느낌 환승 이별은 아니고",
          "original_form": "우리과는 그런 느낌 환승 이별은 아니고",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007371.1.21",
          "form": "비밀연애(?)로 바람핀애는 있어 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "비밀연애(?)로 바람핀애는 있어 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007371.1.22",
          "form": "으잉??",
          "original_form": "으잉??",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007371.1.23",
          "form": "과 내에서 비밀연애하다가",
          "original_form": "과 내에서 비밀연애하다가",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100007371.1.24",
          "form": "바람피게된거야?",
          "original_form": "바람피게된거야?",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100007371.1.25",
          "form": "아니아니 ㅋㅋㅋㅋㅋ 우리과는 아니고 대학은 같은데 이미 졸업한 사람이랑",
          "original_form": "아니아니 ㅋㅋㅋㅋㅋ 우리과는 아니고 대학은 같은데 이미 졸업한 사람이랑",
          "speaker_id": "2",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100007371.1.26",
          "form": "사귀고 있었는데 계속 사귄건데 헤어진척 하더니",
          "original_form": "사귀고 있었는데 계속 사귄건데 헤어진척 하더니",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100007371.1.27",
          "form": "우리과에 동기 오빠랑 ㅋㅋㅋㅋㅋ 비밀연애로 사귄거야...",
          "original_form": "우리과에 동기 오빠랑 ㅋㅋㅋㅋㅋ 비밀연애로 사귄거야...",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100007371.1.28",
          "form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100007371.1.29",
          "form": "우리과 여자애보다",
          "original_form": "우리과 여자애보다",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100007371.1.30",
          "form": "더 뭔가 좀 나쁜데...?",
          "original_form": "더 뭔가 좀 나쁜데...?",
          "speaker_id": "1",
