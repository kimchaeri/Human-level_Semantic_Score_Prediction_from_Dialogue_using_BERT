      "utterance": [
        {
          "id": "MDRW2100030662.1.1",
          "form": "짜장면짬뽕이 땡깁니다 지금은",
          "original_form": "짜장면짬뽕이 땡깁니다 지금은",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.2",
          "form": "삼시 세번 챙겨드시나요",
          "original_form": "삼시 세번 챙겨드시나요",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.4",
          "form": "저는 하루 한 두끼정도",
          "original_form": "저는 하루 한 두끼정도",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.5",
          "form": "찹쌀탕수육도 같이",
          "original_form": "찹쌀탕수육도 같이",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.7",
          "form": "찍먹부먹중 어느쪽",
          "original_form": "찍먹부먹중 어느쪽",
          "speaker_id": "2",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.8",
          "form": "사주는건 전 언제나 잘 먹어요",
          "original_form": "사주는건 전 언제나 잘 먹어요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030662.1.9",
          "form": "저도 차리는건 싫어도 사주는건 대환영",
          "original_form": "저도 차리는건 싫어도 사주는건 대환영",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030662.1.10",
          "form": "그렇군요 굶으세여",
          "original_form": "그렇군요 굶으세여",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030662.1.11",
          "form": "먹는 모습이 복스러우면 예뻐요",
          "original_form": "먹는 모습이 복스러우면 예뻐요",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100030662.1.12",
          "form": "복스러운건 자신잇는데",
          "original_form": "복스러운건 자신잇는데",
          "speaker_id": "1",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030662.1.13",
          "form": "치킨은 후라이드 양념 어느쪽",
          "original_form": "치킨은 후라이드 양념 어느쪽",
          "speaker_id": "2",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030662.1.14",
          "form": "후라이드요",
          "original_form": "후라이드요",
          "speaker_id": "1",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030662.1.15",
          "form": "넹 수박화채 먹고프네요",
          "original_form": "넹 수박화채 먹고프네요",
          "speaker_id": "2",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030662.1.16",
          "form": "치킨에 무엇을 더한다는건 반칙",
          "original_form": "치킨에 무엇을 더한다는건 반칙",
          "speaker_id": "1",
          "time": "20210904 11:10"
        }
      ]
