        {
          "id": "MDRW2100030391.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030391.1.2",
          "form": "사실 내가 연애하고 싶어서",
          "original_form": "사실 내가 연애하고 싶어서",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030391.1.3",
          "form": "이걸로 바꿈",
          "original_form": "이걸로 바꿈",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030391.1.4",
          "form": "아 연애하고 싶다",
          "original_form": "아 연애하고 싶다",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030391.1.5",
          "form": "연애하고 싶다",
          "original_form": "연애하고 싶다",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030391.1.6",
          "form": "당신",
          "original_form": "당신",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030391.1.7",
          "form": "지금 모쏠이게.하실 말씀입니까?",
          "original_form": "지금 모쏠이게.하실 말씀입니까?",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030391.1.8",
          "form": "우선. 님친이 잇엇는지. 물어보는게. 먼저. 아닌가요?",
          "original_form": "우선. 님친이 잇엇는지. 물어보는게. 먼저. 아닌가요?",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030391.1.9",
          "form": "어떻기 샹각하시는지 휴먼?",
          "original_form": "어떻기 샹각하시는지 휴먼?",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030391.1.10",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030391.1.11",
          "form": "name2의 이상형은 무엇인가요",
          "original_form": "&name2&의 이상형은 무엇인가요",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030391.1.12",
          "form": "매우 궁금합니다",
          "original_form": "매우 궁금합니다",
          "speaker_id": "1",
          "time": "20210904 00:29"
        },
        {
          "id": "MDRW2100030391.1.13",
          "form": "아...저의 최애 스트라이크 존은 잇지만 ..",
          "original_form": "아...저의 최애 스트라이크 존은 잇지만 ..",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030391.1.14",
          "form": "이상형이요?",
          "original_form": "이상형이요?",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030391.1.15",
          "form": "우선 얼굴...얼굴..얼굴..",
          "original_form": "우선 얼굴...얼굴..얼굴..",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030391.1.16",
          "form": "그리고 인성과 하나님을 믿어야 함..",
          "original_form": "그리고 인성과 하나님을 믿어야 함..",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100030391.1.17",
          "form": "얼굴이 최고구나",
          "original_form": "얼굴이 최고구나",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030391.1.18",
          "form": "하긴 존잘이 최고지",
          "original_form": "하긴 존잘이 최고지",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030391.1.19",
          "form": "그쵸 우선 외향이 반 이상 목고 들어가지",
          "original_form": "그쵸 우선 외향이 반 이상 목고 들어가지",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.20",
          "form": "너는 아직도 이상형이",
          "original_form": "너는 아직도 이상형이",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.21",
          "form": "추어탕잘먹는남자니?",
          "original_form": "추어탕잘먹는남자니?",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.23",
          "form": "추어탕 잘 먹는",
          "original_form": "추어탕 잘 먹는",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.24",
          "form": "섹시한 남자",
          "original_form": "섹시한 남자",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.25",
          "form": "추가로 아침에 삼겹살 구워주는 남자",
          "original_form": "추가로 아침에 삼겹살 구워주는 남자",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.26",
          "form": "대신 조건이 있음",
          "original_form": "대신 조건이 있음",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030391.1.27",
          "form": "옆에 김치랑 팽이버섯도 같이 구워줘야 함",
          "original_form": "옆에 김치랑 팽이버섯도 같이 구워줘야 함",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030391.1.28",
          "form": "완벽 그 자체",
          "original_form": "완벽 그 자체",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030391.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030391.1.30",
          "form": "세세한 부분까지 ㅋㅋㅋㅋㅋ",
          "original_form": "세세한 부분까지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030391.1.31",
          "form": "나도 더 추가하면",
          "original_form": "나도 더 추가하면",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030391.1.32",
          "form": "나보다 키가 더 커야 해",
          "original_form": "나보다 키가 더 커야 해",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030391.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그리고 조건이 하나 더 있어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그리고 조건이 하나 더 있어",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030391.1.34",
          "form": "잘생겨야 함^^",
          "original_form": "잘생겨야 함^^",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030391.1.35",
          "form": "ㅋㅋㅋㅋㅋ 잘생기고 키큰 추어탕 잘먹고 고기 김치 팽이버섯 잘 굽는 섹시한 남자가",
          "original_form": "ㅋㅋㅋㅋㅋ 잘생기고 키큰 추어탕 잘먹고 고기 김치 팽이버섯 잘 굽는 섹시한 남자가",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030391.1.36",
          "form": "당신 이상형이군요 ㅋㅋㅋㅋㅋ",
          "original_form": "당신 이상형이군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030391.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ넵 맞습니다 바로 그거!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ넵 맞습니다 바로 그거!!",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030391.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 나는 귀엽고 듬직햇음 좋겟네...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 나는 귀엽고 듬직햇음 좋겟네...",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030391.1.39",
          "form": "귀여운데 듬직해..?",
          "original_form": "귀여운데 듬직해..?",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030391.1.40",
          "form": "ㅋㅋㅋㅋㅋ이건 무슨 조합이야",
          "original_form": "ㅋㅋㅋㅋㅋ이건 무슨 조합이야",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030391.1.41",
