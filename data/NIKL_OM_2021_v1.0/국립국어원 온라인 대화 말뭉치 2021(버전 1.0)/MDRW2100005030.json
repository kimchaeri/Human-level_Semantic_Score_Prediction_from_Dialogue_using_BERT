      "utterance": [
        {
          "id": "MDRW2100005030.1.1",
          "form": "맛집 탐방 가고싶다ㅠ",
          "original_form": "맛집 탐방 가고싶다ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005030.1.2",
          "form": "코로나 때문에..ㅜㅜ",
          "original_form": "코로나 때문에..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005030.1.3",
          "form": "맛집 탐방 좋지ㅠㅜ",
          "original_form": "맛집 탐방 좋지ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005030.1.4",
          "form": "기억나니",
          "original_form": "기억나니",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100005030.1.5",
          "form": "우리 애들 대학교 주변 맛집 탐방하자고 했잖아ㅠㅜ",
          "original_form": "우리 애들 대학교 주변 맛집 탐방하자고 했잖아ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100005030.1.6",
          "form": "아 그니까",
          "original_form": "아 그니까",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005030.1.7",
          "form": "언제 가냐ㅠㅠ",
          "original_form": "언제 가냐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005030.1.8",
          "form": "이러다가 맛집 탐방도 못 하고",
          "original_form": "이러다가 맛집 탐방도 못 하고",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005030.1.9",
          "form": "졸업할듯",
          "original_form": "졸업할듯",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005030.1.10",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005030.1.11",
          "form": "하 맛집ㅜㅜ 먹고 싶은 게 너무 많다",
          "original_form": "하 맛집ㅜㅜ 먹고 싶은 게 너무 많다",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005030.1.12",
          "form": "뭐가 제일 먹고 싶어??",
          "original_form": "뭐가 제일 먹고 싶어??",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005030.1.13",
          "form": "나는.. 양식 좋아",
          "original_form": "나는.. 양식 좋아",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005030.1.14",
          "form": "까르보나라도 좋고",
          "original_form": "까르보나라도 좋고",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005030.1.15",
          "form": "양식 좋아",
          "original_form": "양식 좋아",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005030.1.16",
          "form": "돈까스도 사랑",
          "original_form": "돈까스도 사랑",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005030.1.17",
          "form": "나는 일식은 별로 안 좋아하는 거 같음",
          "original_form": "나는 일식은 별로 안 좋아하는 거 같음",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005030.1.18",
          "form": "일식 라멘, 타코야끼 이런 애들은 좋아하는데",
          "original_form": "일식 라멘, 타코야끼 이런 애들은 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005030.1.19",
          "form": "스시 쪽은 진짜 안 좋아해서",
          "original_form": "스시 쪽은 진짜 안 좋아해서",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005030.1.20",
          "form": "안 먹음",
          "original_form": "안 먹음",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005030.1.21",
          "form": "아하 와사비가 별론가?",
          "original_form": "아하 와사비가 별론가?",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005030.1.22",
          "form": "소바 이런 건?",
          "original_form": "소바 이런 건?",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005030.1.23",
          "form": "와사비 싫어해!",
          "original_form": "와사비 싫어해!",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005030.1.24",
          "form": "소바는 커서는 안 먹어봤는데",
          "original_form": "소바는 커서는 안 먹어봤는데",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005030.1.25",
          "form": "어릴때는 소바보다는 돈가스였다",
          "original_form": "어릴때는 소바보다는 돈가스였다",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005030.1.26",
          "form": "ㅠㅠ 나도 그랬는데 요즘은 소바 좋더라",
          "original_form": "ㅠㅠ 나도 그랬는데 요즘은 소바 좋더라",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005030.1.27",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005030.1.28",
          "form": "다음에 도전해봐야겠다",
          "original_form": "다음에 도전해봐야겠다",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005030.1.29",
          "form": "응ㅜ 냉소바 좋아",
          "original_form": "응ㅜ 냉소바 좋아",
          "speaker_id": "1",
          "time": "20210821 22:29"
        }
      ]
    }
  ]
