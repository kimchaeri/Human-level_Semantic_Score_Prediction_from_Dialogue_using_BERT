      "utterance": [
        {
          "id": "MDRW2100023924.1.1",
          "form": "음악쌤이 열정이 대단하시네요",
          "original_form": "음악쌤이 열정이 대단하시네요",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023924.1.2",
          "form": "아그런가요? 실기시험때문이라도 다들열심히하더라구요ㅎㅎ 악기는 신기한게 연습하면 할수록 실력이 늘긴해요ㅎㅎ",
          "original_form": "아그런가요? 실기시험때문이라도 다들열심히하더라구요ㅎㅎ 악기는 신기한게 연습하면 할수록 실력이 늘긴해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023924.1.3",
          "form": "손가락의 고통을 이겨내고",
          "original_form": "손가락의 고통을 이겨내고",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023924.1.4",
          "form": "연습을 계속하셨군요",
          "original_form": "연습을 계속하셨군요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023924.1.5",
          "form": "ㅋㅋ 나중엔굳은살생겨서 안아파요ㅋㅋ 기타랑 조금 비슷한 원리에요ㅋㅋ",
          "original_form": "ㅋㅋ 나중엔굳은살생겨서 안아파요ㅋㅋ 기타랑 조금 비슷한 원리에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023924.1.6",
          "form": "그렇겠어요 둘다 줄을 튕기니!!",
          "original_form": "그렇겠어요 둘다 줄을 튕기니!!",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023924.1.7",
          "form": "학교에서 이런 악기들 다양하게 알려주면 좋을텐데",
          "original_form": "학교에서 이런 악기들 다양하게 알려주면 좋을텐데",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023924.1.8",
          "form": "네네ㅎㅎ 기타는 클래식기타동아리인거예요?",
          "original_form": "네네ㅎㅎ 기타는 클래식기타동아리인거예요?",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023924.1.9",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023924.1.10",
          "form": "저는 연습해도 안 늘더라구요ㅜㅜ",
          "original_form": "저는 연습해도 안 늘더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023924.1.11",
          "form": "손가락이 제 말을 안들어서ㅜㅜ",
          "original_form": "손가락이 제 말을 안들어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023924.1.12",
          "form": "앗!ㅜㅜ ㅋㅋ 그건그래욪",
          "original_form": "앗!ㅜㅜ ㅋㅋ 그건그래욪",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023924.1.13",
          "form": "아랑훼즈 협주곡 이런거 하고싶었는데",
          "original_form": "아랑훼즈 협주곡 이런거 하고싶었는데",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023924.1.14",
          "form": "저는 사실 어릴때부터 피아노를 쳐서 악보보는거나 악기다루는거에 익숙해져서 그런거같아요",
          "original_form": "저는 사실 어릴때부터 피아노를 쳐서 악보보는거나 악기다루는거에 익숙해져서 그런거같아요",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023924.1.15",
          "form": "꿈만 너무 거창",
          "original_form": "꿈만 너무 거창",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023924.1.16",
          "form": "와 그러셨군용",
          "original_form": "와 그러셨군용",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023924.1.17",
          "form": "원래 꿈은 거창해야해요ㅎㅎ",
          "original_form": "원래 꿈은 거창해야해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023924.1.18",
          "form": "근데 동아리는 결국 목적만 다를뿐 친구들 다양하게 사귀고 결국 놀기바쁘고 ㅋㅋㅋ",
          "original_form": "근데 동아리는 결국 목적만 다를뿐 친구들 다양하게 사귀고 결국 놀기바쁘고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023924.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100023924.1.20",
          "form": "맞아요 목적이 친목도모로",
          "original_form": "맞아요 목적이 친목도모로",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023924.1.21",
          "form": "다그런거죠ㅋㅋ학교다닐때가 참좋았는데ㅋㅋ 그리워요ㅋㅋ",
          "original_form": "다그런거죠ㅋㅋ학교다닐때가 참좋았는데ㅋㅋ 그리워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023924.1.22",
          "form": "맞아요  저도 그리워요ㅜㅜㅡ",
          "original_form": "맞아요  저도 그리워요ㅜㅜㅡ",
