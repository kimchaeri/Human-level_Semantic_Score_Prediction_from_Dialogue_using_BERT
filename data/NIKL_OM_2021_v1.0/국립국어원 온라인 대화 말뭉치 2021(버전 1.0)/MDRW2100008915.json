      "utterance": [
        {
          "id": "MDRW2100008915.1.1",
          "form": "유투브 잘보세요?",
          "original_form": "유투브 잘보세요?",
          "speaker_id": "1",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008915.1.2",
          "form": "넹 자주봐요!!",
          "original_form": "넹 자주봐요!!",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008915.1.3",
          "form": "주로 어떤영상 보세여?",
          "original_form": "주로 어떤영상 보세여?",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008915.1.4",
          "form": "저 요즘은 래퍼 한명한테 빠져서 그 관련영상 보고잇어요 ㅋㅋㅋ",
          "original_form": "저 요즘은 래퍼 한명한테 빠져서 그 관련영상 보고잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008915.1.5",
          "form": "오 누군지 물어봐도될까여",
          "original_form": "오 누군지 물어봐도될까여",
          "speaker_id": "1",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008915.1.6",
          "form": "래원이욤",
          "original_form": "래원이욤",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008915.1.7",
          "form": "혹시 아세요?",
          "original_form": "혹시 아세요?",
          "speaker_id": "2",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008915.1.8",
          "form": "아 아니요 ㅠㅋㅋ 덕분에 찾아보게될거같네용",
          "original_form": "아 아니요 ㅠㅋㅋ 덕분에 찾아보게될거같네용",
          "speaker_id": "1",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008915.1.9",
          "form": "전 먹방 술방 뜨는거 봐요 ㅎㅎ",
          "original_form": "전 먹방 술방 뜨는거 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008915.1.10",
          "form": "오오 저도 먹방 자주봐요 ㅋㅋㅋ 대리만족",
          "original_form": "오오 저도 먹방 자주봐요 ㅋㅋㅋ 대리만족",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008915.1.11",
          "form": "자기전에 보는 먹방은 진짜 지옥이에여 ㅠㅠ",
          "original_form": "자기전에 보는 먹방은 진짜 지옥이에여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100008915.1.12",
          "form": "근데도 못끄고 ..",
          "original_form": "근데도 못끄고 ..",
          "speaker_id": "1",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100008915.1.13",
          "form": "ㅋㅋㅋ 마자요 중독이에요 완전",
          "original_form": "ㅋㅋㅋ 마자요 중독이에요 완전",
          "speaker_id": "2",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008915.1.14",
          "form": "요즘 댄스동영상도 많이뜨더라구욥!",
          "original_form": "요즘 댄스동영상도 많이뜨더라구욥!",
          "speaker_id": "1",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008915.1.15",
          "form": "오 저도 가끔씩 떠용",
          "original_form": "오 저도 가끔씩 떠용",
          "speaker_id": "2",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008915.1.16",
          "form": "요즘 다들 다재다능한거 같아용",
          "original_form": "요즘 다들 다재다능한거 같아용",
          "speaker_id": "1",
          "time": "20210822 20:34"
        },
        {
          "id": "MDRW2100008915.1.17",
          "form": "맞아요... 재능이 뛰어난사람들이 많아여...ㅠ",
          "original_form": "맞아요... 재능이 뛰어난사람들이 많아여...ㅠ",
