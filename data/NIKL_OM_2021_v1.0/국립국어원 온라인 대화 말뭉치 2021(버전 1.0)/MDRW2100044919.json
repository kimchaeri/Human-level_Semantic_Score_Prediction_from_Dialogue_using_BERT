      "utterance": [
        {
          "id": "MDRW2100044919.1.1",
          "form": "ㅋㅋㅋ프링글스는 어때요",
          "original_form": "ㅋㅋㅋ프링글스는 어때요",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044919.1.2",
          "form": "프링글스가 짠것같으면서 중독성 미챠요",
          "original_form": "프링글스가 짠것같으면서 중독성 미챠요",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044919.1.3",
          "form": "그져... 포카칩은 근데 더 가벼윤 맛",
          "original_form": "그져... 포카칩은 근데 더 가벼윤 맛",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044919.1.4",
          "form": "ㅋㅋㅋ마자요 가벼워서 잘들어가요",
          "original_form": "ㅋㅋㅋ마자요 가벼워서 잘들어가요",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044919.1.5",
          "form": "매점식 먹고싶어요",
          "original_form": "매점식 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044919.1.6",
          "form": "먼지아심 ?",
          "original_form": "먼지아심 ?",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044919.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ알져알죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ알져알죠",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044919.1.8",
          "form": "편의점 마크정식 아세요?",
          "original_form": "편의점 마크정식 아세요?",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044919.1.9",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044919.1.10",
          "form": "조아하심 ?",
          "original_form": "조아하심 ?",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044919.1.11",
          "form": "네네 약간 싸구려맛인데",
          "original_form": "네네 약간 싸구려맛인데",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044919.1.12",
          "form": "가끔생각나더라두요",
          "original_form": "가끔생각나더라두요",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044919.1.13",
          "form": "ㅋㅋㅋㅋ 추가로 넣는거 잇어요?",
          "original_form": "ㅋㅋㅋㅋ 추가로 넣는거 잇어요?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044919.1.14",
          "form": "아니요 저거만해도",
          "original_form": "아니요 저거만해도",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044919.1.15",
          "form": "양이너무많아요ㅠ",
          "original_form": "양이너무많아요ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044919.1.16",
          "form": "ㅋㅋㅋ그게 머머 들어갓엇죠?",
          "original_form": "ㅋㅋㅋ그게 머머 들어갓엇죠?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044919.1.17",
          "form": "음 스파게티라면이랑 떡볶이랑 삼김 치즈??",
          "original_form": "음 스파게티라면이랑 떡볶이랑 삼김 치즈??",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044919.1.18",
          "form": "아하 기억이잘암낙는대",
          "original_form": "아하 기억이잘암낙는대",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044919.1.19",
          "form": "많앗군요ㅋㅋ",
          "original_form": "많앗군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044919.1.20",
          "form": "친구랑 머거요?",
          "original_form": "친구랑 머거요?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044919.1.21",
          "form": "ㅋㅋ 친구랑 먹었었어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋ 친구랑 먹었었어요 ㅋㅋㅋ",
          "speaker_id": "1",
