      "utterance": [
        {
          "id": "MDRW2100011779.1.1",
          "form": "그래서 남자 더 못믿겠어요 ㅋㅋㅋㅋ",
          "original_form": "그래서 남자 더 못믿겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011779.1.2",
          "form": "그런 범죄에 일상적으로 노출되어있다고 하니",
          "original_form": "그런 범죄에 일상적으로 노출되어있다고 하니",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011779.1.3",
          "form": "맞아요 겉으로 보기에도 좀 그렇지만",
          "original_form": "맞아요 겉으로 보기에도 좀 그렇지만",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011779.1.4",
          "form": "건실해 보이는 사람들도 뭐",
          "original_form": "건실해 보이는 사람들도 뭐",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011779.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011779.1.6",
          "form": "종교적 이유가 있는게 아니면 거의 다 그모양인거같아요",
          "original_form": "종교적 이유가 있는게 아니면 거의 다 그모양인거같아요",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011779.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011779.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.9",
          "form": "종교도",
          "original_form": "종교도",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.10",
          "form": "불교 천주교 가릴 것 없이",
          "original_form": "불교 천주교 가릴 것 없이",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.11",
          "form": "사건 터져서",
          "original_form": "사건 터져서",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.12",
          "form": "아 맞아욬ㅋㅋㅋㅋ",
          "original_form": "아 맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.13",
          "form": "겉으로만 그러는건가 싶기도 해요",
          "original_form": "겉으로만 그러는건가 싶기도 해요",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.14",
          "form": "저 얼마전에 템플스테이 다녀왔는데",
          "original_form": "저 얼마전에 템플스테이 다녀왔는데",
          "speaker_id": "1",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.15",
          "form": "비구니 스님이 남스님들 밥 챙기더라구요",
          "original_form": "비구니 스님이 남스님들 밥 챙기더라구요",
          "speaker_id": "1",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.16",
          "form": "오 자연속에서",
          "original_form": "오 자연속에서",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.17",
          "form": "여자 신도들이 계속 밥해주러오고",
          "original_form": "여자 신도들이 계속 밥해주러오고",
          "speaker_id": "1",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011779.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011779.1.19",
          "form": "기괴하네요",
          "original_form": "기괴하네요",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011779.1.20",
          "form": "비혼 결심 +1",
          "original_form": "비혼 결심 +1",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011779.1.21",
          "form": "자기밥은 자기가 해야 하는거 아닌가요",
          "original_form": "자기밥은 자기가 해야 하는거 아닌가요",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011779.1.22",
          "form": "맞읍니다.. 왜 자기가 종교일 한다고 집나와서 신도들 부려먹는지 모르겟어요",
          "original_form": "맞읍니다.. 왜 자기가 종교일 한다고 집나와서 신도들 부려먹는지 모르겟어요",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011779.1.23",
          "form": "팀플스테이 오는 사람들 밥은 스님들이 하는줄 알았어요",
          "original_form": "팀플스테이 오는 사람들 밥은 스님들이 하는줄 알았어요",
          "speaker_id": "2",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011779.1.24",
          "form": "여자 신도가 대부분 하고 여자 스님도 하셔요",
          "original_form": "여자 신도가 대부분 하고 여자 스님도 하셔요",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011779.1.25",
          "form": "아 여자스님",
          "original_form": "아 여자스님",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011779.1.26",
          "form": "근데 남스님이 하는건 한번도 못봤어요 여러번 가봤는데",
          "original_form": "근데 남스님이 하는건 한번도 못봤어요 여러번 가봤는데",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011779.1.27",
          "form": "정말 놀랍네요",
          "original_form": "정말 놀랍네요",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011779.1.28",
          "form": "그런게 자연스러운 분위기였겠네요",
          "original_form": "그런게 자연스러운 분위기였겠네요",
          "speaker_id": "2",
          "time": "20210824 16:03"
        }
      ]
