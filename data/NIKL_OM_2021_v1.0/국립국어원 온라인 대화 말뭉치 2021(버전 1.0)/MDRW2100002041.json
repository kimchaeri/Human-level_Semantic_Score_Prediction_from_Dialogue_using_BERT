      "utterance": [
        {
          "id": "MDRW2100002041.1.1",
          "form": "싸이 세대 맞지?",
          "original_form": "싸이 세대 맞지?",
          "speaker_id": "1",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100002041.1.2",
          "form": "그치 싸이갬성이지",
          "original_form": "그치 싸이갬성이지",
          "speaker_id": "2",
          "time": "20210819 23:04"
        },
        {
          "id": "MDRW2100002041.1.3",
          "form": "비지엠이 들려오네",
          "original_form": "비지엠이 들려오네",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002041.1.4",
          "form": "싸이 다시 부활했음?",
          "original_form": "싸이 다시 부활했음?",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002041.1.5",
          "form": "그렇다더라",
          "original_form": "그렇다더라",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100002041.1.6",
          "form": "도토리 아니고 무슨 비트코인이래",
          "original_form": "도토리 아니고 무슨 비트코인이래",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100002041.1.7",
          "form": "확실하진 않음 부활은 맞고",
          "original_form": "확실하진 않음 부활은 맞고",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100002041.1.8",
          "form": "코인으로한다고?ㅋㅋㅋ",
          "original_form": "코인으로한다고?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100002041.1.9",
          "form": "어 ㅋㅋㅋ 기사 봤어",
          "original_form": "어 ㅋㅋㅋ 기사 봤어",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100002041.1.10",
          "form": "싸이월드 부활하면 다시할거야?ㅋㅋ",
          "original_form": "싸이월드 부활하면 다시할거야?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002041.1.11",
          "form": "절대 안해...",
          "original_form": "절대 안해...",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002041.1.12",
          "form": "일단 들어가서 사진 남아있나 봐야할듯",
          "original_form": "일단 들어가서 사진 남아있나 봐야할듯",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002041.1.13",
          "form": "흑역사 ㅜ",
          "original_form": "흑역사 ㅜ",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002041.1.14",
          "form": "ㅋㅋㅋㅋ아디뭐임 나중에 구경가게",
          "original_form": "ㅋㅋㅋㅋ아디뭐임 나중에 구경가게",
          "speaker_id": "2",
          "time": "20210820 03:48"
        },
        {
          "id": "MDRW2100002041.1.15",
          "form": "나도 몰라서 못알려줌ㅋㅋㅋㅋㅋ",
          "original_form": "나도 몰라서 못알려줌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 07:21"
        },
        {
          "id": "MDRW2100002041.1.16",
          "form": "찾아봐야돼",
          "original_form": "찾아봐야돼",
          "speaker_id": "1",
          "time": "20210820 07:21"
        },
        {
          "id": "MDRW2100002041.1.17",
          "form": "ㅋㅋㅋㅋㅋ나도 예전상태 그대로있겠지",
          "original_form": "ㅋㅋㅋㅋㅋ나도 예전상태 그대로있겠지",
          "speaker_id": "2",
          "time": "20210820 08:31"
        },
        {
          "id": "MDRW2100002041.1.18",
          "form": "싸이월드 이미 오픈은 했나보네 ㅋㅋ",
          "original_form": "싸이월드 이미 오픈은 했나보네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:34"
        },
        {
          "id": "MDRW2100002041.1.19",
          "form": "아 진짜??? 벌써 한거였어???",
          "original_form": "아 진짜??? 벌써 한거였어???",
          "speaker_id": "1",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100002041.1.20",
          "form": "이미 늦었다.....",
          "original_form": "이미 늦었다.....",
          "speaker_id": "1",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100002041.1.21",
          "form": "되돌릴수 없을듯",
          "original_form": "되돌릴수 없을듯",
          "speaker_id": "1",
          "time": "20210820 09:00"
        },
        {
          "id": "MDRW2100002041.1.22",
          "form": "ㅋㅋㅋㅋㅋ블로그보니 5월달에 오픈한다던거같은데",
          "original_form": "ㅋㅋㅋㅋㅋ블로그보니 5월달에 오픈한다던거같은데",
          "speaker_id": "2",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100002041.1.23",
          "form": "나도 이제 안할듯..",
          "original_form": "나도 이제 안할듯..",
          "speaker_id": "2",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100002041.1.24",
          "form": "할시간이없다",
          "original_form": "할시간이없다",
          "speaker_id": "2",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002041.1.25",
          "form": "엥 5월???",
          "original_form": "엥 5월???",
          "speaker_id": "1",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002041.1.26",
          "form": "벌써 세달이나 된거여??",
          "original_form": "벌써 세달이나 된거여??",
          "speaker_id": "1",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002041.1.27",
          "form": "이미 퍼질대로 퍼졌겠다 사진 남아있으면",
          "original_form": "이미 퍼질대로 퍼졌겠다 사진 남아있으면",
          "speaker_id": "1",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002041.1.28",
          "form": "그런거같은데 ㅋㅋ밀렸나?",
          "original_form": "그런거같은데 ㅋㅋ밀렸나?",
          "speaker_id": "2",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002041.1.29",
          "form": "아디찾기 사전예약도 했던데 ㅋㅋ",
          "original_form": "아디찾기 사전예약도 했던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002041.1.30",
          "form": "아 그거는 했었다...",
          "original_form": "아 그거는 했었다...",
          "speaker_id": "1",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002041.1.31",
          "form": "이메일이랑 비번",
          "original_form": "이메일이랑 비번",
