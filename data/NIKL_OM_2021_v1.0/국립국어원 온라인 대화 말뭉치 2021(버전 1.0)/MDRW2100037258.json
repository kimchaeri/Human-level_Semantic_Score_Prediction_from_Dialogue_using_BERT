      "utterance": [
        {
          "id": "MDRW2100037258.1.1",
          "form": "요즘에는 애니음악을 들어욥",
          "original_form": "요즘에는 애니음악을 들어욥",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100037258.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100037258.1.3",
          "form": "저는 피아노 연주 듣고 있어요",
          "original_form": "저는 피아노 연주 듣고 있어요",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100037258.1.4",
          "form": "다니는 한의원 분위기 나서 편안하더라구요",
          "original_form": "다니는 한의원 분위기 나서 편안하더라구요",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037258.1.5",
          "form": "오오~ 피아노 연주 좋아하시는군요",
          "original_form": "오오~ 피아노 연주 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037258.1.6",
          "form": "어떤거로 들으세요?",
          "original_form": "어떤거로 들으세요?",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100037258.1.7",
          "form": "가요 팝 애니 등을 피아노로 연주하면 그냥 들어요 유튜브에서 피아노 연주로 검색해서요",
          "original_form": "가요 팝 애니 등을 피아노로 연주하면 그냥 들어요 유튜브에서 피아노 연주로 검색해서요",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037258.1.8",
          "form": "오오~ 지브리 노래도 들으세요?",
          "original_form": "오오~ 지브리 노래도 들으세요?",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100037258.1.9",
          "form": "네 지브리 좋아요 애니도요",
          "original_form": "네 지브리 좋아요 애니도요",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037258.1.10",
          "form": "꺄 진짜 노래 너무 좋지않나요?",
          "original_form": "꺄 진짜 노래 너무 좋지않나요?",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037258.1.11",
          "form": "저도 종종 피아노로 듣는데 너무 좋더라구요",
          "original_form": "저도 종종 피아노로 듣는데 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037258.1.12",
          "form": "맞아요 지브리 음악 넘 좋아요",
          "original_form": "맞아요 지브리 음악 넘 좋아요",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100037258.1.13",
          "form": "명곡이 많지만 어떤 곡을 제일 좋아하세요?",
          "original_form": "명곡이 많지만 어떤 곡을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100037258.1.14",
          "form": "아무래도 토토로용!",
          "original_form": "아무래도 토토로용!",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037258.1.15",
          "form": "키키도 좋아해욤",
          "original_form": "키키도 좋아해욤",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100037258.1.16",
          "form": "전 하울이랑 라퓨타요 ㅎㅎ",
          "original_form": "전 하울이랑 라퓨타요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037258.1.17",
          "form": "센치행도 좋아요",
          "original_form": "센치행도 좋아요",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037258.1.18",
          "form": "크으 진짜 다 명곡이죠ㅠ",
          "original_form": "크으 진짜 다 명곡이죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100037258.1.19",
          "form": "그렇죠 다 좋아요ㅎㅎ 우리나라도 좋은 애니가 마니 나와야는데요 음악도 같이요",
          "original_form": "그렇죠 다 좋아요ㅎㅎ 우리나라도 좋은 애니가 마니 나와야는데요 음악도 같이요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037258.1.20",
          "form": "그러게요ㅠ 그럼 좋겠어요",
          "original_form": "그러게요ㅠ 그럼 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037258.1.21",
          "form": "능력자들 많은데 말예오",
          "original_form": "능력자들 많은데 말예오",
          "speaker_id": "2",
          "time": "20210901 22:05"
        }
      ]
    }
