      "utterance": [
        {
          "id": "MDRW2100013613.1.1",
          "form": "name2님은 가족이랑 사시나요??",
          "original_form": "&name2&님은 가족이랑 사시나요??",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013613.1.2",
          "form": "형이랑 둘이 살고있어요ㅎㅎ",
          "original_form": "형이랑 둘이 살고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013613.1.3",
          "form": "오! 그럼 집안일은 어떻게 분담하세요?",
          "original_form": "오! 그럼 집안일은 어떻게 분담하세요?",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013613.1.4",
          "form": "기냥 제가 하죠...",
          "original_form": "기냥 제가 하죠...",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013613.1.5",
          "form": "근데 빨래는 제가 일부러 안해줘서",
          "original_form": "근데 빨래는 제가 일부러 안해줘서",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013613.1.6",
          "form": "자기가 하더라고요ㅋㅋ",
          "original_form": "자기가 하더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013613.1.7",
          "form": "ㅋㅋㅋㅋㅋ 일부러 안해주시다니ㅋㅋㅋ 친형이신가봐요!",
          "original_form": "ㅋㅋㅋㅋㅋ 일부러 안해주시다니ㅋㅋㅋ 친형이신가봐요!",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013613.1.8",
          "form": "ㅋㅋㅋ넵 친형이에요",
          "original_form": "ㅋㅋㅋ넵 친형이에요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013613.1.9",
          "form": "여튼 매번 해줄순없죠",
          "original_form": "여튼 매번 해줄순없죠",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013613.1.10",
          "form": "맞아요ㅋㅋㅋㅋ 전 세탁기를 제가 안돌려서 어머니가 매번 잔소리를 하세요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ 전 세탁기를 제가 안돌려서 어머니가 매번 잔소리를 하세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013613.1.11",
          "form": "ㅋㅋ다른 사람이 돌리는 세탁기가",
          "original_form": "ㅋㅋ다른 사람이 돌리는 세탁기가",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013613.1.12",
          "form": "더 세탁잘되는데ㅋㅋ",
          "original_form": "더 세탁잘되는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013613.1.13",
          "form": "name1님은 가족이랑 사시죠?",
          "original_form": "&name1&님은 가족이랑 사시죠?",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013613.1.14",
          "form": "ㅋㅋㅋㅋ 네 저는 가족이랑 살고 있어요",
          "original_form": "ㅋㅋㅋㅋ 네 저는 가족이랑 살고 있어요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013613.1.15",
          "form": "오오 역시 가족들과 사는게좋죠ㅎ..",
          "original_form": "오오 역시 가족들과 사는게좋죠ㅎ..",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013613.1.16",
          "form": "형제자매있으신가요",
          "original_form": "형제자매있으신가요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013613.1.17",
          "form": "네ㅋㅋㅋㅋ 전 여동생만 2명 있어요ㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ 전 여동생만 2명 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013613.1.18",
          "form": "오우ㅎㅎ 저는 형만 있어서ㅋㅋ",
          "original_form": "오우ㅎㅎ 저는 형만 있어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100013613.1.19",
          "form": "동생있으면 어떤가요ㅎㅅㅇ",
          "original_form": "동생있으면 어떤가요ㅎㅅㅇ",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100013613.1.20",
          "form": "동생 있으면... 가끔 내가 동생인가? 같은 생각도 해요 워낙 허물없이 대해서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "동생 있으면... 가끔 내가 동생인가? 같은 생각도 해요 워낙 허물없이 대해서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013613.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013613.1.22",
          "form": "나이차가 적은가봐요!",
          "original_form": "나이차가 적은가봐요!",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100013613.1.23",
          "form": "넼ㅋㅋㅋㅋ 2살 차이 나는 동생이랑 맨날 싸워요",
          "original_form": "넼ㅋㅋㅋㅋ 2살 차이 나는 동생이랑 맨날 싸워요",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013613.1.24",
          "form": "ㅋㅋ저는 형이랑 3살차이인데",
          "original_form": "ㅋㅋ저는 형이랑 3살차이인데",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013613.1.25",
          "form": "진짜 안 친하거든요",
          "original_form": "진짜 안 친하거든요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013613.1.26",
          "form": "안친하면 싸우지도않죠...",
          "original_form": "안친하면 싸우지도않죠...",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100013613.1.27",
          "form": "아 그건 맞아요... 형이 많이 무뚝뚝한가봐요...!",
          "original_form": "아 그건 맞아요... 형이 많이 무뚝뚝한가봐요...!",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100013613.1.28",
          "form": "ㅋㅋ서로 집에선 무뚝뚝하죠",
          "original_form": "ㅋㅋ서로 집에선 무뚝뚝하죠",
          "speaker_id": "2",
