      "utterance": [
        {
          "id": "MDRW2100004751.1.1",
          "form": "요즘 구매하고싶은거 있나요?",
          "original_form": "요즘 구매하고싶은거 있나요?",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004751.1.2",
          "form": "가을옷좀 사야되는데 알아보기가 귀찮네요",
          "original_form": "가을옷좀 사야되는데 알아보기가 귀찮네요",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004751.1.3",
          "form": "쇼핑좋아해요?",
          "original_form": "쇼핑좋아해요?",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004751.1.4",
          "form": "쇼핑 좋아하죠! 돈이 따라주지않을뿐ㅠㅜ",
          "original_form": "쇼핑 좋아하죠! 돈이 따라주지않을뿐ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004751.1.5",
          "form": "맞아요 돈이 문제ㅋ",
          "original_form": "맞아요 돈이 문제ㅋ",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004751.1.6",
          "form": "주로 뭐쇼핑하세요?",
          "original_form": "주로 뭐쇼핑하세요?",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004751.1.7",
          "form": "옷이요! 에이블리에서 한철 입을 싼 것들 자주 사용",
          "original_form": "옷이요! 에이블리에서 한철 입을 싼 것들 자주 사용",
          "speaker_id": "1",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004751.1.8",
          "form": "에이블리 많이들 하던데 이벤트가 많은가봐요",
          "original_form": "에이블리 많이들 하던데 이벤트가 많은가봐요",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004751.1.9",
          "form": "할인쿠폰 많이 주고 옷 싸고 조아요!",
          "original_form": "할인쿠폰 많이 주고 옷 싸고 조아요!",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004751.1.10",
          "form": "저도 나중에 가입해봐야겠네요",
          "original_form": "저도 나중에 가입해봐야겠네요",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004751.1.11",
          "form": "나이드니 찾아보는것도 귀찮더라구요",
          "original_form": "나이드니 찾아보는것도 귀찮더라구요",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004751.1.12",
          "form": "저도 가을 옷 사야지 하면서 미룬지 일주일째에요ㅋㅋ",
          "original_form": "저도 가을 옷 사야지 하면서 미룬지 일주일째에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004751.1.13",
          "form": "그러나가 겨울옷 사야될지도^^",
          "original_form": "그러나가 겨울옷 사야될지도^^",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004751.1.14",
          "form": "그럴수도 있을것같아요ㅋㅋㅋㅋㅋ",
          "original_form": "그럴수도 있을것같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004751.1.15",
          "form": "저도 틈틈이 가을옷좀 사놔야겠네요",
          "original_form": "저도 틈틈이 가을옷좀 사놔야겠네요",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004751.1.16",
          "form": "중고거래 해보셨어요?",
          "original_form": "중고거래 해보셨어요?",
          "speaker_id": "2",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004751.1.17",
          "form": "네네 당근마켓에서 2번정도 거래해봤어용",
          "original_form": "네네 당근마켓에서 2번정도 거래해봤어용",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100004751.1.18",
          "form": "저도 몇번 당근거래 해봤어요",
          "original_form": "저도 몇번 당근거래 해봤어요",
