      "utterance": [
        {
          "id": "MDRW2100022201.1.1",
          "form": "넵 김볶 먹었습니다!",
          "original_form": "넵 김볶 먹었습니다!",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022201.1.2",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100022201.1.3",
          "form": "김치볶음밥 맛있겠다 ㅎㅎ",
          "original_form": "김치볶음밥 맛있겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022201.1.4",
          "form": "저는 아직 아침을 먹지 못했어요 ㅜㅜ",
          "original_form": "저는 아직 아침을 먹지 못했어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100022201.1.5",
          "form": "아직요??? 완벽한 브런치네요ㅜㅋㅋ 맛난거 드셔야죠!!",
          "original_form": "아직요??? 완벽한 브런치네요ㅜㅋㅋ 맛난거 드셔야죠!!",
          "speaker_id": "2",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022201.1.6",
          "form": "ㅋㅋㅋ아 점심 때 참치뽈찜 먹으려구 지금 밥하고 있어요",
          "original_form": "ㅋㅋㅋ아 점심 때 참치뽈찜 먹으려구 지금 밥하고 있어요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100022201.1.7",
          "form": "참치뽈찜이요??? 만드시는거예요??",
          "original_form": "참치뽈찜이요??? 만드시는거예요??",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022201.1.8",
          "form": "아뇨~ 동원회사에서 만들어서 팔더라구요",
          "original_form": "아뇨~ 동원회사에서 만들어서 팔더라구요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022201.1.9",
          "form": "그냥 간단히 데워서 먹으면 된대요 ㅎㅎ",
          "original_form": "그냥 간단히 데워서 먹으면 된대요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022201.1.10",
          "form": "아! 그런것도 파는군요! 다음에 도전해봐야겠어요!!",
          "original_form": "아! 그런것도 파는군요! 다음에 도전해봐야겠어요!!",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022201.1.11",
          "form": "네 저 처음 먹어봐서 기대반 걱정반이에요",
          "original_form": "네 저 처음 먹어봐서 기대반 걱정반이에요",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022201.1.12",
          "form": "맛 표시가 되게 독특하더라구요? 수원왕갈비맛?ㅋㅋㅋ",
          "original_form": "맛 표시가 되게 독특하더라구요? 수원왕갈비맛?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022201.1.13",
          "form": "ㅋㅋ 다음에 또 드실 생각이신가요?",
          "original_form": "ㅋㅋ 다음에 또 드실 생각이신가요?",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022201.1.14",
          "form": "아직 안드셧죠??",
          "original_form": "아직 안드셧죠??",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100022201.1.15",
          "form": "아직 안먹었어용",
          "original_form": "아직 안먹었어용",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022201.1.16",
          "form": "한 30분 뒤에 먹어보지 않을까 싶어요",
          "original_form": "한 30분 뒤에 먹어보지 않을까 싶어요",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100022201.1.17",
          "form": "ㅋㅋ후기 기대할게요ㅋㅋ신기하네요",
          "original_form": "ㅋㅋ후기 기대할게요ㅋㅋ신기하네요",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022201.1.18",
          "form": "네! 방금 먹고 왔어요~",
          "original_form": "네! 방금 먹고 왔어요~",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022201.1.19",
          "form": "ㅋㅋㅋ안동찜닭맛인데 맛있네요~",
          "original_form": "ㅋㅋㅋ안동찜닭맛인데 맛있네요~",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022201.1.20",
          "form": "아 그럼 맛나겠네요 국물 낭낭하게 사리넣어도 되겠어요",
          "original_form": "아 그럼 맛나겠네요 국물 낭낭하게 사리넣어도 되겠어요",
