      "utterance": [
        {
          "id": "MDRW2100027603.1.1",
          "form": "룸메이트",
          "original_form": "룸메이트",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100027603.1.2",
          "form": "랑 같이 살면 어떨거같아??",
          "original_form": "랑 같이 살면 어떨거같아??",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100027603.1.3",
          "form": "좋긴 좋겟지만",
          "original_form": "좋긴 좋겟지만",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.4",
          "form": "사생활때문에 불편할것같어...",
          "original_form": "사생활때문에 불편할것같어...",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.5",
          "form": "넌어떤뎅?",
          "original_form": "넌어떤뎅?",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.6",
          "form": "나는 좋을꺼같아",
          "original_form": "나는 좋을꺼같아",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.7",
          "form": "일단 집세도 반만 내면 되구",
          "original_form": "일단 집세도 반만 내면 되구",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.8",
          "form": "혼자 살면 무서울거같아.",
          "original_form": "혼자 살면 무서울거같아.",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.9",
          "form": "일리있군",
          "original_form": "일리있군",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.10",
          "form": "기숙사처럼 처음보는사람이랑 룸메이트해도",
          "original_form": "기숙사처럼 처음보는사람이랑 룸메이트해도",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.11",
          "form": "게다가 식비도 절약되지않을까??",
          "original_form": "게다가 식비도 절약되지않을까??",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.13",
          "form": "좋을것같애?",
          "original_form": "좋을것같애?",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027603.1.14",
          "form": "그건 상대방에 따라",
          "original_form": "그건 상대방에 따라",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.15",
          "form": "하지만 식성이안맞으면?!",
          "original_form": "하지만 식성이안맞으면?!",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.16",
          "form": "달라지지않을까?",
          "original_form": "달라지지않을까?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.17",
          "form": "잘맞는 친구면",
          "original_form": "잘맞는 친구면",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.18",
          "form": "룸메이트 해도 재밌고 좋겠지만",
          "original_form": "룸메이트 해도 재밌고 좋겠지만",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.19",
          "form": "안맞으면 정말 짜증나겟지..ㅎㅎ",
          "original_form": "안맞으면 정말 짜증나겟지..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.20",
          "form": "그니까 룸메이트는 복불복이란거지",
          "original_form": "그니까 룸메이트는 복불복이란거지",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.21",
          "form": "가족이랑 사는게 쩰 낫겟다 ㅎㅎ",
          "original_form": "가족이랑 사는게 쩰 낫겟다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.22",
          "form": "흠.. 그러겡",
          "original_form": "흠.. 그러겡",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100027603.1.23",
          "form": "나두 엄마랑 할무니랑 사는게",
          "original_form": "나두 엄마랑 할무니랑 사는게",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100027603.1.24",
          "form": "제일 좋아",
          "original_form": "제일 좋아",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100027603.1.25",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100027603.1.26",
          "form": "난 언니가 업서",
          "original_form": "난 언니가 업서",
          "speaker_id": "1",
          "time": "20210902 21:03"
        }
      ]
    }
