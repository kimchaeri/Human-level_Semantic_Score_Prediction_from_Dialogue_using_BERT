      "utterance": [
        {
          "id": "MDRW2100024486.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024486.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 11:24"
        },
        {
          "id": "MDRW2100024486.1.3",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100024486.1.4",
          "form": "이미 기혼이네요 ㅎ",
          "original_form": "이미 기혼이네요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100024486.1.5",
          "form": "이상형은 유머코드였어요 ㅎ",
          "original_form": "이상형은 유머코드였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100024486.1.6",
          "form": "유머코드가 잘 맞으셨나봐요",
          "original_form": "유머코드가 잘 맞으셨나봐요",
          "speaker_id": "1",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024486.1.7",
          "form": "살다보면 중요한거같아요",
          "original_form": "살다보면 중요한거같아요",
          "speaker_id": "2",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024486.1.8",
          "form": "대화도 잘통하는것이",
          "original_form": "대화도 잘통하는것이",
          "speaker_id": "2",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024486.1.9",
          "form": "소통이 중요하죠 대화 안통하는 사람은 애초에 친해질 수도 없는걸요ㅋㅋ",
          "original_form": "소통이 중요하죠 대화 안통하는 사람은 애초에 친해질 수도 없는걸요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024486.1.10",
          "form": "거의 통하긴하니 ㅋ",
          "original_form": "거의 통하긴하니 ㅋ",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024486.1.11",
          "form": "거의 통한다는건 혹시 답답한 부분이 있으시다는...?",
          "original_form": "거의 통한다는건 혹시 답답한 부분이 있으시다는...?",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024486.1.12",
          "form": "아뇨 모든사람이 잘통한다는거죠 ㅎ",
          "original_form": "아뇨 모든사람이 잘통한다는거죠 ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100024486.1.13",
          "form": "아하 제가 잘못 알아들었네요",
          "original_form": "아하 제가 잘못 알아들었네요",
          "speaker_id": "1",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024486.1.14",
          "form": "결혼하셨나요? ㅎ",
          "original_form": "결혼하셨나요? ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024486.1.15",
          "form": "아직 사회초년생이이에요ㅠ 20살이요",
          "original_form": "아직 사회초년생이이에요ㅠ 20살이요",
          "speaker_id": "1",
          "time": "20210831 11:44"
        },
        {
          "id": "MDRW2100024486.1.16",
          "form": "팍팍 놀아요 ㅎㅎ 많이 만나보시고",
          "original_form": "팍팍 놀아요 ㅎㅎ 많이 만나보시고",
          "speaker_id": "2",
          "time": "20210831 11:45"
        },
        {
          "id": "MDRW2100024486.1.17",
          "form": "위험한사람만 바로 칼차단하시고",
          "original_form": "위험한사람만 바로 칼차단하시고",
          "speaker_id": "2",
          "time": "20210831 11:45"
        },
        {
          "id": "MDRW2100024486.1.18",
          "form": "코로나라서 제약이 많더라구요",
          "original_form": "코로나라서 제약이 많더라구요",
          "speaker_id": "1",
          "time": "20210831 11:47"
        },
        {
          "id": "MDRW2100024486.1.19",
          "form": "맞아요 ㅜ 해외여행도 꿈일텐데",
          "original_form": "맞아요 ㅜ 해외여행도 꿈일텐데",
          "speaker_id": "2",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100024486.1.20",
          "form": "이전에 갔던 여행이 마지막이 될줄 누가 알았겠어요",
          "original_form": "이전에 갔던 여행이 마지막이 될줄 누가 알았겠어요",
          "speaker_id": "1",
          "time": "20210831 11:49"
        }
      ]
