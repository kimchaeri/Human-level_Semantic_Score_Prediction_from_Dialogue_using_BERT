        },
        {
          "id": "MDRW2100011470.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100011470.1.3",
          "form": "최근에 구매한 물건있으세요",
          "original_form": "최근에 구매한 물건있으세요",
          "speaker_id": "1",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100011470.1.4",
          "form": "저는 최근에 브리타정수기 샀어요",
          "original_form": "저는 최근에 브리타정수기 샀어요",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011470.1.5",
          "form": "어떤거 사셨나요?",
          "original_form": "어떤거 사셨나요?",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100011470.1.6",
          "form": "오 저 브리타 정수기 제일 큰 사이즈 6개월째 쓰고있어요",
          "original_form": "오 저 브리타 정수기 제일 큰 사이즈 6개월째 쓰고있어요",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011470.1.7",
          "form": "제일 큰사이즈면 물병같이 생긴거 아니구",
          "original_form": "제일 큰사이즈면 물병같이 생긴거 아니구",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011470.1.8",
          "form": "정수기처럼 생긴거 맞나요?",
          "original_form": "정수기처럼 생긴거 맞나요?",
          "speaker_id": "2",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011470.1.9",
          "form": "맞아요 파란색 너무 잘 쓰고있어요",
          "original_form": "맞아요 파란색 너무 잘 쓰고있어요",
          "speaker_id": "1",
          "time": "20210824 13:07"
        },
        {
          "id": "MDRW2100011470.1.10",
          "form": "필터 얼마만에 갈아주세요?",
          "original_form": "필터 얼마만에 갈아주세요?",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011470.1.11",
          "form": "수돗물 냄새 살짝 나려고하면 갈아요",
          "original_form": "수돗물 냄새 살짝 나려고하면 갈아요",
          "speaker_id": "1",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011470.1.12",
          "form": "아하! 꼭 정해진 주기대로 갈진않으시는군요",
          "original_form": "아하! 꼭 정해진 주기대로 갈진않으시는군요",
          "speaker_id": "2",
          "time": "20210824 13:12"
        },
        {
          "id": "MDRW2100011470.1.13",
          "form": "권장하는 기간대로 교체하니까 아깝더라고요~",
          "original_form": "권장하는 기간대로 교체하니까 아깝더라고요~",
          "speaker_id": "1",
          "time": "20210824 13:13"
        },
        {
          "id": "MDRW2100011470.1.14",
          "form": "중고 거래 해본적있으세요?",
          "original_form": "중고 거래 해본적있으세요?",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011470.1.15",
          "form": "마자요 보통 권장보다 길게하는거같더라구요",
          "original_form": "마자요 보통 권장보다 길게하는거같더라구요",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011470.1.16",
          "form": "아니요 중고거래는 안해봤어요ㅠ",
          "original_form": "아니요 중고거래는 안해봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011470.1.17",
          "form": "당근거래하는데 재밌네요",
          "original_form": "당근거래하는데 재밌네요",
          "speaker_id": "1",
          "time": "20210824 13:39"
        }
