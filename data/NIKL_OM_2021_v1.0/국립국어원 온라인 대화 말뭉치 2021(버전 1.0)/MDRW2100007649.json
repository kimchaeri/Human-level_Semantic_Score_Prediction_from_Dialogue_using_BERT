      "utterance": [
        {
          "id": "MDRW2100007649.1.1",
          "form": "집안일 중에 뭐가 제일 싫어요?",
          "original_form": "집안일 중에 뭐가 제일 싫어요?",
          "speaker_id": "1",
          "time": "20210822 09:04"
        },
        {
          "id": "MDRW2100007649.1.2",
          "form": "빨래 더는 거랑 바닥 닦는 거요ㅜㅜ",
          "original_form": "빨래 더는 거랑 바닥 닦는 거요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 09:09"
        },
        {
          "id": "MDRW2100007649.1.3",
          "form": "빨래 너는 거",
          "original_form": "빨래 너는 거",
          "speaker_id": "2",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007649.1.4",
          "form": "집안일 좋아하세요?",
          "original_form": "집안일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007649.1.5",
          "form": "아니요... 집안일 너무 귀찮아요",
          "original_form": "아니요... 집안일 너무 귀찮아요",
          "speaker_id": "1",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007649.1.6",
          "form": "맞아요. 집안일 해주는 로봇 빨리 나왔으면 좋겠어요....",
          "original_form": "맞아요. 집안일 해주는 로봇 빨리 나왔으면 좋겠어요....",
          "speaker_id": "2",
          "time": "20210822 09:12"
        },
        {
          "id": "MDRW2100007649.1.7",
          "form": "호호이님은 제일 싫어하는 집안일이 어떤 거예요?",
          "original_form": "호호이님은 제일 싫어하는 집안일이 어떤 거예요?",
          "speaker_id": "2",
          "time": "20210822 09:12"
        },
        {
          "id": "MDRW2100007649.1.8",
          "form": "흠 저도 바닥청소가 제일 싫은 것 같아요",
          "original_form": "흠 저도 바닥청소가 제일 싫은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007649.1.9",
          "form": "청소기가 있으면 그나마 편한데 자취방에는 청소기가 없어요",
          "original_form": "청소기가 있으면 그나마 편한데 자취방에는 청소기가 없어요",
          "speaker_id": "1",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007649.1.10",
          "form": "저도 그래서 저렴한 청소기 하나 샀어요",
          "original_form": "저도 그래서 저렴한 청소기 하나 샀어요",
          "speaker_id": "2",
          "time": "20210822 09:18"
        },
        {
          "id": "MDRW2100007649.1.11",
          "form": "저렴한 것도 있으니까 잘 알아봐서 하나 장만하시는거 추천드려요ㅎㅎ",
          "original_form": "저렴한 것도 있으니까 잘 알아봐서 하나 장만하시는거 추천드려요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007649.1.12",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "1",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007649.1.13",
          "form": "나중에는 누구랑 살고싶으신가요?",
          "original_form": "나중에는 누구랑 살고싶으신가요?",
          "speaker_id": "1",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007649.1.14",
          "form": "당근도 잘 살펴보시면 싸고 좋은거 가끔 올라와요ㅎㅎ",
          "original_form": "당근도 잘 살펴보시면 싸고 좋은거 가끔 올라와요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007649.1.15",
          "form": "저는 잘 모르겠어요",
          "original_form": "저는 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007649.1.16",
          "form": "결혼하게 되면 가족이랑 살고, 결혼 안 하면 혼자 살겠죠?ㅎㅎ",
          "original_form": "결혼하게 되면 가족이랑 살고, 결혼 안 하면 혼자 살겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007649.1.17",
          "form": "저는 결혼 생각이 없어서 혼자 살거나 친구들과 살고 싶어요",
          "original_form": "저는 결혼 생각이 없어서 혼자 살거나 친구들과 살고 싶어요",
          "speaker_id": "1",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100007649.1.18",
          "form": "맞아요 그렇게 살면 재미있을거 같아요ㅎㅎ",
          "original_form": "맞아요 그렇게 살면 재미있을거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:28"
        },
        {
          "id": "MDRW2100007649.1.19",
          "form": "혹시 실례가 안된다면 왜 결혼 생각이 없으신지",
          "original_form": "혹시 실례가 안된다면 왜 결혼 생각이 없으신지",
          "speaker_id": "2",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007649.1.20",
          "form": "여쭤봐도 될까요?",
          "original_form": "여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210822 09:29"
        },
        {
          "id": "MDRW2100007649.1.21",
          "form": "비혼주의자입니다!",
          "original_form": "비혼주의자입니다!",
          "speaker_id": "1",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007649.1.22",
          "form": "딱히 필요성을 느끼지 못해요",
          "original_form": "딱히 필요성을 느끼지 못해요",
          "speaker_id": "1",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007649.1.23",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210822 09:34"
        },
        {
          "id": "MDRW2100007649.1.24",
          "form": "저두 혼자 사는게 너무 편하고 좋아서 다른 사람이랑 같이 사는 제 모습이 잘 상상이 안되기는 해요ㅇㅅㅎ",
          "original_form": "저두 혼자 사는게 너무 편하고 좋아서 다른 사람이랑 같이 사는 제 모습이 잘 상상이 안되기는 해요ㅇㅅㅎ",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007649.1.25",
          "form": "맞아요ㅋㅋ 혼자면 집안일도 혼자해야해서 힘들지만",
          "original_form": "맞아요ㅋㅋ 혼자면 집안일도 혼자해야해서 힘들지만",
          "speaker_id": "1",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007649.1.26",
          "form": "제일 편해요",
          "original_form": "제일 편해요",
          "speaker_id": "1",
          "time": "20210822 09:35"
        }
      ]
    }
  ]
