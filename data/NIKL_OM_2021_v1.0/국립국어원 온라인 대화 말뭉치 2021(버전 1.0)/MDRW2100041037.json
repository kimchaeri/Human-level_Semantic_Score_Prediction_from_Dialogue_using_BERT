      "utterance": [
        {
          "id": "MDRW2100041037.1.1",
          "form": "카페 잘 가는 편이에요?",
          "original_form": "카페 잘 가는 편이에요?",
          "speaker_id": "2",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100041037.1.2",
          "form": "넹 지금은 못가지만요ㅠ",
          "original_form": "넹 지금은 못가지만요ㅠ",
          "speaker_id": "1",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100041037.1.3",
          "form": "ㅠㅠ 그렇죠",
          "original_form": "ㅠㅠ 그렇죠",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100041037.1.4",
          "form": "어딜 자주 가는 편이었어요?",
          "original_form": "어딜 자주 가는 편이었어요?",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100041037.1.5",
          "form": "스타벅스나 탐탐도 가고 개인사업장도 가구요ㅋㅋ",
          "original_form": "스타벅스나 탐탐도 가고 개인사업장도 가구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100041037.1.6",
          "form": "특별히 가리진않아욯ㅎ",
          "original_form": "특별히 가리진않아욯ㅎ",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100041037.1.7",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100041037.1.8",
          "form": "실은 저도 가까운데 가요 ㅋㅋ",
          "original_form": "실은 저도 가까운데 가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100041037.1.9",
          "form": "ㅋㅋ네 덜걷고 내부는 깨끗하고 여름엔 시원하고 겨울엔 따뜻한곳...",
          "original_form": "ㅋㅋ네 덜걷고 내부는 깨끗하고 여름엔 시원하고 겨울엔 따뜻한곳...",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100041037.1.10",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100041037.1.11",
          "form": "커피 좋아해요?",
          "original_form": "커피 좋아해요?",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100041037.1.12",
          "form": "네 오늘도 한잔마셨어요 name2님은요?",
          "original_form": "네 오늘도 한잔마셨어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100041037.1.13",
          "form": "전 커피의 맛을 잘 모르겠어요",
          "original_form": "전 커피의 맛을 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100041037.1.14",
          "form": "향은 좋지만! 맛있는지는 잘 ㅎㅎ",
          "original_form": "향은 좋지만! 맛있는지는 잘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:53"
        },
        {
          "id": "MDRW2100041037.1.15",
          "form": "그러시구나 좀 쓰기도 하고 그렇죠ㅎㅎ",
          "original_form": "그러시구나 좀 쓰기도 하고 그렇죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100041037.1.16",
          "form": "카페가면 무슨차 드세요?",
          "original_form": "카페가면 무슨차 드세요?",
          "speaker_id": "1",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100041037.1.17",
          "form": "전 페퍼민트 차를 주로 마셨어요 향도 시원하고",
          "original_form": "전 페퍼민트 차를 주로 마셨어요 향도 시원하고",
          "speaker_id": "2",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100041037.1.18",
          "form": "좋죠 저도 밤에 커피가 부담스러울때는 민트마셔요ㅎㅎ",
          "original_form": "좋죠 저도 밤에 커피가 부담스러울때는 민트마셔요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100041037.1.19",
          "form": "최근에 말차라떼 먹어봤는데 맛있더라고요..!!",
          "original_form": "최근에 말차라떼 먹어봤는데 맛있더라고요..!!",
          "speaker_id": "2",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100041037.1.20",
          "form": "오 그러시구나 차가운거드셨나요?",
          "original_form": "오 그러시구나 차가운거드셨나요?",
          "speaker_id": "1",
          "time": "20210905 15:58"
        },
        {
          "id": "MDRW2100041037.1.21",
          "form": "네! 말차 덩어리가 아이스크림처럼 올려져있길래 몇스푼 떠먹은 기억이 나네요 ㅋㅋ",
          "original_form": "네! 말차 덩어리가 아이스크림처럼 올려져있길래 몇스푼 떠먹은 기억이 나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100041037.1.22",
          "form": "ㅋㅋ맛있었겠어요",
          "original_form": "ㅋㅋ맛있었겠어요",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041037.1.23",
          "form": "이제날이 추워지니 슬슬 따뜻한차 마실때가 된거같아요",
          "original_form": "이제날이 추워지니 슬슬 따뜻한차 마실때가 된거같아요",
          "speaker_id": "1",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100041037.1.24",
          "form": "좋네요~",
          "original_form": "좋네요~",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100041037.1.25",
          "form": "전 좀 어린입맛이에요 코코아부터 생각나요 ㅋㅋ",
          "original_form": "전 좀 어린입맛이에요 코코아부터 생각나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100041037.1.26",
          "form": "맞아요 코코아도 좋아요ㅎㅎ",
          "original_form": "맞아요 코코아도 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:05"
        }
      ]
    }
