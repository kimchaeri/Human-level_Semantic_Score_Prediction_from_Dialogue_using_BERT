      "utterance": [
        {
          "id": "MDRW2100016362.1.1",
          "form": "공유팁은 없습니다ㅋㅋㅋ",
          "original_form": "공유팁은 없습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016362.1.2",
          "form": "왜냐면 저희 아빠가 다 알아서 타주기때문에..ㅋㅋㅋㅋ",
          "original_form": "왜냐면 저희 아빠가 다 알아서 타주기때문에..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016362.1.3",
          "form": "오홐 ㅋㅋㅋㅋㅋ 잘 말아주시는군여...",
          "original_form": "오홐 ㅋㅋㅋㅋㅋ 잘 말아주시는군여...",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016362.1.4",
          "form": "사이다와 막걸리라면 엄청 톡 쏠거 같은데요 ㅋㅋㅋ",
          "original_form": "사이다와 막걸리라면 엄청 톡 쏠거 같은데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016362.1.5",
          "form": "탄산을 품은 막걸리..?",
          "original_form": "탄산을 품은 막걸리..?",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016362.1.6",
          "form": "엄청 까진 아니고ㅋㅋㅋ",
          "original_form": "엄청 까진 아니고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016362.1.7",
          "form": "달달하니 잘 넘어가요ㅎㅎ",
          "original_form": "달달하니 잘 넘어가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016362.1.8",
          "form": "마지막에 조금 쏘고ㅋㅋ",
          "original_form": "마지막에 조금 쏘고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016362.1.9",
          "form": "오오~~ 뭔가 맛이 상상이 안가욬ㅋㅋㅋ",
          "original_form": "오오~~ 뭔가 맛이 상상이 안가욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016362.1.10",
          "form": "제가 막걸리를 못먹어서 그런가봐요ㅠ",
          "original_form": "제가 막걸리를 못먹어서 그런가봐요ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016362.1.11",
          "form": "드시는 막걸리 브랜드 있으세요?",
          "original_form": "드시는 막걸리 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016362.1.12",
          "form": "전 국순당꺼 먹습니다ㅎㅎ",
          "original_form": "전 국순당꺼 먹습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016362.1.13",
          "form": "저도 자주 먹는건 아니라서ㅋㅋㅋ",
          "original_form": "저도 자주 먹는건 아니라서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016362.1.14",
          "form": "비오는날만 먹죠ㅎㅎㅎ",
          "original_form": "비오는날만 먹죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016362.1.15",
          "form": "국순당 막걸리.. 기억하겠습니닼ㅋㅋㅋㅋ",
          "original_form": "국순당 막걸리.. 기억하겠습니닼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016362.1.16",
          "form": "그쳐 비오는날엔 파전에 막걸리!",
          "original_form": "그쳐 비오는날엔 파전에 막걸리!",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016362.1.17",
          "form": "저는 알쓰라서 그나마 맥주 마셔요 ㅋㅋㅋㅋ",
          "original_form": "저는 알쓰라서 그나마 맥주 마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016362.1.18",
          "form": "근데 저도 알콜 냄새가 싫어서 ㅋㅋㅋㅋ",
          "original_form": "근데 저도 알콜 냄새가 싫어서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016362.1.19",
          "form": "잘은 안먹어요ㅎㅎ",
          "original_form": "잘은 안먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016362.1.20",
          "form": "맥주는 뭐 드세요?",
          "original_form": "맥주는 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016362.1.21",
          "form": "저는 그 초록색깔 이름이 뭐드라요....?",
          "original_form": "저는 그 초록색깔 이름이 뭐드라요....?",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016362.1.22",
          "form": "즐겨먹는편은 아니라서 ㅋㅋㅋ 이름 고새 까먹었어요ㅠ",
          "original_form": "즐겨먹는편은 아니라서 ㅋㅋㅋ 이름 고새 까먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016362.1.23",
          "form": "초록색깔?",
          "original_form": "초록색깔?",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016362.1.24",
          "form": "테라?",
          "original_form": "테라?",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016362.1.25",
          "form": "칭따오?",
          "original_form": "칭따오?",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016362.1.26",
          "form": "하이네켄?",
          "original_form": "하이네켄?",
          "speaker_id": "2",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016362.1.27",
          "form": "아아 테라옄ㅋㅋㅋㅋㅋㅊ",
          "original_form": "아아 테라옄ㅋㅋㅋㅋㅋㅊ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.28",
          "form": "초록색이 한가지가 아니었군요....",
          "original_form": "초록색이 한가지가 아니었군요....",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.29",
          "form": "요즘 수입맥주도",
          "original_form": "요즘 수입맥주도",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.30",
          "form": "많이 들어와서ㅋㅋㅋ",
          "original_form": "많이 들어와서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.31",
          "form": "그 전엔 카스먹었었는데 걍 그랬거든요. 테라는 좀 괜찮더라구요ㅠㅎㅎ",
          "original_form": "그 전엔 카스먹었었는데 걍 그랬거든요. 테라는 좀 괜찮더라구요ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.32",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.33",
          "form": "저는 써머쓰비 많이 먹어요ㅋㅋㅋ",
          "original_form": "저는 써머쓰비 많이 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016362.1.34",
          "form": "저 딤섬집에서 알아할때 칭따오 많이들 찾더라구요",
          "original_form": "저 딤섬집에서 알아할때 칭따오 많이들 찾더라구요",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.35",
          "form": "써머스비....! 술이름인가요?",
          "original_form": "써머스비....! 술이름인가요?",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.36",
          "form": "양꼬치엔 칭따오ㅋㅋㅋ",
          "original_form": "양꼬치엔 칭따오ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.37",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.38",
          "form": "술이름치고 너무 이쁜데여? ㅋㅋㅋ",
          "original_form": "술이름치고 너무 이쁜데여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.39",
          "form": "써머스비도 수입맥주인데",
          "original_form": "써머스비도 수입맥주인데",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.40",
          "form": "막 맛있고 상큼한 맛인가요?",
          "original_form": "막 맛있고 상큼한 맛인가요?",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.41",
          "form": "사과향 나는 맥주에요!!",
          "original_form": "사과향 나는 맥주에요!!",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016362.1.42",
          "form": "왠지 과일향 날것 같은!",
          "original_form": "왠지 과일향 날것 같은!",
          "speaker_id": "1",
          "time": "20210826 16:32"
        }
      ]
