        },
        {
          "id": "MDRW2100007952.1.3",
          "form": "집안일 하시나요",
          "original_form": "집안일 하시나요",
          "speaker_id": "1",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100007952.1.4",
          "form": "네 결혼했어오?",
          "original_form": "네 결혼했어오?",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100007952.1.5",
          "form": "네 ~~ 그쪽은?",
          "original_form": "네 ~~ 그쪽은?",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100007952.1.6",
          "form": "네 애기도있어요",
          "original_form": "네 애기도있어요",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100007952.1.7",
          "form": "집안일 당연히하죠",
          "original_form": "집안일 당연히하죠",
          "speaker_id": "2",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100007952.1.8",
          "form": "와 육아! 저는 이제 가지려고요",
          "original_form": "와 육아! 저는 이제 가지려고요",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100007952.1.9",
          "form": "딸,아들 누구원해요?",
          "original_form": "딸,아들 누구원해요?",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100007952.1.10",
          "form": "둘 다요!ㅋㅋ 하나씩이 목표에요",
          "original_form": "둘 다요!ㅋㅋ 하나씩이 목표에요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100007952.1.11",
          "form": "멋있어요ㅋㅋ 저는 딸하나로 만족해요",
          "original_form": "멋있어요ㅋㅋ 저는 딸하나로 만족해요",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100007952.1.12",
          "form": "아직 목표라서 하나 낳고 힘들어서 포기할지도ㅋㅋ",
          "original_form": "아직 목표라서 하나 낳고 힘들어서 포기할지도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100007952.1.13",
          "form": "월6백벌면 하나정도가 적당하데요ㅎㅎ",
          "original_form": "월6백벌면 하나정도가 적당하데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007952.1.14",
          "form": "아 그럼 하나인데요?ㅋㅋ",
          "original_form": "아 그럼 하나인데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100007952.1.15",
          "form": "그래서 저희도하나ㅋㅋ",
          "original_form": "그래서 저희도하나ㅋㅋ",
          "speaker_id": "2",
