      "utterance": [
        {
          "id": "MDRW2100041835.1.1",
          "form": "학창시절에 무슨 수업 제일 좋아하셨나요?",
          "original_form": "학창시절에 무슨 수업 제일 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100041835.1.2",
          "form": "저는 국어시간이요 ㅋㅋ",
          "original_form": "저는 국어시간이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041835.1.3",
          "form": "오 이유가 뭐예요?",
          "original_form": "오 이유가 뭐예요?",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041835.1.4",
          "form": "ㅋㅋㅋ음 .... 다른 과목보다는 조금 쉬운 느낌이 있더라구요 ㅋ",
          "original_form": "ㅋㅋㅋ음 .... 다른 과목보다는 조금 쉬운 느낌이 있더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041835.1.5",
          "form": "근데 반전은 이과갔었어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 반전은 이과갔었어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041835.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041835.1.7",
          "form": "와 ㅋㅋㅋㅋㅋㅋㅋ진짜 반전이네요",
          "original_form": "와 ㅋㅋㅋㅋㅋㅋㅋ진짜 반전이네요",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041835.1.8",
          "form": "저는 물리 좋아했어요",
          "original_form": "저는 물리 좋아했어요",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041835.1.9",
          "form": "잘하진 못하는데 좋아만하는 ㅋㅋㅋㅋㅋ",
          "original_form": "잘하진 못하는데 좋아만하는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041835.1.10",
          "form": "ㅋㅋㅋㅋ물리 와...어렵던데 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ물리 와...어렵던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041835.1.11",
          "form": "저 고등학교때는 문과, 이과 나눴거든요",
          "original_form": "저 고등학교때는 문과, 이과 나눴거든요",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041835.1.12",
          "form": "고2때 이과로 가서 고3때까지 이과공부하다가 ㅋㅋㅋ 대학교 들어갈때는 사회복지과로 냈어요 ㅋ",
          "original_form": "고2때 이과로 가서 고3때까지 이과공부하다가 ㅋㅋㅋ 대학교 들어갈때는 사회복지과로 냈어요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041835.1.13",
          "form": "문과-이과-문과 ㅋㅋ",
          "original_form": "문과-이과-문과 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041835.1.14",
          "form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041835.1.15",
          "form": "독특한 루트를 밟으셧군요",
          "original_form": "독특한 루트를 밟으셧군요",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100041835.1.16",
          "form": "아 사회복지과 어때요?",
          "original_form": "아 사회복지과 어때요?",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041835.1.17",
          "form": "네 ㅋㅋ 왜 그랬는지 몰라요",
          "original_form": "네 ㅋㅋ 왜 그랬는지 몰라요",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041835.1.18",
          "form": "사회복지과 좋아요 일자리도 많고 ㅋ",
          "original_form": "사회복지과 좋아요 일자리도 많고 ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100041835.1.19",
          "form": "재미잇었어요",
          "original_form": "재미잇었어요",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041835.1.20",
          "form": "근데 ㅋㅋㅋ 일이 힘든건 있어요",
          "original_form": "근데 ㅋㅋㅋ 일이 힘든건 있어요",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100041835.1.21",
          "form": "적성에 잘 맞으셨나봐요",
          "original_form": "적성에 잘 맞으셨나봐요",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100041835.1.22",
          "form": "어떤점이 힘들었어요?",
          "original_form": "어떤점이 힘들었어요?",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100041835.1.23",
          "form": "일자체가 많았어요ㅋ",
          "original_form": "일자체가 많았어요ㅋ",
          "speaker_id": "2",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100041835.1.24",
          "form": "할일이 많군요 ㅠ",
          "original_form": "할일이 많군요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 08:03"
        },
        {
          "id": "MDRW2100041835.1.25",
          "form": "저는 공대다녀서 그쪽계열은 전혀 모르거든요",
