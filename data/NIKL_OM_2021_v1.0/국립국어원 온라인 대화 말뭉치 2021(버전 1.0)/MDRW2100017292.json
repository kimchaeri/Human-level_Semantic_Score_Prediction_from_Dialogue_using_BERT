      "utterance": [
        {
          "id": "MDRW2100017292.1.1",
          "form": "맛집 좋아하시나요",
          "original_form": "맛집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017292.1.2",
          "form": "근처에 있으면요",
          "original_form": "근처에 있으면요",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017292.1.3",
          "form": "어떤메뉴요?",
          "original_form": "어떤메뉴요?",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017292.1.4",
          "form": "근처엔 국밥집이 많아요",
          "original_form": "근처엔 국밥집이 많아요",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017292.1.5",
          "form": "오 국밥 소주!",
          "original_form": "오 국밥 소주!",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017292.1.6",
          "form": "좋져ㅋㅋ 해장하기도 좋구요",
          "original_form": "좋져ㅋㅋ 해장하기도 좋구요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017292.1.7",
          "form": "전 돈까스좋아해요",
          "original_form": "전 돈까스좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100017292.1.8",
          "form": "저두요ㅋㅋ 치즈 돈까스가 좋아요",
          "original_form": "저두요ㅋㅋ 치즈 돈까스가 좋아요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100017292.1.9",
          "form": "연돈 가보셨나용",
          "original_form": "연돈 가보셨나용",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017292.1.10",
          "form": "아뇨 가보고싶긴해요",
          "original_form": "아뇨 가보고싶긴해요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100017292.1.11",
          "form": "저도 엄청먹고싶어요",
          "original_form": "저도 엄청먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100017292.1.12",
          "form": "아직도 찾아가는 사람 많은거 보면 진짜 맛있나봐요",
          "original_form": "아직도 찾아가는 사람 많은거 보면 진짜 맛있나봐요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100017292.1.13",
          "form": "아직도 못먹어서난리래요ㅠ",
          "original_form": "아직도 못먹어서난리래요ㅠ",
