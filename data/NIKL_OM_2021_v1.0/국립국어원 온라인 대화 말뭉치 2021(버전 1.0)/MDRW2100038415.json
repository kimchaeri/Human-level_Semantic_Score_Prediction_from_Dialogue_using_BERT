      "utterance": [
        {
          "id": "MDRW2100038415.1.1",
          "form": "지금은 다들 친해졌는지 재밌어요",
          "original_form": "지금은 다들 친해졌는지 재밌어요",
          "speaker_id": "2",
          "time": "20210902 12:40"
        },
        {
          "id": "MDRW2100038415.1.2",
          "form": "곽민정 선수 왜 이렇게",
          "original_form": "곽민정 선수 왜 이렇게",
          "speaker_id": "1",
          "time": "20210902 12:40"
        },
        {
          "id": "MDRW2100038415.1.3",
          "form": "귀여운지ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "귀여운지ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:40"
        },
        {
          "id": "MDRW2100038415.1.4",
          "form": "허당미",
          "original_form": "허당미",
          "speaker_id": "1",
          "time": "20210902 12:40"
        },
        {
          "id": "MDRW2100038415.1.5",
          "form": "아!!! 저 한유미선수 팬됐어요 노는언니보고",
          "original_form": "아!!! 저 한유미선수 팬됐어요 노는언니보고",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.6",
          "form": "ㅋㅋㅋㅋ전 뭐래도 박세리 선수가 제일웃겨요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ전 뭐래도 박세리 선수가 제일웃겨요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.7",
          "form": "한유미선수 기린 ㅋㅋㅋㅋ",
          "original_form": "한유미선수 기린 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.8",
          "form": "진짜ㅋㅋㅋㅌ허우적거리는거랑",
          "original_form": "진짜ㅋㅋㅋㅌ허우적거리는거랑",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.9",
          "form": "배구빼고 다 허당미 넘치는거ㅜㅜㅜㅜ",
          "original_form": "배구빼고 다 허당미 넘치는거ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.10",
          "form": "은근 개그캐고ㅜㅠ",
          "original_form": "은근 개그캐고ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.11",
          "form": "ㅋㅋㅋ 진짜 말그대로 허우적이에요 ㅋㅋ배구에서는 좀 쎈캐라면서요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 진짜 말그대로 허우적이에요 ㅋㅋ배구에서는 좀 쎈캐라면서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038415.1.12",
          "form": "그래서 올림픽 배구중계",
          "original_form": "그래서 올림픽 배구중계",
          "speaker_id": "1",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100038415.1.13",
          "form": "한유미선수 해설하는 방송으로 봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "한유미선수 해설하는 방송으로 봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100038415.1.14",
          "form": "저도요 ㅋㅋㅋㅋ근데 적응이안됬어요 ㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋ근데 적응이안됬어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100038415.1.15",
          "form": "저언니 좀 프로네?! 이런느낌ㅋㅋㅋㅋ",
          "original_form": "저언니 좀 프로네?! 이런느낌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100038415.1.16",
          "form": "당연히프로인데 ㅋㅋㅋㅋ",
          "original_form": "당연히프로인데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100038415.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ박세리 선수 플렉스 삘 나올때",
          "original_form": "ㅋㅋㅋㅋㅋㅋ박세리 선수 플렉스 삘 나올때",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038415.1.18",
          "form": "너무 멋있고",
          "original_form": "너무 멋있고",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038415.1.19",
          "form": "맞아요 ㅋㅋㅋㅋ 얼마전에 대출상담전화받는거 보셨어요?",
          "original_form": "맞아요 ㅋㅋㅋㅋ 얼마전에 대출상담전화받는거 보셨어요?",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100038415.1.20",
          "form": "헐아뇨!",
          "original_form": "헐아뇨!",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038415.1.21",
          "form": "얼마 빌려드리냐고",
          "original_form": "얼마 빌려드리냐고",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038415.1.22",
          "form": "하려고",
          "original_form": "하려고",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038415.1.23",
          "form": "했었나?!",
          "original_form": "했었나?!",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038415.1.24",
          "form": "ㅋㅋㅋㅋ 막 웃으시던데요 ㅋㅋ 나한테 돈빌려준데! 이러면서 ㅋㅋㅋㅋ 그러니까 옆에 다른 선수들이 언니가 더 싸게 빌려준다해요 이러더라고요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 막 웃으시던데요 ㅋㅋ 나한테 돈빌려준데! 이러면서 ㅋㅋㅋㅋ 그러니까 옆에 다른 선수들이 언니가 더 싸게 빌려준다해요 이러더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100038415.1.25",
          "form": "그런게 진짜",
          "original_form": "그런게 진짜",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100038415.1.26",
          "form": "그냥 일상에서 뿜어나오는",
          "original_form": "그냥 일상에서 뿜어나오는",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100038415.1.27",
          "form": "플렉스 아우라 ㅋㅋㅋㅋㅋ",
          "original_form": "플렉스 아우라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 07:00"
        },
        {
          "id": "MDRW2100038415.1.28",
          "form": "그냥 여유 넘치고 자신감 넘치는 모습이 부러워요ㅠ",
          "original_form": "그냥 여유 넘치고 자신감 넘치는 모습이 부러워요ㅠ",
          "speaker_id": "1",
          "time": "20210903 07:00"
        },
        {
          "id": "MDRW2100038415.1.29",
          "form": "그게 다 돈에서 나오는 어유일까요 ㅋㅋㅋ",
          "original_form": "그게 다 돈에서 나오는 어유일까요 ㅋㅋㅋ",
