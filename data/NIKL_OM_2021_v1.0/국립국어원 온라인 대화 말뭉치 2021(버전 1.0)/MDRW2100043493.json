      "utterance": [
        {
          "id": "MDRW2100043493.1.1",
          "form": "그래 세일 하더라",
          "original_form": "그래 세일 하더라",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043493.1.2",
          "form": "오랜만에 본다 ㅋㅋㅋ",
          "original_form": "오랜만에 본다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043493.1.3",
          "form": "이따가 좀 줄까",
          "original_form": "이따가 좀 줄까",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043493.1.4",
          "form": "웅웅~ 먹을래",
          "original_form": "웅웅~ 먹을래",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043493.1.5",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043493.1.6",
          "form": "어릴땐 징그러워서 싫었는데 넘 맛남 ㅋ",
          "original_form": "어릴땐 징그러워서 싫었는데 넘 맛남 ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043493.1.7",
          "form": "그래 맛있다 너무",
          "original_form": "그래 맛있다 너무",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043493.1.8",
          "form": "ㅋㅋ 간이 아주 적절한게 넘 맛남",
          "original_form": "ㅋㅋ 간이 아주 적절한게 넘 맛남",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043493.1.9",
          "form": "고단백 식품이다",
          "original_form": "고단백 식품이다",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043493.1.10",
          "form": "웅웅ㅋㅋㅋ",
          "original_form": "웅웅ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043493.1.11",
          "form": "지금 줄까",
          "original_form": "지금 줄까",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043493.1.12",
          "form": "웅~ 저녁 먹은김에 같이 먹자",
          "original_form": "웅~ 저녁 먹은김에 같이 먹자",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
