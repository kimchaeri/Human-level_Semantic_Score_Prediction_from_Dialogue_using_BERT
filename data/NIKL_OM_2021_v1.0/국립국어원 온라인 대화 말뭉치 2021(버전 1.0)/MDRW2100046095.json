      "utterance": [
        {
          "id": "MDRW2100046095.1.1",
          "form": "크흐ㅠㅠ 부러워요ㅠㅠ",
          "original_form": "크흐ㅠㅠ 부러워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:18"
        },
        {
          "id": "MDRW2100046095.1.2",
          "form": "여럿이 힘을 모으면 금방 하실거예요",
          "original_form": "여럿이 힘을 모으면 금방 하실거예요",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046095.1.3",
          "form": "저 아직도 무슨 요리인지 못 맞췄어요 ㅡㅋㅋㅋㅋㅋ",
          "original_form": "저 아직도 무슨 요리인지 못 맞췄어요 ㅡㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046095.1.4",
          "form": "얼렁 저도 넘기고 싶네요ㅠㅠ",
          "original_form": "얼렁 저도 넘기고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046095.1.5",
          "form": "어서 정답을 맞춰주세요!",
          "original_form": "어서 정답을 맞춰주세요!",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046095.1.6",
          "form": "어렵지 않은 거에요!",
          "original_form": "어렵지 않은 거에요!",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046095.1.7",
          "form": "노래도 있다구요?!",
          "original_form": "노래도 있다구요?!",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046095.1.8",
          "form": "악 노래라니",
          "original_form": "악 노래라니",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046095.1.9",
          "form": "가사가 있어요!!",
          "original_form": "가사가 있어요!!",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046095.1.10",
          "form": "군데 조쿰 다르지만 그래도 알면 금세 맞추실거애요!!",
          "original_form": "군데 조쿰 다르지만 그래도 알면 금세 맞추실거애요!!",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046095.1.11",
          "form": "계란 들어가고 매운거랬죠?",
          "original_form": "계란 들어가고 매운거랬죠?",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046095.1.12",
          "form": "쌀도 면도 빵도 안 들어간댔죠?",
          "original_form": "쌀도 면도 빵도 안 들어간댔죠?",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046095.1.13",
          "form": "고기랑 먹고...",
          "original_form": "고기랑 먹고...",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046095.1.14",
          "form": "넵!!",
          "original_form": "넵!!",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046095.1.15",
          "form": "맵찔이 기준 맵이요!!",
          "original_form": "맵찔이 기준 맵이요!!",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046095.1.16",
          "form": "악 뭐지 면이 안 들어간다니",
          "original_form": "악 뭐지 면이 안 들어간다니",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046095.1.17",
          "form": "냉면인가 싶었는데",
          "original_form": "냉면인가 싶었는데",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046095.1.18",
          "form": "면이 안들어 간다구 한 적은 없는걸요!!",
          "original_form": "면이 안들어 간다구 한 적은 없는걸요!!",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046095.1.19",
          "form": "아진짜요?????",
          "original_form": "아진짜요?????",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046095.1.20",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046095.1.21",
          "form": "면에 대한 질문은 안나왔옸닥 료!!!",
          "original_form": "면에 대한 질문은 안나왔옸닥 료!!!",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046095.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제가 어딘가 잘못 주워들었나봐요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ제가 어딘가 잘못 주워들었나봐요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046095.1.23",
          "form": "어엌ㅋㅋㅋ",
          "original_form": "어엌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046095.1.24",
          "form": "다른분이랑 헷갈리셨나봐욬ㅋㅋㅋ",
          "original_form": "다른분이랑 헷갈리셨나봐욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046095.1.25",
          "form": "그럴 수 있죠!!",
          "original_form": "그럴 수 있죠!!",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046095.1.26",
          "form": "ㅌㅋㅋㅋㅋㅋ이럴수가",
          "original_form": "ㅌㅋㅋㅋㅋㅋ이럴수가",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046095.1.27",
          "form": "혹시 차가운 음식인가요?",
          "original_form": "혹시 차가운 음식인가요?",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046095.1.28",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046095.1.29",
          "form": "차가운 음식이에요!",
          "original_form": "차가운 음식이에요!",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046095.1.30",
          "form": "헉 정답",
          "original_form": "헉 정답",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046095.1.31",
          "form": "비빔냉면!!!!!!",
          "original_form": "비빔냉면!!!!!!",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046095.1.32",
          "form": "딩동댕!!!!",
          "original_form": "딩동댕!!!!",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046095.1.33",
          "form": "비빔냉면이었어요ㅠㅠ",
          "original_form": "비빔냉면이었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046095.1.34",
          "form": "와 멀리 돌아왔네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 멀리 돌아왔네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046095.1.35",
          "form": "계란 들어가는 게",
          "original_form": "계란 들어가는 게",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046095.1.36",
          "form": "헷갈렸어요 ㅋㅋㅋㅋㅋ답을 알면 당연한건데",
          "original_form": "헷갈렸어요 ㅋㅋㅋㅋㅋ답을 알면 당연한건데",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046095.1.37",
          "form": "맞추니 속이 다 시원해요 갸악",
          "original_form": "맞추니 속이 다 시원해요 갸악",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046095.1.38",
          "form": "그러니까요ㅠㅠ 면만 여쭤보셨어도 금세 맞추셨을탠데ㅠㅠ",
          "original_form": "그러니까요ㅠㅠ 면만 여쭤보셨어도 금세 맞추셨을탠데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046095.1.39",
          "form": "오늘 점심으론 비냉을 맛있게 먹을 예정이랍니다ㅠㅠ",
          "original_form": "오늘 점심으론 비냉을 맛있게 먹을 예정이랍니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046095.1.40",
          "form": "더 추워지기전에 먹어야해요ㅠ",
          "original_form": "더 추워지기전에 먹어야해요ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046095.1.41",
          "form": "ㅋㅌㅌㅌㅌㅋㅋㅋㅋㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅌㅌㅋㅋㅋㅋㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046095.1.42",
          "form": "맞아요 추워서 입 돌아가기 전에 얼른 먹어둬야 해요",
          "original_form": "맞아요 추워서 입 돌아가기 전에 얼른 먹어둬야 해요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046095.1.43",
          "form": "이제 조금 있으면 점심때 되겠네요!",
          "original_form": "이제 조금 있으면 점심때 되겠네요!",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046095.1.44",
          "form": "비냉  맛나게 드세요 얌얌긋",
          "original_form": "비냉  맛나게 드세요 얌얌긋",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046095.1.45",
          "form": "넵넵!!",
          "original_form": "넵넵!!",
          "speaker_id": "2",
          "time": "20210908 10:34"
        }
      ]
