        },
        {
          "id": "MDRW2100025996.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025996.1.4",
          "form": "요즘 건강관리 어떻게 하세요?",
          "original_form": "요즘 건강관리 어떻게 하세요?",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025996.1.5",
          "form": "요즘 헬스장 가기 그래서 집에서 홈트해요!",
          "original_form": "요즘 헬스장 가기 그래서 집에서 홈트해요!",
          "speaker_id": "2",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025996.1.6",
          "form": "으음..홈트하시는군요",
          "original_form": "으음..홈트하시는군요",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025996.1.7",
          "form": "홈트는 주로 어떤걸로 보세요?",
          "original_form": "홈트는 주로 어떤걸로 보세요?",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025996.1.8",
          "form": "필라테스? 헬스?",
          "original_form": "필라테스? 헬스?",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025996.1.9",
          "form": "보면서 하진 않구요 피티 받으면서 배웠던거 기구 사서 하고 있어요",
          "original_form": "보면서 하진 않구요 피티 받으면서 배웠던거 기구 사서 하고 있어요",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025996.1.10",
          "form": "운동 하시는거 있으세요?",
          "original_form": "운동 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025996.1.11",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025996.1.12",
          "form": "저는 운동안해요",
          "original_form": "저는 운동안해요",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100025996.1.13",
          "form": "하고싶으신 운동도 없으신가요?",
          "original_form": "하고싶으신 운동도 없으신가요?",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100025996.1.14",
          "form": "저는.. 에어로빅요",
          "original_form": "저는.. 에어로빅요",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100025996.1.15",
          "form": "와 에어로빅! 에어로빅도 재밌을거같아요",
          "original_form": "와 에어로빅! 에어로빅도 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100025996.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100025996.1.17",
          "form": "아직 안해봤지만",
          "original_form": "아직 안해봤지만",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100025996.1.18",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 08:03"
        },
        {
          "id": "MDRW2100025996.1.19",
          "form": "에어로빅하는거 봤는데 재밌긴하겠더라구요\r\n살도 많이 빠지고",
          "original_form": "에어로빅하는거 봤는데 재밌긴하겠더라구요\r\n살도 많이 빠지고",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100025996.1.20",
          "form": "맞아요 재밌어보이더라고요.",
          "original_form": "맞아요 재밌어보이더라고요.",
