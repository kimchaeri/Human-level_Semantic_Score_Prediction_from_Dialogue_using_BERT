      "utterance": [
        {
          "id": "MDRW2100003004.1.1",
          "form": "치킨잘먹지.. 애들은",
          "original_form": "치킨잘먹지.. 애들은",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003004.1.2",
          "form": "응 그러니깐ㅋㅋ",
          "original_form": "응 그러니깐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003004.1.3",
          "form": "치킨 먹고싶네 ㅋㅋ",
          "original_form": "치킨 먹고싶네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003004.1.4",
          "form": "어른도 싫어하는사람 없을듯ㅋㅋㅋ",
          "original_form": "어른도 싫어하는사람 없을듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003004.1.5",
          "form": "시켜ㅋ기",
          "original_form": "시켜ㅋ기",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003004.1.6",
          "form": "맞아ㅋㅋ",
          "original_form": "맞아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003004.1.7",
          "form": "저녁해먹어야되는데",
          "original_form": "저녁해먹어야되는데",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003004.1.8",
          "form": "어제우리는 네네치킨 두마리ㅋㅋ",
          "original_form": "어제우리는 네네치킨 두마리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003004.1.9",
          "form": "시켜서 남겨놨다 또먹고ㅋㅋㅋㅋ",
          "original_form": "시켜서 남겨놨다 또먹고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003004.1.10",
          "form": "치킨시켜먹을까?ㅋㅋ",
          "original_form": "치킨시켜먹을까?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003004.1.11",
          "form": "그러게ㅋㅋ네네치킨 맛있지ㅋ",
          "original_form": "그러게ㅋㅋ네네치킨 맛있지ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003004.1.12",
          "form": "오우 오늘 저녁은 치킨인가요오오",
          "original_form": "오우 오늘 저녁은 치킨인가요오오",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003004.1.13",
          "form": "치킨입니다~~~ ㅋㅋㅋㅋㅋㅋ 저녁반찬은 뭐야?",
          "original_form": "치킨입니다~~~ ㅋㅋㅋㅋㅋㅋ 저녁반찬은 뭐야?",
          "speaker_id": "2",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003004.1.14",
          "form": "가족들 다 먹으려면ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "가족들 다 먹으려면ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003004.1.15",
          "form": "오징어볶음ㅋㅋ",
          "original_form": "오징어볶음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003004.1.16",
          "form": "소스가없는데 잘될라나?",
          "original_form": "소스가없는데 잘될라나?",
          "speaker_id": "1",
          "time": "20210820 17:31"
