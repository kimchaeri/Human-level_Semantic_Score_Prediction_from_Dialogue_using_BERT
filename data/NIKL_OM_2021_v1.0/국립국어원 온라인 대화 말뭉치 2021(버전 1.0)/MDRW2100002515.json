      "utterance": [
        {
          "id": "MDRW2100002515.1.1",
          "form": "name2님은 여름휴가 가세요?",
          "original_form": "&name2&님은 여름휴가 가세요?",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002515.1.2",
          "form": "음 아니요",
          "original_form": "음 아니요",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002515.1.3",
          "form": "집에 있으려구요",
          "original_form": "집에 있으려구요",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002515.1.4",
          "form": "저두 코로나때문에",
          "original_form": "저두 코로나때문에",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002515.1.5",
          "form": "이번여름은 참으려구요",
          "original_form": "이번여름은 참으려구요",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002515.1.6",
          "form": "코로나진짜 언제없어져",
          "original_form": "코로나진짜 언제없어져",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.7",
          "form": "ㄹㅇㅠㅠㅠ",
          "original_form": "ㄹㅇㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.8",
          "form": "해외여행도 가고싶은데",
          "original_form": "해외여행도 가고싶은데",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.9",
          "form": "여행간지가 언젠지...",
          "original_form": "여행간지가 언젠지...",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.10",
          "form": "갈 수가없으니",
          "original_form": "갈 수가없으니",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.11",
          "form": "마지막 여행지가 어디에요?",
          "original_form": "마지막 여행지가 어디에요?",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.12",
          "form": "제주도요!",
          "original_form": "제주도요!",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.13",
          "form": "가족여행갔었어요ㅎㅎ",
          "original_form": "가족여행갔었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.14",
          "form": "너무좋았겠다",
          "original_form": "너무좋았겠다",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.15",
          "form": "저 제주도 가본적이없어서",
          "original_form": "저 제주도 가본적이없어서",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.16",
          "form": "근데 벌써 2년전이라",
          "original_form": "근데 벌써 2년전이라",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.17",
          "form": "어디가 가장 좋았어요?",
          "original_form": "어디가 가장 좋았어요?",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.18",
          "form": "가물가물해요ㅠㅠ",
          "original_form": "가물가물해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002515.1.19",
          "form": "흠.. 코로나 이놈 빨리 사라져야 다시 갈 수 있을텐데",
          "original_form": "흠.. 코로나 이놈 빨리 사라져야 다시 갈 수 있을텐데",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002515.1.20",
          "form": "해외여행가려면",
          "original_form": "해외여행가려면",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002515.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002515.1.22",
          "form": "오래기다려야할듯해여ㅠㅠ",
          "original_form": "오래기다려야할듯해여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002515.1.23",
          "form": "진짜요.. 백신맞는다고 될 일이 아닌거같아요",
          "original_form": "진짜요.. 백신맞는다고 될 일이 아닌거같아요",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002515.1.24",
          "form": "변이도 자꾸나오고ㅡㅠ",
          "original_form": "변이도 자꾸나오고ㅡㅠ",
          "speaker_id": "2",
          "time": "20210820 13:04"
        }
      ]
    }
