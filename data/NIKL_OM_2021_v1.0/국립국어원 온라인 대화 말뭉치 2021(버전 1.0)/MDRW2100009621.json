      "utterance": [
        {
          "id": "MDRW2100009621.1.1",
          "form": "아무 생각 없어",
          "original_form": "아무 생각 없어",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009621.1.2",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009621.1.3",
          "form": "나는 직장인이 있으면 직장인이 더 내는게 맞다고 생각해",
          "original_form": "나는 직장인이 있으면 직장인이 더 내는게 맞다고 생각해",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009621.1.4",
          "form": "이상형은 뭐야?",
          "original_form": "이상형은 뭐야?",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009621.1.5",
          "form": "그건 맞지",
          "original_form": "그건 맞지",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009621.1.6",
          "form": "다정한 사람",
          "original_form": "다정한 사람",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009621.1.7",
          "form": "나는 친절한 사람이 좋아",
          "original_form": "나는 친절한 사람이 좋아",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009621.1.8",
          "form": "매너 있는 사람도 좋고",
          "original_form": "매너 있는 사람도 좋고",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009621.1.9",
          "form": "외모 이상형 있어?",
          "original_form": "외모 이상형 있어?",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009621.1.10",
          "form": "나는 강동원이야",
          "original_form": "나는 강동원이야",
          "speaker_id": "1",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009621.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009621.1.12",
          "form": "나는 서인국이야",
          "original_form": "나는 서인국이야",
          "speaker_id": "2",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009621.1.13",
          "form": "서인국 잘생겼지",
          "original_form": "서인국 잘생겼지",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009621.1.14",
          "form": "서인국 응답하라에서 넘 멋있었지",
          "original_form": "서인국 응답하라에서 넘 멋있었지",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009621.1.15",
          "form": "응답하라를 안봤네",
          "original_form": "응답하라를 안봤네",
          "speaker_id": "2",
          "time": "20210823 02:13"
        },
        {
          "id": "MDRW2100009621.1.16",
          "form": "응답하라 재미있어?",
          "original_form": "응답하라 재미있어?",
          "speaker_id": "2",
          "time": "20210823 02:13"
        },
        {
          "id": "MDRW2100009621.1.17",
          "form": "정은지랑 되게 잘 어울려 ㅎㅎ",
          "original_form": "정은지랑 되게 잘 어울려 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:14"
        },
        {
          "id": "MDRW2100009621.1.18",
          "form": "응답하라 한번도 안봤어?",
          "original_form": "응답하라 한번도 안봤어?",
          "speaker_id": "1",
          "time": "20210823 02:14"
        },
        {
          "id": "MDRW2100009621.1.19",
          "form": "응 한번도 안봤어",
          "original_form": "응 한번도 안봤어",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009621.1.20",
          "form": "티비를 잘 안봐서",
          "original_form": "티비를 잘 안봐서",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009621.1.21",
          "form": "너는 다 봤어?",
          "original_form": "너는 다 봤어?",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009621.1.22",
          "form": "난 유튜브 클립으로 봤어",
          "original_form": "난 유튜브 클립으로 봤어",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009621.1.23",
          "form": "유튜브로 보는거 좋네",
          "original_form": "유튜브로 보는거 좋네",
          "speaker_id": "2",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009621.1.24",
          "form": "서인국 닮은 사람은 주변에 많은 것 같아",
          "original_form": "서인국 닮은 사람은 주변에 많은 것 같아",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009621.1.25",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "2",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009621.1.26",
          "form": "주변 에 있었으면 좋겠다 ㅎㅎㅎ",
          "original_form": "주변 에 있었으면 좋겠다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009621.1.27",
          "form": "그게 무슨망언이야",
          "original_form": "그게 무슨망언이야",
          "speaker_id": "2",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009621.1.28",
          "form": "국민 썸남 느낌",
          "original_form": "국민 썸남 느낌",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009621.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 02:19"
        }
      ]
    }
  ]
