      "utterance": [
        {
          "id": "MDRW2100041918.1.1",
          "form": "오늘 저녁 메뉴 먼가요?",
          "original_form": "오늘 저녁 메뉴 먼가요?",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041918.1.2",
          "form": "오늘은 오리고기요",
          "original_form": "오늘은 오리고기요",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041918.1.3",
          "form": "버섯양파랑같이구워서",
          "original_form": "버섯양파랑같이구워서",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041918.1.4",
          "form": "오리고기 맛있겠답",
          "original_form": "오리고기 맛있겠답",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041918.1.5",
          "form": "저는 치킨먹을꺼에요",
          "original_form": "저는 치킨먹을꺼에요",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041918.1.6",
          "form": "다이어트중인데아이밥주면서 배고파서 급히먹었어요",
          "original_form": "다이어트중인데아이밥주면서 배고파서 급히먹었어요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041918.1.7",
          "form": "어떤치킨드셔요?",
          "original_form": "어떤치킨드셔요?",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041918.1.8",
          "form": "홈플치킨요",
          "original_form": "홈플치킨요",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041918.1.9",
          "form": "소스는KFC꺼에요",
          "original_form": "소스는KFC꺼에요",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041918.1.10",
          "form": "아~~~맛있겠어요",
          "original_form": "아~~~맛있겠어요",
          "speaker_id": "2",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100041918.1.11",
          "form": "저는어제치킨ㅋㅋㅋ",
          "original_form": "저는어제치킨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041918.1.12",
          "form": "오!!치킨먹었네요",
          "original_form": "오!!치킨먹었네요",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041918.1.13",
          "form": "저는 주말에 KFC햄버거랑",
          "original_form": "저는 주말에 KFC햄버거랑",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041918.1.14",
          "form": "치킨먹었어요",
          "original_form": "치킨먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041918.1.15",
          "form": "치킨을 좋아하시는군요^^",
          "original_form": "치킨을 좋아하시는군요^^",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041918.1.16",
          "form": "일주일에 1치킨은 하는거같아요",
          "original_form": "일주일에 1치킨은 하는거같아요",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041918.1.17",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041918.1.18",
          "form": "1인1닭을좋아해요",
          "original_form": "1인1닭을좋아해요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041918.1.19",
          "form": "배우신분이군요^^",
          "original_form": "배우신분이군요^^",
          "speaker_id": "2",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100041918.1.20",
          "form": "치킨은무조건1인1닭이죠",
          "original_form": "치킨은무조건1인1닭이죠",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041918.1.21",
          "form": "먹방찍는사람부러워요",
          "original_form": "먹방찍는사람부러워요",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041918.1.22",
          "form": "1인1닭^^저는다이어트중이니2인1닭할게여ㅋㅋㅋ",
          "original_form": "1인1닭^^저는다이어트중이니2인1닭할게여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041918.1.23",
          "form": "어떻게요ㅜㅜ",
          "original_form": "어떻게요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100041918.1.24",
          "form": "저는 다이어트 치킨때문에 무너졌어요",
          "original_form": "저는 다이어트 치킨때문에 무너졌어요",
          "speaker_id": "1",
          "time": "20210906 18:46"
        },
        {
          "id": "MDRW2100041918.1.25",
          "form": "치킨땜에 망하죠",
          "original_form": "치킨땜에 망하죠",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100041918.1.26",
          "form": "치맥은 다욧의 적ㅠㅠ",
          "original_form": "치맥은 다욧의 적ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100041918.1.27",
          "form": "그니꺄요",
          "original_form": "그니꺄요",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041918.1.28",
          "form": "저는 지금",
          "original_form": "저는 지금",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100041918.1.29",
          "form": "치킨뜯고있어요",
          "original_form": "치킨뜯고있어요",
