      "utterance": [
        {
          "id": "MDRW2100038369.1.1",
          "form": "그래 하루1시간만 투자해보자구",
          "original_form": "그래 하루1시간만 투자해보자구",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038369.1.2",
          "form": "더이상하면 지친다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "더이상하면 지친다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038369.1.3",
          "form": "얍얍ㅋㅋ홧팅하자ㅋ",
          "original_form": "얍얍ㅋㅋ홧팅하자ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038369.1.4",
          "form": "점심 먹었나??",
          "original_form": "점심 먹었나??",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100038369.1.5",
          "form": "응 열무비빔밥ㅋㅋ",
          "original_form": "응 열무비빔밥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100038369.1.6",
          "form": "언니는 먹었어?ㅋ",
          "original_form": "언니는 먹었어?ㅋ",
          "speaker_id": "1",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100038369.1.7",
          "form": "열무비빔밥 맛있겠네",
          "original_form": "열무비빔밥 맛있겠네",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038369.1.8",
          "form": "나는 치즈 돈까스 먹었당",
          "original_form": "나는 치즈 돈까스 먹었당",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038369.1.9",
          "form": "ㅋㅋㅋ치즈 돈까스 맛있었겠네 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ치즈 돈까스 맛있었겠네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100038369.1.10",
          "form": "오늘 저녁 고민 고민하다가 ㅋㅋㅋㅋ 마트가는김에 그냥 소고기랑 삼겹살사와서 먹으려구 ㅋㅋ",
          "original_form": "오늘 저녁 고민 고민하다가 ㅋㅋㅋㅋ 마트가는김에 그냥 소고기랑 삼겹살사와서 먹으려구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038369.1.11",
          "form": "소고기 땡기네",
          "original_form": "소고기 땡기네",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038369.1.12",
          "form": "먹어보고 맛있으면 얘기해줘",
          "original_form": "먹어보고 맛있으면 얘기해줘",
          "speaker_id": "2",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038369.1.13",
          "form": "난 오늘 저녁은 그냥 곰탕",
          "original_form": "난 오늘 저녁은 그냥 곰탕",
          "speaker_id": "2",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038369.1.14",
          "form": "얍얍 ㅋㅋㅋㅋ곰탕 좋지유 ㅋㅋㅋㅋ",
          "original_form": "얍얍 ㅋㅋㅋㅋ곰탕 좋지유 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038369.1.15",
          "form": "반찬은 무엇?ㅋ",
          "original_form": "반찬은 무엇?ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100038369.1.16",
          "form": "반찬은.... 글쎄다",
          "original_form": "반찬은.... 글쎄다",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038369.1.17",
          "form": "반찬 뭐하지??",
          "original_form": "반찬 뭐하지??",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038369.1.18",
          "form": "또 생선 구울까??",
          "original_form": "또 생선 구울까??",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038369.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ생선 ㅋㅋ너무 자주먹네 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ생선 ㅋㅋ너무 자주먹네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038369.1.20",
          "form": "근데 애들은 생선좋아하니 ㅋㅋ",
          "original_form": "근데 애들은 생선좋아하니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038369.1.21",
          "form": "그렇제?? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇제?? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038369.1.22",
          "form": "어제도 먹었는데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어제도 먹었는데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038369.1.23",
          "form": "그래그래 ㅋㅋㅋ 우리 첫째는 어제 먹은거 꺼내면 뭐라한다 ㅋㅋ",
          "original_form": "그래그래 ㅋㅋㅋ 우리 첫째는 어제 먹은거 꺼내면 뭐라한다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038369.1.24",
          "form": "역시 입맛을 아는군",
          "original_form": "역시 입맛을 아는군",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038369.1.25",
          "form": "맞다 맞다 ㅋㅋ",
          "original_form": "맞다 맞다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100038369.1.26",
          "form": "이제 자유시간도 점점 끝나가네 ㅋ",
          "original_form": "이제 자유시간도 점점 끝나가네 ㅋ",
