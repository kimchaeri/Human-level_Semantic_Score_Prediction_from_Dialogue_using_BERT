      "utterance": [
        {
          "id": "MDRW2100006272.1.1",
          "form": "단편소설 몇 편 정도 썼어요 ㅋㅋㅋ",
          "original_form": "단편소설 몇 편 정도 썼어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006272.1.2",
          "form": "우와 어떤 장르에요?",
          "original_form": "우와 어떤 장르에요?",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006272.1.3",
          "form": "주로 고등학생들 나오는 소설을 많이 쓴 거 같아요",
          "original_form": "주로 고등학생들 나오는 소설을 많이 쓴 거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006272.1.4",
          "form": "딱히 어느 장르를 주로 쓰고 그런건 없네요",
          "original_form": "딱히 어느 장르를 주로 쓰고 그런건 없네요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006272.1.5",
          "form": "오오... 혹시 글을 올리시기도 하나요??",
          "original_form": "오오... 혹시 글을 올리시기도 하나요??",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006272.1.6",
          "form": "앗 사실 글 써서",
          "original_form": "앗 사실 글 써서",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006272.1.7",
          "form": "제 블로그에도 올려놨어요 ㅋㅋㅋ",
          "original_form": "제 블로그에도 올려놨어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006272.1.8",
          "form": "앗 블로그에 올려두셨군요!",
          "original_form": "앗 블로그에 올려두셨군요!",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006272.1.9",
          "form": "작가 데뷔도 혹시 염두해두고 계신가요??",
          "original_form": "작가 데뷔도 혹시 염두해두고 계신가요??",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006272.1.10",
          "form": "아님 이미 작가님이시라거나?",
          "original_form": "아님 이미 작가님이시라거나?",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006272.1.11",
          "form": "저도 될 수 있으면 좋겠어요 ㅋㅋㅋ",
          "original_form": "저도 될 수 있으면 좋겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006272.1.12",
          "form": "아핳 ㅋㅋㅋㅋ",
          "original_form": "아핳 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006272.1.13",
          "form": "공모전 같은 거 가끔 내고 있어요",
          "original_form": "공모전 같은 거 가끔 내고 있어요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006272.1.14",
          "form": "그럼 써두신 작품이 조금 있으시다면",
          "original_form": "그럼 써두신 작품이 조금 있으시다면",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006272.1.15",
          "form": "카카오페이지 스테이지 라는 곳이 최근에 오픈했어요",
          "original_form": "카카오페이지 스테이지 라는 곳이 최근에 오픈했어요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006272.1.16",
          "form": "그쪽에 한번 글을 올려보시는 건 어때요??",
          "original_form": "그쪽에 한번 글을 올려보시는 건 어때요??",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006272.1.17",
          "form": "보니까 좋은 작품은 200만원 지원하고",
          "original_form": "보니까 좋은 작품은 200만원 지원하고",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006272.1.18",
          "form": "정식연재로 모셔간다고 하더라구요",
          "original_form": "정식연재로 모셔간다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006272.1.19",
          "form": "순수 문학쪽을 염두해두시고 계신다면 가볍게 넘어가셔도 돼요 ㅋㅋㅋ",
          "original_form": "순수 문학쪽을 염두해두시고 계신다면 가볍게 넘어가셔도 돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006272.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006272.1.21",
          "form": "제가 할 수 있을지 모르겠네요..",
          "original_form": "제가 할 수 있을지 모르겠네요..",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006272.1.22",
          "form": "아 주로 순수문학에 좀 더 가까운 거 같아요",
          "original_form": "아 주로 순수문학에 좀 더 가까운 거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006272.1.23",
          "form": "아하 그렇군요 분류가 워낙 다양하거든요",
          "original_form": "아하 그렇군요 분류가 워낙 다양하거든요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006272.1.24",
          "form": "에세이도 꽤 올라오고",
          "original_form": "에세이도 꽤 올라오고",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006272.1.25",
          "form": "다양한 종류의 글이 올라오고 있어서",
          "original_form": "다양한 종류의 글이 올라오고 있어서",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006272.1.26",
          "form": "생각나서 여쭤본거에요 :)",
          "original_form": "생각나서 여쭤본거에요 :)",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006272.1.27",
          "form": "작가인증하고 가입 절차도 워낙 간단하기도 하구요 ㅋㅋ",
          "original_form": "작가인증하고 가입 절차도 워낙 간단하기도 하구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006272.1.28",
          "form": "나중에 한 번 참고해 봐야겠네요",
          "original_form": "나중에 한 번 참고해 봐야겠네요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006272.1.29",
          "form": "네 꼭 작가님이 되실 수 있을 거에요",
          "original_form": "네 꼭 작가님이 되실 수 있을 거에요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006272.1.30",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "2",
