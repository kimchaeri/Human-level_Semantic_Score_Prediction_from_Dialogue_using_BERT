      "utterance": [
        {
          "id": "MDRW2100035511.1.1",
          "form": "티비보고 계세요?",
          "original_form": "티비보고 계세요?",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100035511.1.2",
          "form": "저는 지금 무한도전 유튜브로",
          "original_form": "저는 지금 무한도전 유튜브로",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035511.1.3",
          "form": "보고있어요ㅎ",
          "original_form": "보고있어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100035511.1.4",
          "form": "무도ㅠㅜ 오랜만이에요",
          "original_form": "무도ㅠㅜ 오랜만이에요",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035511.1.5",
          "form": "사라진거 아직도 아쉬워요",
          "original_form": "사라진거 아직도 아쉬워요",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100035511.1.6",
          "form": "정말ㅠ 아쉬워요",
          "original_form": "정말ㅠ 아쉬워요",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035511.1.7",
          "form": "유돈노 라인 좋아했는데",
          "original_form": "유돈노 라인 좋아했는데",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100035511.1.8",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100035511.1.9",
          "form": "유노 진짜 최고였어요",
          "original_form": "유노 진짜 최고였어요",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100035511.1.10",
          "form": "전성기죠",
          "original_form": "전성기죠",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100035511.1.11",
          "form": "놀면뭐하니에 무도멤버들 몇은 나온대요",
          "original_form": "놀면뭐하니에 무도멤버들 몇은 나온대요",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100035511.1.12",
          "form": "저번에 하하랑 정준하 나오는거 봤어요!",
          "original_form": "저번에 하하랑 정준하 나오는거 봤어요!",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100035511.1.13",
          "form": "앞으로도 종종 나올거라더라고요!",
          "original_form": "앞으로도 종종 나올거라더라고요!",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035511.1.14",
          "form": "오오! 좋네용",
          "original_form": "오오! 좋네용",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100035511.1.15",
          "form": "한번이라도 다 모이면 좋겠어욥!",
          "original_form": "한번이라도 다 모이면 좋겠어욥!",
          "speaker_id": "1",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100035511.1.16",
          "form": "어 맞아요ㅠ",
          "original_form": "어 맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100035511.1.17",
          "form": "완전체보고싶은마음",
          "original_form": "완전체보고싶은마음",
          "speaker_id": "2",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100035511.1.18",
          "form": "맞아요ㅠㅠ 특별기획이라도 나와주면 좋겠어요",
          "original_form": "맞아요ㅠㅠ 특별기획이라도 나와주면 좋겠어요",
          "speaker_id": "1",
          "time": "20210829 19:55"
        },
        {
          "id": "MDRW2100035511.1.19",
          "form": "무한상사같은거ㅋㅋㄱ해주면",
          "original_form": "무한상사같은거ㅋㅋㄱ해주면",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035511.1.20",
          "form": "너무재밌을것같아요",
          "original_form": "너무재밌을것같아요",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035511.1.21",
          "form": "아 진짜 그거 레전드 중 하나죠ㅋㅋ",
          "original_form": "아 진짜 그거 레전드 중 하나죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:09"
        }
      ]
