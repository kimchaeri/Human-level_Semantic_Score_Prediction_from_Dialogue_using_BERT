      "utterance": [
        {
          "id": "MDRW2100036345.1.1",
          "form": "여행가면기념품뭐가좋아용",
          "original_form": "여행가면기념품뭐가좋아용",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036345.1.2",
          "form": "엄마는 냉장고자석 나라별로 사오시더라구요",
          "original_form": "엄마는 냉장고자석 나라별로 사오시더라구요",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036345.1.3",
          "form": "그거랑 스너우볼도 좋아보여요",
          "original_form": "그거랑 스너우볼도 좋아보여요",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036345.1.4",
          "form": "뭐 사오세요",
          "original_form": "뭐 사오세요",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036345.1.5",
          "form": "저는그저먹을거만사왔는데 자석사오려구요! 자석이좋은거같아용",
          "original_form": "저는그저먹을거만사왔는데 자석사오려구요! 자석이좋은거같아용",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036345.1.6",
          "form": "맞아용 어디가고싶으세요",
          "original_form": "맞아용 어디가고싶으세요",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036345.1.7",
          "form": "대만가구싶어요..제주도도..아직제주도도못가봣다구요..흑흑",
          "original_form": "대만가구싶어요..제주도도..아직제주도도못가봣다구요..흑흑",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036345.1.8",
          "form": "헐 제주도",
          "original_form": "헐 제주도",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036345.1.9",
          "form": "어머니랑 제주도 다녀와요",
          "original_form": "어머니랑 제주도 다녀와요",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036345.1.10",
          "form": "제가가이드할자신이없어요..",
          "original_form": "제가가이드할자신이없어요..",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036345.1.11",
          "form": "헤",
          "original_form": "헤",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036345.1.12",
          "form": "헤 ㅋㅋㅋ 가까운데 라도 다녀와용",
          "original_form": "헤 ㅋㅋㅋ 가까운데 라도 다녀와용",
          "speaker_id": "1",
          "time": "20210831 06:35"
        },
        {
          "id": "MDRW2100036345.1.13",
          "form": "ㅋㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100036345.1.14",
          "form": "아빠삐지진않겟지",
          "original_form": "아빠삐지진않겟지",
