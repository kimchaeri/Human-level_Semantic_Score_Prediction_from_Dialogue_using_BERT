      "utterance": [
        {
          "id": "MDRW2100024452.1.1",
          "form": "갈 곳은 없어도 옷은 사줘야해요",
          "original_form": "갈 곳은 없어도 옷은 사줘야해요",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.2",
          "form": "신발도 사줘야하구",
          "original_form": "신발도 사줘야하구",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.4",
          "form": "그럼 또 가방도 하나 사고",
          "original_form": "그럼 또 가방도 하나 사고",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 이렇게 지갑이 털리는거죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 이렇게 지갑이 털리는거죠",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.7",
          "form": "가방도 요즘 하나 사고 싶어서",
          "original_form": "가방도 요즘 하나 사고 싶어서",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.8",
          "form": "찾아보는데",
          "original_form": "찾아보는데",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.9",
          "form": "맞아요ㅋㅋㅋ 쇼핑의 세계는 무궁무진하다구요",
          "original_form": "맞아요ㅋㅋㅋ 쇼핑의 세계는 무궁무진하다구요",
          "speaker_id": "2",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.10",
          "form": "마땅히 맘에 드는건 없네요ㅠ",
          "original_form": "마땅히 맘에 드는건 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024452.1.11",
          "form": "오오!",
          "original_form": "오오!",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024452.1.12",
          "form": "아ㅜㅠ 아쉽네요ㅠ",
          "original_form": "아ㅜㅠ 아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024452.1.13",
          "form": "저 어제 대화하는분이 마르헨제이 알려주셔서 보는데 꽤 귀여워서 구매욕구가 치솟더라구요",
          "original_form": "저 어제 대화하는분이 마르헨제이 알려주셔서 보는데 꽤 귀여워서 구매욕구가 치솟더라구요",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024452.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024452.1.15",
          "form": "저도 한번 봐야겠어요ㅎㅎ",
          "original_form": "저도 한번 봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024452.1.16",
          "form": "맘에 드시는거 있으심 좋겠어용!",
          "original_form": "맘에 드시는거 있으심 좋겠어용!",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.17",
          "form": "넘귀엽네요!!",
          "original_form": "넘귀엽네요!!",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.18",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.19",
          "form": "name2님은 중고거래해보신적은 있나요?",
          "original_form": "&name2&님은 중고거래해보신적은 있나요?",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.20",
          "form": "당근마켓같은거요!",
          "original_form": "당근마켓같은거요!",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.21",
          "form": "넹! 지금 당근마켓에 올린거 답 오는거 기다리고있어용!",
          "original_form": "넹! 지금 당근마켓에 올린거 답 오는거 기다리고있어용!",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.22",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.23",
          "form": "오오 지금 하고 계시군요!",
          "original_form": "오오 지금 하고 계시군요!",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024452.1.24",
          "form": "저도 집에 버리긴 아까운 새옷...근데 안입는 것들이 있으면",
          "original_form": "저도 집에 버리긴 아까운 새옷...근데 안입는 것들이 있으면",
          "speaker_id": "1",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024452.1.25",
          "form": "바로바로 당근에 올려요ㅎㅎ",
          "original_form": "바로바로 당근에 올려요ㅎㅎ",
