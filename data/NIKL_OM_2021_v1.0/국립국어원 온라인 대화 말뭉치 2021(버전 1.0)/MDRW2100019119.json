      "utterance": [
        {
          "id": "MDRW2100019119.1.1",
          "form": "오 뭐노그게",
          "original_form": "오 뭐노그게",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019119.1.2",
          "form": "ㄱ.랴도 비대면이네",
          "original_form": "ㄱ.랴도 비대면이네",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019119.1.3",
          "form": "뭐고",
          "original_form": "뭐고",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019119.1.4",
          "form": "그래도 비대면 개꿀",
          "original_form": "그래도 비대면 개꿀",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019119.1.5",
          "form": "학교갈일이없어졌다",
          "original_form": "학교갈일이없어졌다",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019119.1.6",
          "form": "이번학기는 ㄹㅇ 매일 집에있을듯",
          "original_form": "이번학기는 ㄹㅇ 매일 집에있을듯",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019119.1.7",
          "form": "ㄹㅇㅋㅋㅋㅋ나도 가상3개라",
          "original_form": "ㄹㅇㅋㅋㅋㅋ나도 가상3개라",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019119.1.8",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019119.1.9",
          "form": "계속 자취함? 학교에서",
          "original_form": "계속 자취함? 학교에서",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019119.1.10",
          "form": "ㄴㄴ방학동안 잠깐",
          "original_form": "ㄴㄴ방학동안 잠깐",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019119.1.11",
          "form": "사는거라",
          "original_form": "사는거라",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019119.1.12",
          "form": "그럼 대청교땜에",
          "original_form": "그럼 대청교땜에",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019119.1.13",
          "form": "경산까지 왔다갔다해야하나 ㅋㅋㅋ",
          "original_form": "경산까지 왔다갔다해야하나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019119.1.14",
          "form": "ㅇㅇ나도 왔다갔다할듯ㄱ",
          "original_form": "ㅇㅇ나도 왔다갔다할듯ㄱ",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019119.1.15",
          "form": "그래도 주18시간이니까",
          "original_form": "그래도 주18시간이니까",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019119.1.16",
          "form": "밥먹을 시간은 주나",
          "original_form": "밥먹을 시간은 주나",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019119.1.17",
          "form": "여기서 줌",
          "original_form": "여기서 줌",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100019119.1.18",
          "form": "ㅋㅋㅋ 괜찮네",
          "original_form": "ㅋㅋㅋ 괜찮네",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019119.1.19",
          "form": "거기 선생님들이랑",
          "original_form": "거기 선생님들이랑",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019119.1.20",
          "form": "같이 일하는건가",
          "original_form": "같이 일하는건가",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019119.1.21",
          "form": "아니면 애들 학습지도만?",
          "original_form": "아니면 애들 학습지도만?",
          "speaker_id": "1",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019119.1.22",
          "form": "사무업무보고",
          "original_form": "사무업무보고",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019119.1.23",
          "form": "애들지도는",
          "original_form": "애들지도는",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019119.1.24",
          "form": "대청교쌤글이나 근로쌤들",
          "original_form": "대청교쌤글이나 근로쌤들",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019119.1.25",
          "form": "애들지도 1대1로 계속 붙어있나",
          "original_form": "애들지도 1대1로 계속 붙어있나",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019119.1.26",
          "form": "어디 앉아서 쉬어도됨?",
          "original_form": "어디 앉아서 쉬어도됨?",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019119.1.27",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019119.1.28",
          "form": "그냥 앉아서",
          "original_form": "그냥 앉아서",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019119.1.29",
          "form": "같이해서",
          "original_form": "같이해서",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019119.1.30",
          "form": "5시간동안 애들옆에?",
          "original_form": "5시간동안 애들옆에?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.31",
          "form": "어차피 애들이 한5시되면가서",
          "original_form": "어차피 애들이 한5시되면가서",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.32",
          "form": "2시간은 그냥 가볍게 청소하고",
          "original_form": "2시간은 그냥 가볍게 청소하고",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.33",
          "form": "쌤들끼리 노가리까고",
          "original_form": "쌤들끼리 노가리까고",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.34",
          "form": "5~6시면 애들감",
          "original_form": "5~6시면 애들감",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.35",
          "form": "오호..",
          "original_form": "오호..",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.36",
          "form": "교외근로 선정되면",
          "original_form": "교외근로 선정되면",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.37",
          "form": "신청만 받는거지",
          "original_form": "신청만 받는거지",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019119.1.38",
          "form": "니가 꽂아줄순없나",
          "original_form": "니가 꽂아줄순없나",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019119.1.39",
          "form": "월욜가서",
          "original_form": "월욜가서",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019119.1.40",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019119.1.41",
          "form": "물어봐야하지",
          "original_form": "물어봐야하지",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019119.1.42",
          "form": "꽂아주기엔",
          "original_form": "꽂아주기엔",
          "speaker_id": "2",
          "time": "20210827 15:47"
        }
      ]
    }
  ]
