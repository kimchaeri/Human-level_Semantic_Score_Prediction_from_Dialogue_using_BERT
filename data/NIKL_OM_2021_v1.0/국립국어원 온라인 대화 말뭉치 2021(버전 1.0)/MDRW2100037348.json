      "utterance": [
        {
          "id": "MDRW2100037348.1.1",
          "form": "반려동물 기르시나요",
          "original_form": "반려동물 기르시나요",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100037348.1.2",
          "form": "아니요 ㅜㅜ 키우세요?",
          "original_form": "아니요 ㅜㅜ 키우세요?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037348.1.3",
          "form": "아니요 그럼 과거에도 경험 없으신가요",
          "original_form": "아니요 그럼 과거에도 경험 없으신가요",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037348.1.4",
          "form": "네네 ! 그냥 햄스터는 키워봤네요 ㅎㅎ",
          "original_form": "네네 ! 그냥 햄스터는 키워봤네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037348.1.5",
          "form": "오옷 몇마리요?",
          "original_form": "오옷 몇마리요?",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037348.1.6",
          "form": "한마리요 !",
          "original_form": "한마리요 !",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037348.1.7",
          "form": "이름도 있었나요?",
          "original_form": "이름도 있었나요?",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037348.1.8",
          "form": "아마 햄찌였을거에욬ㅋㅋ",
          "original_form": "아마 햄찌였을거에욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037348.1.9",
          "form": "ㅋㅋㅋ 귀여운 이름이네용",
          "original_form": "ㅋㅋㅋ 귀여운 이름이네용",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037348.1.10",
          "form": "맞죠 ㅋㅋㅋ",
          "original_form": "맞죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037348.1.11",
          "form": "어릴때 키워서.. 기억도 잘 안나요 ㅋㅋ",
          "original_form": "어릴때 키워서.. 기억도 잘 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037348.1.12",
          "form": "ㅋㅋ 어떻게 키우시게.된걸까요",
          "original_form": "ㅋㅋ 어떻게 키우시게.된걸까요",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037348.1.13",
          "form": "아마 엄마가 들고오셨을꺼에요 ㅋㅋㅋ !",
          "original_form": "아마 엄마가 들고오셨을꺼에요 ㅋㅋㅋ !",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037348.1.14",
          "form": "어머 엄마가 들고오시긴 쉽지않았을텐데..자녀를 향한 찐사랑이었을라나요",
          "original_form": "어머 엄마가 들고오시긴 쉽지않았을텐데..자녀를 향한 찐사랑이었을라나요",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037348.1.15",
          "form": "그러셨나봐요 .. ! 갑자기 감동이네요ㅎㅎㅎ",
          "original_form": "그러셨나봐요 .. ! 갑자기 감동이네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037348.1.16",
          "form": "먹이는 뭘 먹죠? 사료??",
          "original_form": "먹이는 뭘 먹죠? 사료??",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037348.1.17",
          "form": "넹 사료가 따로 있어요 !",
          "original_form": "넹 사료가 따로 있어요 !",
          "speaker_id": "2",
