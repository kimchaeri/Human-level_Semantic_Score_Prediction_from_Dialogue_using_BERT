      "utterance": [
        {
          "id": "MDRW2100038544.1.1",
          "form": "배고프다 ㅋㅋㅋ",
          "original_form": "배고프다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038544.1.2",
          "form": "육개장한사발해야겟어",
          "original_form": "육개장한사발해야겟어",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038544.1.3",
          "form": "적.이.요... 라면말고 뭐 없나요",
          "original_form": "적.이.요... 라면말고 뭐 없나요",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038544.1.4",
          "form": "벌써물부엇쥬",
          "original_form": "벌써물부엇쥬",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038544.1.5",
          "form": "ㅋㅋㅋㅋ밥잇는데 귀찮다 반찬도 꺼내먹고하기",
          "original_form": "ㅋㅋㅋㅋ밥잇는데 귀찮다 반찬도 꺼내먹고하기",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038544.1.6",
          "form": "ㅋㅋ알볼로 혹해서 찾아봤는데 구미에 address점밖에 없네",
          "original_form": "ㅋㅋ알볼로 혹해서 찾아봤는데 구미에 &address&점밖에 없네",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100038544.1.7",
          "form": "육개장 안먹은지 진짜 오래됐다",
          "original_form": "육개장 안먹은지 진짜 오래됐다",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100038544.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100038544.1.9",
          "form": "ㅋㅋㅋ강제이별당함",
          "original_form": "ㅋㅋㅋ강제이별당함",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038544.1.10",
          "form": "면얇은ㄱㅔ최고요",
          "original_form": "면얇은ㄱㅔ최고요",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038544.1.11",
          "form": "나도 address점에서 시켜먹엇지만",
          "original_form": "나도 &address&점에서 시켜먹엇지만",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038544.1.12",
          "form": "캬 그럼 스낵면도 좋아하시나요?",
          "original_form": "캬 그럼 스낵면도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038544.1.13",
          "form": "먹어는봐찌",
          "original_form": "먹어는봐찌",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100038544.1.14",
          "form": "근데 너무애들라면같아 ㅋㅋㅋ",
          "original_form": "근데 너무애들라면같아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100038544.1.15",
          "form": "ㅋㅋㅋ걔가 생라면 먹으면 진짜 맛있다",
          "original_form": "ㅋㅋㅋ걔가 생라면 먹으면 진짜 맛있다",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100038544.1.16",
          "form": "과자야",
          "original_form": "과자야",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100038544.1.17",
          "form": "뿌셔뿌셔느낌",
          "original_form": "뿌셔뿌셔느낌",
