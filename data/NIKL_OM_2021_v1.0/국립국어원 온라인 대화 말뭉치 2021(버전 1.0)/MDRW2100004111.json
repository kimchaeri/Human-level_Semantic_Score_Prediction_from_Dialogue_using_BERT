      "utterance": [
        {
          "id": "MDRW2100004111.1.1",
          "form": "주로 어떤 일 하세요?",
          "original_form": "주로 어떤 일 하세요?",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004111.1.2",
          "form": "아 맞다 다른 일 알아보시나요?",
          "original_form": "아 맞다 다른 일 알아보시나요?",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004111.1.3",
          "form": "알아보고는 있는데 쉽지는 않은 거 같아요",
          "original_form": "알아보고는 있는데 쉽지는 않은 거 같아요",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004111.1.4",
          "form": "코로나로 더 어려워진거같아요",
          "original_form": "코로나로 더 어려워진거같아요",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004111.1.5",
          "form": "그쵸 자리가 영 마땅치 않아서 슬퍼요",
          "original_form": "그쵸 자리가 영 마땅치 않아서 슬퍼요",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004111.1.6",
          "form": "name2님은 직장 다니실 때 가장 힘든점이 있었나요?",
          "original_form": "&name2&님은 직장 다니실 때 가장 힘든점이 있었나요?",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004111.1.7",
          "form": "저는 아무래도 출퇴근이 가장 힘들더라구요",
          "original_form": "저는 아무래도 출퇴근이 가장 힘들더라구요",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004111.1.8",
          "form": "저는 사람들과의 마찰요",
          "original_form": "저는 사람들과의 마찰요",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004111.1.9",
          "form": "솔직히 전 혼자 일하고 싶거든요",
          "original_form": "솔직히 전 혼자 일하고 싶거든요",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004111.1.10",
          "form": "인간관계가 가장 어려운거같아요",
          "original_form": "인간관계가 가장 어려운거같아요",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004111.1.11",
          "form": "몸힘든건 참아도 사람힘든건 답이 없더라구요",
          "original_form": "몸힘든건 참아도 사람힘든건 답이 없더라구요",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004111.1.12",
          "form": "공감돼요 정신적 스트레스가 제일 힘들죠",
          "original_form": "공감돼요 정신적 스트레스가 제일 힘들죠",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004111.1.13",
          "form": "돈 버는 게 쉽지만 않겠지만요",
          "original_form": "돈 버는 게 쉽지만 않겠지만요",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004111.1.14",
          "form": "진짜 저는 예전에 폭언도 들었던 적이 있어서",
          "original_form": "진짜 저는 예전에 폭언도 들었던 적이 있어서",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004111.1.15",
          "form": "이렇게 까지 일을 해야 하나 싶더라구요",
          "original_form": "이렇게 까지 일을 해야 하나 싶더라구요",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004111.1.16",
          "form": "어짜피 일하는 입장인데..",
          "original_form": "어짜피 일하는 입장인데..",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004111.1.17",
          "form": "뭘 그리 갈구나싶더라구요ㅠ",
          "original_form": "뭘 그리 갈구나싶더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004111.1.18",
          "form": "헐 폭언요? 맞아요 막말하는 사람들 꽤 있어요",
          "original_form": "헐 폭언요? 맞아요 막말하는 사람들 꽤 있어요",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004111.1.19",
          "form": "전 신입때 넘 슬펐는데",
          "original_form": "전 신입때 넘 슬펐는데",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004111.1.20",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004111.1.21",
          "form": "짬 차니깐 될때로 돼라식이에요",
          "original_form": "짬 차니깐 될때로 돼라식이에요",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004111.1.22",
          "form": "신입은 못하는데 계속 강요하니깐요",
          "original_form": "신입은 못하는데 계속 강요하니깐요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        }
