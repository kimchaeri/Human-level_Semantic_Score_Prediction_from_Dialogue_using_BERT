      "utterance": [
        {
          "id": "MDRW2100017070.1.1",
          "form": "체리 먹고싶어~",
          "original_form": "체리 먹고싶어~",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017070.1.2",
          "form": "시장에 많이 팔던데",
          "original_form": "시장에 많이 팔던데",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017070.1.3",
          "form": "이따 사러가야겠네",
          "original_form": "이따 사러가야겠네",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017070.1.4",
          "form": "아까 말하지 갔다 왔는데",
          "original_form": "아까 말하지 갔다 왔는데",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017070.1.5",
          "form": "ㅋㅋ괜찮 나도 나가는김에 사오면된다",
          "original_form": "ㅋㅋ괜찮 나도 나가는김에 사오면된다",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100017070.1.6",
          "form": "그래 상품권으로 구매 해라",
          "original_form": "그래 상품권으로 구매 해라",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.7",
          "form": "오키오키 ㅋㅋ 체리 많이 먹으면 몸에 좋다",
          "original_form": "오키오키 ㅋㅋ 체리 많이 먹으면 몸에 좋다",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.8",
          "form": "그래 장도 잘 온대",
          "original_form": "그래 장도 잘 온대",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.9",
          "form": "오 맛도 좋은데 영양도 만점이군",
          "original_form": "오 맛도 좋은데 영양도 만점이군",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.10",
          "form": "그래 많이 먹어야 할 듯",
          "original_form": "그래 많이 먹어야 할 듯",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.11",
          "form": "상큼달큼 맛있엉",
          "original_form": "상큼달큼 맛있엉",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.12",
          "form": "체리 주스도 사 먹어라",
          "original_form": "체리 주스도 사 먹어라",
          "speaker_id": "2",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100017070.1.13",
          "form": "그래야겠당",
          "original_form": "그래야겠당",
