      "utterance": [
        {
          "id": "MDRW2100002451.1.1",
          "form": "너 종교 가지고 있어?",
          "original_form": "너 종교 가지고 있어?",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002451.1.2",
          "form": "예전에 있었는데 지금은 없어",
          "original_form": "예전에 있었는데 지금은 없어",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002451.1.3",
          "form": "그럼 지금 무교겠네?",
          "original_form": "그럼 지금 무교겠네?",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002451.1.4",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002451.1.5",
          "form": "전에는 무슨 종교였어?",
          "original_form": "전에는 무슨 종교였어?",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002451.1.6",
          "form": "나 전에는 교회다녔어",
          "original_form": "나 전에는 교회다녔어",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002451.1.7",
          "form": "너는 종교 가지고 있어?",
          "original_form": "너는 종교 가지고 있어?",
          "speaker_id": "2",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002451.1.8",
          "form": "나도 전에 부모님이 교회다니셔서 같이 다니고 그랬는데 지금은 무교야",
          "original_form": "나도 전에 부모님이 교회다니셔서 같이 다니고 그랬는데 지금은 무교야",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002451.1.9",
          "form": "못믿겠더라~",
          "original_form": "못믿겠더라~",
          "speaker_id": "1",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002451.1.10",
          "form": "맞아 나도 그래서 안다녀",
          "original_form": "맞아 나도 그래서 안다녀",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002451.1.11",
          "form": "난 눈으로 직접 봐야 믿는데 그러지 못하니까",
          "original_form": "난 눈으로 직접 봐야 믿는데 그러지 못하니까",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002451.1.12",
          "form": "코로나때 한번씩 별난일들이 있고 그러니까 더 못믿겠더라",
          "original_form": "코로나때 한번씩 별난일들이 있고 그러니까 더 못믿겠더라",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002451.1.13",
          "form": "맞아 이런 때에는 좀 자제하지",
          "original_form": "맞아 이런 때에는 좀 자제하지",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002451.1.14",
          "form": "왜들 그러는지 모르겠어",
          "original_form": "왜들 그러는지 모르겠어",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002451.1.15",
          "form": "사람 피해나주고",
          "original_form": "사람 피해나주고",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002451.1.16",
          "form": "그러니까 더 못믿는거 아닐까 싶어",
          "original_form": "그러니까 더 못믿는거 아닐까 싶어",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002451.1.17",
          "form": "맞아 더이상 피해 안줬으면 좋겠어",
          "original_form": "맞아 더이상 피해 안줬으면 좋겠어",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002451.1.18",
          "form": "혹시 나중에 종교 같은거 가질 생각있어?",
          "original_form": "혹시 나중에 종교 같은거 가질 생각있어?",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002451.1.19",
          "form": "아마 없을걸",
          "original_form": "아마 없을걸",
          "speaker_id": "2",
          "time": "20210820 12:43"
        }
      ]
    }
