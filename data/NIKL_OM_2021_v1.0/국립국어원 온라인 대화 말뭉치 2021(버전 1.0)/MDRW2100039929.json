      "utterance": [
        {
          "id": "MDRW2100039929.1.1",
          "form": "아님 불어라 미풍아에  나옴",
          "original_form": "아님 불어라 미풍아에  나옴",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039929.1.2",
          "form": "예전꺼네요ㅋ 왜요?",
          "original_form": "예전꺼네요ㅋ 왜요?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039929.1.3",
          "form": "상류사회에서도 주연햇었네",
          "original_form": "상류사회에서도 주연햇었네",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039929.1.4",
          "form": "성준 말하는거죠?",
          "original_form": "성준 말하는거죠?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039929.1.5",
          "form": "아니 임지연",
          "original_form": "아니 임지연",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100039929.1.6",
          "form": "아 임지연ㅋ 주연 좀 했어요",
          "original_form": "아 임지연ㅋ 주연 좀 했어요",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039929.1.7",
          "form": "둘이 같이 상류사회 출연을 햇었네",
          "original_form": "둘이 같이 상류사회 출연을 햇었네",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039929.1.8",
          "form": "그랬구나, 그럼 둘이 친하겠네요",
          "original_form": "그랬구나, 그럼 둘이 친하겠네요",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039929.1.9",
          "form": "백성현도 나옴 오랜만이네 정말 잘 컸는데",
          "original_form": "백성현도 나옴 오랜만이네 정말 잘 컸는데",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039929.1.10",
          "form": "애 있다는 말에 깜놀",
          "original_form": "애 있다는 말에 깜놀",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039929.1.11",
          "form": "아 결혼을 한거야 몰랏다 아직 어려보이는데 동안이구나",
          "original_form": "아 결혼을 한거야 몰랏다 아직 어려보이는데 동안이구나",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039929.1.12",
          "form": "깜찍 결혼했다 발표",
          "original_form": "깜찍 결혼했다 발표",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039929.1.13",
          "form": "혼인신고는 했대요",
          "original_form": "혼인신고는 했대요",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039929.1.14",
          "form": "89년생인데 엄청 빨리 했네",
          "original_form": "89년생인데 엄청 빨리 했네",
