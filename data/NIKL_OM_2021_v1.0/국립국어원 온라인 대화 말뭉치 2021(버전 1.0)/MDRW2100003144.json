      "utterance": [
        {
          "id": "MDRW2100003144.1.1",
          "form": "좋아하는 국내 여행지 어디 있으세요?",
          "original_form": "좋아하는 국내 여행지 어디 있으세요?",
          "speaker_id": "1",
          "time": "20210820 19:27"
        },
        {
          "id": "MDRW2100003144.1.2",
          "form": "저는 부산 좋아해요!",
          "original_form": "저는 부산 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003144.1.3",
          "form": "가보신적 있으세요?",
          "original_form": "가보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003144.1.4",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003144.1.5",
          "form": "부산 너무 가보고 싶어요!",
          "original_form": "부산 너무 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003144.1.6",
          "form": "지금 사는 지역은 바다가 없어서",
          "original_form": "지금 사는 지역은 바다가 없어서",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003144.1.7",
          "form": "부산에 바다보러 가는걸 좋아해요",
          "original_form": "부산에 바다보러 가는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003144.1.8",
          "form": "자주가는 여행가는 도시 있으세요?",
          "original_form": "자주가는 여행가는 도시 있으세요?",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003144.1.9",
          "form": "제주도 재작년에 딱 한 번 가봤는데 너무 좋았어요",
          "original_form": "제주도 재작년에 딱 한 번 가봤는데 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003144.1.10",
          "form": "아무데서나 귤 사먹어도 너무 달고 맛있고",
          "original_form": "아무데서나 귤 사먹어도 너무 달고 맛있고",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003144.1.11",
          "form": "되게 다른 나라 같았어요",
          "original_form": "되게 다른 나라 같았어요",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003144.1.12",
          "form": "제주도 너무 좋죠",
          "original_form": "제주도 너무 좋죠",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003144.1.13",
          "form": "저도 귤을 좋아해서 제주도 좋아해요",
          "original_form": "저도 귤을 좋아해서 제주도 좋아해요",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003144.1.14",
          "form": "근데 여름에 갔었어서 귤따기 체험 못한건 아쉬워요",
          "original_form": "근데 여름에 갔었어서 귤따기 체험 못한건 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003144.1.15",
          "form": "우와 그런것도 있군요",
          "original_form": "우와 그런것도 있군요",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003144.1.16",
          "form": "귤따기는 언제쯤 가야 할 수 있어요?",
          "original_form": "귤따기는 언제쯤 가야 할 수 있어요?",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003144.1.17",
          "form": "아마 겨울에 할 수 있을껄요??",
          "original_form": "아마 겨울에 할 수 있을껄요??",
          "speaker_id": "2",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003144.1.18",
          "form": "그리고 제주도는 맛있는거도 너무 많아요",
          "original_form": "그리고 제주도는 맛있는거도 너무 많아요",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003144.1.19",
          "form": "제주도에서 갈치조림 먹어보셨나요?",
          "original_form": "제주도에서 갈치조림 먹어보셨나요?",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003144.1.20",
          "form": "네! 갈치조림 비주얼부터 맛있었어요!!",
          "original_form": "네! 갈치조림 비주얼부터 맛있었어요!!",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003144.1.21",
          "form": "인터넷에서 제주갈치 주문해서 먹어보면 그 맛이 안 나더라고요ㅠㅠ",
          "original_form": "인터넷에서 제주갈치 주문해서 먹어보면 그 맛이 안 나더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:11"
        },
        {
          "id": "MDRW2100003144.1.22",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003144.1.23",
          "form": "바닷가에서 싱싱한거로 한게 진짜 맛있는거같아요",
          "original_form": "바닷가에서 싱싱한거로 한게 진짜 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003144.1.24",
          "form": "제주도에서만 할 수 있는 건 아니지만 카트 타는 것도 너무 재밋었어요",
          "original_form": "제주도에서만 할 수 있는 건 아니지만 카트 타는 것도 너무 재밋었어요",
          "speaker_id": "1",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003144.1.25",
          "form": "공원같은곳에가면 있는 작은 카트 말씀하시는건가요??",
          "original_form": "공원같은곳에가면 있는 작은 카트 말씀하시는건가요??",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003144.1.26",
          "form": "카트라이더 카트처럼 생긴거예요!",
          "original_form": "카트라이더 카트처럼 생긴거예요!",
          "speaker_id": "1",
          "time": "20210821 16:07"
        }
      ]
    }
  ]
