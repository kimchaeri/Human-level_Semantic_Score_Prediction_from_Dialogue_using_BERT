      "utterance": [
        {
          "id": "MDRW2100012943.1.1",
          "form": "아 그래 육아 해보신적 있어요?",
          "original_form": "아 그래 육아 해보신적 있어요?",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100012943.1.2",
          "form": "육아까진 아니구 애보기",
          "original_form": "육아까진 아니구 애보기",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100012943.1.3",
          "form": "음 초등학생 때요ㅋ",
          "original_form": "음 초등학생 때요ㅋ",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100012943.1.4",
          "form": "사촌동생이 같이 살았었는데",
          "original_form": "사촌동생이 같이 살았었는데",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100012943.1.5",
          "form": "제가 그때 11살이었고 사촌동생이 2살?",
          "original_form": "제가 그때 11살이었고 사촌동생이 2살?",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100012943.1.6",
          "form": "애기가 애기를 봤네요ㅋㅋㅋ",
          "original_form": "애기가 애기를 봤네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100012943.1.7",
          "form": "애보기 전 고딩때 한달정도 친척이랑 살았는데",
          "original_form": "애보기 전 고딩때 한달정도 친척이랑 살았는데",
          "speaker_id": "1",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100012943.1.8",
          "form": "짧게 같이 있었는데 와 진짜 못할짓인것같더라구요",
          "original_form": "짧게 같이 있었는데 와 진짜 못할짓인것같더라구요",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100012943.1.9",
          "form": "다섯살짜린데",
          "original_form": "다섯살짜린데",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100012943.1.10",
          "form": "ㅎ....제가 진짜...아후...",
          "original_form": "ㅎ....제가 진짜...아후...",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100012943.1.11",
          "form": "업어주지않으면 잠을안자요",
          "original_form": "업어주지않으면 잠을안자요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100012943.1.12",
          "form": "울음소리도 와",
          "original_form": "울음소리도 와",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100012943.1.13",
          "form": "끝방에서 끝방까지 들리고",
          "original_form": "끝방에서 끝방까지 들리고",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100012943.1.14",
          "form": "잠시도 안쉬고",
          "original_form": "잠시도 안쉬고",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100012943.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ이래서 육아는 해본 사람이 아는 것...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이래서 육아는 해본 사람이 아는 것...",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100012943.1.16",
          "form": "모르는 사람은 육아가 쉬운줄 알아요...",
          "original_form": "모르는 사람은 육아가 쉬운줄 알아요...",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100012943.1.17",
          "form": "육아때매 결혼하는게 무섭기도 합니다...",
          "original_form": "육아때매 결혼하는게 무섭기도 합니다...",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100012943.1.18",
          "form": "ㅋㅋㅋㅋ결혼하고 지옥이라면 애기낳는 순간부터는 불지옥이라고 그러더라구요...?",
          "original_form": "ㅋㅋㅋㅋ결혼하고 지옥이라면 애기낳는 순간부터는 불지옥이라고 그러더라구요...?",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100012943.1.19",
          "form": "ㅋㅋㅋㅋㅋ잠도 못자고...집안일도 해야하고...",
          "original_form": "ㅋㅋㅋㅋㅋ잠도 못자고...집안일도 해야하고...",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100012943.1.20",
          "form": "애 돌보고...",
          "original_form": "애 돌보고...",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100012943.1.21",
          "form": "밤에 깨서 애 우는거 달래고",
          "original_form": "밤에 깨서 애 우는거 달래고",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100012943.1.22",
          "form": "잠도못자고",
          "original_form": "잠도못자고",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100012943.1.23",
          "form": "밖에 나갈때도 신경쓰이고",
          "original_form": "밖에 나갈때도 신경쓰이고",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100012943.1.24",
          "form": "아프면 걱정되고",
          "original_form": "아프면 걱정되고",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100012943.1.25",
          "form": "우는 것도 왜 우는지 모르겠고ㅋㅋㄲ",
          "original_form": "우는 것도 왜 우는지 모르겠고ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100012943.1.26",
          "form": "우리애는 말이 왜캐 느리지?",
          "original_form": "우리애는 말이 왜캐 느리지?",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100012943.1.27",
          "form": "행동이 좀 더딘거 아닌가?",
          "original_form": "행동이 좀 더딘거 아닌가?",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100012943.1.28",
          "form": "ㅋㅋㅋㅋ나중에 좋은아빠 되겠어여",
          "original_form": "ㅋㅋㅋㅋ나중에 좋은아빠 되겠어여",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100012943.1.29",
          "form": "미리 육아공부를 해야하나...",
          "original_form": "미리 육아공부를 해야하나...",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100012943.1.30",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100012943.1.31",
          "form": "그런 책 있잖아요 임신서부터 출산후 아기들 돌보는데 도움되는 책",
          "original_form": "그런 책 있잖아요 임신서부터 출산후 아기들 돌보는데 도움되는 책",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100012943.1.32",
          "form": "전 결혼하기도 싫고 애낳기도 싫어요ㅜㅜ",
          "original_form": "전 결혼하기도 싫고 애낳기도 싫어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100012943.1.33",
          "form": "생각만해도 무서움...",
          "original_form": "생각만해도 무서움...",
          "speaker_id": "1",
          "time": "20210825 15:37"
        }
      ]
    }
