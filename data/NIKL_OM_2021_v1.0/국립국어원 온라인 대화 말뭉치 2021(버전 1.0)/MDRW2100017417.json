      "utterance": [
        {
          "id": "MDRW2100017417.1.1",
          "form": "안녕하세요 종교 있으세요?",
          "original_form": "안녕하세요 종교 있으세요?",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017417.1.2",
          "form": "아니요 무교에요~",
          "original_form": "아니요 무교에요~",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017417.1.3",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017417.1.4",
          "form": "저는 불교에요!!",
          "original_form": "저는 불교에요!!",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017417.1.5",
          "form": "오 저도 원래는 불교였어요 !",
          "original_form": "오 저도 원래는 불교였어요 !",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017417.1.6",
          "form": "지금은 절에 잘 안가서 ㅎㅎ",
          "original_form": "지금은 절에 잘 안가서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017417.1.7",
          "form": "자주 가시나요?",
          "original_form": "자주 가시나요?",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017417.1.8",
          "form": "와 정말요!!",
          "original_form": "와 정말요!!",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017417.1.9",
          "form": "저도 절에 잘은 안 가요ㅠㅜ 분기마다 한 번씩 가는 거 같아요",
          "original_form": "저도 절에 잘은 안 가요ㅠㅜ 분기마다 한 번씩 가는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017417.1.10",
          "form": "맞죠 ㅎㅎ 저도 몇년에 한번씩 가는거 같네요",
          "original_form": "맞죠 ㅎㅎ 저도 몇년에 한번씩 가는거 같네요",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017417.1.11",
          "form": "절은 가면 뭔가 차분해서 좋지않나요",
          "original_form": "절은 가면 뭔가 차분해서 좋지않나요",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017417.1.12",
          "form": "아 맞아요! 원래 그냥 부모님 따라 갔는데",
          "original_form": "아 맞아요! 원래 그냥 부모님 따라 갔는데",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017417.1.13",
          "form": "그 느낌이 좋아서 먼저 가자고 해요ㅎㅎ",
          "original_form": "그 느낌이 좋아서 먼저 가자고 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017417.1.14",
          "form": "불교 종교 자체는 잘 모르지만...",
          "original_form": "불교 종교 자체는 잘 모르지만...",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017417.1.15",
          "form": "저도 엄마가 절에 다니셔서 어렸을때 많이 갔어요 ㅎㅎ",
          "original_form": "저도 엄마가 절에 다니셔서 어렸을때 많이 갔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017417.1.16",
          "form": "그래서 무교지만 살짝 불교인이에요 ㅎㅎ",
          "original_form": "그래서 무교지만 살짝 불교인이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017417.1.17",
          "form": "저도 비슷해요ㅎㅎ 그런 것 때문에 부모님 따라 종교를 많이 정하는거 같아요",
          "original_form": "저도 비슷해요ㅎㅎ 그런 것 때문에 부모님 따라 종교를 많이 정하는거 같아요",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017417.1.18",
          "form": "맞아요 모태신앙이 많죠 ㅎㅎ",
          "original_form": "맞아요 모태신앙이 많죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017417.1.19",
          "form": "저는 성당도 가봤는데 느낌이 아예 다르더라구요",
          "original_form": "저는 성당도 가봤는데 느낌이 아예 다르더라구요",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017417.1.20",
          "form": "와 저는 성당 교회 한 번도 안 가봤어요",
          "original_form": "와 저는 성당 교회 한 번도 안 가봤어요",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017417.1.21",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017417.1.22",
          "form": "저는 교회는 안가봤지만 성당은 좋았어요 !",
          "original_form": "저는 교회는 안가봤지만 성당은 좋았어요 !",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017417.1.23",
          "form": "뭔가 그 느낌이 달랐어요 ㅎㅎ 약간 차분한건 똑같지만",
          "original_form": "뭔가 그 느낌이 달랐어요 ㅎㅎ 약간 차분한건 똑같지만",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017417.1.24",
          "form": "외국 느낌? 이였어요 ㅎㅎ",
          "original_form": "외국 느낌? 이였어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017417.1.25",
          "form": "약간 영화에서 보는 그 느낌인가요!",
          "original_form": "약간 영화에서 보는 그 느낌인가요!",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017417.1.26",
          "form": "한 번쯤 가보고 싶어요",
          "original_form": "한 번쯤 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017417.1.27",
          "form": "네네 맞아요 ! 딱 영화에 나오는 ㅎㅎ 느낌이에요",
          "original_form": "네네 맞아요 ! 딱 영화에 나오는 ㅎㅎ 느낌이에요",
          "speaker_id": "2",
          "time": "20210826 20:03"
        }
      ]
    }
