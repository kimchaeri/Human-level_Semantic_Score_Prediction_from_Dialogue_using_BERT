      "utterance": [
        {
          "id": "MDRW2100024490.1.1",
          "form": "소울도 재밌던데 보셨나요?",
          "original_form": "소울도 재밌던데 보셨나요?",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024490.1.2",
          "form": "앗 아직 소울은 못봤는데 재밌나보군요!!",
          "original_form": "앗 아직 소울은 못봤는데 재밌나보군요!!",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100024490.1.3",
          "form": "포스터 봤을때 색감 이뻐보였는데 나중에 기회되면 한 번 봐야겠군요..!!",
          "original_form": "포스터 봤을때 색감 이뻐보였는데 나중에 기회되면 한 번 봐야겠군요..!!",
          "speaker_id": "2",
          "time": "20210831 12:06"
        },
        {
          "id": "MDRW2100024490.1.4",
          "form": "정말 따스한 이야기였어요 ㅠㅠ 어른이 봤을때 더 배울게 많은 영화? 그런 느낌이었어요",
          "original_form": "정말 따스한 이야기였어요 ㅠㅠ 어른이 봤을때 더 배울게 많은 영화? 그런 느낌이었어요",
          "speaker_id": "1",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100024490.1.5",
          "form": "오오 보면 마음 따듯해지겠네요..!!",
          "original_form": "오오 보면 마음 따듯해지겠네요..!!",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024490.1.6",
          "form": "자주 들으시는 노래는 있으신가요??",
          "original_form": "자주 들으시는 노래는 있으신가요??",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024490.1.7",
          "form": "음 노래~",
          "original_form": "음 노래~",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100024490.1.8",
          "form": "다양하개 들어요",
          "original_form": "다양하개 들어요",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100024490.1.9",
          "form": "고르자면 아이유 노래 ?",
          "original_form": "고르자면 아이유 노래 ?",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100024490.1.10",
          "form": "아이유 너무 좋죠ㅠ 유튜브에서 플레이리스트로 이따금씩 찾아 듣게되요ㅎㅎ",
          "original_form": "아이유 너무 좋죠ㅠ 유튜브에서 플레이리스트로 이따금씩 찾아 듣게되요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024490.1.11",
          "form": "옛날노래들은 추억돋고 ㅠㅠ",
          "original_form": "옛날노래들은 추억돋고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100024490.1.12",
          "form": "뭐들으새요?",
          "original_form": "뭐들으새요?",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100024490.1.13",
          "form": "전 주로 대중가요 조금듣고 대부분은 애니메이션 오프닝 들어요ㅎㅎ",
          "original_form": "전 주로 대중가요 조금듣고 대부분은 애니메이션 오프닝 들어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100024490.1.14",
          "form": "특유의 벽차오르고 활기찬 느낌이 좋더라구요",
          "original_form": "특유의 벽차오르고 활기찬 느낌이 좋더라구요",
          "speaker_id": "2",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100024490.1.15",
          "form": "아 애니메이션 오프닝!",
          "original_form": "아 애니메이션 오프닝!",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100024490.1.16",
          "form": "저도 좋아햐여 ㅋㅋㅋㅋ",
          "original_form": "저도 좋아햐여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100024490.1.17",
          "form": "어느거 제일 좋아하세요?",
          "original_form": "어느거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100024490.1.18",
          "form": "한가지만 뽑는 게 힘들어서 요즘 듣고있는 걸 말씀드리면 '하이큐!' 노래 자주 듣고있어요!!",
          "original_form": "한가지만 뽑는 게 힘들어서 요즘 듣고있는 걸 말씀드리면 '하이큐!' 노래 자주 듣고있어요!!",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100024490.1.19",
          "form": "어떤 애니 오프닝 좋아하시나요??",
          "original_form": "어떤 애니 오프닝 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100024490.1.20",
          "form": "앗 하이큐 재밌게 봤는데 ㅋㅋㅋㅋ",
          "original_form": "앗 하이큐 재밌게 봤는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100024490.1.21",
          "form": "저는 달빛천사요ㅎㅎ",
          "original_form": "저는 달빛천사요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100024490.1.22",
          "form": "앨범도 샀어요",
          "original_form": "앨범도 샀어요",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100024490.1.23",
          "form": "느낌이 좀 다르긴하지만요 ㅎㅎ",
          "original_form": "느낌이 좀 다르긴하지만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100024490.1.24",
          "form": "오오오 저도 샀어요..!! 펀딩으로..!!ㅎㅎ",
          "original_form": "오오오 저도 샀어요..!! 펀딩으로..!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100024490.1.25",
          "form": "우와 패키지 넘이뻣죠 ㅠㅠ",
          "original_form": "우와 패키지 넘이뻣죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100024490.1.26",
          "form": "네네ㅠ 딱 이쁜 보라색이라서ㅠ",
          "original_form": "네네ㅠ 딱 이쁜 보라색이라서ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100024490.1.27",
          "form": "펀딩한 사람 엄청 많아서 몇달 기다렸는데 기다리는 동안 추억 몽글몽글 했어요ㅎㅎ",
          "original_form": "펀딩한 사람 엄청 많아서 몇달 기다렸는데 기다리는 동안 추억 몽글몽글 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100024490.1.28",
          "form": "맞아요 ㅠㅠㅠ 어렸을때 기억이 새록새록..",
          "original_form": "맞아요 ㅠㅠㅠ 어렸을때 기억이 새록새록..",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100024490.1.29",
          "form": "백투더 그 감성..!!",
          "original_form": "백투더 그 감성..!!",
