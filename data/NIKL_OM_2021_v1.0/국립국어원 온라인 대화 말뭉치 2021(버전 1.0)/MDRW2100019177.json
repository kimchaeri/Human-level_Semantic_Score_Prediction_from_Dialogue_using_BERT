      "utterance": [
        {
          "id": "MDRW2100019177.1.1",
          "form": "여긴 오늘 비온다 너네는?",
          "original_form": "여긴 오늘 비온다 너네는?",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019177.1.2",
          "form": "우리도 비가와",
          "original_form": "우리도 비가와",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019177.1.3",
          "form": "우비 입어봤어?",
          "original_form": "우비 입어봤어?",
          "speaker_id": "1",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019177.1.4",
          "form": "우비는 우산보다 편한 면이 많아",
          "original_form": "우비는 우산보다 편한 면이 많아",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019177.1.5",
          "form": "요즘은 예쁜 우비도 많더라",
          "original_form": "요즘은 예쁜 우비도 많더라",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019177.1.6",
          "form": "요즘은 양산도 많이쓰더라",
          "original_form": "요즘은 양산도 많이쓰더라",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019177.1.7",
          "form": "오 맞아 더울때 양산쓰면 좋을듯",
          "original_form": "오 맞아 더울때 양산쓰면 좋을듯",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019177.1.8",
          "form": "자외선을 차단할수있어서 좋아",
          "original_form": "자외선을 차단할수있어서 좋아",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019177.1.9",
          "form": "우양산 있으면 편리하겠다",
          "original_form": "우양산 있으면 편리하겠다",
          "speaker_id": "1",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100019177.1.10",
          "form": "맞아 그리고 가벼워서 좋아",
          "original_form": "맞아 그리고 가벼워서 좋아",
          "speaker_id": "2",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019177.1.11",
          "form": "요즘 비오다 해 쨍쨍하다가 반복이라서 딱 좋을듯",
          "original_form": "요즘 비오다 해 쨍쨍하다가 반복이라서 딱 좋을듯",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019177.1.12",
          "form": "맞아 나도 그래서 늘 들고다녀",
          "original_form": "맞아 나도 그래서 늘 들고다녀",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100019177.1.13",
          "form": "우양산이 필수템이네",
          "original_form": "우양산이 필수템이네",
          "speaker_id": "1",
          "time": "20210827 16:09"
        }
      ]
