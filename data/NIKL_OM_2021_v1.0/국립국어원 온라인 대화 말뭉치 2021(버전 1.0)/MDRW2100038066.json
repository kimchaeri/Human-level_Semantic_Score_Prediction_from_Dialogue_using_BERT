      "utterance": [
        {
          "id": "MDRW2100038066.1.1",
          "form": "바다가고싶다",
          "original_form": "바다가고싶다",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038066.1.2",
          "form": "어라 통했구나",
          "original_form": "어라 통했구나",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038066.1.3",
          "form": "너 바다 좋아해?",
          "original_form": "너 바다 좋아해?",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038066.1.4",
          "form": "나는 겨울바다를 좋아해",
          "original_form": "나는 겨울바다를 좋아해",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038066.1.5",
          "form": "나는 한때 바다가 있는 도시에 살았어",
          "original_form": "나는 한때 바다가 있는 도시에 살았어",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038066.1.6",
          "form": "겨울바다 좋지.",
          "original_form": "겨울바다 좋지.",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.7",
          "form": "근데 너무 추워",
          "original_form": "근데 너무 추워",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.8",
          "form": "어머 그럼 낭만넘쳤겠다",
          "original_form": "어머 그럼 낭만넘쳤겠다",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.9",
          "form": "그럼 창문열면 바다가 보였니?",
          "original_form": "그럼 창문열면 바다가 보였니?",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.10",
          "form": "콧물 흘리며 본 기억이 있어",
          "original_form": "콧물 흘리며 본 기억이 있어",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.11",
          "form": "아니 학교강의실 창문을 열어야했어",
          "original_form": "아니 학교강의실 창문을 열어야했어",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.12",
          "form": "근데 강의실은 수업들을때만 가니까.",
          "original_form": "근데 강의실은 수업들을때만 가니까.",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.13",
          "form": "바다 배경으로 사진찍고싶다",
          "original_form": "바다 배경으로 사진찍고싶다",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.14",
          "form": "못봤다는 얘기야",
          "original_form": "못봤다는 얘기야",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.15",
          "form": "수업들을때 기분좋았겠다",
          "original_form": "수업들을때 기분좋았겠다",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.16",
          "form": "얘전사진이나 넘겨봐야겠오",
          "original_form": "얘전사진이나 넘겨봐야겠오",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.17",
          "form": "아하 그렇구나 다음에 바다보러 여행가자",
          "original_form": "아하 그렇구나 다음에 바다보러 여행가자",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.18",
          "form": "대형강의실이라 창가자리가 치열했어",
          "original_form": "대형강의실이라 창가자리가 치열했어",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.19",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.20",
          "form": "바다는 언제나 좋아",
          "original_form": "바다는 언제나 좋아",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038066.1.21",
          "form": "영덕바다도 생각보다 예쁘더라",
          "original_form": "영덕바다도 생각보다 예쁘더라",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038066.1.22",
          "form": "코로나가 사라지면 좋겠더",
          "original_form": "코로나가 사라지면 좋겠더",
          "speaker_id": "2",
          "time": "20210901 22:57"
        }
      ]
