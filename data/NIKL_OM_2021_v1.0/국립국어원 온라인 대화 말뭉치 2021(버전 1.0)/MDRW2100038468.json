      "utterance": [
        {
          "id": "MDRW2100038468.1.1",
          "form": "단풍 이쁜곳 어디 없을까요",
          "original_form": "단풍 이쁜곳 어디 없을까요",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038468.1.2",
          "form": "아무래도 등산을 마니하시더라고요 단풍보려면 ㅋㅋㅋ",
          "original_form": "아무래도 등산을 마니하시더라고요 단풍보려면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100038468.1.3",
          "form": "역시 답은 등산인건가요",
          "original_form": "역시 답은 등산인건가요",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100038468.1.4",
          "form": "ㅋㅋㅋㅋ저는 산타는걸 싫어하지만 단풍보는건 좋더라구요",
          "original_form": "ㅋㅋㅋㅋ저는 산타는걸 싫어하지만 단풍보는건 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038468.1.5",
          "form": "낙엽밟기...ㅎㅎ",
          "original_form": "낙엽밟기...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038468.1.6",
          "form": "파사삭 하는 소리가 너무 좋아요",
          "original_form": "파사삭 하는 소리가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038468.1.7",
          "form": "한웅큼 집어서 뿌리는것도 재밌구요",
          "original_form": "한웅큼 집어서 뿌리는것도 재밌구요",
          "speaker_id": "1",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038468.1.8",
          "form": "맞아요 ㅋㅋ 가을 좋아요 보는것도 소리도 ㅋㅋ 색도다양하고",
          "original_form": "맞아요 ㅋㅋ 가을 좋아요 보는것도 소리도 ㅋㅋ 색도다양하고",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100038468.1.9",
          "form": "하늘도 이쁘죠",
          "original_form": "하늘도 이쁘죠",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100038468.1.10",
          "form": "맞아요 하늘도 높고 ㅋㅋㅋ 구름도없이",
          "original_form": "맞아요 하늘도 높고 ㅋㅋㅋ 구름도없이",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038468.1.11",
          "form": "맞아요 왜 다른 날보다 높은지는 모르겠지만",
          "original_form": "맞아요 왜 다른 날보다 높은지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100038468.1.12",
          "form": "그 높은게 너무 좋아요",
          "original_form": "그 높은게 너무 좋아요",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038468.1.13",
          "form": "그러게요 ㅋㅋㅋ 저도 조아요 청명하고",
          "original_form": "그러게요 ㅋㅋㅋ 저도 조아요 청명하고",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038468.1.14",
          "form": "청명하다는 표현 너무 좋아요",
          "original_form": "청명하다는 표현 너무 좋아요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038468.1.15",
          "form": "완벽한 표현 같아요",
          "original_form": "완벽한 표현 같아요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038468.1.16",
          "form": "ㅎㅎ저도 좋아해요",
          "original_form": "ㅎㅎ저도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100038468.1.17",
          "form": "오늘은 그렇게 청명하진않네여 ㅎㅎ ^^;",
          "original_form": "오늘은 그렇게 청명하진않네여 ㅎㅎ ^^;",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100038468.1.18",
          "form": "name1님동네는 어떠신가요 ㅎㅎ",
          "original_form": "&name1&님동네는 어떠신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:26"
        },
        {
          "id": "MDRW2100038468.1.19",
          "form": "여긴 아직 흐려요",
          "original_form": "여긴 아직 흐려요",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100038468.1.20",
          "form": "오그러시구나 코로나 풀리면 가고싶은 곳 있으신가여??",
          "original_form": "오그러시구나 코로나 풀리면 가고싶은 곳 있으신가여??",
