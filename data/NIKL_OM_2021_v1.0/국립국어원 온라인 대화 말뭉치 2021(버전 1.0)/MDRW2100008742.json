      "utterance": [
        {
          "id": "MDRW2100008742.1.1",
          "form": "좋아하는음식 있으신지요",
          "original_form": "좋아하는음식 있으신지요",
          "speaker_id": "1",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008742.1.2",
          "form": "먹는거 다 좋아하는데 한식파예요~",
          "original_form": "먹는거 다 좋아하는데 한식파예요~",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008742.1.3",
          "form": "오 저랑 비슷하시네요",
          "original_form": "오 저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008742.1.4",
          "form": "저는 배고플때 먹는게 젤좋지만",
          "original_form": "저는 배고플때 먹는게 젤좋지만",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008742.1.5",
          "form": "탄수화물중독인지 밥이좋아요",
          "original_form": "탄수화물중독인지 밥이좋아요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008742.1.6",
          "form": "밥이 좋아요 ㅋㅋ",
          "original_form": "밥이 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008742.1.7",
          "form": "저는 간된장을 점심으로 먹었는데",
          "original_form": "저는 간된장을 점심으로 먹었는데",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008742.1.8",
          "form": "저녁은아직 못정했어요",
          "original_form": "저녁은아직 못정했어요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008742.1.9",
          "form": "저도 아직인데...",
          "original_form": "저도 아직인데...",
          "speaker_id": "2",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008742.1.10",
          "form": "뭐 먹을까요.",
          "original_form": "뭐 먹을까요.",
          "speaker_id": "2",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008742.1.11",
          "form": "고민이 되네요 빵줏어먹어서 ㅋㅋ 한식을 원해요",
          "original_form": "고민이 되네요 빵줏어먹어서 ㅋㅋ 한식을 원해요",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008742.1.12",
          "form": "간단히 김치찌개를 먹울까 해요",
          "original_form": "간단히 김치찌개를 먹울까 해요",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008742.1.13",
          "form": "아하 그거 괜찮네요",
          "original_form": "아하 그거 괜찮네요",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008742.1.14",
          "form": "저는 이마트에서 명란이 원쁠원하길래 사왔는데",
          "original_form": "저는 이마트에서 명란이 원쁠원하길래 사왔는데",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008742.1.15",
          "form": "구워먹으려구요",
          "original_form": "구워먹으려구요",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008742.1.16",
          "form": "오~ 맛있겠어요^^",
          "original_form": "오~ 맛있겠어요^^",
          "speaker_id": "2",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008742.1.17",
          "form": "네 명란 구이 좋아하는데 늘 구경만하다가 이벤트 해서 냉큼 주워왔어요",
          "original_form": "네 명란 구이 좋아하는데 늘 구경만하다가 이벤트 해서 냉큼 주워왔어요",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008742.1.18",
          "form": "오이 얇게 썬거랑 같이 먹으면 맛있어요",
          "original_form": "오이 얇게 썬거랑 같이 먹으면 맛있어요",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008742.1.19",
          "form": "오~ 요리실력의 내공이 느껴지네요^^",
          "original_form": "오~ 요리실력의 내공이 느껴지네요^^",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008742.1.20",
          "form": "앗 아닙니다",
          "original_form": "앗 아닙니다",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008742.1.21",
          "form": "저는 똥손이라 ㅋㅋㅋ",
          "original_form": "저는 똥손이라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008742.1.22",
          "form": "요린이 ㅋㅋㅋ",
          "original_form": "요린이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008742.1.23",
          "form": "전혀아닙니당 일본요리집에",
          "original_form": "전혀아닙니당 일본요리집에",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008742.1.24",
          "form": "갔다가 먹어본거",
          "original_form": "갔다가 먹어본거",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008742.1.25",
          "form": "따라하는거에요ㅋㅋ",
          "original_form": "따라하는거에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008742.1.26",
          "form": "굽고 썰고 ㅋ",
          "original_form": "굽고 썰고 ㅋ",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008742.1.27",
          "form": "미식가 느낌입니다~",
          "original_form": "미식가 느낌입니다~",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008742.1.28",
          "form": "달님도 하실수있습니다",
          "original_form": "달님도 하실수있습니다",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008742.1.29",
          "form": "만개의 레시피 어플이 있지만... 잠들어 있어요",
          "original_form": "만개의 레시피 어플이 있지만... 잠들어 있어요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008742.1.30",
          "form": "아...잠들어..",
          "original_form": "아...잠들어..",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008742.1.31",
          "form": "저도 마찬가지네요..",
          "original_form": "저도 마찬가지네요..",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008742.1.32",
          "form": "요즘엔 배달음식과 좀 친해졌네요",
          "original_form": "요즘엔 배달음식과 좀 친해졌네요",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008742.1.33",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008742.1.34",
          "form": "그게 속편하지요",
          "original_form": "그게 속편하지요",
          "speaker_id": "1",
          "time": "20210822 18:21"
        }
      ]
    }
