      "utterance": [
        {
          "id": "MDRW2100029817.1.1",
          "form": "콜라보 상품 있으신가요",
          "original_form": "콜라보 상품 있으신가요",
          "speaker_id": "1",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100029817.1.2",
          "form": "갖고있는거요?",
          "original_form": "갖고있는거요?",
          "speaker_id": "2",
          "time": "20210903 14:22"
        },
        {
          "id": "MDRW2100029817.1.3",
          "form": "꼬뜨게랑 가방있어요 널디 돼지바 양말두요",
          "original_form": "꼬뜨게랑 가방있어요 널디 돼지바 양말두요",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029817.1.4",
          "form": "꼬뜨게랑이래서",
          "original_form": "꼬뜨게랑이래서",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100029817.1.5",
          "form": "뭔가 했네요ㅋㅋ",
          "original_form": "뭔가 했네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100029817.1.6",
          "form": "ㅋㅋ 꽃게랑",
          "original_form": "ㅋㅋ 꽃게랑",
          "speaker_id": "2",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100029817.1.7",
          "form": "ㅋㅋ저는 뭐 딱히 산거는 없고",
          "original_form": "ㅋㅋ저는 뭐 딱히 산거는 없고",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029817.1.8",
          "form": "병맥주 병따개?",
          "original_form": "병맥주 병따개?",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029817.1.9",
          "form": "사은품이요? ㅋㅋ 귀엽겠어요",
          "original_form": "사은품이요? ㅋㅋ 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029817.1.10",
          "form": "쪼꼬매서",
          "original_form": "쪼꼬매서",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029817.1.11",
          "form": "아 카카오 인형 콜라보한거 샀네요",
          "original_form": "아 카카오 인형 콜라보한거 샀네요",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029817.1.12",
          "form": "카카오 콜라보 뭐요? 노티드?",
          "original_form": "카카오 콜라보 뭐요? 노티드?",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029817.1.13",
          "form": "아뇨 코레일이랑",
          "original_form": "아뇨 코레일이랑",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100029817.1.14",
          "form": "라이언역장님이라고",
          "original_form": "라이언역장님이라고",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100029817.1.15",
          "form": "치시면 나옵니다",
          "original_form": "치시면 나옵니다",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100029817.1.16",
          "form": "아까 뉴스 보니까. Ktx랑 콜라보한",
          "original_form": "아까 뉴스 보니까. Ktx랑 콜라보한",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100029817.1.17",
          "form": "호두과자 라이언도 있던데",
          "original_form": "호두과자 라이언도 있던데",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100029817.1.18",
