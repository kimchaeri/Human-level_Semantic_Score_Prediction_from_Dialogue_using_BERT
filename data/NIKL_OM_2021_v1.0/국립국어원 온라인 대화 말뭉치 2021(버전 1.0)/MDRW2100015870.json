      "utterance": [
        {
          "id": "MDRW2100015870.1.1",
          "form": "드라이기 사고싶어요",
          "original_form": "드라이기 사고싶어요",
          "speaker_id": "2",
          "time": "20210826 02:08"
        },
        {
          "id": "MDRW2100015870.1.2",
          "form": "드라이기는 항공모터죠!",
          "original_form": "드라이기는 항공모터죠!",
          "speaker_id": "1",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015870.1.3",
          "form": "써보셨어요?!",
          "original_form": "써보셨어요?!",
          "speaker_id": "2",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015870.1.4",
          "form": "제친구가 소리만 크고 별로래서 좌절 \r\nㅜㅜ",
          "original_form": "제친구가 소리만 크고 별로래서 좌절 \r\nㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015870.1.5",
          "form": "머리가 빨리 말라요",
          "original_form": "머리가 빨리 말라요",
          "speaker_id": "1",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015870.1.6",
          "form": "진짜요?! 그런걸 찾고있었어욬ㅋㅋ",
          "original_form": "진짜요?! 그런걸 찾고있었어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:12"
        },
        {
          "id": "MDRW2100015870.1.7",
          "form": "항공모터 역시",
          "original_form": "항공모터 역시",
          "speaker_id": "2",
          "time": "20210826 02:13"
        },
        {
          "id": "MDRW2100015870.1.8",
          "form": "미용실에서 쓰는건다 항공모터예요ㅋㅋ",
          "original_form": "미용실에서 쓰는건다 항공모터예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:13"
        },
        {
          "id": "MDRW2100015870.1.9",
          "form": "ㅋㅋ미용실에서 쓰는건 다 좋아보여요",
          "original_form": "ㅋㅋ미용실에서 쓰는건 다 좋아보여요",
          "speaker_id": "2",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015870.1.10",
          "form": "고데기 따라서 샀다가 기계가 아니라 손이 문데인걸 깨달았됴..",
          "original_form": "고데기 따라서 샀다가 기계가 아니라 손이 문데인걸 깨달았됴..",
          "speaker_id": "2",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015870.1.11",
          "form": "저는 노트북사고싶네요",
          "original_form": "저는 노트북사고싶네요",
          "speaker_id": "1",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015870.1.12",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015870.1.13",
          "form": "전 그램샀어요!",
          "original_form": "전 그램샀어요!",
          "speaker_id": "2",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015870.1.14",
          "form": "엇 저도 19년식 14인치 쓰고있는데",
          "original_form": "엇 저도 19년식 14인치 쓰고있는데",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015870.1.15",
          "form": "17인치로 바꾸고싶어요ㅎ",
          "original_form": "17인치로 바꾸고싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015870.1.16",
          "form": "저는 패드ㅠㅠㅠㅠ아이패드",
          "original_form": "저는 패드ㅠㅠㅠㅠ아이패드",
