      "utterance": [
        {
          "id": "MDRW2100000496.1.1",
          "form": "혹시 반려동물 키우세요?",
          "original_form": "혹시 반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000496.1.2",
          "form": "아니요ㅠ 본가에서는 강아지 키우는데\r\n저는 혼자 살고있어서 못키워요",
          "original_form": "아니요ㅠ 본가에서는 강아지 키우는데\r\n저는 혼자 살고있어서 못키워요",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000496.1.3",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000496.1.4",
          "form": "우와 본가에 강아지는 몇살인가요?",
          "original_form": "우와 본가에 강아지는 몇살인가요?",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000496.1.5",
          "form": "아뇨 ㅠㅜ 저는 키우고싶은데",
          "original_form": "아뇨 ㅠㅜ 저는 키우고싶은데",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000496.1.6",
          "form": "비용이 감당 안될 것 같아서ㅠㅠ",
          "original_form": "비용이 감당 안될 것 같아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000496.1.7",
          "form": "지금 4살정도 됐어요",
          "original_form": "지금 4살정도 됐어요",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000496.1.8",
          "form": "마자요 키우려면 돈이 생각보다 많이들더라구요",
          "original_form": "마자요 키우려면 돈이 생각보다 많이들더라구요",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000496.1.9",
          "form": "강아지 키우고 싶으신거에요?",
          "original_form": "강아지 키우고 싶으신거에요?",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000496.1.10",
          "form": "아뇨 저는 고양이요!",
          "original_form": "아뇨 저는 고양이요!",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000496.1.11",
          "form": "저도 고양이도 한번 키워보고싶어요",
          "original_form": "저도 고양이도 한번 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000496.1.12",
          "form": "고양이 유투버도 엄청 많잖아요!",
          "original_form": "고양이 유투버도 엄청 많잖아요!",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000496.1.13",
          "form": "혹시 보는 반려동물 유투버 있으세요?",
          "original_form": "혹시 보는 반려동물 유투버 있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000496.1.14",
          "form": "아니요 따로 챙겨보는건없는데",
          "original_form": "아니요 따로 챙겨보는건없는데",
          "speaker_id": "2",
          "time": "20210819 16:51"
        },
        {
          "id": "MDRW2100000496.1.15",
          "form": "가끔 강아지나 고양이 나오는 유튜브있으면  보게 되더라구요",
          "original_form": "가끔 강아지나 고양이 나오는 유튜브있으면  보게 되더라구요",
          "speaker_id": "2",
          "time": "20210819 16:51"
        },
        {
          "id": "MDRW2100000496.1.16",
          "form": "보기만해도 힐링 되는거같아요",
          "original_form": "보기만해도 힐링 되는거같아요",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000496.1.17",
          "form": "그쵸 작은 동물들은 보는거로도",
          "original_form": "그쵸 작은 동물들은 보는거로도",
          "speaker_id": "1",
          "time": "20210819 16:53"
        },
        {
          "id": "MDRW2100000496.1.18",
          "form": "너무 힐링돼요",
          "original_form": "너무 힐링돼요",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000496.1.19",
          "form": "강아지는 산책시키기 안힘든가요??",
          "original_form": "강아지는 산책시키기 안힘든가요??",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000496.1.20",
          "form": "마자요 한번 보면 힐링돼서 계속 보게돼요",
          "original_form": "마자요 한번 보면 힐링돼서 계속 보게돼요",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000496.1.21",
          "form": "산책힘들죠ㅠ 한번나오면  잘 안들어가려고하더라구요",
          "original_form": "산책힘들죠ㅠ 한번나오면  잘 안들어가려고하더라구요",
          "speaker_id": "2",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000496.1.22",
          "form": "가끔 길에서 그런 강아지들 보는데",
          "original_form": "가끔 길에서 그런 강아지들 보는데",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000496.1.23",
          "form": "너무 귀여워요 ㅎㅎ",
          "original_form": "너무 귀여워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000496.1.24",
          "form": "주인분들은 힘들겠지만...",
          "original_form": "주인분들은 힘들겠지만...",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000496.1.25",
          "form": "ㅋㅋㅋㅋ너무 오래하면 힘들긴한데\r\n산책나오면 좋아하니까 나와야죠ㅠ",
          "original_form": "ㅋㅋㅋㅋ너무 오래하면 힘들긴한데\r\n산책나오면 좋아하니까 나와야죠ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000496.1.26",
          "form": "ㅋㅋㅋㅋㅋ맞아요 강아지한테 최고의 행복인 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 강아지한테 최고의 행복인 것 같아요",
          "speaker_id": "1",
          "time": "20210819 17:06"
        }
      ]
