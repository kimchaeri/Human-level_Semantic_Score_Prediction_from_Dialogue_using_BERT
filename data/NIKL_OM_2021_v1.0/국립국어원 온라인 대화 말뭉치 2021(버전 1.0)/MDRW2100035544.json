        {
          "id": "MDRW2100035544.1.1",
          "form": "티비 보고 계시나요?",
          "original_form": "티비 보고 계시나요?",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100035544.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100035544.1.3",
          "form": "티비보고계세요?",
          "original_form": "티비보고계세요?",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100035544.1.4",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100035544.1.5",
          "form": "광자매 보고있어요",
          "original_form": "광자매 보고있어요",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100035544.1.6",
          "form": "오 광자매!!",
          "original_form": "오 광자매!!",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035544.1.7",
          "form": "재밌나요?!",
          "original_form": "재밌나요?!",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035544.1.8",
          "form": "광자매에 출연하는 어떤배우분을 좋아하세요?",
          "original_form": "광자매에 출연하는 어떤배우분을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100035544.1.9",
          "form": "환장하겠어요ㅋㅋㅋ",
          "original_form": "환장하겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035544.1.10",
          "form": "좋아지지않네요..ㅋㅋㅋ",
          "original_form": "좋아지지않네요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035544.1.11",
          "form": "처음입니다",
          "original_form": "처음입니다",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035544.1.12",
          "form": "ㅋㄱㅋㅋㅋㅋㄱㄱ아 노답 막장드라만가요?",
          "original_form": "ㅋㄱㅋㅋㅋㅋㄱㄱ아 노답 막장드라만가요?",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100035544.1.13",
          "form": "네ㅋㅋㅋㄲ",
          "original_form": "네ㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035544.1.14",
          "form": "이런 내용 아님 안되나 싶을정도에요ㅋㅋㅋㅋ",
          "original_form": "이런 내용 아님 안되나 싶을정도에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035544.1.15",
          "form": "주말드라마인데 말이죠ㅋㅋㅋㅋ",
          "original_form": "주말드라마인데 말이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035544.1.16",
          "form": "ㅋㄱㅋㅋㅋㄱㄱㅋㅋㄱㄱㄱㄱ주말드라마는 훈훈한 가족드라마여야 하는데ㅠ",
          "original_form": "ㅋㄱㅋㅋㅋㄱㄱㅋㅋㄱㄱㄱㄱ주말드라마는 훈훈한 가족드라마여야 하는데ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035544.1.17",
          "form": "그니까용ㅠ 내가 나이 먹어서 못따라가나 싶었어요ㅋㅋㄲㅋ",
          "original_form": "그니까용ㅠ 내가 나이 먹어서 못따라가나 싶었어요ㅋㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100035544.1.18",
          "form": "ㅋㅋㄱㅋ아니에요 그럴리없어요",
          "original_form": "ㅋㅋㄱㅋ아니에요 그럴리없어요",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035544.1.19",
          "form": "요즘너무자극적",
          "original_form": "요즘너무자극적",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035544.1.20",
          "form": "그쵸?!",
          "original_form": "그쵸?!",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035544.1.21",
          "form": "너무 자극적이라 입벌리고 보다 이제 웃으며 봐요ㅋㅋㅋ",
          "original_form": "너무 자극적이라 입벌리고 보다 이제 웃으며 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100035544.1.22",
          "form": "ㅋㄱㄱㅋㄱㅋㄱㅋㅋㅋㄱㅋㄱㄱㅋ갈수록 막장이 흥미를 끌죠",
          "original_form": "ㅋㄱㄱㅋㄱㅋㄱㅋㅋㅋㄱㅋㄱㄱㅋ갈수록 막장이 흥미를 끌죠",
