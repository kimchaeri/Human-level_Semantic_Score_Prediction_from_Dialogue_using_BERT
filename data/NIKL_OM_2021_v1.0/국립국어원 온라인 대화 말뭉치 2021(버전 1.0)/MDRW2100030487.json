      "utterance": [
        {
          "id": "MDRW2100030487.1.1",
          "form": "제가 일본 캐릭터들을 엄청 좋아해서",
          "original_form": "제가 일본 캐릭터들을 엄청 좋아해서",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030487.1.2",
          "form": "사실 진짜 가고는 싶은데...",
          "original_form": "사실 진짜 가고는 싶은데...",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030487.1.3",
          "form": "과거에 일본을 가신적이 있으신건가요?",
          "original_form": "과거에 일본을 가신적이 있으신건가요?",
          "speaker_id": "2",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030487.1.4",
          "form": "저희 엄마가 일본 다녀오시고 암이 재발됐거든요 ㅠㅠ 원인이 그게 아닐 수 있는데",
          "original_form": "저희 엄마가 일본 다녀오시고 암이 재발됐거든요 ㅠㅠ 원인이 그게 아닐 수 있는데",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030487.1.5",
          "form": "타이밍이...? 괜히 찝찝해서!!!",
          "original_form": "타이밍이...? 괜히 찝찝해서!!!",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030487.1.6",
          "form": "네네 저는 다녀왔었어요!",
          "original_form": "네네 저는 다녀왔었어요!",
          "speaker_id": "1",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100030487.1.7",
          "form": "고등학교 때 쓰나미 전에 다녀왔었어요~! 좋긴 좋더라구요...",
          "original_form": "고등학교 때 쓰나미 전에 다녀왔었어요~! 좋긴 좋더라구요...",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030487.1.8",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "2",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030487.1.9",
          "form": "아 물론 일본 다녀온 것 때문에",
          "original_form": "아 물론 일본 다녀온 것 때문에",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030487.1.10",
          "form": "쓰나미 전에 다녀오셨다니 타이밍이 정말 좋았네요!",
          "original_form": "쓰나미 전에 다녀오셨다니 타이밍이 정말 좋았네요!",
          "speaker_id": "2",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030487.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030487.1.12",
          "form": "재발이 됐다!!!! 이렇다기 보단",
          "original_form": "재발이 됐다!!!! 이렇다기 보단",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030487.1.13",
          "form": "네네!!! 괜히 마음이요!!! ",
          "original_form": "네네!!! 괜히 마음이요!!! ",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030487.1.14",
          "form": "맞아요 쓰나미가 정말 다음 해에 일어났었어요...!!! 기분이 굉장히 묘하더라구요...",
          "original_form": "맞아요 쓰나미가 정말 다음 해에 일어났었어요...!!! 기분이 굉장히 묘하더라구요...",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030487.1.15",
          "form": "저도 고등학교 때 일본으로 수학여행을 갔어서",
          "original_form": "저도 고등학교 때 일본으로 수학여행을 갔어서",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030487.1.16",
          "form": "다시 너무 가고싶더라구요ㅜ",
          "original_form": "다시 너무 가고싶더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030487.1.17",
          "form": "아 그 기분!",
          "original_form": "아 그 기분!",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030487.1.18",
          "form": "저도 그 때 정말 꿈결같았어요 ㅠㅠ",
          "original_form": "저도 그 때 정말 꿈결같았어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030487.1.19",
          "form": "어디로 다녀오셨나요?",
          "original_form": "어디로 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030487.1.20",
          "form": "오사카요!",
          "original_form": "오사카요!",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030487.1.21",
          "form": "거기 유니버셜스튜디오가 있어서",
          "original_form": "거기 유니버셜스튜디오가 있어서",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030487.1.22",
          "form": "그걸 노리고갔어요ㅋㅋ..",
          "original_form": "그걸 노리고갔어요ㅋㅋ..",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030487.1.23",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030487.1.24",
          "form": "진짜 제대로 다녀오셨네요",
          "original_form": "진짜 제대로 다녀오셨네요",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030487.1.25",
          "form": "저는 ㅜㅠ 그냥 시골만 다녀왔어요 부럽습니다ㅠ",
          "original_form": "저는 ㅜㅠ 그냥 시골만 다녀왔어요 부럽습니다ㅠ",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030487.1.26",
          "form": "앗 시골이라면 어디다녀오셨나요?",
          "original_form": "앗 시골이라면 어디다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030487.1.27",
          "form": "정확한 지역명은 기억 안나는데",
          "original_form": "정확한 지역명은 기억 안나는데",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030487.1.28",
          "form": "저희가 화산지대를 다녀왔거든요!",
          "original_form": "저희가 화산지대를 다녀왔거든요!",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030487.1.29",
          "form": "홋카이도~나가사키 이렇게 다녀왔던 기억이.....",
          "original_form": "홋카이도~나가사키 이렇게 다녀왔던 기억이.....",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030487.1.30",
          "form": "왕 나가사키 짬뽕!",
          "original_form": "왕 나가사키 짬뽕!",
          "speaker_id": "2",
          "time": "20210904 02:11"
        }
      ]
    }
