        {
          "id": "MDRW2100022723.1.1",
          "form": "덥죠?",
          "original_form": "덥죠?",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022723.1.2",
          "form": "여름이 그래도 끝나가는것같아요",
          "original_form": "여름이 그래도 끝나가는것같아요",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022723.1.3",
          "form": "네 어느정도",
          "original_form": "네 어느정도",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022723.1.4",
          "form": "더운게 좋으신가요 추운게 좋으신가요",
          "original_form": "더운게 좋으신가요 추운게 좋으신가요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022723.1.5",
          "form": "추운거요",
          "original_form": "추운거요",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022723.1.6",
          "form": "저도 그래요 추운게낫죠",
          "original_form": "저도 그래요 추운게낫죠",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022723.1.7",
          "form": "땀이 너무 나서요",
          "original_form": "땀이 너무 나서요",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022723.1.8",
          "form": "작년엔 눈이 많이 왔죠",
          "original_form": "작년엔 눈이 많이 왔죠",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022723.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022723.1.10",
          "form": "눈오리가 유행이었어요",
          "original_form": "눈오리가 유행이었어요",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022723.1.11",
          "form": "ㅎㅎ 없어서 못했죠",
          "original_form": "ㅎㅎ 없어서 못했죠",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022723.1.12",
          "form": "ㅎㅎㅎ귀엽긴 하더라구요. 얼른 가을이 됐으면 좋겠네요",
          "original_form": "ㅎㅎㅎ귀엽긴 하더라구요. 얼른 가을이 됐으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
