      "utterance": [
        {
          "id": "MDRW2100025372.1.1",
          "form": "저는 밥도 먹었으니 이따가 산책이나 나가려규요",
          "original_form": "저는 밥도 먹었으니 이따가 산책이나 나가려규요",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025372.1.2",
          "form": "오오 산책 좋죠 오늘 날씨도 좋던데요",
          "original_form": "오오 산책 좋죠 오늘 날씨도 좋던데요",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025372.1.3",
          "form": "네 슬쩍 보니까 비도 그쳤도라구요",
          "original_form": "네 슬쩍 보니까 비도 그쳤도라구요",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025372.1.4",
          "form": "아 그쪽지방에서도 비왔나 봐요",
          "original_form": "아 그쪽지방에서도 비왔나 봐요",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025372.1.5",
          "form": "네!! 이쪽도 어제인지 그제부터 계속 비왔어요",
          "original_form": "네!! 이쪽도 어제인지 그제부터 계속 비왔어요",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025372.1.6",
          "form": "아 그러셨구나 제가 사는 곳도 그랬어요ㅠㅠ",
          "original_form": "아 그러셨구나 제가 사는 곳도 그랬어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025372.1.7",
          "form": "되게 우중충하고... 그러더라고요ㅠㅠ",
          "original_form": "되게 우중충하고... 그러더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025372.1.8",
          "form": "네 맞아요 계속 비가 오다보니까 시원은 한데 분위기가 참...",
          "original_form": "네 맞아요 계속 비가 오다보니까 시원은 한데 분위기가 참...",
          "speaker_id": "1",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025372.1.9",
          "form": "쓸쓸하더라구요 이제 정말 가을인가봐요",
          "original_form": "쓸쓸하더라구요 이제 정말 가을인가봐요",
          "speaker_id": "1",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025372.1.10",
          "form": "맞아요 진짜 여름은 가고 가을이 왔어요",
          "original_form": "맞아요 진짜 여름은 가고 가을이 왔어요",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025372.1.11",
          "form": "오늘부터 9월이니까요",
          "original_form": "오늘부터 9월이니까요",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025372.1.12",
          "form": "벌싸 9월이군요 날씨가 더 추워지겠어요",
          "original_form": "벌싸 9월이군요 날씨가 더 추워지겠어요",
          "speaker_id": "1",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025372.1.13",
          "form": "따뜻한 옷 좀 들여야겠다ㅎㅎ",
          "original_form": "따뜻한 옷 좀 들여야겠다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025372.1.14",
          "form": "그쵸 옷차림을 좀 따뜻하게 입어야 할 거 같아요",
          "original_form": "그쵸 옷차림을 좀 따뜻하게 입어야 할 거 같아요",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025372.1.15",
          "form": "춥더라고요 어젠",
          "original_form": "춥더라고요 어젠",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025372.1.16",
          "form": "네 맞아요 밤에 정말 추워서 긴팔 입고 잤어요",
          "original_form": "네 맞아요 밤에 정말 추워서 긴팔 입고 잤어요",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025372.1.17",
          "form": "태풍은 또 오지 않겠죠?ㅠ",
          "original_form": "태풍은 또 오지 않겠죠?ㅠ",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025372.1.18",
          "form": "가을 태풍이 가장 무섭잖아요ㅠㅠㅠ",
          "original_form": "가을 태풍이 가장 무섭잖아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025372.1.19",
          "form": "날씨가 안정적이었으면 좋겠어요",
          "original_form": "날씨가 안정적이었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 10:27"
        }
      ]
