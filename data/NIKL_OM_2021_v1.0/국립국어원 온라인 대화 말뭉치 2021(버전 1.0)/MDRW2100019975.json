      "utterance": [
        {
          "id": "MDRW2100019975.1.1",
          "form": "네ㅜ 롤은 웬만해선 망할 일이 없는 것 같아요",
          "original_form": "네ㅜ 롤은 웬만해선 망할 일이 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019975.1.2",
          "form": "옵치나 배그는 3~4년 전에 한창 유행하고",
          "original_form": "옵치나 배그는 3~4년 전에 한창 유행하고",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019975.1.3",
          "form": "갑자기 다 망해버렸어요ㅜㅠ",
          "original_form": "갑자기 다 망해버렸어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019975.1.4",
          "form": "반짝인가요???",
          "original_form": "반짝인가요???",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019975.1.5",
          "form": "롤의 인기비결이 뭘까요??",
          "original_form": "롤의 인기비결이 뭘까요??",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019975.1.6",
          "form": "네 다른 게임들은 웬만하면 반짝이죠...",
          "original_form": "네 다른 게임들은 웬만하면 반짝이죠...",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019975.1.7",
          "form": "롤은 모르겠어요",
          "original_form": "롤은 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019975.1.8",
          "form": "사실 제가 안 해서",
          "original_form": "사실 제가 안 해서",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019975.1.9",
          "form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019975.1.10",
          "form": "아 name2님은 어떤 게임 하세요?",
          "original_form": "아 &name2&님은 어떤 게임 하세요?",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019975.1.11",
          "form": "저는 망겜만 잘 잡는지 옵치 배그 아직도 해요ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "저는 망겜만 잘 잡는지 옵치 배그 아직도 해요ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019975.1.12",
          "form": "저도 옵치 계속 해요 ㅋㅋ",
          "original_form": "저도 옵치 계속 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019975.1.13",
          "form": "그래도 피씨방에서 여전히 보이던데요",
          "original_form": "그래도 피씨방에서 여전히 보이던데요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019975.1.14",
          "form": "맞아요ㅋㅋㅋㅋ 게임이 돌아가긴 하죠",
          "original_form": "맞아요ㅋㅋㅋㅋ 게임이 돌아가긴 하죠",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019975.1.15",
          "form": "흥행할 때만큼이 아니라 그렇죠",
          "original_form": "흥행할 때만큼이 아니라 그렇죠",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019975.1.16",
          "form": "아하 경쟁전도 하나요?",
          "original_form": "아하 경쟁전도 하나요?",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019975.1.17",
          "form": "잘 못해서 거의 안 해요ㅜ",
          "original_form": "잘 못해서 거의 안 해요ㅜ",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019975.1.18",
          "form": "빠른 대전에서나 아케이드에서 조금 하고",
          "original_form": "빠른 대전에서나 아케이드에서 조금 하고",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019975.1.19",
          "form": "헐 저도요 빠대만 돌려요 ㅋㅋㅋ",
          "original_form": "헐 저도요 빠대만 돌려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019975.1.20",
          "form": "근데 너무 오래걸림 ㅠ",
          "original_form": "근데 너무 오래걸림 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019975.1.21",
          "form": "맞아요ㅋㅋㅋ ㅠ",
          "original_form": "맞아요ㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019975.1.22",
          "form": "어떤포지션하세요?",
          "original_form": "어떤포지션하세요?",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019975.1.23",
          "form": "저는 주로 딜러요",
          "original_form": "저는 주로 딜러요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019975.1.24",
          "form": "잘하는 건 아닌데 다른 거에 비하면 제일... 나아서",
          "original_form": "잘하는 건 아닌데 다른 거에 비하면 제일... 나아서",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019975.1.25",
          "form": "딜러는 진짜 10분 기다리면",
          "original_form": "딜러는 진짜 10분 기다리면",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019975.1.26",
          "form": "연결되던데",
          "original_form": "연결되던데",
          "speaker_id": "1",
          "time": "20210827 23:19"
        }
      ]
    }
