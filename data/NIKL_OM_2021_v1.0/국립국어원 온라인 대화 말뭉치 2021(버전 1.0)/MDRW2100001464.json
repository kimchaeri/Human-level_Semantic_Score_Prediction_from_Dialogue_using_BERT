      "utterance": [
        {
          "id": "MDRW2100001464.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20211014 18:21"
        },
        {
          "id": "MDRW2100001464.1.2",
          "form": "안녕하세요~ 혹시 백신 맞으셨어요?",
          "original_form": "안녕하세요~ 혹시 백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20211014 18:21"
        },
        {
          "id": "MDRW2100001464.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.4",
          "form": "화이자",
          "original_form": "화이자",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.5",
          "form": "맞았어요",
          "original_form": "맞았어요",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.6",
          "form": "아 2차까지 다 맞으셨어요??",
          "original_form": "아 2차까지 다 맞으셨어요??",
          "speaker_id": "2",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.7",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.8",
          "form": "부작용은 없으셨나요?",
          "original_form": "부작용은 없으셨나요?",
          "speaker_id": "2",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.9",
          "form": "2차까지 다 맞았어요",
          "original_form": "2차까지 다 맞았어요",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.10",
          "form": "맞고나서 다음날",
          "original_form": "맞고나서 다음날",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.11",
          "form": "엄청 아팠는데",
          "original_form": "엄청 아팠는데",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.12",
          "form": "그게 부작용이었나봐여 ㅋㅋ",
          "original_form": "그게 부작용이었나봐여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211014 18:22"
        },
        {
          "id": "MDRW2100001464.1.13",
          "form": "헉 엄청 놀라셨겠어요...",
          "original_form": "헉 엄청 놀라셨겠어요...",
          "speaker_id": "2",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.14",
          "form": "요즘 부작용이니 뭐니해서 말 많아서",
          "original_form": "요즘 부작용이니 뭐니해서 말 많아서",
          "speaker_id": "2",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.15",
          "form": "불안하셨을텐데 ㅜㅜ",
          "original_form": "불안하셨을텐데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.16",
          "form": "네네 ㅠㅠ",
          "original_form": "네네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.17",
          "form": "그래도 다들",
          "original_form": "그래도 다들",
          "speaker_id": "1",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.18",
          "form": "비슷하게 열나고 하루 아프다던데",
          "original_form": "비슷하게 열나고 하루 아프다던데",
          "speaker_id": "1",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.19",
          "form": "저도 그래서 그나마 다행이네요",
          "original_form": "저도 그래서 그나마 다행이네요",
          "speaker_id": "1",
          "time": "20211014 18:23"
        },
        {
          "id": "MDRW2100001464.1.20",
          "form": "그나마 다행이네요 ㅠㅠ",
          "original_form": "그나마 다행이네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001464.1.21",
          "form": "부스트샷을 맞아야된다 이런 말도 나오던데",
          "original_form": "부스트샷을 맞아야된다 이런 말도 나오던데",
          "speaker_id": "2",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001464.1.22",
          "form": "그거에 대해서는 어떻게 생각하세요?",
          "original_form": "그거에 대해서는 어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20211014 18:24"
        },
        {
          "id": "MDRW2100001464.1.23",
          "form": "솔직히 일반인은 필요 없다고 생각되는데..",
          "original_form": "솔직히 일반인은 필요 없다고 생각되는데..",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001464.1.24",
          "form": "좀 코로나 확진 위험성이 높은 의료인들은 필요해도",
          "original_form": "좀 코로나 확진 위험성이 높은 의료인들은 필요해도",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001464.1.25",
          "form": "일반인은 아직 어불성설인거 같아요",
          "original_form": "일반인은 아직 어불성설인거 같아요",
          "speaker_id": "1",
          "time": "20211014 18:25"
        },
        {
          "id": "MDRW2100001464.1.26",
          "form": "저랑 생각이 비슷하시네요!\r\n",
          "original_form": "저랑 생각이 비슷하시네요!\r\n",
          "speaker_id": "2",
          "time": "20211014 18:25"
        }
