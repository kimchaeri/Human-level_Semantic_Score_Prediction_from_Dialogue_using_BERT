      "utterance": [
        {
          "id": "MDRW2100002321.1.1",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210820 09:26"
        },
        {
          "id": "MDRW2100002321.1.2",
          "form": "네 회사 다녀요~ 직장인이세요?",
          "original_form": "네 회사 다녀요~ 직장인이세요?",
          "speaker_id": "2",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100002321.1.3",
          "form": "넵 저도 직장인이요 ! 금요일이라 넘 행복해요",
          "original_form": "넵 저도 직장인이요 ! 금요일이라 넘 행복해요",
          "speaker_id": "1",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100002321.1.4",
          "form": "맞아요 회사 너무 힘들어요 ㅠ ㅠ 주말만 기다려져요 항상",
          "original_form": "맞아요 회사 너무 힘들어요 ㅠ ㅠ 주말만 기다려져요 항상",
          "speaker_id": "2",
          "time": "20210820 09:28"
        },
        {
          "id": "MDRW2100002321.1.5",
          "form": "9시 출근 6시퇴근이신가요?",
          "original_form": "9시 출근 6시퇴근이신가요?",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002321.1.6",
          "form": "네 보통 그런데 늦게 끝나는 날도 있어요",
          "original_form": "네 보통 그런데 늦게 끝나는 날도 있어요",
          "speaker_id": "2",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002321.1.7",
          "form": "야근 많이 하세요?",
          "original_form": "야근 많이 하세요?",
          "speaker_id": "2",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002321.1.8",
          "form": "네 휴.. 자주해요",
          "original_form": "네 휴.. 자주해요",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002321.1.9",
          "form": "무슨업계에서 일하시는데 야근이많으세요? 전 it..",
          "original_form": "무슨업계에서 일하시는데 야근이많으세요? 전 it..",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002321.1.10",
          "form": "it쪽이시구나 저는 교육쪽이에요 ㅠㅠ",
          "original_form": "it쪽이시구나 저는 교육쪽이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100002321.1.11",
          "form": "it도 힘드실거같아요",
          "original_form": "it도 힘드실거같아요",
          "speaker_id": "2",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002321.1.12",
          "form": "힘들어요.. 아직 출근 30분밖에 안지났다는 사실이 믿기지않아요",
          "original_form": "힘들어요.. 아직 출근 30분밖에 안지났다는 사실이 믿기지않아요",
          "speaker_id": "1",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100002321.1.13",
          "form": "맞아요 정말 ㅠㅠ 하루하루가 쉽지않아요",
          "original_form": "맞아요 정말 ㅠㅠ 하루하루가 쉽지않아요",
          "speaker_id": "2",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100002321.1.14",
          "form": "그래도 금요일이니깐..!",
          "original_form": "그래도 금요일이니깐..!",
          "speaker_id": "2",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100002321.1.15",
          "form": "취업하신지는 얼마나되셨어요? 전 올해요",
          "original_form": "취업하신지는 얼마나되셨어요? 전 올해요",
          "speaker_id": "1",
          "time": "20210820 09:34"
        },
        {
          "id": "MDRW2100002321.1.16",
          "form": "저는 3년정도 되었어요",
          "original_form": "저는 3년정도 되었어요",
          "speaker_id": "2",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002321.1.17",
          "form": "근데 아직도 막내라서 더 슬퍼요 ㅠㅠㅋ",
          "original_form": "근데 아직도 막내라서 더 슬퍼요 ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210820 09:35"
        },
        {
          "id": "MDRW2100002321.1.18",
          "form": "헉 3년차인데 막내시라니",
          "original_form": "헉 3년차인데 막내시라니",
          "speaker_id": "1",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002321.1.19",
          "form": "신입왜안뽑아준데요?",
          "original_form": "신입왜안뽑아준데요?",
          "speaker_id": "1",
          "time": "20210820 09:36"
        },
        {
          "id": "MDRW2100002321.1.20",
          "form": "모르겠어요 채용을 거의 안하더라구요",
          "original_form": "모르겠어요 채용을 거의 안하더라구요",
          "speaker_id": "2",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002321.1.21",
          "form": "제가 막차탄거라 다행인가 싶으면서도 계속 막내하면 힘들거같아요 ㅠㅠ",
          "original_form": "제가 막차탄거라 다행인가 싶으면서도 계속 막내하면 힘들거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002321.1.22",
          "form": "막내 너무 힘들어요.. 저도 막내인데 계속 잡일 시켜서 현타와요 흑흑",
          "original_form": "막내 너무 힘들어요.. 저도 막내인데 계속 잡일 시켜서 현타와요 흑흑",
          "speaker_id": "1",
          "time": "20210820 09:38"
        },
        {
          "id": "MDRW2100002321.1.23",
          "form": "맞아요 ㅠㅠ 빨리 후배 받으시면 좋겠네요",
          "original_form": "맞아요 ㅠㅠ 빨리 후배 받으시면 좋겠네요",
          "speaker_id": "2",
          "time": "20210820 09:39"
        },
        {
          "id": "MDRW2100002321.1.24",
          "form": "저는 올해도 글른거같아요",
          "original_form": "저는 올해도 글른거같아요",
