      "utterance": [
        {
          "id": "MDRW2100007561.1.1",
          "form": "굿모닝이에요",
          "original_form": "굿모닝이에요",
          "speaker_id": "1",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007561.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007561.1.3",
          "form": "무슨 일 하시나요",
          "original_form": "무슨 일 하시나요",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007561.1.4",
          "form": "전 학생이에요",
          "original_form": "전 학생이에요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100007561.1.5",
          "form": "아 학생 좋죠",
          "original_form": "아 학생 좋죠",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100007561.1.6",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100007561.1.7",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100007561.1.8",
          "form": "어떤 직종이세요?ㅎㅎ",
          "original_form": "어떤 직종이세요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100007561.1.9",
          "form": "저는 공기업 사무직이요ㅎㅎ",
          "original_form": "저는 공기업 사무직이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100007561.1.10",
          "form": "오 취업하신게 대단해보여요ㅎㅎ",
          "original_form": "오 취업하신게 대단해보여요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100007561.1.11",
          "form": "운이 좋았죠ㅎㅎ",
          "original_form": "운이 좋았죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100007561.1.12",
          "form": "취업하실때 준비하신것 있으신가요?",
          "original_form": "취업하실때 준비하신것 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100007561.1.13",
          "form": "음 저는 딱히요!",
          "original_form": "음 저는 딱히요!",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100007561.1.14",
          "form": "헉 정말요?",
          "original_form": "헉 정말요?",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100007561.1.15",
          "form": "서포터즈 하신 적 있으신가요?",
          "original_form": "서포터즈 하신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100007561.1.16",
          "form": "음 기억에 없습니다ㅋㅋ",
          "original_form": "음 기억에 없습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100007561.1.17",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100007561.1.18",
          "form": "직장에서 오는 스트레스도 있을 것같아요",
          "original_form": "직장에서 오는 스트레스도 있을 것같아요",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100007561.1.19",
          "form": "어떤게 있을까요?",
          "original_form": "어떤게 있을까요?",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100007561.1.20",
          "form": "진상이오",
          "original_form": "진상이오",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100007561.1.21",
          "form": "내부외부",
          "original_form": "내부외부",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100007561.1.22",
          "form": "진상이라면 어떤 경우인가요??",
          "original_form": "진상이라면 어떤 경우인가요??",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100007561.1.23",
          "form": "외부는 뭐 땡깡 부리는 노인?",
          "original_form": "외부는 뭐 땡깡 부리는 노인?",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100007561.1.24",
          "form": "내부는요?",
          "original_form": "내부는요?",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100007561.1.25",
          "form": "직원탓 하는 회사와 서울시요ㅋㅋ",
          "original_form": "직원탓 하는 회사와 서울시요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100007561.1.26",
          "form": "서울시ㅋㅋㅋㅋㅋㅋ서울은 왜요?ㅠㅋㅋㅋ",
          "original_form": "서울시ㅋㅋㅋㅋㅋㅋ서울은 왜요?ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100007561.1.27",
          "form": "서울시 산하 공기업이라서요ㅋㅋㅋ",
          "original_form": "서울시 산하 공기업이라서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100007561.1.28",
          "form": "그럼 그 스트레스 푸는 방법 있으세요?",
          "original_form": "그럼 그 스트레스 푸는 방법 있으세요?",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100007561.1.29",
          "form": "ㅋㅋ지인들 만나서 욕하기?",
          "original_form": "ㅋㅋ지인들 만나서 욕하기?",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100007561.1.30",
          "form": "ㅋㅋㅋㅋㅋ스트레스 풀어지겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ스트레스 풀어지겠어요",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100007561.1.31",
          "form": "저는 스트레스 받으면 그냥 자거든요",
          "original_form": "저는 스트레스 받으면 그냥 자거든요",
          "speaker_id": "2",
          "time": "20210825 16:01"
        }
      ]
    }
