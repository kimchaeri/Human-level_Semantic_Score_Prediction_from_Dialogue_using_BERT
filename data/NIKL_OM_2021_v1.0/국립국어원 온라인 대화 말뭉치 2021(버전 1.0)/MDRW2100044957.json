      "utterance": [
        {
          "id": "MDRW2100044957.1.1",
          "form": "신랑분 어떻게 만나셨나요?",
          "original_form": "신랑분 어떻게 만나셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044957.1.2",
          "form": "놀다가 만났어요 ㅎㅎ",
          "original_form": "놀다가 만났어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044957.1.3",
          "form": "님은요>>",
          "original_form": "님은요>>",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044957.1.4",
          "form": "전 남한테 들어온 소개팅.제가 나가서...ㅋㅋ",
          "original_form": "전 남한테 들어온 소개팅.제가 나가서...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044957.1.5",
          "form": "운명이시네요!",
          "original_form": "운명이시네요!",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044957.1.6",
          "form": "전 자연스러운 만남을 추구하는데요",
          "original_form": "전 자연스러운 만남을 추구하는데요",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044957.1.7",
          "form": "자만추..ㅋㅋㅋㅋ",
          "original_form": "자만추..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044957.1.8",
          "form": "놀다 만난게 자연스러운뎅ㅅ",
          "original_form": "놀다 만난게 자연스러운뎅ㅅ",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044957.1.9",
          "form": "데요",
          "original_form": "데요",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044957.1.10",
          "form": "ㅋㅋㅋ ㅋ자연스럽죠???ㅋㅋ",
          "original_form": "ㅋㅋㅋ ㅋ자연스럽죠???ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044957.1.11",
          "form": "네~ 어디서 만나셨나요?",
          "original_form": "네~ 어디서 만나셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044957.1.12",
          "form": "악ㅋㅋㅋ 비밀인데",
          "original_form": "악ㅋㅋㅋ 비밀인데",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044957.1.13",
          "form": "나이트요 ㅋㅋㅋ",
          "original_form": "나이트요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044957.1.14",
          "form": "자연스럽나요??",
          "original_form": "자연스럽나요??",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044957.1.15",
          "form": "나이트냐고 여쭤보려다 말았는데 역시 맞군요 ㅎㅎ 자연스러워요 ㄹ",
          "original_form": "나이트냐고 여쭤보려다 말았는데 역시 맞군요 ㅎㅎ 자연스러워요 ㄹ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044957.1.16",
          "form": "놀다가만날곳은 거기뿐인가봐요",
          "original_form": "놀다가만날곳은 거기뿐인가봐요",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044957.1.17",
          "form": "다른데서 만나면 놀다 만났다고 표현하지 않으니까요 ㅎㅎ",
          "original_form": "다른데서 만나면 놀다 만났다고 표현하지 않으니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        }
