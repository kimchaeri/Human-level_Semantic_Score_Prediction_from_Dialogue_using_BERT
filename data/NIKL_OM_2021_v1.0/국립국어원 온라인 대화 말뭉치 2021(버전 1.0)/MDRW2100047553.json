      "utterance": [
        {
          "id": "MDRW2100047553.1.1",
          "form": "여행 계획이 있으신가요?",
          "original_form": "여행 계획이 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100047553.1.2",
          "form": "앗 아뇨ㅠㅠㅠ 코로나라",
          "original_form": "앗 아뇨ㅠㅠㅠ 코로나라",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100047553.1.3",
          "form": "꿈나라 여행만이 여행인게 되어버렸어요",
          "original_form": "꿈나라 여행만이 여행인게 되어버렸어요",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100047553.1.4",
          "form": "여행 좋아하세여?!",
          "original_form": "여행 좋아하세여?!",
          "speaker_id": "1",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100047553.1.5",
          "form": "앜ㅋㅋ꿈나라여행ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋ꿈나라여행ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047553.1.6",
          "form": "전 좋아해요! 국내해외다좋아요",
          "original_form": "전 좋아해요! 국내해외다좋아요",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100047553.1.7",
          "form": "오오 국내는 어디가 젤 좋으셨나요?",
          "original_form": "오오 국내는 어디가 젤 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100047553.1.8",
          "form": "국내는 의외로 여수가 좋았어요~~",
          "original_form": "국내는 의외로 여수가 좋았어요~~",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100047553.1.9",
          "form": "오오오 여수 좋죠",
          "original_form": "오오오 여수 좋죠",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047553.1.10",
          "form": "게장 드셨나요?",
          "original_form": "게장 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047553.1.11",
          "form": "아뇨 ㅋㅋ 아 여수가 게장이 유명한가요?",
          "original_form": "아뇨 ㅋㅋ 아 여수가 게장이 유명한가요?",
          "speaker_id": "2",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100047553.1.12",
          "form": "아뇨 제가 여수가서 먹은게 생각나서",
          "original_form": "아뇨 제가 여수가서 먹은게 생각나서",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047553.1.13",
          "form": "ㅌㅌㅋㅋ앜ㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "original_form": "ㅌㅌㅋㅋ앜ㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047553.1.14",
          "form": "극히 개인적인 경험이었습니다",
          "original_form": "극히 개인적인 경험이었습니다",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047553.1.15",
          "form": "장범준 씨가 여수밤바다 노래 낸 후로",
          "original_form": "장범준 씨가 여수밤바다 노래 낸 후로",
          "speaker_id": "1",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100047553.1.16",
          "form": "여수 여행객들이 늘었대요",
          "original_form": "여수 여행객들이 늘었대요",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100047553.1.17",
          "form": "아 정말요?? 아 장범준님 목소리 넘좋죠 ㅠㅠ",
          "original_form": "아 정말요?? 아 장범준님 목소리 넘좋죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047553.1.18",
          "form": "그러고보니 여수갔으면서 밤바다는 못보고왔네요 ㅋㅋ",
          "original_form": "그러고보니 여수갔으면서 밤바다는 못보고왔네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047553.1.19",
          "form": "네네ㅜㅜㅜ 여수 밤바다 뷰시면서",
          "original_form": "네네ㅜㅜㅜ 여수 밤바다 뷰시면서",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047553.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047553.1.21",
          "form": "아아닛?!!왜죠?!!",
          "original_form": "아아닛?!!왜죠?!!",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047553.1.22",
          "form": "그러게요 ㅋㅋ 저녁에 뭐했나몰라요 ㅋㅋ 아침바다만잠깐봤네요 ㅋㅋ",
          "original_form": "그러게요 ㅋㅋ 저녁에 뭐했나몰라요 ㅋㅋ 아침바다만잠깐봤네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100047553.1.23",
          "form": "그래도 바다 보고 오셨군요",
          "original_form": "그래도 바다 보고 오셨군요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047553.1.24",
          "form": "해외는 어디가 젤 좋으셨어요?",
          "original_form": "해외는 어디가 젤 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047553.1.25",
          "form": "해외는 미국이 젤 좋았어요 ㅎㅎ",
          "original_form": "해외는 미국이 젤 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100047553.1.26",
          "form": "와우...와우..",
          "original_form": "와우...와우..",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047553.1.27",
          "form": "와우 미국 어디가셨어요?!!!",
          "original_form": "와우 미국 어디가셨어요?!!!",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047553.1.28",
          "form": "앜ㅋㅋㅋㅋㅋ 저는 동부 서부 다가봤는데 서부가 더 좋았어요 ㅎㅎ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 저는 동부 서부 다가봤는데 서부가 더 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047553.1.29",
          "form": "오 서부가..쓰읍..어디죠?",
          "original_form": "오 서부가..쓰읍..어디죠?",
          "speaker_id": "1",
          "time": "20210826 21:18"
