      "utterance": [
        {
          "id": "MDRW2100031640.1.1",
          "form": "곰표 아시나요",
          "original_form": "곰표 아시나요",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031640.1.2",
          "form": "알죠 아직 못마셔봤네요",
          "original_form": "알죠 아직 못마셔봤네요",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031640.1.3",
          "form": "마셔봤나요",
          "original_form": "마셔봤나요",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031640.1.4",
          "form": "곰표 팝콘 은 먹어봤네요 ㅋㅋ",
          "original_form": "곰표 팝콘 은 먹어봤네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031640.1.5",
          "form": "곰표 맥주있던데",
          "original_form": "곰표 맥주있던데",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031640.1.6",
          "form": "팝콘은 처음 들어봤어요",
          "original_form": "팝콘은 처음 들어봤어요",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031640.1.7",
          "form": "맥주는 마셔보고싶네요",
          "original_form": "맥주는 마셔보고싶네요",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031640.1.8",
          "form": "진로소주 좋아하나요",
          "original_form": "진로소주 좋아하나요",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031640.1.9",
          "form": "소주를 제가 안좋아해서",
          "original_form": "소주를 제가 안좋아해서",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031640.1.10",
          "form": "어제 진로 메로나소주?는먹어봤네요",
          "original_form": "어제 진로 메로나소주?는먹어봤네요",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031640.1.11",
          "form": "오 메로나",
          "original_form": "오 메로나",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031640.1.12",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031640.1.13",
          "form": "진짜 메로나맛이더라구요 ㅋㅋ",
          "original_form": "진짜 메로나맛이더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031640.1.14",
          "form": "메로나알콜맛",
          "original_form": "메로나알콜맛",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031640.1.15",
          "form": "사먹어봐야깄네요",
          "original_form": "사먹어봐야깄네요",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031640.1.16",
          "form": "전 지금 맥주음료 보고있어요 쇼핑",
          "original_form": "전 지금 맥주음료 보고있어요 쇼핑",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031640.1.17",
          "form": "산토리니 맥주향 탄산",
          "original_form": "산토리니 맥주향 탄산",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031640.1.18",
          "form": "거긴어디껀가요",
          "original_form": "거긴어디껀가요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031640.1.19",
          "form": "빙그레 꺼네요",
          "original_form": "빙그레 꺼네요",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031640.1.20",
          "form": "요새 콜라보 많이 나오네요",
          "original_form": "요새 콜라보 많이 나오네요",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031640.1.21",
          "form": "그러네요 혼술하기 좋아요",
          "original_form": "그러네요 혼술하기 좋아요",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031640.1.22",
          "form": "회식하면 참이나 이슬만 마시는데",
          "original_form": "회식하면 참이나 이슬만 마시는데",
