        },
        {
          "id": "MDRW2100018451.1.3",
          "form": "통새우 와퍼?ㅠ",
          "original_form": "통새우 와퍼?ㅠ",
          "speaker_id": "1",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018451.1.4",
          "form": "땡!!!!!!!!",
          "original_form": "땡!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018451.1.5",
          "form": "새우가들어가는건맞아요",
          "original_form": "새우가들어가는건맞아요",
          "speaker_id": "2",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018451.1.6",
          "form": "와 이렇게 틀린다니",
          "original_form": "와 이렇게 틀린다니",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018451.1.7",
          "form": "아 알았다 통새우X",
          "original_form": "아 알았다 통새우X",
          "speaker_id": "1",
          "time": "20210827 02:12"
        },
        {
          "id": "MDRW2100018451.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018451.1.9",
          "form": "님...",
          "original_form": "님...",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018451.1.10",
          "form": "당분간 뭐 어디 고르기하시면안될것같아요",
          "original_form": "당분간 뭐 어디 고르기하시면안될것같아요",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018451.1.11",
          "form": "^^....틀렸어요?",
          "original_form": "^^....틀렸어요?",
          "speaker_id": "1",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018451.1.12",
          "form": "^^..",
          "original_form": "^^..",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018451.1.13",
          "form": "정답은 상어버거랍니다",
          "original_form": "정답은 상어버거랍니다",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018451.1.14",
          "form": "자 그럼",
          "original_form": "자 그럼",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018451.1.15",
          "form": "1 아기상어\r\n2 엄마상어\r\n3 아빠상어 무엇일까요?!!!!!!!",
          "original_form": "1 아기상어\r\n2 엄마상어\r\n3 아빠상어 무엇일까요?!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018451.1.16",
          "form": "그런게 있다구요?! 나만 몰랐네",
          "original_form": "그런게 있다구요?! 나만 몰랐네",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018451.1.17",
          "form": "아빠상어!",
          "original_form": "아빠상어!",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018451.1.18",
          "form": "커야지 배부르지요",
          "original_form": "커야지 배부르지요",
          "speaker_id": "1",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018451.1.19",
          "form": "님..^^",
          "original_form": "님..^^",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018451.1.20",
          "form": "당분간..",
          "original_form": "당분간..",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018451.1.21",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018451.1.22",
          "form": "아니예요",
          "original_form": "아니예요",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018451.1.23",
          "form": "마지막기회가 한번더!!!!!!!!!!!",
          "original_form": "마지막기회가 한번더!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018451.1.24",
          "form": "엄마상어ㅠ",
          "original_form": "엄마상어ㅠ",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018451.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018451.1.26",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018451.1.27",
          "form": "진심으로요",
          "original_form": "진심으로요",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018451.1.28",
          "form": "아 올림픽에서",
          "original_form": "아 올림픽에서",
          "speaker_id": "1",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018451.1.29",
          "form": "우승한 느낌",
          "original_form": "우승한 느낌",
          "speaker_id": "1",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018451.1.30",
          "form": "짜릿해!",
          "original_form": "짜릿해!",
          "speaker_id": "1",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018451.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018451.1.32",
          "form": "여러시도의 끝에",
          "original_form": "여러시도의 끝에",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018451.1.33",
          "form": "드디어 성공한 랩소디!!!!!!!!",
          "original_form": "드디어 성공한 랩소디!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018451.1.34",
          "form": "두둥 메달수여식이있겠습니다",
          "original_form": "두둥 메달수여식이있겠습니다",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018451.1.35",
          "form": "아아! 감사함니다 여러분!",
          "original_form": "아아! 감사함니다 여러분!",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018451.1.36",
          "form": "근데 메달 말고 버거로 주세요!",
          "original_form": "근데 메달 말고 버거로 주세요!",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018451.1.37",
          "form": "자자 메달버거왔습니다",
          "original_form": "자자 메달버거왔습니다",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018451.1.38",
          "form": "무슨버거일까요",
          "original_form": "무슨버거일까요",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018451.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018451.1.40",
          "form": "아 맥도날드 베토디",
          "original_form": "아 맥도날드 베토디",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018451.1.41",
          "form": "세트겠죠? 감튀 반드시 필요해요",
          "original_form": "세트겠죠? 감튀 반드시 필요해요",
          "speaker_id": "1",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018451.1.42",
          "form": "ㅋㅋㅋㅋ이건 님이자초한거예요!!!저는 퀴즈를",
          "original_form": "ㅋㅋㅋㅋ이건 님이자초한거예요!!!저는 퀴즈를",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018451.1.43",
          "form": "끝내려구했다고요!!!!",
          "original_form": "끝내려구했다고요!!!!",
          "speaker_id": "2",
          "time": "20210827 02:39"
        },
        {
          "id": "MDRW2100018451.1.44",
          "form": "말도 안돼요..",
          "original_form": "말도 안돼요..",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018451.1.45",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018451.1.46",
          "form": "아니 버거를 왜 드셔가지구",
          "original_form": "아니 버거를 왜 드셔가지구",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018451.1.47",
          "form": "사람을 이렇게 다 틀리게 하시는지ㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "사람을 이렇게 다 틀리게 하시는지ㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:40"
        },
        {
          "id": "MDRW2100018451.1.48",
          "form": "ㅋㅋㅋㅋ 님이 메달버거를 원하셨잖아요!!!!!",
          "original_form": "ㅋㅋㅋㅋ 님이 메달버거를 원하셨잖아요!!!!!",
          "speaker_id": "2",
          "time": "20210827 02:41"
        },
        {
          "id": "MDRW2100018451.1.49",
          "form": "저는 금메달을 드리려했답니다!!!!!!!!!!!!",
          "original_form": "저는 금메달을 드리려했답니다!!!!!!!!!!!!",
