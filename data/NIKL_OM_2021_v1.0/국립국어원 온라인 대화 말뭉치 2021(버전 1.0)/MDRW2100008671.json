      "utterance": [
        {
          "id": "MDRW2100008671.1.1",
          "form": "유혜광 돈까으라고 튀기지않고 냉동해서 오는거 있거든?",
          "original_form": "유혜광 돈까으라고 튀기지않고 냉동해서 오는거 있거든?",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008671.1.2",
          "form": "맛있음ㅋㅋㅋㅋ",
          "original_form": "맛있음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008671.1.3",
          "form": "오 나는 방금 교촌시켰는데",
          "original_form": "오 나는 방금 교촌시켰는데",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.4",
          "form": "배달비 3000원",
          "original_form": "배달비 3000원",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.5",
          "form": "충격받음",
          "original_form": "충격받음",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.7",
          "form": "나는 그래서 배달비 비싸면 안시킴ㅋㅋㅋ",
          "original_form": "나는 그래서 배달비 비싸면 안시킴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.8",
          "form": "아니 언제 오른거야",
          "original_form": "아니 언제 오른거야",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.9",
          "form": "아니면 배민 할인 쿠폰 쓰거나",
          "original_form": "아니면 배민 할인 쿠폰 쓰거나",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.10",
          "form": "뭔 오천원인데도 있대",
          "original_form": "뭔 오천원인데도 있대",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.11",
          "form": "그니까 넘 당황...근데 허니콤보랑 레드콤보 땡겨서",
          "original_form": "그니까 넘 당황...근데 허니콤보랑 레드콤보 땡겨서",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.12",
          "form": "시킴ㅠ",
          "original_form": "시킴ㅠ",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008671.1.13",
          "form": "5천원은 심하다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "5천원은 심하다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.14",
          "form": "맛있게 먹고 뽕뽑아",
          "original_form": "맛있게 먹고 뽕뽑아",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.16",
          "form": "나는 요즘 갤럭시워치 사고싶다",
          "original_form": "나는 요즘 갤럭시워치 사고싶다",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.17",
          "form": "그치만 텅장",
          "original_form": "그치만 텅장",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.18",
          "form": "ㅋㅋㅋ나는 버즈...",
          "original_form": "ㅋㅋㅋ나는 버즈...",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.19",
          "form": "버즈도 케이스 이쁘게 나옴ㅠㅠ",
          "original_form": "버즈도 케이스 이쁘게 나옴ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008671.1.20",
          "form": "나는 케이스 막상 안사게 되더라",
          "original_form": "나는 케이스 막상 안사게 되더라",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008671.1.21",
          "form": "에어팟 사고 처음산 케이스 그대로 쭉 쓰는중",
          "original_form": "에어팟 사고 처음산 케이스 그대로 쭉 쓰는중",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.22",
          "form": "나는 버즈 사게되면 케이스도 사려고ㅋㅋㅋ",
          "original_form": "나는 버즈 사게되면 케이스도 사려고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.23",
          "form": "버즈 막 콜라보하고 그러긴하더라",
          "original_form": "버즈 막 콜라보하고 그러긴하더라",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.24",
          "form": "근데 무선이어폰이 유선보다 나아??",
          "original_form": "근데 무선이어폰이 유선보다 나아??",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.25",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.26",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.27",
          "form": "없는생활 상상못해",
          "original_form": "없는생활 상상못해",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.28",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.29",
          "form": "이젠 줄있는게 신기하고 낯설어",
          "original_form": "이젠 줄있는게 신기하고 낯설어",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008671.1.30",
          "form": "삶의질 상승~~~~",
          "original_form": "삶의질 상승~~~~",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008671.1.31",
          "form": "버스에서도 줄있는 이어폰 잘 못본듯",
          "original_form": "버스에서도 줄있는 이어폰 잘 못본듯",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008671.1.32",
          "form": "거의 무선이어폰 쓰나봐ㅋㅋㅋ",
          "original_form": "거의 무선이어폰 쓰나봐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008671.1.33",
          "form": "요즘은 어르신들도 무선써",
          "original_form": "요즘은 어르신들도 무선써",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008671.1.34",
