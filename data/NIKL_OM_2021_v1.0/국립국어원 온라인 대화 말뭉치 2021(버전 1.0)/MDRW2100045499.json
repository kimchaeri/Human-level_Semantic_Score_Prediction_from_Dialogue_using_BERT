      "utterance": [
        {
          "id": "MDRW2100045499.1.1",
          "form": "전 토마토스파게티요!",
          "original_form": "전 토마토스파게티요!",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045499.1.2",
          "form": "일주일에 두세번은 맨들어먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "일주일에 두세번은 맨들어먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045499.1.3",
          "form": "마약 들은 것 같아요",
          "original_form": "마약 들은 것 같아요",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045499.1.4",
          "form": "요리 잘하시나봐여",
          "original_form": "요리 잘하시나봐여",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045499.1.5",
          "form": "잘 하는 게",
          "original_form": "잘 하는 게",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045499.1.6",
          "form": "솜씨가 좋다기보단 곧잘 한다의 잘에 가까워요ㅋㅋㅋㅋㅋ",
          "original_form": "솜씨가 좋다기보단 곧잘 한다의 잘에 가까워요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045499.1.7",
          "form": "웰메이드보다는",
          "original_form": "웰메이드보다는",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045499.1.8",
          "form": "빈번하게 만들어요 ㅋㅋㅋ맛은 보장할 수 없다만(...)",
          "original_form": "빈번하게 만들어요 ㅋㅋㅋ맛은 보장할 수 없다만(...)",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045499.1.9",
          "form": "주로 외식하는 편이세요?",
          "original_form": "주로 외식하는 편이세요?",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045499.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래도 먹을수있으면 잘만드는거져",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래도 먹을수있으면 잘만드는거져",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045499.1.11",
          "form": "그런편이에여",
          "original_form": "그런편이에여",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045499.1.12",
          "form": "ㅋㅋㅋㅋㅋ아유 감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋ아유 감사합니다",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045499.1.13",
          "form": "고기종류는 뭘 가장 좋아하시나요?",
          "original_form": "고기종류는 뭘 가장 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045499.1.14",
          "form": "음 다좋아해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "음 다좋아해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100045499.1.15",
          "form": "어떤 고기 좋아하세요?",
          "original_form": "어떤 고기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045499.1.16",
          "form": "전 요즘 닭고기가 좋아요 예전에는 아묻따 돼지였는데 입맛이 넓어졌어요",
          "original_form": "전 요즘 닭고기가 좋아요 예전에는 아묻따 돼지였는데 입맛이 넓어졌어요",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045499.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045499.1.18",
          "form": "제일제일 좋은거 딱 하나만 먹어야한다면?",
          "original_form": "제일제일 좋은거 딱 하나만 먹어야한다면?",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045499.1.19",
          "form": "아앙 그럼 닭멉을게여",
          "original_form": "아앙 그럼 닭멉을게여",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045499.1.20",
          "form": "치킨으루",
          "original_form": "치킨으루",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045499.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ치느님",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ치느님",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045499.1.22",
          "form": "넘 환상적이예요..치킨",
          "original_form": "넘 환상적이예요..치킨",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045499.1.23",
          "form": "후라이드치킨파세요 양념파세요 간장파세요?",
          "original_form": "후라이드치킨파세요 양념파세요 간장파세요?",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100045499.1.24",
