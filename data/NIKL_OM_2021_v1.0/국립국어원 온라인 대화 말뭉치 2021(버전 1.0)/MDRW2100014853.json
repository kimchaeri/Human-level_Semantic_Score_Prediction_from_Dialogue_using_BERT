        {
          "id": "MDRW2100014853.1.1",
          "form": "요즘 날씨가 넘 안좋아서 입맛이 없어요",
          "original_form": "요즘 날씨가 넘 안좋아서 입맛이 없어요",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014853.1.2",
          "form": "온종일 비만 주륵주륵",
          "original_form": "온종일 비만 주륵주륵",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014853.1.3",
          "form": "회사는 어떤가요?",
          "original_form": "회사는 어떤가요?",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100014853.1.4",
          "form": "늘 그 날이 그 날 같아요",
          "original_form": "늘 그 날이 그 날 같아요",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100014853.1.5",
          "form": "그래도 날이 선선해서 일하기 편하죠",
          "original_form": "그래도 날이 선선해서 일하기 편하죠",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100014853.1.6",
          "form": "내일 더운 여름에 아무래도 지치기 쉬운데",
          "original_form": "내일 더운 여름에 아무래도 지치기 쉬운데",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100014853.1.7",
          "form": "요즘 선선해서 참 좋아요",
          "original_form": "요즘 선선해서 참 좋아요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100014853.1.8",
          "form": "태풍이 또 올라온다 더라고요",
          "original_form": "태풍이 또 올라온다 더라고요",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100014853.1.9",
          "form": "뉴스 봤어요 걱정이에요",
          "original_form": "뉴스 봤어요 걱정이에요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100014853.1.10",
          "form": "명절 앞두고 큰 피해 없어야 할 텐데",
          "original_form": "명절 앞두고 큰 피해 없어야 할 텐데",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100014853.1.11",
          "form": "요즘 계속 비 오고 쌀쌀하더니 태풍까지 온대서",
          "original_form": "요즘 계속 비 오고 쌀쌀하더니 태풍까지 온대서",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100014853.1.12",
          "form": "맞아요 바람이 차갑지요",
          "original_form": "맞아요 바람이 차갑지요",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100014853.1.13",
          "form": "네 그래서 옷 입기가 애매해요",
          "original_form": "네 그래서 옷 입기가 애매해요",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100014853.1.14",
          "form": "여름 옷 입기 춥고 가을 옷 입기 덥고",
          "original_form": "여름 옷 입기 춥고 가을 옷 입기 덥고",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100014853.1.15",
          "form": "마쟈용",
          "original_form": "마쟈용",
          "speaker_id": "1",
