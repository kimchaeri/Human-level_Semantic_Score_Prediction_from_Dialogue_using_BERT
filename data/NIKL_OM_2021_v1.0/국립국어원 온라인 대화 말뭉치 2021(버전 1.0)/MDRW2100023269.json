      "utterance": [
        {
          "id": "MDRW2100023269.1.1",
          "form": "name2은 책 자주 읽으세요?",
          "original_form": "&name2&은 책 자주 읽으세요?",
          "speaker_id": "1",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023269.1.2",
          "form": "저는 뭐든 읽는걸 별로 안좋아해서 독서를 자주 하진 않아요!",
          "original_form": "저는 뭐든 읽는걸 별로 안좋아해서 독서를 자주 하진 않아요!",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023269.1.3",
          "form": "최근에 백일장때문에 읽은 책이 있긴한데",
          "original_form": "최근에 백일장때문에 읽은 책이 있긴한데",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023269.1.4",
          "form": "'면역에 관하여' 라는 책이에요! 백신에 대해서 말해주고 있더라구요!",
          "original_form": "'면역에 관하여' 라는 책이에요! 백신에 대해서 말해주고 있더라구요!",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023269.1.5",
          "form": "오 백일장 나가시려고요?",
          "original_form": "오 백일장 나가시려고요?",
          "speaker_id": "1",
          "time": "20210830 08:04"
        },
        {
          "id": "MDRW2100023269.1.6",
          "form": "이미 냈는데 그냥 참가상만 받았어요 ㅋㅋㅋㅋ",
          "original_form": "이미 냈는데 그냥 참가상만 받았어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:52"
        },
        {
          "id": "MDRW2100023269.1.7",
          "form": "원래 그걸 노리긴 했지만...",
          "original_form": "원래 그걸 노리긴 했지만...",
          "speaker_id": "2",
          "time": "20210830 09:52"
        },
        {
          "id": "MDRW2100023269.1.8",
          "form": "그래도 상 받았다니 뿌듯하셨겠네요!",
          "original_form": "그래도 상 받았다니 뿌듯하셨겠네요!",
          "speaker_id": "1",
          "time": "20210830 10:54"
        },
        {
          "id": "MDRW2100023269.1.9",
          "form": "전 사랑의 은어라는 책 읽고 싶어서 오늘 읽으려고요",
          "original_form": "전 사랑의 은어라는 책 읽고 싶어서 오늘 읽으려고요",
          "speaker_id": "1",
          "time": "20210830 10:57"
        },
        {
          "id": "MDRW2100023269.1.10",
          "form": "오 독서를 꾸준히 하시나봐요!",
          "original_form": "오 독서를 꾸준히 하시나봐요!",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023269.1.11",
          "form": "사랑의 은어는 어떤 책인가요?",
          "original_form": "사랑의 은어는 어떤 책인가요?",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023269.1.12",
          "form": "꾸준히까지는 아닌데 이제부터 자주 읽으려고요ㅋㅋ",
          "original_form": "꾸준히까지는 아닌데 이제부터 자주 읽으려고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023269.1.13",
          "form": "아직 안 읽어봤는데 서한나 작가가 쓴 신작이라 믿고 보는 거예요",
          "original_form": "아직 안 읽어봤는데 서한나 작가가 쓴 신작이라 믿고 보는 거예요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023269.1.14",
          "form": "서한나 작가를 좋아하시는군요...!",
          "original_form": "서한나 작가를 좋아하시는군요...!",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100023269.1.15",
          "form": "저는 이제보니 언니가 기욤뮈소를 좋아해서 책 한권 읽었던 기억이 있네요!",
          "original_form": "저는 이제보니 언니가 기욤뮈소를 좋아해서 책 한권 읽었던 기억이 있네요!",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023269.1.16",
          "form": "기욤뮈소 책 하나 읽어봤었어요 글을 되게 영화 같이 쓰는 작가 같아요",
          "original_form": "기욤뮈소 책 하나 읽어봤었어요 글을 되게 영화 같이 쓰는 작가 같아요",
          "speaker_id": "1",
          "time": "20210831 09:30"
        },
        {
          "id": "MDRW2100023269.1.17",
          "form": "맞아요 반전도 많고 기억에 남을만한 글을 쓰는 것 같아요",
          "original_form": "맞아요 반전도 많고 기억에 남을만한 글을 쓰는 것 같아요",
          "speaker_id": "2",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100023269.1.18",
          "form": "역시 읽어보셨군요!",
          "original_form": "역시 읽어보셨군요!",
          "speaker_id": "2",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100023269.1.19",
          "form": "맞아요 시집은 좋아하세요?",
          "original_form": "맞아요 시집은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100023269.1.20",
          "form": "시집은 어렸을때 동화시집? 이런 것 말고는 읽어본 적이 없어요ㅜㅜ",
          "original_form": "시집은 어렸을때 동화시집? 이런 것 말고는 읽어본 적이 없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100023269.1.21",
          "form": "시집 좋아하시나요??",
          "original_form": "시집 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210831 11:21"
        },
        {
          "id": "MDRW2100023269.1.22",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100023269.1.23",
          "form": "우와 좋아하는 시인이 있으신가요?",
          "original_form": "우와 좋아하는 시인이 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100023269.1.24",
          "form": "저는 한강 작가가 쓴 시랑 조계숙 시인 좋아해요",
          "original_form": "저는 한강 작가가 쓴 시랑 조계숙 시인 좋아해요",
          "speaker_id": "1",
          "time": "20210831 15:28"
        }
      ]
    }
