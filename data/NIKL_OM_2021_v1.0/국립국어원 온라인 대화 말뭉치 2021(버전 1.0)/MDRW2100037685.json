      "utterance": [
        {
          "id": "MDRW2100037685.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 16:02"
        },
        {
          "id": "MDRW2100037685.1.2",
          "form": "저희 언니가 제 홀맨키링보면 놀리네요ㅠ",
          "original_form": "저희 언니가 제 홀맨키링보면 놀리네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100037685.1.3",
          "form": "뭐라고 놀리죠?",
          "original_form": "뭐라고 놀리죠?",
          "speaker_id": "2",
          "time": "20210901 16:16"
        },
        {
          "id": "MDRW2100037685.1.4",
          "form": "발잘린거 같대요ㅠㅠ 잔인하죠?ㅠ",
          "original_form": "발잘린거 같대요ㅠㅠ 잔인하죠?ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100037685.1.5",
          "form": "ㅠㅠ 좀 잔인한 평가이긴하네요",
          "original_form": "ㅠㅠ 좀 잔인한 평가이긴하네요",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100037685.1.6",
          "form": "근데 솔직히 저도 딱보고 그생각이 났어요ㅋㅋㅋ",
          "original_form": "근데 솔직히 저도 딱보고 그생각이 났어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:21"
        },
        {
          "id": "MDRW2100037685.1.7",
          "form": "ㅜㅠ 왜 고리를 발에다가 달은걸까요?",
          "original_form": "ㅜㅠ 왜 고리를 발에다가 달은걸까요?",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100037685.1.8",
          "form": "그쵸 고리 색이라도 철로 만들면 좋았을텐데",
          "original_form": "그쵸 고리 색이라도 철로 만들면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100037685.1.9",
          "form": "고리 색을 철로 한다구요?",
          "original_form": "고리 색을 철로 한다구요?",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037685.1.10",
          "form": "발에 고리가 흰이거든여 그래서 발길이 짝짝이처럼 보여요ㅋㅋ",
          "original_form": "발에 고리가 흰이거든여 그래서 발길이 짝짝이처럼 보여요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:23"
        },
        {
          "id": "MDRW2100037685.1.11",
          "form": "아ㅋㅋㅋㅋ 무슨 뜻인지 알거 같아요",
          "original_form": "아ㅋㅋㅋㅋ 무슨 뜻인지 알거 같아요",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037685.1.12",
          "form": "발에 고리 있는게 여러모로 아쉬운 부분이네요",
          "original_form": "발에 고리 있는게 여러모로 아쉬운 부분이네요",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100037685.1.13",
          "form": "네맞아요ㅠㅠ",
          "original_form": "네맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100037685.1.14",
          "form": "키링은 써요?",
          "original_form": "키링은 써요?",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100037685.1.15",
          "form": "아직이요 아직 포장을 안 뜯었어요",
          "original_form": "아직이요 아직 포장을 안 뜯었어요",
          "speaker_id": "2",
          "time": "20210901 17:27"
        }
      ]
