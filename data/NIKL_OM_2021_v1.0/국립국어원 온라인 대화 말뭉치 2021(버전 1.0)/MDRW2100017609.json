      "utterance": [
        {
          "id": "MDRW2100017609.1.1",
          "form": "맛집 다니는 거 좋아하세요?",
          "original_form": "맛집 다니는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017609.1.2",
          "form": "네 ㅋㅋ 빵좋아해서 빵집투어간적있어요",
          "original_form": "네 ㅋㅋ 빵좋아해서 빵집투어간적있어요",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017609.1.3",
          "form": "빵 좋아하시는구나!",
          "original_form": "빵 좋아하시는구나!",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017609.1.4",
          "form": "무슨 빵 제일 좋아하세요?",
          "original_form": "무슨 빵 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017609.1.5",
          "form": "저는 사워도우계열좋아해요~ 특히 치아바타! 좋아합니당",
          "original_form": "저는 사워도우계열좋아해요~ 특히 치아바타! 좋아합니당",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017609.1.6",
          "form": "오 치아바타 저도 좋아해요",
          "original_form": "오 치아바타 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017609.1.7",
          "form": "저는 양념? 없는 플레인빵? 좋아해요",
          "original_form": "저는 양념? 없는 플레인빵? 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017609.1.8",
          "form": "오~ 통했네요!",
          "original_form": "오~ 통했네요!",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017609.1.9",
          "form": "고소한 밀가루맛 좋아해요",
          "original_form": "고소한 밀가루맛 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017609.1.10",
          "form": "저두요! 밀가루맛",
          "original_form": "저두요! 밀가루맛",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017609.1.11",
          "form": "혹시 코엑스 쪽 빵맛집이 있나요??",
          "original_form": "혹시 코엑스 쪽 빵맛집이 있나요??",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017609.1.12",
          "form": "음 제가 서울에 안살아서 수도권쪽은 잘모르겠어요ㅜ",
          "original_form": "음 제가 서울에 안살아서 수도권쪽은 잘모르겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017609.1.13",
          "form": "그러시군요 ㅠ",
          "original_form": "그러시군요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017609.1.14",
          "form": "또 어떤 음식좋아하세요?",
          "original_form": "또 어떤 음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017609.1.15",
          "form": "저 아이스크림좋아해요~",
          "original_form": "저 아이스크림좋아해요~",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017609.1.16",
          "form": "베라 최애 맛 3개가 뭔가요?",
          "original_form": "베라 최애 맛 3개가 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017609.1.17",
          "form": "제3픽은 피스타치오아몬드 쿠앤크그리고 아몬드봉봉이요ㅎㅎ",
          "original_form": "제3픽은 피스타치오아몬드 쿠앤크그리고 아몬드봉봉이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017609.1.18",
          "form": "대박 저랑 잘맞으시네요!",
          "original_form": "대박 저랑 잘맞으시네요!",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017609.1.19",
          "form": "저도 그 3개에요",
          "original_form": "저도 그 3개에요",
          "speaker_id": "2",
          "time": "20210826 20:40"
        }
      ]
