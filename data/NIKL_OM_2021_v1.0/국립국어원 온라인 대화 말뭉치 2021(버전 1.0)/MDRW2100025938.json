      "utterance": [
        {
          "id": "MDRW2100025938.1.1",
          "form": "유투브 자주 보는거 있으신가요?",
          "original_form": "유투브 자주 보는거 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100025938.1.2",
          "form": "매탈남 이라고 고양이 채널 좋아해요!",
          "original_form": "매탈남 이라고 고양이 채널 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025938.1.3",
          "form": "좋아하는 채널 있나요?",
          "original_form": "좋아하는 채널 있나요?",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025938.1.4",
          "form": "오 요즘 이사간거 보고 있어요 저도",
          "original_form": "오 요즘 이사간거 보고 있어요 저도",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025938.1.5",
          "form": "파란 벽지? 너무 시강에요 ㅋㅋ",
          "original_form": "파란 벽지? 너무 시강에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100025938.1.6",
          "form": "파이프에서 구해오는거 너무 눈물났어요",
          "original_form": "파이프에서 구해오는거 너무 눈물났어요",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100025938.1.7",
          "form": "그쵸 ㅋㅋㅋㅋ 파란 벽지 분홍색 벽지 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ 파란 벽지 분홍색 벽지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025938.1.8",
          "form": "파이프에서 구해오는 편은 진짜 봐도봐도 너무 뭉클해요ㅠㅠ",
          "original_form": "파이프에서 구해오는 편은 진짜 봐도봐도 너무 뭉클해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025938.1.9",
          "form": "ㅋㅋㅋㅋㅋ 냇가가 집근처 있어서 부러웠어요",
          "original_form": "ㅋㅋㅋㅋㅋ 냇가가 집근처 있어서 부러웠어요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025938.1.10",
          "form": "전 애교많은 꼬리? 젤 좋아해요",
          "original_form": "전 애교많은 꼬리? 젤 좋아해요",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025938.1.11",
          "form": "ㅠㅠ 어째 그 먼데를 따라 오라고 했는지",
          "original_form": "ㅠㅠ 어째 그 먼데를 따라 오라고 했는지",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025938.1.12",
          "form": "애들 진짜 하나하나 다 매력 넘치고 예쁘죠ㅠㅠ",
          "original_form": "애들 진짜 하나하나 다 매력 넘치고 예쁘죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025938.1.13",
          "form": "저는 요즘 쪼꾸미한테도 계속 눈이 가요 ㅎㅎ",
          "original_form": "저는 요즘 쪼꾸미한테도 계속 눈이 가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025938.1.14",
          "form": "할아버지 못알아보고 도망가는 쪼꾸미 웃겼어요 ㅋㅋㅋ",
          "original_form": "할아버지 못알아보고 도망가는 쪼꾸미 웃겼어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025938.1.15",
          "form": "왕서운",
          "original_form": "왕서운",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025938.1.16",
          "form": "맞아요 ㅋㅋㅋㅋ 당연히 냄새는 기억할거라고 생각했는데 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 당연히 냄새는 기억할거라고 생각했는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100025938.1.17",
          "form": "애기라서 그런가봐요ㅠ",
          "original_form": "애기라서 그런가봐요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100025938.1.18",
          "form": "ㅋㅋㅋㅋ 할아버지는 계속 같이 사시는건가요? 긴가민가 다시 집에 가시는건지",
          "original_form": "ㅋㅋㅋㅋ 할아버지는 계속 같이 사시는건가요? 긴가민가 다시 집에 가시는건지",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100025938.1.19",
          "form": "저는 같이 사시는 거라고 생각했는데, 최근 영상에 좀 있으면 누나 집으로 가신다고 하셨던 것도 같고...",
          "original_form": "저는 같이 사시는 거라고 생각했는데, 최근 영상에 좀 있으면 누나 집으로 가신다고 하셨던 것도 같고...",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100025938.1.20",
          "form": "같이 계셨으면 좋겠어요 ㅎㅎ\r\n고양이들 할머니 할아버지 좋아하던데 ㅎㅎ",
          "original_form": "같이 계셨으면 좋겠어요 ㅎㅎ\r\n고양이들 할머니 할아버지 좋아하던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100025938.1.21",
          "form": "그러게 말이에요 ㅎㅎ",
          "original_form": "그러게 말이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100025938.1.22",
          "form": "저는 두마리 키우는것도 힘든데",
          "original_form": "저는 두마리 키우는것도 힘든데",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100025938.1.23",
          "form": "정말 대단하신 매탈남님 ㄷㄷ",
          "original_form": "정말 대단하신 매탈남님 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100025938.1.24",
          "form": "두 마리 키우시는 것도 대단한데요!",
          "original_form": "두 마리 키우시는 것도 대단한데요!",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100025938.1.25",
          "form": "저는 한 마리 키워요 ㅎㅎ",
          "original_form": "저는 한 마리 키워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100025938.1.26",
          "form": "영상 볼 때 애들도 화면 보나요??",
          "original_form": "영상 볼 때 애들도 화면 보나요??",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100025938.1.27",
          "form": "오 집사님이셨군요!!!",
          "original_form": "오 집사님이셨군요!!!",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100025938.1.28",
          "form": "종이 뭐에요 ㅎㅎ",
          "original_form": "종이 뭐에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100025938.1.29",
          "form": "소리나면? 두리번 거리더라구요 ㅋㅋㅋ",
          "original_form": "소리나면? 두리번 거리더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100025938.1.30",
          "form": "코숏이에요!",
          "original_form": "코숏이에요!",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100025938.1.31",
          "form": "저희 애는 무니 좋아하는 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "저희 애는 무니 좋아하는 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100025938.1.32",
          "form": "낭만 고양이 채널이라고 하셨나요??",
          "original_form": "낭만 고양이 채널이라고 하셨나요??",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100025938.1.33",
          "form": "50마리라니 대단하시네요!",
          "original_form": "50마리라니 대단하시네요!",
          "speaker_id": "2",
