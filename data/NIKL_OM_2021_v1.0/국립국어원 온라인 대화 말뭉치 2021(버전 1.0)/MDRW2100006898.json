      "utterance": [
        {
          "id": "MDRW2100006898.1.1",
          "form": "사시는 곳에 오늘 비 왔나요?",
          "original_form": "사시는 곳에 오늘 비 왔나요?",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100006898.1.2",
          "form": "네 오늘 많이 왔어요!",
          "original_form": "네 오늘 많이 왔어요!",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006898.1.3",
          "form": "하늘에 구멍 뚫린 줄 알았습니당ㅠㅠ",
          "original_form": "하늘에 구멍 뚫린 줄 알았습니당ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006898.1.4",
          "form": "사시는 쪽에 비 많이 왔나요?",
          "original_form": "사시는 쪽에 비 많이 왔나요?",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006898.1.5",
          "form": "네ㅠㅠ 엄청 왔어요",
          "original_form": "네ㅠㅠ 엄청 왔어요",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006898.1.6",
          "form": "무섭더라고요",
          "original_form": "무섭더라고요",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006898.1.7",
          "form": "헉ㅠ 올해는 진짜 비가 좀 많이 오려나봐요ㅠㅠ",
          "original_form": "헉ㅠ 올해는 진짜 비가 좀 많이 오려나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006898.1.8",
          "form": "그러게요 가을 장마라 하던데ㅠ",
          "original_form": "그러게요 가을 장마라 하던데ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006898.1.9",
          "form": "여름도 끝났나봐요",
          "original_form": "여름도 끝났나봐요",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006898.1.10",
          "form": "여름에 진짜 너무 더웠는데 이제는 밤에 춥더라구요",
          "original_form": "여름에 진짜 너무 더웠는데 이제는 밤에 춥더라구요",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006898.1.11",
          "form": "이불 덮고 자고 있습니당ㅋㅋㅠ",
          "original_form": "이불 덮고 자고 있습니당ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006898.1.12",
          "form": "헉 저도요ㅋㅋㅋㅋ 그렇게 덥던 게",
          "original_form": "헉 저도요ㅋㅋㅋㅋ 그렇게 덥던 게",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006898.1.13",
          "form": "요즘은 선풍기도 세게 안 틀어요",
          "original_form": "요즘은 선풍기도 세게 안 틀어요",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006898.1.14",
          "form": "여름 내내 에어컨 달고 살았는데 이제는 선풍기도 잠깐씩만 틀어도 괜찮더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "여름 내내 에어컨 달고 살았는데 이제는 선풍기도 잠깐씩만 틀어도 괜찮더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006898.1.15",
          "form": "맞아요ㅋㅋㅋㅋ 여름엔 그렇게 에어컨 안 틀면 못 살겠던데",
          "original_form": "맞아요ㅋㅋㅋㅋ 여름엔 그렇게 에어컨 안 틀면 못 살겠던데",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006898.1.16",
          "form": "이렇게 급 선선해지다니 신기해요",
          "original_form": "이렇게 급 선선해지다니 신기해요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006898.1.17",
          "form": "이제 다시 따뜻한 물로 샤워할 수 있는 게 좋더라구요",
          "original_form": "이제 다시 따뜻한 물로 샤워할 수 있는 게 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006898.1.18",
          "form": "헐 그렇군요",
          "original_form": "헐 그렇군요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006898.1.19",
          "form": "저는 찬물샤워 즐기는 파라서",
          "original_form": "저는 찬물샤워 즐기는 파라서",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006898.1.20",
          "form": "오 대단하세요 저는 나이먹어서... 찬물은 좀 힘들어요ㅋㅋㅋ큐",
          "original_form": "오 대단하세요 저는 나이먹어서... 찬물은 좀 힘들어요ㅋㅋㅋ큐",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006898.1.21",
          "form": "악ㅋㅋㅋㅋㅋ ㅠㅠ 저도 좀 자라면서 따뜻한 물로 샤워하는 걸 즐기긴 했는데",
          "original_form": "악ㅋㅋㅋㅋㅋ ㅠㅠ 저도 좀 자라면서 따뜻한 물로 샤워하는 걸 즐기긴 했는데",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006898.1.22",
          "form": "찬물샤워가 아직 별로진 않더라고요",
          "original_form": "찬물샤워가 아직 별로진 않더라고요",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006898.1.23",
          "form": "그쵸 지금은 딱 시원하게 할 수 있겠어요ㅋㅋㅋㅋ",
          "original_form": "그쵸 지금은 딱 시원하게 할 수 있겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        }
      ]
    }
