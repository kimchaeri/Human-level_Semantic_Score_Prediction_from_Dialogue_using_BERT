      "utterance": [
        {
          "id": "MDRW2100037823.1.1",
          "form": "부산 맛집 소개좀해주세요 ㅎㅎ",
          "original_form": "부산 맛집 소개좀해주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037823.1.2",
          "form": "쭈꾸미 좋아하세요??!",
          "original_form": "쭈꾸미 좋아하세요??!",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037823.1.3",
          "form": "네 좋아해요~",
          "original_form": "네 좋아해요~",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037823.1.4",
          "form": "그러면 윤상옥신주꾸미 추천합니다 ㅎㅎ !",
          "original_form": "그러면 윤상옥신주꾸미 추천합니다 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037823.1.5",
          "form": "오 감사합니다 기억해둘게요",
          "original_form": "오 감사합니다 기억해둘게요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037823.1.6",
          "form": "넵 ㅎㅎ 사상쪽이여서 좀 더 가까우실꺼에요 ㅎㅎ",
          "original_form": "넵 ㅎㅎ 사상쪽이여서 좀 더 가까우실꺼에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100037823.1.7",
          "form": "어머 세심하시네요 감사합니다 ㅎㅎ",
          "original_form": "어머 세심하시네요 감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100037823.1.8",
          "form": "ㅎㅎ 아니에요 !",
          "original_form": "ㅎㅎ 아니에요 !",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100037823.1.9",
          "form": "주로 배달음식 드세요?",
          "original_form": "주로 배달음식 드세요?",
          "speaker_id": "1",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100037823.1.10",
          "form": "아뇨 배달은 잘안해요",
          "original_form": "아뇨 배달은 잘안해요",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100037823.1.11",
          "form": "오늘은 피자시켰네요",
          "original_form": "오늘은 피자시켰네요",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100037823.1.12",
          "form": "헐 어디피자요?",
          "original_form": "헐 어디피자요?",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100037823.1.13",
          "form": "파파존스에서 시켰어요",
          "original_form": "파파존스에서 시켰어요",
          "speaker_id": "2",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100037823.1.14",
          "form": "헐 저 파파존스 안먹어봤는데",
          "original_form": "헐 저 파파존스 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100037823.1.15",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100037823.1.16",
          "form": "파파존스 괜찮아요 가든스페셜에 치즈 추가해서 드셔보세요",
          "original_form": "파파존스 괜찮아요 가든스페셜에 치즈 추가해서 드셔보세요",
          "speaker_id": "2",
