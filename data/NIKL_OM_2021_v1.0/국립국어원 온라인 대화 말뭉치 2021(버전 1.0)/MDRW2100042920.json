      "utterance": [
        {
          "id": "MDRW2100042920.1.1",
          "form": "완전조아",
          "original_form": "완전조아",
          "speaker_id": "2",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100042920.1.2",
          "form": "마켓컬리 안사고 저거만 샀어?",
          "original_form": "마켓컬리 안사고 저거만 샀어?",
          "speaker_id": "1",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100042920.1.3",
          "form": "응 저거만샀어",
          "original_form": "응 저거만샀어",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042920.1.4",
          "form": "잘했네..",
          "original_form": "잘했네..",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042920.1.5",
          "form": "바디로션사고프다",
          "original_form": "바디로션사고프다",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042920.1.6",
          "form": "향조은거모읍냐",
          "original_form": "향조은거모읍냐",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042920.1.7",
          "form": "바디판타지 웨딩데이",
          "original_form": "바디판타지 웨딩데이",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042920.1.8",
          "form": "오 무슨향임",
          "original_form": "오 무슨향임",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042920.1.9",
          "form": "백합?..꽃향기",
          "original_form": "백합?..꽃향기",
          "speaker_id": "2",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042920.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042920.1.11",
          "form": "나 아빠 생선도 사야하는데",
          "original_form": "나 아빠 생선도 사야하는데",
          "speaker_id": "1",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042920.1.12",
          "form": "마땅한게없네",
          "original_form": "마땅한게없네",
          "speaker_id": "1",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042920.1.13",
          "form": "워치는?",
          "original_form": "워치는?",
          "speaker_id": "2",
          "time": "20210907 13:04"
        },
        {
          "id": "MDRW2100042920.1.14",
          "form": "어른들이 의외로 안쓴대서",
          "original_form": "어른들이 의외로 안쓴대서",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042920.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042920.1.16",
          "form": "저번에 신세계그거 배송시작했나벼",
          "original_form": "저번에 신세계그거 배송시작했나벼",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042920.1.17",
          "form": "너 핫도그산거",
          "original_form": "너 핫도그산거",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042920.1.18",
          "form": "아 그래?나도봐야지",
          "original_form": "아 그래?나도봐야지",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042920.1.19",
          "form": "난 아직..",
          "original_form": "난 아직..",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042920.1.20",
          "form": "모야 다들 배송시작이랬는데",
          "original_form": "모야 다들 배송시작이랬는데",
          "speaker_id": "1",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100042920.1.21",
          "form": "폰바꾸고싶은데 아이폰 예상가격 넘 비싸다",
          "original_form": "폰바꾸고싶은데 아이폰 예상가격 넘 비싸다",
          "speaker_id": "1",
          "time": "20210907 13:14"
        },
        {
          "id": "MDRW2100042920.1.22",
          "form": "얼마랬지",
          "original_form": "얼마랬지",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100042920.1.23",
          "form": "나는 아이폰은 생각이음써..",
          "original_form": "나는 아이폰은 생각이음써..",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100042920.1.24",
          "form": "나는 카메라랑 겜만해서 아이폰..갠차늠",
          "original_form": "나는 카메라랑 겜만해서 아이폰..갠차늠",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100042920.1.25",
          "form": "미니가 64기가 100..",
          "original_form": "미니가 64기가 100..",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100042920.1.26",
          "form": "256 이상만 품고싶은뎅",
          "original_form": "256 이상만 품고싶은뎅",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100042920.1.27",
          "form": "나아까",
          "original_form": "나아까",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100042920.1.28",
