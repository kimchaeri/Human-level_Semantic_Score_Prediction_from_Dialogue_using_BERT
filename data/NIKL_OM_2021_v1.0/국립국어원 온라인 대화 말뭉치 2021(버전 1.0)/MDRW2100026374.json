      "utterance": [
        {
          "id": "MDRW2100026374.1.1",
          "form": "정답이 뭐였어???",
          "original_form": "정답이 뭐였어???",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026374.1.2",
          "form": "나못봄",
          "original_form": "나못봄",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026374.1.3",
          "form": "정답은 스시였습니다",
          "original_form": "정답은 스시였습니다",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026374.1.4",
          "form": "핫하",
          "original_form": "핫하",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026374.1.5",
          "form": "육류잖아",
          "original_form": "육류잖아",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026374.1.6",
          "form": "육류와 해산물은 다른 품목이랍니다",
          "original_form": "육류와 해산물은 다른 품목이랍니다",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026374.1.7",
          "form": "젠장!!!!!",
          "original_form": "젠장!!!!!",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026374.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026374.1.9",
          "form": "맞추기 놀이 하니까 빨리 찬다",
          "original_form": "맞추기 놀이 하니까 빨리 찬다",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026374.1.10",
          "form": "먹을거 얘기 하니까 마음이 두둑해지네",
          "original_form": "먹을거 얘기 하니까 마음이 두둑해지네",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026374.1.11",
          "form": "이번엔 니가 생각하는 음식 맞춰볼게",
          "original_form": "이번엔 니가 생각하는 음식 맞춰볼게",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026374.1.12",
          "form": "너무힘드네 육류인지 해산물인지도",
          "original_form": "너무힘드네 육류인지 해산물인지도",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026374.1.13",
          "form": "갈래가 꽤나 나뉜다",
          "original_form": "갈래가 꽤나 나뉜다",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.14",
          "form": "좋아 나 정했어",
          "original_form": "좋아 나 정했어",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.15",
          "form": "좋아 간다",
          "original_form": "좋아 간다",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.16",
          "form": "양식이야?",
          "original_form": "양식이야?",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.17",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.18",
          "form": "한식이야?",
          "original_form": "한식이야?",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.19",
          "form": "한식이야",
          "original_form": "한식이야",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.20",
          "form": "돼지고기 들어가?",
          "original_form": "돼지고기 들어가?",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026374.1.21",
          "form": "응 들어가 자주",
          "original_form": "응 들어가 자주",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026374.1.22",
          "form": "자주 들어간다면 반대로 안 들어갈 때도 있다는 거네?",
          "original_form": "자주 들어간다면 반대로 안 들어갈 때도 있다는 거네?",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026374.1.23",
          "form": "돼지고기 없는 버전도 있다는거지?",
          "original_form": "돼지고기 없는 버전도 있다는거지?",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026374.1.24",
          "form": "응 맞아 안들어갈때도있어",
          "original_form": "응 맞아 안들어갈때도있어",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026374.1.25",
          "form": "빨간 색이야?",
          "original_form": "빨간 색이야?",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026374.1.26",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026374.1.27",
          "form": "빨간색음식이야",
          "original_form": "빨간색음식이야",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026374.1.28",
          "form": "야채 들어가?",
          "original_form": "야채 들어가?",
