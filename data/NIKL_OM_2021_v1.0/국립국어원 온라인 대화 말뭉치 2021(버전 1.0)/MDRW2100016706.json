        },
        {
          "id": "MDRW2100016706.1.3",
          "form": "화이자",
          "original_form": "화이자",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016706.1.4",
          "form": "머리기르고싶으세요?",
          "original_form": "머리기르고싶으세요?",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016706.1.5",
          "form": "오 언니 화이자 맞는거야?",
          "original_form": "오 언니 화이자 맞는거야?",
          "speaker_id": "1",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016706.1.6",
          "form": "네...머리 왜 안길지... 지겨워죽겠어 단발",
          "original_form": "네...머리 왜 안길지... 지겨워죽겠어 단발",
          "speaker_id": "1",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016706.1.7",
          "form": "나랑생각이좀다르군 ㅋㅋㅋ난그냥안자라고되겟는데긔차나또잘라야하자나",
          "original_form": "나랑생각이좀다르군 ㅋㅋㅋ난그냥안자라고되겟는데긔차나또잘라야하자나",
          "speaker_id": "2",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016706.1.8",
          "form": "ㅋㅋㅋ아니 머리 좀 길러서 다이슨 해봐야지요!!",
          "original_form": "ㅋㅋㅋ아니 머리 좀 길러서 다이슨 해봐야지요!!",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016706.1.9",
          "form": "아 ㅋㅋㅋㅋ그럼 엄청길어야할틴디..",
          "original_form": "아 ㅋㅋㅋㅋ그럼 엄청길어야할틴디..",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016706.1.10",
          "form": "나 머리자르고 ㅋㅋㅋ완전 붕뜰꺼라햇는데",
          "original_form": "나 머리자르고 ㅋㅋㅋ완전 붕뜰꺼라햇는데",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016706.1.11",
          "form": "ㅋㅋㅋ진짜 쇄골까지만 와도 티날거같은데",
          "original_form": "ㅋㅋㅋ진짜 쇄골까지만 와도 티날거같은데",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.12",
          "form": "내가 층이 맘에들어서 주기룰 어찌둘까물어봣능데",
          "original_form": "내가 층이 맘에들어서 주기룰 어찌둘까물어봣능데",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.14",
          "form": "2달후면",
          "original_form": "2달후면",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.15",
          "form": "지저분하다",
          "original_form": "지저분하다",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.16",
          "form": "느껴질수있다는데",
          "original_form": "느껴질수있다는데",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.17",
          "form": "또 되려 좀차분해지니깐 내가원하는느낌이더 살꺼라햇어",
          "original_form": "또 되려 좀차분해지니깐 내가원하는느낌이더 살꺼라햇어",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016706.1.18",
          "form": "벌써 머리자른지 두 달 되었나요?",
          "original_form": "벌써 머리자른지 두 달 되었나요?",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.19",
          "form": "자루면서면서 좀더길때자르명 더 이쁘겟다햇는데",
          "original_form": "자루면서면서 좀더길때자르명 더 이쁘겟다햇는데",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.20",
          "form": "아뇨 ㅋㅋ요번달초에짤랏나?",
          "original_form": "아뇨 ㅋㅋ요번달초에짤랏나?",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.21",
          "form": "아닌가 저번달초엿나..",
          "original_form": "아닌가 저번달초엿나..",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.22",
          "form": "무튼 근데 지굼 감고 보면",
          "original_form": "무튼 근데 지굼 감고 보면",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.23",
          "form": "맘에듬 ㅋㅋㅋㅋ",
          "original_form": "맘에듬 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.24",
          "form": "이게내요지야",
          "original_form": "이게내요지야",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016706.1.26",
          "form": "ㅋㅋㅋㅋ현재 헤어스타일 만족",
          "original_form": "ㅋㅋㅋㅋ현재 헤어스타일 만족",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016706.1.27",
          "form": "전 불만족!! ㅋㅋㅋㅋ 그리고 맘에드는 미용실 찾는것도 힘들어",
          "original_form": "전 불만족!! ㅋㅋㅋㅋ 그리고 맘에드는 미용실 찾는것도 힘들어",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016706.1.28",
          "form": "그렇긴해",
          "original_form": "그렇긴해",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016706.1.29",
          "form": "가격은또 꽤비싸서",
          "original_form": "가격은또 꽤비싸서",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016706.1.30",
          "form": "그러니까! 물가가 센 편인가봐",
          "original_form": "그러니까! 물가가 센 편인가봐",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016706.1.31",
          "form": "그리고 비싸도 맘에 들면 참을 수 있는데 비싸면서 맘에 안들면 혼나야돼",
          "original_form": "그리고 비싸도 맘에 들면 참을 수 있는데 비싸면서 맘에 안들면 혼나야돼",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016706.1.32",
          "form": "여자컷트 가격도 더 받고",
          "original_form": "여자컷트 가격도 더 받고",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016706.1.33",
          "form": "진짜 여자 머리 남자 머리 따로 있나! 거참 너무해",
          "original_form": "진짜 여자 머리 남자 머리 따로 있나! 거참 너무해",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016706.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 노래같아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 노래같아",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016706.1.35",
          "form": "남자도 오빠가는데 18인가",
          "original_form": "남자도 오빠가는데 18인가",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016706.1.36",
          "form": "17인가 ㅋㅋㅋ",
          "original_form": "17인가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016706.1.37",
          "form": "여자는 얼만디?!",
          "original_form": "여자는 얼만디?!",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016706.1.38",
          "form": "거긴 15인가",
          "original_form": "거긴 15인가",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.39",
          "form": "오 여자가 더 싸? ㅋㅋㅋ",
          "original_form": "오 여자가 더 싸? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.40",
          "form": "난앞머리꼽사리껴져짜름 ㅋㅋ천원",
          "original_form": "난앞머리꼽사리껴져짜름 ㅋㅋ천원",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.41",
          "form": "여자늨몰라 ㅋㅋㅋ",
          "original_form": "여자늨몰라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.42",
          "form": "내가 오빠가는곳 별로라고",
          "original_form": "내가 오빠가는곳 별로라고",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.43",
          "form": "아파트 입주민이 오픈한곳 갔는데",
          "original_form": "아파트 입주민이 오픈한곳 갔는데",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.44",
          "form": "ㅋㅋㅋㅋ새로운 곳 뚫었나요",
          "original_form": "ㅋㅋㅋㅋ새로운 곳 뚫었나요",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016706.1.45",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016706.1.46",
          "form": "그 이제야..그날이왓어 ㅡㅡ",
          "original_form": "그 이제야..그날이왓어 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100016706.1.47",
          "form": "허리끊어지겟다 ㅋㅋㅋ발목도아프고ㅠㅠ",
