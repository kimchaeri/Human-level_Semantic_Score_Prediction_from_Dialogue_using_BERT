        },
        {
          "id": "MDRW2100014453.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014453.1.3",
          "form": "다이어트 해보신적 있으세요?",
          "original_form": "다이어트 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014453.1.4",
          "form": "엄청 해본적은 없고",
          "original_form": "엄청 해본적은 없고",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014453.1.5",
          "form": "간단히 운동 해본적은 있어요",
          "original_form": "간단히 운동 해본적은 있어요",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014453.1.6",
          "form": "무슨 운동 하셨어요?",
          "original_form": "무슨 운동 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014453.1.7",
          "form": "홈트보고 따라하는 정도만 해봤어요",
          "original_form": "홈트보고 따라하는 정도만 해봤어요",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014453.1.8",
          "form": "패션에 관심 있으세요?",
          "original_form": "패션에 관심 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014453.1.9",
          "form": "패션은 잘은 모르고",
          "original_form": "패션은 잘은 모르고",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014453.1.10",
          "form": "그냥 옷 조금 신경쓰는 정도에요",
          "original_form": "그냥 옷 조금 신경쓰는 정도에요",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014453.1.11",
          "form": "name1님은 관심 있으신가요?",
          "original_form": "&name1&님은 관심 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014453.1.12",
          "form": "저도 최근들어 조금씩 보고있어요 선호하는 옷 스타일 있으세요?",
          "original_form": "저도 최근들어 조금씩 보고있어요 선호하는 옷 스타일 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014453.1.13",
          "form": "놈코어?스타일인가",
          "original_form": "놈코어?스타일인가",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014453.1.14",
          "form": "간단한데 엄청 예뻐보이더라구요",
          "original_form": "간단한데 엄청 예뻐보이더라구요",
          "speaker_id": "2",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014453.1.15",
          "form": "심플하고 깔끔해보이네요!! 전 페미닌룩 좋아해요",
          "original_form": "심플하고 깔끔해보이네요!! 전 페미닌룩 좋아해요",
