      "utterance": [
        {
          "id": "MDRW2100020915.1.1",
          "form": "운동을 해아하는데 귀차니즘",
          "original_form": "운동을 해아하는데 귀차니즘",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020915.1.2",
          "form": "나도 그렇지 ㅎㅎ",
          "original_form": "나도 그렇지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020915.1.3",
          "form": "손목보호대를 드디어삼",
          "original_form": "손목보호대를 드디어삼",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020915.1.4",
          "form": "마음은 이미하지만",
          "original_form": "마음은 이미하지만",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020915.1.5",
          "form": "손목터지기전에 잘삼",
          "original_form": "손목터지기전에 잘삼",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020915.1.6",
          "form": "내손목 아작쓰",
          "original_form": "내손목 아작쓰",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020915.1.7",
          "form": "진짜 시리고 쑤시고 난리",
          "original_form": "진짜 시리고 쑤시고 난리",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020915.1.8",
          "form": "ㅋㅋ이미 병남",
          "original_form": "ㅋㅋ이미 병남",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020915.1.9",
          "form": "손목혼자 북치고 장구치고",
          "original_form": "손목혼자 북치고 장구치고",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020915.1.10",
          "form": "전 거북목",
          "original_form": "전 거북목",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020915.1.11",
          "form": "고질병",
          "original_form": "고질병",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020915.1.12",
          "form": "아 그건 나역시",
          "original_form": "아 그건 나역시",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.13",
          "form": "병 집합체",
          "original_form": "병 집합체",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.14",
          "form": "허리도 아작쓰",
          "original_form": "허리도 아작쓰",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.15",
          "form": "ㅋㅋ내허리ㄷᆢ",
          "original_form": "ㅋㅋ내허리ㄷᆢ",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.16",
          "form": "종합병원이야 ㅋㅋㅋ",
          "original_form": "종합병원이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.17",
          "form": "이미 갓음",
          "original_form": "이미 갓음",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.18",
          "form": "사요나라지ㅋㅋ",
          "original_form": "사요나라지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020915.1.19",
          "form": "ㅋㅋ 곧 오십견도 오겠지",
          "original_form": "ㅋㅋ 곧 오십견도 오겠지",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020915.1.20",
          "form": "이교정도 하고싶고 이제와서ㅋㅋ",
          "original_form": "이교정도 하고싶고 이제와서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020915.1.21",
          "form": "이제와서 ㅋㅋㅋㅋㅋ",
          "original_form": "이제와서 ㅋㅋㅋㅋㅋ",
