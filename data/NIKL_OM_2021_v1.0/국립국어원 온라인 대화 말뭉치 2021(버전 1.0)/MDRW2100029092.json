      "utterance": [
        {
          "id": "MDRW2100029092.1.1",
          "form": "침착맨 유투브에",
          "original_form": "침착맨 유투브에",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029092.1.2",
          "form": "심윤수 작가? 나온편 최근봤는데 잼더라구여",
          "original_form": "심윤수 작가? 나온편 최근봤는데 잼더라구여",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029092.1.3",
          "form": "그 사람 진짜 비범하던데",
          "original_form": "그 사람 진짜 비범하던데",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029092.1.4",
          "form": "심윤수 분이요?",
          "original_form": "심윤수 분이요?",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029092.1.5",
          "form": "네 정상 아닌듯요 ㅋㅋ",
          "original_form": "네 정상 아닌듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029092.1.6",
          "form": "ㅋㅋㅋㅋ 뭐든 고양이로 연결돼버림",
          "original_form": "ㅋㅋㅋㅋ 뭐든 고양이로 연결돼버림",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100029092.1.7",
          "form": "우무? 그거 진짜.....",
          "original_form": "우무? 그거 진짜.....",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029092.1.8",
          "form": "편집본 봤는데 우무는 뭐였을까요 ㅜㅜ",
          "original_form": "편집본 봤는데 우무는 뭐였을까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 08:07"
        },
        {
          "id": "MDRW2100029092.1.9",
          "form": "먹고",
          "original_form": "먹고",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029092.1.10",
          "form": "싸고",
          "original_form": "싸고",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029092.1.11",
          "form": "먹고",
          "original_form": "먹고",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029092.1.12",
