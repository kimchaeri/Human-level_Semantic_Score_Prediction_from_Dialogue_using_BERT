          "original_form": "싸고",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029092.1.13",
          "form": "이건데...흠",
          "original_form": "이건데...흠",
          "speaker_id": "2",
          "time": "20210903 08:13"
        },
        {
          "id": "MDRW2100029092.1.14",
          "form": "앜 ㅜㅜ 모르는게 나을뻔 ㅋㅋㅋㅋㅋ",
          "original_form": "앜 ㅜㅜ 모르는게 나을뻔 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:14"
        },
        {
          "id": "MDRW2100029092.1.15",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210903 08:16"
        },
        {
          "id": "MDRW2100029092.1.16",
          "form": "네 요새 디피 정주행중이에요",
          "original_form": "네 요새 디피 정주행중이에요",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029092.1.17",
          "form": "근데 6화짜리더라고요",
          "original_form": "근데 6화짜리더라고요",
          "speaker_id": "2",
          "time": "20210903 08:17"
        },
        {
          "id": "MDRW2100029092.1.18",
          "form": "오오 그거 재밌다고 난리던데",
          "original_form": "오오 그거 재밌다고 난리던데",
          "speaker_id": "1",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029092.1.19",
          "form": "군대이야기인거죠",
          "original_form": "군대이야기인거죠",
          "speaker_id": "1",
          "time": "20210903 08:19"
        },
        {
          "id": "MDRW2100029092.1.20",
          "form": "네 근데 고증도 잘하구",
          "original_form": "네 근데 고증도 잘하구",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029092.1.21",
          "form": "연기가 ㅎㄷㄷ",
          "original_form": "연기가 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029092.1.22",
          "form": "정해인이 잘하나요? 다들 잘하나바요",
          "original_form": "정해인이 잘하나요? 다들 잘하나바요",
          "speaker_id": "1",
          "time": "20210903 10:11"
        },
        {
          "id": "MDRW2100029092.1.23",
          "form": "다들 잘하고",
          "original_form": "다들 잘하고",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029092.1.24",
          "form": "구교환이 진짜 깐족거리는거 잘해요 ㅎㅎ",
          "original_form": "구교환이 진짜 깐족거리는거 잘해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:15"
        },
        {
          "id": "MDRW2100029092.1.25",
          "form": "한대치고 싶을것같아요 ㅋㅋ",
          "original_form": "한대치고 싶을것같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029092.1.26",
          "form": "한대 치고싶을듯 ㅋㅋ 내용도 리얼하나요?",
