        },
        {
          "id": "MDRW2100045311.1.2",
          "form": "안녕하세요,",
          "original_form": "안녕하세요,",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045311.1.3",
          "form": "일다니시나요??",
          "original_form": "일다니시나요??",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045311.1.4",
          "form": "네, 재택으로 하고 있어요.",
          "original_form": "네, 재택으로 하고 있어요.",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045311.1.5",
          "form": "아! ㅋㅋ",
          "original_form": "아! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045311.1.6",
          "form": "직업이 있으신가요?",
          "original_form": "직업이 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045311.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045311.1.8",
          "form": "저도 내일은 재택",
          "original_form": "저도 내일은 재택",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045311.1.9",
          "form": "어떤일하세요?",
          "original_form": "어떤일하세요?",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045311.1.10",
          "form": "홍보 마케팅 관련 일을 하고 있어요",
          "original_form": "홍보 마케팅 관련 일을 하고 있어요",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045311.1.11",
          "form": "요즘은 쉽지가 않네요. 아무리 온라인으로 해도 사람의 기본은 오프라인인데 .. ㅎㅎ",
          "original_form": "요즘은 쉽지가 않네요. 아무리 온라인으로 해도 사람의 기본은 오프라인인데 .. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045311.1.12",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045311.1.13",
          "form": "마케팅 영업 너무 어려워보여요",
          "original_form": "마케팅 영업 너무 어려워보여요",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045311.1.14",
          "form": "그러게요. 관심 받는 걸 좋아하지 않는데 어째서 이 일을 하게 된 건 지 ㅋㅋ",
          "original_form": "그러게요. 관심 받는 걸 좋아하지 않는데 어째서 이 일을 하게 된 건 지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045311.1.15",
          "form": "매개자라는 부분은 좋습니다.",
          "original_form": "매개자라는 부분은 좋습니다.",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045311.1.16",
          "form": "ㅋㅋ능력자포스가 느껴집니다!!!",
          "original_form": "ㅋㅋ능력자포스가 느껴집니다!!!",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045311.1.17",
          "form": "아니예요 ㅋㅋ 어떤 일 하고 계시나요?",
          "original_form": "아니예요 ㅋㅋ 어떤 일 하고 계시나요?",
