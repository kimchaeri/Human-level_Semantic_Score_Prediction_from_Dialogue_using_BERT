      "utterance": [
        {
          "id": "MDRW2100004570.1.1",
          "form": "네 동물 키우죠!",
          "original_form": "네 동물 키우죠!",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004570.1.2",
          "form": "어떤ㄱ요?",
          "original_form": "어떤ㄱ요?",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004570.1.3",
          "form": "고양이 1마리 키우고 있어요!",
          "original_form": "고양이 1마리 키우고 있어요!",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004570.1.4",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004570.1.5",
          "form": "ㅎㅎ 그럴수있죠",
          "original_form": "ㅎㅎ 그럴수있죠",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004570.1.6",
          "form": "고양이가 강제로 부지런하게 만든것도 있긴하죠",
          "original_form": "고양이가 강제로 부지런하게 만든것도 있긴하죠",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004570.1.7",
          "form": "집사가 되죠",
          "original_form": "집사가 되죠",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004570.1.8",
          "form": "맞아요 집사가 됬어요",
          "original_form": "맞아요 집사가 됬어요",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004570.1.9",
          "form": "고양이 애들이 워낙 까다로워서 흑",
          "original_form": "고양이 애들이 워낙 까다로워서 흑",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004570.1.10",
          "form": "관리 힘들죠?",
          "original_form": "관리 힘들죠?",
          "speaker_id": "1",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004570.1.11",
          "form": "네 힘들더라고요",
          "original_form": "네 힘들더라고요",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004570.1.12",
          "form": "털이 가장 큰 문제인듯요 ㅠㅠ",
          "original_form": "털이 가장 큰 문제인듯요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004570.1.13",
          "form": "네 맞아여",
          "original_form": "네 맞아여",
          "speaker_id": "1",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004570.1.14",
          "form": "그리고 배변하고 난뒤에 뒷처리도 살짝 힘들긴하지만",
          "original_form": "그리고 배변하고 난뒤에 뒷처리도 살짝 힘들긴하지만",
          "speaker_id": "2",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004570.1.15",
          "form": "그래도 애가 귀여워서 힘든게 다 녹네요 ㅎㅎ",
          "original_form": "그래도 애가 귀여워서 힘든게 다 녹네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004570.1.16",
          "form": "귀요미",
          "original_form": "귀요미",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004570.1.17",
          "form": "맞아요 귀여워요 ㅠㅠ",
          "original_form": "맞아요 귀여워요 ㅠㅠ",
          "speaker_id": "2",
