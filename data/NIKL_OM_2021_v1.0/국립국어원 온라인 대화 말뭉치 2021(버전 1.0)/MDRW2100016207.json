        },
        {
          "id": "MDRW2100016207.1.2",
          "form": "퇴근했습니다",
          "original_form": "퇴근했습니다",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100016207.1.3",
          "form": "강아지나 고양이 키우세요?",
          "original_form": "강아지나 고양이 키우세요?",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100016207.1.4",
          "form": "안녕하세요:)\r\n오늘 퇴근이 늦으셨네요",
          "original_form": "안녕하세요:)\r\n오늘 퇴근이 늦으셨네요",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100016207.1.5",
          "form": "저는 지금 키우고 있는\r\n반려동물은 없어요^^",
          "original_form": "저는 지금 키우고 있는\r\n반려동물은 없어요^^",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100016207.1.6",
          "form": "저도요 ㅋ",
          "original_form": "저도요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100016207.1.7",
          "form": "강아지 키우고 싶지만",
          "original_form": "강아지 키우고 싶지만",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100016207.1.8",
          "form": "관리불가일거 같아서,",
          "original_form": "관리불가일거 같아서,",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100016207.1.9",
          "form": "name2이랑\r\n비글 잘 어울릴거 같은데\r\n함 키워보시죵:)",
          "original_form": "&name2&이랑\r\n비글 잘 어울릴거 같은데\r\n함 키워보시죵:)",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100016207.1.10",
          "form": "비글이요?",
          "original_form": "비글이요?",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100016207.1.11",
          "form": "왜 비글?",
          "original_form": "왜 비글?",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100016207.1.12",
          "form": "name2도 비글 같으셔서요ㅋㅋ",
          "original_form": "&name2&도 비글 같으셔서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100016207.1.13",
          "form": "비글의 특징이 뭐죠?ㅋㅋ",
          "original_form": "비글의 특징이 뭐죠?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100016207.1.14",
          "form": "모든 면에서 매우 활동적ㅋㅋ",
          "original_form": "모든 면에서 매우 활동적ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100016207.1.15",
          "form": "그럼 님 아니에요?ㅋㅋㅋ",
          "original_form": "그럼 님 아니에요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100016207.1.16",
          "form": "활동적이시자나요",
          "original_form": "활동적이시자나요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100016207.1.17",
          "form": "아닌데용",
          "original_form": "아닌데용",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100016207.1.18",
          "form": "결혼해서 배우자분이 원하시면\r\n반려동물 키울 의사는 있으세요?",
          "original_form": "결혼해서 배우자분이 원하시면\r\n반려동물 키울 의사는 있으세요?",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100016207.1.19",
          "form": "의사는있는데 신중해야죠...",
          "original_form": "의사는있는데 신중해야죠...",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100016207.1.20",
          "form": "관리 안될게 뻔해서 ㅋㅋ",
          "original_form": "관리 안될게 뻔해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100016207.1.21",
          "form": "여자친구분도\r\n지금 반려동물 안 키우고 계세요?",
          "original_form": "여자친구분도\r\n지금 반려동물 안 키우고 계세요?",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100016207.1.22",
          "form": "네 부모님이 시러하신대요",
          "original_form": "네 부모님이 시러하신대요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100016207.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100016207.1.24",
          "form": "고양이가 키우긴 쉽다던데",
          "original_form": "고양이가 키우긴 쉽다던데",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100016207.1.25",
          "form": "산책도 필요없고",
          "original_form": "산책도 필요없고",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100016207.1.26",
          "form": "근데 전 고양이는\r\n좀 무섭더라구요 ㅋㅋ",
          "original_form": "근데 전 고양이는\r\n좀 무섭더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100016207.1.27",
          "form": "개냥이가 귀엽던데",
          "original_form": "개냥이가 귀엽던데",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100016207.1.28",
          "form": "눈이 무섭죠?ㅋ",
          "original_form": "눈이 무섭죠?ㅋ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100016207.1.29",
          "form": "네ㅋㅋ 눈도 무섭고...\r\n일대일로 있음\r\n왠지 날 해칠거 같음ㅋㅋㅈ",
          "original_form": "네ㅋㅋ 눈도 무섭고...\r\n일대일로 있음\r\n왠지 날 해칠거 같음ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100016207.1.30",
          "form": "ㅋㅋㅋ 고양이에 안좋은 추억이 있으신가보네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ 고양이에 안좋은 추억이 있으신가보네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100016207.1.31",
          "form": "아니요ㅋㅋ 또 그런것도 아닌데\r\n제가 안키워본 동물들에 대한\r\n막연한 두려움과 공포가 있는거 같아요",
          "original_form": "아니요ㅋㅋ 또 그런것도 아닌데\r\n제가 안키워본 동물들에 대한\r\n막연한 두려움과 공포가 있는거 같아요",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100016207.1.32",
          "form": "강아지는 키워보셨군여?",
          "original_form": "강아지는 키워보셨군여?",
          "speaker_id": "2",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100016207.1.33",
          "form": "네:) 강아지는 키워봤어용",
          "original_form": "네:) 강아지는 키워봤어용",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100016207.1.34",
          "form": "햄스터도 키워봤구요ㅋ",
          "original_form": "햄스터도 키워봤구요ㅋ",
