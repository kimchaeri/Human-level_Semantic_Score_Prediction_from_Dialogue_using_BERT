      "utterance": [
        {
          "id": "MDRW2100003140.1.1",
          "form": "그래도 가끔 쇼핑 가긴 해",
          "original_form": "그래도 가끔 쇼핑 가긴 해",
          "speaker_id": "2",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003140.1.2",
          "form": "그건 그렇지 ㅋㅋㅋ",
          "original_form": "그건 그렇지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:21"
        },
        {
          "id": "MDRW2100003140.1.3",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "1",
          "time": "20210820 19:22"
        },
        {
          "id": "MDRW2100003140.1.4",
          "form": "근데 나는 쇼핑 시간 진짜 빨라",
          "original_form": "근데 나는 쇼핑 시간 진짜 빨라",
          "speaker_id": "2",
          "time": "20210820 19:42"
        },
        {
          "id": "MDRW2100003140.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210820 19:42"
        },
        {
          "id": "MDRW2100003140.1.6",
          "form": "너무 귀찮거든 ㅋㅋㅋㅋㄴ",
          "original_form": "너무 귀찮거든 ㅋㅋㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210820 19:43"
        },
        {
          "id": "MDRW2100003140.1.7",
          "form": "ㅋㅋㅋ 그렇구나",
          "original_form": "ㅋㅋㅋ 그렇구나",
          "speaker_id": "1",
          "time": "20210820 19:43"
        },
        {
          "id": "MDRW2100003140.1.8",
          "form": "최대한 빠르게",
          "original_form": "최대한 빠르게",
          "speaker_id": "2",
          "time": "20210820 19:43"
        },
        {
          "id": "MDRW2100003140.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:43"
        },
        {
          "id": "MDRW2100003140.1.10",
          "form": "그래서 내가 온라인 쇼핑을 선호하는 이유야",
          "original_form": "그래서 내가 온라인 쇼핑을 선호하는 이유야",
          "speaker_id": "1",
          "time": "20210820 19:43"
        },
        {
          "id": "MDRW2100003140.1.11",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210820 19:44"
        },
        {
          "id": "MDRW2100003140.1.12",
          "form": "직접 보고 사는 거가 좋지",
          "original_form": "직접 보고 사는 거가 좋지",
          "speaker_id": "2",
          "time": "20210820 19:44"
