      "utterance": [
        {
          "id": "MDRW2100030601.1.1",
          "form": "강원도 제주도 등 어디조아하세요",
          "original_form": "강원도 제주도 등 어디조아하세요",
          "speaker_id": "1",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030601.1.2",
          "form": "제주도!!!!",
          "original_form": "제주도!!!!",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030601.1.3",
          "form": "강원도도 좋아요 근데",
          "original_form": "강원도도 좋아요 근데",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030601.1.4",
          "form": "제주도 마니가보셧나요",
          "original_form": "제주도 마니가보셧나요",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030601.1.5",
          "form": "음 작년 1월에가 마지막이었어요",
          "original_form": "음 작년 1월에가 마지막이었어요",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030601.1.6",
          "form": "겨울에 가셧군요",
          "original_form": "겨울에 가셧군요",
          "speaker_id": "1",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030601.1.7",
          "form": "어디쪽 가셧엇나요",
          "original_form": "어디쪽 가셧엇나요",
          "speaker_id": "1",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030601.1.8",
          "form": "서귀포쪽?",
          "original_form": "서귀포쪽?",
          "speaker_id": "2",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030601.1.9",
          "form": "하고 비자림 갔어요",
          "original_form": "하고 비자림 갔어요",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030601.1.10",
          "form": "우와..비자림 저도 작년 말에갓엇는뎅",
          "original_form": "우와..비자림 저도 작년 말에갓엇는뎅",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030601.1.11",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030601.1.12",
          "form": "ㅋㅋ또 가고 싶군묘",
          "original_form": "ㅋㅋ또 가고 싶군묘",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030601.1.13",
          "form": "강원도 어디 가보셨나요",
          "original_form": "강원도 어디 가보셨나요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030601.1.14",
          "form": "강릉이용",
