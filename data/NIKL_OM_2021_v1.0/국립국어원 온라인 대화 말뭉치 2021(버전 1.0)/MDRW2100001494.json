        },
        {
          "id": "MDRW2100001494.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211015 17:23"
        },
        {
          "id": "MDRW2100001494.1.3",
          "form": "곧 저녁시간인데",
          "original_form": "곧 저녁시간인데",
          "speaker_id": "2",
          "time": "20211015 17:23"
        },
        {
          "id": "MDRW2100001494.1.4",
          "form": "메뉴는 정하셨나요~?",
          "original_form": "메뉴는 정하셨나요~?",
          "speaker_id": "2",
          "time": "20211015 17:23"
        },
        {
          "id": "MDRW2100001494.1.5",
          "form": "아뇨 ㅠ 아직안정했습니다",
          "original_form": "아뇨 ㅠ 아직안정했습니다",
          "speaker_id": "1",
          "time": "20211015 17:24"
        },
        {
          "id": "MDRW2100001494.1.6",
          "form": "어제 저녁에는 뭐드셨어요? ㅋㅋㅋ",
          "original_form": "어제 저녁에는 뭐드셨어요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.7",
          "form": "버거킹먹었어요",
          "original_form": "버거킹먹었어요",
          "speaker_id": "1",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.8",
          "form": "롱치킨버거",
          "original_form": "롱치킨버거",
          "speaker_id": "1",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.9",
          "form": "단품으로시켜가지고",
          "original_form": "단품으로시켜가지고",
          "speaker_id": "1",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.10",
          "form": "오 그거 아직도 파는구나",
          "original_form": "오 그거 아직도 파는구나",
          "speaker_id": "2",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.11",
          "form": "쿠폰으로",
          "original_form": "쿠폰으로",
          "speaker_id": "1",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.12",
          "form": "싸게구매했습니다",
          "original_form": "싸게구매했습니다",
          "speaker_id": "1",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.13",
          "form": "2500원인가",
          "original_form": "2500원인가",
          "speaker_id": "1",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.14",
          "form": "버거킹은 솔직히 쿠폰으로 먹으면 엄청 싼데!",
          "original_form": "버거킹은 솔직히 쿠폰으로 먹으면 엄청 싼데!",
          "speaker_id": "2",
          "time": "20211015 17:25"
        },
        {
          "id": "MDRW2100001494.1.15",
          "form": "인정합니다.",
          "original_form": "인정합니다.",
          "speaker_id": "1",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.16",
          "form": "배달시켜먹으려고하면 정말 비싸요 ㅠ",
          "original_form": "배달시켜먹으려고하면 정말 비싸요 ㅠ",
          "speaker_id": "2",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.17",
          "form": "2500원이면 정말 싸네요 ㅎㅎ",
          "original_form": "2500원이면 정말 싸네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.18",
          "form": "배달로 버거킹은 좀 너무 아깝죠",
          "original_form": "배달로 버거킹은 좀 너무 아깝죠",
          "speaker_id": "1",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.19",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.20",
          "form": "맞아요.. 전 예전에",
          "original_form": "맞아요.. 전 예전에",
          "speaker_id": "2",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.21",
          "form": "그것도 모르고",
          "original_form": "그것도 모르고",
          "speaker_id": "2",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.22",
          "form": "그냥 배달로 비싸게 주고 먹었다가",
          "original_form": "그냥 배달로 비싸게 주고 먹었다가",
          "speaker_id": "2",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.23",
          "form": "나중에 쿠폰 있는거 알고 엄청 후회했었던 ㅋㅋㅋㅋ",
          "original_form": "나중에 쿠폰 있는거 알고 엄청 후회했었던 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 17:26"
        },
        {
          "id": "MDRW2100001494.1.24",
          "form": "ㅋㅋㅋ 쿠폰있더라도 배달비가 요즘 진짜 비싼거같아요 ㅠ",
          "original_form": "ㅋㅋㅋ 쿠폰있더라도 배달비가 요즘 진짜 비싼거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20211015 17:27"
        },
        {
          "id": "MDRW2100001494.1.25",
