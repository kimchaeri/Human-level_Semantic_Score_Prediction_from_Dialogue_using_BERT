      "utterance": [
        {
          "id": "MDRW2100045800.1.1",
          "form": "정답입니다! 저는 외식산업학과를 나왔어요",
          "original_form": "정답입니다! 저는 외식산업학과를 나왔어요",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045800.1.2",
          "form": "대박 식품 관련이 예체능인줄 몰랐어요",
          "original_form": "대박 식품 관련이 예체능인줄 몰랐어요",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045800.1.3",
          "form": "전 이과인 줄 알았어요ㅋㅋㅋㅋ",
          "original_form": "전 이과인 줄 알았어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045800.1.4",
          "form": "에잇 어째서죠??",
          "original_form": "에잇 어째서죠??",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045800.1.5",
          "form": "뭔가 요리도 되게 과학원리가 적용이 되길래",
          "original_form": "뭔가 요리도 되게 과학원리가 적용이 되길래",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045800.1.6",
          "form": "이과쪽으로 가서 과학공부 하게 되는 줄 알았어요",
          "original_form": "이과쪽으로 가서 과학공부 하게 되는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045800.1.7",
          "form": "저는 사식 컬리플라워나 흰딸기나 미니 방울토마토 얘기해서 금세 맞추실줄 알았러요",
          "original_form": "저는 사식 컬리플라워나 흰딸기나 미니 방울토마토 얘기해서 금세 맞추실줄 알았러요",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045800.1.8",
          "form": "아니에욬ㅋㅋㅋㅋ",
          "original_form": "아니에욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045800.1.9",
          "form": "그런건 다 따로 공부해야해욬ㅋㅋㅋㅋ",
          "original_form": "그런건 다 따로 공부해야해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045800.1.10",
          "form": "요즘은 어떨지 모루갰지만요ㅠㅠ",
          "original_form": "요즘은 어떨지 모루갰지만요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045800.1.11",
          "form": "ㅋㅋㅋㄴㅋㅋㅋㅋㅋㅋㅋ온갖 식재료를",
          "original_form": "ㅋㅋㅋㄴㅋㅋㅋㅋㅋㅋㅋ온갖 식재료를",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045800.1.12",
          "form": "다 거기서 배우신건가요",
          "original_form": "다 거기서 배우신건가요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045800.1.13",
          "form": "넼ㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045800.1.14",
          "form": "진짜 별별걸 다 배워요",
          "original_form": "진짜 별별걸 다 배워요",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045800.1.15",
          "form": "ㅋㅌㅋ일반인은 모를만한 식재료를 어쩐지 더 알고계시더라구요",
          "original_form": "ㅋㅌㅋ일반인은 모를만한 식재료를 어쩐지 더 알고계시더라구요",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045800.1.16",
          "form": "한중일양 와인 커피 칵테일 제과제빵",
          "original_form": "한중일양 와인 커피 칵테일 제과제빵",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045800.1.17",
          "form": "요리관련과셨을줄이야",
          "original_form": "요리관련과셨을줄이야",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045800.1.18",
          "form": "가게관련된 거라든지...",
          "original_form": "가게관련된 거라든지...",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045800.1.19",
          "form": "식기나 분위기 같은것도 배우고",
          "original_form": "식기나 분위기 같은것도 배우고",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045800.1.20",
          "form": "계산하는것도 배우기는 해요 ...",
          "original_form": "계산하는것도 배우기는 해요 ...",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045800.1.21",
          "form": "와 전반적으로 통틀어서 배우시는군요!",
          "original_form": "와 전반적으로 통틀어서 배우시는군요!",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045800.1.22",
          "form": "뭔가 요리학교 멋지다..",
          "original_form": "뭔가 요리학교 멋지다..",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045800.1.23",
          "form": "네네 외식관련으로는 다 배웠어요",
          "original_form": "네네 외식관련으로는 다 배웠어요",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045800.1.24",
          "form": "먹는거 관련으로는 큰 틀이죠??ㅋㅋㅋ",
          "original_form": "먹는거 관련으로는 큰 틀이죠??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045800.1.25",
          "form": "네! 너무 신기하고 멋져보여요",
          "original_form": "네! 너무 신기하고 멋져보여요",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045800.1.26",
          "form": "막 프로페셔널해보여요 ㅋㅋㅋ",
          "original_form": "막 프로페셔널해보여요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045800.1.27",
          "form": "전혀 안그래욬ㅋㅋㅋ 지금운 다까먹어섴ㅋㅋ",
          "original_form": "전혀 안그래욬ㅋㅋㅋ 지금운 다까먹어섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045800.1.28",
          "form": "뜨끔.. 저도 다 까먹었는데유",
          "original_form": "뜨끔.. 저도 다 까먹었는데유",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045800.1.29",
          "form": "제 전공 맞춰보실래요?",
          "original_form": "제 전공 맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045800.1.30",
          "form": "좋아요 가보죠.",
          "original_form": "좋아요 가보죠.",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045800.1.31",
          "form": "후.하후하. 음... 뭔가 문과실것같아여!",
          "original_form": "후.하후하. 음... 뭔가 문과실것같아여!",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045800.1.32",
          "form": "아니다 이과이실것같기도. .!!",
          "original_form": "아니다 이과이실것같기도. .!!",
          "speaker_id": "1",
          "time": "20210908 07:37"
        }
      ]
    }
