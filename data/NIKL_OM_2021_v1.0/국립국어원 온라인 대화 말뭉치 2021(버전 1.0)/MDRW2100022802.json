      "utterance": [
        {
          "id": "MDRW2100022802.1.1",
          "form": "저도 배달시켰어요!!!",
          "original_form": "저도 배달시켰어요!!!",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022802.1.2",
          "form": "쌀국수 ㅋㅋㅋㅋ",
          "original_form": "쌀국수 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022802.1.3",
          "form": "요즘 배달이 진짜 너무 잘 돼있어서..",
          "original_form": "요즘 배달이 진짜 너무 잘 돼있어서..",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022802.1.4",
          "form": "그래서 점점 더 살이 찌는걸까요..?",
          "original_form": "그래서 점점 더 살이 찌는걸까요..?",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022802.1.5",
          "form": "먹고싶으면 바로 시켜버리는 나..",
          "original_form": "먹고싶으면 바로 시켜버리는 나..",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022802.1.6",
          "form": "ㅋㅋㅋㅋ체중이 많이 증가하셨나여...",
          "original_form": "ㅋㅋㅋㅋ체중이 많이 증가하셨나여...",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022802.1.7",
          "form": "근데 진짜 배달이 너무 잘되어있어서 나가서 사와도 되는거 배달시키고",
          "original_form": "근데 진짜 배달이 너무 잘되어있어서 나가서 사와도 되는거 배달시키고",
          "speaker_id": "1",
          "time": "20210829 18:06"
        },
        {
          "id": "MDRW2100022802.1.8",
          "form": "좀 참아도 되는거 다 주문해서 먹는거 같아요",
          "original_form": "좀 참아도 되는거 다 주문해서 먹는거 같아요",
          "speaker_id": "1",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022802.1.9",
          "form": "아 맞아요...저는 진짜 카페랑 크로플..",
          "original_form": "아 맞아요...저는 진짜 카페랑 크로플..",
          "speaker_id": "2",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022802.1.10",
          "form": "본식사에 디저트까지 다 주문해버려요..",
          "original_form": "본식사에 디저트까지 다 주문해버려요..",
          "speaker_id": "2",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022802.1.11",
          "form": "재택근무한 이후로는 진짜 더 시켜먹는거같아요",
          "original_form": "재택근무한 이후로는 진짜 더 시켜먹는거같아요",
          "speaker_id": "2",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100022802.1.12",
          "form": "ㅋㅋㅋㅋㅋ본격적 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ본격적 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022802.1.13",
          "form": "아 커피에 크로플... 며칠전 제 메뉸데여...?",
          "original_form": "아 커피에 크로플... 며칠전 제 메뉸데여...?",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022802.1.14",
          "form": "재택하시는구나ㅜㅜㅜㅜㅜ 축복받으신거에여...",
          "original_form": "재택하시는구나ㅜㅜㅜㅜㅜ 축복받으신거에여...",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022802.1.15",
          "form": "아이스크림 크로플도 맛있져!!",
          "original_form": "아이스크림 크로플도 맛있져!!",
          "speaker_id": "1",
          "time": "20210829 18:08"
        },
        {
          "id": "MDRW2100022802.1.16",
          "form": "재택하면 근데 보상심리로 인해..배달이 폭주해요..",
          "original_form": "재택하면 근데 보상심리로 인해..배달이 폭주해요..",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022802.1.17",
          "form": "아침부터 시켜먹었던 날이 있는데 출근하는거보다 식비가 배로 나가요 ㅠㅜ",
          "original_form": "아침부터 시켜먹었던 날이 있는데 출근하는거보다 식비가 배로 나가요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210829 18:23"
        },
        {
          "id": "MDRW2100022802.1.18",
          "form": "아아.... 또르르 ㅋㅋㅋㅋ 그런단점이 ㅠ",
          "original_form": "아아.... 또르르 ㅋㅋㅋㅋ 그런단점이 ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022802.1.19",
          "form": "돈까스 도착했나여??",
          "original_form": "돈까스 도착했나여??",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022802.1.20",
          "form": "저 이미.. 다 먹었습미다만..? ㅎ..",
          "original_form": "저 이미.. 다 먹었습미다만..? ㅎ..",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022802.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ이번 돈까스는 완벽한 경양식 돈까스는 아니어서 아쉬웠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이번 돈까스는 완벽한 경양식 돈까스는 아니어서 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022802.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022802.1.23",
          "form": "저도 쌀국수 다 먹었는데 와... 맛있어요진짜",
          "original_form": "저도 쌀국수 다 먹었는데 와... 맛있어요진짜",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022802.1.24",
          "form": "콧물나도록ㅋㅋㅋㅋㅋㅋ 맛있어요",
          "original_form": "콧물나도록ㅋㅋㅋㅋㅋㅋ 맛있어요",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022802.1.25",
          "form": "헐 저도 갑자기 쌀국수 완전 먹고싶어요",
          "original_form": "헐 저도 갑자기 쌀국수 완전 먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022802.1.26",
          "form": "내일 점심 메뉴로 갑니다..",
          "original_form": "내일 점심 메뉴로 갑니다..",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022802.1.27",
          "form": "진짜 땀이 쫙납니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 땀이 쫙납니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022802.1.28",
          "form": "기운이 팍 나욬ㅋㅋㅋ",
          "original_form": "기운이 팍 나욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022802.1.29",
          "form": "마자요 뜨거운 쌀국수만이 줄 수 있는 에너지..!",
          "original_form": "마자요 뜨거운 쌀국수만이 줄 수 있는 에너지..!",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022802.1.30",
          "form": "전 근데 살이 잘 찌는 편이라 요즘 정말 고통스러워요..!",
          "original_form": "전 근데 살이 잘 찌는 편이라 요즘 정말 고통스러워요..!",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022802.1.31",
          "form": "먹고싶은것도 3번참고 먹는 습관을 들이고있어요",
          "original_form": "먹고싶은것도 3번참고 먹는 습관을 들이고있어요",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022802.1.32",
          "form": "으어어ㅜㅜ그정도인가여... 확실히 재택하면서 많이 드셨나봐요ㅜㅜㅜ",
          "original_form": "으어어ㅜㅜ그정도인가여... 확실히 재택하면서 많이 드셨나봐요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022802.1.33",
          "form": "맛있는걸 참는건 괴로워요..",
          "original_form": "맛있는걸 참는건 괴로워요..",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022802.1.34",
          "form": "맞아요..! 살이 너무 쪄서 피티 결제했습니다. .",
          "original_form": "맞아요..! 살이 너무 쪄서 피티 결제했습니다. .",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100022802.1.35",
          "form": "매일 닭의 어느 부위만.. 먹는중입니다..!!!!!",
          "original_form": "매일 닭의 어느 부위만.. 먹는중입니다..!!!!!",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100022802.1.36",
          "form": "아앗.... 매일 드시면 질리시는거 아니에요? ㅜㅜ",
          "original_form": "아앗.... 매일 드시면 질리시는거 아니에요? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022802.1.37",
          "form": "근데 저도 아무생각없이 먹다보니 몰랐는데 몸무게가 늘었더라구요 ㅋㅋㅋㅋ",
          "original_form": "근데 저도 아무생각없이 먹다보니 몰랐는데 몸무게가 늘었더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:43"
        }
      ]
