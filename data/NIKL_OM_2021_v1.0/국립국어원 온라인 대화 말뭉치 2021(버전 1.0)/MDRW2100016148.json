      "utterance": [
        {
          "id": "MDRW2100016148.1.1",
          "form": "안녕하세요 혹시 싸이월드 하셨었어요?",
          "original_form": "안녕하세요 혹시 싸이월드 하셨었어요?",
          "speaker_id": "1",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016148.1.2",
          "form": "와 추억 돋네요 싸이월드",
          "original_form": "와 추억 돋네요 싸이월드",
          "speaker_id": "2",
          "time": "20210826 10:18"
        },
        {
          "id": "MDRW2100016148.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016148.1.4",
          "form": "많이는 안하고 좀 끼적거리는 정도였어요",
          "original_form": "많이는 안하고 좀 끼적거리는 정도였어요",
          "speaker_id": "2",
          "time": "20210826 10:19"
        },
        {
          "id": "MDRW2100016148.1.5",
          "form": "그러셨군용 저도 많이는 아니고 조금 하는 정도였는데 부활한다니까 괜히 반갑더라구요",
          "original_form": "그러셨군용 저도 많이는 아니고 조금 하는 정도였는데 부활한다니까 괜히 반갑더라구요",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100016148.1.6",
          "form": "복구 신청 하셨어요?",
          "original_form": "복구 신청 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100016148.1.7",
          "form": "네 별거없지만 궁금해서 해봤어요",
          "original_form": "네 별거없지만 궁금해서 해봤어요",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016148.1.8",
          "form": "근데 정작 신청만하고 그 이후 한번도 접속해보질 않았네요",
          "original_form": "근데 정작 신청만하고 그 이후 한번도 접속해보질 않았네요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016148.1.9",
          "form": "일정도 몰라요ㅎㅎㅎㅎ",
          "original_form": "일정도 몰라요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016148.1.10",
          "form": "오 핸드폰번호만 있음되나요?",
          "original_form": "오 핸드폰번호만 있음되나요?",
          "speaker_id": "2",
          "time": "20210826 10:27"
        },
        {
          "id": "MDRW2100016148.1.11",
          "form": "네 되게 간단하더라구요",
          "original_form": "네 되게 간단하더라구요",
          "speaker_id": "1",
          "time": "20210826 10:28"
        },
        {
          "id": "MDRW2100016148.1.12",
          "form": "저는 아마 폰 번호가 바뀐거같아요 ㄲ",
          "original_form": "저는 아마 폰 번호가 바뀐거같아요 ㄲ",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016148.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016148.1.14",
          "form": "넘 오래전이라",
          "original_form": "넘 오래전이라",
          "speaker_id": "2",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100016148.1.15",
          "form": "아 저는 계속 같은 번호라 괜찮았던 모양이네요",
          "original_form": "아 저는 계속 같은 번호라 괜찮았던 모양이네요",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016148.1.16",
          "form": "그치만 역사 속에 묻혀있는게 더 좋은 일일수도 있어요ㅎㅎ",
          "original_form": "그치만 역사 속에 묻혀있는게 더 좋은 일일수도 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100016148.1.17",
          "form": "저 지금 생각해보니 전남친이랑 사진 거기있을거같아요ㄷㄷㄷ",
          "original_form": "저 지금 생각해보니 전남친이랑 사진 거기있을거같아요ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100016148.1.18",
          "form": "앗ㄷㄷㄷㄷ",
          "original_form": "앗ㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016148.1.19",
          "form": "좋은 추억이셨나요ㅋㅋㅋ",
          "original_form": "좋은 추억이셨나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100016148.1.20",
          "form": "아님 완전 똥차 구남친",
          "original_form": "아님 완전 똥차 구남친",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100016148.1.21",
          "form": "상반된감정이 공존하지만",
          "original_form": "상반된감정이 공존하지만",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100016148.1.22",
          "form": "다시 보고싶지는 않은 추억이요ㅋㅋ",
          "original_form": "다시 보고싶지는 않은 추억이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100016148.1.23",
          "form": "그러시군요ㅋㅋㅋ 부활하면 안되겠어요",
          "original_form": "그러시군요ㅋㅋㅋ 부활하면 안되겠어요",
