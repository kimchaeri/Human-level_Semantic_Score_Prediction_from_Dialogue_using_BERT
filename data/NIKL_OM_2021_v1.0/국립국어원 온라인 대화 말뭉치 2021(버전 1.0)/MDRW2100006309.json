      "utterance": [
        {
          "id": "MDRW2100006309.1.1",
          "form": "코로나 시기엔",
          "original_form": "코로나 시기엔",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006309.1.2",
          "form": "그럼 여행 겸 겸사겸사",
          "original_form": "그럼 여행 겸 겸사겸사",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006309.1.3",
          "form": "갔던 건가보네요",
          "original_form": "갔던 건가보네요",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006309.1.4",
          "form": "테이크 아웃해서 차에서도 식사 많이 했어요",
          "original_form": "테이크 아웃해서 차에서도 식사 많이 했어요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006309.1.5",
          "form": "네네 바람도 쐬고 좋던데요",
          "original_form": "네네 바람도 쐬고 좋던데요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006309.1.6",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006309.1.7",
          "form": "차가 있으면",
          "original_form": "차가 있으면",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006309.1.8",
          "form": "좋군요",
          "original_form": "좋군요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006309.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006309.1.10",
          "form": "국내여행 종종 다니시나요",
          "original_form": "국내여행 종종 다니시나요",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006309.1.11",
          "form": "앞서 말씀드린 것처럼",
          "original_form": "앞서 말씀드린 것처럼",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006309.1.12",
          "form": "아직 차가 없어서",
          "original_form": "아직 차가 없어서",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006309.1.13",
          "form": "한계가 분명하네요",
          "original_form": "한계가 분명하네요",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006309.1.14",
          "form": "저는 뚜벅이 여행도 많이 다녀요ㅋㅋ그냥 친구들이랑?",
          "original_form": "저는 뚜벅이 여행도 많이 다녀요ㅋㅋ그냥 친구들이랑?",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.15",
          "form": "제주도나 여수 이런데도 최근에 다녀오고",
          "original_form": "제주도나 여수 이런데도 최근에 다녀오고",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.16",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.17",
          "form": "여수랑 제주 모두 좋죠",
          "original_form": "여수랑 제주 모두 좋죠",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.18",
          "form": "기차나 버스로 갈 수 있는 등산도 종종 가거든요ㅋㅋㅋ",
          "original_form": "기차나 버스로 갈 수 있는 등산도 종종 가거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.19",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.20",
          "form": "내일로라던가",
          "original_form": "내일로라던가",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006309.1.21",
          "form": "오 내일로 저도 한번 가봤어요ㅋㅋ",
          "original_form": "오 내일로 저도 한번 가봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006309.1.22",
          "form": "이런걸로",
          "original_form": "이런걸로",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006309.1.23",
          "form": "오다녔었는데",
          "original_form": "오다녔었는데",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006309.1.24",
          "form": "여수를 내일로로 해서 갔었어요",
          "original_form": "여수를 내일로로 해서 갔었어요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006309.1.25",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006309.1.26",
          "form": "저도 여수 내일로 갔었는데!!",
          "original_form": "저도 여수 내일로 갔었는데!!",
          "speaker_id": "2",
          "time": "20210907 22:45"
        }
      ]
    }
