      "utterance": [
        {
          "id": "MDRW2100033164.1.1",
          "form": "지금도 비가 오고 있는데",
          "original_form": "지금도 비가 오고 있는데",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100033164.1.2",
          "form": "언제부터 가을장마가 한국에 있었나 싶습니다",
          "original_form": "언제부터 가을장마가 한국에 있었나 싶습니다",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100033164.1.3",
          "form": "맞아요 이번주도 비소식이 있죠",
          "original_form": "맞아요 이번주도 비소식이 있죠",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033164.1.4",
          "form": "비 많이오나요",
          "original_form": "비 많이오나요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033164.1.5",
          "form": "좀 전에 시작해서 조금씩 오고 있습니다",
          "original_form": "좀 전에 시작해서 조금씩 오고 있습니다",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100033164.1.6",
          "form": "내일까지 온다고 하는군요",
          "original_form": "내일까지 온다고 하는군요",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100033164.1.7",
          "form": "어쩐지 무릎이 아프네요..",
          "original_form": "어쩐지 무릎이 아프네요..",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033164.1.8",
          "form": "주말동안 정말 딱 초가을 날씨여서 좋았는데 말이지요",
          "original_form": "주말동안 정말 딱 초가을 날씨여서 좋았는데 말이지요",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033164.1.9",
          "form": "맞아요 저번주 날씨 정말 좋았는데",
          "original_form": "맞아요 저번주 날씨 정말 좋았는데",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100033164.1.10",
          "form": "이번  비도 금방 끝나고 다시 가을날씨가 되었으면 좋겠습니다",
          "original_form": "이번  비도 금방 끝나고 다시 가을날씨가 되었으면 좋겠습니다",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033164.1.11",
          "form": "이번 비가 끝나면 왠지 급 추워질거같긴 하네요..",
          "original_form": "이번 비가 끝나면 왠지 급 추워질거같긴 하네요..",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100033164.1.12",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033164.1.13",
          "form": "가을은 비가 올 수록 추워지니까요",
          "original_form": "가을은 비가 올 수록 추워지니까요",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033164.1.14",
          "form": "그러니까요.. 이번 겨울도 추울까요",
          "original_form": "그러니까요.. 이번 겨울도 추울까요",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100033164.1.15",
          "form": "그런데 겨울에 추워야 다음 해에 벌레들이 덜 생긴다 하더군요",
          "original_form": "그런데 겨울에 추워야 다음 해에 벌레들이 덜 생긴다 하더군요",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100033164.1.16",
          "form": "너무 포근한 겨울이 되면 다음 봄 여름에 벌레가 많아진답니다",
          "original_form": "너무 포근한 겨울이 되면 다음 봄 여름에 벌레가 많아진답니다",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100033164.1.17",
          "form": "맞아요 그래서 올해 벌레가 많이 적다고 했어요",
          "original_form": "맞아요 그래서 올해 벌레가 많이 적다고 했어요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033164.1.18",
          "form": "작년에 정말 추었는데",
          "original_form": "작년에 정말 추었는데",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033164.1.19",
          "form": "벌레가 없는건 좋네요",
          "original_form": "벌레가 없는건 좋네요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100033164.1.20",
          "form": "참 딜레마 입니다",
          "original_form": "참 딜레마 입니다",
          "speaker_id": "2",
          "time": "20210907 05:52"
        },
        {
          "id": "MDRW2100033164.1.21",
          "form": "겨울에 추운 건 싫지만",
          "original_form": "겨울에 추운 건 싫지만",
          "speaker_id": "2",
          "time": "20210907 05:53"
        },
        {
          "id": "MDRW2100033164.1.22",
          "form": "또 안추우면 벌레가 많아지니까요",
          "original_form": "또 안추우면 벌레가 많아지니까요",
          "speaker_id": "2",
          "time": "20210907 05:53"
        },
        {
          "id": "MDRW2100033164.1.23",
          "form": "장단점이 있는 듯 합니다",
          "original_form": "장단점이 있는 듯 합니다",
          "speaker_id": "2",
          "time": "20210907 05:53"
        },
        {
          "id": "MDRW2100033164.1.24",
          "form": "여기는 오늘도 비가 옵니다",
          "original_form": "여기는 오늘도 비가 옵니다",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100033164.1.25",
          "form": "아직도 장맛비라고 하네요",
          "original_form": "아직도 장맛비라고 하네요",
          "speaker_id": "2",
          "time": "20210907 14:06"
        }
      ]
    }
