      "utterance": [
        {
          "id": "MDRW2100018512.1.1",
          "form": "쇼핑 좋아하시나요?",
          "original_form": "쇼핑 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 07:22"
        },
        {
          "id": "MDRW2100018512.1.2",
          "form": "음 요새 안나가서 주로 온라인에서 하는데",
          "original_form": "음 요새 안나가서 주로 온라인에서 하는데",
          "speaker_id": "1",
          "time": "20210827 07:23"
        },
        {
          "id": "MDRW2100018512.1.3",
          "form": "좋아해요 ㅋㅋ",
          "original_form": "좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:23"
        },
        {
          "id": "MDRW2100018512.1.4",
          "form": "온라인 정말 간편해졌어요ㅋㅋ",
          "original_form": "온라인 정말 간편해졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:23"
        },
        {
          "id": "MDRW2100018512.1.5",
          "form": "네 바로 클릭한번에 주문",
          "original_form": "네 바로 클릭한번에 주문",
          "speaker_id": "1",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.6",
          "form": "가장 좋은건",
          "original_form": "가장 좋은건",
          "speaker_id": "1",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.7",
          "form": "할인이죠",
          "original_form": "할인이죠",
          "speaker_id": "1",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.8",
          "form": "쇼핑 자주 하세요?",
          "original_form": "쇼핑 자주 하세요?",
          "speaker_id": "1",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.10",
          "form": "자주 이용하시는 사이트는 어디인가요?",
          "original_form": "자주 이용하시는 사이트는 어디인가요?",
          "speaker_id": "2",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.11",
          "form": "쿠팡 위메프 티몬",
          "original_form": "쿠팡 위메프 티몬",
          "speaker_id": "1",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.12",
          "form": "저도 인터넷으로 자주해요ㅎㅎ",
          "original_form": "저도 인터넷으로 자주해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.13",
          "form": "11번가요",
          "original_form": "11번가요",
          "speaker_id": "1",
          "time": "20210827 07:24"
        },
        {
          "id": "MDRW2100018512.1.14",
          "form": "오프라인 비싸요",
          "original_form": "오프라인 비싸요",
          "speaker_id": "1",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.15",
          "form": "어디가 할인이 제일 좋은가요?",
          "original_form": "어디가 할인이 제일 좋은가요?",
          "speaker_id": "2",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.16",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.17",
          "form": "다 그때그때 달라요",
          "original_form": "다 그때그때 달라요",
          "speaker_id": "1",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.18",
          "form": "세일할때 사는거라",
          "original_form": "세일할때 사는거라",
          "speaker_id": "1",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.19",
          "form": "물건마다 달리사야죠 ㅎㅎ",
          "original_form": "물건마다 달리사야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.20",
          "form": "어디 이용하세요?",
          "original_form": "어디 이용하세요?",
          "speaker_id": "1",
          "time": "20210827 07:25"
        },
        {
          "id": "MDRW2100018512.1.21",
          "form": "오 고수느낌이 물씬이시네요",
          "original_form": "오 고수느낌이 물씬이시네요",
          "speaker_id": "2",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018512.1.22",
          "form": "저는 네이버스토어를 가장 많이 이용해요",
          "original_form": "저는 네이버스토어를 가장 많이 이용해요",
          "speaker_id": "2",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018512.1.23",
          "form": "아니예요",
          "original_form": "아니예요",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018512.1.24",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018512.1.25",
          "form": "거긴 스탬프 찍어",
          "original_form": "거긴 스탬프 찍어",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018512.1.26",
          "form": "10프로할인",
          "original_form": "10프로할인",
          "speaker_id": "1",
          "time": "20210827 07:26"
        },
        {
          "id": "MDRW2100018512.1.27",
          "form": "근데 안되는데가 더많아요 ㅜㅜ",
          "original_form": "근데 안되는데가 더많아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018512.1.28",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018512.1.29",
          "form": "맞아요ㅠ 그게 좀 아쉬워요ㅜ",
          "original_form": "맞아요ㅠ 그게 좀 아쉬워요ㅜ",
          "speaker_id": "2",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018512.1.30",
          "form": "그래도 포인트 쌓이는게 꽤 괜찮더라구요",
          "original_form": "그래도 포인트 쌓이는게 꽤 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018512.1.31",
          "form": "그렇죠 다쓰게 해주지",
          "original_form": "그렇죠 다쓰게 해주지",
          "speaker_id": "1",
          "time": "20210827 07:27"
        },
        {
          "id": "MDRW2100018512.1.32",
          "form": "맞아요 그걸로 쇼핑하고",
          "original_form": "맞아요 그걸로 쇼핑하고",
          "speaker_id": "1",
          "time": "20210827 07:27"
        }
      ]
    }
