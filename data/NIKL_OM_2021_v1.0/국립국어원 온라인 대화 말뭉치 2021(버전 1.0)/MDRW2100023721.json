      "utterance": [
        {
          "id": "MDRW2100023721.1.1",
          "form": "신곡 뭐 들은거있어?",
          "original_form": "신곡 뭐 들은거있어?",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100023721.1.2",
          "form": "난 신곡은 아니고 요새 드림캐쳐 노래에 꽂혀가지구 드림캐쳐 노래 들엌ㅋㅋㅋㅋㅋ",
          "original_form": "난 신곡은 아니고 요새 드림캐쳐 노래에 꽂혀가지구 드림캐쳐 노래 들엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100023721.1.3",
          "form": "아그래? 괜찮은거있어?",
          "original_form": "아그래? 괜찮은거있어?",
          "speaker_id": "1",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100023721.1.4",
          "form": "타이틀곡은 다 좋은거 같아 약간 메탈이랰ㅋㅋ 넌 뭐 들어?",
          "original_form": "타이틀곡은 다 좋은거 같아 약간 메탈이랰ㅋㅋ 넌 뭐 들어?",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.5",
          "form": "내취향은 스키즈 소리꾼알아? 그런건데ㅋㅋ 사무실에서 듣긴 좀 그래서",
          "original_form": "내취향은 스키즈 소리꾼알아? 그런건데ㅋㅋ 사무실에서 듣긴 좀 그래서",
          "speaker_id": "1",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.6",
          "form": "헐 너도 스키즈 알아?ㅋㅋㅋ",
          "original_form": "헐 너도 스키즈 알아?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.7",
          "form": "알긴알지 킹덤에도 나왔자너!",
          "original_form": "알긴알지 킹덤에도 나왔자너!",
          "speaker_id": "1",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.8",
          "form": "오 스키즈를 알다니",
          "original_form": "오 스키즈를 알다니",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.9",
          "form": "아이엔도 안다구ㅋㅋㅋ",
          "original_form": "아이엔도 안다구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.10",
          "form": "그래 아이엔을 알아?ㅋㅋㅋ",
          "original_form": "그래 아이엔을 알아?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.11",
          "form": "나 주말에 데드풀도 봐서ㅋㅋ",
          "original_form": "나 주말에 데드풀도 봐서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100023721.1.12",
          "form": "저번까지는 몰랐는데",
          "original_form": "저번까지는 몰랐는데",
