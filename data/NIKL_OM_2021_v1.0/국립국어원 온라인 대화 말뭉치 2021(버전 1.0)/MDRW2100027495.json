      "utterance": [
        {
          "id": "MDRW2100027495.1.1",
          "form": "코로나가 빨리 없어져야 할텐되요.",
          "original_form": "코로나가 빨리 없어져야 할텐되요.",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027495.1.2",
          "form": "오늘도 재택 근무네요.ㅎ",
          "original_form": "오늘도 재택 근무네요.ㅎ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027495.1.3",
          "form": "재택근무하시나요?",
          "original_form": "재택근무하시나요?",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027495.1.4",
          "form": "아.그렇군요",
          "original_form": "아.그렇군요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027495.1.5",
          "form": "네~~\r\n코로나 전부터 쭉 해왔어요.",
          "original_form": "네~~\r\n코로나 전부터 쭉 해왔어요.",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027495.1.6",
          "form": "좋은 직장이시네요",
          "original_form": "좋은 직장이시네요",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027495.1.7",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027495.1.8",
          "form": "재택근무하시면 휠씬편하시죠?",
          "original_form": "재택근무하시면 휠씬편하시죠?",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027495.1.9",
          "form": "부럽기는요.\r\n직장은 아닌거 같아요.",
          "original_form": "부럽기는요.\r\n직장은 아닌거 같아요.",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027495.1.10",
          "form": "알바신가요?",
          "original_form": "알바신가요?",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027495.1.11",
          "form": "아믄튼 설명하기 힘들고 애매해요.",
          "original_form": "아믄튼 설명하기 힘들고 애매해요.",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027495.1.12",
          "form": "아..그렇군요..세상에 일이 많으니",
          "original_form": "아..그렇군요..세상에 일이 많으니",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027495.1.13",
          "form": "세상일이라고 하기도 좀 그렇구요",
          "original_form": "세상일이라고 하기도 좀 그렇구요",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027495.1.14",
          "form": "뭐든 일하시면 돼죠",
          "original_form": "뭐든 일하시면 돼죠",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027495.1.15",
          "form": "재택근무편하게 하세요",
          "original_form": "재택근무편하게 하세요",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027495.1.16",
          "form": "뭐든 하는것도 아닌거 같아요.",
          "original_form": "뭐든 하는것도 아닌거 같아요.",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027495.1.17",
          "form": "아..무슨일이신지 궁금하네요",
          "original_form": "아..무슨일이신지 궁금하네요",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
