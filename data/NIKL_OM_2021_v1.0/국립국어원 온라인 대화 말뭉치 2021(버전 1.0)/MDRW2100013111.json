        },
        {
          "id": "MDRW2100013111.1.5",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013111.1.6",
          "form": "막 좋아하는건 아니고",
          "original_form": "막 좋아하는건 아니고",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013111.1.7",
          "form": "신전은 좋아해요 그래도 ㅎㅎ",
          "original_form": "신전은 좋아해요 그래도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013111.1.8",
          "form": "ㅋㅋㅋ 엽떡vs신전은요?",
          "original_form": "ㅋㅋㅋ 엽떡vs신전은요?",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013111.1.9",
          "form": "전 온니 신전 ㅋㅋㅋ",
          "original_form": "전 온니 신전 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013111.1.10",
          "form": "name1님은 어떤 떡볶이를 좋아하세요",
          "original_form": "&name1&님은 어떤 떡볶이를 좋아하세요",
