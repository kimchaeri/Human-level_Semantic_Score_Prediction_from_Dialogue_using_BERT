          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013111.1.14",
          "form": "아 ~",
          "original_form": "아 ~",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013111.1.15",
          "form": "로제 정말맛있죠 ㅎㅎ",
          "original_form": "로제 정말맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013111.1.16",
          "form": "저도 로제에빠져서 두찜에 로제 찜닭 많이먹어요 ㅎㅎ",
          "original_form": "저도 로제에빠져서 두찜에 로제 찜닭 많이먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013111.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013111.1.18",
          "form": "로제찜닭맛있나요?",
          "original_form": "로제찜닭맛있나요?",
