      "utterance": [
        {
          "id": "MDRW2100037356.1.1",
          "form": "파스타를 먹었어",
          "original_form": "파스타를 먹었어",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037356.1.2",
          "form": "나는 토마토파스타는 그저그런데",
          "original_form": "나는 토마토파스타는 그저그런데",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037356.1.3",
          "form": "볼로네제는 맛있더라",
          "original_form": "볼로네제는 맛있더라",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037356.1.4",
          "form": "나도 토마토파스타는 별론데",
          "original_form": "나도 토마토파스타는 별론데",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037356.1.5",
          "form": "롯데푸드 볼로네이즈 먹어봤어m",
          "original_form": "롯데푸드 볼로네이즈 먹어봤어m",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037356.1.6",
          "form": "양은 정말 적어",
          "original_form": "양은 정말 적어",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.7",
          "form": "그거 한번 시도해볼께 니가 추천했으니까!",
          "original_form": "그거 한번 시도해볼께 니가 추천했으니까!",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.8",
          "form": "0.33인분같아",
          "original_form": "0.33인분같아",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.9",
          "form": "가격은 얼마야??",
          "original_form": "가격은 얼마야??",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.10",
          "form": "기억이 안나",
          "original_form": "기억이 안나",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.11",
          "form": "근데 비싼거같아",
          "original_form": "근데 비싼거같아",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.12",
          "form": "양이 적거든 ㅠ",
          "original_form": "양이 적거든 ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.13",
          "form": "흠 다음에 생각나면 알려줘",
          "original_form": "흠 다음에 생각나면 알려줘",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.14",
          "form": "미리 돈을 준비해가야해",
          "original_form": "미리 돈을 준비해가야해",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.15",
          "form": "근데 우리집 주변은 그런 파스타가 업ㄱ어",
          "original_form": "근데 우리집 주변은 그런 파스타가 업ㄱ어",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.16",
          "form": "맞아.. 근데 편의점움식은 너무 비싸",
          "original_form": "맞아.. 근데 편의점움식은 너무 비싸",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.17",
          "form": "식당이 없다는거야?",
          "original_form": "식당이 없다는거야?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.18",
          "form": "배달파스타는 어때?",
          "original_form": "배달파스타는 어때?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.19",
          "form": "삼김도 비싸졌어",
          "original_form": "삼김도 비싸졌어",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.20",
          "form": "응 식당이 없어",
          "original_form": "응 식당이 없어",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.21",
          "form": "배달파스타중에 볼로네제 볼로네이즈 라구 다 없어",
          "original_form": "배달파스타중에 볼로네제 볼로네이즈 라구 다 없어",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.22",
          "form": "라구는 한군데있던데 고기범벅이야",
          "original_form": "라구는 한군데있던데 고기범벅이야",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037356.1.23",
          "form": "난 조화를 원해.",
          "original_form": "난 조화를 원해.",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037356.1.24",
          "form": "한번도 안먹어뽰어",
          "original_form": "한번도 안먹어뽰어",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037356.1.25",
          "form": "라구 라는 음식 맜있니??",
          "original_form": "라구 라는 음식 맜있니??",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037356.1.26",
          "form": "한번 츄라이츄라이.",
          "original_form": "한번 츄라이츄라이.",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037356.1.27",
          "form": "그냥 넓은 면이야",
          "original_form": "그냥 넓은 면이야",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037356.1.28",
          "form": "볼로네이즈 소스에.",
          "original_form": "볼로네이즈 소스에.",
          "speaker_id": "2",
          "time": "20210831 22:49"
        }
      ]
    }
  ]
