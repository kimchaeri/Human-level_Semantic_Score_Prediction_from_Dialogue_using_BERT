      "utterance": [
        {
          "id": "MDRW2100045669.1.1",
          "form": "안녕하세요 name2님, 좋아하는 음식이 있나요?",
          "original_form": "안녕하세요 &name2&님, 좋아하는 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045669.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045669.1.3",
          "form": "전 요즘 그릭복숭아에 꽂혔어요 혹시 아시나요?",
          "original_form": "전 요즘 그릭복숭아에 꽂혔어요 혹시 아시나요?",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045669.1.4",
          "form": "헐 완전 맛있죠ㅠㅠㅠㅠㅠㅠ",
          "original_form": "헐 완전 맛있죠ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045669.1.5",
          "form": "그냥 그릭요거트도 맛있는데 복숭아까지...",
          "original_form": "그냥 그릭요거트도 맛있는데 복숭아까지...",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045669.1.6",
          "form": "전 요즘 육회만 왕창 먹고있어요ㅎㅎ",
          "original_form": "전 요즘 육회만 왕창 먹고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045669.1.7",
          "form": "육회 오",
          "original_form": "육회 오",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.8",
          "form": "맛있죠ㅠㅠㅠㅠㅠㅠ",
          "original_form": "맛있죠ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.9",
          "form": "밤 되니까 또 떡볶이도 땡기네요ㅠㅠ",
          "original_form": "밤 되니까 또 떡볶이도 땡기네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.10",
          "form": "사드시나요? 아니면 배달? 마트?",
          "original_form": "사드시나요? 아니면 배달? 마트?",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.11",
          "form": "저두요ㅠㅠㅠㅠ",
          "original_form": "저두요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.12",
          "form": "로제떡볶이 먹어보고 싶어요",
          "original_form": "로제떡볶이 먹어보고 싶어요",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.13",
          "form": "전 정육점에서 사먹어요! 완전 싱싱하고 맛있어요",
          "original_form": "전 정육점에서 사먹어요! 완전 싱싱하고 맛있어요",
          "speaker_id": "1",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.14",
          "form": "저도요ㅜㅜ 맨날 시도해야지 하다가 미루게 돼서ㅋㅋㅋ",
          "original_form": "저도요ㅜㅜ 맨날 시도해야지 하다가 미루게 돼서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.15",
          "form": "와 가장 확실하고 맛있는 방법이죠ㅠㅠㅠㅠ",
          "original_form": "와 가장 확실하고 맛있는 방법이죠ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045669.1.16",
          "form": "무슨 양념뿌려드시나요?",
          "original_form": "무슨 양념뿌려드시나요?",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045669.1.17",
          "form": "저 간장양념 같이 들어있는 거요!",
          "original_form": "저 간장양념 같이 들어있는 거요!",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045669.1.18",
          "form": "근데 핵심은 역시 배인 것 같아요",
          "original_form": "근데 핵심은 역시 배인 것 같아요",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045669.1.19",
          "form": "촵촵 썰어서 아삭아삭ㅠㅠ",
          "original_form": "촵촵 썰어서 아삭아삭ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045669.1.20",
          "form": "와 배까지",
          "original_form": "와 배까지",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.21",
          "form": "충실하게 드시네요ㅠㅠㅠ 너무 맛있겠다",
          "original_form": "충실하게 드시네요ㅠㅠㅠ 너무 맛있겠다",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.22",
          "form": "계란 노른자도 얹으시나요?",
          "original_form": "계란 노른자도 얹으시나요?",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.23",
          "form": "배 너무 맛있어요ㅠㅠ식감도 재밌구",
          "original_form": "배 너무 맛있어요ㅠㅠ식감도 재밌구",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.24",
          "form": "하 이 시간에 먹는 이야기는 너무 즐겁네요",
          "original_form": "하 이 시간에 먹는 이야기는 너무 즐겁네요",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.25",
          "form": "맞아요ㅠㅠㅠ 배 너무 좋아요",
          "original_form": "맞아요ㅠㅠㅠ 배 너무 좋아요",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.26",
          "form": "배의 계절이네요 그러고보니",
          "original_form": "배의 계절이네요 그러고보니",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.27",
          "form": "계란도 풀어야죠! 크으ㅠㅠㅠ",
          "original_form": "계란도 풀어야죠! 크으ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045669.1.28",
          "form": "계란 크 무조건 넣어야죠!!!",
          "original_form": "계란 크 무조건 넣어야죠!!!",
          "speaker_id": "2",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045669.1.29",
          "form": "그래서 가을이 좋아요ㅠㅠ 추석이 있잖아요ㅠㅠ",
          "original_form": "그래서 가을이 좋아요ㅠㅠ 추석이 있잖아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:10"
        }
      ]
