      "utterance": [
        {
          "id": "MDRW2100030048.1.1",
          "form": "여행얘기를 해봅시다",
          "original_form": "여행얘기를 해봅시다",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100030048.1.2",
          "form": "ㅎㅎ 여행 좋아하시나요",
          "original_form": "ㅎㅎ 여행 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030048.1.3",
          "form": "일본 좋아합니다",
          "original_form": "일본 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030048.1.4",
          "form": "바다가고싶군요",
          "original_form": "바다가고싶군요",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030048.1.5",
          "form": "오사카만 한 7번 갔다온 것 같네요",
          "original_form": "오사카만 한 7번 갔다온 것 같네요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030048.1.6",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030048.1.7",
          "form": "저 오사카 한번이용",
          "original_form": "저 오사카 한번이용",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030048.1.8",
          "form": "다코야끼 진짜 존맛..",
          "original_form": "다코야끼 진짜 존맛..",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030048.1.9",
          "form": "ㅋㅋㅋㅋ전 여러번 먹었는데",
          "original_form": "ㅋㅋㅋㅋ전 여러번 먹었는데",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030048.1.10",
          "form": "속을 거의 안익혀줘서",
          "original_form": "속을 거의 안익혀줘서",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030048.1.11",
          "form": "전 불호오",
          "original_form": "전 불호오",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030048.1.12",
          "form": "불호요",
          "original_form": "불호요",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030048.1.13",
          "form": "일본음식이 확실히 부드럽더라구요",
          "original_form": "일본음식이 확실히 부드럽더라구요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030048.1.14",
          "form": "애들 잇몸이 약해서 그런듯",
          "original_form": "애들 잇몸이 약해서 그런듯",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030048.1.15",
          "form": "맞아요 물렁물렁한거 좋아하고",
          "original_form": "맞아요 물렁물렁한거 좋아하고",
          "speaker_id": "2",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100030048.1.16",
          "form": "다 간장베이스고",
          "original_form": "다 간장베이스고",
          "speaker_id": "2",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100030048.1.17",
          "form": "나중에 또 가신다면 교토 가보세요 여기 진짜 좋음",
          "original_form": "나중에 또 가신다면 교토 가보세요 여기 진짜 좋음",
          "speaker_id": "2",
          "time": "20210904 09:59"
        },
        {
          "id": "MDRW2100030048.1.18",
          "form": "그래도 친절하고 깨끗하고 음식이 맛있던 기억이 나네요",
          "original_form": "그래도 친절하고 깨끗하고 음식이 맛있던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100030048.1.19",
          "form": "담엔 교토로 가봐야겠습니다",
          "original_form": "담엔 교토로 가봐야겠습니다",
          "speaker_id": "1",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100030048.1.20",
          "form": "삿포로 예약했었는데",
          "original_form": "삿포로 예약했었는데",
          "speaker_id": "2",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100030048.1.21",
          "form": "노노재팬 터져서 취소한게 아쉽네요",
          "original_form": "노노재팬 터져서 취소한게 아쉽네요",
          "speaker_id": "2",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100030048.1.22",
          "form": "코로나로 이렇게 못갈줄은",
          "original_form": "코로나로 이렇게 못갈줄은",
          "speaker_id": "2",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100030048.1.23",
          "form": "그놈에 노노재팬",
          "original_form": "그놈에 노노재팬",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030048.1.24",
          "form": "국민성 이상합니다",
          "original_form": "국민성 이상합니다",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030048.1.25",
          "form": "어느 정도는 이해합니다ㅎ 강요가 문제지",
          "original_form": "어느 정도는 이해합니다ㅎ 강요가 문제지",
          "speaker_id": "2",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030048.1.26",
          "form": "일본차에 김치테러하고",
          "original_form": "일본차에 김치테러하고",
          "speaker_id": "1",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030048.1.27",
          "form": "엄청 한심합니다",
          "original_form": "엄청 한심합니다",
          "speaker_id": "1",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030048.1.28",
          "form": "자영업자들 망하게하고",
          "original_form": "자영업자들 망하게하고",
          "speaker_id": "1",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100030048.1.29",
          "form": "어우 그건 덜떨어진 사람들이죠",
          "original_form": "어우 그건 덜떨어진 사람들이죠",
          "speaker_id": "2",
          "time": "20210904 10:08"
        }
      ]
    }
