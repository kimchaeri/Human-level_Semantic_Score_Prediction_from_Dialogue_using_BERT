      "utterance": [
        {
          "id": "MDRW2100006120.1.1",
          "form": "콘서트 가보신적있나요?",
          "original_form": "콘서트 가보신적있나요?",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006120.1.2",
          "form": "가보고싶어요!",
          "original_form": "가보고싶어요!",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006120.1.3",
          "form": "연극이마 뮤지컬은요?",
          "original_form": "연극이마 뮤지컬은요?",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006120.1.4",
          "form": "어릴때는봤었는데",
          "original_form": "어릴때는봤었는데",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006120.1.5",
          "form": "다 오년전이래요",
          "original_form": "다 오년전이래요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006120.1.6",
          "form": "저는 코로나 이후로는 한 번도 못갔어요",
          "original_form": "저는 코로나 이후로는 한 번도 못갔어요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006120.1.7",
          "form": "영화는 보셨나요?",
          "original_form": "영화는 보셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006120.1.8",
          "form": "아니요 극장도 안갔어요",
          "original_form": "아니요 극장도 안갔어요",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100006120.1.9",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006120.1.10",
          "form": "그럼 콘서트나",
          "original_form": "그럼 콘서트나",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006120.1.11",
          "form": "연극 뮤지컬은요?",
          "original_form": "연극 뮤지컬은요?",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006120.1.12",
          "form": "망할 코로나때문에 ㅜㅜ",
          "original_form": "망할 코로나때문에 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006120.1.13",
          "form": "전시회나 이런것도 다 취소되거나 해서 못갔어요 ㅜ",
          "original_form": "전시회나 이런것도 다 취소되거나 해서 못갔어요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006120.1.14",
          "form": "아....ㅠㅠㅠㅠㅠㅠ이런",
          "original_form": "아....ㅠㅠㅠㅠㅠㅠ이런",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006120.1.15",
          "form": "가보고싶은 가수 콘서트 있나요?",
          "original_form": "가보고싶은 가수 콘서트 있나요?",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006120.1.16",
          "form": "아이유!!@",
          "original_form": "아이유!!@",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006120.1.17",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006120.1.18",
