      "utterance": [
        {
          "id": "MDRW2100027966.1.1",
          "form": "여행 가고싶네요ㅠㅠ",
          "original_form": "여행 가고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027966.1.2",
          "form": "저는 제주도라 여행객들이 많이오세요",
          "original_form": "저는 제주도라 여행객들이 많이오세요",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027966.1.3",
          "form": "오늘은 비가와서 여행객들 다니지 못할거에요",
          "original_form": "오늘은 비가와서 여행객들 다니지 못할거에요",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027966.1.4",
          "form": "최근에 여행 가신곳이 어디인가요",
          "original_form": "최근에 여행 가신곳이 어디인가요",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027966.1.5",
          "form": "앗...제주도 요즘도 사람들 많이 가나요?",
          "original_form": "앗...제주도 요즘도 사람들 많이 가나요?",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027966.1.6",
          "form": "저는 작년에 제주도 다녀왔구",
          "original_form": "저는 작년에 제주도 다녀왔구",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100027966.1.7",
          "form": "네 렌터카 없어서 못빌릴정도래요",
          "original_form": "네 렌터카 없어서 못빌릴정도래요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.8",
          "form": "군산에 종종 가요. 할머니집이 있어요",
          "original_form": "군산에 종종 가요. 할머니집이 있어요",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.9",
          "form": "군산은 한번도 안가봤어요",
          "original_form": "군산은 한번도 안가봤어요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.10",
          "form": "헉 4단곈데도 사람이 많군요ㅠㅠ",
          "original_form": "헉 4단곈데도 사람이 많군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.11",
          "form": "자연경관이 좋은곳인가요",
          "original_form": "자연경관이 좋은곳인가요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.12",
          "form": "네 여기도  4단계에여",
          "original_form": "네 여기도  4단계에여",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.13",
          "form": "군산은...이성당이라는 빵집이 있어요",
          "original_form": "군산은...이성당이라는 빵집이 있어요",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.14",
          "form": "그래도 많이들 와요",
          "original_form": "그래도 많이들 와요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100027966.1.15",
          "form": "철길마을이랑 역사박물관이 가까이 있어서",
          "original_form": "철길마을이랑 역사박물관이 가까이 있어서",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027966.1.16",
          "form": "오 전통있는 곳이군요!",
          "original_form": "오 전통있는 곳이군요!",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027966.1.17",
          "form": "들어봤어요",
          "original_form": "들어봤어요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027966.1.18",
          "form": "근대역사 관련 관광코스 돌고",
          "original_form": "근대역사 관련 관광코스 돌고",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027966.1.19",
          "form": "멋지네요!",
          "original_form": "멋지네요!",
          "speaker_id": "1",
          "time": "20210902 16:30"
        }
      ]
