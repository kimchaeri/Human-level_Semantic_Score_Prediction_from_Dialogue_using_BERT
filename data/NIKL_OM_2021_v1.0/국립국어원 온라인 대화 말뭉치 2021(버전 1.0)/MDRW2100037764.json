      "utterance": [
        {
          "id": "MDRW2100037764.1.1",
          "form": "최근쇼핑템 잇으신가용",
          "original_form": "최근쇼핑템 잇으신가용",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100037764.1.2",
          "form": "이사를해서 자잘하게 사는게 많네요",
          "original_form": "이사를해서 자잘하게 사는게 많네요",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037764.1.3",
          "form": "아 이사하셧군요",
          "original_form": "아 이사하셧군요",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037764.1.4",
          "form": "축하드림대",
          "original_form": "축하드림대",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037764.1.5",
          "form": "다!!",
          "original_form": "다!!",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037764.1.6",
          "form": "정말 사도사도 끊임없이 살게 나오네요",
          "original_form": "정말 사도사도 끊임없이 살게 나오네요",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100037764.1.7",
          "form": "ㅋㅋㅋ당근거래도 이용해보셧나용",
          "original_form": "ㅋㅋㅋ당근거래도 이용해보셧나용",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037764.1.8",
          "form": "짐정리하면서 옛날거 좀 판매했어요",
          "original_form": "짐정리하면서 옛날거 좀 판매했어요",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037764.1.9",
          "form": "그럼 요즘 위시템들 잇으신가요",
          "original_form": "그럼 요즘 위시템들 잇으신가요",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037764.1.10",
          "form": "비가자주오니 건조기가 사고싶네요\r\n있으세요?",
          "original_form": "비가자주오니 건조기가 사고싶네요\r\n있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037764.1.11",
          "form": "내년에 이사하면 본격적으로 살꺼라서",
          "original_form": "내년에 이사하면 본격적으로 살꺼라서",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037764.1.12",
          "form": "지금은 없어요",
          "original_form": "지금은 없어요",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037764.1.13",
          "form": "필수템이져 건조기ㅎㅎ",
          "original_form": "필수템이져 건조기ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037764.1.14",
          "form": "네 냄새걱정없어 좋을거같아요",
          "original_form": "네 냄새걱정없어 좋을거같아요",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037764.1.15",
          "form": "지금은 제습기 틀어놓고 빨래말리는데 좋아요ㅋㅋ",
          "original_form": "지금은 제습기 틀어놓고 빨래말리는데 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037764.1.16",
          "form": "아 그거 좋을거같네요",
          "original_form": "아 그거 좋을거같네요",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037764.1.17",
          "form": "위닉스꺼 뽕뽑앗어요",
          "original_form": "위닉스꺼 뽕뽑앗어요",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037764.1.18",
          "form": "요즘날씨에 유용하죠",
          "original_form": "요즘날씨에 유용하죠",
          "speaker_id": "2",
