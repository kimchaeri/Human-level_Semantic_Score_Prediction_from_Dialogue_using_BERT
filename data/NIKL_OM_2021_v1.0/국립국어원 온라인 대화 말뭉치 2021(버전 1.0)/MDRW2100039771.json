      "utterance": [
        {
          "id": "MDRW2100039771.1.1",
          "form": "월세 때문인거죠?",
          "original_form": "월세 때문인거죠?",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039771.1.2",
          "form": "친한 사람인가요?",
          "original_form": "친한 사람인가요?",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039771.1.3",
          "form": "ㅋㅋㅋ넹넹\r\n완전 부랄친구요!! 한..12년친구!",
          "original_form": "ㅋㅋㅋ넹넹\r\n완전 부랄친구요!! 한..12년친구!",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039771.1.4",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039771.1.5",
          "form": "완전좋자나요",
          "original_form": "완전좋자나요",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039771.1.6",
          "form": "저도 친구랑 살면 심심할틈이없을듯요 ㅎㄹ",
          "original_form": "저도 친구랑 살면 심심할틈이없을듯요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039771.1.7",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039771.1.8",
          "form": "그대신 집안일은 잘 분배해야할듯해요",
          "original_form": "그대신 집안일은 잘 분배해야할듯해요",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039771.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ솔직히..벌레만 잡아주면\r\n제가 다 해도 괜찮을거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ솔직히..벌레만 잡아주면\r\n제가 다 해도 괜찮을거같아요",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039771.1.10",
          "form": "헉 혜자친구",
          "original_form": "헉 혜자친구",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039771.1.11",
          "form": "벌레는 에프킬라나 그 치약처럼 생긴 바퀴약이면 끝나요",
          "original_form": "벌레는 에프킬라나 그 치약처럼 생긴 바퀴약이면 끝나요",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039771.1.12",
          "form": "헉...그치만..벌레를 못잡아요 ㅋㅋㅋㅋㅋ\r\n징그러워서ㅠㅠㅠㅠㅠ아주 쫄보랍니다..",
          "original_form": "헉...그치만..벌레를 못잡아요 ㅋㅋㅋㅋㅋ\r\n징그러워서ㅠㅠㅠㅠㅠ아주 쫄보랍니다..",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039771.1.13",
          "form": "벌레나오면 당근마켓에 도와달라고 올릴것같아요 ㅋㅋㅋ",
          "original_form": "벌레나오면 당근마켓에 도와달라고 올릴것같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039771.1.14",
          "form": "맥스포스겔  뿌리면",
          "original_form": "맥스포스겔  뿌리면",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039771.1.15",
          "form": "바퀴는  박멸이에요",
          "original_form": "바퀴는  박멸이에요",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039771.1.16",
          "form": "저도ㅜ바퀴 너무 무서워요 ㅜㅜㅠ",
          "original_form": "저도ㅜ바퀴 너무 무서워요 ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039771.1.17",
          "form": "헐..그럼 눈앞에도 안보이나요?? 그냥..하수구에서 혼자 죽을까요..??",
          "original_form": "헐..그럼 눈앞에도 안보이나요?? 그냥..하수구에서 혼자 죽을까요..??",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039771.1.18",
          "form": "자기 구역 동료바퀴랑 약을 나눠먹고",
          "original_form": "자기 구역 동료바퀴랑 약을 나눠먹고",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039771.1.19",
          "form": "같이 죽는대요",
          "original_form": "같이 죽는대요",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039771.1.20",
          "form": "처음 돌아댕길때 잡지말고 약을 곳곳에 짜놔요",
          "original_form": "처음 돌아댕길때 잡지말고 약을 곳곳에 짜놔요",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.21",
          "form": "바퀴 전멸돼요 ㅎㅎ",
          "original_form": "바퀴 전멸돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.22",
          "form": "헐..꿀팁이네요..",
          "original_form": "헐..꿀팁이네요..",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.23",
          "form": "세스코 필요없다는",
          "original_form": "세스코 필요없다는",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.24",
          "form": "근데 사실 바퀴나오면...집을 버릴것같아요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "근데 사실 바퀴나오면...집을 버릴것같아요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.25",
          "form": "유명한약이에요",
          "original_form": "유명한약이에요",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.26",
          "form": "싹사라져요 그약하나면 돼요 ㅜㅜ",
          "original_form": "싹사라져요 그약하나면 돼요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039771.1.27",
          "form": "ㅋㅋㅋㅋㅋ넹\r\n자취할때 나오면 꼭!! 사용해볼게요",
