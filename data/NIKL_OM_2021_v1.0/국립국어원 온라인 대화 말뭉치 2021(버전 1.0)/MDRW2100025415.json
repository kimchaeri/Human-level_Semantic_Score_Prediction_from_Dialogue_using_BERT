      "utterance": [
        {
          "id": "MDRW2100025415.1.1",
          "form": "아침에 좀 내렸어요",
          "original_form": "아침에 좀 내렸어요",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025415.1.2",
          "form": "여긴 아직",
          "original_form": "여긴 아직",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025415.1.3",
          "form": "계속 비 올거라던데요",
          "original_form": "계속 비 올거라던데요",
          "speaker_id": "2",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100025415.1.4",
          "form": "지겹네여",
          "original_form": "지겹네여",
          "speaker_id": "1",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100025415.1.5",
          "form": "날 좀 개면 좋겠네요",
          "original_form": "날 좀 개면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025415.1.6",
          "form": "습해서 더워요",
          "original_form": "습해서 더워요",
          "speaker_id": "1",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025415.1.7",
          "form": "날 좀 지나면 더 추워질거같아요",
          "original_form": "날 좀 지나면 더 추워질거같아요",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025415.1.8",
          "form": "금방 가을이죠",
          "original_form": "금방 가을이죠",
          "speaker_id": "1",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025415.1.9",
          "form": "요샌 가을도 잠깐이라ㅎ",
          "original_form": "요샌 가을도 잠깐이라ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025415.1.10",
          "form": "바로ㅜ겨율이요",
          "original_form": "바로ㅜ겨율이요",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025415.1.11",
          "form": "너무 춥지만 않으면 좋을거 같아요",
          "original_form": "너무 춥지만 않으면 좋을거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025415.1.12",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025415.1.13",
          "form": "긴 옷도 꺼내야겠네요",
          "original_form": "긴 옷도 꺼내야겠네요",
          "speaker_id": "2",
          "time": "20210901 14:24"
        },
        {
          "id": "MDRW2100025415.1.14",
          "form": "정리가 필요하죠",
          "original_form": "정리가 필요하죠",
          "speaker_id": "1",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100025415.1.15",
          "form": "선선하죠?",
          "original_form": "선선하죠?",
          "speaker_id": "1",
