      "utterance": [
        {
          "id": "MDRW2100022397.1.1",
          "form": "언니는 나중에 결혼이 하고싶어?",
          "original_form": "언니는 나중에 결혼이 하고싶어?",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022397.1.2",
          "form": "나 요즘 로봇청소기가 청소해주니까 너무 편해",
          "original_form": "나 요즘 로봇청소기가 청소해주니까 너무 편해",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022397.1.3",
          "form": "결혼? 음 글쎄",
          "original_form": "결혼? 음 글쎄",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022397.1.4",
          "form": "로봇청소기 있으면 결혼 안해도 되지",
          "original_form": "로봇청소기 있으면 결혼 안해도 되지",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022397.1.5",
          "form": "ㅋㅋㅋㅋㅋ음쓰버려주는 로봇청소기가 나오면 결혼안할듯",
          "original_form": "ㅋㅋㅋㅋㅋ음쓰버려주는 로봇청소기가 나오면 결혼안할듯",
          "speaker_id": "1",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022397.1.6",
          "form": "그치 아무래도 남편의 용도는 청소니깐...",
          "original_form": "그치 아무래도 남편의 용도는 청소니깐...",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022397.1.7",
          "form": "앜ㅋㅋㅋㅋ그래도 돈도벌어오고 청소도하니까 로봇보다는 나을지도..",
          "original_form": "앜ㅋㅋㅋㅋ그래도 돈도벌어오고 청소도하니까 로봇보다는 나을지도..",
          "speaker_id": "1",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022397.1.8",
          "form": "아 그치 로봇은 돈은 못버니깐....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그치 로봇은 돈은 못버니깐....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022397.1.9",
          "form": "너는 나중에 결혼하고싶어?",
          "original_form": "너는 나중에 결혼하고싶어?",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022397.1.10",
          "form": "아닠ㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022397.1.11",
          "form": "이유가 있어?",
          "original_form": "이유가 있어?",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022397.1.12",
          "form": "결혼 제도가 내 맘에 안들어서ㅋㅋㅋ 좋은 사람 만나면 동거까지는 해도 결혼은 안할거 같아",
          "original_form": "결혼 제도가 내 맘에 안들어서ㅋㅋㅋ 좋은 사람 만나면 동거까지는 해도 결혼은 안할거 같아",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022397.1.13",
          "form": "아 하긴 동거해도 청소랑 음쓰버리기 시킬수있으니까 괜찮네..",
          "original_form": "아 하긴 동거해도 청소랑 음쓰버리기 시킬수있으니까 괜찮네..",
          "speaker_id": "1",
          "time": "20210829 13:30"
        }
      ]
