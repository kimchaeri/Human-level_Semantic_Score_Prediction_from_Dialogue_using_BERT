      "utterance": [
        {
          "id": "MDRW2100008043.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008043.1.2",
          "form": "무교요",
          "original_form": "무교요",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008043.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008043.1.4",
          "form": "부모님은불교요",
          "original_form": "부모님은불교요",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008043.1.5",
          "form": "저도 부모님 불교요 ㅎㅎ",
          "original_form": "저도 부모님 불교요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008043.1.6",
          "form": "다들 부모님 불교 본인무교 많네요",
          "original_form": "다들 부모님 불교 본인무교 많네요",
          "speaker_id": "2",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008043.1.7",
          "form": "어른들은 대부분이죠",
          "original_form": "어른들은 대부분이죠",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008043.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008043.1.9",
          "form": "매년 엄마가 연등 달아 주세요 ㅎㅎ",
          "original_form": "매년 엄마가 연등 달아 주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008043.1.10",
          "form": "항상 감사해요 ㅎㅎ",
          "original_form": "항상 감사해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008043.1.11",
          "form": "연등 비싸던데",
          "original_form": "연등 비싸던데",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008043.1.12",
          "form": "비싸죠 절이 그날 벌어야 하니",
          "original_form": "비싸죠 절이 그날 벌어야 하니",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008043.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008043.1.14",
          "form": "그날 비빔밥 맛나요",
          "original_form": "그날 비빔밥 맛나요",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008043.1.15",
          "form": "공양밤 좋죠",
          "original_form": "공양밤 좋죠",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008043.1.16",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008043.1.17",
          "form": "근데 줄이 길어서 ㅡㅡ",
          "original_form": "근데 줄이 길어서 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008043.1.18",
          "form": "종교는 보면 불교가 좋은듯 해요",
          "original_form": "종교는 보면 불교가 좋은듯 해요",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008043.1.19",
          "form": "강요없고",
          "original_form": "강요없고",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008043.1.20",
