      "utterance": [
        {
          "id": "MDRW2100029702.1.1",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100029702.1.2",
          "form": "저는 김치찌개 좋아해요! 부대찌개도요!",
          "original_form": "저는 김치찌개 좋아해요! 부대찌개도요!",
          "speaker_id": "2",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029702.1.3",
          "form": "먹어도 먹어도 안질려요ㅠ",
          "original_form": "먹어도 먹어도 안질려요ㅠ",
          "speaker_id": "2",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029702.1.4",
          "form": "찌개 종류 좋죠~",
          "original_form": "찌개 종류 좋죠~",
          "speaker_id": "1",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100029702.1.5",
          "form": "가끔 짠거 확 당길때 밥이랑 먹으면 진짜 환상이에요",
          "original_form": "가끔 짠거 확 당길때 밥이랑 먹으면 진짜 환상이에요",
          "speaker_id": "1",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100029702.1.6",
          "form": "맞아요ㅠ 많이 먹으면 소금 섭취 너무 많이 했을까봐 겁나지만...",
          "original_form": "맞아요ㅠ 많이 먹으면 소금 섭취 너무 많이 했을까봐 겁나지만...",
          "speaker_id": "2",
          "time": "20210903 12:05"
        },
        {
          "id": "MDRW2100029702.1.7",
          "form": "진짜 밥도둑이에요ㅠ",
          "original_form": "진짜 밥도둑이에요ㅠ",
          "speaker_id": "2",
          "time": "20210903 12:05"
        },
        {
          "id": "MDRW2100029702.1.8",
          "form": "그리고 카레도 좋구 파스타도 좋아해요",
          "original_form": "그리고 카레도 좋구 파스타도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 12:05"
        },
        {
          "id": "MDRW2100029702.1.9",
          "form": "카레 좋죠!! 저는 일본식 카레 진짜 좋아해요",
          "original_form": "카레 좋죠!! 저는 일본식 카레 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029702.1.10",
          "form": "튀김이랑 곁들여 먹으면 너무 맛있어요ㅠㅠ",
          "original_form": "튀김이랑 곁들여 먹으면 너무 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029702.1.11",
          "form": "저는 카레 한국 일본 인도식 다 좋더라구요ㅠㅠ",
          "original_form": "저는 카레 한국 일본 인도식 다 좋더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029702.1.12",
          "form": "맞아요 일본식은 진짜 튀김이죠..",
          "original_form": "맞아요 일본식은 진짜 튀김이죠..",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029702.1.13",
          "form": "저는 가라아게랑 먹는거 좋아해요 ㅠ0ㅠ",
          "original_form": "저는 가라아게랑 먹는거 좋아해요 ㅠ0ㅠ",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029702.1.14",
          "form": "헉 저는 완전 제대로 된 인도식은 먹어본 적이 없어서 궁금하네요",
          "original_form": "헉 저는 완전 제대로 된 인도식은 먹어본 적이 없어서 궁금하네요",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100029702.1.15",
          "form": "가라아게는 진짜 인정이죠...",
          "original_form": "가라아게는 진짜 인정이죠...",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100029702.1.16",
          "form": "현지인이 요리사 하고 있는 커리집 한번 찾아가보세요!!",
          "original_form": "현지인이 요리사 하고 있는 커리집 한번 찾아가보세요!!",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029702.1.17",
          "form": "난이랑 같이 먹으면 정말 맛있서요ㅠ 저는 허니난 좋아해요",
          "original_form": "난이랑 같이 먹으면 정말 맛있서요ㅠ 저는 허니난 좋아해요",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100029702.1.18",
          "form": "그리고 라씨도!!",
          "original_form": "그리고 라씨도!!",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100029702.1.19",
          "form": "우와 뭔가 전문가 같으세요 ㅋㅋㅋㅋ",
          "original_form": "우와 뭔가 전문가 같으세요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029702.1.20",
          "form": "일식 중식 한식 양식 이런거 중에선 뭘 젤 좋아하시나용",
          "original_form": "일식 중식 한식 양식 이런거 중에선 뭘 젤 좋아하시나용",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100029702.1.21",
          "form": "저는 양식을 제일 좋아해요!",
          "original_form": "저는 양식을 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029702.1.22",
          "form": "집에서 야매로 크림스튜나 크림 리조또 만들어먹는거도 좋아하거든요 ㅎㅎ",
          "original_form": "집에서 야매로 크림스튜나 크림 리조또 만들어먹는거도 좋아하거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100029702.1.23",
          "form": "아하 파스타같은건 만들기 되게 쉽다고 들어봤는데",
          "original_form": "아하 파스타같은건 만들기 되게 쉽다고 들어봤는데",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029702.1.24",
          "form": "직접 만들어본 적은 이상하게 없네요ㅠㅋㅋㅋ",
          "original_form": "직접 만들어본 적은 이상하게 없네요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029702.1.25",
          "form": "저도 시중에서 파는 소스로만 해요ㅠㅠ 직접 해먹는건 엄두가 안나가지구..",
          "original_form": "저도 시중에서 파는 소스로만 해요ㅠㅠ 직접 해먹는건 엄두가 안나가지구..",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100029702.1.26",
          "form": "생각보다 퀄리티가 괜찮아요",
          "original_form": "생각보다 퀄리티가 괜찮아요",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100029702.1.27",
          "form": "오오 ㅎㅎ 저도 다음에 한 번 시도해볼게요",
          "original_form": "오오 ㅎㅎ 저도 다음에 한 번 시도해볼게요",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100029702.1.28",
          "form": "추천해 주고 싶은 레시피 있으신가요?",
          "original_form": "추천해 주고 싶은 레시피 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 22:34"
        }
      ]
