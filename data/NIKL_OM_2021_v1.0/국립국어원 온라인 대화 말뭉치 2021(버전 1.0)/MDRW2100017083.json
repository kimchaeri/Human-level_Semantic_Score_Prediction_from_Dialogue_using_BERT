        {
          "id": "MDRW2100017083.1.1",
          "form": "강아지는 귀엽지",
          "original_form": "강아지는 귀엽지",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100017083.1.2",
          "form": "강아지는 귀여워",
          "original_form": "강아지는 귀여워",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017083.1.3",
          "form": "마치 두리처럼!",
          "original_form": "마치 두리처럼!",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017083.1.4",
          "form": "그렇지!",
          "original_form": "그렇지!",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017083.1.5",
          "form": "두리 강아지는 왜 이름이 두리야",
          "original_form": "두리 강아지는 왜 이름이 두리야",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017083.1.6",
          "form": "두리는 두리안의 두리야",
          "original_form": "두리는 두리안의 두리야",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017083.1.7",
          "form": "웰시코기 키워야지 결심했을때 두리안 포스터를 봤거든",
          "original_form": "웰시코기 키워야지 결심했을때 두리안 포스터를 봤거든",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017083.1.8",
          "form": "농담이야 진담이야",
          "original_form": "농담이야 진담이야",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017083.1.9",
          "form": "진담이야",
          "original_form": "진담이야",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017083.1.10",
          "form": "진짜로",
          "original_form": "진짜로",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017083.1.11",
          "form": "버스타고 가는데",
          "original_form": "버스타고 가는데",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017083.1.12",
          "form": "벽에 붙어있는걸 봤어",
          "original_form": "벽에 붙어있는걸 봤어",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017083.1.13",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017083.1.14",
          "form": "상상도 못한 이유",
          "original_form": "상상도 못한 이유",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017083.1.15",
          "form": "그렇지..사람들도 농담인줄 알더라구",
          "original_form": "그렇지..사람들도 농담인줄 알더라구",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017083.1.16",
          "form": "아무리 봐도 농담 같은데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무리 봐도 농담 같은데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017083.1.17",
          "form": "ㅋㅋㄱㅋㄱㅋ아녀요 진짜여요",
