      "utterance": [
        {
          "id": "MDRW2100028792.1.1",
          "form": "요즘 날씨 어떤거같애?",
          "original_form": "요즘 날씨 어떤거같애?",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028792.1.2",
          "form": "요즘은 전보다 덜 더운것같애",
          "original_form": "요즘은 전보다 덜 더운것같애",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028792.1.3",
          "form": "그래서 좋아",
          "original_form": "그래서 좋아",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028792.1.4",
          "form": "일리가있어",
          "original_form": "일리가있어",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028792.1.5",
          "form": "미세먼지 요즘에 많나?",
          "original_form": "미세먼지 요즘에 많나?",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028792.1.6",
          "form": "잘 모르겟어",
          "original_form": "잘 모르겟어",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028792.1.7",
          "form": "미세먼지도 요즘은 덜한것같아",
          "original_form": "미세먼지도 요즘은 덜한것같아",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028792.1.8",
          "form": "글쿠나 !!",
          "original_form": "글쿠나 !!",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.9",
          "form": "역시 가을쯤 되면",
          "original_form": "역시 가을쯤 되면",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.10",
          "form": "좀 나아지는 거 같아 ㅎㅎ",
          "original_form": "좀 나아지는 거 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.11",
          "form": "여름이 제일 별루야",
          "original_form": "여름이 제일 별루야",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.12",
          "form": "하지만 미세먼지가 있든 없든 어차피 마스크는 써야댄다는 사실...",
          "original_form": "하지만 미세먼지가 있든 없든 어차피 마스크는 써야댄다는 사실...",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.13",
          "form": "덥구",
          "original_form": "덥구",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.14",
          "form": "벌레두 많잖어",
          "original_form": "벌레두 많잖어",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.15",
          "form": "안그래?",
          "original_form": "안그래?",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.16",
          "form": "난 여름 좋던뎅",
          "original_form": "난 여름 좋던뎅",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.17",
          "form": "언닌 무슨 계절이 제일 좋아?",
          "original_form": "언닌 무슨 계절이 제일 좋아?",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.18",
          "form": "헐.",
          "original_form": "헐.",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.19",
          "form": "나무가 초록초록해서 보기좋잖아 ㅎㅎ",
          "original_form": "나무가 초록초록해서 보기좋잖아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.20",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.22",
          "form": "겨울은 너무 노잼이야",
          "original_form": "겨울은 너무 노잼이야",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.23",
          "form": "벌레가 없는건 좋아 ㅎㅎ",
          "original_form": "벌레가 없는건 좋아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.24",
          "form": "눈 오잖아",
          "original_form": "눈 오잖아",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028792.1.25",
          "form": "그렇지 ㅎㅎ",
          "original_form": "그렇지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028792.1.26",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028792.1.27",
          "form": "여름엔 비오잖아",
          "original_form": "여름엔 비오잖아",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028792.1.28",
          "form": "비오는것도 좋아",
          "original_form": "비오는것도 좋아",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028792.1.29",
          "form": "요즘엔 무슨 옷을 입어야하는걸까..",
          "original_form": "요즘엔 무슨 옷을 입어야하는걸까..",
          "speaker_id": "1",
          "time": "20210903 23:50"
        }
      ]
    }
  ]
