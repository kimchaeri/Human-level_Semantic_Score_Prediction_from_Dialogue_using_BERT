      "utterance": [
        {
          "id": "MDRW2100022353.1.1",
          "form": "저 지금 냄비에 물 올리고 왔어요",
          "original_form": "저 지금 냄비에 물 올리고 왔어요",
          "speaker_id": "2",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100022353.1.2",
          "form": "물 올리고오셨구나",
          "original_form": "물 올리고오셨구나",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.3",
          "form": "네 저 고민 많이 했어요",
          "original_form": "네 저 고민 많이 했어요",
          "speaker_id": "2",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.4",
          "form": "어떤고민이요",
          "original_form": "어떤고민이요",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.5",
          "form": "처음부터 참치뽈찜 넣고 끓일지 물부터 끓이고 넣을지요",
          "original_form": "처음부터 참치뽈찜 넣고 끓일지 물부터 끓이고 넣을지요",
          "speaker_id": "2",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.6",
          "form": "넣고끓이면",
          "original_form": "넣고끓이면",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.7",
          "form": "좀 더 빨리익지않을까요",
          "original_form": "좀 더 빨리익지않을까요",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.8",
          "form": "아 그래요? 근데 뭔가 질겨지거나 그럴까봐요 ㅠ",
          "original_form": "아 그래요? 근데 뭔가 질겨지거나 그럴까봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.9",
          "form": "서서히 익으니까 들여다 볼 필요없지않나",
          "original_form": "서서히 익으니까 들여다 볼 필요없지않나",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022353.1.10",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.11",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.12",
          "form": "제가 뽈찜을 안먹어봐서.. 음..",
          "original_form": "제가 뽈찜을 안먹어봐서.. 음..",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.13",
          "form": "이거 배송받자마자 포장지 바로 버려가지고",
          "original_form": "이거 배송받자마자 포장지 바로 버려가지고",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.14",
          "form": "그럼 끓으면 넣으세요",
          "original_form": "그럼 끓으면 넣으세요",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.15",
          "form": "기억에 나는건 7분정도 끓여라",
          "original_form": "기억에 나는건 7분정도 끓여라",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.16",
          "form": "이거에요 ㅋㅋㅋㅋ",
          "original_form": "이거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.17",
          "form": "비닐팩 뜯어서 넣고 싶은데 통째로 데우래요",
          "original_form": "비닐팩 뜯어서 넣고 싶은데 통째로 데우래요",
          "speaker_id": "2",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.18",
          "form": "아 그럼 끓으면 데우세요",
          "original_form": "아 그럼 끓으면 데우세요",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100022353.1.19",
          "form": "네 그래야겠어요",
          "original_form": "네 그래야겠어요",
          "speaker_id": "2",
          "time": "20210829 13:01"
        }
      ]
