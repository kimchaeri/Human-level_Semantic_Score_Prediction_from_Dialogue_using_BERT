      "utterance": [
        {
          "id": "MDRW2100041091.1.1",
          "form": "ㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅌㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅌㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041091.1.2",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041091.1.3",
          "form": "나이많은사람좋아하니까",
          "original_form": "나이많은사람좋아하니까",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041091.1.4",
          "form": "아 저기요",
          "original_form": "아 저기요",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041091.1.5",
          "form": "물론 맞긴하지만",
          "original_form": "물론 맞긴하지만",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041091.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041091.1.7",
          "form": "어쨋든",
          "original_form": "어쨋든",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.8",
          "form": "무슨얘기하고있었지?",
          "original_form": "무슨얘기하고있었지?",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.9",
          "form": "전도연 차기작이 망한 얘기",
          "original_form": "전도연 차기작이 망한 얘기",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.10",
          "form": "전도연은 연기 진심 잘한대",
          "original_form": "전도연은 연기 진심 잘한대",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.11",
          "form": "거기서 유일한 빛이래",
          "original_form": "거기서 유일한 빛이래",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.13",
          "form": "다만 큰 진입장벽이 있을 뿐",
          "original_form": "다만 큰 진입장벽이 있을 뿐",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.14",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.15",
          "form": "미션같은느낌",
          "original_form": "미션같은느낌",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.16",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.17",
          "form": "남주가너무",
          "original_form": "남주가너무",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.18",
          "form": "장벽임",
          "original_form": "장벽임",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041091.1.19",
          "form": "그니가 그럴 거면 아예 빼라고",
          "original_form": "그니가 그럴 거면 아예 빼라고",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.20",
          "form": "뭐하는 거야...",
          "original_form": "뭐하는 거야...",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.21",
          "form": "걔는양심이있으면",
          "original_form": "걔는양심이있으면",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.22",
          "form": "은퇴했으면함",
          "original_form": "은퇴했으면함",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.23",
          "form": "류쥰열",
          "original_form": "류쥰열",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.24",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.25",
          "form": "기만이야",
          "original_form": "기만이야",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.26",
          "form": "그얼굴로배우한다는게",
          "original_form": "그얼굴로배우한다는게",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.27",
          "form": "걔 진짜 되먹지도 않는 설정 많이 맡는다고",
          "original_form": "걔 진짜 되먹지도 않는 설정 많이 맡는다고",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.28",
          "form": "여자들기만하는거아님?",
          "original_form": "여자들기만하는거아님?",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.29",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.30",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041091.1.31",
          "form": "운빨로맨스 보고",
          "original_form": "운빨로맨스 보고",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041091.1.32",
          "form": "걔를써주는게신기하다",
          "original_form": "걔를써주는게신기하다",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041091.1.33",
          "form": "뒷목ㅈ잡았잖아",
          "original_form": "뒷목ㅈ잡았잖아",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041091.1.34",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041091.1.35",
          "form": "제발 잘생긴 역에는 잘생긴 애를 쓰라고",
          "original_form": "제발 잘생긴 역에는 잘생긴 애를 쓰라고",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041091.1.36",
          "form": "걔는로맨스대본",
          "original_form": "걔는로맨스대본",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041091.1.37",
          "form": "받으면",
          "original_form": "받으면",
          "speaker_id": "1",
          "time": "20210905 16:53"
        }
      ]
    }
