      "utterance": [
        {
          "id": "MDRW2100021396.1.1",
          "form": "케이크 데코 동아리요?",
          "original_form": "케이크 데코 동아리요?",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021396.1.2",
          "form": "네 데코만 하는거에요 ㅋㅋㅋ",
          "original_form": "네 데코만 하는거에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021396.1.3",
          "form": "완성된 케이크에 데코만하는 건가봐요",
          "original_form": "완성된 케이크에 데코만하는 건가봐요",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021396.1.4",
          "form": "네 시트는 주고",
          "original_form": "네 시트는 주고",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021396.1.5",
          "form": "크림바르고 꾸미고 ㅋㅋㅋㅋ 그런 간단한거였어요 중학교때였거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "크림바르고 꾸미고 ㅋㅋㅋㅋ 그런 간단한거였어요 중학교때였거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021396.1.6",
          "form": "재미있는 동아리네요",
          "original_form": "재미있는 동아리네요",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100021396.1.7",
          "form": "저는 학생때 한지공예 십자수 이런 동아리했어쇼",
          "original_form": "저는 학생때 한지공예 십자수 이런 동아리했어쇼",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021396.1.8",
          "form": "네 재밌었어요 ㅋㅋ",
          "original_form": "네 재밌었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021396.1.9",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021396.1.10",
          "form": "십자수 추억이네요 ㅋㅋㅋ",
          "original_form": "십자수 추억이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021396.1.11",
          "form": "네네 한때 열심히 했는데",
          "original_form": "네네 한때 열심히 했는데",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021396.1.12",
          "form": "요즘 참여하시는 동호회 있으세요?",
          "original_form": "요즘 참여하시는 동호회 있으세요?",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100021396.1.13",
          "form": "요즘엔 없어요 있으신가요??",
          "original_form": "요즘엔 없어요 있으신가요??",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021396.1.14",
          "form": "저도 요즘은 동호회 활동은 못하고 있어요",
          "original_form": "저도 요즘은 동호회 활동은 못하고 있어요",
          "speaker_id": "1",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100021396.1.15",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 18:36"
        },
        {
          "id": "MDRW2100021396.1.16",
          "form": "랜선으로 독서모임 정도는 참여해볼까 싶긴 해요",
          "original_form": "랜선으로 독서모임 정도는 참여해볼까 싶긴 해요",
          "speaker_id": "1",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100021396.1.17",
          "form": "요새는 랜선모임도 있군요",
          "original_form": "요새는 랜선모임도 있군요",
