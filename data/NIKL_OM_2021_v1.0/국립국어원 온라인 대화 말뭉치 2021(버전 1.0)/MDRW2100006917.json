      "utterance": [
        {
          "id": "MDRW2100006917.1.1",
          "form": "아직 우리에겐 일요일\r\n삼시세.끼가 남아있습니다",
          "original_form": "아직 우리에겐 일요일\r\n삼시세.끼가 남아있습니다",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006917.1.2",
          "form": "내일의 메뉴는 뭔가요 ㅋㅋㅋ",
          "original_form": "내일의 메뉴는 뭔가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006917.1.3",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006917.1.4",
          "form": "일단 고기죠",
          "original_form": "일단 고기죠",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006917.1.5",
          "form": "스테이크 고기 샀어요",
          "original_form": "스테이크 고기 샀어요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006917.1.6",
          "form": "구워서 먹게요",
          "original_form": "구워서 먹게요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006917.1.7",
          "form": "샐러드랑 ㅋㅋ",
          "original_form": "샐러드랑 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006917.1.8",
          "form": "뭐 드실거예요?",
          "original_form": "뭐 드실거예요?",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006917.1.9",
          "form": "생각해보니 그것밖에 없네요",
          "original_form": "생각해보니 그것밖에 없네요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006917.1.10",
          "form": "저는 오늘은 보쌈해서 먹었고",
          "original_form": "저는 오늘은 보쌈해서 먹었고",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006917.1.11",
          "form": "내일은 생선 굽굽하고",
          "original_form": "내일은 생선 굽굽하고",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006917.1.12",
          "form": "또 뭘해먹을지 모르겠어서요ㅠㅋㅋ",
          "original_form": "또 뭘해먹을지 모르겠어서요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006917.1.13",
          "form": "삼겹이 사다 구울까요?ㅋㅋ",
          "original_form": "삼겹이 사다 구울까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006917.1.14",
          "form": "오 정말 맛난거 하셨네요",
          "original_form": "오 정말 맛난거 하셨네요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006917.1.15",
          "form": "네 주말은 섬겹이죠",
          "original_form": "네 주말은 섬겹이죠",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006917.1.16",
          "form": "삼겹",
          "original_form": "삼겹",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006917.1.17",
          "form": "남편이 좋아하고\r\n아이들도 잘 먹어서\r\n보쌈은 종종 하게 되는거 같아요ㅋㅋ",
          "original_form": "남편이 좋아하고\r\n아이들도 잘 먹어서\r\n보쌈은 종종 하게 되는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006917.1.18",
          "form": "고기밖에 생각이 안나서 저도 고기 샀어요",
          "original_form": "고기밖에 생각이 안나서 저도 고기 샀어요",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006917.1.19",
          "form": "보쌈 하나면 최고죠",
          "original_form": "보쌈 하나면 최고죠",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006917.1.20",
          "form": "오늘 쌈도 많이 남았는데\r\n내일은 삼겹이 당첨!! ㅋㅋ",
          "original_form": "오늘 쌈도 많이 남았는데\r\n내일은 삼겹이 당첨!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006917.1.21",
          "form": "애들도 좋아하고",
          "original_form": "애들도 좋아하고",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006917.1.22",
          "form": "그래요 남으면 또 고고 죠",
          "original_form": "그래요 남으면 또 고고 죠",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006917.1.23",
          "form": "이렇게 또 한 끼가 해결되네요 ㅋㅋ",
          "original_form": "이렇게 또 한 끼가 해결되네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006917.1.24",
          "form": "네 매일 그게 젤 고민돼요",
          "original_form": "네 매일 그게 젤 고민돼요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006917.1.25",
          "form": "뭘 할지",
          "original_form": "뭘 할지",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006917.1.26",
          "form": "저희딸과 커피숍 대이트 즐겼는데 다욧 한다고 안가서 전 디저트 중단 중이예요",
          "original_form": "저희딸과 커피숍 대이트 즐겼는데 다욧 한다고 안가서 전 디저트 중단 중이예요",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006917.1.27",
          "form": "아 디저트 정말 좋아 하는데",
          "original_form": "아 디저트 정말 좋아 하는데",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006917.1.28",
          "form": "오잉 벌써 커피숍 데이트가 가능해요?ㄱㅋ",
          "original_form": "오잉 벌써 커피숍 데이트가 가능해요?ㄱㅋ",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006917.1.29",
          "form": "네 초6요",
          "original_form": "네 초6요",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006917.1.30",
          "form": "와 다 키우셨네요!",
          "original_form": "와 다 키우셨네요!",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006917.1.31",
          "form": "음료 마시니까요",
          "original_form": "음료 마시니까요",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006917.1.32",
          "form": "디저트나",
          "original_form": "디저트나",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006917.1.33",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006917.1.34",
          "form": "근데 아직 멀었어요",
          "original_form": "근데 아직 멀었어요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006917.1.35",
          "form": "중2 무섭네요",
          "original_form": "중2 무섭네요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006917.1.36",
          "form": "케이크 빵 먹었었는데 안가네요",
          "original_form": "케이크 빵 먹었었는데 안가네요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006917.1.37",
          "form": "점심은  낼간단히 국수 할까봐요",
          "original_form": "점심은  낼간단히 국수 할까봐요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006917.1.38",
          "form": "애들이 국수 좋아해요 ㅎㅎ",
          "original_form": "애들이 국수 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006917.1.39",
          "form": "2끼만 하고 싶네요 ㅋㅋ",
          "original_form": "2끼만 하고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006917.1.40",
          "form": "우왕 국수 레시피 공유 좀 해주세요ㅋㅋ",
          "original_form": "우왕 국수 레시피 공유 좀 해주세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006917.1.41",
          "form": "엄마가 해주는 비빔국수가 땡기네요ㅋ",
          "original_form": "엄마가 해주는 비빔국수가 땡기네요ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006917.1.42",
          "form": "그런거 없어요 ㅋㅋ",
          "original_form": "그런거 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006917.1.43",
          "form": "멸치 육수에 기냥 달걀지단 많이요",
          "original_form": "멸치 육수에 기냥 달걀지단 많이요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006917.1.44",
          "form": "맞아요 아 열무국수 비빔 땡기네요 ㅋㅋ",
          "original_form": "맞아요 아 열무국수 비빔 땡기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006917.1.45",
          "form": "엄마가 김치 잘담그셔서 열무비빔 맛나요",
          "original_form": "엄마가 김치 잘담그셔서 열무비빔 맛나요",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006917.1.46",
          "form": "하... 저도 엄마김치 맛있는데",
          "original_form": "하... 저도 엄마김치 맛있는데",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006917.1.47",
          "form": "때마침 열무김치도 주신거 있는데",
          "original_form": "때마침 열무김치도 주신거 있는데",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006917.1.48",
          "form": "내일은 비벼줘야겠어요 ㅋㅋ",
          "original_form": "내일은 비벼줘야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006917.1.49",
          "form": "달걀지단 듬뿍 넣은 국수도 제 스탈",
          "original_form": "달걀지단 듬뿍 넣은 국수도 제 스탈{emoji:♡}",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006917.1.50",
          "form": "네 비비세요",
          "original_form": "네 비비세요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006917.1.51",
          "form": "국수는 진리죠 ㅎㅎ",
          "original_form": "국수는 진리죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:13"
        }
