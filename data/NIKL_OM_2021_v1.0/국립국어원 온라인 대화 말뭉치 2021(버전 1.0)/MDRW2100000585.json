        },
        {
          "id": "MDRW2100000585.1.5",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000585.1.6",
          "form": "저도 가족이랑 살아요",
          "original_form": "저도 가족이랑 살아요",
          "speaker_id": "1",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000585.1.7",
          "form": "독립하고싶네요",
          "original_form": "독립하고싶네요",
          "speaker_id": "1",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000585.1.8",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000585.1.9",
          "form": "근데 저두요",
          "original_form": "근데 저두요",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000585.1.10",
          "form": "집안일 다 저 혼자 해도 되니까",
          "original_form": "집안일 다 저 혼자 해도 되니까",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000585.1.11",
          "form": "혼자 살고 싶어요",
          "original_form": "혼자 살고 싶어요",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000585.1.12",
          "form": "방금 저도 그 생각했어요",
          "original_form": "방금 저도 그 생각했어요",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000585.1.13",
          "form": "호호",
          "original_form": "호호",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000585.1.14",
          "form": "외롭지 않으려나요",
          "original_form": "외롭지 않으려나요",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000585.1.15",
          "form": "학교때문에 기숙사에 한달쯤 있었던 기간이 있었는데",
          "original_form": "학교때문에 기숙사에 한달쯤 있었던 기간이 있었는데",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000585.1.16",
          "form": "오히려 안 보고 있으니까",
          "original_form": "오히려 안 보고 있으니까",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000585.1.17",
          "form": "애틋해지고",
          "original_form": "애틋해지고",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000585.1.18",
          "form": "좋은 것 같아요",
          "original_form": "좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000585.1.19",
          "form": "좋은거네요 그래도 저는 룸메이트는 있어야할거같아요",
          "original_form": "좋은거네요 그래도 저는 룸메이트는 있어야할거같아요",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000585.1.20",
          "form": "혼자면 심심하니까",
          "original_form": "혼자면 심심하니까",
          "speaker_id": "1",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000585.1.21",
          "form": "확실히 그렇긴해요",
          "original_form": "확실히 그렇긴해요",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000585.1.22",
          "form": "월세도 비싸고...",
          "original_form": "월세도 비싸고...",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000585.1.23",
          "form": "그쵸 관리비부담도 있고",
          "original_form": "그쵸 관리비부담도 있고",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000585.1.24",
          "form": "그런데 룸메이트는 살아보기 전까지는 룸메로 좋은 사람인지 알 수 없는게 문제인것 같아요",
          "original_form": "그런데 룸메이트는 살아보기 전까지는 룸메로 좋은 사람인지 알 수 없는게 문제인것 같아요",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000585.1.25",
          "form": "작게는....아침형 인간이라던가",
          "original_form": "작게는....아침형 인간이라던가",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000585.1.26",
          "form": "청소 주기라던가",
          "original_form": "청소 주기라던가",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000585.1.27",
          "form": "맞아요 그래서 생각중인게 결혼전에 몇달간 동거는 필수인거같아요",
          "original_form": "맞아요 그래서 생각중인게 결혼전에 몇달간 동거는 필수인거같아요",
          "speaker_id": "1",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000585.1.28",
          "form": "와 맞아요.",
          "original_form": "와 맞아요.",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000585.1.29",
          "form": "그래야 트러블이 적을거같아요",
          "original_form": "그래야 트러블이 적을거같아요",
          "speaker_id": "1",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000585.1.30",
          "form": "막상 결혼해서 살아보니 막 너무 더럽거나..빤쓰만 입고 다니면...",
          "original_form": "막상 결혼해서 살아보니 막 너무 더럽거나..빤쓰만 입고 다니면...",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000585.1.31",
          "form": "맞아요맞아요.",
          "original_form": "맞아요맞아요.",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000585.1.32",
          "form": "저는 꼭 바지입어야해요",
          "original_form": "저는 꼭 바지입어야해요",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000585.1.33",
          "form": "속옷만 입고 돌아다니는건 저도 별로..",
          "original_form": "속옷만 입고 돌아다니는건 저도 별로..",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000585.1.34",
          "form": "정말로요..가족이 가끔 덥다고 그러는데",
          "original_form": "정말로요..가족이 가끔 덥다고 그러는데",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000585.1.35",
          "form": "정말.....",
          "original_form": "정말.....",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000585.1.36",
          "form": "머리만 아픕니다",
          "original_form": "머리만 아픕니다",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000585.1.37",
          "form": "보면서 나는 안 그래야지 하는 생각들 많아요",
          "original_form": "보면서 나는 안 그래야지 하는 생각들 많아요",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000585.1.38",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000585.1.39",
          "form": "그리고 결혼하기 전에 룸메로 살면 집에서 어떻게 컸을지도 보일 것 같아요.",
          "original_form": "그리고 결혼하기 전에 룸메로 살면 집에서 어떻게 컸을지도 보일 것 같아요.",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000585.1.40",
          "form": "집안일같은 경우도 엄마 혼자 부담하시는거 보면서 어느정도 도와드려야지 하는 생각도 들어요",
          "original_form": "집안일같은 경우도 엄마 혼자 부담하시는거 보면서 어느정도 도와드려야지 하는 생각도 들어요",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000585.1.41",
          "form": "좋은 생각이시네요!",
          "original_form": "좋은 생각이시네요!",
          "speaker_id": "2",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000585.1.42",
          "form": "지금 연애하는 중이세요?",
          "original_form": "지금 연애하는 중이세요?",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000585.1.43",
          "form": "오, 아니요.",
          "original_form": "오, 아니요.",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000585.1.44",
          "form": "제 앞가림도 바쁜^^;;",
          "original_form": "제 앞가림도 바쁜^^;;",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000585.1.45",
          "form": "아 바쁜 분이셨구나",
          "original_form": "아 바쁜 분이셨구나",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000585.1.46",
          "form": "결혼은 언제 생각중이신가요",
          "original_form": "결혼은 언제 생각중이신가요",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000585.1.47",
          "form": "어른이 되면 빨래두 내가 다하구~설거지두 다하구~ 할줄 알았는데",
          "original_form": "어른이 되면 빨래두 내가 다하구~설거지두 다하구~ 할줄 알았는데",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000585.1.48",
          "form": "결혼은 음...사람이 있어야...",
          "original_form": "결혼은 음...사람이 있어야...",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000585.1.49",
          "form": "그걸 다 떠나서 언제쯤 하고 싶냐면은 한 서른초중반?즈음하면",
          "original_form": "그걸 다 떠나서 언제쯤 하고 싶냐면은 한 서른초중반?즈음하면",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000585.1.50",
          "form": "좋을 것 같네요",
          "original_form": "좋을 것 같네요",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000585.1.51",
          "form": "저도 어른되면 다를줄알았는데 그냥 몸만 컸어요",
          "original_form": "저도 어른되면 다를줄알았는데 그냥 몸만 컸어요",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000585.1.52",
          "form": "정말로요. 언제나 어머니께 감사하며 삽니다.",
          "original_form": "정말로요. 언제나 어머니께 감사하며 삽니다.",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000585.1.53",
          "form": "지금은 몇살이세요??",
          "original_form": "지금은 몇살이세요??",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000585.1.54",
          "form": "스물다섯이요!",
          "original_form": "스물다섯이요!",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000585.1.55",
          "form": "아 그래요? 생각보다 나이차가 안 나네요",
          "original_form": "아 그래요? 생각보다 나이차가 안 나네요",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000585.1.56",
          "form": "전 98년생이에요",
          "original_form": "전 98년생이에요",
          "speaker_id": "1",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000585.1.57",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000585.1.58",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "2",
          "time": "20210819 17:13"
        },
        {
          "id": "MDRW2100000585.1.59",
          "form": "저는 좀더 성숙한 분이신줄 알았어요",
          "original_form": "저는 좀더 성숙한 분이신줄 알았어요",
          "speaker_id": "1",
          "time": "20210819 17:14"
        },
