      "utterance": [
        {
          "id": "MDRW2100007767.1.1",
          "form": "저는 치트키인가",
          "original_form": "저는 치트키인가",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007767.1.2",
          "form": "에디터도 다운받아서 쓰고 그랬던거같아요 ㅋㅋ",
          "original_form": "에디터도 다운받아서 쓰고 그랬던거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007767.1.3",
          "form": "앜ㅋㅋㅋ 저도 마인크래프트 할 때 그런거 꼼수 많이 썼었어요!",
          "original_form": "앜ㅋㅋㅋ 저도 마인크래프트 할 때 그런거 꼼수 많이 썼었어요!",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007767.1.4",
          "form": "하면 게임은 쉬워지는데 재미는 없어지는..ㅋㅋㅋ",
          "original_form": "하면 게임은 쉬워지는데 재미는 없어지는..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007767.1.5",
          "form": "아 공감!\r\n재미가없어져요ㅜ",
          "original_form": "아 공감!\r\n재미가없어져요ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007767.1.6",
          "form": "저는 그래서 에디터로 돈만 만땅으로 채워서 딸키웠어요",
          "original_form": "저는 그래서 에디터로 돈만 만땅으로 채워서 딸키웠어요",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007767.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007767.1.8",
          "form": "엌ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007767.1.9",
          "form": "부잣집 딸을..",
          "original_form": "부잣집 딸을..",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007767.1.10",
          "form": "지금 생각해보니 그거야말로 돈많은 백수아빠였네요",
          "original_form": "지금 생각해보니 그거야말로 돈많은 백수아빠였네요",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007767.1.11",
          "form": "진짜 너무 재미있었어요 ㅋㅋ",
          "original_form": "진짜 너무 재미있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007767.1.12",
          "form": "돈많으니까 ㅋㅋㅋ",
          "original_form": "돈많으니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007767.1.13",
          "form": "그 딸은 좋았겠네욬ㅋㅋㅋ",
          "original_form": "그 딸은 좋았겠네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007767.1.14",
          "form": "받고 싶은거 다 받고",
          "original_form": "받고 싶은거 다 받고",
          "speaker_id": "1",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007767.1.15",
          "form": "진짜 웃긴건",
          "original_form": "진짜 웃긴건",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007767.1.16",
          "form": "원래딸이 알바해서 돈벌고",
          "original_form": "원래딸이 알바해서 돈벌고",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007767.1.17",
          "form": "그돈으로 생활하는데",
          "original_form": "그돈으로 생활하는데",
          "speaker_id": "2",
          "time": "20210822 10:29"
        },
        {
          "id": "MDRW2100007767.1.18",
          "form": "계속 공부만시키니 병났더라는 ㅋㅋ",
          "original_form": "계속 공부만시키니 병났더라는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007767.1.19",
          "form": "아 알바도해요?",
          "original_form": "아 알바도해요?",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007767.1.20",
          "form": "앜ㅋㅋㅋㅋ 적당한 신체활동도 필요하구나",
          "original_form": "앜ㅋㅋㅋㅋ 적당한 신체활동도 필요하구나",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007767.1.21",
          "form": "너무 재밌을거같아욬ㅋㅋㅋ",
          "original_form": "너무 재밌을거같아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:30"
        },
        {
          "id": "MDRW2100007767.1.22",
          "form": "은근 잼있었어요",
          "original_form": "은근 잼있었어요",
          "speaker_id": "2",
          "time": "20210822 10:32"
        },
        {
          "id": "MDRW2100007767.1.23",
          "form": "웃긴건 그부잣집 딸이",
          "original_form": "웃긴건 그부잣집 딸이",
          "speaker_id": "2",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007767.1.24",
          "form": "원하는 옷 다사주고",
          "original_form": "원하는 옷 다사주고",
          "speaker_id": "2",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007767.1.25",
          "form": "여하간ㅜㅜ 그냥평범한 ㅜ 남자랑결혼함",
          "original_form": "여하간ㅜㅜ 그냥평범한 ㅜ 남자랑결혼함",
          "speaker_id": "2",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007767.1.26",
          "form": "가정주부됨ㅜ",
          "original_form": "가정주부됨ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007767.1.27",
          "form": "헐 부잣집딸이 가정주부가되다니..ㅠㅠ",
          "original_form": "헐 부잣집딸이 가정주부가되다니..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100007767.1.28",
          "form": "공부도 엄청시켰는데 너무 아까워요",
          "original_form": "공부도 엄청시켰는데 너무 아까워요",
          "speaker_id": "1",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100007767.1.29",
          "form": "게임 난이도가 어렵네욬ㅋㅋㅋ",
          "original_form": "게임 난이도가 어렵네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100007767.1.30",
          "form": "그담 부잣집 딸은 최대한 놀았어요",
          "original_form": "그담 부잣집 딸은 최대한 놀았어요",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007767.1.31",
          "form": "바캉스 기능? 메뉴?가 있어서 ㅋㅋ",
          "original_form": "바캉스 기능? 메뉴?가 있어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007767.1.32",
          "form": "그랬더니 아빠랑 결혼하겠다고 ㄷㄷ",
          "original_form": "그랬더니 아빠랑 결혼하겠다고 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 10:37"
        },
        {
          "id": "MDRW2100007767.1.33",
          "form": "ㄷㄷㄷㄷ 그런 엔딩도 있어요?",
          "original_form": "ㄷㄷㄷㄷ 그런 엔딩도 있어요?",
          "speaker_id": "1",
          "time": "20210822 10:37"
        },
        {
          "id": "MDRW2100007767.1.34",
          "form": "무서운데욬ㅋㅋㅋ",
          "original_form": "무서운데욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:38"
        },
        {
          "id": "MDRW2100007767.1.35",
          "form": "놀면 아빠랑 결혼하고\r\n공부하면 병나고..",
          "original_form": "놀면 아빠랑 결혼하고\r\n공부하면 병나고..",
          "speaker_id": "1",
          "time": "20210822 10:38"
        },
        {
          "id": "MDRW2100007767.1.36",
          "form": "그담엔 집사랑결혼했어요",
          "original_form": "그담엔 집사랑결혼했어요",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007767.1.37",
          "form": "아플때 집사가 간병해줘서 둘이 급친해짐 ㅋㅋ",
          "original_form": "아플때 집사가 간병해줘서 둘이 급친해짐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007767.1.38",
          "form": "아니 ㅋㅋㅋㅋ 프메에 집사랑 결혼하는 엔딩도 있어요?",
          "original_form": "아니 ㅋㅋㅋㅋ 프메에 집사랑 결혼하는 엔딩도 있어요?",
          "speaker_id": "1",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007767.1.39",
          "form": "왤케 딸이 ㅋㅋㅋㅋ 자유롭죠",
          "original_form": "왤케 딸이 ㅋㅋㅋㅋ 자유롭죠",
          "speaker_id": "1",
          "time": "20210822 10:43"
        }
      ]
    }
