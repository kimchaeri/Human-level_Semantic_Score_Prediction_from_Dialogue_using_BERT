      "utterance": [
        {
          "id": "MDRW2100021702.1.1",
          "form": "겜 조아하시나요",
          "original_form": "겜 조아하시나요",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021702.1.2",
          "form": "저 메이플이요! 예전에 하셨다고 하셨죠? 렙 몇까지 키우셨어요?",
          "original_form": "저 메이플이요! 예전에 하셨다고 하셨죠? 렙 몇까지 키우셨어요?",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021702.1.3",
          "form": "음 기억은안나는데 송편이벤트 기억나네요ㅎㅎ",
          "original_form": "음 기억은안나는데 송편이벤트 기억나네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021702.1.4",
          "form": "메이플은 이벤트 자주하죠",
          "original_form": "메이플은 이벤트 자주하죠",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021702.1.5",
          "form": "그래서 초보들이 하기에 좋아요ㅋㅋㅋ",
          "original_form": "그래서 초보들이 하기에 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021702.1.6",
          "form": "직업은 뭐였어요?",
          "original_form": "직업은 뭐였어요?",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100021702.1.7",
          "form": "직업 법사였던것같아요ㅋㅋㅋ",
          "original_form": "직업 법사였던것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021702.1.8",
          "form": "마법사도 여러가지 잖아요ㅋㅋㅋ",
          "original_form": "마법사도 여러가지 잖아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021702.1.9",
          "form": "가물가물해요ㅠㅠ 오래전이라",
          "original_form": "가물가물해요ㅠㅠ 오래전이라",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021702.1.10",
          "form": "그럼 인내의 숲 해보셨어요?",
          "original_form": "그럼 인내의 숲 해보셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021702.1.11",
          "form": "아아 해봤죠 기억나요ㅋㅋㅋㅋㅋ",
          "original_form": "아아 해봤죠 기억나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021702.1.12",
          "form": "드디어 대화거리를 찾았네요...",
          "original_form": "드디어 대화거리를 찾았네요...",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021702.1.13",
          "form": "성공하셨어요?",
          "original_form": "성공하셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021702.1.14",
          "form": "네 깼었던것같아요ㅋㅋ 중간에 떨어지면 엄청빡치고",
          "original_form": "네 깼었던것같아요ㅋㅋ 중간에 떨어지면 엄청빡치고",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021702.1.15",
          "form": "오 저는 실패자입니다",
          "original_form": "오 저는 실패자입니다",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021702.1.16",
          "form": "ㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021702.1.17",
          "form": "인내의 숲을 비롯한 모든 컨트롤 미니게임은 다 실패해요",
          "original_form": "인내의 숲을 비롯한 모든 컨트롤 미니게임은 다 실패해요",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021702.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021702.1.19",
          "form": "팁있나요?",
          "original_form": "팁있나요?",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021702.1.20",
          "form": "팁은 될때까지 도전ㅋㅋㅋ",
          "original_form": "팁은 될때까지 도전ㅋㅋㅋ",
