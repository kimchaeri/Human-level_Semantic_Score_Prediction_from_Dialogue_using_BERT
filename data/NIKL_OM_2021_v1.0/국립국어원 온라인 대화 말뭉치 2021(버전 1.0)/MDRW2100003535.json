      "utterance": [
        {
          "id": "MDRW2100003535.1.1",
          "form": "안주무시나요?ㅎ",
          "original_form": "안주무시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003535.1.2",
          "form": "자다깻다가 어느새 또 잠들었네요 ㅎ",
          "original_form": "자다깻다가 어느새 또 잠들었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 08:21"
        },
        {
          "id": "MDRW2100003535.1.3",
          "form": "name2님은 정원샵에서 뭐 안사셨어요?",
          "original_form": "&name2&님은 정원샵에서 뭐 안사셨어요?",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003535.1.4",
          "form": "네 정원샵에 머 있었어요?",
          "original_form": "네 정원샵에 머 있었어요?",
          "speaker_id": "2",
          "time": "20210821 12:11"
        },
        {
          "id": "MDRW2100003535.1.5",
          "form": "카톡 플친 40프로 쿠폰줬거든요",
          "original_form": "카톡 플친 40프로 쿠폰줬거든요",
          "speaker_id": "1",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003535.1.6",
          "form": "아 그래요? 본거같은데",
          "original_form": "아 그래요? 본거같은데",
          "speaker_id": "2",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003535.1.7",
          "form": "페이코쿠폰도 있어야할것같아서",
          "original_form": "페이코쿠폰도 있어야할것같아서",
          "speaker_id": "2",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003535.1.8",
          "form": "그쿠폰없으면 왠지 손해보는 느낌이라 안샀어요 ㅎ",
          "original_form": "그쿠폰없으면 왠지 손해보는 느낌이라 안샀어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003535.1.9",
          "form": "전 신한서 40쿠폰준걸로 이미 주문해서 오늘 도착한대요",
          "original_form": "전 신한서 40쿠폰준걸로 이미 주문해서 오늘 도착한대요",
          "speaker_id": "1",
          "time": "20210821 12:12"
        },
        {
          "id": "MDRW2100003535.1.10",
          "form": "페이코도 10프로 있는거같아요",
          "original_form": "페이코도 10프로 있는거같아요",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003535.1.11",
          "form": "신한카드에서 40퍼도 있었어요?",
          "original_form": "신한카드에서 40퍼도 있었어요?",
          "speaker_id": "2",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003535.1.12",
          "form": "네 탑스클럽가면 매달 주더라구요",
          "original_form": "네 탑스클럽가면 매달 주더라구요",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003535.1.13",
          "form": "신한카드로 결제해야되긴해요",
          "original_form": "신한카드로 결제해야되긴해요",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003535.1.14",
          "form": "탑스클럽...그건 등급이 얼마이상되야되요?",
          "original_form": "탑스클럽...그건 등급이 얼마이상되야되요?",
          "speaker_id": "2",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003535.1.15",
          "form": "그거상관없이 걍 다주는거같아요",
          "original_form": "그거상관없이 걍 다주는거같아요",
          "speaker_id": "1",
          "time": "20210821 12:14"
        },
        {
          "id": "MDRW2100003535.1.16",
          "form": "그래요? 저도 한번찾아봐야겠네요",
          "original_form": "그래요? 저도 한번찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210821 12:16"
        },
        {
          "id": "MDRW2100003535.1.17",
          "form": "저는 씨제이 기다리고 있거든요\r\n거기가 살것이 많아서...",
          "original_form": "저는 씨제이 기다리고 있거든요\r\n거기가 살것이 많아서...",
          "speaker_id": "2",
          "time": "20210821 12:16"
        },
        {
          "id": "MDRW2100003535.1.18",
          "form": "토스혜택에 정원샵없나요?",
          "original_form": "토스혜택에 정원샵없나요?",
          "speaker_id": "1",
          "time": "20210821 12:16"
        },
        {
          "id": "MDRW2100003535.1.19",
          "form": "네 그것도 없어요 ㅎ",
          "original_form": "네 그것도 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:17"
        },
        {
          "id": "MDRW2100003535.1.20",
          "form": "아 씨제이더마켓은 저번주인가 준거같은데 또할지 모르겠어요",
          "original_form": "아 씨제이더마켓은 저번주인가 준거같은데 또할지 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 12:17"
        },
        {
          "id": "MDRW2100003535.1.21",
          "form": "담달되야할듯 해요 ㅎ",
          "original_form": "담달되야할듯 해요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 12:17"
        }
      ]
