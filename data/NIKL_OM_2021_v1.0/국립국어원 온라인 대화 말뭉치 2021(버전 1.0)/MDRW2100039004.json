      "utterance": [
        {
          "id": "MDRW2100039004.1.1",
          "form": "다알자나",
          "original_form": "다알자나",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100039004.1.2",
          "form": "그래서 나한테 물어보더라고",
          "original_form": "그래서 나한테 물어보더라고",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.3",
          "form": "알죠 알죠",
          "original_form": "알죠 알죠",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.4",
          "form": "해라했지",
          "original_form": "해라했지",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.5",
          "form": "뭐라고요",
          "original_form": "뭐라고요",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.6",
          "form": "아무사이도 아니니까",
          "original_form": "아무사이도 아니니까",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.7",
          "form": "아 그 친구가 계속 그걸 전달해 줬구나",
          "original_form": "아 그 친구가 계속 그걸 전달해 줬구나",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.8",
          "form": "응나한테",
          "original_form": "응나한테",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.9",
          "form": "그 이모랑",
          "original_form": "그 이모랑",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.10",
          "form": "미리물어본거지",
          "original_form": "미리물어본거지",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.11",
          "form": "언 그 친구도 별론데요",
          "original_form": "언 그 친구도 별론데요",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.12",
          "form": "나랑 소개팅했던사이니까",
          "original_form": "나랑 소개팅했던사이니까",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.13",
          "form": "아냐 소개팅 나갔다면",
          "original_form": "아냐 소개팅 나갔다면",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.14",
          "form": "나한테 허락맡은거지",
          "original_form": "나한테 허락맡은거지",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.15",
          "form": "친구소개팅 나갔어요",
          "original_form": "친구소개팅 나갔어요",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.16",
          "form": "별사이도 아니었고",
          "original_form": "별사이도 아니었고",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100039004.1.17",
          "form": "이모님이 하도 부탁해서",
          "original_form": "이모님이 하도 부탁해서",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.18",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.19",
          "form": "나갔지",
          "original_form": "나갔지",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.20",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.21",
          "form": "나갔구나",
          "original_form": "나갔구나",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.22",
          "form": "나랑똑같은상황 연출됨",
          "original_form": "나랑똑같은상황 연출됨",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.23",
          "form": "어 별로다 별로 최악이다 쟤야",
          "original_form": "어 별로다 별로 최악이다 쟤야",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.24",
          "form": "근데 그분이",
          "original_form": "근데 그분이",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.25",
          "form": "간이 아좋은지",
          "original_form": "간이 아좋은지",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.26",
          "form": "눈밑이 검고 퀭해서",
          "original_form": "눈밑이 검고 퀭해서",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.27",
          "form": "병약해보였대",
          "original_form": "병약해보였대",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.28",
          "form": "콜 어디 아팠던 거 아니에요",
          "original_form": "콜 어디 아팠던 거 아니에요",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.29",
          "form": "뭔가 지병이있는듯",
          "original_form": "뭔가 지병이있는듯",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100039004.1.30",
          "form": "근데 치 병도 숨기고 나온 거 가",
          "original_form": "근데 치 병도 숨기고 나온 거 가",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.31",
          "form": "뒤에 어떤여자랑",
          "original_form": "뒤에 어떤여자랑",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.32",
          "form": "결혼한 사진",
          "original_form": "결혼한 사진",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.33",
          "form": "뭐 다행이네요",
          "original_form": "뭐 다행이네요",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.34",
          "form": "카톡프사됐었는데",
          "original_form": "카톡프사됐었는데",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.35",
          "form": "여자가 여우같아보였은",
          "original_form": "여자가 여우같아보였은",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.36",
          "form": "남자가곰같으니",
          "original_form": "남자가곰같으니",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.37",
          "form": "돈이 많았어요",
          "original_form": "돈이 많았어요",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.38",
          "form": "ㅇ잘살겠지",
          "original_form": "ㅇ잘살겠지",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.39",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.40",
          "form": "됐네요",
          "original_form": "됐네요",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.41",
          "form": "돈이있는집안이었지",
          "original_form": "돈이있는집안이었지",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.42",
          "form": "소개팅 얘기가 끝이 없네요",
          "original_form": "소개팅 얘기가 끝이 없네요",
          "speaker_id": "1",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100039004.1.43",
          "form": "심지어 재밌네요",
          "original_form": "심지어 재밌네요",
          "speaker_id": "1",
          "time": "20210903 01:05"
        }
      ]
    }
