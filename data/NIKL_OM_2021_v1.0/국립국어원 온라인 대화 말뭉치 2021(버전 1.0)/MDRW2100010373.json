      "utterance": [
        {
          "id": "MDRW2100010373.1.1",
          "form": "세상에 어디서인지 힌트를 줘",
          "original_form": "세상에 어디서인지 힌트를 줘",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.2",
          "form": "바로 강아지였어 ㅎㅎ 좀 시시하니?",
          "original_form": "바로 강아지였어 ㅎㅎ 좀 시시하니?",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.3",
          "form": "그냥 산책중이었어!",
          "original_form": "그냥 산책중이었어!",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.4",
          "form": "푸들을 보았단다.",
          "original_form": "푸들을 보았단다.",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.5",
          "form": "응..맞추라고 했으면서",
          "original_form": "응..맞추라고 했으면서",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.6",
          "form": "푸들 알지?",
          "original_form": "푸들 알지?",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.7",
          "form": "너 강아지 좋아하니?",
          "original_form": "너 강아지 좋아하니?",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.8",
          "form": "그냥 술술 불어버리네",
          "original_form": "그냥 술술 불어버리네",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.9",
          "form": "당연하지 강아지를 어떻게 안좋아해",
          "original_form": "당연하지 강아지를 어떻게 안좋아해",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.10",
          "form": "마음에 안들어?",
          "original_form": "마음에 안들어?",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.11",
          "form": "일반적인 토이푸들말고 그냥 푸들을 보았어",
          "original_form": "일반적인 토이푸들말고 그냥 푸들을 보았어",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.12",
          "form": "아주 멋있었단다.",
          "original_form": "아주 멋있었단다.",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010373.1.13",
          "form": "견주와 함께 산책을 하는 그 늠름한 모습에 반해버렸어.",
          "original_form": "견주와 함께 산책을 하는 그 늠름한 모습에 반해버렸어.",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.14",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.15",
          "form": "너도 큰 푸들을 본 적 있니?",
          "original_form": "너도 큰 푸들을 본 적 있니?",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.17",
          "form": "사실 푸들이 엄청",
          "original_form": "사실 푸들이 엄청",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.18",
          "form": "강아지랑 고양이 중에 어떤 동물을 더 좋아하니?",
          "original_form": "강아지랑 고양이 중에 어떤 동물을 더 좋아하니?",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.19",
          "form": "큰 동물이라는거만 들어봤어",
          "original_form": "큰 동물이라는거만 들어봤어",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.20",
          "form": "나는 근데 고양이가 더 좋아",
          "original_form": "나는 근데 고양이가 더 좋아",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.21",
          "form": "무슨 상상속 동물 말하듯 하는구나",
          "original_form": "무슨 상상속 동물 말하듯 하는구나",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.22",
          "form": "그렇구나!",
          "original_form": "그렇구나!",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010373.1.23",
          "form": "ㅠㅠ본적이없으니까",
          "original_form": "ㅠㅠ본적이없으니까",
          "speaker_id": "2",
          "time": "20210823 17:18"
        }
      ]
