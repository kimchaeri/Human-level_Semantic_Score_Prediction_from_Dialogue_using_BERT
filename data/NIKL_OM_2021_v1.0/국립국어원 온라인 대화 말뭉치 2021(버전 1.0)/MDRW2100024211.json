      "utterance": [
        {
          "id": "MDRW2100024211.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024211.1.2",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024211.1.3",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024211.1.4",
          "form": "집에서 부대찌개 밀키트 끓여먹었어요. 맛있네요.",
          "original_form": "집에서 부대찌개 밀키트 끓여먹었어요. 맛있네요.",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024211.1.5",
          "form": "우와 부대찌개",
          "original_form": "우와 부대찌개",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024211.1.6",
          "form": "저는 소고기뭇국 먹었어요",
          "original_form": "저는 소고기뭇국 먹었어요",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024211.1.7",
          "form": "오 직접 요리하셨나요?",
          "original_form": "오 직접 요리하셨나요?",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024211.1.8",
          "form": "네네어제",
          "original_form": "네네어제",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024211.1.9",
          "form": "고춧가루 너무 많이넣어서",
          "original_form": "고춧가루 너무 많이넣어서",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024211.1.10",
          "form": "엄마가 다시 해쥬셨어요..ㅋ",
          "original_form": "엄마가 다시 해쥬셨어요..ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024211.1.11",
          "form": "하핫 소고기 무국에 고춧가루넣는건 경상도 식인줄알았는데요",
          "original_form": "하핫 소고기 무국에 고춧가루넣는건 경상도 식인줄알았는데요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024211.1.12",
          "form": "아 제가 경상도라 ㅋㅋ",
          "original_form": "아 제가 경상도라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 06:37"
        },
        {
          "id": "MDRW2100024211.1.13",
          "form": "그렇게먹어요",
          "original_form": "그렇게먹어요",
          "speaker_id": "1",
          "time": "20210831 06:37"
        },
        {
          "id": "MDRW2100024211.1.14",
          "form": "맞췄네요ㅎㅎ 시레기도 넣구 육개장처럼 먹는거~\r\n저도 대구가 고향인데 생각하니 먹고싶네요.\r\n근데 못끓인다는거ㅠ",
          "original_form": "맞췄네요ㅎㅎ 시레기도 넣구 육개장처럼 먹는거~\r\n저도 대구가 고향인데 생각하니 먹고싶네요.\r\n근데 못끓인다는거ㅠ",
          "speaker_id": "2",
          "time": "20210831 09:50"
        },
        {
          "id": "MDRW2100024211.1.15",
          "form": "맞습니다 ㅠㅠ 쉬워요 도전해보세요",
          "original_form": "맞습니다 ㅠㅠ 쉬워요 도전해보세요",
          "speaker_id": "1",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024211.1.16",
          "form": "망치면 소고기가 아까워서ㅠ",
          "original_form": "망치면 소고기가 아까워서ㅠ",
          "speaker_id": "2",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100024211.1.17",
          "form": "그것도 그래요 ㅋㅋ",
          "original_form": "그것도 그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:17"
        }
      ]
