      "utterance": [
        {
          "id": "MDRW2100033883.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100033883.1.2",
          "form": "네 저는 라면먹었어요 name2님은요",
          "original_form": "네 저는 라면먹었어요 &name2&님은요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033883.1.3",
          "form": "전 아침을 늦게 먹어서 건너뛰려고요",
          "original_form": "전 아침을 늦게 먹어서 건너뛰려고요",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100033883.1.4",
          "form": "아하 아침뭐드셨어요",
          "original_form": "아하 아침뭐드셨어요",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033883.1.5",
          "form": "샌드위치 먹었어요~ 아침 항상 드시나요?",
          "original_form": "샌드위치 먹었어요~ 아침 항상 드시나요?",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033883.1.6",
          "form": "전 아침을 잘안먹어요 name2님은 아침꼬박 드시나요?",
          "original_form": "전 아침을 잘안먹어요 &name2&님은 아침꼬박 드시나요?",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100033883.1.7",
          "form": "저도 안먹어요. 시간이 없더라고요. 전 밥보다 잠이 중요해서",
          "original_form": "저도 안먹어요. 시간이 없더라고요. 전 밥보다 잠이 중요해서",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100033883.1.8",
          "form": "네 저도 밥보다 잠이에요ㅜ",
          "original_form": "네 저도 밥보다 잠이에요ㅜ",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033883.1.9",
          "form": "샌드위치는 어떤종류드셨나요",
          "original_form": "샌드위치는 어떤종류드셨나요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033883.1.10",
          "form": "파바꺼 먹었어요",
          "original_form": "파바꺼 먹었어요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033883.1.11",
          "form": "파바 좋죠 간단하게먹기 좋은것같아요",
          "original_form": "파바 좋죠 간단하게먹기 좋은것같아요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033883.1.12",
          "form": "서브웨이 샌드위치 좋아하시나요",
          "original_form": "서브웨이 샌드위치 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033883.1.13",
          "form": "그럼요~ 플랫브레드에 먹는거 좋아해요 ㅎ",
          "original_form": "그럼요~ 플랫브레드에 먹는거 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100033883.1.14",
          "form": "전 파마산오레가노인가 뭐시기요 ㅎㄹ",
          "original_form": "전 파마산오레가노인가 뭐시기요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100033883.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100033883.1.16",
          "form": "어떤메뉴 자주드세요",
          "original_form": "어떤메뉴 자주드세요",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100033883.1.17",
          "form": "2~3년 전쯤에 한창 사먹었는데 가물가물 하네요",
          "original_form": "2~3년 전쯤에 한창 사먹었는데 가물가물 하네요",
