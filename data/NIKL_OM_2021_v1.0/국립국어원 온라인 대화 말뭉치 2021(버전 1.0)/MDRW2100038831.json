        {
          "id": "MDRW2100038831.1.1",
          "form": "쏜살같아요 정말",
          "original_form": "쏜살같아요 정말",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100038831.1.2",
          "form": "맞아요 쇼핑 좋아하시나요?",
          "original_form": "맞아요 쇼핑 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038831.1.3",
          "form": "그런편인듯해요",
          "original_form": "그런편인듯해요",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038831.1.4",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038831.1.5",
          "form": "전 인터넷 쇼핑은 엄청 좋아합니다ㅋㅋ",
          "original_form": "전 인터넷 쇼핑은 엄청 좋아합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100038831.1.6",
          "form": "인터넷이 편하죠",
          "original_form": "인터넷이 편하죠",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038831.1.7",
          "form": "쇼핑은 삶의 원동력 이죠ㅋㅋ",
          "original_form": "쇼핑은 삶의 원동력 이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038831.1.8",
          "form": "비교하기도 좋고",
          "original_form": "비교하기도 좋고",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038831.1.9",
          "form": "가격이 일단 너무 차이나요ㅋㅋ",
          "original_form": "가격이 일단 너무 차이나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038831.1.10",
          "form": "맞아요 훨 저렴해요. 몸도 안 피곤하고",
          "original_form": "맞아요 훨 저렴해요. 몸도 안 피곤하고",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038831.1.11",
          "form": "맞아요 혹시나 맘에 안 들어도 반품도 편하구요ㅋ",
          "original_form": "맞아요 혹시나 맘에 안 들어도 반품도 편하구요ㅋ",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038831.1.12",
          "form": "어떤 쇼핑몰 자주 이용하세요?",
          "original_form": "어떤 쇼핑몰 자주 이용하세요?",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038831.1.13",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038831.1.14",
          "form": "저 이곳저곳",
          "original_form": "저 이곳저곳",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038831.1.15",
          "form": "옥션 지마켓 쿠팡 11번가",
          "original_form": "옥션 지마켓 쿠팡 11번가",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100038831.1.16",
          "form": "저도 검색해보고 싼 곳으로 간답니다ㅋㅋㅋ",
          "original_form": "저도 검색해보고 싼 곳으로 간답니다ㅋㅋㅋ",
