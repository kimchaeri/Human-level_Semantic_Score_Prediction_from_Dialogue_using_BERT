      "utterance": [
        {
          "id": "MDRW2100019865.1.1",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019865.1.2",
          "form": "ㅋㅋ 돈만 쓰고 왔어요",
          "original_form": "ㅋㅋ 돈만 쓰고 왔어요",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019865.1.3",
          "form": "저희동네엔 코스트코만 있어요",
          "original_form": "저희동네엔 코스트코만 있어요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019865.1.4",
          "form": "저도 원래 코스트코 다니다가",
          "original_form": "저도 원래 코스트코 다니다가",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.5",
          "form": "옮겼어요 ㅋㅋ",
          "original_form": "옮겼어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.6",
          "form": "트레이더스가 더 좋아요?",
          "original_form": "트레이더스가 더 좋아요?",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.7",
          "form": "음 그냥 연회비가 없어서요",
          "original_form": "음 그냥 연회비가 없어서요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.8",
          "form": "코스트코 연회비 비싸죠",
          "original_form": "코스트코 연회비 비싸죠",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.9",
          "form": "네 저흰 많이 안사서..",
          "original_form": "네 저흰 많이 안사서..",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.10",
          "form": "저희도 그래서 아까워요",
          "original_form": "저희도 그래서 아까워요",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.11",
          "form": "코스트코 바나나 맛있잖아요?",
          "original_form": "코스트코 바나나 맛있잖아요?",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019865.1.12",
          "form": "네 쫀득쫀득해요",
          "original_form": "네 쫀득쫀득해요",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019865.1.13",
          "form": "그거 많이 샀었는데 ㅎㅎ",
          "original_form": "그거 많이 샀었는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019865.1.14",
          "form": "저도 갈때마다 사와요",
          "original_form": "저도 갈때마다 사와요",
