      "utterance": [
        {
          "id": "MDRW2100022718.1.1",
          "form": "아 육룡이나르샤 생각나서요 ㅋㅋㅋㅋ",
          "original_form": "아 육룡이나르샤 생각나서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.2",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.3",
          "form": "이름 되게 독특하다",
          "original_form": "이름 되게 독특하다",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.4",
          "form": "넷플에는 없겟네요 ㅠ",
          "original_form": "넷플에는 없겟네요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.5",
          "form": "전 일단 이름 독특하면 보고 싶은 마음이 안들어요",
          "original_form": "전 일단 이름 독특하면 보고 싶은 마음이 안들어요",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.6",
          "form": "ㅋㅋㅋㅋㅋ유아인나와요 연기미쳣어요",
          "original_form": "ㅋㅋㅋㅋㅋ유아인나와요 연기미쳣어요",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.7",
          "form": "김명민두나오고요",
          "original_form": "김명민두나오고요",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.8",
          "form": "유아인?!!!!",
          "original_form": "유아인?!!!!",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.9",
          "form": "김영민?!!",
          "original_form": "김영민?!!",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.10",
          "form": "꼭 볼게요",
          "original_form": "꼭 볼게요",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.11",
          "form": "네네 태종역할이에욬ㅋㅋㅋㅋㅋ",
          "original_form": "네네 태종역할이에욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.12",
          "form": "유아인 나오는 영화는ㄷ ㅏ 챙겨봤어요",
          "original_form": "유아인 나오는 영화는ㄷ ㅏ 챙겨봤어요",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022718.1.13",
          "form": "캐릭터가 넘 찰떡이더라구요 ㅋㅋ",
          "original_form": "캐릭터가 넘 찰떡이더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.14",
          "form": "오 글쿤요 저는 다는 못봣지먼",
          "original_form": "오 글쿤요 저는 다는 못봣지먼",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.15",
          "form": "유아인좋아해요 ㅠㅠ",
          "original_form": "유아인좋아해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.16",
          "form": "그 시카고타자기도",
          "original_form": "그 시카고타자기도",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.17",
          "form": "재밋어요 드라마에요",
          "original_form": "재밋어요 드라마에요",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.18",
          "form": "오호 ㅋㅋㅋㅋ",
          "original_form": "오호 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.19",
          "form": "되게 많이 보셨네요",
          "original_form": "되게 많이 보셨네요",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.20",
          "form": "ㅋㅋㅋㅋㅋ네 저는 드라마 좋아해서요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ네 저는 드라마 좋아해서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.21",
          "form": "인생 최고작은 뭐에요?",
          "original_form": "인생 최고작은 뭐에요?",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.22",
          "form": "드라마 중에서요",
          "original_form": "드라마 중에서요",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022718.1.23",
          "form": "아 사실 매번 인생작이라고 하는편리랔ㅋㅋㅋㅋㅋ",
          "original_form": "아 사실 매번 인생작이라고 하는편리랔ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.24",
          "form": "미생이랑 육룡이나르샤?",
          "original_form": "미생이랑 육룡이나르샤?",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.25",
          "form": "우위를 가릴 수 없으시구나..",
          "original_form": "우위를 가릴 수 없으시구나..",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.26",
          "form": "그리고 또 여럿잇긴햇어욬ㅋㅋㅋㅋ",
          "original_form": "그리고 또 여럿잇긴햇어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.27",
          "form": "네...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.28",
          "form": "전 드라마 보게 된 계기가 있는데",
          "original_form": "전 드라마 보게 된 계기가 있는데",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.29",
          "form": "그 작품이 인생 최고작이에요",
          "original_form": "그 작품이 인생 최고작이에요",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.30",
          "form": "몬가요!",
          "original_form": "몬가요!",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.31",
          "form": "SKY캐슬 보셨어요?ㅎㅎ",
          "original_form": "SKY캐슬 보셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022718.1.32",
          "form": "아 당연하죠",
          "original_form": "아 당연하죠",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.33",
          "form": "아 그거 진짜 명작인듯해요 ㅋㅋㅋㅋ",
          "original_form": "아 그거 진짜 명작인듯해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.34",
          "form": "미쳐있엇죠",
          "original_form": "미쳐있엇죠",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.36",
          "form": "결말빼구요",
          "original_form": "결말빼구요",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.37",
          "form": "이때부터 드라마 매력 빠져서 보기 시작했어요",
          "original_form": "이때부터 드라마 매력 빠져서 보기 시작했어요",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.38",
          "form": "아 드라마를 본게 얼마안되시구너",
          "original_form": "아 드라마를 본게 얼마안되시구너",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.39",
          "form": "결말",
          "original_form": "결말",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022718.1.40",
          "form": "아 네네 별로 안됐어요",
          "original_form": "아 네네 별로 안됐어요",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022718.1.41",
          "form": "그 드라마",
          "original_form": "그 드라마",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022718.1.42",
          "form": "되게 특이한 제목 있었느네 ㅋㅋㅋㅋ",
          "original_form": "되게 특이한 제목 있었느네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
