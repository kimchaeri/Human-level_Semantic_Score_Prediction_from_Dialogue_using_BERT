      "utterance": [
        {
          "id": "MDRW2100006563.1.1",
          "form": "건강을.위해 챙기는거 있나요",
          "original_form": "건강을.위해 챙기는거 있나요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100006563.1.2",
          "form": "음 저는 아주 가끔 집에서 실내자전거타는 정도요",
          "original_form": "음 저는 아주 가끔 집에서 실내자전거타는 정도요",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100006563.1.3",
          "form": "영양제는 안드시나요",
          "original_form": "영양제는 안드시나요",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100006563.1.4",
          "form": "비타민 b먹고있어요 누가 줘서 그냥 먹고 있습니다..ㅋㅋㅋ",
          "original_form": "비타민 b먹고있어요 누가 줘서 그냥 먹고 있습니다..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100006563.1.5",
          "form": "소미님은요?",
          "original_form": "소미님은요?",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006563.1.6",
          "form": "저도 비타민.유산균 먹어요 ㅎ",
          "original_form": "저도 비타민.유산균 먹어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100006563.1.7",
          "form": "효과가 있으신가요!",
          "original_form": "효과가 있으신가요!",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100006563.1.8",
          "form": "비타민b는.안먹으면 피곤한것 같긴해요..",
          "original_form": "비타민b는.안먹으면 피곤한것 같긴해요..",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100006563.1.9",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100006563.1.10",
          "form": "안빼먹고 꾸준히 먹어야겠어요",
          "original_form": "안빼먹고 꾸준히 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100006563.1.11",
          "form": "네 저는 효과가 느껴졌어요 ㅎㅎ",
          "original_form": "네 저는 효과가 느껴졌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006563.1.12",
          "form": "운동은 따로 하시나요?",
          "original_form": "운동은 따로 하시나요?",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100006563.1.13",
          "form": "네 헬스합니다",
          "original_form": "네 헬스합니다",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100006563.1.14",
          "form": "오 얼마나 하셨어요?",
          "original_form": "오 얼마나 하셨어요?",
