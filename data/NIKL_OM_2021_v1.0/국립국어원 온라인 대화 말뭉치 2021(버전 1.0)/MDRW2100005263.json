      "utterance": [
        {
          "id": "MDRW2100005263.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005263.1.2",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005263.1.3",
          "form": "네 ㅎㅎ 초밥 치킨요",
          "original_form": "네 ㅎㅎ 초밥 치킨요",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005263.1.4",
          "form": "뭐 드셨나요 ?",
          "original_form": "뭐 드셨나요 ?",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005263.1.5",
          "form": "국밥이요",
          "original_form": "국밥이요",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005263.1.6",
          "form": "국밥 저도 잘 먹어요",
          "original_form": "국밥 저도 잘 먹어요",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005263.1.7",
          "form": "배달은 이상하게 빈약해서 가서 먹는게 맛나요",
          "original_form": "배달은 이상하게 빈약해서 가서 먹는게 맛나요",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005263.1.8",
          "form": "저도 가서 먹얶어요",
          "original_form": "저도 가서 먹얶어요",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005263.1.9",
          "form": "네 그래야 맛나죠",
          "original_form": "네 그래야 맛나죠",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005263.1.10",
          "form": "코로나라 조심스러워도 가야 맛나요",
          "original_form": "코로나라 조심스러워도 가야 맛나요",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.11",
          "form": "넵 따뜻하니 맛나요",
          "original_form": "넵 따뜻하니 맛나요",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.13",
          "form": "풋고추에 부추 넣고 ㅎㅎ",
          "original_form": "풋고추에 부추 넣고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.14",
          "form": "부추 새우젓 좋죠",
          "original_form": "부추 새우젓 좋죠",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.15",
          "form": "저 감자탕도 좋아해요",
          "original_form": "저 감자탕도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.16",
          "form": "뼈다귀해장국요",
          "original_form": "뼈다귀해장국요",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.17",
          "form": "악 저도",
          "original_form": "악 저도",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.18",
          "form": "발라먹는 재미가",
          "original_form": "발라먹는 재미가",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005263.1.19",
          "form": "맞아요 아 살이 꽉찬게 뜯어먹는 재미있죠",
          "original_form": "맞아요 아 살이 꽉찬게 뜯어먹는 재미있죠",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005263.1.20",
          "form": "저희집옆에 한점 더주는데 있어요 ㅋㅋ",
          "original_form": "저희집옆에 한점 더주는데 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005263.1.21",
          "form": "아주 맛나고 정이 넘쳐요",
          "original_form": "아주 맛나고 정이 넘쳐요",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005263.1.22",
          "form": "아 급 땡기네요",
          "original_form": "아 급 땡기네요",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005263.1.23",
          "form": "소주도 하셨어요?",
          "original_form": "소주도 하셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005263.1.24",
          "form": "술은 끊었어요",
          "original_form": "술은 끊었어요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        }
      ]
