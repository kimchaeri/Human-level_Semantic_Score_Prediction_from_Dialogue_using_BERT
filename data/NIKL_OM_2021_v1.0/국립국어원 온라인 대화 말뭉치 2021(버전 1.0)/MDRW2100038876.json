      "utterance": [
        {
          "id": "MDRW2100038876.1.1",
          "form": "내일은 뭐 먹을까요",
          "original_form": "내일은 뭐 먹을까요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100038876.1.2",
          "form": "후보 있나요",
          "original_form": "후보 있나요",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038876.1.3",
          "form": "후보는 따로 없어요",
          "original_form": "후보는 따로 없어요",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038876.1.4",
          "form": "그럼 어려운데요",
          "original_form": "그럼 어려운데요",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038876.1.5",
          "form": "칼국수? 더우면 돈가스냉면이요",
          "original_form": "칼국수? 더우면 돈가스냉면이요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038876.1.6",
          "form": "오 돈가스냉면도 있나요?",
          "original_form": "오 돈가스냉면도 있나요?",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038876.1.7",
          "form": "먹을거면 더 추워지기 전에 먹어야겠네요",
          "original_form": "먹을거면 더 추워지기 전에 먹어야겠네요",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038876.1.8",
          "form": "아뇨ㅋㅋㅋ둘을 세트로 먹으면 맛있더라고요",
          "original_form": "아뇨ㅋㅋㅋ둘을 세트로 먹으면 맛있더라고요",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038876.1.9",
          "form": "아하 세트로 먹는거였군요",
          "original_form": "아하 세트로 먹는거였군요",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038876.1.10",
          "form": "네 환상의궁합!",
          "original_form": "네 환상의궁합!",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100038876.1.11",
          "form": "주변에 파는데 있나 알아보겠습니다",
          "original_form": "주변에 파는데 있나 알아보겠습니다",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100038876.1.12",
          "form": "ㅎㅎ드시면 후기남겨주세요~",
          "original_form": "ㅎㅎ드시면 후기남겨주세요~",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038876.1.13",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038876.1.14",
          "form": "전 내일뭐먹을지고민이네요",
          "original_form": "전 내일뭐먹을지고민이네요",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038876.1.15",
          "form": "햇별님도 떡볶이 드실래요",
          "original_form": "햇별님도 떡볶이 드실래요",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038876.1.16",
          "form": "금요일이면 탈 생각 안하고 먹어도 되는데",
          "original_form": "금요일이면 탈 생각 안하고 먹어도 되는데",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038876.1.17",
          "form": "그럴까요 라볶이가 땡기네요",
          "original_form": "그럴까요 라볶이가 땡기네요",
          "speaker_id": "1",
