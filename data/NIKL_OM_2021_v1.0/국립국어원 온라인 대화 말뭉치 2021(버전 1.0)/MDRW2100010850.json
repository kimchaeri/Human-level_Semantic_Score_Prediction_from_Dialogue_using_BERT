      "utterance": [
        {
          "id": "MDRW2100010850.1.1",
          "form": "사과 지금 딱 나올땐데",
          "original_form": "사과 지금 딱 나올땐데",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010850.1.2",
          "form": "아오리사과좋아하세요?",
          "original_form": "아오리사과좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010850.1.3",
          "form": "넵 좋아해요",
          "original_form": "넵 좋아해요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010850.1.4",
          "form": "음 주변에 맛집잇으세요?",
          "original_form": "음 주변에 맛집잇으세요?",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010850.1.5",
          "form": "물닭갈비아세요?",
          "original_form": "물닭갈비아세요?",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010850.1.6",
          "form": "아뇨??? 그게 머예오???",
          "original_form": "아뇨??? 그게 머예오???",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010850.1.7",
          "form": "약간 닭도리탕 같은건가??",
          "original_form": "약간 닭도리탕 같은건가??",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010850.1.8",
          "form": "음 비슷해요ㅎ",
          "original_form": "음 비슷해요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010850.1.9",
          "form": "국물많은?",
          "original_form": "국물많은?",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010850.1.10",
          "form": "오옹 맛있어요??",
          "original_form": "오옹 맛있어요??",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010850.1.11",
          "form": "어렷을때부터 단골요ㅎ",
          "original_form": "어렷을때부터 단골요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010850.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 어렷을때부터 한 곳에 계속 사셧나바요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 어렷을때부터 한 곳에 계속 사셧나바요",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010850.1.13",
          "form": "넵ㅎ 거의 비슷한동네라",
          "original_form": "넵ㅎ 거의 비슷한동네라",
          "speaker_id": "2",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010850.1.14",
          "form": "배달은 자주 시켜드새요?",
          "original_form": "배달은 자주 시켜드새요?",
          "speaker_id": "1",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010850.1.15",
          "form": "요새는 자주요",
          "original_form": "요새는 자주요",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010850.1.16",
          "form": "뭐 드세요?",
          "original_form": "뭐 드세요?",
          "speaker_id": "1",
          "time": "20210824 18:45"
        },
