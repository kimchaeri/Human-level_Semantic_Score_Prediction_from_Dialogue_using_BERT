      "utterance": [
        {
          "id": "MDRW2100003989.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003989.1.2",
          "form": "저는무교예요",
          "original_form": "저는무교예요",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003989.1.3",
          "form": "저도 무교에요",
          "original_form": "저도 무교에요",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003989.1.4",
          "form": "근데 교회랑 절은 많이가봤네요",
          "original_form": "근데 교회랑 절은 많이가봤네요",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003989.1.5",
          "form": "와 저도 친구따라 교회가봤어요",
          "original_form": "와 저도 친구따라 교회가봤어요",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003989.1.6",
          "form": "가면애슐리 2인권준다고해서갔어요",
          "original_form": "가면애슐리 2인권준다고해서갔어요",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003989.1.7",
          "form": "오 애슐리진짜 주던가여?",
          "original_form": "오 애슐리진짜 주던가여?",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003989.1.8",
          "form": "제가다닐때에는 그냥 과자하나였는데ㅋㅋㅋㅋㅋ",
          "original_form": "제가다닐때에는 그냥 과자하나였는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003989.1.9",
          "form": "네네 주더라고욬ㅋㅋㅋ",
          "original_form": "네네 주더라고욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003989.1.10",
          "form": "근데 2주동안 교회나왔어야했어서",
          "original_form": "근데 2주동안 교회나왔어야했어서",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003989.1.11",
          "form": "조금 품을 들이긴했죠",
          "original_form": "조금 품을 들이긴했죠",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003989.1.12",
          "form": "오 그러면 좀 갈만하죠ㅋㅋㅋㅋㅋㅋ투자가치가있었네요",
          "original_form": "오 그러면 좀 갈만하죠ㅋㅋㅋㅋㅋㅋ투자가치가있었네요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003989.1.13",
          "form": "저도 친구따라다녔는데 보면 완전 모태신앙처럼 엄청열심히다녔어요 ㅋㅋㅋㅋ",
          "original_form": "저도 친구따라다녔는데 보면 완전 모태신앙처럼 엄청열심히다녔어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100003989.1.14",
          "form": "초등부 성가대도하고 ㅋㅋㅋㅋ",
          "original_form": "초등부 성가대도하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100003989.1.15",
          "form": "아니 성가대까지하셨으면 진짜열심히다니셨는데요?ㅋㅋㅋ",
          "original_form": "아니 성가대까지하셨으면 진짜열심히다니셨는데요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100003989.1.16",
          "form": "근데 교회는 막 이것저것 많이하니까 나름 다니면서 재밌을거같기도해요",
          "original_form": "근데 교회는 막 이것저것 많이하니까 나름 다니면서 재밌을거같기도해요",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100003989.1.17",
          "form": "초등학생들은 친구도사귀고",
          "original_form": "초등학생들은 친구도사귀고",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100003989.1.18",
          "form": "네 친구따라다니던게 정신차려보니 제가 요직에앉아있더라구여 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 친구따라다니던게 정신차려보니 제가 요직에앉아있더라구여 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100003989.1.19",
          "form": "중딩때 빠져나왔습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "중딩때 빠져나왔습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100003989.1.20",
          "form": "아ㅋㅋㅋㅋㅋㅋ대박",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ대박",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100003989.1.21",
          "form": "그래도 적절한시기에 빠져나오셨네요ㅋㅋ",
          "original_form": "그래도 적절한시기에 빠져나오셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100003989.1.22",
          "form": "절은 어떻게 자주가게된거예요?",
          "original_form": "절은 어떻게 자주가게된거예요?",
          "speaker_id": "1",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100003989.1.23",
          "form": "네 용돈받기시작해서 과자얻어먹으러갈필요가없더라구여 ㅋㅋㅋㅋ",
          "original_form": "네 용돈받기시작해서 과자얻어먹으러갈필요가없더라구여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100003989.1.24",
          "form": "절은 원래 부처님오시는날에 절밥먹으러가는거 국룰아니겠습니까 ㅋㅋㅋ",
          "original_form": "절은 원래 부처님오시는날에 절밥먹으러가는거 국룰아니겠습니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100003989.1.25",
          "form": "약간 절은 관광지가는느낌으러 놀러많이갔던것같아요",
          "original_form": "약간 절은 관광지가는느낌으러 놀러많이갔던것같아요",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100003989.1.26",
          "form": "작년인가 제작년에 템플스테이도갔다왔는데 좋더라구여",
          "original_form": "작년인가 제작년에 템플스테이도갔다왔는데 좋더라구여",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100003989.1.27",
          "form": "와 저도 몇년전에 템플스테이갔어요",
          "original_form": "와 저도 몇년전에 템플스테이갔어요",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100003989.1.28",
          "form": "근데 그때 밤에 벌레가 너무많이나와서 잠을못잤어요",
          "original_form": "근데 그때 밤에 벌레가 너무많이나와서 잠을못잤어요",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100003989.1.29",
          "form": "담날 아침에 탈출했어요..",
          "original_form": "담날 아침에 탈출했어요..",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100003989.1.30",
          "form": "헉 벌레라니ㅜㅜㅜ",
          "original_form": "헉 벌레라니ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100003989.1.31",
          "form": "제가갔던절은 리모델링을 싹해서",
          "original_form": "제가갔던절은 리모델링을 싹해서",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100003989.1.32",
          "form": "게스트하우스인줄알았는데",
          "original_form": "게스트하우스인줄알았는데",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100003989.1.33",
          "form": "와 어딘진몰라도 가보고싶네요..",
          "original_form": "와 어딘진몰라도 가보고싶네요..",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100003989.1.34",
          "form": "제가갔던곳은 뭔가템플스테이를 그닥 본격적으로 하는곳은 아닌거같더라고요",
          "original_form": "제가갔던곳은 뭔가템플스테이를 그닥 본격적으로 하는곳은 아닌거같더라고요",
          "speaker_id": "1",
          "time": "20210821 14:57"
        }
      ]
