      "utterance": [
        {
          "id": "MDRW2100009968.1.1",
          "form": "무슨 빵 좋아하세요",
          "original_form": "무슨 빵 좋아하세요",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009968.1.2",
          "form": "저 피자빵이요 ㅎ",
          "original_form": "저 피자빵이요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009968.1.3",
          "form": "무슨 빵 좋아하세요",
          "original_form": "무슨 빵 좋아하세요",
          "speaker_id": "1",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009968.1.4",
          "form": "저는 파리바게트는 모카빵이랑 소시지빵 좋아해요",
          "original_form": "저는 파리바게트는 모카빵이랑 소시지빵 좋아해요",
          "speaker_id": "2",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009968.1.5",
          "form": "오늘 빵 주문하시나요",
          "original_form": "오늘 빵 주문하시나요",
          "speaker_id": "2",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009968.1.6",
          "form": "저 고민이에요...",
          "original_form": "저 고민이에요...",
          "speaker_id": "1",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009968.1.7",
          "form": "혹시 소시지빵이 돌돌말이 뭐 그건가요?",
          "original_form": "혹시 소시지빵이 돌돌말이 뭐 그건가요?",
          "speaker_id": "1",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009968.1.8",
          "form": "아니요 뽀드득 그릴소시지요",
          "original_form": "아니요 뽀드득 그릴소시지요",
          "speaker_id": "2",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009968.1.9",
          "form": "가운데 무채가 있는데 그게 핵심이에요",
          "original_form": "가운데 무채가 있는데 그게 핵심이에요",
          "speaker_id": "2",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009968.1.10",
          "form": "궁굼하네뇨",
          "original_form": "궁굼하네뇨",
          "speaker_id": "1",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009968.1.11",
          "form": "파바는 빵도 빵인데",
          "original_form": "파바는 빵도 빵인데",
          "speaker_id": "2",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009968.1.12",
          "form": "아이스크림도 진짜 맛있어요",
          "original_form": "아이스크림도 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009968.1.13",
          "form": "아 정말요? 어떤 아이스크림이요?",
          "original_form": "아 정말요? 어떤 아이스크림이요?",
          "speaker_id": "1",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009968.1.14",
          "form": "캔디바있는데",
          "original_form": "캔디바있는데",
          "speaker_id": "2",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009968.1.15",
          "form": "전부맛있어요",
          "original_form": "전부맛있어요",
          "speaker_id": "2",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009968.1.16",
          "form": "약간 뻔한맛일거라 생각해서 기대가 없었는데",
          "original_form": "약간 뻔한맛일거라 생각해서 기대가 없었는데",
          "speaker_id": "2",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009968.1.17",
          "form": "기대이상으로 맛있더라구요",
          "original_form": "기대이상으로 맛있더라구요",
          "speaker_id": "2",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009968.1.18",
          "form": "아 ㅋㅋ 거기에도 캔디바가 있군요",
          "original_form": "아 ㅋㅋ 거기에도 캔디바가 있군요",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009968.1.19",
          "form": "저 마트에서 파는 캔디바 좋아해요",
          "original_form": "저 마트에서 파는 캔디바 좋아해요",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009968.1.20",
          "form": "그거 소다맛이라 맛있어요",
          "original_form": "그거 소다맛이라 맛있어요",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009968.1.21",
          "form": "아래는 밀크쉐이크맛ㅎ",
          "original_form": "아래는 밀크쉐이크맛ㅎ",
          "speaker_id": "2",
          "time": "20210823 12:51"
        }
