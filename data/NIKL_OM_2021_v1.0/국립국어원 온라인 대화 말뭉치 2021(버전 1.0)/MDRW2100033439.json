      "utterance": [
        {
          "id": "MDRW2100033439.1.1",
          "form": "어떤 계절을 가장 좋아하세요?",
          "original_form": "어떤 계절을 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100033439.1.2",
          "form": "저는 봄, 가을이 좋아요!\r\nname2님은요?",
          "original_form": "저는 봄, 가을이 좋아요!\r\n&name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100033439.1.3",
          "form": "저도 봄 가을이요 ㅎ ㅎ",
          "original_form": "저도 봄 가을이요 ㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100033439.1.4",
          "form": "둘중에 어느 계절이 좋으세요?",
          "original_form": "둘중에 어느 계절이 좋으세요?",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100033439.1.5",
          "form": "가을이요! 선선한 바람부는 ^^",
          "original_form": "가을이요! 선선한 바람부는 ^^",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100033439.1.6",
          "form": "오 저두요 ~",
          "original_form": "오 저두요 ~",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100033439.1.7",
          "form": "봄은 그래도 꽃가루 때문에 ㅋㅋㅋㅋㅋ!",
          "original_form": "봄은 그래도 꽃가루 때문에 ㅋㅋㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100033439.1.8",
          "form": "봄이 따숩기는 하지만 황사도 있고 확실히 가을이 좋아요",
          "original_form": "봄이 따숩기는 하지만 황사도 있고 확실히 가을이 좋아요",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100033439.1.9",
          "form": "맞아요 ㅎ",
          "original_form": "맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100033439.1.10",
          "form": "얼른 가을이 오면 좋겠네요",
          "original_form": "얼른 가을이 오면 좋겠네요",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100033439.1.11",
          "form": "오늘 처서라더라구요 \r\n태풍올라고 있어서 그런지 조금은 가을같기도 했어요",
          "original_form": "오늘 처서라더라구요 \r\n태풍올라고 있어서 그런지 조금은 가을같기도 했어요",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100033439.1.12",
          "form": "맞아요 비만 안오면",
          "original_form": "맞아요 비만 안오면",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100033439.1.13",
          "form": "창문 열고 있을 거 같은데",
          "original_form": "창문 열고 있을 거 같은데",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100033439.1.14",
          "form": "좀 아쉬웠어요",
          "original_form": "좀 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100033439.1.15",
          "form": "태풍도 원래 대구로 오는 거였는데 방향이 조금 바뀌었더라구요",
          "original_form": "태풍도 원래 대구로 오는 거였는데 방향이 조금 바뀌었더라구요",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100033439.1.16",
          "form": "방향이 바껴서 다행이네요 ㅠㅠ",
          "original_form": "방향이 바껴서 다행이네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100033439.1.17",
          "form": "아무 탈 없이 지나가면 좋겠어요",
          "original_form": "아무 탈 없이 지나가면 좋겠어요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100033439.1.18",
          "form": "네 !  아무런 피해없이 자연소멸 되서 지나갔으면 좋겠네요!",
          "original_form": "네 !  아무런 피해없이 자연소멸 되서 지나갔으면 좋겠네요!",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100033439.1.19",
          "form": "이게 마지막 태풍인가요?",
          "original_form": "이게 마지막 태풍인가요?",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100033439.1.20",
          "form": "아마 그렇지 않을까요?",
          "original_form": "아마 그렇지 않을까요?",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033439.1.21",
          "form": "자세히는 모르지망요 하ㅏ하",
          "original_form": "자세히는 모르지망요 하ㅏ하",
          "speaker_id": "2",
          "time": "20210823 22:36"
        }
      ]
