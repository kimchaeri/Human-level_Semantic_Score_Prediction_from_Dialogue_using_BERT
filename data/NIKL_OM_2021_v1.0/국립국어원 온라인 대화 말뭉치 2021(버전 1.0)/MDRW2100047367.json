        {
          "id": "MDRW2100047367.1.2",
          "form": "아내의 유록",
          "original_form": "아내의 유록",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100047367.1.3",
          "form": "유혹",
          "original_form": "유혹",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100047367.1.4",
          "form": "아 맞다 저도 ㅋㅋㅋ",
          "original_form": "아 맞다 저도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100047367.1.5",
          "form": "언니가 살아있다",
          "original_form": "언니가 살아있다",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100047367.1.6",
          "form": "점찍소 살아나고 ㅋㅋ",
          "original_form": "점찍소 살아나고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100047367.1.7",
          "form": "그것도 재밌나요",
          "original_form": "그것도 재밌나요",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100047367.1.8",
          "form": "점찍는건 아내의 유혹 ㅋㅋ",
          "original_form": "점찍는건 아내의 유혹 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047367.1.9",
          "form": "옛날꺼라 감성이 좀 틀리죠",
          "original_form": "옛날꺼라 감성이 좀 틀리죠",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047367.1.10",
          "form": "언니가는",
          "original_form": "언니가는",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047367.1.11",
          "form": "틱톡애 짤 돌아다녀요 ㅋㅋ",
          "original_form": "틱톡애 짤 돌아다녀요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047367.1.12",
          "form": "웃겨서 ㅋㅋ",
          "original_form": "웃겨서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047367.1.13",
          "form": "아 정말요? ㅋㅋㅋㅋㅋ",
          "original_form": "아 정말요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047367.1.14",
          "form": "황당한 장면이 많은가봐요",
          "original_form": "황당한 장면이 많은가봐요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047367.1.15",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047367.1.16",
          "form": "김수미가 악역 막혼내는데 웃겨요",
          "original_form": "김수미가 악역 막혼내는데 웃겨요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047367.1.17",
          "form": "짤만 보셔요 ㅋㅋ것도 잼나요",
          "original_form": "짤만 보셔요 ㅋㅋ것도 잼나요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047367.1.18",
          "form": "아 ㅋㅋㅋㅋㅋ 김수미도 나오는군요",
          "original_form": "아 ㅋㅋㅋㅋㅋ 김수미도 나오는군요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047367.1.19",
          "form": "찾아봐야겠어여 ㅋㅋㅋ",
          "original_form": "찾아봐야겠어여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047367.1.20",
          "form": "장보리도 보셨나요?",
          "original_form": "장보리도 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047367.1.21",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047367.1.22",
          "form": "아 석터져 죽을뻔했어요 ㅜㅜ장보리",
          "original_form": "아 석터져 죽을뻔했어요 ㅜㅜ장보리",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047367.1.23",
          "form": "그건 근데 이유리가 더 인기 많았었죠?",
          "original_form": "그건 근데 이유리가 더 인기 많았었죠?",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047367.1.24",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047367.1.25",
          "form": "진짜 인기였죠",
          "original_form": "진짜 인기였죠",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100047367.1.26",
          "form": "그쵸.. 전 안 봤는데 짤은 다 본거 같아요 ㅋㅋㅋ",
          "original_form": "그쵸.. 전 안 봤는데 짤은 다 본거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100047367.1.27",
          "form": "네 저도 중간중간 속터져서 보다말다 했어요",
          "original_form": "네 저도 중간중간 속터져서 보다말다 했어요",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100047367.1.28",
          "form": "저는 임성한 드라마도 가끔 보는데 골때려서 웃겨요 ㅋㅋㅋ",
          "original_form": "저는 임성한 드라마도 가끔 보는데 골때려서 웃겨요 ㅋㅋㅋ",
