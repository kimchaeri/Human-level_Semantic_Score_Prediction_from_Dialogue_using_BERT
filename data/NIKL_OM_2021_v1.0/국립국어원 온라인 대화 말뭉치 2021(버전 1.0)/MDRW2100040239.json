      "utterance": [
        {
          "id": "MDRW2100040239.1.1",
          "form": "저는 카페인에 약해서 커피 잘 안마시는데",
          "original_form": "저는 카페인에 약해서 커피 잘 안마시는데",
          "speaker_id": "2",
          "time": "20210904 14:41"
        },
        {
          "id": "MDRW2100040239.1.2",
          "form": "마시면 가끔 라떼로 마셔요",
          "original_form": "마시면 가끔 라떼로 마셔요",
          "speaker_id": "2",
          "time": "20210904 14:41"
        },
        {
          "id": "MDRW2100040239.1.3",
          "form": "산미조아하세요?ㅠ",
          "original_form": "산미조아하세요?ㅠ",
          "speaker_id": "1",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100040239.1.4",
          "form": "저는 산미보다는 고소한 쪽",
          "original_form": "저는 산미보다는 고소한 쪽",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100040239.1.5",
          "form": "사실 달달한 쪽이요ㅎㅎ",
          "original_form": "사실 달달한 쪽이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100040239.1.6",
          "form": "커피 잘 못마셔서",
          "original_form": "커피 잘 못마셔서",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100040239.1.7",
          "form": "아항 카페가면 그럼 머드세요",
          "original_form": "아항 카페가면 그럼 머드세요",
          "speaker_id": "1",
          "time": "20210904 14:45"
        },
        {
          "id": "MDRW2100040239.1.8",
          "form": "저는 주로 차종류나 스무디",
          "original_form": "저는 주로 차종류나 스무디",
          "speaker_id": "2",
          "time": "20210904 14:46"
        },
        {
          "id": "MDRW2100040239.1.9",
          "form": "아하 스무디 머조아해용",
          "original_form": "아하 스무디 머조아해용",
          "speaker_id": "1",
          "time": "20210904 14:47"
        },
        {
          "id": "MDRW2100040239.1.10",
          "form": "요거트 스무디나 딸기 스무디요",
          "original_form": "요거트 스무디나 딸기 스무디요",
          "speaker_id": "2",
          "time": "20210904 14:48"
        },
        {
          "id": "MDRW2100040239.1.11",
          "form": "저도조아해요",
          "original_form": "저도조아해요",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040239.1.12",
          "form": "차는 무슨차드세요",
          "original_form": "차는 무슨차드세요",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040239.1.13",
          "form": "허브티나 과일티 마셔요",
          "original_form": "허브티나 과일티 마셔요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040239.1.14",
          "form": "허브티~ 캐모마일 조아하세요?",
          "original_form": "허브티~ 캐모마일 조아하세요?",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040239.1.15",
          "form": "캐모마일 페퍼민트 다 좋아해요",
          "original_form": "캐모마일 페퍼민트 다 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040239.1.16",
          "form": "근데 카페서 차마시면 넘 비싸요 ㅋㅋ",
          "original_form": "근데 카페서 차마시면 넘 비싸요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040239.1.17",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040239.1.18",
          "form": "근데 카페인에 약해서 커피를 잘 못마시거든요",
          "original_form": "근데 카페인에 약해서 커피를 잘 못마시거든요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040239.1.19",
          "form": "거의 티백인데 비싸서 ㅜㅋㅋㅋ",
          "original_form": "거의 티백인데 비싸서 ㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040239.1.20",
          "form": "저두 약해짐...",
          "original_form": "저두 약해짐...",
