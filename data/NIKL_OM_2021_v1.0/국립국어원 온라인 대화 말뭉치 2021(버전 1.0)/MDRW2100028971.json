      "utterance": [
        {
          "id": "MDRW2100028971.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028971.1.2",
          "form": "책을 좋아하시나봐요?",
          "original_form": "책을 좋아하시나봐요?",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028971.1.3",
          "form": "네~ name2님은요?",
          "original_form": "네~ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028971.1.4",
          "form": "저는 추리소설 해리포터  이런거만 봤어요 ㅜㅜ",
          "original_form": "저는 추리소설 해리포터  이런거만 봤어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028971.1.5",
          "form": "오 추리소설 좋죠!",
          "original_form": "오 추리소설 좋죠!",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028971.1.6",
          "form": "저도 해리포터 영화보다 책으로 먼저 접했어요 ㅎㅎ",
          "original_form": "저도 해리포터 영화보다 책으로 먼저 접했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100028971.1.7",
          "form": "책이 상상하며 봐서 잼더라구요 ㅎㅎ",
          "original_form": "책이 상상하며 봐서 잼더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028971.1.8",
          "form": "영화는 짤린거도있으니 ㅜ",
          "original_form": "영화는 짤린거도있으니 ㅜ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028971.1.9",
          "form": "그쵸! 재미있게 읽은 추리소설 있으신가요?",
          "original_form": "그쵸! 재미있게 읽은 추리소설 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028971.1.10",
          "form": "검은집이랑 푸른불꽃 잼게봤어요",
          "original_form": "검은집이랑 푸른불꽃 잼게봤어요",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028971.1.11",
          "form": "둘다 기시유스케 작가 꺼래요",
          "original_form": "둘다 기시유스케 작가 꺼래요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028971.1.12",
          "form": "오 제목이 뭔가 멋있네요!",
          "original_form": "오 제목이 뭔가 멋있네요!",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028971.1.13",
          "form": "추리소설 좋아하시는 분들은 대체적으로 일본 쪽 많이 보시는 것 같아용",
          "original_form": "추리소설 좋아하시는 분들은 대체적으로 일본 쪽 많이 보시는 것 같아용",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028971.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028971.1.15",
          "form": "음침해서 그런쪽으로 잘쓰는걸까요 ㅋㅋ",
          "original_form": "음침해서 그런쪽으로 잘쓰는걸까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028971.1.16",
          "form": "ㅋㅋㅋ 워낙에 괴담이나 그런게 많다보니",
          "original_form": "ㅋㅋㅋ 워낙에 괴담이나 그런게 많다보니",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028971.1.17",
          "form": "아이디어가 쑥쑥 솟아나나봐요",
          "original_form": "아이디어가 쑥쑥 솟아나나봐요",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028971.1.18",
          "form": "확실히 한국 추리소설이랑 다른 으스스함이 있죠",
          "original_form": "확실히 한국 추리소설이랑 다른 으스스함이 있죠",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028971.1.19",
          "form": "님은 어떤거 좋아하세요",
          "original_form": "님은 어떤거 좋아하세요",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028971.1.20",
          "form": "저는 가리지 않고 다 읽는 편이에요 ㅎㅎ",
          "original_form": "저는 가리지 않고 다 읽는 편이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028971.1.21",
          "form": "한참 히가시노 게이고에 빠졌던 적이 있어요!",
          "original_form": "한참 히가시노 게이고에 빠졌던 적이 있어요!",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028971.1.22",
          "form": "오 그분 숨쉬듯",
          "original_form": "오 그분 숨쉬듯",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028971.1.23",
          "form": "일본 문학은 학교 배경이 많다 보니 풋풋함이 그리울 때 많이 읽어보는 것 같아요",
          "original_form": "일본 문학은 학교 배경이 많다 보니 풋풋함이 그리울 때 많이 읽어보는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028971.1.24",
          "form": "책을 내심",
          "original_form": "책을 내심",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028971.1.25",
          "form": "앜ㅋㅋㅋ 맞죠",
          "original_form": "앜ㅋㅋㅋ 맞죠",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028971.1.26",
          "form": "매번 신간이 있어서 신기해요..",
          "original_form": "매번 신간이 있어서 신기해요..",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028971.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 대단한 아이디어에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 대단한 아이디어에요",
          "speaker_id": "2",
          "time": "20210902 23:26"
        }
      ]
