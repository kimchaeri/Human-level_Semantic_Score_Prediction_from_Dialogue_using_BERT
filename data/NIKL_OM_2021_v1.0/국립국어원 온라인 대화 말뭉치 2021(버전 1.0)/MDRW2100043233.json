      "utterance": [
        {
          "id": "MDRW2100043233.1.1",
          "form": "근데 지금가면 잘 놀 수 있을 것 같아요",
          "original_form": "근데 지금가면 잘 놀 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043233.1.2",
          "form": "프랑스어를 써볼 수 있다는 것과",
          "original_form": "프랑스어를 써볼 수 있다는 것과",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043233.1.3",
          "form": "미드나잇인파리",
          "original_form": "미드나잇인파리",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043233.1.4",
          "form": "같은 영화에 나오는 장면에 내가 들어간 기분이랄까요",
          "original_form": "같은 영화에 나오는 장면에 내가 들어간 기분이랄까요",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043233.1.5",
          "form": "로맨틱하네요",
          "original_form": "로맨틱하네요",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043233.1.6",
          "form": "홈스테이 경험은 괜찮으셨어요?",
          "original_form": "홈스테이 경험은 괜찮으셨어요?",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043233.1.7",
          "form": "지금도 그 느낌때문에 다시 가보고싶어요 ㅎㅎ",
          "original_form": "지금도 그 느낌때문에 다시 가보고싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043233.1.8",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043233.1.9",
          "form": "영화에 나온 곳 여행가보고싶은 곳 있으신가요?",
          "original_form": "영화에 나온 곳 여행가보고싶은 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043233.1.10",
          "form": "음 저는 영화는 아닌데 미드를 좋아해서",
          "original_form": "음 저는 영화는 아닌데 미드를 좋아해서",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043233.1.11",
          "form": "뉴욕 가보고싶어요",
          "original_form": "뉴욕 가보고싶어요",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043233.1.12",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043233.1.13",
          "form": "네 미드배경이라 봤던장소들",
          "original_form": "네 미드배경이라 봤던장소들",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043233.1.14",
          "form": "하나하나 가보고싶어요",
          "original_form": "하나하나 가보고싶어요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043233.1.15",
          "form": "name2님은 있으세요?",
          "original_form": "&name2&님은 있으세요?",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043233.1.16",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043233.1.17",
          "form": "예를 들어주실 수 있나요??",
          "original_form": "예를 들어주실 수 있나요??",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043233.1.18",
          "form": "저는 미드를 안봐서용 ㅜㅠㅠ",
          "original_form": "저는 미드를 안봐서용 ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043233.1.19",
          "form": "외국영화도 전애인덕분에 관심갖기시작했답니다",
          "original_form": "외국영화도 전애인덕분에 관심갖기시작했답니다",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043233.1.20",
          "form": "음 설명하기 애매하긴한데 주요장면에 나왔던 큰 다리라던지 건물 이런곳이요",
          "original_form": "음 설명하기 애매하긴한데 주요장면에 나왔던 큰 다리라던지 건물 이런곳이요",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100043233.1.21",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100043233.1.22",
          "form": "그럼 그런거와 상관없이",
          "original_form": "그럼 그런거와 상관없이",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043233.1.23",
          "form": "그런 곳은 사진한장이 좋은 추억이되겠네요",
          "original_form": "그런 곳은 사진한장이 좋은 추억이되겠네요",
          "speaker_id": "2",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043233.1.24",
          "form": "가보고싶으신곳 있으세요?",
          "original_form": "가보고싶으신곳 있으세요?",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043233.1.25",
          "form": "그냥 가보고싶은 곳은",
          "original_form": "그냥 가보고싶은 곳은",
          "speaker_id": "2",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043233.1.26",
          "form": "지금운 부산입니다!",
          "original_form": "지금운 부산입니다!",
          "speaker_id": "2",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043233.1.27",
          "form": "저 부산사람이라",
          "original_form": "저 부산사람이라",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043233.1.28",
          "form": "많은 인파 속에 혼자 덩그러니 남아서 바다를 보고싶어료",
          "original_form": "많은 인파 속에 혼자 덩그러니 남아서 바다를 보고싶어료",
          "speaker_id": "2",
