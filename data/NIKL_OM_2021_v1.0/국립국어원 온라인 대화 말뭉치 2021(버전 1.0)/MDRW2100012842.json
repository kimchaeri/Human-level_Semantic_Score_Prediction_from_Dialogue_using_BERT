      "utterance": [
        {
          "id": "MDRW2100012842.1.1",
          "form": "접속중이시네요 ㅎㅎ",
          "original_form": "접속중이시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100012842.1.2",
          "form": "전 게임하고 왔어요 ㅋㅋ",
          "original_form": "전 게임하고 왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.3",
          "form": "무슨 게임하셨어요?",
          "original_form": "무슨 게임하셨어요?",
          "speaker_id": "2",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.4",
          "form": "롤토체스요",
          "original_form": "롤토체스요",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.5",
          "form": "아실까요?",
          "original_form": "아실까요?",
          "speaker_id": "1",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.6",
          "form": "롤은 들어봤는ㄴ",
          "original_form": "롤은 들어봤는ㄴ",
          "speaker_id": "2",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.7",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.8",
          "form": "롤은 잘 모르겠네요",
          "original_form": "롤은 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210825 02:34"
        },
        {
          "id": "MDRW2100012842.1.9",
          "form": "그 롤에서 하는 게임이에요!!",
          "original_form": "그 롤에서 하는 게임이에요!!",
          "speaker_id": "1",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.10",
          "form": "친구가 알려줘서 해봤어요 ㅋㅋㅋ",
          "original_form": "친구가 알려줘서 해봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.11",
          "form": "저는 게임을 잘 못해서",
          "original_form": "저는 게임을 잘 못해서",
          "speaker_id": "2",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.12",
          "form": "잘 몰랐네요 ㅎㅎ",
          "original_form": "잘 몰랐네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.13",
          "form": "저도 잘 못해서 잘 안하는데",
          "original_form": "저도 잘 못해서 잘 안하는데",
          "speaker_id": "1",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.14",
          "form": "너무 심심해서 ㅋㅋㅋ",
          "original_form": "너무 심심해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.15",
          "form": "폰 게임도 안하세요?",
          "original_form": "폰 게임도 안하세요?",
          "speaker_id": "1",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012842.1.16",
          "form": "옛날에 애니팡만 해봤어요 ㅎㅎ",
          "original_form": "옛날에 애니팡만 해봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.17",
          "form": "ㅋㅋㅋ 저도 그거 했었는데",
          "original_form": "ㅋㅋㅋ 저도 그거 했었는데",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.18",
          "form": "엄청 열심히 ㅋㅋㅋ",
          "original_form": "엄청 열심히 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.19",
          "form": "그 때 다들 난리였죠",
          "original_form": "그 때 다들 난리였죠",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.20",
          "form": "네 ㅋㅋ 지금은 브롤스타즈 하고있어요",
          "original_form": "네 ㅋㅋ 지금은 브롤스타즈 하고있어요",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.21",
          "form": "그거 아들도 하는 것 같더라구요",
          "original_form": "그거 아들도 하는 것 같더라구요",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.22",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012842.1.23",
          "form": "네 ㅋㅋㅋ 재밌어요",
          "original_form": "네 ㅋㅋㅋ 재밌어요",
          "speaker_id": "1",
          "time": "20210825 02:37"
        },
        {
          "id": "MDRW2100012842.1.24",
          "form": "이제 좀 잘 해서 ㅋㅋㅋ",
          "original_form": "이제 좀 잘 해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:37"
        },
        {
          "id": "MDRW2100012842.1.25",
          "form": "잘하시는군요",
          "original_form": "잘하시는군요",
          "speaker_id": "2",
          "time": "20210825 02:37"
        }
      ]
