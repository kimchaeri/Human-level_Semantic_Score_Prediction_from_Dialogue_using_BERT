      "utterance": [
        {
          "id": "MDRW2100041717.1.1",
          "form": "음 얼마나 기분이 좋든지ㅋㅋㅋㅋ",
          "original_form": "음 얼마나 기분이 좋든지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041717.1.2",
          "form": "좋지 좋지",
          "original_form": "좋지 좋지",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041717.1.3",
          "form": "나도 ㅋㅋ 어제 시댁갔거든",
          "original_form": "나도 ㅋㅋ 어제 시댁갔거든",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041717.1.4",
          "form": "어머님이 이마트 가자는거야. 자기 바지좀사자구",
          "original_form": "어머님이 이마트 가자는거야. 자기 바지좀사자구",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041717.1.5",
          "form": "어어 이마트",
          "original_form": "어어 이마트",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041717.1.6",
          "form": "그래서 출동했지",
          "original_form": "그래서 출동했지",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041717.1.7",
          "form": "출동해야지 출동해야지",
          "original_form": "출동해야지 출동해야지",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041717.1.8",
          "form": "어머님 바지3벌사고 ㅋㅋㅋ",
          "original_form": "어머님 바지3벌사고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041717.1.9",
          "form": "나는 치마 들었다가 가격보고 놔두고 ㅋㅋㅋ name3 바지를 골랐지",
          "original_form": "나는 치마 들었다가 가격보고 놔두고 ㅋㅋㅋ &name3& 바지를 골랐지",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.10",
          "form": "근데 가을시즌이라 그런가 다 비싼거야",
          "original_form": "근데 가을시즌이라 그런가 다 비싼거야",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.11",
          "form": "니 것도 딱 넣었어야지ㅋㅋㅋㅋ",
          "original_form": "니 것도 딱 넣었어야지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.12",
          "form": "맞아 지금 나오는거는 다 비 쌈",
          "original_form": "맞아 지금 나오는거는 다 비 쌈",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.13",
          "form": "그래가꼬 계속 고르다가",
          "original_form": "그래가꼬 계속 고르다가",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.14",
          "form": "응응 5900원짜리 레깅스를 봣거든?",
          "original_form": "응응 5900원짜리 레깅스를 봣거든?",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.15",
          "form": "대단한데~",
          "original_form": "대단한데~",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.16",
          "form": "솔직히 바지 하나 몇만원주고 사는건",
          "original_form": "솔직히 바지 하나 몇만원주고 사는건",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.17",
          "form": "낭비인것같아서",
          "original_form": "낭비인것같아서",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041717.1.18",
          "form": "어른껏도 아니구 둘째꺼라가 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어른껏도 아니구 둘째꺼라가 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.19",
          "form": "멋지다 멋지다~",
          "original_form": "멋지다 멋지다~",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.20",
          "form": "애들은 금방 크니까",
          "original_form": "애들은 금방 크니까",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.21",
          "form": "그래 첫째 거랑은 또 다르지ㅋㅋㅋㅋ",
          "original_form": "그래 첫째 거랑은 또 다르지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.22",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.23",
          "form": "그래서 ㅋㅋ",
          "original_form": "그래서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.24",
          "form": "그걸 사서 왔는데 ㅋㅋ",
          "original_form": "그걸 사서 왔는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.25",
          "form": "나도 둘째는 맨날 고민한다ㅋㅋㅋ",
          "original_form": "나도 둘째는 맨날 고민한다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.26",
          "form": "어머님이 더 골라라했는데 ㅋㅋ",
          "original_form": "어머님이 더 골라라했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:54"
        },
        {
          "id": "MDRW2100041717.1.27",
          "form": "그냥 그거 하나만 샀어",
          "original_form": "그냥 그거 하나만 샀어",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041717.1.28",
          "form": "첫째 거는 그냥 사주는데ㅋㅋㅋㅋ",
          "original_form": "첫째 거는 그냥 사주는데ㅋㅋㅋㅋ",
