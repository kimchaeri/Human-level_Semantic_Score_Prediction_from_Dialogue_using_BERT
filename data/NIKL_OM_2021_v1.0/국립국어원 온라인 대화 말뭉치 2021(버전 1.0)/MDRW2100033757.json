      "utterance": [
        {
          "id": "MDRW2100033757.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100033757.1.2",
          "form": "최근에 레이싱게임 휠을 샀어요",
          "original_form": "최근에 레이싱게임 휠을 샀어요",
          "speaker_id": "1",
          "time": "20210825 10:05"
        },
        {
          "id": "MDRW2100033757.1.3",
          "form": "휠이요??",
          "original_form": "휠이요??",
          "speaker_id": "2",
          "time": "20210825 11:30"
        },
        {
          "id": "MDRW2100033757.1.4",
          "form": "어플에서인가요?",
          "original_form": "어플에서인가요?",
          "speaker_id": "2",
          "time": "20210825 11:30"
        },
        {
          "id": "MDRW2100033757.1.5",
          "form": "어플이요?",
          "original_form": "어플이요?",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100033757.1.6",
          "form": "레이싱게임이라고하셔서요ㅋㅋㅋ",
          "original_form": "레이싱게임이라고하셔서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100033757.1.7",
          "form": "컴퓨터에 연결해서 쓰는",
          "original_form": "컴퓨터에 연결해서 쓰는",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100033757.1.8",
          "form": "도구...?컨트롤러 예요",
          "original_form": "도구...?컨트롤러 예요",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100033757.1.9",
          "form": "아 레이싱게임 좋아하시나봐요",
          "original_form": "아 레이싱게임 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033757.1.10",
          "form": "유로트럭이라는 게임을 좋아해서",
          "original_form": "유로트럭이라는 게임을 좋아해서",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033757.1.11",
          "form": "보다 재밌게하려고 사봤어요",
          "original_form": "보다 재밌게하려고 사봤어요",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033757.1.12",
          "form": "최근에 사신거 있으신가요?",
          "original_form": "최근에 사신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033757.1.13",
          "form": "아 유로트럭 들어봤어요ㅋㅋㅋ",
          "original_form": "아 유로트럭 들어봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033757.1.14",
          "form": "저는 운동화요!",
          "original_form": "저는 운동화요!",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033757.1.15",
          "form": "날씨풀리면 새신신고 산책다니시면 좋겠네요!",
          "original_form": "날씨풀리면 새신신고 산책다니시면 좋겠네요!",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100033757.1.16",
          "form": "네네ㅋㅋㅋ 아디다스꺼",
          "original_form": "네네ㅋㅋㅋ 아디다스꺼",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033757.1.17",
          "form": "샀어요",
          "original_form": "샀어요",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100033757.1.18",
          "form": "오프라인으로 사셨나요?",
          "original_form": "오프라인으로 사셨나요?",
          "speaker_id": "1",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100033757.1.19",
          "form": "아니요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100033757.1.20",
          "form": "롯데몰에서",
          "original_form": "롯데몰에서",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100033757.1.21",
          "form": "구입했답니다",
          "original_form": "구입했답니다",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100033757.1.22",
          "form": "저도 신발은 신어보고 사게되더라구요",
          "original_form": "저도 신발은 신어보고 사게되더라구요",
          "speaker_id": "1",
