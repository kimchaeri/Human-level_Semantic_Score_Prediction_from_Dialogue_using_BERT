      "utterance": [
        {
          "id": "MDRW2100024090.1.1",
          "form": "학생 축제들 가보고싶어요",
          "original_form": "학생 축제들 가보고싶어요",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024090.1.2",
          "form": "일반인도 된다니 궁금해요",
          "original_form": "일반인도 된다니 궁금해요",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100024090.1.3",
          "form": "아? 그래요?",
          "original_form": "아? 그래요?",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024090.1.4",
          "form": "되는구나 처음알았어요",
          "original_form": "되는구나 처음알았어요",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024090.1.5",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024090.1.6",
          "form": "일반인이 참가할 수 있는 날이 있고 학생들만 되는 날 있고 그렇더라구요",
          "original_form": "일반인이 참가할 수 있는 날이 있고 학생들만 되는 날 있고 그렇더라구요",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100024090.1.7",
          "form": "오 좋다 가보고싶네요",
          "original_form": "오 좋다 가보고싶네요",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024090.1.8",
          "form": "그감성느끼로",
          "original_form": "그감성느끼로",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100024090.1.9",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024090.1.10",
          "form": "코로나 좀 풀렸으면..좋겠어유",
          "original_form": "코로나 좀 풀렸으면..좋겠어유",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024090.1.11",
          "form": "흐흑 저두요...ㅠ",
          "original_form": "흐흑 저두요...ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100024090.1.12",
          "form": "또 가고싶은곳 있으세요?",
          "original_form": "또 가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100024090.1.13",
          "form": "파리요!",
          "original_form": "파리요!",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024090.1.14",
          "form": "미국이랑",
          "original_form": "미국이랑",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024090.1.15",
          "form": "아 미국 가보고 싶죠",
          "original_form": "아 미국 가보고 싶죠",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100024090.1.16",
          "form": "유럽도 가보고 싶네요",
          "original_form": "유럽도 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024090.1.17",
          "form": "저두요..가고싶은데는많은데ㅠ",
          "original_form": "저두요..가고싶은데는많은데ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024090.1.18",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024090.1.19",
          "form": "여행가면 기념품 사시는편이신가요?",
          "original_form": "여행가면 기념품 사시는편이신가요?",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024090.1.20",
          "form": "먹을거 위주로사갑니다ㅋㅋㅋ",
          "original_form": "먹을거 위주로사갑니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024090.1.21",
          "form": "크으 먹는게 최고긴 하죵ㅋㅋ",
          "original_form": "크으 먹는게 최고긴 하죵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024090.1.22",
          "form": "그쵸 먹는건 못참죠",
          "original_form": "그쵸 먹는건 못참죠",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024090.1.23",
          "form": "그럼용!",
          "original_form": "그럼용!",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024090.1.24",
          "form": "선물로도 좋구요~",
          "original_form": "선물로도 좋구요~",
          "speaker_id": "1",
          "time": "20210830 22:48"
        }
      ]
