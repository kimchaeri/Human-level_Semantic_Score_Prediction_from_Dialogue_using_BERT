      "utterance": [
        {
          "id": "MDRW2100005663.1.1",
          "form": "저녁 뭐드셨어요?",
          "original_form": "저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005663.1.2",
          "form": "그러니까요ㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005663.1.3",
          "form": "저 점저로 간짜장 먹었어요",
          "original_form": "저 점저로 간짜장 먹었어요",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005663.1.4",
          "form": "간짜장 맛있었겠어요..",
          "original_form": "간짜장 맛있었겠어요..",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005663.1.5",
          "form": "양파 아삭해서 간짜장 맛있죠",
          "original_form": "양파 아삭해서 간짜장 맛있죠",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005663.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005663.1.7",
          "form": "짬뽕도 먹었는디",
          "original_form": "짬뽕도 먹었는디",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005663.1.8",
          "form": "국물이 좋더라고요",
          "original_form": "국물이 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005663.1.9",
          "form": "간짜장에 짬뽕 국물까지 완벽한 점저였네요..",
          "original_form": "간짜장에 짬뽕 국물까지 완벽한 점저였네요..",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005663.1.10",
          "form": "그쵸... 최고의 조합이었습니다",
          "original_form": "그쵸... 최고의 조합이었습니다",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005663.1.11",
          "form": "저는 마라탕 시켜먹었어요 !",
          "original_form": "저는 마라탕 시켜먹었어요 !",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005663.1.12",
          "form": "어우 맛있었겠어요ㅠㅠㅠㅠ",
          "original_form": "어우 맛있었겠어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005663.1.13",
          "form": "어제 먹었는데 또 먹고 싶네요 전",
          "original_form": "어제 먹었는데 또 먹고 싶네요 전",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005663.1.14",
          "form": "마라탕에 어떤 재료 넣으세요? 저는 건두부랑 옥수수당면은 꼭 넣어요ㅎㅎ",
          "original_form": "마라탕에 어떤 재료 넣으세요? 저는 건두부랑 옥수수당면은 꼭 넣어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005663.1.15",
          "form": "오 저도 넣어요!!",
          "original_form": "오 저도 넣어요!!",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005663.1.16",
          "form": "저는 고구마떡 꼭 넣어요",
          "original_form": "저는 고구마떡 꼭 넣어요",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005663.1.17",
          "form": "그 외에는 청경채랑 햄 같은 거?",
          "original_form": "그 외에는 청경채랑 햄 같은 거?",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005663.1.18",
          "form": "고구마떡 맛있죠 달콤하구 ㅠㅠ",
          "original_form": "고구마떡 맛있죠 달콤하구 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.19",
          "form": "얘기하니까 또 먹고싶네요..",
          "original_form": "얘기하니까 또 먹고싶네요..",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.20",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.21",
          "form": "저도...",
          "original_form": "저도...",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.22",
          "form": "꿔바로우도 같이 먹으면 더맛있죠..",
          "original_form": "꿔바로우도 같이 먹으면 더맛있죠..",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.23",
          "form": "마라탕은 마약이에요",
          "original_form": "마라탕은 마약이에요",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.24",
          "form": "마라샹궈는 드셔보셨너요",
          "original_form": "마라샹궈는 드셔보셨너요",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005663.1.25",
          "form": "샹궈도 좋아해요! 마라탕과는 또다른 매력있어요ㅠ",
          "original_form": "샹궈도 좋아해요! 마라탕과는 또다른 매력있어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005663.1.26",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:57"
        }
      ]
    }
