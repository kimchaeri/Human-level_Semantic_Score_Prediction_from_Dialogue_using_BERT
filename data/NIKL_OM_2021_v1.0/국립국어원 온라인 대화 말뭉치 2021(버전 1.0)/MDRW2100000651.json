        },
        {
          "id": "MDRW2100000651.1.2",
          "form": "연애하고 계신가요",
          "original_form": "연애하고 계신가요",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000651.1.3",
          "form": "사랑하고있습니다",
          "original_form": "사랑하고있습니다",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000651.1.4",
          "form": "ㅋㅋㅋㅋ오호..",
          "original_form": "ㅋㅋㅋㅋ오호..",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000651.1.5",
          "form": "얼마나 되셧나요",
          "original_form": "얼마나 되셧나요",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000651.1.6",
          "form": "624일째입니다",
          "original_form": "624일째입니다",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000651.1.7",
          "form": "핫할때죠",
          "original_form": "핫할때죠",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000651.1.8",
          "form": "와 오래 되셨네요!",
          "original_form": "와 오래 되셨네요!",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000651.1.9",
          "form": "연상인가요 연하인가요",
          "original_form": "연상인가요 연하인가요",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000651.1.10",
          "form": "2살 연하입니다.",
          "original_form": "2살 연하입니다.",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000651.1.11",
          "form": "근데 동갑같아요",
          "original_form": "근데 동갑같아요",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000651.1.12",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000651.1.13",
          "form": "덩치가 일단 저랑 비슷하구",
          "original_form": "덩치가 일단 저랑 비슷하구",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000651.1.14",
          "form": "성격이 아주 괄괄합니다",
          "original_form": "성격이 아주 괄괄합니다",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000651.1.15",
          "form": "ㅋㅋ아하",
          "original_form": "ㅋㅋ아하",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000651.1.16",
          "form": "괄괄한 성격",
          "original_form": "괄괄한 성격",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000651.1.17",
          "form": "어디서 만나셧어요??",
          "original_form": "어디서 만나셧어요??",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000651.1.18",
          "form": "운동하다 만났어요",
          "original_form": "운동하다 만났어요",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000651.1.19",
          "form": "주짓수",
          "original_form": "주짓수",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000651.1.20",
          "form": "오..둘다 건강하시겠네요",
          "original_form": "오..둘다 건강하시겠네요",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000651.1.21",
          "form": "저는 연약한데",
          "original_form": "저는 연약한데",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000651.1.22",
          "form": "그친구는 진짜",
          "original_form": "그친구는 진짜",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000651.1.23",
          "form": "가끔 제가 얻어맞아요",
          "original_form": "가끔 제가 얻어맞아요",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000651.1.24",
          "form": "아..?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아..?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000651.1.25",
          "form": "천생연분같은데요",
          "original_form": "천생연분같은데요",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000651.1.26",
          "form": "그런 취향 아니긴 한데",
          "original_form": "그런 취향 아니긴 한데",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000651.1.27",
          "form": "어쩌다 보니 그렇게 된겁니다",
          "original_form": "어쩌다 보니 그렇게 된겁니다",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000651.1.28",
          "form": "오해하지 마세요",
          "original_form": "오해하지 마세요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000651.1.29",
          "form": "ㅋㅋ ㅋ ㅋㅋ오해안했습니다 ㅎ",
          "original_form": "ㅋㅋ ㅋ ㅋㅋ오해안했습니다 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000651.1.30",
          "form": "name1님은 사랑하고 계시나요?",
          "original_form": "&name1&님은 사랑하고 계시나요?",
