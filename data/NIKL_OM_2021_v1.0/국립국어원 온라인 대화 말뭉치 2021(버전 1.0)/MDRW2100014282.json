      "utterance": [
        {
          "id": "MDRW2100014282.1.1",
          "form": "가상세계는 기억나는게 최근에 데뷔한 아이돌 그룹 밖에 없네요",
          "original_form": "가상세계는 기억나는게 최근에 데뷔한 아이돌 그룹 밖에 없네요",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014282.1.2",
          "form": "에스파요?",
          "original_form": "에스파요?",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014282.1.3",
          "form": "에스파라고 사이버 동료와 함께 활동하더라구요",
          "original_form": "에스파라고 사이버 동료와 함께 활동하더라구요",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014282.1.4",
          "form": "그쵸 4명인데 8명이라고 하더라구요",
          "original_form": "그쵸 4명인데 8명이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014282.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014282.1.6",
          "form": "처음에는 신기한데 또 익숙해지는게 웃기네요",
          "original_form": "처음에는 신기한데 또 익숙해지는게 웃기네요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014282.1.7",
          "form": "그쵸 이번에 제페토라는 앱을 알게되었는데",
          "original_form": "그쵸 이번에 제페토라는 앱을 알게되었는데",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014282.1.8",
          "form": "메타버스 앱인데 사용해보셨나요?",
          "original_form": "메타버스 앱인데 사용해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014282.1.9",
          "form": "아뇨 처음 들어봐요.",
          "original_form": "아뇨 처음 들어봐요.",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014282.1.10",
          "form": "혹시 어떤 어플인지 알 수 있을까요?",
          "original_form": "혹시 어떤 어플인지 알 수 있을까요?",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014282.1.11",
          "form": "예전 싸이월드나 심즈와 비슷한 앱이에요",
          "original_form": "예전 싸이월드나 심즈와 비슷한 앱이에요",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014282.1.12",
          "form": "캐릭터 만들고 맵을 돌아다니며 활동해요",
          "original_form": "캐릭터 만들고 맵을 돌아다니며 활동해요",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014282.1.13",
          "form": "오 심즈나 싸이월드면 캐릭터",
          "original_form": "오 심즈나 싸이월드면 캐릭터",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014282.1.14",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014282.1.15",
          "form": "한번 체험해볼만해요",
          "original_form": "한번 체험해볼만해요",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014282.1.16",
          "form": "한참 심즈 게임에 빠졌었는데",
          "original_form": "한참 심즈 게임에 빠졌었는데",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014282.1.17",
          "form": "해보고싶네요",
          "original_form": "해보고싶네요",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014282.1.18",
          "form": "사실 회사 이벤트 참여하려고 가입했어요 ㅋㅋ",
          "original_form": "사실 회사 이벤트 참여하려고 가입했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014282.1.19",
          "form": "오ㅎㅎ 이벤트가 중요하죠",
          "original_form": "오ㅎㅎ 이벤트가 중요하죠",
          "speaker_id": "2",
