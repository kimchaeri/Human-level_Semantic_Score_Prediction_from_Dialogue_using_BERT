      "utterance": [
        {
          "id": "MDRW2100022444.1.1",
          "form": "곰표맥주 드셔보셨나여",
          "original_form": "곰표맥주 드셔보셨나여",
          "speaker_id": "1",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022444.1.2",
          "form": "아뇨 그런데 곰표과자는 먹어봤어요",
          "original_form": "아뇨 그런데 곰표과자는 먹어봤어요",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022444.1.3",
          "form": "맥주를 드셔보셨나요?",
          "original_form": "맥주를 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022444.1.4",
          "form": "오 네네 먹어봤어용",
          "original_form": "오 네네 먹어봤어용",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022444.1.5",
          "form": "일반 맥주랑 맛이 다른가요?",
          "original_form": "일반 맥주랑 맛이 다른가요?",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022444.1.6",
          "form": "음 그냥 한번먹어볼 정도였네요ㅋㅋ",
          "original_form": "음 그냥 한번먹어볼 정도였네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022444.1.7",
          "form": "저도 그랬습니다 그냥 평범한 맛ㅋㅋ",
          "original_form": "저도 그랬습니다 그냥 평범한 맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022444.1.8",
          "form": "오 드셔보셨군요 왜그렇게 난리였는지ㅋㅋ",
          "original_form": "오 드셔보셨군요 왜그렇게 난리였는지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022444.1.9",
          "form": "그냥 심심하구 사람들은 돈이 없구",
          "original_form": "그냥 심심하구 사람들은 돈이 없구",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022444.1.10",
          "form": "적당히 싼 즐길거리가 필요해서라고 생각합니다",
          "original_form": "적당히 싼 즐길거리가 필요해서라고 생각합니다",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022444.1.11",
          "form": "마케팅의 힘인듯해요ㅋㅋ",
          "original_form": "마케팅의 힘인듯해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022444.1.12",
          "form": "네 이런 시대에 맞는 마케팅이네요 ㅋㅋㅋ",
          "original_form": "네 이런 시대에 맞는 마케팅이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022444.1.13",
          "form": "예전엔 진짜사기 힘들었는데ㅎㅎ",
          "original_form": "예전엔 진짜사기 힘들었는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022444.1.14",
          "form": "그래도 한동안 잘 팔리고 이제서야 살 수 있으니 성공한 마케팅이죠",
          "original_form": "그래도 한동안 잘 팔리고 이제서야 살 수 있으니 성공한 마케팅이죠",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100022444.1.15",
          "form": "허니버터인가 그 정도는 잘 팔렸네요",
          "original_form": "허니버터인가 그 정도는 잘 팔렸네요",
