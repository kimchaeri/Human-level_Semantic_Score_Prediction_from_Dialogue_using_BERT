      "utterance": [
        {
          "id": "MDRW2100037919.1.1",
          "form": "똥괭이 보시나요!",
          "original_form": "똥괭이 보시나요!",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037919.1.2",
          "form": "할배 보고싶네요ㅠㅠ",
          "original_form": "할배 보고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037919.1.3",
          "form": "보셨었군요 ㅠㅠ",
          "original_form": "보셨었군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037919.1.4",
          "form": "마지막 보고 너무울었어요",
          "original_form": "마지막 보고 너무울었어요",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037919.1.5",
          "form": "병원에서 집오는걸 기다렸나봐요",
          "original_form": "병원에서 집오는걸 기다렸나봐요",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100037919.1.6",
          "form": "똥괭이들 당근 봅니다 ㅎㅎ",
          "original_form": "똥괭이들 당근 봅니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100037919.1.7",
          "form": "할배 가면 쁘니가 많이 무너지면 어쩌나했는데 잘 지내줘서 너무 다행이에요 ㅎㅎ",
          "original_form": "할배 가면 쁘니가 많이 무너지면 어쩌나했는데 잘 지내줘서 너무 다행이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100037919.1.8",
          "form": "헉 생각해보니 제가 떠올린 애는 나나였네요 ㅜㅜ",
          "original_form": "헉 생각해보니 제가 떠올린 애는 나나였네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037919.1.9",
          "form": "그러게요 쁘니가 나름 분홍방에서 적응을 잘해서 다행이에요 ㅎㅎ",
          "original_form": "그러게요 쁘니가 나름 분홍방에서 적응을 잘해서 다행이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037919.1.10",
          "form": "나나 보셨나요 치매 걸린냥이",
          "original_form": "나나 보셨나요 치매 걸린냥이",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100037919.1.11",
          "form": "앗 아니요 처음 들어봤어요!",
          "original_form": "앗 아니요 처음 들어봤어요!",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037919.1.12",
          "form": "채널이름도 나나인가요?",
          "original_form": "채널이름도 나나인가요?",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100037919.1.13",
          "form": "나나&나 인가 그래요",
          "original_form": "나나&나 인가 그래요",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037919.1.14",
          "form": "노묘고양이 유투브였는데",
          "original_form": "노묘고양이 유투브였는데",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037919.1.15",
          "form": "얼마전에 별나라가서 ㅠㅠ",
          "original_form": "얼마전에 별나라가서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037919.1.16",
          "form": "아..ㅠㅠ",
          "original_form": "아..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037919.1.17",
          "form": "만 20 살이라는데",
          "original_form": "만 20 살이라는데",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.18",
          "form": "같이 걱정하시던 분들도 슬프시겠네요ㅠㅠ",
          "original_form": "같이 걱정하시던 분들도 슬프시겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.19",
          "form": "고양이는 나이먹어도 티가 안나는거 같아요",
          "original_form": "고양이는 나이먹어도 티가 안나는거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.20",
          "form": "그러게요 ㅠㅠ 넘나 충격...",
          "original_form": "그러게요 ㅠㅠ 넘나 충격...",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.21",
          "form": "맞아요 항상 애긴데ㅠㅠ",
          "original_form": "맞아요 항상 애긴데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.22",
          "form": "영원한 세살 ㅜㅜㅜㅜㅜ",
          "original_form": "영원한 세살 ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.23",
          "form": "낭만 고양이는 보시나요?",
          "original_form": "낭만 고양이는 보시나요?",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037919.1.24",
          "form": "낭만 고양이도 처음 들어봐요ㅠㅠ",
          "original_form": "낭만 고양이도 처음 들어봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037919.1.25",
          "form": "모르는 채널이 많네요ㅠㅠ",
          "original_form": "모르는 채널이 많네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037919.1.26",
          "form": "살은 괜찮은가요?",
          "original_form": "살은 괜찮은가요?",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100037919.1.27",
          "form": "저희애들은 다이어트가 시급합니다",
          "original_form": "저희애들은 다이어트가 시급합니다",
          "speaker_id": "1",
          "time": "20210901 22:34"
        }
      ]
    }
  ]
