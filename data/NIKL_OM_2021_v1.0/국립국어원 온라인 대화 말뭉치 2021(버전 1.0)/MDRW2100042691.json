        {
          "id": "MDRW2100042691.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "2",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042691.1.2",
          "form": "요즘 배달음식 뭐 먹었어",
          "original_form": "요즘 배달음식 뭐 먹었어",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042691.1.3",
          "form": "오늘은 에그 드랍을 시켜 먹었어",
          "original_form": "오늘은 에그 드랍을 시켜 먹었어",
          "speaker_id": "2",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042691.1.4",
          "form": "그거 궁금한데 어때",
          "original_form": "그거 궁금한데 어때",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042691.1.5",
          "form": "에그 드랍은 값이 싸서 좋아",
          "original_form": "에그 드랍은 값이 싸서 좋아",
          "speaker_id": "2",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042691.1.6",
          "form": "맛은 어때",
          "original_form": "맛은 어때",
          "speaker_id": "1",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042691.1.7",
          "form": "마늘 맛이 정말 맛있어",
          "original_form": "마늘 맛이 정말 맛있어",
          "speaker_id": "2",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042691.1.8",
          "form": "나는 아직 못 먹어 봤어",
          "original_form": "나는 아직 못 먹어 봤어",
          "speaker_id": "1",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042691.1.9",
          "form": "나는 학교 가는 길에 가볍게 하나씩 사먹어봤어",
          "original_form": "나는 학교 가는 길에 가볍게 하나씩 사먹어봤어",
          "speaker_id": "2",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042691.1.10",
          "form": "나는 학교갈때 서브웨이가 좋더라",
          "original_form": "나는 학교갈때 서브웨이가 좋더라",
          "speaker_id": "1",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042691.1.11",
          "form": "서브웨이에서는 어떤 거 좋아하니",
          "original_form": "서브웨이에서는 어떤 거 좋아하니",
          "speaker_id": "2",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042691.1.12",
          "form": "에그마요좋아해",
          "original_form": "에그마요좋아해",
          "speaker_id": "1",
          "time": "20210907 01:34"
        },
        {
          "id": "MDRW2100042691.1.13",
          "form": "나는 크레미",
          "original_form": "나는 크레미",
          "speaker_id": "2",
          "time": "20210907 01:34"
        },
        {
          "id": "MDRW2100042691.1.14",
          "form": "거기 쿠키도 맛있어",
          "original_form": "거기 쿠키도 맛있어",
          "speaker_id": "1",
          "time": "20210907 01:34"
        },
        {
          "id": "MDRW2100042691.1.15",
          "form": "마카다미아 쿠키가 정말 맛있지",
          "original_form": "마카다미아 쿠키가 정말 맛있지",
          "speaker_id": "2",
          "time": "20210907 01:34"
