      "utterance": [
        {
          "id": "MDRW2100046259.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 20:57"
        },
        {
          "id": "MDRW2100046259.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20211015 20:57"
        },
        {
          "id": "MDRW2100046259.1.3",
          "form": "벌써 주말이네요 ^^ 내일은 뭐하시나요?",
          "original_form": "벌써 주말이네요 ^^ 내일은 뭐하시나요?",
          "speaker_id": "2",
          "time": "20211015 20:57"
        },
        {
          "id": "MDRW2100046259.1.4",
          "form": "날씨가 안 좋아서 놀러가진 못하고, 마트가서 먹을 것이나 사와야 지요",
          "original_form": "날씨가 안 좋아서 놀러가진 못하고, 마트가서 먹을 것이나 사와야 지요",
          "speaker_id": "1",
          "time": "20211015 20:58"
        },
        {
          "id": "MDRW2100046259.1.5",
          "form": "가족과 함께 보내시나요?",
          "original_form": "가족과 함께 보내시나요?",
          "speaker_id": "2",
          "time": "20211015 20:58"
        },
        {
          "id": "MDRW2100046259.1.6",
          "form": "오후부터는 날이 갠다고 하네요 ^^",
          "original_form": "오후부터는 날이 갠다고 하네요 ^^",
          "speaker_id": "2",
          "time": "20211015 20:58"
        },
        {
          "id": "MDRW2100046259.1.7",
          "form": "가족과 보냅니다.",
          "original_form": "가족과 보냅니다.",
          "speaker_id": "1",
          "time": "20211015 20:59"
        },
        {
          "id": "MDRW2100046259.1.8",
          "form": "날은 개는데, 바람이 많이 분다네요",
          "original_form": "날은 개는데, 바람이 많이 분다네요",
          "speaker_id": "1",
          "time": "20211015 20:59"
        },
        {
          "id": "MDRW2100046259.1.9",
          "form": "아, 주말에 날씨가 추워진다고 하더라구요",
          "original_form": "아, 주말에 날씨가 추워진다고 하더라구요",
          "speaker_id": "2",
          "time": "20211015 21:00"
        },
        {
          "id": "MDRW2100046259.1.10",
          "form": "집에서 맛있는 거 먹으면서 가족분들과 쉬는 것도 참 좋겠네요~!",
          "original_form": "집에서 맛있는 거 먹으면서 가족분들과 쉬는 것도 참 좋겠네요~!",
          "speaker_id": "2",
          "time": "20211015 21:00"
        },
        {
          "id": "MDRW2100046259.1.11",
          "form": "맞아요. 날씨가 좋으면 부모님 모시고, 가까운 바닷가라도 가려고 했었는데, 조금 아쉽네요",
          "original_form": "맞아요. 날씨가 좋으면 부모님 모시고, 가까운 바닷가라도 가려고 했었는데, 조금 아쉽네요",
          "speaker_id": "1",
          "time": "20211015 21:01"
        },
        {
          "id": "MDRW2100046259.1.12",
          "form": "그러셨구나 ㅠㅠ",
          "original_form": "그러셨구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 21:01"
        },
        {
          "id": "MDRW2100046259.1.13",
          "form": "저번주에 대부도에 갔다왔는데 참 좋더라구요",
          "original_form": "저번주에 대부도에 갔다왔는데 참 좋더라구요",
          "speaker_id": "2",
          "time": "20211015 21:01"
        },
        {
          "id": "MDRW2100046259.1.14",
          "form": "다음주에 날 풀리면 다시 도전해보셔요 ㅎㅎ",
          "original_form": "다음주에 날 풀리면 다시 도전해보셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 21:01"
        },
        {
          "id": "MDRW2100046259.1.15",
          "form": "네. 그래야겠어요.",
          "original_form": "네. 그래야겠어요.",
          "speaker_id": "1",
          "time": "20211015 21:02"
        },
        {
          "id": "MDRW2100046259.1.16",
          "form": "대부도 가까워서 잘 갔었는데...",
          "original_form": "대부도 가까워서 잘 갔었는데...",
          "speaker_id": "1",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.17",
          "form": "너무 좋죠!",
          "original_form": "너무 좋죠!",
          "speaker_id": "2",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.18",
          "form": "바지락칼국수가 좋지요",
          "original_form": "바지락칼국수가 좋지요",
          "speaker_id": "1",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.19",
          "form": "바지락 칼국수도 먹고 왔어요 ㅎㅎ",
          "original_form": "바지락 칼국수도 먹고 왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.20",
          "form": "해물파전도요",
          "original_form": "해물파전도요",
          "speaker_id": "1",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.21",
          "form": "메타세콰이어 길 가서",
          "original_form": "메타세콰이어 길 가서",
          "speaker_id": "2",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.22",
          "form": "전동 바이크도 탔는데 참 재밌더라구요",
          "original_form": "전동 바이크도 탔는데 참 재밌더라구요",
          "speaker_id": "2",
          "time": "20211015 21:04"
        },
        {
          "id": "MDRW2100046259.1.23",
          "form": "잘 다녀오셨네요",
          "original_form": "잘 다녀오셨네요",
          "speaker_id": "1",
          "time": "20211015 21:05"
        },
        {
          "id": "MDRW2100046259.1.24",
          "form": "옆에 있는 제부도도 좋고,",
          "original_form": "옆에 있는 제부도도 좋고,",
          "speaker_id": "1",
          "time": "20211015 21:05"
        },
        {
          "id": "MDRW2100046259.1.25",
          "form": "오, 제부도는 한번도 안 가봤는데 저도 다음에 가봐야겠네요!",
          "original_form": "오, 제부도는 한번도 안 가봤는데 저도 다음에 가봐야겠네요!",
          "speaker_id": "2",
          "time": "20211015 21:05"
        },
        {
          "id": "MDRW2100046259.1.26",
          "form": "또 옆에 있는 탄도항이나, 바다향기 수목원 같은 곳도 좋습니다.",
          "original_form": "또 옆에 있는 탄도항이나, 바다향기 수목원 같은 곳도 좋습니다.",
          "speaker_id": "1",
          "time": "20211015 21:05"
        },
        {
          "id": "MDRW2100046259.1.27",
          "form": "추천 감사합니다! ㅎㅎ",
          "original_form": "추천 감사합니다! ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 21:05"
        },
        {
          "id": "MDRW2100046259.1.28",
          "form": "저도 감사합니다",
          "original_form": "저도 감사합니다",
          "speaker_id": "1",
          "time": "20211015 21:06"
        }
      ]
