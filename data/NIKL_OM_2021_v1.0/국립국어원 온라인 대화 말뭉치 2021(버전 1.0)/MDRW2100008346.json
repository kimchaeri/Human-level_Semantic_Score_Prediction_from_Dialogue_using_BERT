        {
          "id": "MDRW2100008346.1.1",
          "form": "다이어트 해야하는데 아이스크림 엄청 먹었네여",
          "original_form": "다이어트 해야하는데 아이스크림 엄청 먹었네여",
          "speaker_id": "2",
          "time": "20210822 15:20"
        },
        {
          "id": "MDRW2100008346.1.2",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008346.1.3",
          "form": "저 아이스크림 먹으러 가려고요",
          "original_form": "저 아이스크림 먹으러 가려고요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008346.1.4",
          "form": "딱 걸렸네요 ㅋㅋ",
          "original_form": "딱 걸렸네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008346.1.5",
          "form": "저도 진짜ㅠ좋아해요",
          "original_form": "저도 진짜ㅠ좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008346.1.6",
          "form": "레인보우 아몬드봉봉 초코 다 좋아해요 ㅜㅜ",
          "original_form": "레인보우 아몬드봉봉 초코 다 좋아해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008346.1.7",
          "form": "저더 다욧 해야하는데 ㅜㅜ",
          "original_form": "저더 다욧 해야하는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008346.1.8",
          "form": "근데 맛난게 넘많아요",
          "original_form": "근데 맛난게 넘많아요",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008346.1.9",
          "form": "무슨 아이스크림 좋아하세요?",
          "original_form": "무슨 아이스크림 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008346.1.10",
          "form": "저는 녹차랑 초코랑",
          "original_form": "저는 녹차랑 초코랑",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008346.1.11",
          "form": "사실 아이스크림은 뭔들 ㅋㅋ",
          "original_form": "사실 아이스크림은 뭔들 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008346.1.12",
          "form": "멎아요 다좋아요",
          "original_form": "멎아요 다좋아요",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008346.1.13",
          "form": "오 저 녹차 초코 완전좋아요",
          "original_form": "오 저 녹차 초코 완전좋아요",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008346.1.14",
          "form": "요새 아이스크림 무인판매점도 많아서 ㅋㅋ",
          "original_form": "요새 아이스크림 무인판매점도 많아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008346.1.15",
          "form": "도저히 지나치지못하고 ㅜ",
          "original_form": "도저히 지나치지못하고 ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008346.1.16",
          "form": "맞아요 자 한보따리 사요 ㅋㅋ",
          "original_form": "맞아요 자 한보따리 사요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008346.1.17",
          "form": "옥동자 쿠엔크 콘 ㅋㅋ",
          "original_form": "옥동자 쿠엔크 콘 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008346.1.18",
          "form": "저희동네는 과자도 팔아서",
          "original_form": "저희동네는 과자도 팔아서",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008346.1.19",
          "form": "세계과자요 천원짜리",
          "original_form": "세계과자요 천원짜리",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008346.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008346.1.21",
          "form": "한보따리요 ㅋㅋ",
          "original_form": "한보따리요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008346.1.22",
          "form": "전 막사요",
          "original_form": "전 막사요",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008346.1.23",
          "form": "오 저도 못지나쳐요ㅜㅜ",
          "original_form": "오 저도 못지나쳐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008346.1.24",
          "form": "새로나온거 막 사고 그랬어요",
