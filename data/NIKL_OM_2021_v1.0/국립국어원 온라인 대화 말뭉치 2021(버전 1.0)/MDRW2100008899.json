      "utterance": [
        {
          "id": "MDRW2100008899.1.1",
          "form": "귀엽네요",
          "original_form": "귀엽네요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008899.1.2",
          "form": "이럴수가 빨간보석이네요",
          "original_form": "이럴수가 빨간보석이네요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008899.1.3",
          "form": "이렇게 귀여운데 왜 분양보내셨나요",
          "original_form": "이렇게 귀여운데 왜 분양보내셨나요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008899.1.4",
          "form": "너무 힘들어서요ㅋㅋ이쁘긴했는데 그리고 새우는 자꾸 죽더라구요ㅠ",
          "original_form": "너무 힘들어서요ㅋㅋ이쁘긴했는데 그리고 새우는 자꾸 죽더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100008899.1.5",
          "form": "넘 가련한 생명체라ㅜㅜㅜ",
          "original_form": "넘 가련한 생명체라ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008899.1.6",
          "form": "예민한사람 세심한사람이 키유야하나봐요ㅜ",
          "original_form": "예민한사람 세심한사람이 키유야하나봐요ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008899.1.7",
          "form": "그니까요ㅠㅠ",
          "original_form": "그니까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008899.1.8",
          "form": "물품도 한계가ㅠ",
          "original_form": "물품도 한계가ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008899.1.9",
          "form": "전문적인건지ㅋㅋ",
          "original_form": "전문적인건지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008899.1.10",
          "form": "오늘 나오면서 물고기 밥주는걸깜박 햏네요ㅜㅜ",
          "original_form": "오늘 나오면서 물고기 밥주는걸깜박 햏네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 09:30"
        },
        {
          "id": "MDRW2100008899.1.11",
          "form": "하루에 한번 주는데 집에 있는사람한테",
          "original_form": "하루에 한번 주는데 집에 있는사람한테",
          "speaker_id": "2",
          "time": "20210823 09:31"
        },
        {
          "id": "MDRW2100008899.1.12",
          "form": "연락해놔야겠어요ㅋㅋㅋ",
          "original_form": "연락해놔야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:31"
        },
        {
          "id": "MDRW2100008899.1.13",
          "form": "구피들 배고프겠다 하루종일 한끼라니ㅜㅜ",
          "original_form": "구피들 배고프겠다 하루종일 한끼라니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 09:31"
        },
        {
          "id": "MDRW2100008899.1.14",
          "form": "자기들 똥도 먹더라구요ㅋㅋ 하루안줘도 잘살아요",
          "original_form": "자기들 똥도 먹더라구요ㅋㅋ 하루안줘도 잘살아요",
          "speaker_id": "1",
          "time": "20210823 10:44"
        },
        {
          "id": "MDRW2100008899.1.15",
          "form": "안돼~ 먹지마라 얘들아",
          "original_form": "안돼~ 먹지마라 얘들아",
          "speaker_id": "2",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100008899.1.16",
          "form": "물을자주갈아주기엔 바빠요ㅜ",
          "original_form": "물을자주갈아주기엔 바빠요ㅜ",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100008899.1.17",
          "form": "ㅋㅋㅋ똥도 먹고 어린구피도먹고 새우도 먹고 잡식성",
          "original_form": "ㅋㅋㅋ똥도 먹고 어린구피도먹고 새우도 먹고 잡식성",
          "speaker_id": "1",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100008899.1.18",
          "form": "안돼~",
          "original_form": "안돼~",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100008899.1.19",
          "form": "햄스터처럼 자기종족도 먹나요?!",
          "original_form": "햄스터처럼 자기종족도 먹나요?!",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100008899.1.20",
          "form": "네ㅜㅜ 그래서 혹시 치어. 안받아보셨나요?ㅋ",
          "original_form": "네ㅜㅜ 그래서 혹시 치어. 안받아보셨나요?ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100008899.1.21",
          "form": "구피 출산요~",
          "original_form": "구피 출산요~",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100008899.1.22",
          "form": "치어 낳는거 못보셨어요?",
          "original_form": "치어 낳는거 못보셨어요?",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100008899.1.23",
          "form": "치어가 아기구핀가요?",
          "original_form": "치어가 아기구핀가요?",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100008899.1.24",
          "form": "아직 못본거같아요",
          "original_form": "아직 못본거같아요",
          "speaker_id": "2",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100008899.1.25",
          "form": "네ㅡ키운지 얼마나 되셨어요? 배부른 구피가 없나요?",
          "original_form": "네ㅡ키운지 얼마나 되셨어요? 배부른 구피가 없나요?",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100008899.1.26",
          "form": "아직 두달돼서그런가봐요",
          "original_form": "아직 두달돼서그런가봐요",
          "speaker_id": "2",
          "time": "20210823 10:57"
        }
      ]
