      "utterance": [
        {
          "id": "MDRW2100021387.1.1",
          "form": "마트가면 주로 뭐 사시나요",
          "original_form": "마트가면 주로 뭐 사시나요",
          "speaker_id": "1",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100021387.1.2",
          "form": "전 계란 우우는 무조건 사요 \n집에 떨어지면 안된니까요\n근데 에어프라이기 산 후로\n자꾸 냉동식품에 손이 가네요 ㅎㅎ",
          "original_form": "전 계란 우우는 무조건 사요 \n집에 떨어지면 안된니까요\n근데 에어프라이기 산 후로\n자꾸 냉동식품에 손이 가네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 06:30"
        },
        {
          "id": "MDRW2100021387.1.3",
          "form": "요즘 계란값이 올라서",
          "original_form": "요즘 계란값이 올라서",
          "speaker_id": "1",
          "time": "20210829 07:41"
        },
        {
          "id": "MDRW2100021387.1.4",
          "form": "많이 부담스러우시겠어요",
          "original_form": "많이 부담스러우시겠어요",
          "speaker_id": "1",
          "time": "20210829 07:41"
        },
        {
          "id": "MDRW2100021387.1.5",
          "form": "와 진짜 어쩜 이렇게 비싸요? 엄청 부담이었어요",
          "original_form": "와 진짜 어쩜 이렇게 비싸요? 엄청 부담이었어요",
          "speaker_id": "2",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100021387.1.6",
          "form": "진짜.. 가격이 너무 올라서 계란하나 사먹기가 너무 부담되더라구요",
          "original_form": "진짜.. 가격이 너무 올라서 계란하나 사먹기가 너무 부담되더라구요",
          "speaker_id": "1",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100021387.1.7",
          "form": "우유도 슬그슬금 오르고 있어요",
          "original_form": "우유도 슬그슬금 오르고 있어요",
          "speaker_id": "2",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100021387.1.8",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100021387.1.9",
          "form": "사실 저렴한 식자섀가 없는 듯 해요",
          "original_form": "사실 저렴한 식자섀가 없는 듯 해요",
          "speaker_id": "2",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100021387.1.10",
          "form": "다 점점 비싸져요",
          "original_form": "다 점점 비싸져요",
          "speaker_id": "2",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100021387.1.11",
          "form": "그건그래요.. 그나저나 우유도 오른다니",
          "original_form": "그건그래요.. 그나저나 우유도 오른다니",
          "speaker_id": "1",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100021387.1.12",
          "form": "장보기 무섭다니까요",
          "original_form": "장보기 무섭다니까요",
          "speaker_id": "2",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100021387.1.13",
          "form": "티날 정돈 아닌데 자주 사니까 보여요",
          "original_form": "티날 정돈 아닌데 자주 사니까 보여요",
          "speaker_id": "2",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100021387.1.14",
          "form": "진짜 몇개안담아도",
          "original_form": "진짜 몇개안담아도",
          "speaker_id": "1",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100021387.1.15",
          "form": "5만원 훌쩍넘고",
          "original_form": "5만원 훌쩍넘고",
          "speaker_id": "1",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100021387.1.16",
          "form": "진짜 담다보면 와.. 소리 절로나오지않나요",
          "original_form": "진짜 담다보면 와.. 소리 절로나오지않나요",
          "speaker_id": "1",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100021387.1.17",
          "form": "그니까요 장바구니 물가가 장난아니게 올랐어요",
          "original_form": "그니까요 장바구니 물가가 장난아니게 올랐어요",
          "speaker_id": "2",
          "time": "20210829 08:48"
        },
        {
          "id": "MDRW2100021387.1.18",
          "form": "진짜 장보는게 무서워요ㅋㅋ",
          "original_form": "진짜 장보는게 무서워요ㅋㅋ",
