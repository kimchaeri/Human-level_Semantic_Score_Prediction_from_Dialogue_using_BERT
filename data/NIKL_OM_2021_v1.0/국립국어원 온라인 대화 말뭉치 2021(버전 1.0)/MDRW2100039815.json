      "utterance": [
        {
          "id": "MDRW2100039815.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039815.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039815.1.3",
          "form": "여행이 주제라 그런데 혹시 여행 좋아하시나여?",
          "original_form": "여행이 주제라 그런데 혹시 여행 좋아하시나여?",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039815.1.4",
          "form": "기억나는 여행지 있으신가요",
          "original_form": "기억나는 여행지 있으신가요",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039815.1.5",
          "form": "아 국내는 울산 해외는 베트남이여!",
          "original_form": "아 국내는 울산 해외는 베트남이여!",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039815.1.6",
          "form": "여행 좋아해요",
          "original_form": "여행 좋아해요",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039815.1.7",
          "form": "울산 가본적 없는데 어떤가요",
          "original_form": "울산 가본적 없는데 어떤가요",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039815.1.8",
          "form": "바다보러 갔는데 꽤 괜찮았아요! 울산 갔다가 부산 이렇게 갔었어요 ㅎㅎ",
          "original_form": "바다보러 갔는데 꽤 괜찮았아요! 울산 갔다가 부산 이렇게 갔었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039815.1.9",
          "form": "아하 둘이 붙어있으니 그럴 수도 있군요",
          "original_form": "아하 둘이 붙어있으니 그럴 수도 있군요",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039815.1.10",
          "form": "네넵 그렇죠",
          "original_form": "네넵 그렇죠",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039815.1.11",
          "form": "제주도도 좋아해요!!",
          "original_form": "제주도도 좋아해요!!",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039815.1.12",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039815.1.13",
          "form": "오랜만에 제주도 가고 싶어요",
          "original_form": "오랜만에 제주도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039815.1.14",
          "form": "말하니까 가고싶네요 ㅎㅎ안 간 지 2년 정도 됐네요 ㅠㅠ",
          "original_form": "말하니까 가고싶네요 ㅎㅎ안 간 지 2년 정도 됐네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039815.1.15",
          "form": "혹시 여행은 혼자 가시는 걸 좋아하세요 아님 친구랑 같이 다니는 걸 좋아하세요?",
          "original_form": "혹시 여행은 혼자 가시는 걸 좋아하세요 아님 친구랑 같이 다니는 걸 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039815.1.16",
          "form": "저는 가족이랑만 가봤어요",
          "original_form": "저는 가족이랑만 가봤어요",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039815.1.17",
          "form": "아하 혹시 기억나시는 여행지 있으신가요?",
          "original_form": "아하 혹시 기억나시는 여행지 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039815.1.18",
          "form": "오키나와가 기억에 남네요",
          "original_form": "오키나와가 기억에 남네요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039815.1.19",
          "form": "오키나와 저도 가보고 싶네요 ㅠㅠ",
          "original_form": "오키나와 저도 가보고 싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:07"
        }
      ]
