      "utterance": [
        {
          "id": "MDRW2100042932.1.1",
          "form": "저도 지금 샌들 신고 있어요 ㅋㅋㅋㅋ",
          "original_form": "저도 지금 샌들 신고 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100042932.1.2",
          "form": "알바 되시면 좋을텐데!!",
          "original_form": "알바 되시면 좋을텐데!!",
          "speaker_id": "2",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.3",
          "form": "아직은 더운거죠ㅎㅎ",
          "original_form": "아직은 더운거죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.4",
          "form": "네 그럼좋겠어요 알바비로",
          "original_form": "네 그럼좋겠어요 알바비로",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.5",
          "form": "쇼핑하자요",
          "original_form": "쇼핑하자요",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.6",
          "form": "저희 지역은 비가 많이 와서 아직까지는 괜찮네여",
          "original_form": "저희 지역은 비가 많이 와서 아직까지는 괜찮네여",
          "speaker_id": "2",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.7",
          "form": "아 비가 많이내리는군요",
          "original_form": "아 비가 많이내리는군요",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.8",
          "form": "알바비로 쇼핑하는거 좋네여 ㅋㅋㅋ",
          "original_form": "알바비로 쇼핑하는거 좋네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.9",
          "form": "네 인천은 비 많이 와요",
          "original_form": "네 인천은 비 많이 와요",
          "speaker_id": "2",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042932.1.10",
          "form": "알바 면접 잘 보고 오세영!!",
          "original_form": "알바 면접 잘 보고 오세영!!",
          "speaker_id": "2",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100042932.1.11",
          "form": "저 어제 또 화장품 보다가 샀어요",
          "original_form": "저 어제 또 화장품 보다가 샀어요",
          "speaker_id": "2",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100042932.1.12",
          "form": "우와 뭐사셨어요",
          "original_form": "우와 뭐사셨어요",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100042932.1.13",
          "form": "제가 완전 건성이라 크림 하나 살수밖에 없었어요",
          "original_form": "제가 완전 건성이라 크림 하나 살수밖에 없었어요",
          "speaker_id": "2",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100042932.1.14",
          "form": "전 화장귀찮아서 쿠션쓰는데  오늘아침에보니 쿠션이 다떨어져가더라구요",
          "original_form": "전 화장귀찮아서 쿠션쓰는데  오늘아침에보니 쿠션이 다떨어져가더라구요",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100042932.1.15",
          "form": "아하 크림사셨군용ㅎㅎㅎ",
          "original_form": "아하 크림사셨군용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.16",
          "form": "하나 사셔야겠네요",
          "original_form": "하나 사셔야겠네요",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.17",
          "form": "네 다떨어져서 얼른 알아봐야할거같아요~",
          "original_form": "네 다떨어져서 얼른 알아봐야할거같아요~",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.18",
          "form": "쿠션 맞는거 사기도 힘들더라고요",
          "original_form": "쿠션 맞는거 사기도 힘들더라고요",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.19",
          "form": "애들 로션크림워시는 떨어지기전에 척척인데",
          "original_form": "애들 로션크림워시는 떨어지기전에 척척인데",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.20",
          "form": "제껀 다떨어져야 겨우 주문하네요 ㅋㅋㅋ",
          "original_form": "제껀 다떨어져야 겨우 주문하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.21",
          "form": "쿠션도 잘못쓰면 다크닝이 아주아주",
          "original_form": "쿠션도 잘못쓰면 다크닝이 아주아주",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.22",
          "form": "아이쿠야",
          "original_form": "아이쿠야",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.23",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.24",
          "form": "역시 어머니시군요 ㅠㅠ",
          "original_form": "역시 어머니시군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042932.1.25",
          "form": "시간지나면 얼굴이 까매져있......ㅋㅋㄱㄱ 세수하면 돌아오구요 ㅋㄱㅋㅋ",
          "original_form": "시간지나면 얼굴이 까매져있......ㅋㅋㄱㄱ 세수하면 돌아오구요 ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042932.1.26",
          "form": "근데 아이들도 다섯살 미만 아니니까 이젠 엄마 좀 챙겨도 될거 같아요 ㅎㅎㅎ",
          "original_form": "근데 아이들도 다섯살 미만 아니니까 이젠 엄마 좀 챙겨도 될거 같아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042932.1.27",
          "form": "맞아여 까매지더라고요 ㅠㅠ",
          "original_form": "맞아여 까매지더라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042932.1.28",
          "form": "어머니 화이팅!!",
          "original_form": "어머니 화이팅!!",
          "speaker_id": "2",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042932.1.29",
          "form": "ㅋㅋㅋ 갈수록 돈이 많이들어가는게 함정이에요",
          "original_form": "ㅋㅋㅋ 갈수록 돈이 많이들어가는게 함정이에요",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042932.1.30",
          "form": "ㅋㅋㄱㅋ name2님도 화이팅!!",
          "original_form": "ㅋㅋㄱㅋ &name2&님도 화이팅!!",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042932.1.31",
          "form": "그렇겠져!! 클수록 더",
          "original_form": "그렇겠져!! 클수록 더",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042932.1.32",
          "form": "네 저도!! ㅎㅎㅎ",
          "original_form": "네 저도!! ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042932.1.33",
          "form": "저 폰케이스 헤지스가와서",
          "original_form": "저 폰케이스 헤지스가와서",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042932.1.34",
          "form": "중고거래하고싶은데",
          "original_form": "중고거래하고싶은데",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042932.1.35",
          "form": "판매는 많이해봤는데 구매를 안해봐서 두근두근해요 ㅋㄱㅋㅋ",
          "original_form": "판매는 많이해봤는데 구매를 안해봐서 두근두근해요 ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042932.1.36",
          "form": "제꺼 내놓고 원하는걸로 구매하고싶은데 ㅎㅎ",
          "original_form": "제꺼 내놓고 원하는걸로 구매하고싶은데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100042932.1.37",
          "form": "오!!! 저 당근 같은거 한번도 안해봤어요",
          "original_form": "오!!! 저 당근 같은거 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100042932.1.38",
          "form": "파는것도 부지런해야 가능하더라고요",
          "original_form": "파는것도 부지런해야 가능하더라고요",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100042932.1.39",
          "form": "생필품 같은것도 은근히 많이 사게 되는거 같아요",
          "original_form": "생필품 같은것도 은근히 많이 사게 되는거 같아요",
