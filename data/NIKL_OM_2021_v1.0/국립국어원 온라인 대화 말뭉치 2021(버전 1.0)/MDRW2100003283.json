      "utterance": [
        {
          "id": "MDRW2100003283.1.1",
          "form": "혹시 사고싶은 물건 있어요?",
          "original_form": "혹시 사고싶은 물건 있어요?",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003283.1.2",
          "form": "요즘은 물욕이 사라져서 딱히없네요ㅎ 가을오니 옷을 사긴해야되는데",
          "original_form": "요즘은 물욕이 사라져서 딱히없네요ㅎ 가을오니 옷을 사긴해야되는데",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003283.1.3",
          "form": "그러면 중고 직거래 해보셨어요?",
          "original_form": "그러면 중고 직거래 해보셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:25"
        },
        {
          "id": "MDRW2100003283.1.4",
          "form": "구매는안하고 판매만 해봤어요",
          "original_form": "구매는안하고 판매만 해봤어요",
          "speaker_id": "2",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003283.1.5",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "2",
          "time": "20210820 21:26"
        },
        {
          "id": "MDRW2100003283.1.6",
          "form": "저도 책팔고있는데",
          "original_form": "저도 책팔고있는데",
          "speaker_id": "1",
          "time": "20210820 21:28"
        },
        {
          "id": "MDRW2100003283.1.7",
          "form": "되게 안팔리네요",
          "original_form": "되게 안팔리네요",
          "speaker_id": "1",
          "time": "20210820 21:28"
        },
        {
          "id": "MDRW2100003283.1.8",
          "form": "아 책은 진짜 싸게내놓지 않으면 안팔릴듯요",
          "original_form": "아 책은 진짜 싸게내놓지 않으면 안팔릴듯요",
          "speaker_id": "2",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003283.1.9",
          "form": "어디다 올리세요?",
          "original_form": "어디다 올리세요?",
          "speaker_id": "2",
          "time": "20210820 21:29"
        },
        {
          "id": "MDRW2100003283.1.10",
          "form": "번개장터에 올리고있어요",
          "original_form": "번개장터에 올리고있어요",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003283.1.11",
          "form": "당근마켓은 아무리올려도 안팔리더라구요",
          "original_form": "당근마켓은 아무리올려도 안팔리더라구요",
          "speaker_id": "1",
          "time": "20210820 21:31"
        },
        {
          "id": "MDRW2100003283.1.12",
          "form": "책은 당근에서 안팔릴거같아요",
          "original_form": "책은 당근에서 안팔릴거같아요",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003283.1.13",
          "form": "전 주로 기프티콘 팔아요",
          "original_form": "전 주로 기프티콘 팔아요",
          "speaker_id": "2",
          "time": "20210820 21:32"
        },
        {
          "id": "MDRW2100003283.1.14",
          "form": "기프티콘 어떻게 얻으셔요?",
          "original_form": "기프티콘 어떻게 얻으셔요?",
          "speaker_id": "1",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003283.1.15",
          "form": "이벤트 응모해서 당첨되기도 하고 적립금으로 살거없을때 사기도 하고요",
          "original_form": "이벤트 응모해서 당첨되기도 하고 적립금으로 살거없을때 사기도 하고요",
          "speaker_id": "2",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100003283.1.16",
          "form": "이벤트 응모는 1년에 얼마나 당첨되셔요?",
          "original_form": "이벤트 응모는 1년에 얼마나 당첨되셔요?",
          "speaker_id": "1",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003283.1.17",
          "form": "앱테크 열심히할땐  한달에 몇개씩도 당첨되기도하고",
          "original_form": "앱테크 열심히할땐  한달에 몇개씩도 당첨되기도하고",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003283.1.18",
          "form": "백프로 주는거 하기도 해서",
          "original_form": "백프로 주는거 하기도 해서",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003283.1.19",
          "form": "오늘 스벅만 5개 왔더라구요ㅎ가족꺼포함",
          "original_form": "오늘 스벅만 5개 왔더라구요ㅎ가족꺼포함",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003283.1.20",
          "form": "오 그러면 한달에 얼마나 버셔요?",
          "original_form": "오 그러면 한달에 얼마나 버셔요?",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003283.1.21",
          "form": "계산은 안해봤는데 제가먹기도하고ㅎ 보통 스벅10잔정도는 생기더라구요",
          "original_form": "계산은 안해봤는데 제가먹기도하고ㅎ 보통 스벅10잔정도는 생기더라구요",
          "speaker_id": "2",
          "time": "20210820 21:46"
        },
        {
          "id": "MDRW2100003283.1.22",
          "form": "짜잘한것도 있고",
          "original_form": "짜잘한것도 있고",
          "speaker_id": "2",
          "time": "20210820 21:46"
        }
      ]
    }
