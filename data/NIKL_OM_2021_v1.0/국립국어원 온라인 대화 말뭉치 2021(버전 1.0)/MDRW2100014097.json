      "utterance": [
        {
          "id": "MDRW2100014097.1.1",
          "form": "주지훈이면 진짜",
          "original_form": "주지훈이면 진짜",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014097.1.2",
          "form": "얼굴이랑 몸이랑 둘다 보시네요",
          "original_form": "얼굴이랑 몸이랑 둘다 보시네요",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014097.1.3",
          "form": "찾아보니",
          "original_form": "찾아보니",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014097.1.4",
          "form": "이런 양심없는 인간",
          "original_form": "이런 양심없는 인간",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014097.1.5",
          "form": "제 최애연예인 중 주지훈 이동욱은 최상위군...",
          "original_form": "제 최애연예인 중 주지훈 이동욱은 최상위군...",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014097.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014097.1.7",
          "form": "전 얼굴만보는데 덤으로 몸까지 좋은거죠!!",
          "original_form": "전 얼굴만보는데 덤으로 몸까지 좋은거죠!!",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014097.1.8",
          "form": "저는 성격만 보는데 그런사람들이 얼굴도 이쁘더라구요",
          "original_form": "저는 성격만 보는데 그런사람들이 얼굴도 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014097.1.9",
          "form": "ㅋㅋㄱ뭔가...내가 쓰레기가 된 기분이지 왜",
          "original_form": "ㅋㅋㄱ뭔가...내가 쓰레기가 된 기분이지 왜",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014097.1.10",
          "form": "아녀요 아녀요",
          "original_form": "아녀요 아녀요",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014097.1.11",
          "form": "이상형은 다 있응께",
          "original_form": "이상형은 다 있응께",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014097.1.12",
          "form": "저는 진짜 좋아하는 스타일은 블핑 지수",
          "original_form": "저는 진짜 좋아하는 스타일은 블핑 지수",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014097.1.13",
          "form": "ㅋㅋㅋㅋㅋ그 다음이 악뮤 이수현?",
          "original_form": "ㅋㅋㅋㅋㅋ그 다음이 악뮤 이수현?",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014097.1.14",
          "form": "지수 이쁘죠 엄청ㅜ",
          "original_form": "지수 이쁘죠 엄청ㅜ",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014097.1.15",
          "form": "누님 연예인 누구 닮았단 소리 들어봤어요?",
          "original_form": "누님 연예인 누구 닮았단 소리 들어봤어요?",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014097.1.16",
          "form": "음...김고은이여..?",
          "original_form": "음...김고은이여..?",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014097.1.17",
          "form": "닮기보다는 분위기가 비슷해서...?",
          "original_form": "닮기보다는 분위기가 비슷해서...?",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014097.1.18",
          "form": "무쌍이고 하다보니ㅋㅋ",
          "original_form": "무쌍이고 하다보니ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014097.1.19",
          "form": "name1은 김리......?",
          "original_form": "&name1&은 김리......?",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014097.1.20",
          "form": "전형적으로 주지훈 좋아할 상이시네요",
          "original_form": "전형적으로 주지훈 좋아할 상이시네요",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014097.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ왜죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜죠",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014097.1.22",
          "form": "관상이 그런 상입니다",
          "original_form": "관상이 그런 상입니다",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014097.1.23",
          "form": "name1은 누구닮았다고 들었나여",
          "original_form": "&name1&은 누구닮았다고 들었나여",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.24",
          "form": "저는 제가 변요한 스타일이라고 생각하는데",
          "original_form": "저는 제가 변요한 스타일이라고 생각하는데",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.25",
          "form": "여자친구가 고창석이래요",
          "original_form": "여자친구가 고창석이래요",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.26",
          "form": "아하 김리...고창석....",
          "original_form": "아하 김리...고창석....",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.27",
          "form": "혹시...",
          "original_form": "혹시...",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.28",
          "form": "변요한이라고 불러주세요",
          "original_form": "변요한이라고 불러주세요",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.29",
          "form": "요환인가",
          "original_form": "요환인가",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.30",
          "form": "양궁 오진혁선수도 닮았다고 들었나요...",
          "original_form": "양궁 오진혁선수도 닮았다고 들었나요...",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014097.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014097.1.32",
          "form": "변요한이에여",
          "original_form": "변요한이에여",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014097.1.33",
          "form": "너무하시네 진짜",
          "original_form": "너무하시네 진짜",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014097.1.34",
          "form": "변요한닮았습니다",
          "original_form": "변요한닮았습니다",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014097.1.35",
          "form": "ㅋㅋㅋㅋㅋ니예니예...",
          "original_form": "ㅋㅋㅋㅋㅋ니예니예...",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014097.1.36",
          "form": "저 근데 진짜 곰상은 아니고",
          "original_form": "저 근데 진짜 곰상은 아니고",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014097.1.37",
          "form": "살찌기전엔 임슬옹 닮았댔어요",
          "original_form": "살찌기전엔 임슬옹 닮았댔어요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014097.1.38",
          "form": "아 뭔지 알 거같아요",
          "original_form": "아 뭔지 알 거같아요",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014097.1.39",
          "form": "임슬옹이 살찌면 고창석이 되는건가...",
          "original_form": "임슬옹이 살찌면 고창석이 되는건가...",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014097.1.40",
          "form": "쒸익쒸익",
          "original_form": "쒸익쒸익",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014097.1.41",
          "form": "변요한입니다",
          "original_form": "변요한입니다",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014097.1.42",
          "form": "갑자기 이진욱도 생각나네요",
          "original_form": "갑자기 이진욱도 생각나네요",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014097.1.43",
          "form": "이진욱도 참 잘생겼는데",
          "original_form": "이진욱도 참 잘생겼는데",
          "speaker_id": "2",
          "time": "20210825 17:48"
        }
      ]
    }
