        {
          "id": "MDRW2100035406.1.1",
          "form": "벌써 추석 선물세트 광고 한다 정관장",
          "original_form": "벌써 추석 선물세트 광고 한다 정관장",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035406.1.2",
          "form": "할때 됐지 팔아야 되니깐",
          "original_form": "할때 됐지 팔아야 되니깐",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035406.1.3",
          "form": "벌써 추석이다 이번 추석명절영화는 뭐해줄지",
          "original_form": "벌써 추석이다 이번 추석명절영화는 뭐해줄지",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035406.1.4",
          "form": "한다 빌리엘리어트",
          "original_form": "한다 빌리엘리어트",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035406.1.5",
          "form": "나 얘기하고 있다",
          "original_form": "나 얘기하고 있다",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100035406.1.6",
          "form": "틀었다ㅋ",
          "original_form": "틀었다ㅋ",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100035406.1.7",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100035406.1.8",
          "form": "선생님 제자집에 방문한건가",
          "original_form": "선생님 제자집에 방문한건가",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035406.1.9",
          "form": "오디션에 못갔다 형때문에",
          "original_form": "오디션에 못갔다 형때문에",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100035406.1.10",
          "form": "아 그거 얘기하네",
          "original_form": "아 그거 얘기하네",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100035406.1.11",
          "form": "엉 집안사정 얘기하네",
          "original_form": "엉 집안사정 얘기하네",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100035406.1.12",
          "form": "확고하네 신념이",
          "original_form": "확고하네 신념이",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035406.1.13",
          "form": "그러게 해봐라고 하거든 진짜 한다",
          "original_form": "그러게 해봐라고 하거든 진짜 한다",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035406.1.14",
          "form": "그것도 식탁위에서",
          "original_form": "그것도 식탁위에서",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100035406.1.15",
          "form": "아직은 안추네",
          "original_form": "아직은 안추네",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100035406.1.16",
          "form": "응 곧 출꺼다",
          "original_form": "응 곧 출꺼다",
          "speaker_id": "2",
          "time": "20210829 16:30"
        }
      ]
