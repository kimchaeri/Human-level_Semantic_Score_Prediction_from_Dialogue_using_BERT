        },
        {
          "id": "MDRW2100046581.1.5",
          "form": "name1 백신 다 맞았음?",
          "original_form": "&name1& 백신 다 맞았음?",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046581.1.6",
          "form": "네 2차 다 맞았습니다",
          "original_form": "네 2차 다 맞았습니다",
          "speaker_id": "1",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046581.1.7",
          "form": "name2는 백신 맞았음?",
          "original_form": "&name2&는 백신 맞았음?",
          "speaker_id": "1",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046581.1.8",
          "form": "name2는 2차 아직임 :)",
          "original_form": "&name2&는 2차 아직임 :)",
          "speaker_id": "2",
          "time": "20211102 00:11"
        },
        {
          "id": "MDRW2100046581.1.9",
          "form": "2차 많이 아프다던데 사실임? ",
          "original_form": "2차 많이 아프다던데 사실임? ",
          "speaker_id": "2",
          "time": "20211102 00:11"
        },
        {
          "id": "MDRW2100046581.1.10",
          "form": "난 1차가 더 아팠고 2차는 조금 덜 아팠음",
          "original_form": "난 1차가 더 아팠고 2차는 조금 덜 아팠음",
          "speaker_id": "1",
          "time": "20211102 00:11"
        },
        {
          "id": "MDRW2100046581.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046581.1.12",
          "form": "1차 맞은 뒤에 더 건강해진건가 ",
          "original_form": "1차 맞은 뒤에 더 건강해진건가 ",
          "speaker_id": "2",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046581.1.13",
          "form": "난 2차 아직인데 아프다고해서 무서워서 ",
          "original_form": "난 2차 아직인데 아프다고해서 무서워서 ",
          "speaker_id": "2",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046581.1.14",
          "form": "사람마다 다 다른 캐바캐 같음",
          "original_form": "사람마다 다 다른 캐바캐 같음",
          "speaker_id": "1",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046581.1.15",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "2",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046581.1.16",
          "form": "부스터샷도 맞을 계획 있음?",
          "original_form": "부스터샷도 맞을 계획 있음?",
          "speaker_id": "2",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046581.1.17",
          "form": "케바케가 맞는 표준어 인가?",
          "original_form": "케바케가 맞는 표준어 인가?",
          "speaker_id": "1",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046581.1.18",
          "form": "외래어 표기법상 '케'가 맞긴 할거야",
          "original_form": "외래어 표기법상 '케'가 맞긴 할거야",
          "speaker_id": "2",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046581.1.19",
          "form": "부스터샷 아직까지는 생각 안해봤어",
          "original_form": "부스터샷 아직까지는 생각 안해봤어",
          "speaker_id": "1",
          "time": "20211102 00:13"
        },
        {
          "id": "MDRW2100046581.1.20",
          "form": "그렇군 맞춤법이 어렵네",
          "original_form": "그렇군 맞춤법이 어렵네",
          "speaker_id": "1",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046581.1.21",
          "form": "부스터샷 같은 걱정 없이 얼른 치료제라도 나왔으면 좋겠다ㅠㅠ ",
          "original_form": "부스터샷 같은 걱정 없이 얼른 치료제라도 나왔으면 좋겠다ㅠㅠ ",
          "speaker_id": "2",
          "time": "20211102 00:16"
        },
