      "utterance": [
        {
          "id": "MDRW2100022985.1.1",
          "form": "저녁 안 먹어유?",
          "original_form": "저녁 안 먹어유?",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022985.1.2",
          "form": "먹었습니다유 ㅎㅎㅎ",
          "original_form": "먹었습니다유 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022985.1.3",
          "form": "어느세 무엇을 어떻게 얼마나 드셧나요?",
          "original_form": "어느세 무엇을 어떻게 얼마나 드셧나요?",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022985.1.4",
          "form": "하나씩 말씀드리죠 ㅎㅎ",
          "original_form": "하나씩 말씀드리죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022985.1.5",
          "form": "7시에 !",
          "original_form": "7시에 !",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022985.1.6",
          "form": "과일 많이 먹었습니다 ㅋㅋ",
          "original_form": "과일 많이 먹었습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100022985.1.7",
          "form": "띵동띵동.. 심야 과일시식 시간입니다.. ㅋㅋ",
          "original_form": "띵동띵동.. 심야 과일시식 시간입니다.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100022985.1.8",
          "form": "아니 저녁 드셨어요 ㅎㅎ ?",
          "original_form": "아니 저녁 드셨어요 ㅎㅎ ?",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100022985.1.9",
          "form": "운동하고 지금 먹을려고요.. ㅋㅋㅋ",
          "original_form": "운동하고 지금 먹을려고요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022985.1.10",
          "form": "샐러드랑 콜라. ㅋㅋ",
          "original_form": "샐러드랑 콜라. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022985.1.11",
          "form": "엥 다이어트 하시는군요?!",
          "original_form": "엥 다이어트 하시는군요?!",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100022985.1.12",
          "form": "아..이게 평소 식단이에여.. ㅋㅋㅋ",
          "original_form": "아..이게 평소 식단이에여.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100022985.1.13",
          "form": "저녁은 항상 가볍게.. 치킨이나 떡볶이랑..  샐러드.. ? ㅋㅋ",
          "original_form": "저녁은 항상 가볍게.. 치킨이나 떡볶이랑..  샐러드.. ? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100022985.1.14",
          "form": "아니 ㅋㅋ 치킨 떡볶이 뭐에요",
          "original_form": "아니 ㅋㅋ 치킨 떡볶이 뭐에요",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022985.1.15",
          "form": "아 탄단지 노리셨구나 ㅎㅎ",
          "original_form": "아 탄단지 노리셨구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100022985.1.16",
          "form": "ㅋㅋㅋ 평소 영양생각해서 드시나요?",
          "original_form": "ㅋㅋㅋ 평소 영양생각해서 드시나요?",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022985.1.17",
          "form": "전 탄수화물쟁입니다만",
          "original_form": "전 탄수화물쟁입니다만",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022985.1.18",
          "form": "챙기려고 노력은 합니디ㆍ..",
          "original_form": "챙기려고 노력은 합니디ㆍ..",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100022985.1.19",
          "form": "단백질이 항상 부족하거든요",
          "original_form": "단백질이 항상 부족하거든요",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100022985.1.20",
          "form": "치킨이나 삼겹살 탕수육 드심 단백질 해결되는데.. ㅋㅋㅋ",
          "original_form": "치킨이나 삼겹살 탕수육 드심 단백질 해결되는데.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100022985.1.21",
          "form": "그 혹시 .. 탄수화물도 같이 채우는 마법의 식단 아닙니까요?",
          "original_form": "그 혹시 .. 탄수화물도 같이 채우는 마법의 식단 아닙니까요?",
