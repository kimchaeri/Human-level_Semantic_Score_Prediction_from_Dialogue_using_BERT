      "utterance": [
        {
          "id": "MDRW2100008883.1.1",
          "form": "안녕하세요! 저녁 드셨어요?",
          "original_form": "안녕하세요! 저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008883.1.2",
          "form": "아직이요! 저녁 드셨어요?",
          "original_form": "아직이요! 저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008883.1.3",
          "form": "네 저는 먹었어요!! 배고프지 않으세요?",
          "original_form": "네 저는 먹었어요!! 배고프지 않으세요?",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008883.1.4",
          "form": "아까 점심을 너무 많이 먹었나봐요",
          "original_form": "아까 점심을 너무 많이 먹었나봐요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008883.1.5",
          "form": "저도 점심 많이 먹어서 저녁 간단히 먹었어요",
          "original_form": "저도 점심 많이 먹어서 저녁 간단히 먹었어요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008883.1.6",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008883.1.7",
          "form": "늦어서 죄송해요. 점심은 김밥이요",
          "original_form": "늦어서 죄송해요. 점심은 김밥이요",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100008883.1.8",
          "form": "저녁은 뭐드실거에요?",
          "original_form": "저녁은 뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100008883.1.9",
          "form": "저 아직 못 정했어요ㅜㅠ",
          "original_form": "저 아직 못 정했어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100008883.1.10",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100008883.1.11",
          "form": "네 저는 오징어덮밥이요 ㅎㅎ",
          "original_form": "네 저는 오징어덮밥이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100008883.1.12",
          "form": "간만에 먹으니까 맛있네용",
          "original_form": "간만에 먹으니까 맛있네용",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100008883.1.13",
          "form": "오 맛있었을 거 같아요ㅜㅠ 사드셨어요?",
          "original_form": "오 맛있었을 거 같아요ㅜㅠ 사드셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100008883.1.14",
          "form": "아뇨. 오늘 엄마가 해주셔가지구...ㅎㅎㅎㅎ",
          "original_form": "아뇨. 오늘 엄마가 해주셔가지구...ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100008883.1.15",
          "form": "숟가락만 얹었습니다.",
          "original_form": "숟가락만 얹었습니다.",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100008883.1.16",
          "form": "얼른 저녁 드셔야죵",
          "original_form": "얼른 저녁 드셔야죵",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100008883.1.17",
          "form": "헉 넘 부럽습니다ㅠㅜㅠ",
          "original_form": "헉 넘 부럽습니다ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100008883.1.18",
          "form": "안그래도 고민하고 있어요 뭐 먹을지ㅎㅎ",
          "original_form": "안그래도 고민하고 있어요 뭐 먹을지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100008883.1.19",
          "form": "더 늦기전에 드세요 ㅠㅠ",
          "original_form": "더 늦기전에 드세요 ㅠㅠ",
