      "utterance": [
        {
          "id": "MDRW2100007816.1.1",
          "form": "그나마 학교 생활이 쉬워보이네요",
          "original_form": "그나마 학교 생활이 쉬워보이네요",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.2",
          "form": "좋네요~~~",
          "original_form": "좋네요~~~",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.3",
          "form": "name1님 지금 대학 다니고 계시조...",
          "original_form": "&name1&님 지금 대학 다니고 계시조...",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.4",
          "form": "넵ㅠ",
          "original_form": "넵ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.5",
          "form": "지금 코로나 때문에",
          "original_form": "지금 코로나 때문에",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.6",
          "form": "개강해도 전부 비대면이라네요",
          "original_form": "개강해도 전부 비대면이라네요",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.7",
          "form": "name2님은 3분의2 등교시죠?",
          "original_form": "&name2&님은 3분의2 등교시죠?",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007816.1.8",
          "form": "헐 그건 좀 부럽다구요~~~~ 그래서... 넵... (^^)",
          "original_form": "헐 그건 좀 부럽다구요~~~~ 그래서... 넵... (^^)",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007816.1.9",
          "form": "그런데 또 9월부터는 걍 전면등교 한대는데... 제발!!!",
          "original_form": "그런데 또 9월부터는 걍 전면등교 한대는데... 제발!!!",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007816.1.10",
          "form": "전면등교라니...",
          "original_form": "전면등교라니...",
          "speaker_id": "1",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007816.1.11",
          "form": "마스크를 다 잘 끼고 다니면 몰라요",
          "original_form": "마스크를 다 잘 끼고 다니면 몰라요",
          "speaker_id": "1",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007816.1.12",
          "form": "코로나 걸려도 나오라고 할 것 같애요;ㅠ",
          "original_form": "코로나 걸려도 나오라고 할 것 같애요;ㅠ",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007816.1.13",
          "form": "헐ㅠ",
          "original_form": "헐ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.14",
          "form": "그건 진짜 선넘죠",
          "original_form": "그건 진짜 선넘죠",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.15",
          "form": "특히 양아치스런 애들이 쉬는시간마다 마스크를... 절대 안 쓰고 있으니 말 다 했어용",
          "original_form": "특히 양아치스런 애들이 쉬는시간마다 마스크를... 절대 안 쓰고 있으니 말 다 했어용",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.17",
          "form": "저도 그거 때문에 고3 내내",
          "original_form": "저도 그거 때문에 고3 내내",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.18",
          "form": "쌈닭으로 살았는데",
          "original_form": "쌈닭으로 살았는데",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.19",
          "form": "아이고..............",
          "original_form": "아이고..............",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.20",
          "form": "아니 근데 쌈닭이요???ㅠㅠㅜㅠㅠ 아 뻘하게 웃기다",
          "original_form": "아니 근데 쌈닭이요???ㅠㅠㅜㅠㅠ 아 뻘하게 웃기다",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007816.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007816.1.22",
          "form": "안쓰고 다니는 애들한테",
          "original_form": "안쓰고 다니는 애들한테",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007816.1.23",
          "form": "자주 눈을 야려서...ㅋㅋ",
          "original_form": "자주 눈을 야려서...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007816.1.24",
          "form": "정의의 사도가 되셨군요......... 머쪄",
          "original_form": "정의의 사도가 되셨군요......... 머쪄",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007816.1.25",
          "form": "마지막 1년이라서 가능했지요...",
          "original_form": "마지막 1년이라서 가능했지요...",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007816.1.26",
          "form": "고등학겨 되니까 그냥... 마스크 안 써도 뭐라 안 하고... 다들 이상하고...",
          "original_form": "고등학겨 되니까 그냥... 마스크 안 써도 뭐라 안 하고... 다들 이상하고...",
          "speaker_id": "2",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.27",
          "form": "좋은 방법은 아닙니다",
          "original_form": "좋은 방법은 아닙니다",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.28",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.29",
          "form": "오히려 고등학교 가서",
          "original_form": "오히려 고등학교 가서",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.30",
          "form": "아 고3들은 학교 매일........갔죠 그건 좀 양심이 찔리지만...",
          "original_form": "아 고3들은 학교 매일........갔죠 그건 좀 양심이 찔리지만...",
          "speaker_id": "2",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.31",
          "form": "애들을 방목하는 느낑",
          "original_form": "애들을 방목하는 느낑",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.32",
          "form": "솔직히 고3은 등교안해도 되는데",
          "original_form": "솔직히 고3은 등교안해도 되는데",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.33",
          "form": "스카나 독서실 보내고",
          "original_form": "스카나 독서실 보내고",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007816.1.34",
          "form": "ㅋ.ㅋ........... 에휴.. 학교를 꼭 나오는 게 그렇게 중요할까요?",
          "original_form": "ㅋ.ㅋ........... 에휴.. 학교를 꼭 나오는 게 그렇게 중요할까요?",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007816.1.35",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007816.1.36",
          "form": "다들 공부가 건강보다 중요한가봐요",
          "original_form": "다들 공부가 건강보다 중요한가봐요",
          "speaker_id": "1",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007816.1.37",
          "form": "고등학교 처음 다니니 그냥 넘 힘들구 그래요 ㄷ.ㄷ........ᆞ온라인 없었으면 쓰러졌을덧...",
          "original_form": "고등학교 처음 다니니 그냥 넘 힘들구 그래요 ㄷ.ㄷ........ᆞ온라인 없었으면 쓰러졌을덧...",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007816.1.38",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007816.1.39",
          "form": "name2님 코로나 이후에 입학하신거죠?",
          "original_form": "&name2&님 코로나 이후에 입학하신거죠?",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007816.1.40",
          "form": "그랬죠~~!!!!",
          "original_form": "그랬죠~~!!!!",
          "speaker_id": "2",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007816.1.41",
          "form": "그런데도힘들고죽겠어요~~!!! ㅠㅠ배가부른소리지만. .",
          "original_form": "그런데도힘들고죽겠어요~~!!! ㅠㅠ배가부른소리지만. .",
          "speaker_id": "2",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007816.1.42",
          "form": "고등학교가 힘들긴 하죠",
          "original_form": "고등학교가 힘들긴 하죠",
          "speaker_id": "1",
          "time": "20210822 11:10"
        },
        {
          "id": "MDRW2100007816.1.43",
          "form": "아무리 공부해도 진도 따라가기 힘든데",
          "original_form": "아무리 공부해도 진도 따라가기 힘든데",
          "speaker_id": "1",
          "time": "20210822 11:10"
        },
        {
          "id": "MDRW2100007816.1.44",
          "form": "그 사이에 있는 이벤트도 없어지니",
          "original_form": "그 사이에 있는 이벤트도 없어지니",
          "speaker_id": "1",
          "time": "20210822 11:10"
        },
        {
          "id": "MDRW2100007816.1.45",
          "form": "고생이 많아요ㅠㅜ",
          "original_form": "고생이 많아요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 11:10"
        },
        {
          "id": "MDRW2100007816.1.46",
          "form": "ㅋ.ㅋ....... 아녜요... 고생은 따지고보면 여태 다닌 분들이 더했을 거고...",
