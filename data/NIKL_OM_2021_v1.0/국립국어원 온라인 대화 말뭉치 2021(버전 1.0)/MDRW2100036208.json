      "utterance": [
        {
          "id": "MDRW2100036208.1.1",
          "form": "이러니 반밖에못한겁니다",
          "original_form": "이러니 반밖에못한겁니다",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.2",
          "form": "요즘은 몸 아픈 구석은 없나옷",
          "original_form": "요즘은 몸 아픈 구석은 없나옷",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.3",
          "form": "나약한인간.",
          "original_form": "나약한인간.",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.4",
          "form": "나약한 인간님",
          "original_form": "나약한 인간님",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.5",
          "form": "저는늘아픕니다",
          "original_form": "저는늘아픕니다",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.6",
          "form": "ㄱㅈㅋㅈㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㄱㅈㅋㅈㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.7",
          "form": "그거 참 안타깝군오",
          "original_form": "그거 참 안타깝군오",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.8",
          "form": "어디아픈데없으신가요요새",
          "original_form": "어디아픈데없으신가요요새",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036208.1.9",
          "form": "전 요즘 심장만 아파요",
          "original_form": "전 요즘 심장만 아파요",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.10",
          "form": "나머지는 건강해요",
          "original_form": "나머지는 건강해요",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.11",
          "form": "근데 화이자 맞고 진짜 심근염온건 아닐까",
          "original_form": "근데 화이자 맞고 진짜 심근염온건 아닐까",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.12",
          "form": "사랑의병인가요",
          "original_form": "사랑의병인가요",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.14",
          "form": "내과 가고싶은 생각이 잇어요",
          "original_form": "내과 가고싶은 생각이 잇어요",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.15",
          "form": "진지한거였군요",
          "original_form": "진지한거였군요",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.16",
          "form": "그럼안돼요 병원가봐야해요..",
          "original_form": "그럼안돼요 병원가봐야해요..",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.17",
          "form": "아니 연애 전에도 심장이 좀 아픈거같다는 생각을 햇는데",
          "original_form": "아니 연애 전에도 심장이 좀 아픈거같다는 생각을 햇는데",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100036208.1.18",
          "form": "보기보다 심약한 사람",
          "original_form": "보기보다 심약한 사람",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036208.1.19",
          "form": "저번준가 수업 가던 중에 계단에서",
          "original_form": "저번준가 수업 가던 중에 계단에서",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036208.1.20",
          "form": "어떤 친구가 왁!!!!!하고 놀래켯는데",
          "original_form": "어떤 친구가 왁!!!!!하고 놀래켯는데",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036208.1.21",
          "form": "진짜 심장마비 오는 줄",
          "original_form": "진짜 심장마비 오는 줄",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100036208.1.22",
          "form": "엌..",
          "original_form": "엌..",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.23",
          "form": "안돼이어린자식들아..",
          "original_form": "안돼이어린자식들아..",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.25",
          "form": "우린죽는단ㅁㆍㄹ야",
          "original_form": "우린죽는단ㅁㆍㄹ야",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.26",
          "form": "ㅇㅈㄹ",
          "original_form": "ㅇㅈㄹ",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.27",
          "form": "내가오늘벌때문에난리쳣는데",
          "original_form": "내가오늘벌때문에난리쳣는데",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.28",
          "form": "심약자들이 살기 힘든 세상 ㅌㅋㅋㅋㅋㅋ",
          "original_form": "심약자들이 살기 힘든 세상 ㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100036208.1.29",
          "form": "아줌마가놀래키려다가 진짜죽을꺼봐말았대요",
          "original_form": "아줌마가놀래키려다가 진짜죽을꺼봐말았대요",
          "speaker_id": "2",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100036208.1.30",
          "form": "ㅋㄱㅈㅋㅈㄱㅈㅋㅈㄱㅈㅋㅈㄱ",
          "original_form": "ㅋㄱㅈㅋㅈㄱㅈㅋㅈㄱㅈㅋㅈㄱ",
