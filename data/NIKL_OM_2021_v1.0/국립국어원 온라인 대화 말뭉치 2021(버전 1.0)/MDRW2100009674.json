      "utterance": [
        {
          "id": "MDRW2100009674.1.1",
          "form": "오 이상형이 어케되세요?!",
          "original_form": "오 이상형이 어케되세요?!",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009674.1.2",
          "form": "저는 다정한 사람이 좋아요",
          "original_form": "저는 다정한 사람이 좋아요",
          "speaker_id": "1",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009674.1.3",
          "form": "님은 이상형이 어떻게 되세여?",
          "original_form": "님은 이상형이 어떻게 되세여?",
          "speaker_id": "1",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009674.1.4",
          "form": "저는 귀여운 사람이 이상형이에요",
          "original_form": "저는 귀여운 사람이 이상형이에요",
          "speaker_id": "2",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009674.1.5",
          "form": "그렇다면",
          "original_form": "그렇다면",
          "speaker_id": "2",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009674.1.6",
          "form": "연상 vs 연하",
          "original_form": "연상 vs 연하",
          "speaker_id": "2",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009674.1.7",
          "form": "연하! 연하가 좋아요",
          "original_form": "연하! 연하가 좋아요",
          "speaker_id": "1",
          "time": "20210823 03:00"
        },
        {
          "id": "MDRW2100009674.1.8",
          "form": "님은 연상이 좋아요 연하가 좋아요!",
          "original_form": "님은 연상이 좋아요 연하가 좋아요!",
          "speaker_id": "1",
          "time": "20210823 03:00"
        },
        {
          "id": "MDRW2100009674.1.9",
          "form": "쓰읍 저는 연상",
          "original_form": "쓰읍 저는 연상",
          "speaker_id": "2",
          "time": "20210823 03:00"
        },
        {
          "id": "MDRW2100009674.1.10",
          "form": "그러면 행동으로 다정하기 vs 말로 다정하기",
          "original_form": "그러면 행동으로 다정하기 vs 말로 다정하기",
          "speaker_id": "2",
          "time": "20210823 03:00"
        },
        {
          "id": "MDRW2100009674.1.11",
          "form": "행동으로 다정하기!",
          "original_form": "행동으로 다정하기!",
          "speaker_id": "1",
          "time": "20210823 03:01"
        },
        {
          "id": "MDRW2100009674.1.12",
          "form": "님도 행동으로 다정한거 좋아할거 같아요",
          "original_form": "님도 행동으로 다정한거 좋아할거 같아요",
          "speaker_id": "1",
          "time": "20210823 03:01"
        },
        {
          "id": "MDRW2100009674.1.13",
          "form": "헉 어케아셨지",
          "original_form": "헉 어케아셨지",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009674.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009674.1.15",
          "form": "아니 말은 다할수있잖아요",
          "original_form": "아니 말은 다할수있잖아요",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009674.1.16",
          "form": "그러면 또 뭐가 있을까 밸런스게임",
          "original_form": "그러면 또 뭐가 있을까 밸런스게임",
          "speaker_id": "2",
          "time": "20210823 03:05"
        },
        {
          "id": "MDRW2100009674.1.17",
          "form": "여사친 많지만 연락 꼬박꼬박 하는 사람 vs 여사친 없는데 연락 두절 잦은 사람",
          "original_form": "여사친 많지만 연락 꼬박꼬박 하는 사람 vs 여사친 없는데 연락 두절 잦은 사람",
          "speaker_id": "2",
          "time": "20210823 03:06"
        },
        {
          "id": "MDRW2100009674.1.18",
          "form": "전 차라리 전자가 나아요",
          "original_form": "전 차라리 전자가 나아요",
          "speaker_id": "1",
          "time": "20210823 03:23"
        },
        {
          "id": "MDRW2100009674.1.19",
          "form": "친구한테 물어보니까 친구는 후자!",
          "original_form": "친구한테 물어보니까 친구는 후자!",
          "speaker_id": "1",
          "time": "20210823 03:23"
        },
        {
          "id": "MDRW2100009674.1.20",
          "form": "님은 어때요?",
          "original_form": "님은 어때요?",
          "speaker_id": "1",
          "time": "20210823 03:23"
        },
        {
          "id": "MDRW2100009674.1.21",
          "form": "헐 저도 전자요",
          "original_form": "헐 저도 전자요",
          "speaker_id": "2",
          "time": "20210823 03:24"
        },
        {
          "id": "MDRW2100009674.1.22",
          "form": "엇 렉걸리셨근욬ㅋㅋㅋㅋ",
          "original_form": "엇 렉걸리셨근욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 03:24"
        },
        {
          "id": "MDRW2100009674.1.23",
          "form": "그러면 혹시 살면서 다정하다고 느꼈던 행동 있으신가요?",
          "original_form": "그러면 혹시 살면서 다정하다고 느꼈던 행동 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 03:27"
        },
        {
          "id": "MDRW2100009674.1.24",
          "form": "저는 밥 잘 챙겨주고 아플때 걱정해주면 다정하다고 느껴요",
          "original_form": "저는 밥 잘 챙겨주고 아플때 걱정해주면 다정하다고 느껴요",
          "speaker_id": "1",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009674.1.25",
          "form": "님은 언제 다른사람이 다정하다고 여겨요?",
          "original_form": "님은 언제 다른사람이 다정하다고 여겨요?",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009674.1.26",
          "form": "저는 뭔가 제가 말한거 기억했다가 추천할 때 다정한거 느끼는 것 같아요!!",
          "original_form": "저는 뭔가 제가 말한거 기억했다가 추천할 때 다정한거 느끼는 것 같아요!!",
          "speaker_id": "2",
          "time": "20210823 13:07"
        },
        {
          "id": "MDRW2100009674.1.27",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100009674.1.28",
          "form": "내가 못먹는거 기억할때! 다정하게 느껴져요",
          "original_form": "내가 못먹는거 기억할때! 다정하게 느껴져요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100009674.1.29",
          "form": "그리고 소개팅이나 데이트 할 때 메뉴 내가 싫어하지 않는 것으로 고르게 물어보는거요~",
          "original_form": "그리고 소개팅이나 데이트 할 때 메뉴 내가 싫어하지 않는 것으로 고르게 물어보는거요~",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100009674.1.30",
          "form": "헐 맞아요 싫어하는거 물어보는 거에서 다정함이 느껴지죠 맞아요",
          "original_form": "헐 맞아요 싫어하는거 물어보는 거에서 다정함이 느껴지죠 맞아요",
          "speaker_id": "2",
          "time": "20210907 12:12"
        }
      ]
