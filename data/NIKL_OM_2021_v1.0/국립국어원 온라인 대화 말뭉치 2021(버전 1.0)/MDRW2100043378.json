        },
        {
          "id": "MDRW2100043378.1.2",
          "form": "맞아욮ㅋㅋㅋㅋㅋ",
          "original_form": "맞아욮ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043378.1.3",
          "form": "떨어진 인류애 올려주시는 타입ㅋㅋㅋ",
          "original_form": "떨어진 인류애 올려주시는 타입ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043378.1.4",
          "form": "그리고 블로그에 이웃해듀오",
          "original_form": "그리고 블로그에 이웃해듀오",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043378.1.5",
          "form": "올리시면서 막 다들 칭구친구 만들어주고",
          "original_form": "올리시면서 막 다들 칭구친구 만들어주고",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043378.1.6",
          "form": "블로그 열심히 하시는것도 넘나 대단해요",
          "original_form": "블로그 열심히 하시는것도 넘나 대단해요",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043378.1.7",
          "form": "칭구친구ㅋㅋ",
          "original_form": "칭구친구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043378.1.8",
          "form": "맞아요ㅋㅋ 모든 sns에 푸린 매일 올라오는것도 귀여워요",
          "original_form": "맞아요ㅋㅋ 모든 sns에 푸린 매일 올라오는것도 귀여워요",
          "speaker_id": "1",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043378.1.9",
          "form": "ㅋㅋㅋㅋ맞아요 푸린이랑 와들디 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아요 푸린이랑 와들디 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.10",
          "form": "넘귀엽",
          "original_form": "넘귀엽",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.11",
          "form": "어딜가나 와들디 함께인거 너무웃겨욬ㅋㅋㅋㅋ",
          "original_form": "어딜가나 와들디 함께인거 너무웃겨욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.12",
          "form": "(근데 사실...저 와들디가 어디에 나오는 캐릭터인지 모른답니다..ㅎ",
          "original_form": "(근데 사실...저 와들디가 어디에 나오는 캐릭터인지 모른답니다..ㅎ",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.13",
          "form": "어 그러고보니",
          "original_form": "어 그러고보니",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.14",
          "form": "저도몰라요(?)",
          "original_form": "저도몰라요(?)",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.15",
          "form": "어디서 많이 봤는데",
          "original_form": "어디서 많이 봤는데",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.16",
          "form": "푸린은 포켓몬이고",
          "original_form": "푸린은 포켓몬이고",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043378.1.17",
          "form": "그쵸 푸린은 푸~ 푸루루루루루루``~~",
          "original_form": "그쵸 푸린은 푸~ 푸루루루루루루``~~",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043378.1.18",
          "form": "ㅋㅋㅋㅋ푸린이 잠들게하는 친구였나요?",
          "original_form": "ㅋㅋㅋㅋ푸린이 잠들게하는 친구였나요?",
