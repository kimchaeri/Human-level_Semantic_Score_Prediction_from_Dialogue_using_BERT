      "utterance": [
        {
          "id": "MDRW2100013490.1.1",
          "form": "다시 육출",
          "original_form": "다시 육출",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013490.1.2",
          "form": "퇴근후 육출",
          "original_form": "퇴근후 육출",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013490.1.3",
          "form": "오늘 저녁은 뭐 해 먹니",
          "original_form": "오늘 저녁은 뭐 해 먹니",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100013490.1.4",
          "form": "집안일은 다 했나",
          "original_form": "집안일은 다 했나",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100013490.1.5",
          "form": "응 언니 이제 집에 도착했다. 아까전에 시댁다녀옴.",
          "original_form": "응 언니 이제 집에 도착했다. 아까전에 시댁다녀옴.",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100013490.1.6",
          "form": "첫째 데리고 왔다.",
          "original_form": "첫째 데리고 왔다.",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100013490.1.7",
          "form": "첫째 좀 어때??",
          "original_form": "첫째 좀 어때??",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100013490.1.8",
          "form": "가라앉았나??",
          "original_form": "가라앉았나??",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100013490.1.9",
          "form": "아니 언니 아직 빨갛다",
          "original_form": "아니 언니 아직 빨갛다",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100013490.1.10",
          "form": "알러지 맞는것같아.",
          "original_form": "알러지 맞는것같아.",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100013490.1.11",
          "form": "내일 일단은 안과 한번 데리고 가게.",
          "original_form": "내일 일단은 안과 한번 데리고 가게.",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100013490.1.12",
          "form": "학교 들어가기전에 안과에 가고싶은데 등교시간이 있으니 난감하네. ㅠㅠ",
          "original_form": "학교 들어가기전에 안과에 가고싶은데 등교시간이 있으니 난감하네. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100013490.1.13",
          "form": "그렇네 내일 등교 전까지 가라앉으면 좋은데",
          "original_form": "그렇네 내일 등교 전까지 가라앉으면 좋은데",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100013490.1.14",
          "form": "뭐 잘못 먹었나 보다",
          "original_form": "뭐 잘못 먹었나 보다",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100013490.1.15",
          "form": "간지럽다고는 안하나??",
          "original_form": "간지럽다고는 안하나??",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100013490.1.16",
          "form": "살짝 간지럽대",
          "original_form": "살짝 간지럽대",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.17",
          "form": "내일 일단 학교 갔다가",
          "original_form": "내일 일단 학교 갔다가",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.18",
          "form": "눈꼽끼는건 없어서",
          "original_form": "눈꼽끼는건 없어서",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.19",
          "form": "돌봄 안 하고 바로 와야겠다",
          "original_form": "돌봄 안 하고 바로 와야겠다",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.20",
          "form": "눈병은 아닌것 같은데",
          "original_form": "눈병은 아닌것 같은데",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.21",
          "form": "그래 눈 주위가 빨간 거 보니까",
          "original_form": "그래 눈 주위가 빨간 거 보니까",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.22",
          "form": "학교 먼저 가야되나?ㅠ ㅋㅋ",
          "original_form": "학교 먼저 가야되나?ㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.23",
          "form": "알러지가 맞는 듯",
          "original_form": "알러지가 맞는 듯",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100013490.1.24",
          "form": "개학이니 학교를 안 갈 수는 없고",
          "original_form": "개학이니 학교를 안 갈 수는 없고",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100013490.1.25",
          "form": "그런가보다..ㅠㅠ",
          "original_form": "그런가보다..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100013490.1.26",
          "form": "일단 아침에 일어나서",
          "original_form": "일단 아침에 일어나서",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100013490.1.27",
          "form": "계속 빨가면 병원부터 가고",
          "original_form": "계속 빨가면 병원부터 가고",
          "speaker_id": "2",
          "time": "20210825 20:52"
        }
      ]
    }
  ]
