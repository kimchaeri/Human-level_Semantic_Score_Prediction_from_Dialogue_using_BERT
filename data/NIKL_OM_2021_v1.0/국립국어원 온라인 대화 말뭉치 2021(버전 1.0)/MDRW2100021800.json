      "utterance": [
        {
          "id": "MDRW2100021800.1.1",
          "form": "그러게요.. yg는 얼마나 더럽게 장사한건지..",
          "original_form": "그러게요.. yg는 얼마나 더럽게 장사한건지..",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021800.1.2",
          "form": "근데 블핑이 넘사기는 하죠..",
          "original_form": "근데 블핑이 넘사기는 하죠..",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021800.1.3",
          "form": "맞아요 블핑때문에 아직 잘나가죠 ㅋㅋㅋ",
          "original_form": "맞아요 블핑때문에 아직 잘나가죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021800.1.4",
          "form": "아직 삼대기획사하면 dm yg 는 들어가니까 ,,",
          "original_form": "아직 삼대기획사하면 dm yg 는 들어가니까 ,,",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021800.1.5",
          "form": "블핑 뮤비도 보셧어요?",
          "original_form": "블핑 뮤비도 보셧어요?",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021800.1.6",
          "form": "네 봤어요 ! 거의 다 본거같아요 ㅋㅋ",
          "original_form": "네 봤어요 ! 거의 다 본거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021800.1.7",
          "form": "블핑은 라이브는 쏘쏘인데..\n뮤비퀄이 엄청 나던데여..",
          "original_form": "블핑은 라이브는 쏘쏘인데..\n뮤비퀄이 엄청 나던데여..",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021800.1.8",
          "form": "괜히 억단위가 아닌듯..",
          "original_form": "괜히 억단위가 아닌듯..",
          "speaker_id": "2",
          "time": "20210828 23:00"
        },
        {
          "id": "MDRW2100021800.1.9",
          "form": "맞아요 ㅋㅋ 전 솔직히 블핑은 스타성?이 좋은거같아요",
          "original_form": "맞아요 ㅋㅋ 전 솔직히 블핑은 스타성?이 좋은거같아요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021800.1.10",
          "form": "라이브도 춤도 쏘쏘",
          "original_form": "라이브도 춤도 쏘쏘",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021800.1.11",
          "form": "투자도 많이 해주고 잘받아 먹기도 하구요 ㅋㅋ",
          "original_form": "투자도 많이 해주고 잘받아 먹기도 하구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021800.1.12",
          "form": "그.. 비쥬얼이 압도적인거 같아요.. \n구멍 멤버도 없고..",
          "original_form": "그.. 비쥬얼이 압도적인거 같아요.. \n구멍 멤버도 없고..",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021800.1.13",
          "form": "맞아요 진짜 에이스 4명인 기분 ㅋㅋ",
          "original_form": "맞아요 진짜 에이스 4명인 기분 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021800.1.14",
          "form": "저 실제로 본적있는데",
          "original_form": "저 실제로 본적있는데",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021800.1.15",
          "form": "리사가 젤 이뻤어요 ㅋㅋㅋ",
          "original_form": "리사가 젤 이뻤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021800.1.16",
          "form": "어디서 보셧어요? 리사가 젤 인기 많잖아요..",
          "original_form": "어디서 보셧어요? 리사가 젤 인기 많잖아요..",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021800.1.17",
          "form": "저 시상식에서 봤었어요 ㅎㅎ",
          "original_form": "저 시상식에서 봤었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021800.1.18",
          "form": "무대하는거 ㅎㅎ",
          "original_form": "무대하는거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100021800.1.19",
          "form": "어느 시상식이요?",
          "original_form": "어느 시상식이요?",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021800.1.20",
          "form": "아마 멜론이였던거같아요 !",
          "original_form": "아마 멜론이였던거같아요 !",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021800.1.21",
          "form": "부럽네여",
          "original_form": "부럽네여",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021800.1.22",
          "form": "솔직히 수진은 진작에 나갔어야되는데 ㅋㅋㅋ 맞죠 ?",
