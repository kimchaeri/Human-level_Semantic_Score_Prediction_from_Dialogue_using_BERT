      "utterance": [
        {
          "id": "MDRW2100040296.1.1",
          "form": "나도 손목 아파",
          "original_form": "나도 손목 아파",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040296.1.2",
          "form": "그래도여름보단 건강해지고있다",
          "original_form": "그래도여름보단 건강해지고있다",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100040296.1.3",
          "form": "아 일열심히해서근가숩  \r\n손목이아파서어째",
          "original_form": "아 일열심히해서근가숩  \r\n손목이아파서어째",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.4",
          "form": "ㅠㅡㅠ",
          "original_form": "ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.5",
          "form": "아까엄마가발목보더니\r\n이모집에부항있다고피빼러가자곶ㄱㅋㄱㅋ",
          "original_form": "아까엄마가발목보더니\r\n이모집에부항있다고피빼러가자곶ㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.6",
          "form": "굴쎄ㅠㅠ 손목 보호대 하고있더",
          "original_form": "굴쎄ㅠㅠ 손목 보호대 하고있더",
          "speaker_id": "2",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.7",
          "form": "구래 부항이라도 해 해원아",
          "original_form": "구래 부항이라도 해 해원아",
          "speaker_id": "2",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.8",
          "form": "내가무섭다고거절했어",
          "original_form": "내가무섭다고거절했어",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.9",
          "form": "ㄱㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㄱㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100040296.1.10",
          "form": "엄마가발목상태안좋아ㆍ보인다하긴하더라",
          "original_form": "엄마가발목상태안좋아ㆍ보인다하긴하더라",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040296.1.11",
          "form": "해원아 제발 ... 나즁에 고생한다",
          "original_form": "해원아 제발 ... 나즁에 고생한다",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040296.1.12",
          "form": "피빼러가래ㅋㅋㄱㅋㅈㄱ빨리",
          "original_form": "피빼러가래ㅋㅋㄱㅋㅈㄱ빨리",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040296.1.13",
          "form": "jnj",
          "original_form": "jnj",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040296.1.14",
          "form": "한번아프면고질적으로아픈데생긴다더니",
          "original_form": "한번아프면고질적으로아픈데생긴다더니",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040296.1.15",
          "form": "약간오른쪽발목이그렇게된거같다",
          "original_form": "약간오른쪽발목이그렇게된거같다",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040296.1.16",
          "form": "아자꾸나쁜말이래!!!",
          "original_form": "아자꾸나쁜말이래!!!",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040296.1.17",
          "form": "그치 짜증나 ㅋㅋ",
          "original_form": "그치 짜증나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040296.1.18",
          "form": "어ㅠㅠ조심해야해 해원아 항상",
          "original_form": "어ㅠㅠ조심해야해 해원아 항상",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040296.1.19",
          "form": "니도손목잘보호해ㅠㅡㅠ일할때계속써야하는데..",
          "original_form": "니도손목잘보호해ㅠㅡㅠ일할때계속써야하는데..",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040296.1.20",
          "form": "버티컬마우스는안쓰지?",
          "original_form": "버티컬마우스는안쓰지?",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040296.1.21",
          "form": "그러게ㅠㅠ 손목 보호대 하나살까",
          "original_form": "그러게ㅠㅠ 손목 보호대 하나살까",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040296.1.22",
          "form": "사실 핸드폰 땨문인거같아",
          "original_form": "사실 핸드폰 땨문인거같아",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040296.1.23",
          "form": "심심이땸에 ㅋㅋㅋ",
          "original_form": "심심이땸에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040296.1.24",
          "form": "ㅋㄱㅋㅋㄱㅈㅋㅈㅋㅈㄱㅈㅋㅈㄱ",
          "original_form": "ㅋㄱㅋㅋㄱㅈㅋㅈㅋㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040296.1.25",
          "form": "돈을벌고 건강을잃는거냐",
          "original_form": "돈을벌고 건강을잃는거냐",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040296.1.26",
          "form": "ㅋㄱㅋㄱㅈㄱㅈㅋㅈㄱ",
          "original_form": "ㅋㄱㅋㄱㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040296.1.27",
          "form": "그러게 너뮤힘들다",
          "original_form": "그러게 너뮤힘들다",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040296.1.28",
          "form": "나 단발 진짜 윳겨 눈물난다",
          "original_form": "나 단발 진짜 윳겨 눈물난다",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040296.1.29",
          "form": "요즘 살빠져서 그거 믿고 까불다가 머리 넘 잘랐어",
          "original_form": "요즘 살빠져서 그거 믿고 까불다가 머리 넘 잘랐어",
          "speaker_id": "2",
