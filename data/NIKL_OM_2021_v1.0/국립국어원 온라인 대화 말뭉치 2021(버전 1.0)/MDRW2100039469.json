      "utterance": [
        {
          "id": "MDRW2100039469.1.1",
          "form": "name2님은 이상형이 어떻게 되세요?",
          "original_form": "&name2&님은 이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039469.1.2",
          "form": "우선 외관적으로는 키 크고 어깨 넓은 사람요",
          "original_form": "우선 외관적으로는 키 크고 어깨 넓은 사람요",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039469.1.3",
          "form": "일단 저보단 커야해요 ㅋㅋㅋㅋㅋ",
          "original_form": "일단 저보단 커야해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039469.1.4",
          "form": "님은 어떻게 되나요?",
          "original_form": "님은 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039469.1.5",
          "form": "와우 저도요 근데 170넘어서 별루 없오요",
          "original_form": "와우 저도요 근데 170넘어서 별루 없오요",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039469.1.6",
          "form": "키큰사람잌ㅋㅋㅋㅋㅋ",
          "original_form": "키큰사람잌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039469.1.7",
          "form": "성격 좋고..",
          "original_form": "성격 좋고..",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039469.1.8",
          "form": "강아지같은 사람...",
          "original_form": "강아지같은 사람...",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039469.1.9",
          "form": "저도 170 넘어서 키 큰 사람 잘 없더라구요",
          "original_form": "저도 170 넘어서 키 큰 사람 잘 없더라구요",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039469.1.10",
          "form": "오 강아지요?",
          "original_form": "오 강아지요?",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039469.1.11",
          "form": "옼ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "옼ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039469.1.12",
          "form": "넹 강쥐같이 귀여운..",
          "original_form": "넹 강쥐같이 귀여운..",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039469.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039469.1.14",
          "form": "전 자기 일 잘하고 공감 능력 있는 사람도 좋네요",
          "original_form": "전 자기 일 잘하고 공감 능력 있는 사람도 좋네요",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039469.1.15",
          "form": "그죠 공감능력 진짜",
          "original_form": "그죠 공감능력 진짜",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039469.1.16",
          "form": "공감능력없으면 호감조차 생기지 않아요",
          "original_form": "공감능력없으면 호감조차 생기지 않아요",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039469.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039469.1.18",
          "form": "애초에 말이 통하지 않을것 같아서요",
          "original_form": "애초에 말이 통하지 않을것 같아서요",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039469.1.19",
          "form": "마자요 애초에 차단하는게 답..",
          "original_form": "마자요 애초에 차단하는게 답..",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039469.1.20",
          "form": "요즘은 공감 능력 없고 냅따 화내거나 비난하는 사람들이 너무 많은것같아요",
          "original_form": "요즘은 공감 능력 없고 냅따 화내거나 비난하는 사람들이 너무 많은것같아요",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039469.1.21",
          "form": "아 그래요? 요즘 사람들이 화를 많이 내는가봐요",
          "original_form": "아 그래요? 요즘 사람들이 화를 많이 내는가봐요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039469.1.22",
          "form": "별거 아닌걸로 그런것같아서요",
          "original_form": "별거 아닌걸로 그런것같아서요",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039469.1.23",
          "form": "아 그러면 같이 예민해지죠 애인이 그러면 더욱..",
          "original_form": "아 그러면 같이 예민해지죠 애인이 그러면 더욱..",
          "speaker_id": "1",
          "time": "20210903 22:32"
        }
      ]
    }
  ]
