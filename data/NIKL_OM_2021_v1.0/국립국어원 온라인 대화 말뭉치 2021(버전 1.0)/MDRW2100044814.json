      "utterance": [
        {
          "id": "MDRW2100044814.1.1",
          "form": "커피좋아하시나요?",
          "original_form": "커피좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044814.1.2",
          "form": "커피 좋긴한데ㅜㅜ요즘 속이 안좋아서 잘 못마셔요ㅠㅜ",
          "original_form": "커피 좋긴한데ㅜㅜ요즘 속이 안좋아서 잘 못마셔요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044814.1.3",
          "form": "그렇군요ㅠ 그럼 프차 카페중에 어딜 가장좋아하나요",
          "original_form": "그렇군요ㅠ 그럼 프차 카페중에 어딜 가장좋아하나요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044814.1.4",
          "form": "저는 개인카페가 더좋아욤 ㅋㅋㅋㅋ 사진찍는걸 좋아하거든요",
          "original_form": "저는 개인카페가 더좋아욤 ㅋㅋㅋㅋ 사진찍는걸 좋아하거든요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044814.1.5",
          "form": "저번주말엔 한옥카페에 갔었어요~",
          "original_form": "저번주말엔 한옥카페에 갔었어요~",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044814.1.6",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044814.1.7",
          "form": "개인카페는 어떻게 찾으세요?",
          "original_form": "개인카페는 어떻게 찾으세요?",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044814.1.8",
          "form": "저는 인스타 보다가 저장해두기도 하고 ㅋㅋㅋ아님 블로그에 검색해용",
          "original_form": "저는 인스타 보다가 저장해두기도 하고 ㅋㅋㅋ아님 블로그에 검색해용",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044814.1.9",
          "form": "오 인스타에 맛집 많이올라오나요?",
          "original_form": "오 인스타에 맛집 많이올라오나요?",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044814.1.10",
          "form": "은근 태그로 검색해도 많고요 ㅋㅋㅋㅋ 인스타 유명인들이 올리는곳도 저장하구욬ㅋㅋㅋ",
          "original_form": "은근 태그로 검색해도 많고요 ㅋㅋㅋㅋ 인스타 유명인들이 올리는곳도 저장하구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044814.1.11",
          "form": "아항 태그는 뭐라고 검색해야해요??",
          "original_form": "아항 태그는 뭐라고 검색해야해요??",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044814.1.12",
          "form": "삼청동에 가면 삼청동카페라고 검색해요...참...쉽죠...? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "삼청동에 가면 삼청동카페라고 검색해요...참...쉽죠...? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044814.1.13",
          "form": "ㅋㅋㅋㅋ맛집 검색 꿀팁이네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맛집 검색 꿀팁이네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044814.1.14",
          "form": "근데 요즘 광고글이 너무 많아서 잘 골라봐여 하긴해요",
          "original_form": "근데 요즘 광고글이 너무 많아서 잘 골라봐여 하긴해요",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044814.1.15",
          "form": "그리고 전혀 상관없는 사진에도 맛집태그 걸어둔 이상한 사람들도 있더라구요",
          "original_form": "그리고 전혀 상관없는 사진에도 맛집태그 걸어둔 이상한 사람들도 있더라구요",
          "speaker_id": "1",
          "time": "20210908 00:12"
        }
      ]
