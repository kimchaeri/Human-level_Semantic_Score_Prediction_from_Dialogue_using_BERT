      "utterance": [
        {
          "id": "MDRW2100012152.1.1",
          "form": "달팽이 엄청 먹어댔지 ㅎㅎ",
          "original_form": "달팽이 엄청 먹어댔지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012152.1.2",
          "form": "요즘도 버리는 야채 보면 가끔 생각 나",
          "original_form": "요즘도 버리는 야채 보면 가끔 생각 나",
          "speaker_id": "1",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012152.1.3",
          "form": "애호박이랑 상추 당근 다 잘먹었는데",
          "original_form": "애호박이랑 상추 당근 다 잘먹었는데",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100012152.1.4",
          "form": "달팽이 먹는 색깔대로 나오는 거 너무 신기했는데",
          "original_form": "달팽이 먹는 색깔대로 나오는 거 너무 신기했는데",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012152.1.5",
          "form": "근데 우리가 왜 키우다 말았지?",
          "original_form": "근데 우리가 왜 키우다 말았지?",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012152.1.6",
          "form": "겨울에 보일러 틀었더니 너무 건조해서 다 말라 죽었잖아 ㅠㅠ",
          "original_form": "겨울에 보일러 틀었더니 너무 건조해서 다 말라 죽었잖아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012152.1.7",
          "form": "아.. 맞다ㅠ",
          "original_form": "아.. 맞다ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:43"
        },
        {
          "id": "MDRW2100012152.1.8",
          "form": "알을 너무 많이 낳아서 깜놀했는데 ㅎㅎ",
          "original_form": "알을 너무 많이 낳아서 깜놀했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012152.1.9",
          "form": "한번에 수십마리씩 낳으니 감당이 안돼",
          "original_form": "한번에 수십마리씩 낳으니 감당이 안돼",
          "speaker_id": "2",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012152.1.10",
          "form": "그래도 귀엽긴 했어",
          "original_form": "그래도 귀엽긴 했어",
          "speaker_id": "1",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100012152.1.11",
          "form": "이렇게 비오는 날 동네 기어다니던 달팽이도 같이 키웠었잖아",
          "original_form": "이렇게 비오는 날 동네 기어다니던 달팽이도 같이 키웠었잖아",
          "speaker_id": "2",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100012152.1.12",
          "form": "맞아 ㅎㅎ 꾸물꾸물 너무 예뻤어",
          "original_form": "맞아 ㅎㅎ 꾸물꾸물 너무 예뻤어",
          "speaker_id": "1",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100012152.1.13",
          "form": "사진 찍어놓은 게 없어서 아쉽다",
          "original_form": "사진 찍어놓은 게 없어서 아쉽다",
