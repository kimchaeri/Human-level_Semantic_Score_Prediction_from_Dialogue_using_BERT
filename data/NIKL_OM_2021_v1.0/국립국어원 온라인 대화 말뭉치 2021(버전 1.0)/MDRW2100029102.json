        },
        {
          "id": "MDRW2100029102.1.2",
          "form": "대구 곱창 맛있음",
          "original_form": "대구 곱창 맛있음",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029102.1.3",
          "form": "배달받기 힘들어서 먹을건 잘 안시킴",
          "original_form": "배달받기 힘들어서 먹을건 잘 안시킴",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029102.1.4",
          "form": "대구곱창인듯",
          "original_form": "대구곱창인듯",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029102.1.5",
          "form": "부산진역에 할매 곱창 엄청 맛있어요",
          "original_form": "부산진역에 할매 곱창 엄청 맛있어요",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029102.1.6",
          "form": "대존맛이었음",
          "original_form": "대존맛이었음",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029102.1.7",
          "form": "곱창 먹으러 가고 싶다",
          "original_form": "곱창 먹으러 가고 싶다",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029102.1.8",
          "form": "몇번만나야해",
          "original_form": "몇번만나야해",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
