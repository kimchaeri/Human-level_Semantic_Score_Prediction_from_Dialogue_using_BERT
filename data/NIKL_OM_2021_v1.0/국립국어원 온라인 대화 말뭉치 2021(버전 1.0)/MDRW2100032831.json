      "utterance": [
        {
          "id": "MDRW2100032831.1.1",
          "form": "닭을 볶는 요리야?",
          "original_form": "닭을 볶는 요리야?",
          "speaker_id": "2",
          "time": "20210906 00:39"
        },
        {
          "id": "MDRW2100032831.1.2",
          "form": "응 닭을 볶는 요리맞아",
          "original_form": "응 닭을 볶는 요리맞아",
          "speaker_id": "1",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100032831.1.3",
          "form": "닭볶음탕 먹었구나! 맞지?",
          "original_form": "닭볶음탕 먹었구나! 맞지?",
          "speaker_id": "2",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100032831.1.4",
          "form": "아니야....닭볶음탕",
          "original_form": "아니야....닭볶음탕",
          "speaker_id": "1",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100032831.1.5",
          "form": "근데 궤는 비슷해",
          "original_form": "근데 궤는 비슷해",
          "speaker_id": "1",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100032831.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이런",
          "speaker_id": "2",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100032831.1.7",
          "form": "닭을 볶는 멕시코요리가 뭐가 있지...",
          "original_form": "닭을 볶는 멕시코요리가 뭐가 있지...",
          "speaker_id": "2",
          "time": "20210906 00:41"
        },
        {
          "id": "MDRW2100032831.1.8",
          "form": "야채 들어가?",
          "original_form": "야채 들어가?",
          "speaker_id": "2",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100032831.1.9",
          "form": "야채를넣는집도 있는데",
          "original_form": "야채를넣는집도 있는데",
          "speaker_id": "1",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100032831.1.10",
          "form": "아 양념 때문에",
          "original_form": "아 양념 때문에",
          "speaker_id": "1",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100032831.1.11",
          "form": "무조건 들어갈지도",
          "original_form": "무조건 들어갈지도",
          "speaker_id": "1",
          "time": "20210906 00:42"
        },
        {
          "id": "MDRW2100032831.1.12",
          "form": "와 진짜 무슨 음식인지 감도 안 잡히네",
          "original_form": "와 진짜 무슨 음식인지 감도 안 잡히네",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032831.1.13",
          "form": "닭갈비?",
          "original_form": "닭갈비?",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032831.1.14",
          "form": "진짜???? 그냥 답 말해줄까?",
          "original_form": "진짜???? 그냥 답 말해줄까?",
          "speaker_id": "1",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032831.1.15",
          "form": "아니 내가 맞춰볼래 ㅋㅋㅋㅌㅌㅌㅌㅋ너의 저녁식사",
          "original_form": "아니 내가 맞춰볼래 ㅋㅋㅋㅌㅌㅌㅌㅋ너의 저녁식사",
          "speaker_id": "2",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032831.1.16",
          "form": "하나의 공정이 더 들어가서 헤매는걸지도",
          "original_form": "하나의 공정이 더 들어가서 헤매는걸지도",
          "speaker_id": "1",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032831.1.17",
          "form": "아 진짜? 그럼 그 또다른 조리법이 뭐니",
          "original_form": "아 진짜? 그럼 그 또다른 조리법이 뭐니",
          "speaker_id": "2",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032831.1.18",
          "form": "튀기지",
          "original_form": "튀기지",
          "speaker_id": "1",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100032831.1.19",
          "form": "츠차차차차차!!!!!!",
          "original_form": "츠차차차차차!!!!!!",
          "speaker_id": "1",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100032831.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ튀김소리를 그렇게 말하는 거 처음 봐서 너무 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ튀김소리를 그렇게 말하는 거 처음 봐서 너무 웃기다",
          "speaker_id": "2",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100032831.1.21",
          "form": "나도 입술 꽉 깨물었음",
          "original_form": "나도 입술 꽉 깨물었음",
          "speaker_id": "1",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100032831.1.22",
          "form": "ㅋzzzzㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋzzzzㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032831.1.23",
          "form": "볶은 다음 튀겨, 아니면 튀긴 다음 볶아?",
          "original_form": "볶은 다음 튀겨, 아니면 튀긴 다음 볶아?",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032831.1.24",
          "form": "튀긴다음 볶을걸",
          "original_form": "튀긴다음 볶을걸",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032831.1.25",
          "form": "정답 매운닭튀김볶음",
          "original_form": "정답 매운닭튀김볶음",
          "speaker_id": "2",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032831.1.26",
          "form": "너무 개미핥기만큼 정직한 요리명이다",
          "original_form": "너무 개미핥기만큼 정직한 요리명이다",
          "speaker_id": "2",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032831.1.27",
          "form": "어때 정답이니?",
          "original_form": "어때 정답이니?",
          "speaker_id": "2",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100032831.1.28",
          "form": "ㅣㄱㄱㅋㄱ기ㄱㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㅣㄱㄱㅋㄱ기ㄱㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100032831.1.29",
          "form": "양념통닭인데요...",
          "original_form": "양념통닭인데요...",
          "speaker_id": "1",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100032831.1.30",
          "form": "아이고 말해버리믄 우짜",
          "original_form": "아이고 말해버리믄 우짜",
