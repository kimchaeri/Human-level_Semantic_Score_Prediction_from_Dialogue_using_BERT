        },
        {
          "id": "MDRW2100020243.1.2",
          "form": "돈까스 좋아하니",
          "original_form": "돈까스 좋아하니",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020243.1.3",
          "form": "돈까스 메일 먹을 수 있어",
          "original_form": "돈까스 메일 먹을 수 있어",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020243.1.4",
          "form": "치즈 돈까스 좋아하니",
          "original_form": "치즈 돈까스 좋아하니",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020243.1.5",
          "form": "너무 좋아하지",
          "original_form": "너무 좋아하지",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020243.1.6",
          "form": "티즈 돈까스는 치즈가 너무 빨리 굳어",
          "original_form": "티즈 돈까스는 치즈가 너무 빨리 굳어",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020243.1.7",
          "form": "맞아 그래서 빨리 먹어야 해",
          "original_form": "맞아 그래서 빨리 먹어야 해",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020243.1.8",
          "form": "돈까스 김치나베 도 맛있어",
          "original_form": "돈까스 김치나베 도 맛있어",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020243.1.9",
          "form": "나는 돈부리 도 좋아해",
          "original_form": "나는 돈부리 도 좋아해",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020243.1.10",
          "form": "돈부리는 계란과 양파와 소스와 돈까스 저기 맛있어",
          "original_form": "돈부리는 계란과 양파와 소스와 돈까스 저기 맛있어",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020243.1.11",
          "form": "눅눅한 거 싫어하는데 이상하게 건 맛있더라",
          "original_form": "눅눅한 거 싫어하는데 이상하게 건 맛있더라",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020243.1.12",
          "form": "얇은 돈까스 도 좋아하니",
          "original_form": "얇은 돈까스 도 좋아하니",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020243.1.13",
          "form": "얇은 여러 겹 돈가스는 좋아",
          "original_form": "얇은 여러 겹 돈가스는 좋아",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020243.1.14",
          "form": "돈까스 에는 깨가 어울려",
          "original_form": "돈까스 에는 깨가 어울려",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020243.1.15",
          "form": "돈까스 소스 찍어 먹는 거 좋아해?",
          "original_form": "돈까스 소스 찍어 먹는 거 좋아해?",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020243.1.16",
          "form": "뿌려먹는것도맛있어",
          "original_form": "뿌려먹는것도맛있어",
          "speaker_id": "1",
          "time": "20210828 00:41"
