      "utterance": [
        {
          "id": "MDRW2100039175.1.1",
          "form": "음식하는 건 좋아하시나요",
          "original_form": "음식하는 건 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 12:45"
        },
        {
          "id": "MDRW2100039175.1.2",
          "form": "먹는것만 좋아합니다ㅜ",
          "original_form": "먹는것만 좋아합니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039175.1.3",
          "form": "요리 잘하시나요",
          "original_form": "요리 잘하시나요",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039175.1.4",
          "form": "하는 것만 잘해요",
          "original_form": "하는 것만 잘해요",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039175.1.5",
          "form": "그런데 김치찌개는 최악입니다",
          "original_form": "그런데 김치찌개는 최악입니다",
          "speaker_id": "2",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100039175.1.6",
          "form": "헉 찌개를 만드시는 게 어디예요",
          "original_form": "헉 찌개를 만드시는 게 어디예요",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039175.1.7",
          "form": "고수시군요",
          "original_form": "고수시군요",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039175.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039175.1.9",
          "form": "찌개가...",
          "original_form": "찌개가...",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039175.1.10",
          "form": "찌개 맛이 나야 찌개 아닐까욥..ㅠ..",
          "original_form": "찌개 맛이 나야 찌개 아닐까욥..ㅠ..",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039175.1.11",
          "form": "아니ㅋㅋ 대체 어떻길래요ㅋㅋ",
          "original_form": "아니ㅋㅋ 대체 어떻길래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100039175.1.12",
          "form": "쓴맛이 나요.....",
          "original_form": "쓴맛이 나요.....",
          "speaker_id": "2",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100039175.1.13",
          "form": "대체 왜인지는 모르겠지만....",
          "original_form": "대체 왜인지는 모르겠지만....",
          "speaker_id": "2",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100039175.1.14",
          "form": "??  김치는 뭐 쓰시나요",
          "original_form": "??  김치는 뭐 쓰시나요",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039175.1.15",
          "form": "김치는",
          "original_form": "김치는",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039175.1.16",
          "form": "묵은지 김치를 씁니다",
          "original_form": "묵은지 김치를 씁니다",
          "speaker_id": "2",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039175.1.17",
          "form": "그런데 같은 김치와 같은 재료로 한 엄마의 찌개는 맛있는데",
          "original_form": "그런데 같은 김치와 같은 재료로 한 엄마의 찌개는 맛있는데",
          "speaker_id": "2",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039175.1.18",
          "form": "제 찌개는..........",
          "original_form": "제 찌개는..........",
          "speaker_id": "2",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039175.1.19",
          "form": "아니ㅜ 무슨 일일까요 대체ㅜㅜ",
          "original_form": "아니ㅜ 무슨 일일까요 대체ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 12:59"
        },
        {
          "id": "MDRW2100039175.1.20",
          "form": "그런데 또 의문인 점은",
          "original_form": "그런데 또 의문인 점은",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100039175.1.21",
          "form": "김치찜은 기가 먹히게 잘합니다",
          "original_form": "김치찜은 기가 먹히게 잘합니다",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100039175.1.22",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039175.1.23",
          "form": "고기김치찜인가요?",
          "original_form": "고기김치찜인가요?",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039175.1.24",
          "form": "어유 당연하죠",
          "original_form": "어유 당연하죠",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039175.1.25",
          "form": "고기없으면 어디 그게 김치찜인가요",
          "original_form": "고기없으면 어디 그게 김치찜인가요",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100039175.1.26",
          "form": "김치찜엔 고기가 있어야 해요",
          "original_form": "김치찜엔 고기가 있어야 해요",
          "speaker_id": "2",
          "time": "20210903 13:03"
        }
      ]
