      "utterance": [
        {
          "id": "MDRW2100013384.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013384.1.2",
          "form": "안녕하세요 최근에 여행가신곳 있나요?",
          "original_form": "안녕하세요 최근에 여행가신곳 있나요?",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013384.1.3",
          "form": "최근에는 없고 저번세 부산 광안리다녀왔어요",
          "original_form": "최근에는 없고 저번세 부산 광안리다녀왔어요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013384.1.4",
          "form": "여행 다녀오신 곳 있으신가요",
          "original_form": "여행 다녀오신 곳 있으신가요",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013384.1.5",
          "form": "저도 시국이 시국이라 최근은 아니구요 2,3년전에 전주 다녀왔어요",
          "original_form": "저도 시국이 시국이라 최근은 아니구요 2,3년전에 전주 다녀왔어요",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013384.1.6",
          "form": "전주다녀오셨구나 전주 어땠나요?",
          "original_form": "전주다녀오셨구나 전주 어땠나요?",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013384.1.7",
          "form": "전주가면 보통 한옥마을 가시나요?",
          "original_form": "전주가면 보통 한옥마을 가시나요?",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013384.1.8",
          "form": "전주 느긋하게 맛있는 밥 먹고 한옥거리 걷기 좋았어요",
          "original_form": "전주 느긋하게 맛있는 밥 먹고 한옥거리 걷기 좋았어요",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013384.1.9",
          "form": "네 한옥마을 갔었어요",
          "original_form": "네 한옥마을 갔었어요",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013384.1.10",
          "form": "한옥 거리가셨구나 너무 좋으셨겠어요",
          "original_form": "한옥 거리가셨구나 너무 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013384.1.11",
          "form": "전주에서는 뭘 드셨나요",
          "original_form": "전주에서는 뭘 드셨나요",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013384.1.12",
          "form": "국밥을 좋아해서 순대국밥 콩나물 국밥을 먹었구요",
          "original_form": "국밥을 좋아해서 순대국밥 콩나물 국밥을 먹었구요",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013384.1.13",
          "form": "베테랑칼국수도 먹었어요",
          "original_form": "베테랑칼국수도 먹었어요",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013384.1.14",
          "form": "국밥 너무 맛있었겠다 전주하면 피순대국밥이라던데",
          "original_form": "국밥 너무 맛있었겠다 전주하면 피순대국밥이라던데",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013384.1.15",
          "form": "맞아요! 피순대국밥!",
          "original_form": "맞아요! 피순대국밥!",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013384.1.16",
          "form": "그거때문에 또 가고싶어요",
          "original_form": "그거때문에 또 가고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013384.1.17",
          "form": "궁금하긴하네요",
          "original_form": "궁금하긴하네요",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013384.1.18",
          "form": "피순대국밥이라니",
          "original_form": "피순대국밥이라니",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013384.1.19",
          "form": "좀 다르던가요?",
          "original_form": "좀 다르던가요?",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013384.1.20",
          "form": "좀더 진하고 얼큰한 맛이였어요",
          "original_form": "좀더 진하고 얼큰한 맛이였어요",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013384.1.21",
          "form": "오 한번먹으러 가봐야겠어요",
          "original_form": "오 한번먹으러 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        }
      ]
