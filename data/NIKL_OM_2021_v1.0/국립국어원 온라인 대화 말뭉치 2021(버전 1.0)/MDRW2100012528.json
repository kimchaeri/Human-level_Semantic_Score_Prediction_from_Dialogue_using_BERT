      "utterance": [
        {
          "id": "MDRW2100012528.1.1",
          "form": "나 군대를 안 가봤는데..",
          "original_form": "나 군대를 안 가봤는데..",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012528.1.2",
          "form": "간다면 전갈부대 가고 싶음",
          "original_form": "간다면 전갈부대 가고 싶음",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012528.1.3",
          "form": "아님 독도수비대나 유해발굴단",
          "original_form": "아님 독도수비대나 유해발굴단",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012528.1.4",
          "form": "군대는 자고로 선임을 잘 만나야 함",
          "original_form": "군대는 자고로 선임을 잘 만나야 함",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012528.1.5",
          "form": "넌 선임 잘 만났어?",
          "original_form": "넌 선임 잘 만났어?",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012528.1.6",
          "form": "아님 너는 좋은 선임이었냐",
          "original_form": "아님 너는 좋은 선임이었냐",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012528.1.7",
          "form": "나는 좋은 선임을 만나진 못했어.",
          "original_form": "나는 좋은 선임을 만나진 못했어.",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012528.1.8",
          "form": "나는 좋은 선임이 되려고 노력했지만 후임들이 무서워했어.",
          "original_form": "나는 좋은 선임이 되려고 노력했지만 후임들이 무서워했어.",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012528.1.9",
          "form": "왜 무서워함?",
          "original_form": "왜 무서워함?",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012528.1.10",
          "form": "엄격했으니까",
          "original_form": "엄격했으니까",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012528.1.11",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012528.1.12",
          "form": "터레스팅...",
          "original_form": "터레스팅...",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012528.1.13",
          "form": "그리고 화도 많이 냈었지",
          "original_form": "그리고 화도 많이 냈었지",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012528.1.14",
          "form": "넌 너무 화가 많아",
          "original_form": "넌 너무 화가 많아",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012528.1.15",
          "form": "아님",
          "original_form": "아님",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012528.1.16",
          "form": "ㅋㅋㅋㅋㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012528.1.17",
          "form": "제대한지 얼마나 됐지??",
          "original_form": "제대한지 얼마나 됐지??",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012528.1.18",
          "form": "제대는 아직. 전역을 했을 뿐",
          "original_form": "제대는 아직. 전역을 했을 뿐",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012528.1.19",
          "form": "엄격하네",
          "original_form": "엄격하네",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012528.1.20",
          "form": "제대도 맞는 말인가?",
          "original_form": "제대도 맞는 말인가?",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012528.1.21",
          "form": "요즘 기사 보니까 탈레반이 우리나라 옛날 군복 갖다가 지들 군복으로 쓴다던데",
          "original_form": "요즘 기사 보니까 탈레반이 우리나라 옛날 군복 갖다가 지들 군복으로 쓴다던데",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012528.1.22",
          "form": "디지털군복 전에 쓰던거",
          "original_form": "디지털군복 전에 쓰던거",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012528.1.23",
          "form": "개구리",
          "original_form": "개구리",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012528.1.24",
          "form": "제대랑 전역이랑 뭔 차인데?",
          "original_form": "제대랑 전역이랑 뭔 차인데?",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012528.1.25",
          "form": "역 바뀐 것이 전역",
          "original_form": "역 바뀐 것이 전역",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012528.1.26",
          "form": "현역에서 예비역된거?",
          "original_form": "현역에서 예비역된거?",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012528.1.27",
          "form": "제대는 뭔데",
          "original_form": "제대는 뭔데",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012528.1.28",
          "form": "제대는 현역에서 해제됨을 말함",
          "original_form": "제대는 현역에서 해제됨을 말함",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012528.1.29",
          "form": "내게 제대도 반쯤은 맞는 말",
          "original_form": "내게 제대도 반쯤은 맞는 말",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012528.1.30",
          "form": "예비역도 현역 아니잖아",
          "original_form": "예비역도 현역 아니잖아",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012528.1.31",
          "form": "군인이잖아",
          "original_form": "군인이잖아",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012528.1.32",
          "form": "예비역도",
          "original_form": "예비역도",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012528.1.33",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012528.1.34",
          "form": "제대는 그럼 걍 관습적으로 쓰는건가?",
          "original_form": "제대는 그럼 걍 관습적으로 쓰는건가?",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012528.1.35",
          "form": "실제론 아닌데?",
          "original_form": "실제론 아닌데?",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012528.1.36",
          "form": "어 맞아",
          "original_form": "어 맞아",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012528.1.37",
          "form": "귀찮아보인다 너",
          "original_form": "귀찮아보인다 너",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012528.1.38",
          "form": "그냥 관습적인 표현",
          "original_form": "그냥 관습적인 표현",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012528.1.39",
          "form": "대답하기 싫으냐",
          "original_form": "대답하기 싫으냐",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012528.1.40",
          "form": "지금 졸려ㅋㅋ",
          "original_form": "지금 졸려ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012528.1.41",
          "form": "정신차려",
          "original_form": "정신차려",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012528.1.42",
          "form": "양심적병역거부는 어때",
          "original_form": "양심적병역거부는 어때",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012528.1.43",
          "form": "노관심?",
          "original_form": "노관심?",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012528.1.44",
          "form": "비양심임",
          "original_form": "비양심임",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012528.1.45",
          "form": "너가?",
          "original_form": "너가?",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012528.1.46",
          "form": "병역거부 자체가",
          "original_form": "병역거부 자체가",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012528.1.47",
          "form": "이름을 바꾸면 어때",
          "original_form": "이름을 바꾸면 어때",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012528.1.48",
          "form": "양심이란 단어가 불편하다면",
          "original_form": "양심이란 단어가 불편하다면",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012528.1.49",
          "form": "신념적병역거부 정도로",
          "original_form": "신념적병역거부 정도로",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.50",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.51",
          "form": "나도 지금 그 단어 생각했는데",
          "original_form": "나도 지금 그 단어 생각했는데",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.52",
          "form": "근데 양식이나 신념이나",
          "original_form": "근데 양식이나 신념이나",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.53",
          "form": "양신",
          "original_form": "양신",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.54",
          "form": "양심",
          "original_form": "양심",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.55",
          "form": "오타 쩌네",
          "original_form": "오타 쩌네",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012528.1.56",
          "form": "법률용어자네",
          "original_form": "법률용어자네",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012528.1.57",
          "form": "글구 사전상으로도 양심이 맞는듯",
          "original_form": "글구 사전상으로도 양심이 맞는듯",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012528.1.58",
          "form": "몰라ㅋㅋㅋ",
          "original_form": "몰라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012528.1.59",
          "form": "대화에 성의가 없구만",
          "original_form": "대화에 성의가 없구만",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
