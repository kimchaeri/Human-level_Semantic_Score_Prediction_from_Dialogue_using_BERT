      "utterance": [
        {
          "id": "MDRW2100005322.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005322.1.2",
          "form": "저는 프리랜서인데",
          "original_form": "저는 프리랜서인데",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005322.1.3",
          "form": "name2님은 학생이신가요? 직장인?",
          "original_form": "&name2&님은 학생이신가요? 직장인?",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005322.1.4",
          "form": "저는 직장인이요!",
          "original_form": "저는 직장인이요!",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005322.1.5",
          "form": "프리랜서면 힘든점 없으세요?",
          "original_form": "프리랜서면 힘든점 없으세요?",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005322.1.6",
          "form": "시간관리가 잘 안 된달까요",
          "original_form": "시간관리가 잘 안 된달까요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005322.1.7",
          "form": "이건 제가 게으른 게 문제긴 한데",
          "original_form": "이건 제가 게으른 게 문제긴 한데",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005322.1.8",
          "form": "놀려면 신나게 놀수 있어서",
          "original_form": "놀려면 신나게 놀수 있어서",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005322.1.9",
          "form": "문제예요",
          "original_form": "문제예요",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005322.1.10",
          "form": "그리고 집안에만 있으니",
          "original_form": "그리고 집안에만 있으니",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005322.1.11",
          "form": "좀 지루한 것?",
          "original_form": "좀 지루한 것?",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005322.1.12",
          "form": "아 근데 저도 집에서 공부할때 그랬던 것 같아요",
          "original_form": "아 근데 저도 집에서 공부할때 그랬던 것 같아요",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005322.1.13",
          "form": "그럼 좋은점은 뭔가요??",
          "original_form": "그럼 좋은점은 뭔가요??",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005322.1.14",
          "form": "이것도 시간관리인 게",
          "original_form": "이것도 시간관리인 게",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005322.1.15",
          "form": "마감만 지키면",
          "original_form": "마감만 지키면",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005322.1.16",
          "form": "언제 일을 해도 쉬어도 상관없으니까",
          "original_form": "언제 일을 해도 쉬어도 상관없으니까",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005322.1.17",
          "form": "여행이나 약속 잡을 때 곤란한게 없어요",
          "original_form": "여행이나 약속 잡을 때 곤란한게 없어요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005322.1.18",
          "form": "아 그건 너무 부럽네요",
          "original_form": "아 그건 너무 부럽네요",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005322.1.19",
          "form": "일반 직장 다니면 친구들과 여행 약속 잡기가 너무 힘들어요",
          "original_form": "일반 직장 다니면 친구들과 여행 약속 잡기가 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005322.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005322.1.21",
          "form": "다들 직장이 있으니",
          "original_form": "다들 직장이 있으니",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005322.1.22",
          "form": "출장이다 연수다 뭐다 하면",
          "original_form": "출장이다 연수다 뭐다 하면",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005322.1.23",
          "form": "시간 맞추기가 힘들더라구요",
          "original_form": "시간 맞추기가 힘들더라구요",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005322.1.24",
          "form": "회사생활보 해보신적 있는건가요??",
          "original_form": "회사생활보 해보신적 있는건가요??",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005322.1.25",
          "form": "네, 근데 아직 길게는 해본적이 없어요",
          "original_form": "네, 근데 아직 길게는 해본적이 없어요",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005322.1.26",
          "form": "이것저것 해보다가",
          "original_form": "이것저것 해보다가",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005322.1.27",
          "form": "프리랜서 하면서",
          "original_form": "프리랜서 하면서",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005322.1.28",
          "form": "정규직으로 찾아보는 중이에요",
          "original_form": "정규직으로 찾아보는 중이에요",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005322.1.29",
          "form": "아 하긴 오래하려면 정규직이 안정감있긴 한 것 같아요",
          "original_form": "아 하긴 오래하려면 정규직이 안정감있긴 한 것 같아요",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005322.1.30",
          "form": "다 장단점이 존재하죠",
          "original_form": "다 장단점이 존재하죠",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005322.1.31",
          "form": "프리랜서면 일을 직접 구해서 하시나요?",
          "original_form": "프리랜서면 일을 직접 구해서 하시나요?",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005322.1.32",
          "form": "네, 공고 보고 연락해서 조건이 맞으면",
          "original_form": "네, 공고 보고 연락해서 조건이 맞으면",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005322.1.33",
          "form": "작업하는 식이에요",
          "original_form": "작업하는 식이에요",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005322.1.34",
          "form": "그래도 직접 하고싶은 일을 골라서 할 수 있는게 좋을 것 같아요",
          "original_form": "그래도 직접 하고싶은 일을 골라서 할 수 있는게 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005322.1.35",
          "form": "그리고 상사가 없다는점도 부러워요 ㅠㅠ ㅎㅎ",
          "original_form": "그리고 상사가 없다는점도 부러워요 ㅠㅠ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005322.1.36",
          "form": "맞아요, 하고 싶은 일이나 잘하는 일만 집중적으로 하는 느낌이죠",
          "original_form": "맞아요, 하고 싶은 일이나 잘하는 일만 집중적으로 하는 느낌이죠",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005322.1.37",
          "form": "사람이랑 부대낄 일이 없는 게 장점이자 단점ㅎㅎ",
          "original_form": "사람이랑 부대낄 일이 없는 게 장점이자 단점ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:31"
        }
      ]
