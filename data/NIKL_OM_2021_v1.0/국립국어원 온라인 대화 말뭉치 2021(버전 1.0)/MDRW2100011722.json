      "utterance": [
        {
          "id": "MDRW2100011722.1.1",
          "form": "아빠랑 백화점 간 김에 이마트서 장 봤어",
          "original_form": "아빠랑 백화점 간 김에 이마트서 장 봤어",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011722.1.2",
          "form": "오 잘했네",
          "original_form": "오 잘했네",
          "speaker_id": "2",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011722.1.3",
          "form": "저녁식사 차릴 거리 좀 샀겠군",
          "original_form": "저녁식사 차릴 거리 좀 샀겠군",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011722.1.4",
          "form": "저녁식사로 삼치~",
          "original_form": "저녁식사로 삼치~",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011722.1.5",
          "form": "저녁거리보단 점심거리를 사서 왔지",
          "original_form": "저녁거리보단 점심거리를 사서 왔지",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011722.1.6",
          "form": "집에 있는 가족들이 장 봐서 식사거리를 마련했구만!!",
          "original_form": "집에 있는 가족들이 장 봐서 식사거리를 마련했구만!!",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011722.1.7",
          "form": "아빠 모자 받아온거지?",
          "original_form": "아빠 모자 받아온거지?",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011722.1.8",
          "form": "생일선물 맘에 드시나~",
          "original_form": "생일선물 맘에 드시나~",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011722.1.9",
          "form": "응 마음에 드시는 듯",
          "original_form": "응 마음에 드시는 듯",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011722.1.10",
          "form": "아까 내 앞에서 쓰고 자랑도 하셨어",
          "original_form": "아까 내 앞에서 쓰고 자랑도 하셨어",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011722.1.11",
          "form": "아빠가 생신선물 맘에 들어하시니 다행이야",
          "original_form": "아빠가 생신선물 맘에 들어하시니 다행이야",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011722.1.12",
          "form": "나는 방금 내일 아침거리 만듦.",
          "original_form": "나는 방금 내일 아침거리 만듦.",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100011722.1.13",
          "form": "오 가족의 식사를 책임지시네",
          "original_form": "오 가족의 식사를 책임지시네",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100011722.1.14",
          "form": "메뉴는 뭔가요",
          "original_form": "메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100011722.1.15",
          "form": "짜치키와 피타빵, 크래미가 내일 우리가족 아침식사입니다",
          "original_form": "짜치키와 피타빵, 크래미가 내일 우리가족 아침식사입니다",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100011722.1.16",
          "form": "훌륭합니다",
          "original_form": "훌륭합니다",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100011722.1.17",
          "form": "식기세척기도 정리해야하는데",
          "original_form": "식기세척기도 정리해야하는데",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100011722.1.18",
          "form": "식기세척기 정리 엄마아빠가 끝내시고",
          "original_form": "식기세척기 정리 엄마아빠가 끝내시고",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100011722.1.19",
          "form": "저녁 먹은 것도 넣었습니다~",
          "original_form": "저녁 먹은 것도 넣었습니다~",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100011722.1.20",
          "form": "와 최고의 가족",
          "original_form": "와 최고의 가족",
          "speaker_id": "1",
          "time": "20210824 23:26"
        },
        {
          "id": "MDRW2100011722.1.21",
          "form": "ㅎㅎ호흡이 잘맞는 가족이지",
          "original_form": "ㅎㅎ호흡이 잘맞는 가족이지",
          "speaker_id": "2",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100011722.1.22",
          "form": "엄마는 주무시다 방금 전에 깨셔서",
          "original_form": "엄마는 주무시다 방금 전에 깨셔서",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100011722.1.23",
          "form": "오늘 밤 늦게 주무실듯",
          "original_form": "오늘 밤 늦게 주무실듯",
          "speaker_id": "2",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100011722.1.24",
          "form": "아 그래 주무시다 깨신겨?",
          "original_form": "아 그래 주무시다 깨신겨?",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100011722.1.25",
          "form": "낼 고생하시겠네",
          "original_form": "낼 고생하시겠네",
          "speaker_id": "1",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100011722.1.26",
          "form": "응 엄마는 주무시다 깨신겨",
          "original_form": "응 엄마는 주무시다 깨신겨",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011722.1.27",
          "form": "아빠는 아니고?",
          "original_form": "아빠는 아니고?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011722.1.28",
          "form": "아빠는 아닌듯",
          "original_form": "아빠는 아닌듯",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011722.1.29",
          "form": "오키오키",
          "original_form": "오키오키",
