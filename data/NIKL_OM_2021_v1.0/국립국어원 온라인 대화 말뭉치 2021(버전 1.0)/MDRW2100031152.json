      "utterance": [
        {
          "id": "MDRW2100031152.1.1",
          "form": "국물에 찍어먹으면 완전 환상이잔ㅎ아요",
          "original_form": "국물에 찍어먹으면 완전 환상이잔ㅎ아요",
          "speaker_id": "1",
          "time": "20210904 17:35"
        },
        {
          "id": "MDRW2100031152.1.2",
          "form": "맞아요...진짜 최고...",
          "original_form": "맞아요...진짜 최고...",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031152.1.3",
          "form": "지금도 얘기하니 먹고 싶네요",
          "original_form": "지금도 얘기하니 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031152.1.4",
          "form": "저녁은 뭐 드세요?",
          "original_form": "저녁은 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031152.1.5",
          "form": "지금 메뉴 고민중이에요 고기 먹구 싶어서!",
          "original_form": "지금 메뉴 고민중이에요 고기 먹구 싶어서!",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031152.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 19:03"
        },
        {
          "id": "MDRW2100031152.1.7",
          "form": "저는 게 먹으러 왔어요",
          "original_form": "저는 게 먹으러 왔어요",
          "speaker_id": "2",
          "time": "20210904 19:04"
        },
        {
          "id": "MDRW2100031152.1.8",
          "form": "진짜 비싸네요",
          "original_form": "진짜 비싸네요",
          "speaker_id": "2",
          "time": "20210904 19:04"
        },
        {
          "id": "MDRW2100031152.1.9",
          "form": "게요!?? 대게!?!?",
          "original_form": "게요!?? 대게!?!?",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100031152.1.10",
          "form": "맛있겠다!!",
          "original_form": "맛있겠다!!",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100031152.1.11",
          "form": "네네, 대게! 무려 kg당 75,000원입니다",
          "original_form": "네네, 대게! 무려 kg당 75,000원입니다",
          "speaker_id": "2",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100031152.1.12",
          "form": "네...?",
          "original_form": "네...?",
          "speaker_id": "1",
          "time": "20210904 19:21"
        },
        {
          "id": "MDRW2100031152.1.13",
          "form": "아니 원래 이렇게 비쌌나?????",
          "original_form": "아니 원래 이렇게 비쌌나?????",
          "speaker_id": "1",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100031152.1.14",
          "form": "왤케 비싸요???",
          "original_form": "왤케 비싸요???",
          "speaker_id": "1",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100031152.1.15",
          "form": "모르겠어욬ㅋㅋㅋ, 금게예요 금게",
          "original_form": "모르겠어욬ㅋㅋㅋ, 금게예요 금게",
          "speaker_id": "2",
          "time": "20210904 19:36"
        },
        {
          "id": "MDRW2100031152.1.16",
          "form": "name1님은 이제 메뉴 정하셨어요?",
          "original_form": "&name1&님은 이제 메뉴 정하셨어요?",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031152.1.17",
          "form": "저 방금 다사랑 치뿌링 먹고 왓어요!!",
          "original_form": "저 방금 다사랑 치뿌링 먹고 왓어요!!",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031152.1.18",
          "form": "저는 이게 뿌링클보다 100배 맛나더라고쿄!",
          "original_form": "저는 이게 뿌링클보다 100배 맛나더라고쿄!",
          "speaker_id": "1",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031152.1.19",
          "form": "오, 첨 들어봐요",
          "original_form": "오, 첨 들어봐요",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031152.1.20",
          "form": "치뿌링의 치는 치즈??",
          "original_form": "치뿌링의 치는 치즈??",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031152.1.21",
          "form": "네네! 뿌링클이랑 비슷한데 더 맛나요!",
          "original_form": "네네! 뿌링클이랑 비슷한데 더 맛나요!",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031152.1.22",
          "form": "덜 느끼하고!!",
          "original_form": "덜 느끼하고!!",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100031152.1.23",
          "form": "우옹, 다음에 치킨 먹으면 시켜볼까봐요",
