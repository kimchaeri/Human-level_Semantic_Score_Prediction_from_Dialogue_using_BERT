      "utterance": [
        {
          "id": "MDRW2100009202.1.1",
          "form": "좋아하는 음식있나요",
          "original_form": "좋아하는 음식있나요",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009202.1.2",
          "form": "저는 고기요..",
          "original_form": "저는 고기요..",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.3",
          "form": "단백질류...",
          "original_form": "단백질류...",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.4",
          "form": "어떤거 좋아하시나요?",
          "original_form": "어떤거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.5",
          "form": "오 해산물쪽은 별론가요?",
          "original_form": "오 해산물쪽은 별론가요?",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.6",
          "form": "충청도 출신이라.. 바다생물은 오징어정도요..",
          "original_form": "충청도 출신이라.. 바다생물은 오징어정도요..",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.7",
          "form": "아하 ㅎ",
          "original_form": "아하 ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.8",
          "form": "초밥도 좋고.. 회도 좋고..",
          "original_form": "초밥도 좋고.. 회도 좋고..",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.9",
          "form": "저는 안가리는데",
          "original_form": "저는 안가리는데",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009202.1.10",
          "form": "아.. 다 좋아하나봐요.. ㅋㅋㅋ",
          "original_form": "아.. 다 좋아하나봐요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009202.1.11",
          "form": "매콤한 음식 좋아해용",
          "original_form": "매콤한 음식 좋아해용",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009202.1.12",
          "form": "그치만 회 고기 다좋아해요 ㅋㅋ",
          "original_form": "그치만 회 고기 다좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009202.1.13",
          "form": "닭발? 이런거도 드세요?",
          "original_form": "닭발? 이런거도 드세요?",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009202.1.14",
          "form": "종류불문이요",
          "original_form": "종류불문이요",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009202.1.15",
          "form": "네네 좋아하죵",
          "original_form": "네네 좋아하죵",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009202.1.16",
          "form": "닭발 안 무서운가요?",
          "original_form": "닭발 안 무서운가요?",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009202.1.17",
          "form": "생긴게...",
          "original_form": "생긴게...",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009202.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009202.1.19",
