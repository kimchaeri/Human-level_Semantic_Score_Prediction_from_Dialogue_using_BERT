      "utterance": [
        {
          "id": "MDRW2100042276.1.1",
          "form": "지금 드시고 싶으신거 있으세요?",
          "original_form": "지금 드시고 싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042276.1.2",
          "form": "음음 마카롱에 아아욮ㅋ",
          "original_form": "음음 마카롱에 아아욮ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042276.1.3",
          "form": "오 마카롱 좋아하세요?",
          "original_form": "오 마카롱 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042276.1.4",
          "form": "가끔 기분전환할때용",
          "original_form": "가끔 기분전환할때용",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042276.1.5",
          "form": "뚱카롱 맛잇죠 ㅋ",
          "original_form": "뚱카롱 맛잇죠 ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042276.1.6",
          "form": "네넼ㅋㅋ 어떤맛 좋아하세요",
          "original_form": "네넼ㅋㅋ 어떤맛 좋아하세요",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042276.1.7",
          "form": "음 전  과일 들어있는거이 좋아해요 ㅎㅎ",
          "original_form": "음 전  과일 들어있는거이 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042276.1.8",
          "form": "오오 과일 !!",
          "original_form": "오오 과일 !!",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042276.1.9",
          "form": "몇개까지 드세요?? 저는 한개이상은 달아서 못먹겟드라구요",
          "original_form": "몇개까지 드세요?? 저는 한개이상은 달아서 못먹겟드라구요",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042276.1.10",
          "form": "저도 한개 끝이요 ㅋ",
          "original_form": "저도 한개 끝이요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042276.1.11",
          "form": "아아랑 같이 먹으면 어울리겟어요 ㅋㅋ",
          "original_form": "아아랑 같이 먹으면 어울리겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042276.1.12",
          "form": "아아는 필수져 ㅋㅋㅋ",
          "original_form": "아아는 필수져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042276.1.13",
          "form": "단거 +단거 안멍어지더라구요",
          "original_form": "단거 +단거 안멍어지더라구요",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042276.1.14",
          "form": "네네!!! 디저트류 좋아하세요??",
          "original_form": "네네!!! 디저트류 좋아하세요??",
