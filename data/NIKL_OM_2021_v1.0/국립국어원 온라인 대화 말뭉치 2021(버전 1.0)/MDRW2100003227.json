      "utterance": [
        {
          "id": "MDRW2100003227.1.1",
          "form": "오늘 잼난거 뭐하나요?",
          "original_form": "오늘 잼난거 뭐하나요?",
          "speaker_id": "1",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003227.1.2",
          "form": "나혼자산다? 그거 한번씩봐요",
          "original_form": "나혼자산다? 그거 한번씩봐요",
          "speaker_id": "2",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003227.1.3",
          "form": "오늘 하는날이죠?",
          "original_form": "오늘 하는날이죠?",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003227.1.4",
          "form": "점점 재미없어지긴해요",
          "original_form": "점점 재미없어지긴해요",
          "speaker_id": "2",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003227.1.5",
          "form": "네  금요일 밤11시에 해요",
          "original_form": "네  금요일 밤11시에 해요",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003227.1.6",
          "form": "기안84  몰카로 말 많던데요",
          "original_form": "기안84  몰카로 말 많던데요",
          "speaker_id": "1",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003227.1.7",
          "form": "저도 그거봤는데 좀 너무했다싶더라구요",
          "original_form": "저도 그거봤는데 좀 너무했다싶더라구요",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003227.1.8",
          "form": "그래요? 저는 못봤어요",
          "original_form": "그래요? 저는 못봤어요",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003227.1.9",
          "form": "그 마감샤워인가 하는거요?",
          "original_form": "그 마감샤워인가 하는거요?",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003227.1.10",
          "form": "네네 전현무랑 둘이 여행갔는데",
          "original_form": "네네 전현무랑 둘이 여행갔는데",
          "speaker_id": "1",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003227.1.11",
          "form": "지난주 보다가 잠든거같은데 둘이 스케이트같은거 만드는거까지만 봤어요",
          "original_form": "지난주 보다가 잠든거같은데 둘이 스케이트같은거 만드는거까지만 봤어요",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003227.1.12",
          "form": "엄청기대하고 계획짜고갔더니 다른이들은 안오고 속였더라구요",
          "original_form": "엄청기대하고 계획짜고갔더니 다른이들은 안오고 속였더라구요",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003227.1.13",
          "form": "올것처럼 말하고",
          "original_form": "올것처럼 말하고",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003227.1.14",
          "form": "마지막엔 오지않을까요?",
          "original_form": "마지막엔 오지않을까요?",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003227.1.15",
          "form": "진짜 안왔을카요?",
          "original_form": "진짜 안왔을카요?",
          "speaker_id": "2",
          "time": "20210820 21:00"
        },
        {
          "id": "MDRW2100003227.1.16",
          "form": "네 안오고 장난치듯 웃기만 하더라구요",
          "original_form": "네 안오고 장난치듯 웃기만 하더라구요",
          "speaker_id": "1",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003227.1.17",
          "form": "기안84  진짜 황당한표정이었어요",
          "original_form": "기안84  진짜 황당한표정이었어요",
          "speaker_id": "1",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003227.1.18",
          "form": "헐 욕먹겠는데요",
          "original_form": "헐 욕먹겠는데요",
          "speaker_id": "2",
          "time": "20210820 21:01"
        },
        {
          "id": "MDRW2100003227.1.19",
          "form": "저는 그래도 마지막엔 짠하고 나타날줄알았어요",
          "original_form": "저는 그래도 마지막엔 짠하고 나타날줄알았어요",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003227.1.20",
          "form": "아닌가보더라구요 그래서 다시보기영상 삭제했대요",
          "original_form": "아닌가보더라구요 그래서 다시보기영상 삭제했대요",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003227.1.21",
          "form": "욕많이 먹었군요\r\n전현무 들어온지 얼마안되서 이게 먼일이래요?",
          "original_form": "욕많이 먹었군요\r\n전현무 들어온지 얼마안되서 이게 먼일이래요?",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003227.1.22",
          "form": "누구 아이디어인지...",
          "original_form": "누구 아이디어인지...",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003227.1.23",
          "form": "그니까요 나혼자산다도 이제 끝물인듯요",
          "original_form": "그니까요 나혼자산다도 이제 끝물인듯요",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003227.1.24",
          "form": "나혼자산다 보시나요? 오상욱선수 나와요ㅎ",
          "original_form": "나혼자산다 보시나요? 오상욱선수 나와요ㅎ",
          "speaker_id": "1",
          "time": "20210821 00:00"
        }
      ]
    }
