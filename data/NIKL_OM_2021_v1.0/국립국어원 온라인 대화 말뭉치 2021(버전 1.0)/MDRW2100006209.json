      "utterance": [
        {
          "id": "MDRW2100006209.1.1",
          "form": "혹시 미드 좋아하시는 거 있으세요??",
          "original_form": "혹시 미드 좋아하시는 거 있으세요??",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006209.1.2",
          "form": "저 미드는 잘안봐요ㅠ 미드 어떤거보세요?",
          "original_form": "저 미드는 잘안봐요ㅠ 미드 어떤거보세요?",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006209.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006209.1.4",
          "form": "브루클린 나인나인이라고 아시나요...?",
          "original_form": "브루클린 나인나인이라고 아시나요...?",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006209.1.5",
          "form": "헐 알아요",
          "original_form": "헐 알아요",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006209.1.6",
          "form": "제가 그나마 보는게 그거데요!! ㅋㅋㅋㅋㅋ",
          "original_form": "제가 그나마 보는게 그거데요!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006209.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006209.1.8",
          "form": "완전 대박이군요",
          "original_form": "완전 대박이군요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006209.1.9",
          "form": "noice",
          "original_form": "noice",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006209.1.10",
          "form": "ㅋㅋ아직 시즌1이긴하지만 그래도 보고잇다구요",
          "original_form": "ㅋㅋ아직 시즌1이긴하지만 그래도 보고잇다구요",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006209.1.11",
          "form": "아 시즌 1이시구나!!",
          "original_form": "아 시즌 1이시구나!!",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006209.1.12",
          "form": "진짜 완전 재밌어요...",
          "original_form": "진짜 완전 재밌어요...",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006209.1.13",
          "form": "ㅋㅋㅋ남주 처음에는 이상하다고 생각했는데 볼수록 귀여워요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ남주 처음에는 이상하다고 생각했는데 볼수록 귀여워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006209.1.14",
          "form": "저 너드좋아하나봐요ㅋㅋㅋㅋ",
          "original_form": "저 너드좋아하나봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006209.1.15",
          "form": "너무 귀엽죠...",
          "original_form": "너무 귀엽죠...",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006209.1.16",
          "form": "저 에이미랑 로사도 진짜 좋아해요...",
          "original_form": "저 에이미랑 로사도 진짜 좋아해요...",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006209.1.17",
          "form": "너드라면... 혹시 빅뱅이론도 보셨을까요...?",
          "original_form": "너드라면... 혹시 빅뱅이론도 보셨을까요...?",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006209.1.18",
          "form": "악 둘다 너무좋죠 ㅋㅋㅋ 로사 매력쩔어요",
          "original_form": "악 둘다 너무좋죠 ㅋㅋㅋ 로사 매력쩔어요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006209.1.19",
          "form": "당연하죠!!!!!",
          "original_form": "당연하죠!!!!!",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006209.1.20",
          "form": "와 저랑 엄청 잘 맞으신데,.,?",
          "original_form": "와 저랑 엄청 잘 맞으신데,.,?",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006209.1.21",
          "form": "저 시트콤 완전 좋아하거든요 ㅠㅠ",
          "original_form": "저 시트콤 완전 좋아하거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006209.1.22",
          "form": "저두요 ㅠㅠㅠ  저 시트콤만봐요",
          "original_form": "저두요 ㅠㅠㅠ  저 시트콤만봐요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006209.1.23",
          "form": "모던패밀리도 보셨나요?",
          "original_form": "모던패밀리도 보셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006209.1.24",
          "form": "앗 모팸은 아직 못봤어요...",
          "original_form": "앗 모팸은 아직 못봤어요...",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006209.1.25",
          "form": "요즘은 오피스랑 시트크릭 보고 있어요!",
          "original_form": "요즘은 오피스랑 시트크릭 보고 있어요!",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006209.1.26",
          "form": "모팸도 가볍게보기 좋아요 ㅋㅋㅋㅋ",
          "original_form": "모팸도 가볍게보기 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100006209.1.27",
          "form": "시트크릭은 처음들어봐요!",
          "original_form": "시트크릭은 처음들어봐요!",
          "speaker_id": "2",
