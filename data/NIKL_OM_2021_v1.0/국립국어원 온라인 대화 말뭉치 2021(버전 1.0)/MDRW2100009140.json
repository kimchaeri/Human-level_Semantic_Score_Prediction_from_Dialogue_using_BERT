      "utterance": [
        {
          "id": "MDRW2100009140.1.1",
          "form": "ㅇ오늘 저녁은 뭐드셨나요",
          "original_form": "ㅇ오늘 저녁은 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009140.1.2",
          "form": "오늘 김치볶음밥을 누룽지로 만들어서 거기에 치즈를 올려 먹었어요",
          "original_form": "오늘 김치볶음밥을 누룽지로 만들어서 거기에 치즈를 올려 먹었어요",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009140.1.3",
          "form": "와대박 요리 잘하시나봐요",
          "original_form": "와대박 요리 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009140.1.4",
          "form": "아뇨 ㅋㅋㅋ 그냥 대충해먹은거에요 반찬이 뭐가 없었어서",
          "original_form": "아뇨 ㅋㅋㅋ 그냥 대충해먹은거에요 반찬이 뭐가 없었어서",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009140.1.5",
          "form": "근데 궁금한게 김치볶음밥을 누룽지로 만들려면",
          "original_form": "근데 궁금한게 김치볶음밥을 누룽지로 만들려면",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009140.1.6",
          "form": "그냥 후라이팬에 넓적하게 붙여놓고 있으면 되나요?",
          "original_form": "그냥 후라이팬에 넓적하게 붙여놓고 있으면 되나요?",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009140.1.7",
          "form": "한번도 성공해봉적이없어서요",
          "original_form": "한번도 성공해봉적이없어서요",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009140.1.8",
          "form": "네 전 기름 좀 넉넉하게 두르고 했어요",
          "original_form": "네 전 기름 좀 넉넉하게 두르고 했어요",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009140.1.9",
          "form": "약간 튀기듯해서 맛있더라구요",
          "original_form": "약간 튀기듯해서 맛있더라구요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009140.1.10",
          "form": "기름을 넉넉하게 둘러야 하는 거ㅛ이군요",
          "original_form": "기름을 넉넉하게 둘러야 하는 거ㅛ이군요",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009140.1.11",
          "form": "꿀팁입니다요",
          "original_form": "꿀팁입니다요",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009140.1.12",
          "form": "가장 잘하는 요리신가요 김볶밥???",
          "original_form": "가장 잘하는 요리신가요 김볶밥???",
          "speaker_id": "1",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009140.1.13",
          "form": "김치맛에 따라 달라지는 것 같아요 ㅋㅋㅋ",
          "original_form": "김치맛에 따라 달라지는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009140.1.14",
          "form": "요리하는 거야 그냥 밥에 김치 볶는 거니깐 어려운 건 없는데",
          "original_form": "요리하는 거야 그냥 밥에 김치 볶는 거니깐 어려운 건 없는데",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009140.1.15",
          "form": "맛은 김치가 얼마나 익었냐에 달라지니까요",
          "original_form": "맛은 김치가 얼마나 익었냐에 달라지니까요",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009140.1.16",
          "form": "전문가의 느낌이 물씬 느껴지는 멘트군요",
          "original_form": "전문가의 느낌이 물씬 느껴지는 멘트군요",
          "speaker_id": "1",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009140.1.17",
          "form": "어떤 요리를 제일 잘하세요?",
          "original_form": "어떤 요리를 제일 잘하세요?",
          "speaker_id": "1",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009140.1.18",
          "form": "전혀 아니에요 ㅋㅋ",
          "original_form": "전혀 아니에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009140.1.19",
          "form": "딱히 잘하는 요리가 있지도 않아요",
          "original_form": "딱히 잘하는 요리가 있지도 않아요",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009140.1.20",
          "form": "요즘엔 밀키트가 잘 나와서 그걸로 많이 해먹고 있어요",
          "original_form": "요즘엔 밀키트가 잘 나와서 그걸로 많이 해먹고 있어요",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009140.1.21",
          "form": "맞아요 밀키트 너무 잘나오져 프레시지 이런거 괜찮더라구요",
          "original_form": "맞아요 밀키트 너무 잘나오져 프레시지 이런거 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009140.1.22",
          "form": "어떤 음식을 제일 좋아하세요?",
          "original_form": "어떤 음식을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009140.1.23",
          "form": "음식 먹는 걸 좋아해서 딱 하나 꼽긴 어려운데 지금은 족발이 생각나네요",
          "original_form": "음식 먹는 걸 좋아해서 딱 하나 꼽긴 어려운데 지금은 족발이 생각나네요",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009140.1.24",
          "form": "족발 맛잇겠네요 막국수에..",
          "original_form": "족발 맛잇겠네요 막국수에..",
          "speaker_id": "1",
          "time": "20210822 21:54"
        }
      ]
