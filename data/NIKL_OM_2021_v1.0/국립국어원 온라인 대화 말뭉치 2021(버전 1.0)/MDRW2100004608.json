      "utterance": [
        {
          "id": "MDRW2100004608.1.1",
          "form": "여행 얘기나 합세",
          "original_form": "여행 얘기나 합세",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004608.1.2",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004608.1.3",
          "form": "여행 가고 싶어!",
          "original_form": "여행 가고 싶어!",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004608.1.4",
          "form": "서울 여행!",
          "original_form": "서울 여행!",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004608.1.5",
          "form": "서울!!!",
          "original_form": "서울!!!",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004608.1.6",
          "form": "내가 박물관 일일 역사 가이드 할거야",
          "original_form": "내가 박물관 일일 역사 가이드 할거야",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004608.1.7",
          "form": "좋다!",
          "original_form": "좋다!",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004608.1.8",
          "form": "특히 한국사",
          "original_form": "특히 한국사",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004608.1.9",
          "form": "엥? 나 나쁜말 안썼는데",
          "original_form": "엥? 나 나쁜말 안썼는데",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004608.1.10",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004608.1.11",
          "form": "님 여기서 대화해죠ㅋㅋㅋ",
          "original_form": "님 여기서 대화해죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004608.1.12",
          "form": "하여튼 역사 가이드 해보고 싶은 마음은 있는데",
          "original_form": "하여튼 역사 가이드 해보고 싶은 마음은 있는데",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004608.1.13",
          "form": "내 기력이 남아있을지 모르겠다",
          "original_form": "내 기력이 남아있을지 모르겠다",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004608.1.14",
          "form": "박물관 말고 들리고픈 곳 있어?",
          "original_form": "박물관 말고 들리고픈 곳 있어?",
          "speaker_id": "2",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004608.1.15",
          "form": "시기 잘 겹쳐서 뮤지컬 보러 가보고싶음",
          "original_form": "시기 잘 겹쳐서 뮤지컬 보러 가보고싶음",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004608.1.16",
          "form": "아무래도 서울이 뮤지컬이나 연극 많이 해주니깐",
          "original_form": "아무래도 서울이 뮤지컬이나 연극 많이 해주니깐",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004608.1.17",
          "form": "아 그것도 좋지",
          "original_form": "아 그것도 좋지",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004608.1.18",
          "form": "에버랜드는 이제 별로인가?",
          "original_form": "에버랜드는 이제 별로인가?",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.19",
          "form": "뮤지컬이나 연극이라... 정확한 여행 날짜가 정해지면 알아봐야겠네",
          "original_form": "뮤지컬이나 연극이라... 정확한 여행 날짜가 정해지면 알아봐야겠네",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.20",
          "form": "에버랜드는 난 항상 좋은걸",
          "original_form": "에버랜드는 난 항상 좋은걸",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.21",
          "form": "에버랜드 요즘 탈 거 별로 없더라ㅠ",
          "original_form": "에버랜드 요즘 탈 거 별로 없더라ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.22",
          "form": "환상의 나라니까",
          "original_form": "환상의 나라니까",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.24",
          "form": "탈 것들 환상되버림",
          "original_form": "탈 것들 환상되버림",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004608.1.25",
          "form": "롯데월드도 가보고싶고",
          "original_form": "롯데월드도 가보고싶고",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004608.1.26",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004608.1.27",
          "form": "의외로 경주도 괜찮음",
          "original_form": "의외로 경주도 괜찮음",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004608.1.28",
          "form": "경주도 가보자",
          "original_form": "경주도 가보자",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004608.1.29",
          "form": "가까우니깐",
          "original_form": "가까우니깐",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004608.1.30",
          "form": "경주 좋지~",
          "original_form": "경주 좋지~",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004608.1.31",
          "form": "이게 정말, 학교에서 단체로 가는 경주랑",
          "original_form": "이게 정말, 학교에서 단체로 가는 경주랑",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004608.1.32",
          "form": "자발적으로 가는 경주가 달라",
          "original_form": "자발적으로 가는 경주가 달라",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004608.1.33",
          "form": "자발적으로 가는 게 훨씬 재밌어!",
          "original_form": "자발적으로 가는 게 훨씬 재밌어!",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004608.1.34",
          "form": "그치ㅋㅋㅋ",
          "original_form": "그치ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004608.1.35",
          "form": "경주도 예쁜곳많더라",
          "original_form": "경주도 예쁜곳많더라",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004608.1.36",
          "form": "특히 야경",
          "original_form": "특히 야경",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004608.1.37",
          "form": "그 물에 비치는 야경 본적있는데",
          "original_form": "그 물에 비치는 야경 본적있는데",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004608.1.38",
          "form": "잠시 사진 찾아와보겠음",
          "original_form": "잠시 사진 찾아와보겠음",
          "speaker_id": "1",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004608.1.39",
          "form": "사진 카톡으로 보냈어",
          "original_form": "사진 카톡으로 보냈어",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004608.1.40",
          "form": "봐봐",
          "original_form": "봐봐",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004608.1.41",
          "form": "와 야경 진짜 멋지다",
          "original_form": "와 야경 진짜 멋지다",
          "speaker_id": "2",
          "time": "20210821 17:39"
        }
      ]
    }
  ]
