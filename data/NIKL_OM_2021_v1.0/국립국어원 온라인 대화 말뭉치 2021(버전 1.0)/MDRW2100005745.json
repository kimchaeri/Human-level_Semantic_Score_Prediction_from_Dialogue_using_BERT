      "utterance": [
        {
          "id": "MDRW2100005745.1.1",
          "form": "비 많이 온다 ㅋ",
          "original_form": "비 많이 온다 ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100005745.1.2",
          "form": "가을 장마가 자주오네",
          "original_form": "가을 장마가 자주오네",
          "speaker_id": "1",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005745.1.3",
          "form": "지난 번 장마가 아직 안 끝난듯",
          "original_form": "지난 번 장마가 아직 안 끝난듯",
          "speaker_id": "2",
          "time": "20210907 10:38"
        },
        {
          "id": "MDRW2100005745.1.4",
          "form": "장마가 띄엄띄엄 오나",
          "original_form": "장마가 띄엄띄엄 오나",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005745.1.5",
          "form": "생각보다 비가 많이 왔네 ㅋ",
          "original_form": "생각보다 비가 많이 왔네 ㅋ",
          "speaker_id": "2",
          "time": "20210908 05:23"
        },
        {
          "id": "MDRW2100005745.1.6",
          "form": "가을에 이렇게 비가오니 덥지",
          "original_form": "가을에 이렇게 비가오니 덥지",
          "speaker_id": "1",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100005745.1.7",
          "form": "습하고",
          "original_form": "습하고",
          "speaker_id": "1",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100005745.1.8",
          "form": "그래도 아침 저녁으론 좀 시원함 ㅋ",
          "original_form": "그래도 아침 저녁으론 좀 시원함 ㅋ",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100005745.1.9",
          "form": "낮에는 덥더라",
          "original_form": "낮에는 덥더라",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100005745.1.10",
          "form": "일교차가 심한 것이 딱 가을이다",
          "original_form": "일교차가 심한 것이 딱 가을이다",
          "speaker_id": "2",
          "time": "20210908 06:12"
        },
        {
          "id": "MDRW2100005745.1.11",
          "form": "일교차는 진짜 대박",
          "original_form": "일교차는 진짜 대박",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100005745.1.12",
          "form": "낮에는 너무 덥다",
          "original_form": "낮에는 너무 덥다",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100005745.1.13",
          "form": "그래도 제일 움직이기 편한 계절이 가을이당",
          "original_form": "그래도 제일 움직이기 편한 계절이 가을이당",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100005745.1.14",
          "form": "난 초겨울이 좋더라",
          "original_form": "난 초겨울이 좋더라",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100005745.1.15",
          "form": "초가을은 좀 덥당",
          "original_form": "초가을은 좀 덥당",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100005745.1.16",
          "form": "오후에만 좀 괜찮으면 완전 가을인데~",
          "original_form": "오후에만 좀 괜찮으면 완전 가을인데~",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100005745.1.17",
          "form": "추운 가을이 되어라",
          "original_form": "추운 가을이 되어라",
          "speaker_id": "1",
          "time": "20210908 10:43"
        }
