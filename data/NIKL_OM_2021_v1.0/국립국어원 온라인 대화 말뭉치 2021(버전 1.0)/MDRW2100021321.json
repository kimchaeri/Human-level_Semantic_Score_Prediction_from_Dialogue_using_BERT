      "utterance": [
        {
          "id": "MDRW2100021321.1.1",
          "form": "안녕하세요? 아침은 드셨나요?",
          "original_form": "안녕하세요? 아침은 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100021321.1.2",
          "form": "전 토스트먹엇어요ㅎ",
          "original_form": "전 토스트먹엇어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100021321.1.3",
          "form": "와 전 늦잠자서 그냥 커피랑 과자만 먹는데",
          "original_form": "와 전 늦잠자서 그냥 커피랑 과자만 먹는데",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100021321.1.4",
          "form": "대화를 트려고 그냥 여쭤봤어요 ㅎㅎㅎㅎ",
          "original_form": "대화를 트려고 그냥 여쭤봤어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100021321.1.5",
          "form": "토스트엔 잼발라 드시나요?",
          "original_form": "토스트엔 잼발라 드시나요?",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100021321.1.6",
          "form": "딸기잼요ㅎ",
          "original_form": "딸기잼요ㅎ",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100021321.1.7",
          "form": "우와 맛있겠다",
          "original_form": "우와 맛있겠다",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100021321.1.8",
          "form": "전 버터랑 잼파에요",
          "original_form": "전 버터랑 잼파에요",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100021321.1.9",
          "form": "버터도 맛잇겟네요ㅎ",
          "original_form": "버터도 맛잇겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100021321.1.10",
          "form": "점싱은 드셧나요?",
          "original_form": "점싱은 드셧나요?",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100021321.1.11",
          "form": "네 집에서 꼬마김밥 해먹었어요 찰떡님은요?",
          "original_form": "네 집에서 꼬마김밥 해먹었어요 찰떡님은요?",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100021321.1.12",
          "form": "저는 꽁치김치찌개요!",
          "original_form": "저는 꽁치김치찌개요!",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100021321.1.13",
          "form": "맛있겠어요 밥도둑 메뉴네요",
          "original_form": "맛있겠어요 밥도둑 메뉴네요",
          "speaker_id": "1",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100021321.1.14",
          "form": "네 묵은지로해서 더!",
          "original_form": "네 묵은지로해서 더!",
          "speaker_id": "2",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100021321.1.15",
          "form": "와 침넘어가요 ㅠㅠ 꽁치통조림도 좋아해서 다 좋아하는 조함ㅂ이에요",
          "original_form": "와 침넘어가요 ㅠㅠ 꽁치통조림도 좋아해서 다 좋아하는 조함ㅂ이에요",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100021321.1.16",
          "form": "저도요ㅎ 뀰맛",
          "original_form": "저도요ㅎ 뀰맛",
          "speaker_id": "2",
          "time": "20210829 15:21"
        }
      ]
