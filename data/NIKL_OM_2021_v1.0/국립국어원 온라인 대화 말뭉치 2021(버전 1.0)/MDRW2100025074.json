        },
        {
          "id": "MDRW2100025074.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025074.1.3",
          "form": "네 고양이 키워요",
          "original_form": "네 고양이 키워요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025074.1.4",
          "form": "털관리는요?",
          "original_form": "털관리는요?",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025074.1.5",
          "form": "자주 빗고 청소하죠",
          "original_form": "자주 빗고 청소하죠",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025074.1.6",
          "form": "많이 빠지죠",
          "original_form": "많이 빠지죠",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025074.1.7",
          "form": "네 아무래도 그렇죠",
          "original_form": "네 아무래도 그렇죠",
          "speaker_id": "2",
          "time": "20210901 07:46"
        },
        {
          "id": "MDRW2100025074.1.8",
          "form": "검은바지에 붙으면 ㅋㅋ",
          "original_form": "검은바지에 붙으면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 07:48"
        },
        {
          "id": "MDRW2100025074.1.9",
          "form": "함께 살려면 포기해여죠 ㅋㅋ",
          "original_form": "함께 살려면 포기해여죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025074.1.10",
          "form": "적응이안되서",
          "original_form": "적응이안되서",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025074.1.11",
          "form": "익숙해져요",
          "original_form": "익숙해져요",
          "speaker_id": "2",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100025074.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025074.1.13",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025074.1.14",
          "form": "조금만 부지런떨면 문제없어요! ㅎㅎ",
          "original_form": "조금만 부지런떨면 문제없어요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
