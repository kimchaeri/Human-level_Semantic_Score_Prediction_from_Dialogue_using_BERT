      "utterance": [
        {
          "id": "MDRW2100005079.1.1",
          "form": "네 족발먹었어요",
          "original_form": "네 족발먹었어요",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005079.1.2",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005079.1.3",
          "form": "네 저희는 친정에서 된장이랑 호박잎 주셔서 그걸로 저녁 먹었어요",
          "original_form": "네 저희는 친정에서 된장이랑 호박잎 주셔서 그걸로 저녁 먹었어요",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005079.1.4",
          "form": "주1회 족발드시는 날이 오늘이셨군요 ㅎㅎ",
          "original_form": "주1회 족발드시는 날이 오늘이셨군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005079.1.5",
          "form": "단골집이 있으시겠네요",
          "original_form": "단골집이 있으시겠네요",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005079.1.6",
          "form": "시댁에서 만들어줬어요",
          "original_form": "시댁에서 만들어줬어요",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005079.1.7",
          "form": "친정이랑 가까워요?",
          "original_form": "친정이랑 가까워요?",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005079.1.8",
          "form": "족발을요?",
          "original_form": "족발을요?",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005079.1.9",
          "form": "네 친정집까지 차로 15분이요 ㅎ",
          "original_form": "네 친정집까지 차로 15분이요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005079.1.10",
          "form": "와우 부러워요ㅠㅠ",
          "original_form": "와우 부러워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005079.1.11",
          "form": "저는시댁이15분거리",
          "original_form": "저는시댁이15분거리",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005079.1.12",
          "form": "시댁이 족발집하세요?",
          "original_form": "시댁이 족발집하세요?",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005079.1.13",
          "form": "친정은 왕복3시간거리네유",
          "original_form": "친정은 왕복3시간거리네유",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005079.1.14",
          "form": "아니요 어제 오빠가 군대갔을때 먹었던 수육이 기억난다해서 시어머님이 시아버님한테 수육사오랬는데 시아버님이 족발을 사와서 족발먹었어요ㅋㅋ",
          "original_form": "아니요 어제 오빠가 군대갔을때 먹었던 수육이 기억난다해서 시어머님이 시아버님한테 수육사오랬는데 시아버님이 족발을 사와서 족발먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005079.1.15",
          "form": "ㅎㅎ 헷갈리셨나보네요",
          "original_form": "ㅎㅎ 헷갈리셨나보네요",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005079.1.16",
          "form": "스벅에 새로나온 음료 드셔보셨어요?",
          "original_form": "스벅에 새로나온 음료 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005079.1.17",
          "form": "아이스크림 블렌딩 콜드브루 였던거같아요",
          "original_form": "아이스크림 블렌딩 콜드브루 였던거같아요",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005079.1.18",
          "form": "오 가봐야되는데\r\n스벅에는 항상 사람이 많아서 피했어요ㅠㅠ",
          "original_form": "오 가봐야되는데\r\n스벅에는 항상 사람이 많아서 피했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005079.1.19",
          "form": "스타벅스는 항상 많지 않아요?",
          "original_form": "스타벅스는 항상 많지 않아요?",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005079.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005079.1.21",
          "form": "저는 주로 드라이브스루로 아침에 가요",
          "original_form": "저는 주로 드라이브스루로 아침에 가요",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005079.1.22",
          "form": "맛이 비싼 더위사냥 맛이예요ㅎㅎ",
          "original_form": "맛이 비싼 더위사냥 맛이예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005079.1.23",
          "form": "요즘은 바닐라라떼에 빠져서 카페순회중이예요",
          "original_form": "요즘은 바닐라라떼에 빠져서 카페순회중이예요",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005079.1.24",
          "form": "점심시간에!",
          "original_form": "점심시간에!",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005079.1.25",
          "form": "바닐라라떼도 맛있죠 ㅎㅎ",
          "original_form": "바닐라라떼도 맛있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005079.1.26",
          "form": "스벅말고도 가세요?",
          "original_form": "스벅말고도 가세요?",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005079.1.27",
          "form": "네 요즘은 카페가 워낙많아서",
          "original_form": "네 요즘은 카페가 워낙많아서",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005079.1.28",
          "form": "글쵸 브랜드도 많고 브랜드 아닌곳도 맛난곳이 있더라구요",
          "original_form": "글쵸 브랜드도 많고 브랜드 아닌곳도 맛난곳이 있더라구요",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005079.1.29",
          "form": "회사점심시간이라 점심먹고 식당주변으로 가요",
          "original_form": "회사점심시간이라 점심먹고 식당주변으로 가요",
          "speaker_id": "2",
          "time": "20210821 20:41"
        }
      ]
