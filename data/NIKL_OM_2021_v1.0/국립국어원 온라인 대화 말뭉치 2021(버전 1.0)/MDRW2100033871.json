        },
        {
          "id": "MDRW2100033871.1.2",
          "form": "공모주는 어디 증권 넣으셨나요?",
          "original_form": "공모주는 어디 증권 넣으셨나요?",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033871.1.3",
          "form": "송모주",
          "original_form": "송모주",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033871.1.4",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033871.1.5",
          "form": "현대차 가려고요",
          "original_form": "현대차 가려고요",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033871.1.6",
          "form": "너넹 부업ㅋㅋㅋ알바죠",
          "original_form": "너넹 부업ㅋㅋㅋ알바죠",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033871.1.7",
          "form": "어디가실 거예요?",
          "original_form": "어디가실 거예요?",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033871.1.8",
          "form": "그럼요 ㅋㅋ",
          "original_form": "그럼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100033871.1.9",
          "form": "저도  현대차 보고 잇는데 ..",
          "original_form": "저도  현대차 보고 잇는데 ..",
          "speaker_id": "2",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100033871.1.10",
          "form": "다들 그런가봐요",
          "original_form": "다들 그런가봐요",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.11",
          "form": "3시이후에 청약하시는 분들도 많으셔서",
          "original_form": "3시이후에 청약하시는 분들도 많으셔서",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.12",
          "form": "결국은 비슷해질듯요",
          "original_form": "결국은 비슷해질듯요",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.13",
          "form": "고민되네요",
          "original_form": "고민되네요",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.15",
          "form": "저도요 요거 많이받아서",
          "original_form": "저도요 요거 많이받아서",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.16",
          "form": "애들 태블릿 살때 보태야 하는데",
          "original_form": "애들 태블릿 살때 보태야 하는데",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.17",
          "form": "ㅋㅋ많이 나옴 좋겠어요",
          "original_form": "ㅋㅋ많이 나옴 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.18",
          "form": "올만에 괜찮다 하네요",
          "original_form": "올만에 괜찮다 하네요",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100033871.1.19",
          "form": "오늘 바이젠셀 ㅋㅋ완전 소소해요",
          "original_form": "오늘 바이젠셀 ㅋㅋ완전 소소해요",
          "speaker_id": "1",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100033871.1.20",
          "form": "아지금 경쟁률 비슷해졌어요 ㅋㅋ",
          "original_form": "아지금 경쟁률 비슷해졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033871.1.21",
          "form": "아 삼성해야하나",
          "original_form": "아 삼성해야하나",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033871.1.22",
          "form": "어디가 많이 벌게해줄지 모르겠네요",
          "original_form": "어디가 많이 벌게해줄지 모르겠네요",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100033871.1.23",
          "form": "바이젠셀 저는... 0주받아서.,",
          "original_form": "바이젠셀 저는... 0주받아서.,",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100033871.1.24",
          "form": "아 다들 못받았어요",
          "original_form": "아 다들 못받았어요",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100033871.1.25",
          "form": "저희 가족도 저만 받았어요",
          "original_form": "저희 가족도 저만 받았어요",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100033871.1.26",
          "form": "처음 공모주..할땐 중복도되고 해서",
          "original_form": "처음 공모주..할땐 중복도되고 해서",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100033871.1.27",
          "form": "요전에 많이 받아서 대박나세요 ㅎㅎ",
          "original_form": "요전에 많이 받아서 대박나세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
