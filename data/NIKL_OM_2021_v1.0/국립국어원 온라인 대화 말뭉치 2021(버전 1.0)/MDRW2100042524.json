      "utterance": [
        {
          "id": "MDRW2100042524.1.1",
          "form": "그거 먹는다고 살쪘으면 저는 이미 몸무게 세자릿수거든요",
          "original_form": "그거 먹는다고 살쪘으면 저는 이미 몸무게 세자릿수거든요",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042524.1.2",
          "form": "맨날 떡볶이나 좋아하고",
          "original_form": "맨날 떡볶이나 좋아하고",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042524.1.3",
          "form": "근데 떡볶이는 엄마도 좋아하잖아요 ㅋㅋ",
          "original_form": "근데 떡볶이는 엄마도 좋아하잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042524.1.4",
          "form": "맛있잖아 설탕 많이 들어가서 그래",
          "original_form": "맛있잖아 설탕 많이 들어가서 그래",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042524.1.5",
          "form": "ㅠㅠ 알거든요",
          "original_form": "ㅠㅠ 알거든요",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042524.1.6",
          "form": "살빼면 사줄게",
          "original_form": "살빼면 사줄게",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042524.1.7",
          "form": "살 안빼두 먹을수있긴 하지만 참고할게요",
          "original_form": "살 안빼두 먹을수있긴 하지만 참고할게요",
          "speaker_id": "2",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042524.1.8",
          "form": "빼고 먹어야지 이녀석이",
          "original_form": "빼고 먹어야지 이녀석이",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042524.1.9",
          "form": "같이 엽떡 먹어요 중국당면 넣어서 ㅋㅋㅋ",
          "original_form": "같이 엽떡 먹어요 중국당면 넣어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042524.1.10",
          "form": "엽떡 신맛 나왔던데 아니",
          "original_form": "엽떡 신맛 나왔던데 아니",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042524.1.11",
          "form": "신맛이요??",
          "original_form": "신맛이요??",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042524.1.12",
          "form": "신메뉴",
          "original_form": "신메뉴",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042524.1.13",
          "form": "아 로제랑 ㅉ장이요 ㅋㅋㅋ",
          "original_form": "아 로제랑 ㅉ장이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042524.1.14",
          "form": "응 맞아 로제 맛있더라",
          "original_form": "응 맞아 로제 맛있더라",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042524.1.15",
          "form": "그거 나온지 좀 됐는데 ㅠ 짜장은 맛없대요",
          "original_form": "그거 나온지 좀 됐는데 ㅠ 짜장은 맛없대요",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042524.1.16",
          "form": "집에서 자장떡볶이나 만들어먹어요",
          "original_form": "집에서 자장떡볶이나 만들어먹어요",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042524.1.17",
          "form": "중국당면도 맛있는데 파는데가 없네",
          "original_form": "중국당면도 맛있는데 파는데가 없네",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042524.1.18",
          "form": "저희가 시골에 살아서",
          "original_form": "저희가 시골에 살아서{emoji:…}",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042524.1.19",
          "form": "그래도 식료품가게 많은데 대용량만 있어서 집에서 못쓰겠더라",
          "original_form": "그래도 식료품가게 많은데 대용량만 있어서 집에서 못쓰겠더라",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042524.1.20",
          "form": "당면도 살찌니까 많이 못먹는다구요",
          "original_form": "당면도 살찌니까 많이 못먹는다구요",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042524.1.21",
          "form": "언제부터 그런거 많이 신경썼대",
          "original_form": "언제부터 그런거 많이 신경썼대",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042524.1.22",
          "form": "다이어트 시작하고부터˜",
          "original_form": "다이어트 시작하고부터˜",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042524.1.23",
          "form": "근데 로제가 왜 로제냐??",
          "original_form": "근데 로제가 왜 로제냐??",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042524.1.24",
          "form": "뜻은 저도 모르는데",
          "original_form": "뜻은 저도 모르는데",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042524.1.25",
          "form": "그 로제 파스타라고 토마토소스랑 크림소스랑 섞은거 있어서",
          "original_form": "그 로제 파스타라고 토마토소스랑 크림소스랑 섞은거 있어서",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042524.1.26",
          "form": "거기서따온거일듯한",
          "original_form": "거기서따온거일듯한",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042524.1.27",
          "form": "근데 토마토 맛 안나던데",
          "original_form": "근데 토마토 맛 안나던데",
          "speaker_id": "1",
          "time": "20210906 22:44"
        }
      ]
