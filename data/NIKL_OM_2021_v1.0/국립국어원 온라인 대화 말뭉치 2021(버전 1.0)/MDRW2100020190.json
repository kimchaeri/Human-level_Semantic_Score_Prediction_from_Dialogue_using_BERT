      "utterance": [
        {
          "id": "MDRW2100020190.1.1",
          "form": "모츠나베 좋아하세요?",
          "original_form": "모츠나베 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020190.1.2",
          "form": "그 달짝지근한 거요?",
          "original_form": "그 달짝지근한 거요?",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020190.1.3",
          "form": "곱창전골!",
          "original_form": "곱창전골!",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020190.1.4",
          "form": "후쿠오카갔을때 처음 먹어봤는데",
          "original_form": "후쿠오카갔을때 처음 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020190.1.5",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020190.1.6",
          "form": "후쿠오카는 돈코츠라면이..",
          "original_form": "후쿠오카는 돈코츠라면이..",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020190.1.7",
          "form": "원조이기도하고..",
          "original_form": "원조이기도하고..",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020190.1.8",
          "form": "아 라멘 진짜 역시 일본은 달라...",
          "original_form": "아 라멘 진짜 역시 일본은 달라...",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020190.1.9",
          "form": "너무 맛있어요",
          "original_form": "너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020190.1.10",
          "form": "맞아요.. 우리는 다 인스턴트인데..\n거기는 면이랑 국구릉 직접 만드니까..",
          "original_form": "맞아요.. 우리는 다 인스턴트인데..\n거기는 면이랑 국구릉 직접 만드니까..",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020190.1.11",
          "form": "진짜 요리같기도하고..",
          "original_form": "진짜 요리같기도하고..",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020190.1.12",
          "form": "옆에 진짜 일본사람이고 일본어 들리고 그러니까 더 맛있게 느껴지는 기분이었어요ㅋㅋ",
          "original_form": "옆에 진짜 일본사람이고 일본어 들리고 그러니까 더 맛있게 느껴지는 기분이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020190.1.13",
          "form": "ㅋㅋ 근데 원래 맛자체가 퀄이 좋아요..ㅋㅋ \n유명한 라면집에서 인스턴트?비슷하게 선물용으로 팔거든요..",
          "original_form": "ㅋㅋ 근데 원래 맛자체가 퀄이 좋아요..ㅋㅋ \n유명한 라면집에서 인스턴트?비슷하게 선물용으로 팔거든요..",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020190.1.14",
          "form": "그거 사와서 나중에 한국에서 먹어도.. \n진짜 개꿀맛임..",
          "original_form": "그거 사와서 나중에 한국에서 먹어도.. \n진짜 개꿀맛임..",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020190.1.15",
          "form": "이치란라멘인가?",
          "original_form": "이치란라멘인가?",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020190.1.16",
          "form": "가게명은 오래되서 까먹었어요..ㅋㅋㅋ",
          "original_form": "가게명은 오래되서 까먹었어요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020190.1.17",
          "form": "ㅋㅋㅋㅋ어쨋든",
          "original_form": "ㅋㅋㅋㅋ어쨋든",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020190.1.18",
          "form": "라멘 좋아요....",
          "original_form": "라멘 좋아요....",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020190.1.19",
          "form": "다른거 일본음식 좋아하는거 잇어요?",
          "original_form": "다른거 일본음식 좋아하는거 잇어요?",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020190.1.20",
          "form": "일존에서 회전초밥 가봣어여?",
          "original_form": "일존에서 회전초밥 가봣어여?",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020190.1.21",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020190.1.22",
          "form": "태블릿?으로 주문해서",
          "original_form": "태블릿?으로 주문해서",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020190.1.23",
          "form": "기차같은게 슝 와서 초밥 주는거 넘 재밌었어요",
          "original_form": "기차같은게 슝 와서 초밥 주는거 넘 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020190.1.24",
          "form": "ㅋㅋㅋ 한국보다 현지가 더 싼데.. \n맛잇고 재미도 있고.. ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 한국보다 현지가 더 싼데.. \n맛잇고 재미도 있고.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:24"
        }
      ]
    }
