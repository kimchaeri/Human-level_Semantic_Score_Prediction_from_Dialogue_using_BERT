      "utterance": [
        {
          "id": "MDRW2100012790.1.1",
          "form": "중식 좋아하세요?!",
          "original_form": "중식 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012790.1.2",
          "form": "네네!!중식은 즐겨먹어요 ㅎㅎ",
          "original_form": "네네!!중식은 즐겨먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012790.1.3",
          "form": "탕수육 좋아해요",
          "original_form": "탕수육 좋아해요",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012790.1.4",
          "form": "짜장 vs 짬뽕은요?!",
          "original_form": "짜장 vs 짬뽕은요?!",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012790.1.5",
          "form": "이건 완전 그 식당따라서요!ㅋㅋ짬뽕 맛집에서는 짬뽕 짜장면 맛집은짜장면이요!",
          "original_form": "이건 완전 그 식당따라서요!ㅋㅋ짬뽕 맛집에서는 짬뽕 짜장면 맛집은짜장면이요!",
          "speaker_id": "2",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100012790.1.6",
          "form": "기본은 짜장면이 더 좋은거같긴해요 ㅋㅋ",
          "original_form": "기본은 짜장면이 더 좋은거같긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100012790.1.7",
          "form": "ㅋㅋㅋ저랑다르군요 저는 짬뽕파랍니다",
          "original_form": "ㅋㅋㅋ저랑다르군요 저는 짬뽕파랍니다",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100012790.1.8",
          "form": "짜장 vs 간짜장?!",
          "original_form": "짜장 vs 간짜장?!",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100012790.1.9",
          "form": "전 간짜장이요!ㅋㅋㅋㅋ",
          "original_form": "전 간짜장이요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100012790.1.10",
          "form": "짬뽕도 넘좋아요 요즘같은날ㅆ~",
          "original_form": "짬뽕도 넘좋아요 요즘같은날ㅆ~{emoji:☆}",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100012790.1.11",
          "form": "날씨에!",
          "original_form": "날씨에!",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100012790.1.12",
          "form": "ㅋㅋㅋ저도 간짜장파입니다",
          "original_form": "ㅋㅋㅋ저도 간짜장파입니다",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012790.1.13",
          "form": "찍먹vs부먹은요?",
          "original_form": "찍먹vs부먹은요?",
          "speaker_id": "1",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012790.1.14",
          "form": "찍먹이요!!부먹은 참을수없어요",
          "original_form": "찍먹이요!!부먹은 참을수없어요",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012790.1.15",
          "form": "부먹파이신가요!?",
          "original_form": "부먹파이신가요!?",
          "speaker_id": "2",
          "time": "20210825 01:33"
        },
        {
          "id": "MDRW2100012790.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012790.1.17",
          "form": "찍먹이죠ㅋㅋㅋㅋ",
          "original_form": "찍먹이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012790.1.18",
          "form": "부먹은 정말",
          "original_form": "부먹은 정말",
          "speaker_id": "1",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012790.1.19",
          "form": "있을수없어요",
          "original_form": "있을수없어요",
          "speaker_id": "1",
          "time": "20210825 01:39"
        },
        {
          "id": "MDRW2100012790.1.20",
          "form": "하 다행이네요 부먹이시면 대화 그만할뻔했어요....!",
          "original_form": "하 다행이네요 부먹이시면 대화 그만할뻔했어요....!",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100012790.1.21",
          "form": "서울에 사시나요??",
          "original_form": "서울에 사시나요??",
          "speaker_id": "2",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100012790.1.22",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100012790.1.23",
          "form": "부먹이 어떻게 가능한일일까요?",
          "original_form": "부먹이 어떻게 가능한일일까요?",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100012790.1.24",
          "form": "정말 이해할수가없어요",
          "original_form": "정말 이해할수가없어요",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100012790.1.25",
          "form": "탕수육을 안먹는게 낫지않나요?",
          "original_form": "탕수육을 안먹는게 낫지않나요?",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100012790.1.26",
          "form": "그러니가요...눅눅한 탕수육은 탕수육이아니죠ㅜㅜㅜ",
          "original_form": "그러니가요...눅눅한 탕수육은 탕수육이아니죠ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100012790.1.27",
          "form": "경기도에 진짜 맛있는 짜장면집 있어서 추천해드리려고했어요 ㅎㅎ 좀 멀지만 ㅜㅠ",
          "original_form": "경기도에 진짜 맛있는 짜장면집 있어서 추천해드리려고했어요 ㅎㅎ 좀 멀지만 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 01:48"
        },
        {
          "id": "MDRW2100012790.1.28",
          "form": "어딘가요?!!!",
          "original_form": "어딘가요?!!!",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012790.1.29",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012790.1.30",
          "form": "중국집맛있는곳이",
          "original_form": "중국집맛있는곳이",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012790.1.31",
          "form": "요즘 잘 없잖아요ㅠㅠ",
          "original_form": "요즘 잘 없잖아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012790.1.32",
          "form": "경기도 양주에 천안문 이라는곳인데",
          "original_form": "경기도 양주에 천안문 이라는곳인데",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012790.1.33",
          "form": "간짜장이 진짜 끝내줘요ㅜㅜ면발도 그렇고 짬뽕도 맛있어서",
          "original_form": "간짜장이 진짜 끝내줘요ㅜㅜ면발도 그렇고 짬뽕도 맛있어서",
          "speaker_id": "2",
          "time": "20210825 01:53"
        }
      ]
