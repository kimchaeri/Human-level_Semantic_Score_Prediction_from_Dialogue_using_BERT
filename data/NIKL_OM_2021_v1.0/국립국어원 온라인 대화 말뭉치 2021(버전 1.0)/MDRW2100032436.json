      "utterance": [
        {
          "id": "MDRW2100032436.1.1",
          "form": "걔는 성인이었어?",
          "original_form": "걔는 성인이었어?",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032436.1.2",
          "form": "걍 알거 다아는 성인 여성이 성인 여성을... 합의하에 하는 건데",
          "original_form": "걍 알거 다아는 성인 여성이 성인 여성을... 합의하에 하는 건데",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100032436.1.3",
          "form": "너무 갑작스러워서",
          "original_form": "너무 갑작스러워서",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100032436.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100032436.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ딱지 붙은 작품이야?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ딱지 붙은 작품이야?",
          "speaker_id": "2",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100032436.1.6",
          "form": "아니었을걸?",
          "original_form": "아니었을걸?",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100032436.1.7",
          "form": "붙으면 잔인쪽이기도 하고",
          "original_form": "붙으면 잔인쪽이기도 하고",
          "speaker_id": "1",
          "time": "20210905 17:31"
        },
        {
          "id": "MDRW2100032436.1.8",
          "form": "근데 글케 요즘 나오는 작품치고도",
          "original_form": "근데 글케 요즘 나오는 작품치고도",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.9",
          "form": "잔인하진 않아서 아닐 듯하고",
          "original_form": "잔인하진 않아서 아닐 듯하고",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.10",
          "form": "그런씬이 주요 전개도 아니어서",
          "original_form": "그런씬이 주요 전개도 아니어서",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.11",
          "form": "아무튼 아닐듯",
          "original_form": "아무튼 아닐듯",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.12",
          "form": "오옹...",
          "original_form": "오옹...",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.13",
          "form": "근데 뭐 재미있긴 함",
          "original_form": "근데 뭐 재미있긴 함",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.14",
          "form": "무슨공무슨수야",
          "original_form": "무슨공무슨수야",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.16",
          "form": "이런게 궁금함",
          "original_form": "이런게 궁금함",
          "speaker_id": "2",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100032436.1.17",
          "form": "그냥 그런 것도 아닌데 미인 정보상(도도 냉철 도s)과 그녀의",
          "original_form": "그냥 그런 것도 아닌데 미인 정보상(도도 냉철 도s)과 그녀의",
          "speaker_id": "1",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032436.1.18",
          "form": "충실한 부하들의",
          "original_form": "충실한 부하들의",
          "speaker_id": "1",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032436.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032436.1.20",
          "form": "한페이지 정도 나온건데",
          "original_form": "한페이지 정도 나온건데",
          "speaker_id": "1",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032436.1.21",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032436.1.22",
          "form": "깜짝 놀랏음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "깜짝 놀랏음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100032436.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.24",
          "form": "난 나의 마녀는 역시",
          "original_form": "난 나의 마녀는 역시",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.25",
          "form": "두번째가 좋았어",
          "original_form": "두번째가 좋았어",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.26",
          "form": "이름까먹었어",
          "original_form": "이름까먹었어",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.27",
          "form": "금발즈",
          "original_form": "금발즈",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.28",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.29",
          "form": "서사좋긴하지",
          "original_form": "서사좋긴하지",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032436.1.30",
          "form": "남주가 어릴때 구해준 여주",
          "original_form": "남주가 어릴때 구해준 여주",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.31",
          "form": "코델리아랑 미카엘",
          "original_form": "코델리아랑 미카엘",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.32",
          "form": "하 진짜 애기때부터",
          "original_form": "하 진짜 애기때부터",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.33",
          "form": "벤츠",
          "original_form": "벤츠",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.34",
          "form": "미카엘 너무 이상적인 남주라ㅋㅋㅋㅋㅋ",
          "original_form": "미카엘 너무 이상적인 남주라ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.35",
          "form": "집안일도 잘함",
          "original_form": "집안일도 잘함",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.36",
          "form": "사랑도넘침",
          "original_form": "사랑도넘침",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋ 조..좋앗다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋ 조..좋앗다",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.38",
          "form": "맞아 사랑이 넘쳤다",
          "original_form": "맞아 사랑이 넘쳤다",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100032436.1.39",
          "form": "모두에게 상냥하고 친절한 남주, 그러나 여주한테 한결같고 순정적인ㅋㅋㅋㅋ",
          "original_form": "모두에게 상냥하고 친절한 남주, 그러나 여주한테 한결같고 순정적인ㅋㅋㅋㅋ",
