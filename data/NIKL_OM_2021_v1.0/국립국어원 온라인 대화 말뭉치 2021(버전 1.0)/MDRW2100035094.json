      "utterance": [
        {
          "id": "MDRW2100035094.1.1",
          "form": "마장요",
          "original_form": "마장요",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035094.1.2",
          "form": "드셔보셨어용?",
          "original_form": "드셔보셨어용?",
          "speaker_id": "1",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035094.1.3",
          "form": "동네에 마주보고밀키트매장이",
          "original_form": "동네에 마주보고밀키트매장이",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035094.1.4",
          "form": "2군데로 일주일사이로 오픈했어요",
          "original_form": "2군데로 일주일사이로 오픈했어요",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.5",
          "form": "밀키트 매장이 두곳이나 있다니 부러워요ㅋㅋㅋㅋㅋ",
          "original_form": "밀키트 매장이 두곳이나 있다니 부러워요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.6",
          "form": "들어가보고 사본건없어요",
          "original_form": "들어가보고 사본건없어요",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.7",
          "form": "부대찌개랑 찜닭 먹어봤는데",
          "original_form": "부대찌개랑 찜닭 먹어봤는데",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.8",
          "form": "전에 인터넷이나 마트에서는 사본적잇어요",
          "original_form": "전에 인터넷이나 마트에서는 사본적잇어요",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.9",
          "form": "배달 음식보다는 뭔가 조미료가 덜한맛이에요",
          "original_form": "배달 음식보다는 뭔가 조미료가 덜한맛이에요",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.10",
          "form": "감바스 맛있더라구요",
          "original_form": "감바스 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.11",
          "form": "아~ 구런가요?!9",
          "original_form": "아~ 구런가요?!9",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035094.1.12",
          "form": "전하나사면 듈이먹긴 양이 아쉽더라구요ㅠㅠ",
          "original_form": "전하나사면 듈이먹긴 양이 아쉽더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.13",
          "form": "새우 좋아하시면 꼭 드셔보세용!",
          "original_form": "새우 좋아하시면 꼭 드셔보세용!",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.14",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.15",
          "form": "2인분이라고 적혀있는대",
          "original_form": "2인분이라고 적혀있는대",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.16",
          "form": "배부른 1인분이에요",
          "original_form": "배부른 1인분이에요",
          "speaker_id": "1",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.17",
          "form": "또 막상 할인하고 하면 배달이 싸서 그냥 완제품시키자 이렇게되요",
          "original_form": "또 막상 할인하고 하면 배달이 싸서 그냥 완제품시키자 이렇게되요",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.18",
          "form": "맞아요 먹성괜찮은 1인이에요ㅠ",
          "original_form": "맞아요 먹성괜찮은 1인이에요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:32"
        },
        {
          "id": "MDRW2100035094.1.19",
          "form": "저두 그래서 밀키트는 가끔 시켜요ㅋㅋㅋㅋㅋ",
          "original_form": "저두 그래서 밀키트는 가끔 시켜요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035094.1.20",
          "form": "좀 퀄리티 괜찮은건",
          "original_form": "좀 퀄리티 괜찮은건",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035094.1.21",
          "form": "사먹는거랑 금액이 큰 차이가 없어서",
          "original_form": "사먹는거랑 금액이 큰 차이가 없어서",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035094.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:33"
        },
        {
          "id": "MDRW2100035094.1.23",
          "form": "요새진짜 없는게 없긴해요",
          "original_form": "요새진짜 없는게 없긴해요",
          "speaker_id": "2",
          "time": "20210829 02:33"
        }
      ]
    }
