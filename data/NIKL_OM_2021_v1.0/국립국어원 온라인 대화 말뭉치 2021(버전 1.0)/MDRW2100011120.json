      "utterance": [
        {
          "id": "MDRW2100011120.1.1",
          "form": "요즘 12시입니다ㅋㅋ",
          "original_form": "요즘 12시입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011120.1.2",
          "form": "시간 잘 모르시는걸 보니",
          "original_form": "시간 잘 모르시는걸 보니",
          "speaker_id": "2",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011120.1.3",
          "form": "음주를 즐기지 않으시네요!ㅋㅋ",
          "original_form": "음주를 즐기지 않으시네요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011120.1.4",
          "form": "코로나땜에 일찍 끝나는군요ㅎ",
          "original_form": "코로나땜에 일찍 끝나는군요ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011120.1.5",
          "form": "전 술 일년에 한번먹을까말까요ㅎ",
          "original_form": "전 술 일년에 한번먹을까말까요ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011120.1.6",
          "form": "요즘 회식도 없고 회사서 강요하는 스탈도 아니라서",
          "original_form": "요즘 회식도 없고 회사서 강요하는 스탈도 아니라서",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011120.1.7",
          "form": "ㅎㅎ회식 줄어든거는 좋네요!",
          "original_form": "ㅎㅎ회식 줄어든거는 좋네요!",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011120.1.8",
          "form": "예전에는 회식 참 많았는데",
          "original_form": "예전에는 회식 참 많았는데",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011120.1.9",
          "form": "고기먹어서 전 좋던데ㅋ",
          "original_form": "고기먹어서 전 좋던데ㅋ",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011120.1.10",
          "form": "다들 싫어하더라구요",
          "original_form": "다들 싫어하더라구요",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011120.1.11",
          "form": "ㅋㅋ좋은 사람과 먹으면 좋은데",
          "original_form": "ㅋㅋ좋은 사람과 먹으면 좋은데",
          "speaker_id": "2",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011120.1.12",
          "form": "아빠뻘 아저씨들과는....",
          "original_form": "아빠뻘 아저씨들과는....",
          "speaker_id": "2",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011120.1.13",
          "form": "ㅎㅎ전 뭘 먹냐가 중요해서",
          "original_form": "ㅎㅎ전 뭘 먹냐가 중요해서",
          "speaker_id": "1",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011120.1.14",
          "form": "전혀불편하지 않던데 다들 싫어해요",
          "original_form": "전혀불편하지 않던데 다들 싫어해요",
          "speaker_id": "1",
          "time": "20210824 01:24"
        },
        {
          "id": "MDRW2100011120.1.15",
          "form": "성별이 같으면 직장에선 더 불편한가봐요 어렵고",
          "original_form": "성별이 같으면 직장에선 더 불편한가봐요 어렵고",
          "speaker_id": "1",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011120.1.16",
          "form": "음 한달에 5번이여도요?ㅎㅎ",
          "original_form": "음 한달에 5번이여도요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011120.1.17",
          "form": "네 전 먹는거 좋아해요ㅋ 술강요하면 싫겠지만",
          "original_form": "네 전 먹는거 좋아해요ㅋ 술강요하면 싫겠지만",
          "speaker_id": "1",
          "time": "20210824 01:27"
        },
        {
          "id": "MDRW2100011120.1.18",
          "form": "상사분들 크게 불편해하지 않아서그런가봐요",
          "original_form": "상사분들 크게 불편해하지 않아서그런가봐요",
          "speaker_id": "1",
          "time": "20210824 01:29"
        },
        {
          "id": "MDRW2100011120.1.19",
          "form": "ㅋㅋ저도 불편하진 않았는데",
          "original_form": "ㅋㅋ저도 불편하진 않았는데",
          "speaker_id": "2",
          "time": "20210824 01:29"
        },
        {
          "id": "MDRW2100011120.1.20",
          "form": "그 시간에 차라리 친구와ㅋㅋ",
          "original_form": "그 시간에 차라리 친구와ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:29"
        },
        {
          "id": "MDRW2100011120.1.21",
          "form": "글케따지면 친구랑 노는게 좋죠ㅎㅎ",
          "original_form": "글케따지면 친구랑 노는게 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011120.1.22",
          "form": "전 점심밥멤버가 이사님이라서",
          "original_form": "전 점심밥멤버가 이사님이라서",
          "speaker_id": "1",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011120.1.23",
          "form": "58  60  이런분들이랑 놀아요ㅡㅡ",
          "original_form": "58  60  이런분들이랑 놀아요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 01:30"
        },
        {
          "id": "MDRW2100011120.1.24",
          "form": "ㅋㅋ그거 싫어요!!! 전직장에서도 높아봐야 차장이었는데",
          "original_form": "ㅋㅋ그거 싫어요!!! 전직장에서도 높아봐야 차장이었는데",
          "speaker_id": "2",
          "time": "20210824 01:31"
        },
        {
          "id": "MDRW2100011120.1.25",
          "form": "ㅎㅎ 성격이 이상한지 친구처럼 지내가지고ㅎ",
          "original_form": "ㅎㅎ 성격이 이상한지 친구처럼 지내가지고ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:32"
        },
        {
          "id": "MDRW2100011120.1.26",
          "form": "친화력 좋으면 나중에 시댁에 잘하시겠어요 ㅎㅎ",
          "original_form": "친화력 좋으면 나중에 시댁에 잘하시겠어요 ㅎㅎ",
          "speaker_id": "2",
