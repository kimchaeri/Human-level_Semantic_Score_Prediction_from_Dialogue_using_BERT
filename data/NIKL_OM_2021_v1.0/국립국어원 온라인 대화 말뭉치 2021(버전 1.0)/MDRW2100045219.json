      "utterance": [
        {
          "id": "MDRW2100045219.1.1",
          "form": "당연히 했죵",
          "original_form": "당연히 했죵",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045219.1.2",
          "form": "맞춰보실래요?",
          "original_form": "맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045219.1.3",
          "form": "좋아요 후후",
          "original_form": "좋아요 후후",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045219.1.4",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045219.1.5",
          "form": "음 쌀은 그냥 쌀로만 먹었어요 ㅋㅋㅋ",
          "original_form": "음 쌀은 그냥 쌀로만 먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045219.1.6",
          "form": "ㅋㅋㅋㅋ그렇담 밥이랑 곁들여 먹는 음식이군요!",
          "original_form": "ㅋㅋㅋㅋ그렇담 밥이랑 곁들여 먹는 음식이군요!",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045219.1.7",
          "form": "그쳐 아무래도 밥이랑 같이 먹으면 맛이 좋아용",
          "original_form": "그쳐 아무래도 밥이랑 같이 먹으면 맛이 좋아용",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045219.1.8",
          "form": "국물요리인가요?",
          "original_form": "국물요리인가요?",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045219.1.9",
          "form": "넵 빨간 국물 요리라고 할 수 있는 거 같아여",
          "original_form": "넵 빨간 국물 요리라고 할 수 있는 거 같아여",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045219.1.10",
          "form": "고기가 들어가나요!",
          "original_form": "고기가 들어가나요!",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045219.1.11",
          "form": "넹 고기 들어가용",
          "original_form": "넹 고기 들어가용",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045219.1.12",
          "form": "야채가 들어가나요?",
          "original_form": "야채가 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045219.1.13",
          "form": "꼭 안넣어도 되긴하는데 넣었어요 돼지고기 !",
          "original_form": "꼭 안넣어도 되긴하는데 넣었어요 돼지고기 !",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045219.1.14",
          "form": "앗 고기가 필수가 아니군요",
          "original_form": "앗 고기가 필수가 아니군요",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045219.1.15",
          "form": "매운 음식인가요?",
          "original_form": "매운 음식인가요?",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045219.1.16",
          "form": "넵 빨간 국물 요리면 아무래도 맵죻ㅎ",
          "original_form": "넵 빨간 국물 요리면 아무래도 맵죻ㅎ",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045219.1.17",
          "form": "김치가 들어가나요?",
          "original_form": "김치가 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045219.1.18",
          "form": "오 넹 김치가 제일 중요해요 ㅋㅋㅋ",
          "original_form": "오 넹 김치가 제일 중요해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045219.1.19",
          "form": "해산물도 들어가나요?",
          "original_form": "해산물도 들어가나요?",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045219.1.20",
          "form": "아녀 ! 해산물 넣는건 못봤네염",
          "original_form": "아녀 ! 해산물 넣는건 못봤네염",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045219.1.21",
          "form": "으으으으음 국 종류인가요?",
          "original_form": "으으으으음 국 종류인가요?",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045219.1.22",
          "form": "국 요리 종류인가요?\r\n~~국",
          "original_form": "국 요리 종류인가요?\r\n~~국",
          "speaker_id": "1",
