      "utterance": [
        {
          "id": "MDRW2100039089.1.1",
          "form": "저희는 4인 가족에 강아지 한마리 있습니다",
          "original_form": "저희는 4인 가족에 강아지 한마리 있습니다",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039089.1.2",
          "form": "그쪽은여?",
          "original_form": "그쪽은여?",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039089.1.3",
          "form": "전 혼자 살아요",
          "original_form": "전 혼자 살아요",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039089.1.4",
          "form": "대가족이시네요",
          "original_form": "대가족이시네요",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039089.1.5",
          "form": "저도 혼자 살아보고싶어요 강아지는 데리구",
          "original_form": "저도 혼자 살아보고싶어요 강아지는 데리구",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039089.1.6",
          "form": "어때요 혼자사는거",
          "original_form": "어때요 혼자사는거",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.7",
          "form": "좋죠 아주",
          "original_form": "좋죠 아주",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.8",
          "form": "다조아요 .. 식사가 한정된거말곤",
          "original_form": "다조아요 .. 식사가 한정된거말곤",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.9",
          "form": "돈이아주마느면 그거도 상관없겟네용",
          "original_form": "돈이아주마느면 그거도 상관없겟네용",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.10",
          "form": "무섭다거나 할일이 넘 많다거나 그런건 없어요?",
          "original_form": "무섭다거나 할일이 넘 많다거나 그런건 없어요?",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.11",
          "form": "돈많은면 장땡이죠 넵",
          "original_form": "돈많은면 장땡이죠 넵",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.12",
          "form": "무섭다라... 갠차나요 그정돈",
          "original_form": "무섭다라... 갠차나요 그정돈",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.13",
          "form": "할일이 넘 많다라.. 청소 설거지 귀찮긴하네요",
          "original_form": "할일이 넘 많다라.. 청소 설거지 귀찮긴하네요",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039089.1.14",
          "form": "집안일 잘하시나봐요",
          "original_form": "집안일 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039089.1.15",
          "form": "글쵸 자취경력이 몇년인데",
          "original_form": "글쵸 자취경력이 몇년인데",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039089.1.16",
          "form": "걍뭐 살림꾼이죠",
          "original_form": "걍뭐 살림꾼이죠",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039089.1.17",
          "form": "오올 예전에 장래희망이 현모양처..이런거 있었어요??",
          "original_form": "오올 예전에 장래희망이 현모양처..이런거 있었어요??",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039089.1.18",
          "form": "오....그럴리가요",
          "original_form": "오....그럴리가요",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039089.1.19",
          "form": "결혼했는데 남편이 집안일 하나도 안도와주면 어떻게 할거에요?",
          "original_form": "결혼했는데 남편이 집안일 하나도 안도와주면 어떻게 할거에요?",
          "speaker_id": "2",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039089.1.20",
          "form": "결혼을 안해야죠 그런인간이라걸랑",
          "original_form": "결혼을 안해야죠 그런인간이라걸랑",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039089.1.21",
          "form": "결혼전엔 막 니 손에 물한방울 안묻히게 해줄게!! 이러다가 돌변할수도있잖아요",
          "original_form": "결혼전엔 막 니 손에 물한방울 안묻히게 해줄게!! 이러다가 돌변할수도있잖아요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        }
      ]
