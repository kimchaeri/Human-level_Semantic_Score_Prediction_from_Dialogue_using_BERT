      "utterance": [
        {
          "id": "MDRW2100006905.1.1",
          "form": "다이어트 하세요?",
          "original_form": "다이어트 하세요?",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006905.1.2",
          "form": "따로 하지는 않고 운동은 열심히 해요 ㅋㅋ",
          "original_form": "따로 하지는 않고 운동은 열심히 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006905.1.3",
          "form": "오 어떤 운동 하세요?",
          "original_form": "오 어떤 운동 하세요?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006905.1.4",
          "form": "저는 걷는게 끝이에요ㅎㅎ",
          "original_form": "저는 걷는게 끝이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006905.1.5",
          "form": "저도 걷는거 좋아해요 ㅋㅋ",
          "original_form": "저도 걷는거 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006905.1.6",
          "form": "달리기도 좋더라구요 ㅋㅋ",
          "original_form": "달리기도 좋더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006905.1.7",
          "form": "맞아요 달리기도 좋죠",
          "original_form": "맞아요 달리기도 좋죠",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006905.1.8",
          "form": "보통 퇴근하고 하세요?",
          "original_form": "보통 퇴근하고 하세요?",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006905.1.9",
          "form": "네 ㅋㅋ 저녁에 많이 하는 것 같아요",
          "original_form": "네 ㅋㅋ 저녁에 많이 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006905.1.10",
          "form": "부자님은요?",
          "original_form": "부자님은요?",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006905.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006905.1.12",
          "form": "근데 사람이 많아서 새벽으로 옮길까 생각중이에요ㅠㅠ",
          "original_form": "근데 사람이 많아서 새벽으로 옮길까 생각중이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006905.1.13",
          "form": "아 ㅜㅜ 코로난데도 사람이 많은가봐요",
          "original_form": "아 ㅜㅜ 코로난데도 사람이 많은가봐요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006905.1.14",
          "form": "네ㅠㅠ마스크 끼고 하는데 안답답하세요 운동?",
          "original_form": "네ㅠㅠ마스크 끼고 하는데 안답답하세요 운동?",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006905.1.15",
          "form": "맞아요 ㅜㅜ 숨이 너무 차더라구요 그래두 나중에 마스크 벗으면 더 잘할수 있겠거니 해요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅜㅜ 숨이 너무 차더라구요 그래두 나중에 마스크 벗으면 더 잘할수 있겠거니 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006905.1.16",
          "form": "맞아요ㅎㅎ저는 요새 필라테스 알아보고 있어요!",
          "original_form": "맞아요ㅎㅎ저는 요새 필라테스 알아보고 있어요!",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006905.1.17",
          "form": "필라테스는 어떠세요?",
          "original_form": "필라테스는 어떠세요?",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006905.1.18",
          "form": "요새 많이들 하더라구요!",
          "original_form": "요새 많이들 하더라구요!",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006905.1.19",
          "form": "어떨지 궁금해요",
          "original_form": "어떨지 궁금해요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006905.1.20",
          "form": "저두요! 근력을 키워 준다는데 운동 고르기도 쉽지 않네요ㅎㅎ",
          "original_form": "저두요! 근력을 키워 준다는데 운동 고르기도 쉽지 않네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:10"
        }
      ]
