          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.4",
          "form": "오늘 날씨 어땠어?",
          "original_form": "오늘 날씨 어땠어?",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.5",
          "form": "대구는 어때?? 날씨?",
          "original_form": "대구는 어때?? 날씨?",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.6",
          "form": "서울은 선선해졌어",
          "original_form": "서울은 선선해졌어",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.7",
          "form": "대구는... 비오다 말다했어",
          "original_form": "대구는... 비오다 말다했어",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.8",
          "form": "헐 그렇구나",
          "original_form": "헐 그렇구나",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.9",
          "form": "긴팔 입어도 돼",
          "original_form": "긴팔 입어도 돼",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.10",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.11",
          "form": "여긴 조금 흐리긴했는데",
          "original_form": "여긴 조금 흐리긴했는데",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.12",
          "form": "아 아침에 왔나?",
          "original_form": "아 아침에 왔나?",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026111.1.14",
          "form": "하여튼 일과 중에는",
          "original_form": "하여튼 일과 중에는",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026111.1.15",
          "form": "안왔어",
          "original_form": "안왔어",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026111.1.16",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026111.1.17",
          "form": "난 아침에 일어나니",
          "original_form": "난 아침에 일어나니",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026111.1.18",
          "form": "비가 오더라",
          "original_form": "비가 오더라",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026111.1.19",
          "form": "문학소설 주인공처럼 우산 쓰고 밥먹으러 갔어",
          "original_form": "문학소설 주인공처럼 우산 쓰고 밥먹으러 갔어",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026111.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
