      "utterance": [
        {
          "id": "MDRW2100019245.1.1",
          "form": "저 요즘에는 없어요!",
          "original_form": "저 요즘에는 없어요!",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019245.1.2",
          "form": "name1님은 있으신가요?",
          "original_form": "&name1&님은 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019245.1.3",
          "form": "저 요즘엔 카리나 너무 예뻐서 ㅋㅋㅋㅋㅋ",
          "original_form": "저 요즘엔 카리나 너무 예뻐서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100019245.1.4",
          "form": "엄텅 이쁘죠.. 카리나...",
          "original_form": "엄텅 이쁘죠.. 카리나...",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019245.1.5",
          "form": "카리나본다고 넥스트레벨 엄청들엇더니",
          "original_form": "카리나본다고 넥스트레벨 엄청들엇더니",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019245.1.6",
          "form": "그것도 귀에서 안떠나요 ㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "그것도 귀에서 안떠나요 ㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019245.1.7",
          "form": "완전 중독성 있죠",
          "original_form": "완전 중독성 있죠",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019245.1.8",
          "form": "처음 들엇을땐 가사가 좀... 이랫는데ㅋㅋㅋㅋㅋ",
          "original_form": "처음 들엇을땐 가사가 좀... 이랫는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019245.1.9",
          "form": "그 별로인가사가 계속 맴돌아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 별로인가사가 계속 맴돌아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019245.1.10",
          "form": "마자요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019245.1.11",
          "form": "입에 달라붙어서 떨어지지않아요 ㅋㅋㅋㅋ",
          "original_form": "입에 달라붙어서 떨어지지않아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019245.1.12",
          "form": "가사가 ㅋㅋㅋㅋ",
          "original_form": "가사가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019245.1.13",
