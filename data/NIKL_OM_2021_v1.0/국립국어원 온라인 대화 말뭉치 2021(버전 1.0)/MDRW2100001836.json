      "utterance": [
        {
          "id": "MDRW2100001836.1.1",
          "form": "운동했더니 배고파",
          "original_form": "운동했더니 배고파",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001836.1.2",
          "form": "이런 뭐 좀 먹어요",
          "original_form": "이런 뭐 좀 먹어요",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001836.1.3",
          "form": "뭘 먹지? 지금 시간에",
          "original_form": "뭘 먹지? 지금 시간에",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001836.1.4",
          "form": "지금 먹을 수 있는 거라곤 라면 밖에 없는데",
          "original_form": "지금 먹을 수 있는 거라곤 라면 밖에 없는데",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001836.1.5",
          "form": "라면에 계란 탁 파송송 후루룹 !!!",
          "original_form": "라면에 계란 탁 파송송 후루룹 !!!",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001836.1.6",
          "form": "크아 맛있겠다 나도 먹고 싶다",
          "original_form": "크아 맛있겠다 나도 먹고 싶다",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001836.1.7",
          "form": "아 다이어트 중이라 먹으면 안되는데",
          "original_form": "아 다이어트 중이라 먹으면 안되는데",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001836.1.8",
          "form": "무너진다아~~~!!!",
          "original_form": "무너진다아~~~!!!",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001836.1.9",
          "form": "안돼~~ ㅠㅠ",
          "original_form": "안돼~~ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001836.1.10",
          "form": "라면 참아야 하는데 ㅎㅎ",
          "original_form": "라면 참아야 하는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001836.1.11",
          "form": "배에서 계속 꼬르륵거려",
          "original_form": "배에서 계속 꼬르륵거려",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001836.1.12",
          "form": "이런 어쩌나",
          "original_form": "이런 어쩌나",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001836.1.13",
          "form": "집에 무슨 라면 있지",
          "original_form": "집에 무슨 라면 있지",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001836.1.14",
          "form": "씽크대 열어봐~ 신랑 좋아하는 건 다 있지 ㅎㅎ",
          "original_form": "씽크대 열어봐~ 신랑 좋아하는 건 다 있지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001836.1.15",
          "form": "흠랴 고민되네 ㅠ",
          "original_form": "흠랴 고민되네 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001836.1.16",
          "form": "하나 정도는 먹어도 살 안찔껴",
          "original_form": "하나 정도는 먹어도 살 안찔껴",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001836.1.17",
          "form": "그..럴까? 너무 배고파 ㅠ",
          "original_form": "그..럴까? 너무 배고파 ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001836.1.18",
          "form": "라면은 언제 먹어도 안질려 ㅎㅎ 나도 먹고프다",
          "original_form": "라면은 언제 먹어도 안질려 ㅎㅎ 나도 먹고프다",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001836.1.19",
          "form": "두 개 끓일까? ㅋㅋ",
          "original_form": "두 개 끓일까? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001836.1.20",
          "form": "촵촵 ㅎㅎ 부탁해도 될까용",
          "original_form": "촵촵 ㅎㅎ 부탁해도 될까용",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
