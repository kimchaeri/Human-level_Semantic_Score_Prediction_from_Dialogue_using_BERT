      "utterance": [
        {
          "id": "MDRW2100031715.1.1",
          "form": "이상형이 어떻게 되세요",
          "original_form": "이상형이 어떻게 되세요",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100031715.1.2",
          "form": "이상형은 rm이요",
          "original_form": "이상형은 rm이요",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100031715.1.3",
          "form": "누구죠 알엠이 비티에슨가요",
          "original_form": "누구죠 알엠이 비티에슨가요",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100031715.1.4",
          "form": "네 비티에스",
          "original_form": "네 비티에스",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100031715.1.5",
          "form": "랩몬스터요",
          "original_form": "랩몬스터요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.6",
          "form": "아 랩몬스터",
          "original_form": "아 랩몬스터",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.7",
          "form": "미안해요 잘 몰라가지구 ㅋㅋㅋ",
          "original_form": "미안해요 잘 몰라가지구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.8",
          "form": "요새 미팅은 안하시나요",
          "original_form": "요새 미팅은 안하시나요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.9",
          "form": "안해요 ㅋㅋ",
          "original_form": "안해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.10",
          "form": "코로나라 만날 기회가 더욱 좁아졌네요",
          "original_form": "코로나라 만날 기회가 더욱 좁아졌네요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.11",
          "form": "하긴 요새는 미혼도 괜찮은거같아요",
          "original_form": "하긴 요새는 미혼도 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.12",
          "form": "요개는 화상채팅으로 만난대요 ㅋㅋ",
          "original_form": "요개는 화상채팅으로 만난대요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031715.1.13",
          "form": "전 그렇게 만나고싶진 않네요ㅋㅋ",
          "original_form": "전 그렇게 만나고싶진 않네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031715.1.14",
          "form": "맞아요 굳이 생각없아면",
          "original_form": "맞아요 굳이 생각없아면",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031715.1.15",
          "form": "대구를 가야할것같아요",
          "original_form": "대구를 가야할것같아요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031715.1.16",
          "form": "대구가 낫됴 아무래도",
          "original_form": "대구가 낫됴 아무래도",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031715.1.17",
          "form": "여기 있으면 걍 비혼이네요",
          "original_form": "여기 있으면 걍 비혼이네요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031715.1.18",
          "form": "비혼 희망자는 아니긴거군요",
          "original_form": "비혼 희망자는 아니긴거군요",
