      "utterance": [
        {
          "id": "MDRW2100036710.1.1",
          "form": "11번가 아니면 네이버에서 주로 구매하는거같아요",
          "original_form": "11번가 아니면 네이버에서 주로 구매하는거같아요",
          "speaker_id": "2",
          "time": "20210831 09:57"
        },
        {
          "id": "MDRW2100036710.1.2",
          "form": "저도주로 네이버. 그리고지마켓.",
          "original_form": "저도주로 네이버. 그리고지마켓.",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100036710.1.3",
          "form": "네이버 멤버쉽 이용하세요?",
          "original_form": "네이버 멤버쉽 이용하세요?",
          "speaker_id": "2",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036710.1.4",
          "form": "네ㅎㅎ 유용히써요",
          "original_form": "네ㅎㅎ 유용히써요",
          "speaker_id": "1",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036710.1.5",
          "form": "저는 그거때문에 포인트 적립받으려고 네이버 이용해요",
          "original_form": "저는 그거때문에 포인트 적립받으려고 네이버 이용해요",
          "speaker_id": "2",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036710.1.6",
          "form": "저도요ㅋㅋㅋ 좋아요",
          "original_form": "저도요ㅋㅋㅋ 좋아요",
          "speaker_id": "1",
          "time": "20210831 10:09"
        },
        {
          "id": "MDRW2100036710.1.7",
          "form": "오늘멤버십데이에요",
          "original_form": "오늘멤버십데이에요",
          "speaker_id": "1",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100036710.1.8",
          "form": "근데 그게 문제예요ㅋㅋㅋㅋㅋ",
          "original_form": "근데 그게 문제예요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100036710.1.9",
          "form": "살거는 이미 그저께 다 사버려서",
          "original_form": "살거는 이미 그저께 다 사버려서",
          "speaker_id": "2",
          "time": "20210831 10:10"
        },
        {
          "id": "MDRW2100036710.1.10",
          "form": "괜히 멤버쉽데이라니까 뭘사야할까 계속 찾아보게되는거 있쬬 ㅜ",
          "original_form": "괜히 멤버쉽데이라니까 뭘사야할까 계속 찾아보게되는거 있쬬 ㅜ",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100036710.1.11",
          "form": "저도요ㅜ이미 다사서 ㅠ",
          "original_form": "저도요ㅜ이미 다사서 ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100036710.1.12",
          "form": "그래서 물티슈 사신거예요?",
          "original_form": "그래서 물티슈 사신거예요?",
          "speaker_id": "2",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100036710.1.13",
          "form": "도저히 살게 없어서 그냥 먹을거 사야겠어요ㅋㅋㅋ",
          "original_form": "도저히 살게 없어서 그냥 먹을거 사야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100036710.1.14",
          "form": "물티슈는 오늘의집 쿠폰줘서 사써용",
          "original_form": "물티슈는 오늘의집 쿠폰줘서 사써용",
          "speaker_id": "1",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036710.1.15",
          "form": "아~ 나중에 가입해서 사려고 봐두기만 했는데",
          "original_form": "아~ 나중에 가입해서 사려고 봐두기만 했는데",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100036710.1.16",
          "form": "오늘의집도 자주 이용하세요?",
          "original_form": "오늘의집도 자주 이용하세요?",
          "speaker_id": "2",
          "time": "20210831 10:38"
        },
        {
          "id": "MDRW2100036710.1.17",
          "form": "첨이에요ㅋㅋ",
          "original_form": "첨이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:39"
        },
        {
          "id": "MDRW2100036710.1.18",
          "form": "저는 지금 고민하다가 캔음료 묶음 샀어요ㅋㅋㅋ",
          "original_form": "저는 지금 고민하다가 캔음료 묶음 샀어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:56"
        },
