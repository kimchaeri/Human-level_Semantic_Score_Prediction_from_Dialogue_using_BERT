      "utterance": [
        {
          "id": "MDRW2100034128.1.1",
          "form": "name2님은 머 키우시는건 없나요",
          "original_form": "&name2&님은 머 키우시는건 없나요",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100034128.1.2",
          "form": "예전에 햄스터랑\r\n강아지 키웠었는데 지금은 없어용",
          "original_form": "예전에 햄스터랑\r\n강아지 키웠었는데 지금은 없어용",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034128.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034128.1.4",
          "form": "저는 고양이 사료 들고다녀요 길고앵이 주려구",
          "original_form": "저는 고양이 사료 들고다녀요 길고앵이 주려구",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034128.1.5",
          "form": "햄스터랑 강쥐",
          "original_form": "햄스터랑 강쥐",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034128.1.6",
          "form": "이뻣겠네요 저는 구피키워요",
          "original_form": "이뻣겠네요 저는 구피키워요",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034128.1.7",
          "form": "오 구피 키우시는 분들이\r\n은근 많으신거 같아요:)",
          "original_form": "오 구피 키우시는 분들이\r\n은근 많으신거 같아요:)",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100034128.1.8",
          "form": "키우기 어렵지 않나요?\r\n물고기는 안키워봐서...",
          "original_form": "키우기 어렵지 않나요?\r\n물고기는 안키워봐서...",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100034128.1.9",
          "form": "아 물고기 편해서 키워요",
          "original_form": "아 물고기 편해서 키워요",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100034128.1.10",
          "form": "산책안시켜도대요ㅋㅋ",
          "original_form": "산책안시켜도대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100034128.1.11",
          "form": "앗ㅋㅋㅋ 그러네요\r\n물고기는 산책을 안하죠ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ 그러네요\r\n물고기는 산책을 안하죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034128.1.12",
          "form": "키운지는 얼마나 되셨어요?",
          "original_form": "키운지는 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034128.1.13",
          "form": "세달밖에안됐어요",
          "original_form": "세달밖에안됐어요",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100034128.1.14",
          "form": "햄스터랑 강아지는 어디갔나요",
          "original_form": "햄스터랑 강아지는 어디갔나요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100034128.1.15",
          "form": "다들 무지개 다리를 건넜어요ㅠ",
          "original_form": "다들 무지개 다리를 건넜어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100034128.1.16",
          "form": "아 햄스터 한마리는\r\n입양 보냈었네용",
          "original_form": "아 햄스터 한마리는\r\n입양 보냈었네용",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100034128.1.17",
          "form": "아유  글쿤요ㅜ",
          "original_form": "아유  글쿤요ㅜ",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100034128.1.18",
          "form": "강지는 더 안키우시나요",
          "original_form": "강지는 더 안키우시나요",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100034128.1.19",
          "form": "네ㅠㅠ 오랜시간 같이 지냈는데\r\n보내고나니 너무 힘들더라구요",
          "original_form": "네ㅠㅠ 오랜시간 같이 지냈는데\r\n보내고나니 너무 힘들더라구요",
          "speaker_id": "2",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100034128.1.20",
          "form": "그래서 더는 키우는건 안하려구요",
          "original_form": "그래서 더는 키우는건 안하려구요",
          "speaker_id": "2",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034128.1.21",
          "form": "ㅜㅜㅜ안타깝네요",
          "original_form": "ㅜㅜㅜ안타깝네요",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034128.1.22",
          "form": "강쥐는 사랑받아서 좋은데 갔을거에요ㅜㅜㅜㅜㅜㅜ",
          "original_form": "강쥐는 사랑받아서 좋은데 갔을거에요ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100034128.1.23",
          "form": "그랬길바래요ㅠㅜ",
          "original_form": "그랬길바래요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100034128.1.24",
          "form": "ㅜㅜ저는 강아지 고양이 키울환경이 못되서ㅜ",
          "original_form": "ㅜㅜ저는 강아지 고양이 키울환경이 못되서ㅜ",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034128.1.25",
          "form": "이별을 경험해보지 못했네요",
          "original_form": "이별을 경험해보지 못했네요",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034128.1.26",
          "form": "ㅠㅠ 이별은 언제나 아프죠ㅠㅠ",
          "original_form": "ㅠㅠ 이별은 언제나 아프죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100034128.1.27",
          "form": "그렇지요ㅜㅜ 아쉬울거같아요 ㅜㅜ",
          "original_form": "그렇지요ㅜㅜ 아쉬울거같아요 ㅜㅜ",
