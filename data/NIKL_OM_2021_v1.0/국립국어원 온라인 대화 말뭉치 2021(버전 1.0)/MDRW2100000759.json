      "utterance": [
        {
          "id": "MDRW2100000759.1.1",
          "form": "현대백화점가서",
          "original_form": "현대백화점가서",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000759.1.2",
          "form": "포인트도 바꿔야함",
          "original_form": "포인트도 바꿔야함",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000759.1.3",
          "form": "ㅇㅇ 포인트로 뭐 살거야?",
          "original_form": "ㅇㅇ 포인트로 뭐 살거야?",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000759.1.4",
          "form": "글쎄 백화점 둘러보고",
          "original_form": "글쎄 백화점 둘러보고",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000759.1.5",
          "form": "닫기전에 빨리봐야할듯",
          "original_form": "닫기전에 빨리봐야할듯",
          "speaker_id": "2",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000759.1.6",
          "form": "몇시까지하지?",
          "original_form": "몇시까지하지?",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000759.1.7",
          "form": "아홉시 아냐?",
          "original_form": "아홉시 아냐?",
          "speaker_id": "2",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000759.1.8",
          "form": "시간이 없다",
          "original_form": "시간이 없다",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000759.1.9",
          "form": "그러게 얼른가야겠다",
          "original_form": "그러게 얼른가야겠다",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000759.1.10",
          "form": "가면 세일좀 하려나",
          "original_form": "가면 세일좀 하려나",
          "speaker_id": "1",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000759.1.11",
          "form": "요즘 코로나라 사람많이 안와서 할거같아",
          "original_form": "요즘 코로나라 사람많이 안와서 할거같아",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000759.1.12",
          "form": "원피스나 좀보고 나와야지",
          "original_form": "원피스나 좀보고 나와야지",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000759.1.13",
          "form": "네이비색?",
          "original_form": "네이비색?",
          "speaker_id": "2",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000759.1.14",
          "form": "아니 좀 밝은 베이지",
          "original_form": "아니 좀 밝은 베이지",
          "speaker_id": "1",
          "time": "20210819 19:03"
        }
      ]
