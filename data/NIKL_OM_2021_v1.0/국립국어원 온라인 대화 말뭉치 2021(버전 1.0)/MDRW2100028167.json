        {
          "id": "MDRW2100028167.1.1",
          "form": "아 요새",
          "original_form": "아 요새",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028167.1.2",
          "form": "방학이신가",
          "original_form": "방학이신가",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028167.1.3",
          "form": "아니요 어제 개강했어요ㅎㅎ",
          "original_form": "아니요 어제 개강했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028167.1.4",
          "form": "ㅋㅋ 막 대외활동 이런게",
          "original_form": "ㅋㅋ 막 대외활동 이런게",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028167.1.5",
          "form": "취업에 도움되지 싶은데",
          "original_form": "취업에 도움되지 싶은데",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028167.1.6",
          "form": "요새 시기가 참",
          "original_form": "요새 시기가 참",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028167.1.7",
          "form": "대외활동 보다는 개인 포트폴리오 위주라 그런건 좋아요",
          "original_form": "대외활동 보다는 개인 포트폴리오 위주라 그런건 좋아요",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028167.1.8",
          "form": "오 그러면 다행이군요 ㅋㅋ",
          "original_form": "오 그러면 다행이군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100028167.1.9",
          "form": "대신 팀프로젝트를 해본적이 없어요",
          "original_form": "대신 팀프로젝트를 해본적이 없어요",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100028167.1.10",
          "form": "그건 좀 심심하겠어요",
          "original_form": "그건 좀 심심하겠어요",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028167.1.11",
          "form": "제 성격 봐서는 안하는게 좋을거 같기도 하지만요...",
          "original_form": "제 성격 봐서는 안하는게 좋을거 같기도 하지만요...",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028167.1.12",
          "form": "보통 소심이가 아니라ㅋㅋ",
          "original_form": "보통 소심이가 아니라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028167.1.13",
          "form": "ㅋㅋ뭐 혼자서도 일할수 있고",
          "original_form": "ㅋㅋ뭐 혼자서도 일할수 있고",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028167.1.14",
          "form": "비대면도 갈수록 활발해질듯해요",
          "original_form": "비대면도 갈수록 활발해질듯해요",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028167.1.15",
          "form": "전보다는 많이 활발해졌죠",
          "original_form": "전보다는 많이 활발해졌죠",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028167.1.16",
          "form": "ㅋㅋ코로나가 더 가속화시킨듯합니다",
          "original_form": "ㅋㅋ코로나가 더 가속화시킨듯합니다",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028167.1.17",
          "form": "그런거 같아요ㅋㅋ 코로나의 순기능",
          "original_form": "그런거 같아요ㅋㅋ 코로나의 순기능",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028167.1.18",
          "form": "순기능 있긴하죠",
          "original_form": "순기능 있긴하죠",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028167.1.19",
          "form": "자연도 좀 많이 정화된거 같고 ㅎㅎ",
          "original_form": "자연도 좀 많이 정화된거 같고 ㅎㅎ",
