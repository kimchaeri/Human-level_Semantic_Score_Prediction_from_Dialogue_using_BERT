      "utterance": [
        {
          "id": "MDRW2100002907.1.1",
          "form": "요즘 날씨풀렷죵",
          "original_form": "요즘 날씨풀렷죵",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002907.1.2",
          "form": "네 지금은 비안오네요",
          "original_form": "네 지금은 비안오네요",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002907.1.3",
          "form": "주말에도 비 안오면 좋겠어요",
          "original_form": "주말에도 비 안오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002907.1.4",
          "form": "맞아요 담주말에 강원도갈건데",
          "original_form": "맞아요 담주말에 강원도갈건데",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002907.1.5",
          "form": "비안왓으면 ㅠㅠ",
          "original_form": "비안왓으면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002907.1.6",
          "form": "병원에서 일하는데 타지이동 가능해요?",
          "original_form": "병원에서 일하는데 타지이동 가능해요?",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002907.1.7",
          "form": "우리는 타지이동하면 상사한테 보고해야되요 ㅎ.ㅎ",
          "original_form": "우리는 타지이동하면 상사한테 보고해야되요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002907.1.8",
          "form": "회사 집이 끝이랍니다",
          "original_form": "회사 집이 끝이랍니다",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002907.1.9",
          "form": "ㅋㅋㅋㅋ네 상관없죠",
          "original_form": "ㅋㅋㅋㅋ네 상관없죠",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002907.1.10",
          "form": "작은 동네치과라서요",
          "original_form": "작은 동네치과라서요",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002907.1.11",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002907.1.12",
          "form": "요즘 옷 어떻게 입어요?",
          "original_form": "요즘 옷 어떻게 입어요?",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002907.1.13",
          "form": "유니폼입어요?",
          "original_form": "유니폼입어요?",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002907.1.14",
          "form": "네네 유니폼입어요",
          "original_form": "네네 유니폼입어요",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002907.1.15",
          "form": "여름엔 일주일에 두번씩..",
          "original_form": "여름엔 일주일에 두번씩..",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002907.1.16",
          "form": "땀냄새나서여 ㅋㅋ",
          "original_form": "땀냄새나서여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002907.1.17",
          "form": "옷값 안들겠네요!",
          "original_form": "옷값 안들겠네요!",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002907.1.18",
          "form": "주말에는 모입으시나요",
          "original_form": "주말에는 모입으시나요",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002907.1.19",
          "form": "음 그날그날 다른거같아요",
          "original_form": "음 그날그날 다른거같아요",
          "speaker_id": "1",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002907.1.20",
          "form": "간절기옷좀 사놔야겟어요",
          "original_form": "간절기옷좀 사놔야겟어요",
          "speaker_id": "1",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002907.1.21",
          "form": "가디건이나 걸치는정도죠?",
          "original_form": "가디건이나 걸치는정도죠?",
          "speaker_id": "2",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100002907.1.22",
          "form": "가을이 너무짧아서 애매해요",
          "original_form": "가을이 너무짧아서 애매해요",
          "speaker_id": "2",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100002907.1.23",
          "form": "낮에는 여전히 더워서",
          "original_form": "낮에는 여전히 더워서",
          "speaker_id": "2",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100002907.1.24",
          "form": "맞아요 아직은 반팔만 있음 돼요",
          "original_form": "맞아요 아직은 반팔만 있음 돼요",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002907.1.25",
          "form": "저는 주로 사무직이라 아예길거나 7부 블라우스나 와이셔츠입어요ㅎ.ㅎ",
          "original_form": "저는 주로 사무직이라 아예길거나 7부 블라우스나 와이셔츠입어요ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100002907.1.26",
          "form": "부럽다 프리하게 입어도 괜찮아요?",
          "original_form": "부럽다 프리하게 입어도 괜찮아요?",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100002907.1.27",
          "form": "네네 진짜 일할땐 막입고다녀요 ㅋㅋ",
          "original_form": "네네 진짜 일할땐 막입고다녀요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:40"
        }
      ]
    }
