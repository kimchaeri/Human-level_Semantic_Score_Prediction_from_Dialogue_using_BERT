      "utterance": [
        {
          "id": "MDRW2100018629.1.1",
          "form": "최근에 영화 보신거 있으신가요",
          "original_form": "최근에 영화 보신거 있으신가요",
          "speaker_id": "1",
          "time": "20210827 09:23"
        },
        {
          "id": "MDRW2100018629.1.2",
          "form": "저 최근에 본거 잠시만요",
          "original_form": "저 최근에 본거 잠시만요",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018629.1.3",
          "form": "옛날건데 재밌더라구요",
          "original_form": "옛날건데 재밌더라구요",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018629.1.4",
          "form": "외국 영화 스파이 봤어요",
          "original_form": "외국 영화 스파이 봤어요",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018629.1.5",
          "form": "name1님은 뭐보셨어요?",
          "original_form": "&name1&님은 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018629.1.6",
          "form": "저는 최근에 그거 28세 미성년인가",
          "original_form": "저는 최근에 그거 28세 미성년인가",
          "speaker_id": "1",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018629.1.7",
          "form": "그거봤어요",
          "original_form": "그거봤어요",
          "speaker_id": "1",
          "time": "20210827 09:25"
        },
        {
          "id": "MDRW2100018629.1.8",
          "form": "이상한 영화 아닙니다..",
          "original_form": "이상한 영화 아닙니다..",
          "speaker_id": "1",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018629.1.9",
          "form": "찾아보고왔는데 왕대륙 나오네요",
          "original_form": "찾아보고왔는데 왕대륙 나오네요",
          "speaker_id": "2",
          "time": "20210827 09:26"
        },
        {
          "id": "MDRW2100018629.1.10",
          "form": "맞아요 그 분 나오시는데 비중은 크지않아요",
          "original_form": "맞아요 그 분 나오시는데 비중은 크지않아요",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.11",
          "form": "저 나의남자친구에게 인가? 그거 어우 재미없어 하면서 두번봤어요 ^^;",
          "original_form": "저 나의남자친구에게 인가? 그거 어우 재미없어 하면서 두번봤어요 ^^;",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.12",
          "form": "왕대륙 나왔던거 ㅋㅋㅋㅋ",
          "original_form": "왕대륙 나왔던거 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.13",
          "form": "그런 영화있어요ㅋㅋ",
          "original_form": "그런 영화있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.14",
          "form": "재미는 없는데 뭔가 보고싶어지는",
          "original_form": "재미는 없는데 뭔가 보고싶어지는",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.15",
          "form": "왕대륙 좋아하시나요",
          "original_form": "왕대륙 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.16",
          "form": "나의소녀시대네용..",
          "original_form": "나의소녀시대네용..",
          "speaker_id": "2",
          "time": "20210827 09:27"
        },
        {
          "id": "MDRW2100018629.1.17",
          "form": "아녀 그거 하나봤어요! 잘생겼긴 한데 말 안통하는 사람 덕질은 힘들어요(?)",
          "original_form": "아녀 그거 하나봤어요! 잘생겼긴 한데 말 안통하는 사람 덕질은 힘들어요(?)",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018629.1.18",
          "form": "엇 저는 나의 소녀시대는 재밌게 봤는데",
          "original_form": "엇 저는 나의 소녀시대는 재밌게 봤는데",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018629.1.19",
          "form": "결말이..",
          "original_form": "결말이..",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018629.1.20",
          "form": "결말빼고 봐야되는 영화",
          "original_form": "결말빼고 봐야되는 영화",
          "speaker_id": "2",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018629.1.21",
          "form": "좀 충격이었다고해야하나",
          "original_form": "좀 충격이었다고해야하나",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018629.1.22",
          "form": "왜 그렇게 역변을..",
          "original_form": "왜 그렇게 역변을..",
          "speaker_id": "1",
          "time": "20210827 09:28"
        },
        {
          "id": "MDRW2100018629.1.23",
          "form": "ㅇㅈ 캐스팅좀 제발.",
          "original_form": "ㅇㅈ 캐스팅좀 제발.",
          "speaker_id": "2",
          "time": "20210827 09:29"
        },
        {
          "id": "MDRW2100018629.1.24",
          "form": "좀 더 괜찮은 사람이 있었을텐데.. 아쉽더라구요",
          "original_form": "좀 더 괜찮은 사람이 있었을텐데.. 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210827 09:29"
        }
      ]
