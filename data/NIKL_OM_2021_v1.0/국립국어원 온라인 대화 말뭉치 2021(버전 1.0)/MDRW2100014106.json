      "utterance": [
        {
          "id": "MDRW2100014106.1.1",
          "form": "좋아하는 음식 있으신가요?!?",
          "original_form": "좋아하는 음식 있으신가요?!?",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014106.1.2",
          "form": "떡볶이요!!",
          "original_form": "떡볶이요!!",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014106.1.3",
          "form": "헐 저두 떡볶이요!! 어디 떡볶이 좋아하세요?",
          "original_form": "헐 저두 떡볶이요!! 어디 떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014106.1.4",
          "form": "저는 안가리고 다좋아해용 ㅎㅎ",
          "original_form": "저는 안가리고 다좋아해용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014106.1.5",
          "form": "특별히 좋아하신 브랜드 있나요?",
          "original_form": "특별히 좋아하신 브랜드 있나요?",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014106.1.6",
          "form": "배떡이요 ! 사실 다른곳은 아직 안먹어봐서 ㅋㅋㅋㅋ",
          "original_form": "배떡이요 ! 사실 다른곳은 아직 안먹어봐서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014106.1.7",
          "form": "전 배떡을 안먹어봤어요 ㅠㅠ",
          "original_form": "전 배떡을 안먹어봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014106.1.8",
          "form": "조만간 먹어보려구요",
          "original_form": "조만간 먹어보려구요",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014106.1.9",
          "form": "오아우 배떡....진짜 맛있어요",
          "original_form": "오아우 배떡....진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014106.1.10",
          "form": "저는 밤만 되면 배떡이 생각나서 아침에 시켜먹어요",
          "original_form": "저는 밤만 되면 배떡이 생각나서 아침에 시켜먹어요",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014106.1.11",
          "form": "야식은 참으세욘?",
          "original_form": "야식은 참으세욘?",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014106.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ부모님이랑 같이 살아서...^ㅡ^ 먹고 싶지만 참아야해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ부모님이랑 같이 살아서...^ㅡ^ 먹고 싶지만 참아야해요",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014106.1.13",
          "form": "어디 떡볶이 좋아하세요?",
          "original_form": "어디 떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014106.1.14",
          "form": "저는 신전좋아해용",
          "original_form": "저는 신전좋아해용",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014106.1.15",
          "form": "오 저 신전도 좋아해요 자극적인맛",
          "original_form": "오 저 신전도 좋아해요 자극적인맛",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014106.1.16",
          "form": "떡볶이 이야기하니까 저녁으류 먹고싶어지네요 ㅋㅋ",
          "original_form": "떡볶이 이야기하니까 저녁으류 먹고싶어지네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014106.1.17",
          "form": "아직 저녁안드샸나요?",
          "original_form": "아직 저녁안드샸나요?",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014106.1.18",
          "form": "넵 ㅋㅋㅋㅋ 이제 시켜먹으려고요 !",
          "original_form": "넵 ㅋㅋㅋㅋ 이제 시켜먹으려고요 !",
