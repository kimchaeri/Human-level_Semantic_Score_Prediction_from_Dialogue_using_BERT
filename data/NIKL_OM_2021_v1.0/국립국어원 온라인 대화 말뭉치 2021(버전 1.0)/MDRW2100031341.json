        {
          "id": "MDRW2100031341.1.1",
          "form": "못 본 거 같아요",
          "original_form": "못 본 거 같아요",
          "speaker_id": "2",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031341.1.2",
          "form": "요새 수입이 잘 안 돼서 편의점에 안 나오더라구요",
          "original_form": "요새 수입이 잘 안 돼서 편의점에 안 나오더라구요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.3",
          "form": "역시 그래서 못 봤군요",
          "original_form": "역시 그래서 못 봤군요",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.4",
          "form": "내이마트 24에서 팔았는데",
          "original_form": "내이마트 24에서 팔았는데",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.5",
          "form": "아 우리 동네 없어요",
          "original_form": "아 우리 동네 없어요",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.6",
          "form": "진짜 맛있어요 요거트 맛있는데",
          "original_form": "진짜 맛있어요 요거트 맛있는데",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.7",
          "form": "요거트 맛만 있나요",
          "original_form": "요거트 맛만 있나요",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.8",
          "form": "아뇨 녹차도 있어요 녹차도 맛있어요",
          "original_form": "아뇨 녹차도 있어요 녹차도 맛있어요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.9",
          "form": "내 저는 요거트가 더 맛있어요",
          "original_form": "내 저는 요거트가 더 맛있어요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031341.1.10",
          "form": "한번 먹고 싶네요",
          "original_form": "한번 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031341.1.11",
          "form": "근데 요새 안 나와요",
          "original_form": "근데 요새 안 나와요",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031341.1.12",
          "form": "아쉬운 오시겠어요",
          "original_form": "아쉬운 오시겠어요",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100031341.1.13",
          "form": "예 그래서 인터넷으로 검색해봐야겠어요",
          "original_form": "예 그래서 인터넷으로 검색해봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
