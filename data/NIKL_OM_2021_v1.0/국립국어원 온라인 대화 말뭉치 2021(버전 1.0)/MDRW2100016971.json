      "utterance": [
        {
          "id": "MDRW2100016971.1.1",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
