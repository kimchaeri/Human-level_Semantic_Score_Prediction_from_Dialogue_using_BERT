      "utterance": [
        {
          "id": "MDRW2100003893.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003893.1.2",
          "form": "하죠 청소빨래 설거지도하고요",
          "original_form": "하죠 청소빨래 설거지도하고요",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003893.1.3",
          "form": "집안일도와주시나요?",
          "original_form": "집안일도와주시나요?",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003893.1.4",
          "form": "도와야져",
          "original_form": "도와야져",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003893.1.5",
          "form": "요즘은 다 같이하니",
          "original_form": "요즘은 다 같이하니",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003893.1.6",
          "form": "그렇죠 요즘은 돕는게 아니고 분담이죠",
          "original_form": "그렇죠 요즘은 돕는게 아니고 분담이죠",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003893.1.7",
          "form": "주로 어떤가사일 하시나요?",
          "original_form": "주로 어떤가사일 하시나요?",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003893.1.8",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003893.1.9",
          "form": "설거지 청소요",
          "original_form": "설거지 청소요",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003893.1.10",
          "form": "분리수거는 안해주세요?ㅎ",
          "original_form": "분리수거는 안해주세요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003893.1.11",
          "form": "청소하면 나오는 쓰레긴ㄴ 당연",
          "original_form": "청소하면 나오는 쓰레긴ㄴ 당연",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003893.1.12",
          "form": "와이프 음식물 쓰레기 버리는 법도 몰라요 제가 항상 해서",
          "original_form": "와이프 음식물 쓰레기 버리는 법도 몰라요 제가 항상 해서",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003893.1.13",
          "form": "오 가정적이시네요ㅎ",
          "original_form": "오 가정적이시네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003893.1.14",
          "form": "화장실 청소가 제일 귀찮아요",
          "original_form": "화장실 청소가 제일 귀찮아요",
          "speaker_id": "2",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003893.1.15",
          "form": "안하면 혼납니다",
          "original_form": "안하면 혼납니다",
          "speaker_id": "1",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003893.1.16",
          "form": "요즘 다 비슷하군요 ㅎ",
          "original_form": "요즘 다 비슷하군요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003893.1.17",
          "form": "맞춰 살아야죠",
          "original_form": "맞춰 살아야죠",
          "speaker_id": "1",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003893.1.18",
          "form": "그렇죠 요리도 하세요?",
          "original_form": "그렇죠 요리도 하세요?",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003893.1.19",
          "form": "요똥이에요",
          "original_form": "요똥이에요",
          "speaker_id": "1",
          "time": "20210821 13:29"
        }
      ]
