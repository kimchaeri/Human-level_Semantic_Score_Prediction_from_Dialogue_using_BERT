      "utterance": [
        {
          "id": "MDRW2100003392.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003392.1.2",
          "form": "요즘 날씨 선선해서 좋지 않나요?",
          "original_form": "요즘 날씨 선선해서 좋지 않나요?",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003392.1.3",
          "form": "안녕하세요~ 요즘 날씨가 덥지않아서 산책하기 정말 좋은 것 같아요",
          "original_form": "안녕하세요~ 요즘 날씨가 덥지않아서 산책하기 정말 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003392.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003392.1.5",
          "form": "저녁에 산책하기 좋은 날씨~",
          "original_form": "저녁에 산책하기 좋은 날씨~",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003392.1.6",
          "form": "제가 있는 곳은 비가 내리는데 그쪽에도 비가 오나요?",
          "original_form": "제가 있는 곳은 비가 내리는데 그쪽에도 비가 오나요?",
          "speaker_id": "1",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003392.1.7",
          "form": "여기는 잠깐 한두방울 내리다가 지금은 안와요~",
          "original_form": "여기는 잠깐 한두방울 내리다가 지금은 안와요~",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003392.1.8",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003392.1.9",
          "form": "비오는날 좋아하시나요,",
          "original_form": "비오는날 좋아하시나요,",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003392.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003392.1.11",
          "form": "비 오는 날을 좋아하지는 않아요ㅜㅜ 그 습한 느낌이 별루더라고요",
          "original_form": "비 오는 날을 좋아하지는 않아요ㅜㅜ 그 습한 느낌이 별루더라고요",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003392.1.12",
          "form": "혹시 비 오는 날 좋아하세요?",
          "original_form": "혹시 비 오는 날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003392.1.13",
          "form": "저는 빗소리 들으면 잠이 잘와서 잘땐 좋더라구요",
          "original_form": "저는 빗소리 들으면 잠이 잘와서 잘땐 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003392.1.14",
          "form": "그 외엔 저도 습해서 별로 안좋아해요ㅠㅠ",
          "original_form": "그 외엔 저도 습해서 별로 안좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003392.1.15",
          "form": "저도 다음에는 빗소리를 자장가삼아서 잠들어봐야겠네요",
          "original_form": "저도 다음에는 빗소리를 자장가삼아서 잠들어봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 23:55"
        },
        {
          "id": "MDRW2100003392.1.16",
          "form": "아직도 거기는 비가오나요?",
          "original_form": "아직도 거기는 비가오나요?",
          "speaker_id": "2",
          "time": "20210820 23:56"
        },
        {
          "id": "MDRW2100003392.1.17",
          "form": "네 잠 잘오더라구요ㅎㅎ",
          "original_form": "네 잠 잘오더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:56"
        },
        {
          "id": "MDRW2100003392.1.18",
          "form": "지금은 비 안오네요!",
          "original_form": "지금은 비 안오네요!",
          "speaker_id": "1",
          "time": "20210820 23:57"
        },
        {
          "id": "MDRW2100003392.1.19",
          "form": "무슨 계절 좋아하시나요?",
          "original_form": "무슨 계절 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 23:58"
        },
        {
          "id": "MDRW2100003392.1.20",
          "form": "저는 가을을 가장 좋아해요~ 날씨도 시원해서 돌아다니기 좋아요",
          "original_form": "저는 가을을 가장 좋아해요~ 날씨도 시원해서 돌아다니기 좋아요",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003392.1.21",
          "form": "구마님은 어떤 계절 좋아하세요?",
          "original_form": "구마님은 어떤 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003392.1.22",
          "form": "저도 가을을 가장 좋아해요!",
          "original_form": "저도 가을을 가장 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 01:05"
        },
        {
          "id": "MDRW2100003392.1.23",
          "form": "이제 곧 가을이라 다행이네요!!",
          "original_form": "이제 곧 가을이라 다행이네요!!",
          "speaker_id": "1",
          "time": "20210821 01:05"
        },
        {
          "id": "MDRW2100003392.1.24",
          "form": "맞아요~ 가을이 온다고 생각하니 기분이 좋아요",
          "original_form": "맞아요~ 가을이 온다고 생각하니 기분이 좋아요",
          "speaker_id": "2",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003392.1.25",
          "form": "이번주 주말까지 계속 비가올까요?",
          "original_form": "이번주 주말까지 계속 비가올까요?",
          "speaker_id": "2",
          "time": "20210821 12:38"
        },
        {
          "id": "MDRW2100003392.1.26",
          "form": "일기예보 보니 계속 올거같네요ㅠㅠ",
          "original_form": "일기예보 보니 계속 올거같네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 13:07"
        }
      ]
    }
