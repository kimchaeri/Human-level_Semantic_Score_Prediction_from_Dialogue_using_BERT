      "utterance": [
        {
          "id": "MDRW2100006356.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100006356.1.2",
          "form": "여행 자주 다니세요",
          "original_form": "여행 자주 다니세요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006356.1.3",
          "form": "요즘엔 자주 못다니구있어요",
          "original_form": "요즘엔 자주 못다니구있어요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006356.1.4",
          "form": "원래는 국내 해외 다 많이다니셨나요",
          "original_form": "원래는 국내 해외 다 많이다니셨나요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100006356.1.5",
          "form": "네 해외는 많이는 아닌데 가끔 가봤어요",
          "original_form": "네 해외는 많이는 아닌데 가끔 가봤어요",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006356.1.6",
          "form": "어디가 좋으셨나요",
          "original_form": "어디가 좋으셨나요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006356.1.7",
          "form": "전 태국이 좋더라구요",
          "original_form": "전 태국이 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006356.1.8",
          "form": "태국 툭툭이 타는거 재밌었어요ㅋㅋㅋㅋ",
          "original_form": "태국 툭툭이 타는거 재밌었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006356.1.9",
          "form": "툭툭이가 뭐에요??",
          "original_form": "툭툭이가 뭐에요??",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006356.1.10",
          "form": "어 그 캄보디아나 태국에",
          "original_form": "어 그 캄보디아나 태국에",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006356.1.11",
          "form": "대중교통 수단 같은거요",
          "original_form": "대중교통 수단 같은거요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006356.1.12",
          "form": "아아 제가 옛날에 다녀와서그런가 첨들어봐요",
          "original_form": "아아 제가 옛날에 다녀와서그런가 첨들어봐요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006356.1.13",
          "form": "열대과일 제가 엄청 좋아하거든요",
          "original_form": "열대과일 제가 엄청 좋아하거든요",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006356.1.14",
          "form": "오 가면 저렴하게 엄청 많이 먹을수 있죠?",
          "original_form": "오 가면 저렴하게 엄청 많이 먹을수 있죠?",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006356.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006356.1.16",
          "form": "그래서 동남아 여행을 좋아해요",
          "original_form": "그래서 동남아 여행을 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006356.1.17",
          "form": "약간 잘 체하는 체질이라ㅋㅋㅋ동남아갈때마다 조심해요ㅋㅋㅋ",
          "original_form": "약간 잘 체하는 체질이라ㅋㅋㅋ동남아갈때마다 조심해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006356.1.18",
          "form": "엥 왜요??",
          "original_form": "엥 왜요??",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006356.1.19",
          "form": "많이 먹어야되는데 체하면 안되자나요..? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "많이 먹어야되는데 체하면 안되자나요..? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006356.1.20",
          "form": "아아 천천히 드시면 되죵",
          "original_form": "아아 천천히 드시면 되죵",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006356.1.21",
          "form": "ㅋㅋㅋㅋㅋ한번 베트남 갔다오다가",
          "original_form": "ㅋㅋㅋㅋㅋ한번 베트남 갔다오다가",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006356.1.22",
          "form": "씨게 체해서",
          "original_form": "씨게 체해서",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006356.1.23",
          "form": "서울 돌아와서 일주일 동안 강제 다이어트 한족 있거든요",
          "original_form": "서울 돌아와서 일주일 동안 강제 다이어트 한족 있거든요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006356.1.24",
          "form": "끙",
          "original_form": "끙",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006356.1.25",
          "form": "아아...많이 힘드셨겠어요ㅠ",
          "original_form": "아아...많이 힘드셨겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006356.1.26",
          "form": "으으 그때는 진짜 여행때의 나한테 돌아가서 그만 먹으라고 하고 싶었던ㅋㅋㅋ",
          "original_form": "으으 그때는 진짜 여행때의 나한테 돌아가서 그만 먹으라고 하고 싶었던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006356.1.27",
          "form": "ㅋㅋㅋㅋ무슨과일 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋ무슨과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006356.1.28",
          "form": "음 누가 깎아만주면 다 좋아해요",
          "original_form": "음 누가 깎아만주면 다 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006356.1.29",
          "form": "사과 수박 배 감 포도 토마토 참외ㅋㅋㅋㅋ",
          "original_form": "사과 수박 배 감 포도 토마토 참외ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006356.1.30",
          "form": "골고루 다 먹어요 겨울에는 귤을 끼고 살고",
          "original_form": "골고루 다 먹어요 겨울에는 귤을 끼고 살고",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006356.1.31",
          "form": "열대과일은요??",
          "original_form": "열대과일은요??",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006356.1.32",
          "form": "아ㅋㅋㅋ 열대과일 용과 같은건가요?",
          "original_form": "아ㅋㅋㅋ 열대과일 용과 같은건가요?",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100006356.1.33",
          "form": "음 열대과일보다는 위에 과일들을 선호합니다..밍밍한 애들 많지않아요?",
          "original_form": "음 열대과일보다는 위에 과일들을 선호합니다..밍밍한 애들 많지않아요?",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100006356.1.34",
          "form": "용과 코코넛 이런거..?",
          "original_form": "용과 코코넛 이런거..?",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100006356.1.35",
          "form": "망고랑 망고스틴 완전마싰어요",
          "original_form": "망고랑 망고스틴 완전마싰어요",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006356.1.36",
          "form": "오옹 망고 맛있죠 넘니 비싼거...",
          "original_form": "오옹 망고 맛있죠 넘니 비싼거...",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006356.1.37",
          "form": "망고스틴이 그 흰뿌연 과일이죠? 까기 함들었던거 같은데",
          "original_form": "망고스틴이 그 흰뿌연 과일이죠? 까기 함들었던거 같은데",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100006356.1.38",
          "form": "동남아가면 망고 엄청 싸요!!",
          "original_form": "동남아가면 망고 엄청 싸요!!",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100006356.1.39",
          "form": "가서는 많이 먹었죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "가서는 많이 먹었죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006356.1.40",
          "form": "주스로도 먹고 음식으로도 맘ㅎ이먹구",
          "original_form": "주스로도 먹고 음식으로도 맘ㅎ이먹구",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006356.1.41",
          "form": "맞아요 그리고 패션프룻!!",
          "original_form": "맞아요 그리고 패션프룻!!",
          "speaker_id": "2",
          "time": "20210908 01:14"
        }
      ]
