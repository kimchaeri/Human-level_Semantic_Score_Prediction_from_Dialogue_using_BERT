      "utterance": [
        {
          "id": "MDRW2100006547.1.1",
          "form": "오버워치?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오버워치?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100006547.1.2",
          "form": "애쉬",
          "original_form": "애쉬",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100006547.1.3",
          "form": "아 최근에도 했었구나",
          "original_form": "아 최근에도 했었구나",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100006547.1.4",
          "form": "이제 좀 해??",
          "original_form": "이제 좀 해??",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100006547.1.5",
          "form": "아 맞다 걔 애쉬",
          "original_form": "아 맞다 걔 애쉬",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100006547.1.6",
          "form": "아니 더못할걸 ㅋㅋㅋ",
          "original_form": "아니 더못할걸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100006547.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ나도 그렇지 뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ나도 그렇지 뭐",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100006547.1.8",
          "form": "후후.. 나 근데 마지막에 혼자했어 친구가 하다가",
          "original_form": "후후.. 나 근데 마지막에 혼자했어 친구가 하다가",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100006547.1.9",
          "form": "재미없다고 꺼버리고 딴거해서",
          "original_form": "재미없다고 꺼버리고 딴거해서",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ같이 해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ같이 해",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.11",
          "form": "근데 욕 안먹었어 애들도 못하는 애들 걸리더랑",
          "original_form": "근데 욕 안먹었어 애들도 못하는 애들 걸리더랑",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.12",
          "form": "오랜만에 해보고싶다",
          "original_form": "오랜만에 해보고싶다",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.13",
          "form": "그래서 나 나름 열심히 했어 근데 그래도 잘하는 애가 있어서",
          "original_form": "그래서 나 나름 열심히 했어 근데 그래도 잘하는 애가 있어서",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.14",
          "form": "1등? 그건 못함",
          "original_form": "1등? 그건 못함",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.15",
          "form": "응응 나도 잘해!!",
          "original_form": "응응 나도 잘해!!",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.16",
          "form": "아니다 그 하이라이트도 안떳어",
          "original_form": "아니다 그 하이라이트도 안떳어",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100006547.1.17",
          "form": "뭐 그래도 재밌게 하는게 좋은거지 게임인데",
          "original_form": "뭐 그래도 재밌게 하는게 좋은거지 게임인데",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100006547.1.18",
          "form": "그래 난 못하는 애들이랑 하고 싶어 그게 좋아",
          "original_form": "그래 난 못하는 애들이랑 하고 싶어 그게 좋아",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
