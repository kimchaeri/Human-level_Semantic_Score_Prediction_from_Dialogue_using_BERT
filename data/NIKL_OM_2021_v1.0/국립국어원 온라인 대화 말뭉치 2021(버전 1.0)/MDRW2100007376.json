      "utterance": [
        {
          "id": "MDRW2100007376.1.1",
          "form": "알바 많이 해보셨나요",
          "original_form": "알바 많이 해보셨나요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007376.1.2",
          "form": "전 단기알바 위주로 많이 해봤는데",
          "original_form": "전 단기알바 위주로 많이 해봤는데",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007376.1.3",
          "form": "본의 아니게 아까 한 말을 반복하는데",
          "original_form": "본의 아니게 아까 한 말을 반복하는데",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007376.1.4",
          "form": "제가 한 우물 파는 스타일이라..",
          "original_form": "제가 한 우물 파는 스타일이라..",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007376.1.5",
          "form": "같은 편의점에서 3년째 일하고 있어요ㅋㅋㅋㅋ",
          "original_form": "같은 편의점에서 3년째 일하고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007376.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007376.1.7",
          "form": "전 한 곳에서 오래 일 못하겠던데",
          "original_form": "전 한 곳에서 오래 일 못하겠던데",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007376.1.8",
          "form": "그래서 단기알바만 해봤어요",
          "original_form": "그래서 단기알바만 해봤어요",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007376.1.9",
          "form": "저도 이렇게 오래하게 될 줄은 몰랐어요ㅋㅋㅋㅋ",
          "original_form": "저도 이렇게 오래하게 될 줄은 몰랐어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007376.1.10",
          "form": "벌써 3년이나 지나버린..",
          "original_form": "벌써 3년이나 지나버린..",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007376.1.11",
          "form": "단기알바는 뭔가 제가 일자리 못 구해서",
          "original_form": "단기알바는 뭔가 제가 일자리 못 구해서",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007376.1.12",
          "form": "못할거 같은 느낌...?ㅠㅠ",
          "original_form": "못할거 같은 느낌...?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007376.1.13",
          "form": "아 그쵸 그리고 몸쓰는 일이 많은 거 같아요",
          "original_form": "아 그쵸 그리고 몸쓰는 일이 많은 거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007376.1.14",
          "form": "전단지 돌리고 이런 거",
          "original_form": "전단지 돌리고 이런 거",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007376.1.15",
          "form": "편의점은 근데 시급 안맞춰주지않나요?",
          "original_form": "편의점은 근데 시급 안맞춰주지않나요?",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007376.1.16",
          "form": "안 쳐줘요...",
          "original_form": "안 쳐줘요...",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007376.1.17",
          "form": "근데 나름 괜찮은게 사람이 적어서",
          "original_form": "근데 나름 괜찮은게 사람이 적어서",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007376.1.18",
          "form": "일이 많이 없어요",
          "original_form": "일이 많이 없어요",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007376.1.19",
          "form": "아직도 그렇군요",
          "original_form": "아직도 그렇군요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007376.1.20",
          "form": "옛날엔 진짜 한 시간에 삼천얼마 사천얼마 이렇게 주는 데도 있었는데",
          "original_form": "옛날엔 진짜 한 시간에 삼천얼마 사천얼마 이렇게 주는 데도 있었는데",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007376.1.21",
          "form": "시급 안지는 건 너무한 거 같아요",
          "original_form": "시급 안지는 건 너무한 거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007376.1.22",
          "form": "편의점 시급 많이 줘봤자",
          "original_form": "편의점 시급 많이 줘봤자",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007376.1.23",
          "form": "거기서 하루종일 있으니까",
          "original_form": "거기서 하루종일 있으니까",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007376.1.24",
          "form": "거기서 다 쓰는데",
          "original_form": "거기서 다 쓰는데",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007376.1.25",
          "form": "좀 많이 주지는ㅡㅡ",
          "original_form": "좀 많이 주지는ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007376.1.26",
          "form": "아 그렇네요",
          "original_form": "아 그렇네요",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007376.1.27",
          "form": "그럼 식사 사먹어야 해요?",
          "original_form": "그럼 식사 사먹어야 해요?",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007376.1.28",
          "form": "폐기 먹어도 되긴 한데",
          "original_form": "폐기 먹어도 되긴 한데",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007376.1.29",
          "form": "이제 3년먹으니까",
          "original_form": "이제 3년먹으니까",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007376.1.30",
          "form": "포장지만 봐도 무슨 맛인지 알고 있어요..",
          "original_form": "포장지만 봐도 무슨 맛인지 알고 있어요..",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007376.1.31",
          "form": "아ㅋㅋㅋ 그렇겠네요",
          "original_form": "아ㅋㅋㅋ 그렇겠네요",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007376.1.32",
          "form": "그래도 오래 일하신 거 보면 사장님이 좋으신 분인가봐요",
          "original_form": "그래도 오래 일하신 거 보면 사장님이 좋으신 분인가봐요",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007376.1.33",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007376.1.34",
          "form": "사장님 별롭니다^^",
          "original_form": "사장님 별롭니다^^",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
