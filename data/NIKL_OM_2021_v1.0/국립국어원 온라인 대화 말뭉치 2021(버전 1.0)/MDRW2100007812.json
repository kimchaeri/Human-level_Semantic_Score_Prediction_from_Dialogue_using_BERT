      "utterance": [
        {
          "id": "MDRW2100007812.1.1",
          "form": "밥좀 하고올게요 ㅎㅎ",
          "original_form": "밥좀 하고올게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007812.1.2",
          "form": "네 !  집안일 하셔야지용 !!\r\n저두 설거지를 좀 ㅎㅎㅎㅎ",
          "original_form": "네 !  집안일 하셔야지용 !!\r\n저두 설거지를 좀 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007812.1.3",
          "form": "밥하고 왔어요",
          "original_form": "밥하고 왔어요",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007812.1.4",
          "form": "식사 설거지 했나요?",
          "original_form": "식사 설거지 했나요?",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007812.1.5",
          "form": "전 밥차리고 설거지 ㅡㅡ 모른척 하고 있네요 ㅎㅎ",
          "original_form": "전 밥차리고 설거지 ㅡㅡ 모른척 하고 있네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007812.1.6",
          "form": "저도 점심까지 다먹고 설거지 해놓고 이제 조금 쉬어요ㅠㅠ",
          "original_form": "저도 점심까지 다먹고 설거지 해놓고 이제 조금 쉬어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100007812.1.7",
          "form": "건조기도 벌써 다돌았네요ㅜㅜ",
          "original_form": "건조기도 벌써 다돌았네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100007812.1.8",
          "form": "전 빨래 또 한판해요",
          "original_form": "전 빨래 또 한판해요",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007812.1.9",
          "form": "주말엔 빨래 최소2번은 해야하죠ㅋㅋ",
          "original_form": "주말엔 빨래 최소2번은 해야하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007812.1.10",
          "form": "네 어제 비와서 더 많아요",
          "original_form": "네 어제 비와서 더 많아요",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007812.1.11",
          "form": "설거지도 클리어 했네요 ㅎㅎ",
          "original_form": "설거지도 클리어 했네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007812.1.12",
          "form": "이제 식사만 고민하면돼요",
          "original_form": "이제 식사만 고민하면돼요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.13",
          "form": "이제 저녁까지는 그래도 조금 쉴시간이 생겼어요ㅋ",
          "original_form": "이제 저녁까지는 그래도 조금 쉴시간이 생겼어요ㅋ",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.14",
          "form": "저녁 뭐 드시나요?",
          "original_form": "저녁 뭐 드시나요?",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.15",
          "form": "저녁은 뭐드시려구요?",
          "original_form": "저녁은 뭐드시려구요?",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.16",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.17",
          "form": "엇ㅋㅋㄱㅋ",
          "original_form": "엇ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.18",
          "form": "저는 bhc시켜먹을까..하는",
          "original_form": "저는 bhc시켜먹을까..하는",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.19",
          "form": "아 커피 한전 하고 싶네요",
          "original_form": "아 커피 한전 하고 싶네요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.20",
          "form": "전 카페서 샌드위치 먹고오게요",
          "original_form": "전 카페서 샌드위치 먹고오게요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007812.1.21",
          "form": "역시 주말은 외식 배달이죠",
          "original_form": "역시 주말은 외식 배달이죠",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100007812.1.22",
          "form": "주말꺼지 집안일 밥 청소",
          "original_form": "주말꺼지 집안일 밥 청소",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100007812.1.23",
          "form": "주부는 쉴수가 없어요",
          "original_form": "주부는 쉴수가 없어요",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100007812.1.24",
          "form": "커피 좋아하시나봐요!",
          "original_form": "커피 좋아하시나봐요!",
