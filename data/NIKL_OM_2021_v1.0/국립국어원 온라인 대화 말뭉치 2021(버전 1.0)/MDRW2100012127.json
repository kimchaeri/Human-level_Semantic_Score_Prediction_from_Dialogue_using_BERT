      "utterance": [
        {
          "id": "MDRW2100012127.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012127.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012127.1.3",
          "form": "아뇨! 아직이요ㅋㅋ곧 먹어야죠",
          "original_form": "아뇨! 아직이요ㅋㅋ곧 먹어야죠",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012127.1.4",
          "form": "재프리님은요?",
          "original_form": "재프리님은요?",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012127.1.5",
          "form": "저도 곧 먹으려고요",
          "original_form": "저도 곧 먹으려고요",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012127.1.6",
          "form": "뭐 드실건가용",
          "original_form": "뭐 드실건가용",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012127.1.7",
          "form": "오늘은 완전 집밥이에요ㅋㅋ도가니탕이랑 햄이랑 계란찜이랑 김치랑 나물반찬들...?",
          "original_form": "오늘은 완전 집밥이에요ㅋㅋ도가니탕이랑 햄이랑 계란찜이랑 김치랑 나물반찬들...?",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012127.1.8",
          "form": "오 맛있겠어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 맛있겠어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012127.1.9",
          "form": "도가니탕은 약간 곰탕 같은 느낌인가요?",
          "original_form": "도가니탕은 약간 곰탕 같은 느낌인가요?",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012127.1.10",
          "form": "네ㅋㅋㅋ국물은 거의 똑같을거고 내용물이 달라요",
          "original_form": "네ㅋㅋㅋ국물은 거의 똑같을거고 내용물이 달라요",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012127.1.11",
          "form": "하얗고 투명한 쫄깃쫄깃 도가니들이 많이 들어있죠!",
          "original_form": "하얗고 투명한 쫄깃쫄깃 도가니들이 많이 들어있죠!",
          "speaker_id": "2",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012127.1.12",
          "form": "크 깍두기랑 먹으면",
          "original_form": "크 깍두기랑 먹으면",
          "speaker_id": "1",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012127.1.13",
          "form": "되게 맛있겠어요",
          "original_form": "되게 맛있겠어요",
          "speaker_id": "1",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100012127.1.14",
          "form": "재프리님은 뭐드실거억요!",
          "original_form": "재프리님은 뭐드실거억요!",
          "speaker_id": "2",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100012127.1.15",
          "form": "깍두기 곰탕 설렁탕 도가니탕하고 잘 맞는다고 했는데 혹시 톡 갔나요?  글썼더니 없어졌어요...",
          "original_form": "깍두기 곰탕 설렁탕 도가니탕하고 잘 맞는다고 했는데 혹시 톡 갔나요?  글썼더니 없어졌어요...",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012127.1.16",
          "form": "전 떡볶이 생각중이요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 떡볶이 생각중이요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012127.1.17",
          "form": "아니요...안왔습니담",
          "original_form": "아니요...안왔습니담",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012127.1.18",
          "form": "직접 해드시나요?",
          "original_form": "직접 해드시나요?",
          "speaker_id": "2",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100012127.1.19",
          "form": "네 키트가 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "네 키트가 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012127.1.20",
          "form": "떡볶이ㅜㅜ다이어트 중이라 참고있는데...",
          "original_form": "떡볶이ㅜㅜ다이어트 중이라 참고있는데...",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012127.1.21",
          "form": "맛있겠어요ㅜㅜㅜ",
          "original_form": "맛있겠어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012127.1.22",
          "form": "아직 안먹었지만",
          "original_form": "아직 안먹었지만",
          "speaker_id": "1",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012127.1.23",
          "form": "빨리 먹고싶네요 !!!!",
          "original_form": "빨리 먹고싶네요 !!!!",
          "speaker_id": "1",
          "time": "20210824 19:36"
        }
      ]
    }
  ]
