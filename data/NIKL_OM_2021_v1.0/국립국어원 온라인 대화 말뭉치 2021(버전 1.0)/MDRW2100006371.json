      "utterance": [
        {
          "id": "MDRW2100006371.1.1",
          "form": "두산은 또 잘할거에요",
          "original_form": "두산은 또 잘할거에요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006371.1.2",
          "form": "희망이 아예 없는팀도 많자나요 ㅋㅋㅋ",
          "original_form": "희망이 아예 없는팀도 많자나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006371.1.3",
          "form": "그러길 바라고 있어요ㅎㅎ 선수 풀이 안 좋아지긴 했는데..",
          "original_form": "그러길 바라고 있어요ㅎㅎ 선수 풀이 안 좋아지긴 했는데..",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006371.1.4",
          "form": "한화는 그래도 방향성 명확하게 리빌딩 시도 중인 것 같더라구요",
          "original_form": "한화는 그래도 방향성 명확하게 리빌딩 시도 중인 것 같더라구요",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006371.1.5",
          "form": "두산은 신인 잘키우니까",
          "original_form": "두산은 신인 잘키우니까",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006371.1.6",
          "form": "그리고 함덕주주고 데려온 양석환 잘하자나요",
          "original_form": "그리고 함덕주주고 데려온 양석환 잘하자나요",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006371.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006371.1.8",
          "form": "양석환마저 없었으면 큰일날 뻔 했어요 ㅋㅋ",
          "original_form": "양석환마저 없었으면 큰일날 뻔 했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006371.1.9",
          "form": "트레이드 잘했죠",
          "original_form": "트레이드 잘했죠",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006371.1.10",
          "form": "최정이 있어서 부럽지 않지먀",
          "original_form": "최정이 있어서 부럽지 않지먀",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006371.1.11",
          "form": "만",
          "original_form": "만",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006371.1.12",
          "form": "투수가 망 ㅜㅜ",
          "original_form": "투수가 망 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006371.1.13",
          "form": "최정은 진짜 대단하죠ㅋㅋ",
          "original_form": "최정은 진짜 대단하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006371.1.14",
          "form": "잘한 6년 계약.. 정수빈 생각나네요ㅎㅎ",
          "original_form": "잘한 6년 계약.. 정수빈 생각나네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006371.1.15",
          "form": "최정마저 없었다면....",
          "original_form": "최정마저 없었다면....",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006371.1.16",
          "form": "끔찍",
          "original_form": "끔찍",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006371.1.17",
          "form": "힐만감독때가 좋았는디",
          "original_form": "힐만감독때가 좋았는디",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006371.1.18",
          "form": "김원형 감독에 대해서는 어떻게 생각하세요?",
          "original_form": "김원형 감독에 대해서는 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006371.1.19",
          "form": "전 아직 중립인데",
          "original_form": "전 아직 중립인데",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006371.1.20",
          "form": "워낙 부상이 많은 시즌이라",
          "original_form": "워낙 부상이 많은 시즌이라",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006371.1.21",
          "form": "내년에도 이러면",
          "original_form": "내년에도 이러면",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006371.1.22",
          "form": "저는 좀 별로...",
          "original_form": "저는 좀 별로...",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006371.1.23",
          "form": "하긴 올해는 부상이 너무 많아서 투수 쓰기가 힘들 것 같아요",
          "original_form": "하긴 올해는 부상이 너무 많아서 투수 쓰기가 힘들 것 같아요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006371.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006371.1.25",
          "form": "선발두명이 팔꿈치 수술이라니",
          "original_form": "선발두명이 팔꿈치 수술이라니",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006371.1.26",
          "form": "근데 내년 후반에나 복귀 예정 아닌가요?",
          "original_form": "근데 내년 후반에나 복귀 예정 아닌가요?",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006371.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006371.1.28",
          "form": "용병을 잘 뽑아야죠",
          "original_form": "용병을 잘 뽑아야죠",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006371.1.29",
          "form": "올해처럼 이러며ㆍ",
          "original_form": "올해처럼 이러며ㆍ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006371.1.30",
          "form": "망하는거죠",
          "original_form": "망하는거죠",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006371.1.31",
          "form": "용병 둘에 나머지 신인들로 버티면 후반기에 복귀 두명하니까",
          "original_form": "용병 둘에 나머지 신인들로 버티면 후반기에 복귀 두명하니까",
          "speaker_id": "2",
          "time": "20210907 23:39"
        }
      ]
    }
