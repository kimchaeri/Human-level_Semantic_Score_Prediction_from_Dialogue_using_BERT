      "utterance": [
        {
          "id": "MDRW2100010671.1.1",
          "form": "촬영도 문제지만 스토리가 개망해서...",
          "original_form": "촬영도 문제지만 스토리가 개망해서...",
          "speaker_id": "2",
          "time": "20210823 20:30"
        },
        {
          "id": "MDRW2100010671.1.2",
          "form": "주인공들이 너무 다 죽었어요",
          "original_form": "주인공들이 너무 다 죽었어요",
          "speaker_id": "2",
          "time": "20210823 20:30"
        },
        {
          "id": "MDRW2100010671.1.3",
          "form": "좀비가 붐벼야하는데 ㅋㅋㅋㅋㅋ",
          "original_form": "좀비가 붐벼야하는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.4",
          "form": "그러니까요 ㅋㅋ",
          "original_form": "그러니까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.5",
          "form": "인간들끼리만 싸우는데",
          "original_form": "인간들끼리만 싸우는데",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.6",
          "form": "세계관 발전도 없고",
          "original_form": "세계관 발전도 없고",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.7",
          "form": "아띠 제가좋아하는 주인공 죽어서",
          "original_form": "아띠 제가좋아하는 주인공 죽어서",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.8",
          "form": "너무 답답함 ㅋㅋ",
          "original_form": "너무 답답함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.9",
          "form": "넘 화났었어요ㅋㅋ",
          "original_form": "넘 화났었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010671.1.10",
          "form": "누구 좋아했는데요??",
          "original_form": "누구 좋아했는데요??",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.11",
          "form": "그 한국인분이요",
          "original_form": "그 한국인분이요",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.12",
          "form": "글렌 리!!",
          "original_form": "글렌 리!!",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.13",
          "form": "좀비는 그냥 넘약하고 인간이 강해져버린 워킹데드 ㅋㅋ",
          "original_form": "좀비는 그냥 넘약하고 인간이 강해져버린 워킹데드 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.14",
          "form": "스티브 연",
          "original_form": "스티브 연",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.15",
          "form": "아 글렌 맞아요",
          "original_form": "아 글렌 맞아요",
          "speaker_id": "1",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.16",
          "form": "너무너무 약해졌죠 좀비가...",
          "original_form": "너무너무 약해졌죠 좀비가...",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.17",
          "form": "시즌2까지는 진짜 신박했는데",
          "original_form": "시즌2까지는 진짜 신박했는데",
          "speaker_id": "2",
          "time": "20210823 20:32"
        },
        {
          "id": "MDRW2100010671.1.18",
          "form": "니간 나왔을때까지는 그래도 말된다 생각했는데",
          "original_form": "니간 나왔을때까지는 그래도 말된다 생각했는데",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010671.1.19",
          "form": "미숀까지 나가버려서 망한듯...",
          "original_form": "미숀까지 나가버려서 망한듯...",
          "speaker_id": "2",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010671.1.20",
          "form": "누구 때렿다던가",
          "original_form": "누구 때렿다던가",
          "speaker_id": "1",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010671.1.21",
          "form": "뉴스봤어요",
          "original_form": "뉴스봤어요",
          "speaker_id": "1",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010671.1.22",
          "form": "미숀하고 연애는 좀 아니었던거같아요",
          "original_form": "미숀하고 연애는 좀 아니었던거같아요",
          "speaker_id": "1",
          "time": "20210823 20:33"
        },
        {
          "id": "MDRW2100010671.1.23",
          "form": "주인공하고 자꾸 연애시켜 누구를 ㅋㅋㅋ",
          "original_form": "주인공하고 자꾸 연애시켜 누구를 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100010671.1.24",
          "form": "갑자기 급발전됨... ㅋㅋ",
          "original_form": "갑자기 급발전됨... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010671.1.25",
          "form": "원래 와이프가 완전 고구마긴 했는데",
          "original_form": "원래 와이프가 완전 고구마긴 했는데",
          "speaker_id": "2",
          "time": "20210823 20:39"
        },
        {
          "id": "MDRW2100010671.1.26",
          "form": "너무 급진적이었음 ㅋㅋ",
          "original_form": "너무 급진적이었음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010671.1.27",
          "form": "칼도 죽고 칼 여친도 죽고 ㅋㅋ",
          "original_form": "칼도 죽고 칼 여친도 죽고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010671.1.28",
          "form": "너무 빡침... ㅋㅋ",
          "original_form": "너무 빡침... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010671.1.29",
          "form": "휴 워킹데드 돌려내 ㅋㅋㅋㅋ",
          "original_form": "휴 워킹데드 돌려내 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010671.1.30",
          "form": "그냥 중간까지만 보라구 추천해야지",
          "original_form": "그냥 중간까지만 보라구 추천해야지",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010671.1.31",
          "form": "잔인한거도 좀 그래요ㅜ",
          "original_form": "잔인한거도 좀 그래요ㅜ",
          "speaker_id": "1",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010671.1.32",
          "form": "잔인한거 잘 못보시는구나",
          "original_form": "잔인한거 잘 못보시는구나",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010671.1.33",
          "form": "저는 그게 오히려 더 현실적이라서 더 와닿는거 같아요",
          "original_form": "저는 그게 오히려 더 현실적이라서 더 와닿는거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010671.1.34",
          "form": "킹덤이 진짜 신박했던거 같아요",
          "original_form": "킹덤이 진짜 신박했던거 같아요",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010671.1.35",
          "form": "오리지널 좀비 느낌도 많이 나고",
          "original_form": "오리지널 좀비 느낌도 많이 나고",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010671.1.36",
          "form": "킹덤 2까지밖에 못봤어요ㅜㅜㅜㅜ",
          "original_form": "킹덤 2까지밖에 못봤어요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010671.1.37",
