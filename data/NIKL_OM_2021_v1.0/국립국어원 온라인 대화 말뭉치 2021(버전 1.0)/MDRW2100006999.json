      "utterance": [
        {
          "id": "MDRW2100006999.1.1",
          "form": "다이어트 중이신가요?",
          "original_form": "다이어트 중이신가요?",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006999.1.2",
          "form": "다이어트 하려 했는데 먹고 싶은 게 많아서 또 미뤘어요 ㅣ...^^",
          "original_form": "다이어트 하려 했는데 먹고 싶은 게 많아서 또 미뤘어요 ㅣ...^^",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006999.1.3",
          "form": "아 그렇죠ㅠㅠ 방학때가 더 살빼기 어려운거같아요",
          "original_form": "아 그렇죠ㅠㅠ 방학때가 더 살빼기 어려운거같아요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006999.1.4",
          "form": "맞아요ㅜ 다이어트 하려 하면",
          "original_form": "맞아요ㅜ 다이어트 하려 하면",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006999.1.5",
          "form": "또 먹고 싶은 게 생각나더라고요",
          "original_form": "또 먹고 싶은 게 생각나더라고요",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006999.1.6",
          "form": "음식 모든 메뉴로 무한돌려막기...",
          "original_form": "음식 모든 메뉴로 무한돌려막기...",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006999.1.7",
          "form": "그니까요 세상에 맛있는거 너무 많죠",
          "original_form": "그니까요 세상에 맛있는거 너무 많죠",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006999.1.8",
          "form": "맞아요ㅠ ㅠ 패스트푸드 최고",
          "original_form": "맞아요ㅠ ㅠ 패스트푸드 최고",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006999.1.9",
          "form": "다이어트할땐 확실히 식이가 80퍼는 차지 하는거 같아요",
          "original_form": "다이어트할땐 확실히 식이가 80퍼는 차지 하는거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006999.1.10",
          "form": "운동하면 건강한돼지만 될뿐ㅠㅠ",
          "original_form": "운동하면 건강한돼지만 될뿐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006999.1.11",
          "form": "맞아요ㅠㅠ 식단 중요하죠",
          "original_form": "맞아요ㅠㅠ 식단 중요하죠",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006999.1.12",
          "form": "방학끝나면 다시 다이어트 해야할거같아요",
          "original_form": "방학끝나면 다시 다이어트 해야할거같아요",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006999.1.13",
          "form": "저도요,,",
          "original_form": "저도요,,",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006999.1.14",
          "form": "운동어떤거 주로 하세요?",
          "original_form": "운동어떤거 주로 하세요?",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006999.1.15",
          "form": "저 배드민턴이랑 홈트 했었는데",
          "original_form": "저 배드민턴이랑 홈트 했었는데",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006999.1.16",
          "form": "수영도 다니고 싶어요",
          "original_form": "수영도 다니고 싶어요",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006999.1.17",
          "form": "아 활동적인거 좋아하시나봐요",
          "original_form": "아 활동적인거 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006999.1.18",
          "form": "저는 pt받아보고싶은데 넘 비싸네요ㅠㅠ흙",
          "original_form": "저는 pt받아보고싶은데 넘 비싸네요ㅠㅠ흙",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006999.1.19",
          "form": "활동적인 거 좋아하지 않은 편이었는데",
          "original_form": "활동적인 거 좋아하지 않은 편이었는데",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006999.1.20",
          "form": "운동 시작하면 그렇게 변하더라고요",
          "original_form": "운동 시작하면 그렇게 변하더라고요",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006999.1.21",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006999.1.22",
          "form": "pt 너무 비싸죠ㅠㅠ",
          "original_form": "pt 너무 비싸죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006999.1.23",
          "form": "저는 좀 정적인거 좋아해서 운동엔 취미를 못붙이겠더라구요",
          "original_form": "저는 좀 정적인거 좋아해서 운동엔 취미를 못붙이겠더라구요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006999.1.24",
          "form": "그래서 더 살을 못빼나봐요",
          "original_form": "그래서 더 살을 못빼나봐요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006999.1.25",
          "form": "아무래도 살 빼려면 활동적인 게 좋긴 하죠ㅠ",
          "original_form": "아무래도 살 빼려면 활동적인 게 좋긴 하죠ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006999.1.26",
          "form": "그래도 요가나 필라테스 같은? 건 자세 교정에 많이 좋아서",
          "original_form": "그래도 요가나 필라테스 같은? 건 자세 교정에 많이 좋아서",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006999.1.27",
          "form": "필라테스 하다가 허리아파서 물리치료 다녔어요. 뭐든 무리하면 안되는데 너무 욕심부렸나봐요ㅎㅎ",
          "original_form": "필라테스 하다가 허리아파서 물리치료 다녔어요. 뭐든 무리하면 안되는데 너무 욕심부렸나봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:28"
        }
      ]
