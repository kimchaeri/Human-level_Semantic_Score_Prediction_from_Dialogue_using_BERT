      "utterance": [
        {
          "id": "MDRW2100039056.1.1",
          "form": "운동은 안좋아해서요ㅋ",
          "original_form": "운동은 안좋아해서요ㅋ",
          "speaker_id": "1",
          "time": "20210903 08:27"
        },
        {
          "id": "MDRW2100039056.1.2",
          "form": "물론 저도 운동을 좋아하는 편은 아닙니다",
          "original_form": "물론 저도 운동을 좋아하는 편은 아닙니다",
          "speaker_id": "2",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100039056.1.3",
          "form": "전 먹고 바로 움직이면 체해서 누워야해요ㅋㅋ",
          "original_form": "전 먹고 바로 움직이면 체해서 누워야해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100039056.1.4",
          "form": "바로 움직이진 않죠ㅋㅋ",
          "original_form": "바로 움직이진 않죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100039056.1.5",
          "form": "1시간쯤 후에 활동 시작합니다",
          "original_form": "1시간쯤 후에 활동 시작합니다",
          "speaker_id": "2",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100039056.1.6",
          "form": "눕는것도 좋진 않을텐데요",
          "original_form": "눕는것도 좋진 않을텐데요",
          "speaker_id": "2",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100039056.1.7",
          "form": "안좋은건 아는데 몸이 자꾸 눕자네요ㅋ",
          "original_form": "안좋은건 아는데 몸이 자꾸 눕자네요ㅋ",
          "speaker_id": "1",
          "time": "20210903 08:32"
        },
        {
          "id": "MDRW2100039056.1.8",
          "form": "저 예전에 그랬다가 역류성식도염 한번 크게 앓았어요",
          "original_form": "저 예전에 그랬다가 역류성식도염 한번 크게 앓았어요",
          "speaker_id": "2",
          "time": "20210903 08:33"
        },
        {
          "id": "MDRW2100039056.1.9",
          "form": "전 다행히 그건 없어서요\r\n걷는거만 겨우하는 편이에요",
          "original_form": "전 다행히 그건 없어서요\r\n걷는거만 겨우하는 편이에요",
          "speaker_id": "1",
          "time": "20210903 08:34"
        },
        {
          "id": "MDRW2100039056.1.10",
          "form": "없으시다니 다행이에요",
          "original_form": "없으시다니 다행이에요",
          "speaker_id": "2",
          "time": "20210903 08:35"
        },
        {
          "id": "MDRW2100039056.1.11",
          "form": "네. 요즘은 선선해서 저녁에도 걷기 좋더라고요",
          "original_form": "네. 요즘은 선선해서 저녁에도 걷기 좋더라고요",
          "speaker_id": "1",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100039056.1.12",
          "form": "그거 참 다행이네요",
          "original_form": "그거 참 다행이네요",
          "speaker_id": "2",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100039056.1.13",
          "form": "이제 날도 갰으니 한번 나가야겠어요",
          "original_form": "이제 날도 갰으니 한번 나가야겠어요",
          "speaker_id": "2",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100039056.1.14",
          "form": "오늘은 진짜 날좋네요",
          "original_form": "오늘은 진짜 날좋네요",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100039056.1.15",
          "form": "장마가 끝났죠ㅎㅎ",
          "original_form": "장마가 끝났죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100039056.1.16",
          "form": "아 끝났나요 \r\n지금이 딱 운동하기 좋은때같아요",
          "original_form": "아 끝났나요 \r\n지금이 딱 운동하기 좋은때같아요",
          "speaker_id": "1",
          "time": "20210903 08:39"
        },
        {
          "id": "MDRW2100039056.1.17",
          "form": "옷도 적당히 얇게 입을 수 있고",
          "original_form": "옷도 적당히 얇게 입을 수 있고",
          "speaker_id": "2",
          "time": "20210903 08:40"
        },
        {
          "id": "MDRW2100039056.1.18",
          "form": "무엇보다 땀이 안나는 마성의 계절",
          "original_form": "무엇보다 땀이 안나는 마성의 계절",
          "speaker_id": "2",
          "time": "20210903 08:40"
        }
      ]
