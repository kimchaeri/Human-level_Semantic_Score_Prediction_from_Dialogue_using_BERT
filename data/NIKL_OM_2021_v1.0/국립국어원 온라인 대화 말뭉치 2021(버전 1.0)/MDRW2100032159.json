      "utterance": [
        {
          "id": "MDRW2100032159.1.1",
          "form": "저는 특히 지난 여름이 너무 더웠어서",
          "original_form": "저는 특히 지난 여름이 너무 더웠어서",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032159.1.2",
          "form": "당황했었어요,,,",
          "original_form": "당황했었어요,,,",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032159.1.3",
          "form": "옛날엔 여름에 해변이라도 갔는데 요즘은 코로나 때문에 그것도 쉽지 않더라구요",
          "original_form": "옛날엔 여름에 해변이라도 갔는데 요즘은 코로나 때문에 그것도 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032159.1.4",
          "form": "저두요ㅋㅋㅋ정말 너무 더웠죠",
          "original_form": "저두요ㅋㅋㅋ정말 너무 더웠죠",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032159.1.5",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032159.1.6",
          "form": "그리고 마스크까지 쓰려니까",
          "original_form": "그리고 마스크까지 쓰려니까",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032159.1.7",
          "form": "정말....!!!",
          "original_form": "정말....!!!",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032159.1.8",
          "form": "와 맞아요 엄청 습하고 덥고 땀나고",
          "original_form": "와 맞아요 엄청 습하고 덥고 땀나고",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032159.1.9",
          "form": "겨울엔 코로나가 끝날 줄 알았는데..",
          "original_form": "겨울엔 코로나가 끝날 줄 알았는데..",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032159.1.10",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032159.1.11",
          "form": "솔직히 2021년에는",
          "original_form": "솔직히 2021년에는",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032159.1.12",
          "form": "코로나 없을 줄 알았어요...",
          "original_form": "코로나 없을 줄 알았어요...",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032159.1.13",
          "form": "이렇게 장기간 간 게 거의 처음 아닌가요?",
          "original_form": "이렇게 장기간 간 게 거의 처음 아닌가요?",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032159.1.14",
          "form": "그쵸 저도 솔직히 여름 지나면 끝날 줄 알았어요 그런데 변이가 생기더라고요...",
          "original_form": "그쵸 저도 솔직히 여름 지나면 끝날 줄 알았어요 그런데 변이가 생기더라고요...",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032159.1.15",
          "form": "그러니까요....",
          "original_form": "그러니까요....",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032159.1.16",
          "form": "이게 인구 많은 나라에서 시작되어서 그런지....",
          "original_form": "이게 인구 많은 나라에서 시작되어서 그런지....",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032159.1.17",
          "form": "면적은 좁고 인구는 많아서 더 그런가 싶기도 해요",
          "original_form": "면적은 좁고 인구는 많아서 더 그런가 싶기도 해요",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032159.1.18",
          "form": "하효 얼른 끝나야할텐데ㅜ",
          "original_form": "하효 얼른 끝나야할텐데ㅜ",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032159.1.19",
          "form": "그러게 말이에요... 8ㅅ8",
          "original_form": "그러게 말이에요... 8ㅅ8",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032159.1.20",
          "form": "진짜 내년 여름에는 제발!",
          "original_form": "진짜 내년 여름에는 제발!",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032159.1.21",
          "form": "마스크 벗을 수 있으면 좋겠어요",
          "original_form": "마스크 벗을 수 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032159.1.22",
          "form": "맞아요 올해 지나면 더 이상 코로나 없음 좋겠어요..",
          "original_form": "맞아요 올해 지나면 더 이상 코로나 없음 좋겠어요..",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032159.1.23",
          "form": "아맞다 이번 겨울에는",
          "original_form": "아맞다 이번 겨울에는",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032159.1.24",
          "form": "미세먼지가 어떨 거 같으세요?!",
          "original_form": "미세먼지가 어떨 거 같으세요?!",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032159.1.25",
          "form": "음 그래도 코로나 때문에 좀 줄었다는 기사를 봤는데",
          "original_form": "음 그래도 코로나 때문에 좀 줄었다는 기사를 봤는데",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032159.1.26",
          "form": "공기청정기를 사야하나.... 여태 안사고 버텼거든요 ㅠㅠㅋㅋ",
          "original_form": "공기청정기를 사야하나.... 여태 안사고 버텼거든요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032159.1.27",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032159.1.28",
          "form": "오오 코로나 때문에요?!",
          "original_form": "오오 코로나 때문에요?!",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032159.1.29",
          "form": "아이고 그러셨구나ㅜㅜ",
          "original_form": "아이고 그러셨구나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032159.1.30",
          "form": "네네 그렇다 그러더라구요",
          "original_form": "네네 그렇다 그러더라구요",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032159.1.31",
          "form": "저는 공기청정기 장만했어요....",
          "original_form": "저는 공기청정기 장만했어요....",
          "speaker_id": "2",
          "time": "20210905 13:41"
        }
      ]
    }
