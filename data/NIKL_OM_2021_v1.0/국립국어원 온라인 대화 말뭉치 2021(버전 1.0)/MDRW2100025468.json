      "utterance": [
        {
          "id": "MDRW2100025468.1.1",
          "form": "어떤 음료 좋아하세요?",
          "original_form": "어떤 음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025468.1.2",
          "form": "저는 제로사이다요",
          "original_form": "저는 제로사이다요",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025468.1.3",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025468.1.4",
          "form": "오 당없는 음료 요즘 유행이더라구요",
          "original_form": "오 당없는 음료 요즘 유행이더라구요",
          "speaker_id": "1",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025468.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025468.1.6",
          "form": "음식 거의 잘먹는데 역시 가을이라 전어가 땡기네요",
          "original_form": "음식 거의 잘먹는데 역시 가을이라 전어가 땡기네요",
          "speaker_id": "1",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025468.1.7",
          "form": "오늘 식사 메뉴는 뭔가요",
          "original_form": "오늘 식사 메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025468.1.8",
          "form": "그렇군요 오늘은 아마 된장찌개 먹을 것 같네요 당신은요",
          "original_form": "그렇군요 오늘은 아마 된장찌개 먹을 것 같네요 당신은요",
          "speaker_id": "2",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025468.1.9",
          "form": "된찌 맛있지요",
          "original_form": "된찌 맛있지요",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025468.1.10",
          "form": "저는 꽃게 사서",
          "original_form": "저는 꽃게 사서",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025468.1.11",
          "form": "꽃게 쪄먹을려구요",
          "original_form": "꽃게 쪄먹을려구요",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025468.1.12",
          "form": "꽃게가 더 맛있겠어요",
          "original_form": "꽃게가 더 맛있겠어요",
          "speaker_id": "2",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025468.1.13",
          "form": "제철이라 쫌 세일 하더라구요",
          "original_form": "제철이라 쫌 세일 하더라구요",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100025468.1.14",
          "form": "점심은 뭐드세요",
          "original_form": "점심은 뭐드세요",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025468.1.15",
          "form": "저도 한번 봐야겠네요 점심은 고민중이에요",
          "original_form": "저도 한번 봐야겠네요 점심은 고민중이에요",
          "speaker_id": "2",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025468.1.16",
          "form": "뭐드실거에요",
          "original_form": "뭐드실거에요",
          "speaker_id": "2",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025468.1.17",
          "form": "방금 유부초밥 먹었어요",
          "original_form": "방금 유부초밥 먹었어요",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025468.1.18",
          "form": "배가 부르네요",
          "original_form": "배가 부르네요",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025468.1.19",
          "form": "맛있었겠네요",
          "original_form": "맛있었겠네요",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025468.1.20",
          "form": "배달 음식 자주 애용하세요?",
          "original_form": "배달 음식 자주 애용하세요?",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025468.1.21",
          "form": "메뉴 고르기 힘들어요",
          "original_form": "메뉴 고르기 힘들어요",
          "speaker_id": "1",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025468.1.22",
          "form": "맞아요 근데 요즘은 어플로 주문하기편해서 자주 애용해요 할인도 많이하고",
          "original_form": "맞아요 근데 요즘은 어플로 주문하기편해서 자주 애용해요 할인도 많이하고",
          "speaker_id": "2",
          "time": "20210901 12:08"
        }
      ]
