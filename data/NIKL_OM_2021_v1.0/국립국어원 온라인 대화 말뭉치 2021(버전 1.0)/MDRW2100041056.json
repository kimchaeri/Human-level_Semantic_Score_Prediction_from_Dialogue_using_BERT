        {
          "id": "MDRW2100041056.1.1",
          "form": "세상에서 제일 예쁜 내딸에서 한기웅 주연이엇네",
          "original_form": "세상에서 제일 예쁜 내딸에서 한기웅 주연이엇네",
          "speaker_id": "2",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100041056.1.2",
          "form": "그것도 안봤고ㅋ",
          "original_form": "그것도 안봤고ㅋ",
          "speaker_id": "1",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100041056.1.3",
          "form": "끝까지 사랑은 조연",
          "original_form": "끝까지 사랑은 조연",
          "speaker_id": "2",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100041056.1.4",
          "form": "그것도 보적 없고ㅋ",
          "original_form": "그것도 보적 없고ㅋ",
          "speaker_id": "1",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100041056.1.5",
          "form": "대박에서도 쌍둥이 출연인가",
          "original_form": "대박에서도 쌍둥이 출연인가",
          "speaker_id": "2",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100041056.1.6",
          "form": "인기드라마는 아니었나봐요",
          "original_form": "인기드라마는 아니었나봐요",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041056.1.7",
          "form": "연금술사 서준오역",
          "original_form": "연금술사 서준오역",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041056.1.8",
          "form": "전혀  모르겠어요",
          "original_form": "전혀  모르겠어요",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041056.1.9",
          "form": "빛과 그림자",
          "original_form": "빛과 그림자",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041056.1.10",
          "form": "ㅋㅋ 안 봤어요",
          "original_form": "ㅋㅋ 안 봤어요",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041056.1.11",
          "form": "심지어 압구정 백야도 조연으로 나왔나봐",
          "original_form": "심지어 압구정 백야도 조연으로 나왔나봐",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041056.1.12",
          "form": "아 그거는 봤는데ㅋ",
          "original_form": "아 그거는 봤는데ㅋ",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
