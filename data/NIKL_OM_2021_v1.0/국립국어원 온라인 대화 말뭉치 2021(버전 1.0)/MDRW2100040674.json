      "utterance": [
        {
          "id": "MDRW2100040674.1.1",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋ 낮잠안자면 징징거리나??",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋ 낮잠안자면 징징거리나??",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100040674.1.2",
          "form": "아니ㅜ6시에자니깐ㅜㅋㅋ",
          "original_form": "아니ㅜ6시에자니깐ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100040674.1.3",
          "form": "그럼  진짜 난감하거든ㅋㅋ",
          "original_form": "그럼  진짜 난감하거든ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100040674.1.4",
          "form": "덜징징거리는데 6시에자서 8시에일어나면ㅋㅋ더 힘들어지니",
          "original_form": "덜징징거리는데 6시에자서 8시에일어나면ㅋㅋ더 힘들어지니",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040674.1.5",
          "form": "2시30분부터 눕혔는데 결국4시넘어서 잠",
          "original_form": "2시30분부터 눕혔는데 결국4시넘어서 잠",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040674.1.6",
          "form": "아 그래 그러면 정말 힘들지",
          "original_form": "아 그래 그러면 정말 힘들지",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040674.1.7",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040674.1.8",
          "form": "안자길래 씅냈더니",
          "original_form": "안자길래 씅냈더니",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100040674.1.9",
          "form": "내한테 안오더라고",
          "original_form": "내한테 안오더라고",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.10",
          "form": "눕히기는 일찍 눕혔네",
          "original_form": "눕히기는 일찍 눕혔네",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.12",
          "form": "눈치가 백단이 군",
          "original_form": "눈치가 백단이 군",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.13",
          "form": "웃긴다 name3",
          "original_form": "웃긴다 &name3&",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.14",
          "form": "응응 그러니깐",
          "original_form": "응응 그러니깐",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.15",
          "form": "눈치는 장난아이다",
          "original_form": "눈치는 장난아이다",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.17",
          "form": "말은못하는기",
          "original_form": "말은못하는기",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.18",
          "form": "행동은 빠르다",
          "original_form": "행동은 빠르다",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.19",
          "form": "그렇다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.20",
          "form": "웃긴다 정말 ㅋㅋㅋㄱ",
          "original_form": "웃긴다 정말 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040674.1.21",
          "form": "오늘 하루도 고생했구먼",
          "original_form": "오늘 하루도 고생했구먼",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040674.1.22",
          "form": "난 내일 신랑없어서",
          "original_form": "난 내일 신랑없어서",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040674.1.23",
          "form": "ㅋㅋㅋㅋㅋ응 ㅋㅋ",
