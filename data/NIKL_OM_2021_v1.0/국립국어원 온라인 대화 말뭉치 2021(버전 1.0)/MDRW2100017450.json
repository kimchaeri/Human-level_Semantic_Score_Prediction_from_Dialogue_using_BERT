      "utterance": [
        {
          "id": "MDRW2100017450.1.1",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 00:47"
        },
        {
          "id": "MDRW2100017450.1.2",
          "form": "닭볶음탕 먹엇어요",
          "original_form": "닭볶음탕 먹엇어요",
          "speaker_id": "2",
          "time": "20210827 00:50"
        },
        {
          "id": "MDRW2100017450.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 00:50"
        },
        {
          "id": "MDRW2100017450.1.4",
          "form": "저는 빵먹구 만두쪄먹었어요",
          "original_form": "저는 빵먹구 만두쪄먹었어요",
          "speaker_id": "1",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100017450.1.5",
          "form": "닭볶음탕 간장맛조아하세요? 아니면 빨간양념?",
          "original_form": "닭볶음탕 간장맛조아하세요? 아니면 빨간양념?",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100017450.1.6",
          "form": "빨간양념이요 ㅋㅋㅋㅋ",
          "original_form": "빨간양념이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100017450.1.7",
          "form": "오 저 만두도 엄청좋아해요",
          "original_form": "오 저 만두도 엄청좋아해요",
          "speaker_id": "2",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100017450.1.8",
          "form": "저는 간장 좋아햐요ㅋㅋㅋㅋ",
          "original_form": "저는 간장 좋아햐요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100017450.1.9",
          "form": "저두요 ㅋㅋㅋㅋ만두 냉동실에 상시구비",
          "original_form": "저두요 ㅋㅋㅋㅋ만두 냉동실에 상시구비",
          "speaker_id": "1",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100017450.1.10",
          "form": "무슨 만두 좋아하세요?",
          "original_form": "무슨 만두 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100017450.1.11",
          "form": "고기만두요! 갈비만두도좋아해요 감자만두도 좋아하구욬ㅋㅋㅋ",
          "original_form": "고기만두요! 갈비만두도좋아해요 감자만두도 좋아하구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100017450.1.12",
          "form": "어떤만두좋아하세요?",
          "original_form": "어떤만두좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100017450.1.13",
          "form": "일단 만두면 다 좋아하는데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "일단 만두면 다 좋아하는데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100017450.1.14",
          "form": "그건 사실 저도그래요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 사실 저도그래요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100017450.1.15",
          "form": "제일 좋아하는건 고기만두같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "제일 좋아하는건 고기만두같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100017450.1.16",
          "form": "얼마전에 얇은피 행사해서 많이쟁여놧어요 ㅋㅋㅋㅋ",
          "original_form": "얼마전에 얇은피 행사해서 많이쟁여놧어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100017450.1.17",
          "form": "생각보다 김치만두도맛잇도라구요",
          "original_form": "생각보다 김치만두도맛잇도라구요",
          "speaker_id": "1",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100017450.1.18",
          "form": "얇은피만두도 맛잇죠",
          "original_form": "얇은피만두도 맛잇죠",
          "speaker_id": "2",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100017450.1.19",
          "form": "김치만두는 만둣국먹을때 특히 맛잇는것같아요",
          "original_form": "김치만두는 만둣국먹을때 특히 맛잇는것같아요",
          "speaker_id": "2",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100017450.1.20",
          "form": "오 만둣국에 김치만두는 안넣어봣어요",
          "original_form": "오 만둣국에 김치만두는 안넣어봣어요",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100017450.1.21",
          "form": "낼 한벙먹어봐랴겟네요ㅋㅋㅋㅋ",
          "original_form": "낼 한벙먹어봐랴겟네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100017450.1.22",
          "form": "군만두랑 찐만두중엔 뭐 좋아하세요?",
          "original_form": "군만두랑 찐만두중엔 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100017450.1.23",
          "form": "어렵네요...둘다맛있는데...",
          "original_form": "어렵네요...둘다맛있는데...",
          "speaker_id": "1",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100017450.1.24",
          "form": "둘다 다른 매력이지않아요?",
          "original_form": "둘다 다른 매력이지않아요?",
          "speaker_id": "1",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100017450.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100017450.1.26",
          "form": "맞아요 둘다 놓칠수없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 둘다 놓칠수없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100017450.1.27",
          "form": "그때그때 땡기는 만두가 다를뿐...!!",
          "original_form": "그때그때 땡기는 만두가 다를뿐...!!",
