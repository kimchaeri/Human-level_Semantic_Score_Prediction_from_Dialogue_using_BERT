      "utterance": [
        {
          "id": "MDRW2100025270.1.1",
          "form": "안녕하세요 요즘 날씨가 별로죠",
          "original_form": "안녕하세요 요즘 날씨가 별로죠",
          "speaker_id": "1",
          "time": "20210901 04:12"
        },
        {
          "id": "MDRW2100025270.1.2",
          "form": "네 계속 비가 왔다 안왔다 하네요",
          "original_form": "네 계속 비가 왔다 안왔다 하네요",
          "speaker_id": "2",
          "time": "20210901 04:13"
        },
        {
          "id": "MDRW2100025270.1.3",
          "form": "그러니까요 우중충해서 기분도 별로고 얼른 비도 그치고 시원해지면 좋겠어요ㅠㅠ",
          "original_form": "그러니까요 우중충해서 기분도 별로고 얼른 비도 그치고 시원해지면 좋겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 04:14"
        },
        {
          "id": "MDRW2100025270.1.4",
          "form": "그쵸 우산 맨날 들고다니기도 무거워요ㅋㅋ",
          "original_form": "그쵸 우산 맨날 들고다니기도 무거워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 04:16"
        },
        {
          "id": "MDRW2100025270.1.5",
          "form": "맞아요 항상 우산 챙겨다녀야하니까ㅋㅋ",
          "original_form": "맞아요 항상 우산 챙겨다녀야하니까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 04:17"
        },
        {
          "id": "MDRW2100025270.1.6",
          "form": "저번에는 가벼운 우산만 들고 나갔는데 거의 태풍오듯이 비가와서 깜짝놀랐어요",
          "original_form": "저번에는 가벼운 우산만 들고 나갔는데 거의 태풍오듯이 비가와서 깜짝놀랐어요",
          "speaker_id": "1",
          "time": "20210901 04:17"
        },
        {
          "id": "MDRW2100025270.1.7",
          "form": "네 기상예보 맨날 틀리니까 우산 꼭 갖고 가야되요ㅋㅋ",
          "original_form": "네 기상예보 맨날 틀리니까 우산 꼭 갖고 가야되요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 04:21"
        },
        {
          "id": "MDRW2100025270.1.8",
          "form": "갑자기 소나기 올때도 있고 사람 놀리는 느낌이에요ㅋㅋ",
          "original_form": "갑자기 소나기 올때도 있고 사람 놀리는 느낌이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 04:22"
        },
        {
          "id": "MDRW2100025270.1.9",
          "form": "맞아요! 무슨 스콜처럼 소나기 쏟아지다가 그치더라구요",
          "original_form": "맞아요! 무슨 스콜처럼 소나기 쏟아지다가 그치더라구요",
          "speaker_id": "1",
          "time": "20210901 04:22"
        },
        {
          "id": "MDRW2100025270.1.10",
          "form": "비오면 기온도 떨어지니까 겉옷도 챙겨다녀야하고..",
          "original_form": "비오면 기온도 떨어지니까 겉옷도 챙겨다녀야하고..",
          "speaker_id": "1",
          "time": "20210901 04:23"
        },
        {
          "id": "MDRW2100025270.1.11",
          "form": "가을장마 얼른 끝나면 좋겠어요  ㅎㅎ",
          "original_form": "가을장마 얼른 끝나면 좋겠어요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 04:23"
        },
        {
          "id": "MDRW2100025270.1.12",
          "form": "네 이번 가을은 묘하게 장마가 긴거같아요",
          "original_form": "네 이번 가을은 묘하게 장마가 긴거같아요",
          "speaker_id": "2",
          "time": "20210901 04:26"
        },
        {
          "id": "MDRW2100025270.1.13",
          "form": "여름에 비가 많이 안왔어서 그런가..",
          "original_form": "여름에 비가 많이 안왔어서 그런가..",
          "speaker_id": "2",
          "time": "20210901 04:26"
        },
        {
          "id": "MDRW2100025270.1.14",
          "form": "아침에 일어나는건 좀 괜찮으세요? 저는 습도가높고 하니까 너무 힘들더라구요",
          "original_form": "아침에 일어나는건 좀 괜찮으세요? 저는 습도가높고 하니까 너무 힘들더라구요",
          "speaker_id": "1",
          "time": "20210901 04:28"
        },
        {
          "id": "MDRW2100025270.1.15",
          "form": "뭔가 개운한느낌이없어요ㅠㅠ",
          "original_form": "뭔가 개운한느낌이없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 04:28"
        },
        {
          "id": "MDRW2100025270.1.16",
          "form": "저 지금 습도 높아서 잠깐 선풍기 틀었어요ㅋㅋ",
          "original_form": "저 지금 습도 높아서 잠깐 선풍기 틀었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 04:29"
        },
        {
          "id": "MDRW2100025270.1.17",
          "form": "저도 며칠전부터 밤에는 에어컨틀고있어요ㅋㅋㅋ",
          "original_form": "저도 며칠전부터 밤에는 에어컨틀고있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 04:30"
        },
        {
          "id": "MDRW2100025270.1.18",
          "form": "에어컨이 습기 제거엔 딱이죠ㅎㅎ",
          "original_form": "에어컨이 습기 제거엔 딱이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 04:31"
        },
        {
          "id": "MDRW2100025270.1.19",
          "form": "내일은 좀 비가 안왔으면 좋겠어요",
          "original_form": "내일은 좀 비가 안왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 04:32"
        }
      ]
    }
