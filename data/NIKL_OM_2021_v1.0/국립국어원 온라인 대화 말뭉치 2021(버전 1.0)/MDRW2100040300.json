      "utterance": [
        {
          "id": "MDRW2100040300.1.1",
          "form": "어떤 음식 좋아하세요,,",
          "original_form": "어떤 음식 좋아하세요,,",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040300.1.2",
          "form": "가리는건 딱히 없어서ㅎㅎ 면요리 특히 좋아하긴 해요",
          "original_form": "가리는건 딱히 없어서ㅎㅎ 면요리 특히 좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040300.1.3",
          "form": "좋아하는 음식있나요?",
          "original_form": "좋아하는 음식있나요?",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040300.1.4",
          "form": "저도 다좋아하긴한데",
          "original_form": "저도 다좋아하긴한데",
          "speaker_id": "1",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040300.1.5",
          "form": "추어탕같은건 안먹어요",
          "original_form": "추어탕같은건 안먹어요",
          "speaker_id": "1",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040300.1.6",
          "form": "점심 메뉴 뭐였어요?",
          "original_form": "점심 메뉴 뭐였어요?",
          "speaker_id": "2",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100040300.1.7",
          "form": "아 아직 못먹었어요 오늘 돈벌라고 출근했거든요ㅋㅋ",
          "original_form": "아 아직 못먹었어요 오늘 돈벌라고 출근했거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:58"
        },
        {
          "id": "MDRW2100040300.1.8",
          "form": "주말 출근ㅠㅠ 얼른 식사 하셔야겠네요",
          "original_form": "주말 출근ㅠㅠ 얼른 식사 하셔야겠네요",
          "speaker_id": "2",
          "time": "20210904 16:02"
        },
        {
          "id": "MDRW2100040300.1.9",
          "form": "넵넵 저녁이 기대되네요",
          "original_form": "넵넵 저녁이 기대되네요",
          "speaker_id": "1",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100040300.1.10",
          "form": "점심 맛난거 드셨어요?",
          "original_form": "점심 맛난거 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100040300.1.11",
          "form": "네 피자 사먹었어요",
          "original_form": "네 피자 사먹었어요",
          "speaker_id": "2",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100040300.1.12",
          "form": "와 어디꺼요?",
          "original_form": "와 어디꺼요?",
          "speaker_id": "1",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100040300.1.13",
          "form": "근처에 파파존스 생겨서 시켜봤어요",
          "original_form": "근처에 파파존스 생겨서 시켜봤어요",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100040300.1.14",
          "form": "맛있네요ㅋㅋㅋ",
          "original_form": "맛있네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100040300.1.15",
          "form": "아 저 이름은 아는데",
          "original_form": "아 저 이름은 아는데",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040300.1.16",
          "form": "한번도 안먹어봤는데",
          "original_form": "한번도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040300.1.17",
          "form": "무슨 메뉴가 좋은가요?",
          "original_form": "무슨 메뉴가 좋은가요?",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100040300.1.18",
          "form": "저도 처음 먹어봐서ㅋㅋ 대표메뉴로 시켜봤어요",
          "original_form": "저도 처음 먹어봐서ㅋㅋ 대표메뉴로 시켜봤어요",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040300.1.19",
          "form": "그냥 일반 콤비네이션 피자 같던데요?",
          "original_form": "그냥 일반 콤비네이션 피자 같던데요?",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040300.1.20",
          "form": "아 그래요? 이번에 처음 드셔보셨어요?",
          "original_form": "아 그래요? 이번에 처음 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040300.1.21",
          "form": "네 근데 괜찮았어요. 맛있더라구요",
          "original_form": "네 근데 괜찮았어요. 맛있더라구요",
          "speaker_id": "2",
          "time": "20210904 16:52"
        }
