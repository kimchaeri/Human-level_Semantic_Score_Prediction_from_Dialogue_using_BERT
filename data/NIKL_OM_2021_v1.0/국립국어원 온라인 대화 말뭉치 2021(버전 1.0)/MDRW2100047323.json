      "utterance": [
        {
          "id": "MDRW2100047323.1.1",
          "form": "요즘 너무 습하죠",
          "original_form": "요즘 너무 습하죠",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100047323.1.2",
          "form": "오늘도  10분 걸으니 온몸에 땀이 범벅 ㅠ",
          "original_form": "오늘도  10분 걸으니 온몸에 땀이 범벅 ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100047323.1.3",
          "form": "네 전 지금 집에서도 땀을 흘리고 있어요",
          "original_form": "네 전 지금 집에서도 땀을 흘리고 있어요",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047323.1.4",
          "form": "윽 습도가",
          "original_form": "윽 습도가",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047323.1.5",
          "form": "청소를 할수가 없네요",
          "original_form": "청소를 할수가 없네요",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047323.1.6",
          "form": "에어컨을 키4고 싶은데 또 덥진 않으니깐 켜기가 아까워요",
          "original_form": "에어컨을 키4고 싶은데 또 덥진 않으니깐 켜기가 아까워요",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100047323.1.7",
          "form": "전 키려고요",
          "original_form": "전 키려고요",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047323.1.8",
          "form": "이게 습도때문에 힘들어서",
          "original_form": "이게 습도때문에 힘들어서",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047323.1.9",
          "form": "습도 날리게요 ㅜㅜ",
          "original_form": "습도 날리게요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047323.1.10",
          "form": "돈이 슬프네요",
          "original_form": "돈이 슬프네요",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047323.1.11",
          "form": "전 좀만 더 참았다가..ㅜㅠ",
          "original_form": "전 좀만 더 참았다가..ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047323.1.12",
          "form": "전기세가 무서워요",
          "original_form": "전기세가 무서워요",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100047323.1.13",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047323.1.14",
          "form": "그래도 안되면 트셔요",
          "original_form": "그래도 안되면 트셔요",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047323.1.15",
          "form": "여름엔 에어컨 전기세 겨울엔 보일러 가스비~",
          "original_form": "여름엔 에어컨 전기세 겨울엔 보일러 가스비~",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047323.1.16",
          "form": "네 겨울이 엄청나와요",
          "original_form": "네 겨울이 엄청나와요",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047323.1.17",
          "form": "작년에 폭탄을",
          "original_form": "작년에 폭탄을",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047323.1.18",
          "form": "겨울에 한 2~30도 나와봤어요",
          "original_form": "겨울에 한 2~30도 나와봤어요",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047323.1.19",
          "form": "전 맨날 그래요 ㅋㅋ",
          "original_form": "전 맨날 그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047323.1.20",
          "form": "40",
          "original_form": "40",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047323.1.21",
          "form": "춥게산다고 생각했ㄴ는데 ㅠ",
          "original_form": "춥게산다고 생각했ㄴ는데 ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047323.1.22",
          "form": "저도요 하나도 안따뜻해요",
          "original_form": "저도요 하나도 안따뜻해요",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100047323.1.23",
          "form": "사계절덕에 2계절에 돈이 팍팍",
          "original_form": "사계절덕에 2계절에 돈이 팍팍",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100047323.1.24",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100047323.1.25",
          "form": "그땐 허리띠 빡 졸라야죠",
          "original_form": "그땐 허리띠 빡 졸라야죠",
