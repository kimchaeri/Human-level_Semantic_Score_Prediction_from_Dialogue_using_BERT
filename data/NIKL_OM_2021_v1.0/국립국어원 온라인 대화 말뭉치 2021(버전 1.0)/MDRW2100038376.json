      "utterance": [
        {
          "id": "MDRW2100038376.1.1",
          "form": "저도 좋아해여",
          "original_form": "저도 좋아해여",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100038376.1.2",
          "form": "예전에 중국 출장가서 처음 먹어봤거든요",
          "original_form": "예전에 중국 출장가서 처음 먹어봤거든요",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.3",
          "form": "어떤 재료 좋아하셔요?!",
          "original_form": "어떤 재료 좋아하셔요?!",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.4",
          "form": "와 헐",
          "original_form": "와 헐",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.5",
          "form": "그때부터 마라를 드셔보셨군요!",
          "original_form": "그때부터 마라를 드셔보셨군요!",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.6",
          "form": "현지꺼는 어때요?",
          "original_form": "현지꺼는 어때요?",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.7",
          "form": "그땐 그게 마라탕인지도 몰랐는데",
          "original_form": "그땐 그게 마라탕인지도 몰랐는데",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038376.1.9",
          "form": "여튼 거기 뷔페에서",
          "original_form": "여튼 거기 뷔페에서",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038376.1.10",
          "form": "엨ㅋㄱㅋㄱ네네",
          "original_form": "엨ㅋㄱㅋㄱ네네",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038376.1.11",
          "form": "우리나라 뷔페에서 쌀국수나 그런거 말아주는거처럼",
          "original_form": "우리나라 뷔페에서 쌀국수나 그런거 말아주는거처럼",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100038376.1.12",
          "form": "아아 네네",
          "original_form": "아아 네네",
          "speaker_id": "1",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100038376.1.13",
          "form": "야채랑 몇가지 재료 챙겨서 주방장한테주면",
          "original_form": "야채랑 몇가지 재료 챙겨서 주방장한테주면",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100038376.1.14",
          "form": "그걸 뜨거운물에 데친다음 빨간육수를 부어주는 형식이었어요",
          "original_form": "그걸 뜨거운물에 데친다음 빨간육수를 부어주는 형식이었어요",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100038376.1.15",
          "form": "오오오 중국은 그렇게도 먹는군요?",
          "original_form": "오오오 중국은 그렇게도 먹는군요?",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038376.1.16",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038376.1.17",
          "form": "저도 한군데밖에 안가보긴했는데",
          "original_form": "저도 한군데밖에 안가보긴했는데",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038376.1.18",
          "form": "한국에서 파는거랑 많이 다른가여?",
          "original_form": "한국에서 파는거랑 많이 다른가여?",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038376.1.19",
          "form": "근데 거기 재료에 천엽이랑 뭐 그런 내장류가 있었거든요",
          "original_form": "근데 거기 재료에 천엽이랑 뭐 그런 내장류가 있었거든요",
          "speaker_id": "2",
          "time": "20210902 11:58"
        }
      ]
