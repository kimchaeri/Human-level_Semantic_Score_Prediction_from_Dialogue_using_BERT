      "utterance": [
        {
          "id": "MDRW2100033720.1.1",
          "form": "너구리 좋아해",
          "original_form": "너구리 좋아해",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.2",
          "form": "너구리 알지?",
          "original_form": "너구리 알지?",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.3",
          "form": "응응 동물 너구리 말고",
          "original_form": "응응 동물 너구리 말고",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.4",
          "form": "난 다시마가 좋아.",
          "original_form": "난 다시마가 좋아.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.5",
          "form": "농심 오동통통 너구리 말하는거지?",
          "original_form": "농심 오동통통 너구리 말하는거지?",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.6",
          "form": "거기 다시마는 꼭 먹어야 해",
          "original_form": "거기 다시마는 꼭 먹어야 해",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.7",
          "form": "어머. 당연하지",
          "original_form": "어머. 당연하지",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.8",
          "form": "나는 다시마를 그렇게 좋아하지않아",
          "original_form": "나는 다시마를 그렇게 좋아하지않아",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.9",
          "form": "너는 그걸 생으로 먹어본적도있니",
          "original_form": "너는 그걸 생으로 먹어본적도있니",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.10",
          "form": "오동통통이랑은 다른 라면이라는 것도 알아줘.",
          "original_form": "오동통통이랑은 다른 라면이라는 것도 알아줘.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.11",
          "form": "없어.",
          "original_form": "없어.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.12",
          "form": "그건 생으로 먹는 라면이 아냐.",
          "original_form": "그건 생으로 먹는 라면이 아냐.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.13",
          "form": "세상에 다른거야?",
          "original_form": "세상에 다른거야?",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.14",
          "form": "정신차려주라.",
          "original_form": "정신차려주라.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.15",
          "form": "이가 나가.",
          "original_form": "이가 나가.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.16",
          "form": "그걸 생으로 먹으면 이가 나갈거야.",
          "original_form": "그걸 생으로 먹으면 이가 나갈거야.",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.17",
          "form": "ㅠㅠ예전에 우리학교 선생님은 그걸 생으로 드셨어",
          "original_form": "ㅠㅠ예전에 우리학교 선생님은 그걸 생으로 드셨어",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033720.1.18",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033720.1.19",
          "form": "내가 실례를 했구나",
          "original_form": "내가 실례를 했구나",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033720.1.20",
          "form": "아주ㅠ건치를 가지셨구나",
          "original_form": "아주ㅠ건치를 가지셨구나",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033720.1.21",
          "form": "그치 취향을 존준해줘",
          "original_form": "그치 취향을 존준해줘",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033720.1.22",
          "form": "그런데 어느학교를 말하는거야?",
          "original_form": "그런데 어느학교를 말하는거야?",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033720.1.23",
          "form": "고등학교!",
          "original_form": "고등학교!",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100033720.1.24",
          "form": "응. 건치를 존중할게.",
          "original_form": "응. 건치를 존중할게.",
          "speaker_id": "2",
          "time": "20210824 23:35"
        }
      ]
    }
