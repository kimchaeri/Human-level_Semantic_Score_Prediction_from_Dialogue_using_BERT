      "utterance": [
        {
          "id": "MDRW2100025388.1.1",
          "form": "그럴땐 카라 고데기로 말면됨",
          "original_form": "그럴땐 카라 고데기로 말면됨",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025388.1.2",
          "form": "다음달에 1주년인데 선물 추천좀요",
          "original_form": "다음달에 1주년인데 선물 추천좀요",
          "speaker_id": "1",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025388.1.3",
          "form": "고데기 읍는데요",
          "original_form": "고데기 읍는데요",
          "speaker_id": "2",
          "time": "20210901 10:40"
        },
        {
          "id": "MDRW2100025388.1.4",
          "form": "1주년 추천을 왜 저한테",
          "original_form": "1주년 추천을 왜 저한테",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025388.1.5",
          "form": "물어보시죠 약올리는건가",
          "original_form": "물어보시죠 약올리는건가",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025388.1.6",
          "form": "뭐 좋아하는데요",
          "original_form": "뭐 좋아하는데요",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025388.1.7",
          "form": "아뇨 ㅋㅋㅋㅋㅋ물어보면안됩니까 ㅠ",
          "original_form": "아뇨 ㅋㅋㅋㅋㅋ물어보면안됩니까 ㅠ",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025388.1.8",
          "form": "조언을 얻는거죠",
          "original_form": "조언을 얻는거죠",
          "speaker_id": "1",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025388.1.9",
          "form": "얻어가기만 하시는 것 같은 느낌적인 느낌",
          "original_form": "얻어가기만 하시는 것 같은 느낌적인 느낌",
          "speaker_id": "2",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025388.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025388.1.11",
          "form": "26에 뭐했지",
          "original_form": "26에 뭐했지",
          "speaker_id": "2",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025388.1.12",
          "form": "일했죠뭐",
          "original_form": "일했죠뭐",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025388.1.13",
          "form": "사원이었겟네",
          "original_form": "사원이었겟네",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025388.1.14",
          "form": "애인이 놀고있으면 선물도 부담되지 않아요?",
          "original_form": "애인이 놀고있으면 선물도 부담되지 않아요?",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025388.1.15",
          "form": "전 그랬는데",
          "original_form": "전 그랬는데",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025388.1.16",
          "form": "부담된다는게 어떤의미?",
          "original_form": "부담된다는게 어떤의미?",
          "speaker_id": "1",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025388.1.17",
          "form": "안해줘도 돼 하는데",
          "original_form": "안해줘도 돼 하는데",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025388.1.18",
          "form": "아 뭔가 그렇게 넘어가면 안될 것 같고",
          "original_form": "아 뭔가 그렇게 넘어가면 안될 것 같고",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025388.1.19",
          "form": "뭐 그런",
          "original_form": "뭐 그런",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025388.1.20",
          "form": "음,,,",
          "original_form": "음,,,",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025388.1.21",
          "form": "그래서 그냥 부담안가는선에서 하자니 뭘해야하나싶고",
          "original_form": "그래서 그냥 부담안가는선에서 하자니 뭘해야하나싶고",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.22",
          "form": "전여친한테 뭐뭐받앗엇음?",
          "original_form": "전여친한테 뭐뭐받앗엇음?",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.23",
          "form": "ㅇㅇ 그런 느낌... 만나면서 큰 선물은 안했는듯요",
          "original_form": "ㅇㅇ 그런 느낌... 만나면서 큰 선물은 안했는듯요",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.24",
          "form": "옷",
          "original_form": "옷",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.25",
          "form": "시계",
          "original_form": "시계",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.26",
          "form": "또 뭐 있지",
          "original_form": "또 뭐 있지",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.27",
          "form": "끝인가",
          "original_form": "끝인가",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025388.1.28",
          "form": "오 시계를 선물해줬어요?",
          "original_form": "오 시계를 선물해줬어요?",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.29",
          "form": "사랑을 받았겠죠",
          "original_form": "사랑을 받았겠죠",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.30",
          "form": "시계 20만언 안되는거요ㅎㅎ",
          "original_form": "시계 20만언 안되는거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.31",
          "form": "근데 제가 차고다니는걸 싫어함",
          "original_form": "근데 제가 차고다니는걸 싫어함",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.32",
          "form": "그래도 알바하면서 삿을텐데",
          "original_form": "그래도 알바하면서 삿을텐데",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.33",
          "form": "알바도 안했어요 집에서 반대해서ㅋ",
          "original_form": "알바도 안했어요 집에서 반대해서ㅋ",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.34",
          "form": "뭐 군말없이 차고다님",
          "original_form": "뭐 군말없이 차고다님",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.35",
          "form": "헥 그럼 용돈으로만?",
          "original_form": "헥 그럼 용돈으로만?",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.36",
          "form": "해줬으니까",
          "original_form": "해줬으니까",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.37",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025388.1.38",
          "form": "아 향수도 있다",
          "original_form": "아 향수도 있다",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025388.1.39",
          "form": "아 향수 진짜 취향이 갈려가지고",
          "original_form": "아 향수 진짜 취향이 갈려가지고",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025388.1.40",
          "form": "나 이 향 좋아 뿌리고와! 이랬는듯",
          "original_form": "나 이 향 좋아 뿌리고와! 이랬는듯",
          "speaker_id": "2",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025388.1.41",
          "form": "아조씨 냄새 나는 향이긴 했는데..",
          "original_form": "아조씨 냄새 나는 향이긴 했는데..",
          "speaker_id": "2",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025388.1.42",
          "form": "저희는 주로 여행갔는데",
          "original_form": "저희는 주로 여행갔는데",
          "speaker_id": "2",
          "time": "20210901 10:53"
        },
        {
          "id": "MDRW2100025388.1.43",
          "form": "코로나 때문에 이젠 그러지도 못할테니",
          "original_form": "코로나 때문에 이젠 그러지도 못할테니",
          "speaker_id": "2",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025388.1.44",
          "form": "아 제주도가기로함",
          "original_form": "아 제주도가기로함",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025388.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025388.1.46",
          "form": "얘도 부담스러워할거같긴한데 그냥 편지나 써줘야겠네요",
          "original_form": "얘도 부담스러워할거같긴한데 그냥 편지나 써줘야겠네요",
          "speaker_id": "1",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100025388.1.47",
          "form": "아 머야 그래서 가시는거군",
          "original_form": "아 머야 그래서 가시는거군",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025388.1.48",
          "form": "그럼 됐죠 경비 님이 더 내죠?",
          "original_form": "그럼 됐죠 경비 님이 더 내죠?",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025388.1.49",
          "form": "여행경비는 반반내는데",
          "original_form": "여행경비는 반반내는데",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025388.1.50",
          "form": "짜잘한건 제가내려고하는편",
          "original_form": "짜잘한건 제가내려고하는편",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025388.1.51",
          "form": "커피먹거나 이런건 ㅋㅋㅋ",
          "original_form": "커피먹거나 이런건 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025388.1.52",
          "form": "전 거의 7:3으로 제가 냈어서ㅋㅋㅋ",
          "original_form": "전 거의 7:3으로 제가 냈어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025388.1.53",
          "form": "보통 여행가면 최소 100은 쓰니까 나름 이게 선물이다 생각했는데 상대는 어떻게 받아드렸을진 모르겠네요",
          "original_form": "보통 여행가면 최소 100은 쓰니까 나름 이게 선물이다 생각했는데 상대는 어떻게 받아드렸을진 모르겠네요",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025388.1.54",
          "form": "숙소가 호,텔이였나봄",
          "original_form": "숙소가 호,텔이였나봄",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025388.1.55",
          "form": "딱히요ㅋㅋㅋ 해외를 주로 나갔었어요",
          "original_form": "딱히요ㅋㅋㅋ 해외를 주로 나갔었어요",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025388.1.56",
          "form": "호.텔은 넘 비쌈",
          "original_form": "호.텔은 넘 비쌈",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025388.1.57",
          "form": "가성비로 주로 찾아다녔어요",
          "original_form": "가성비로 주로 찾아다녔어요",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025388.1.58",
          "form": "사회 초년생이 돈이 어딨슴",
          "original_form": "사회 초년생이 돈이 어딨슴",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025388.1.59",
          "form": "아 맞다 만난지오래됏엇으면",
          "original_form": "아 맞다 만난지오래됏엇으면",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025388.1.60",
          "form": "그때 코로나도없엇겟네",
          "original_form": "그때 코로나도없엇겟네",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025388.1.61",
          "form": "그래서 선물 머할거임?",
          "original_form": "그래서 선물 머할거임?",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025388.1.62",
          "form": "모르겠음 ㅠ",
          "original_form": "모르겠음 ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025388.1.63",
          "form": "뭐가좋음?",
          "original_form": "뭐가좋음?",
