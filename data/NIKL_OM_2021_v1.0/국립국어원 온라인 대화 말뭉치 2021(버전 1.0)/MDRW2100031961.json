      "utterance": [
        {
          "id": "MDRW2100031961.1.1",
          "form": "저랑 남편이 사는데 한시간 거리에 사는 딸이 주말마다 와요",
          "original_form": "저랑 남편이 사는데 한시간 거리에 사는 딸이 주말마다 와요",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100031961.1.2",
          "form": "엄마는 위대합니다",
          "original_form": "엄마는 위대합니다",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100031961.1.3",
          "form": "딸은 결혼했나요",
          "original_form": "딸은 결혼했나요",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100031961.1.4",
          "form": "네 그래서 손자랑 같이 왔어요",
          "original_form": "네 그래서 손자랑 같이 왔어요",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031961.1.5",
          "form": "요즘 거리두기때문에 몇달 못오다가 왔네요",
          "original_form": "요즘 거리두기때문에 몇달 못오다가 왔네요",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031961.1.6",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031961.1.7",
          "form": "저 희도 지금 처갓집에 와  씁니다",
          "original_form": "저 희도 지금 처갓집에 와  씁니다",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031961.1.8",
          "form": "저희도 1시간 거리에요",
          "original_form": "저희도 1시간 거리에요",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031961.1.9",
          "form": "창원 대구",
          "original_form": "창원 대구",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031961.1.10",
          "form": "아들이 15개월이라",
          "original_form": "아들이 15개월이라",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031961.1.11",
          "form": "많이 보고 싶어 하세요",
          "original_form": "많이 보고 싶어 하세요",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031961.1.12",
          "form": "그러니까요.. 1시간 거리라 원래 주말마다 친정오던 애가",
          "original_form": "그러니까요.. 1시간 거리라 원래 주말마다 친정오던 애가",
          "speaker_id": "1",
          "time": "20210905 09:55"
        },
        {
          "id": "MDRW2100031961.1.13",
          "form": "거리두기로 그동안 못왔으니 얼마나 오고 싶었겠어요",
          "original_form": "거리두기로 그동안 못왔으니 얼마나 오고 싶었겠어요",
          "speaker_id": "1",
          "time": "20210905 09:55"
        },
        {
          "id": "MDRW2100031961.1.14",
          "form": "저희도 손주 엄청 보고 싶었구요",
          "original_form": "저희도 손주 엄청 보고 싶었구요",
          "speaker_id": "1",
          "time": "20210905 09:56"
        },
        {
          "id": "MDRW2100031961.1.15",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031961.1.16",
          "form": "오늘  좋은 시간 되실 겁니다",
          "original_form": "오늘  좋은 시간 되실 겁니다",
          "speaker_id": "2",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031961.1.17",
          "form": "원래 가족들은 이렇게 자주봐야하는데",
          "original_form": "원래 가족들은 이렇게 자주봐야하는데",
          "speaker_id": "1",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031961.1.18",
          "form": "코로나가 참 힘들게 하네요",
          "original_form": "코로나가 참 힘들게 하네요",
          "speaker_id": "1",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031961.1.19",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210905 10:02"
        },
        {
          "id": "MDRW2100031961.1.20",
          "form": "오늘 장날이라서",
          "original_form": "오늘 장날이라서",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031961.1.21",
          "form": "장 구경갈 계획입니다",
          "original_form": "장 구경갈 계획입니다",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031961.1.22",
          "form": "장모님하고 같이 장보는거 좋아합니다",
          "original_form": "장모님하고 같이 장보는거 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031961.1.23",
          "form": "그것도 좋은 계획이네요",
          "original_form": "그것도 좋은 계획이네요",
          "speaker_id": "1",
          "time": "20210905 10:03"
        },
        {
          "id": "MDRW2100031961.1.24",
          "form": "스마트폰을 잘 사용하시네요",
          "original_form": "스마트폰을 잘 사용하시네요",
          "speaker_id": "2",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100031961.1.25",
          "form": "딸한테 열심히 배웠답니다",
          "original_form": "딸한테 열심히 배웠답니다",
          "speaker_id": "1",
          "time": "20210905 10:05"
        },
        {
          "id": "MDRW2100031961.1.26",
          "form": "대단하십니다",
          "original_form": "대단하십니다",
          "speaker_id": "2",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100031961.1.27",
          "form": "의지의 차이인 것 같습니다",
          "original_form": "의지의 차이인 것 같습니다",
          "speaker_id": "2",
          "time": "20210905 19:11"
        },
        {
          "id": "MDRW2100031961.1.28",
          "form": "외 아버지는 아직 잘못 하시더라구요",
          "original_form": "외 아버지는 아직 잘못 하시더라구요",
          "speaker_id": "2",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100031961.1.29",
          "form": "스마트폰으로 뭘 할 수 있는지 아시면 흥미가 있으실텐데요",
          "original_form": "스마트폰으로 뭘 할 수 있는지 아시면 흥미가 있으실텐데요",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100031961.1.30",
          "form": "저도 처음엔 굳이 싶다가",
          "original_form": "저도 처음엔 굳이 싶다가",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100031961.1.31",
          "form": "막상 하면서 이것저것 할 수 있는게 많아지니 점점 폰하는 시간이 늘고 있답니다",
          "original_form": "막상 하면서 이것저것 할 수 있는게 많아지니 점점 폰하는 시간이 늘고 있답니다",
          "speaker_id": "1",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100031961.1.32",
          "form": "딸의 역할과 엄마의 의지가 해내셨네오",
          "original_form": "딸의 역할과 엄마의 의지가 해내셨네오",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100031961.1.33",
          "form": "그런데 정말 스마트폰으로는 할게 무궁무진하더라구요",
          "original_form": "그런데 정말 스마트폰으로는 할게 무궁무진하더라구요",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100031961.1.34",
          "form": "터치만 하면 되고",
          "original_form": "터치만 하면 되고",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100031961.1.35",
          "form": "딸의 도움도 컸죠",
          "original_form": "딸의 도움도 컸죠",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100031961.1.36",
          "form": "네 손안에 있는 컴퓨터입니다",
          "original_form": "네 손안에 있는 컴퓨터입니다",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100031961.1.37",
          "form": "가족이랑 영상통화도 맘껏하고",
          "original_form": "가족이랑 영상통화도 맘껏하고",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100031961.1.38",
          "form": "보고싶은 손주사진도 꺼내보고",
          "original_form": "보고싶은 손주사진도 꺼내보고",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100031961.1.39",
          "form": "은행업무에",
          "original_form": "은행업무에",
          "speaker_id": "2",
          "time": "20210905 21:09"
        }
      ]
