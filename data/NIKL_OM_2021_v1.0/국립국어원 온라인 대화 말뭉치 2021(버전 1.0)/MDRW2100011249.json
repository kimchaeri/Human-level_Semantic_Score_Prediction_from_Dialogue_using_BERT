      "utterance": [
        {
          "id": "MDRW2100011249.1.1",
          "form": "저는 기혼입니다  ㅎㅎ",
          "original_form": "저는 기혼입니다  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:24"
        },
        {
          "id": "MDRW2100011249.1.2",
          "form": "아~네~! 저두요~ㅎㅎ 결혼하신지 얼마나 되셨어요?",
          "original_form": "아~네~! 저두요~ㅎㅎ 결혼하신지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210824 10:35"
        },
        {
          "id": "MDRW2100011249.1.3",
          "form": "저는 5년 조금넘었어요",
          "original_form": "저는 5년 조금넘었어요",
          "speaker_id": "2",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100011249.1.4",
          "form": "오~저랑 비슷하시네요~! 저는 2016년 1월에 했거든요",
          "original_form": "오~저랑 비슷하시네요~! 저는 2016년 1월에 했거든요",
          "speaker_id": "1",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100011249.1.5",
          "form": "엇 저도요 ㅋㅋ",
          "original_form": "엇 저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100011249.1.6",
          "form": "오 대박! 1월에 하셨어요?",
          "original_form": "오 대박! 1월에 하셨어요?",
          "speaker_id": "1",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100011249.1.7",
          "form": "1월9일에했어요",
          "original_form": "1월9일에했어요",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100011249.1.8",
          "form": "대받 신기하다 ㅋㅋㅋ",
          "original_form": "대받 신기하다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100011249.1.9",
          "form": "아 저는 1월 26일욯ㅎ",
          "original_form": "아 저는 1월 26일욯ㅎ",
          "speaker_id": "1",
          "time": "20210824 10:37"
        },
        {
          "id": "MDRW2100011249.1.10",
          "form": "어떻게만나셨어요?",
          "original_form": "어떻게만나셨어요?",
          "speaker_id": "2",
          "time": "20210824 10:39"
        },
        {
          "id": "MDRW2100011249.1.11",
          "form": "아~저는 학교에서 만났었어요~",
          "original_form": "아~저는 학교에서 만났었어요~",
          "speaker_id": "1",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100011249.1.12",
          "form": "저도 대학원에서 만났어요",
          "original_form": "저도 대학원에서 만났어요",
          "speaker_id": "2",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011249.1.13",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "2",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011249.1.14",
          "form": "오~네 정말 비슷하네요! 저희는 6년 좀 넘게 사귀고 결혼했는데 파이팅티님은요?",
          "original_form": "오~네 정말 비슷하네요! 저희는 6년 좀 넘게 사귀고 결혼했는데 파이팅티님은요?",
          "speaker_id": "1",
          "time": "20210824 10:48"
        },
        {
          "id": "MDRW2100011249.1.15",
          "form": "저는 1년만났어요 ㅎㅎ",
          "original_form": "저는 1년만났어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011249.1.16",
          "form": "결혼까지 1년 반 걸린거같아요",
          "original_form": "결혼까지 1년 반 걸린거같아요",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011249.1.17",
          "form": "뭐에 씌여서..",
          "original_form": "뭐에 씌여서..",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011249.1.18",
          "form": "아~ 저보다  빨리 결혼하셨네요~",
          "original_form": "아~ 저보다  빨리 결혼하셨네요~",
