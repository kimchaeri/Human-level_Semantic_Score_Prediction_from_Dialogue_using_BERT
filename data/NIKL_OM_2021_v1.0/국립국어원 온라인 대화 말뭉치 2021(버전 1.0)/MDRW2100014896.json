      "utterance": [
        {
          "id": "MDRW2100014896.1.1",
          "form": "최근 구매 잘 한 물건 있으신가요?",
          "original_form": "최근 구매 잘 한 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014896.1.2",
          "form": "이케아 제품이",
          "original_form": "이케아 제품이",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014896.1.3",
          "form": "최근에는",
          "original_form": "최근에는",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014896.1.4",
          "form": "스위치 타이틀 괜찮은 가격에",
          "original_form": "스위치 타이틀 괜찮은 가격에",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014896.1.5",
          "form": "산거같아요",
          "original_form": "산거같아요",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014896.1.6",
          "form": "스위치 재밌나요?",
          "original_form": "스위치 재밌나요?",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014896.1.7",
          "form": "제 친구들도 요새 구입하더라구요",
          "original_form": "제 친구들도 요새 구입하더라구요",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014896.1.8",
          "form": "저는 워낙 게임을 좋아하다보니 재밌어요",
          "original_form": "저는 워낙 게임을 좋아하다보니 재밌어요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014896.1.9",
          "form": "가장쉽게 접할수있는 콘솔이기도 하구요",
          "original_form": "가장쉽게 접할수있는 콘솔이기도 하구요",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014896.1.10",
          "form": "게임 즐겨하시는군요!",
          "original_form": "게임 즐겨하시는군요!",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014896.1.11",
          "form": "얖에서 보면 아기자기 귀엽더라구요",
          "original_form": "얖에서 보면 아기자기 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014896.1.12",
          "form": "동숲 많이하던데",
          "original_form": "동숲 많이하던데",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014896.1.13",
          "form": "그죠 동숲같은 귀여운게임도 있고",
          "original_form": "그죠 동숲같은 귀여운게임도 있고",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014896.1.14",
          "form": "젤다같은 명작도 있고",
          "original_form": "젤다같은 명작도 있고",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014896.1.15",
          "form": "링피트로 운동도 하고 그래요",
          "original_form": "링피트로 운동도 하고 그래요",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014896.1.16",
          "form": "링피트",
          "original_form": "링피트",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014896.1.17",
          "form": "링피트빼고는 중고로 샀어요",
          "original_form": "링피트빼고는 중고로 샀어요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014896.1.18",
          "form": "여러모로 쓸모가 많네요",
          "original_form": "여러모로 쓸모가 많네요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014896.1.19",
          "form": "링피트 어때요?",
          "original_form": "링피트 어때요?",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014896.1.20",
          "form": "잘 하게 되나요?",
          "original_form": "잘 하게 되나요?",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014896.1.21",
          "form": "링피트 하다보면 체력이 느는게 느껴지긴해요",
          "original_form": "링피트 하다보면 체력이 느는게 느껴지긴해요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.22",
          "form": "반년도 채 안했지만 좀 몸이 가벼워요",
          "original_form": "반년도 채 안했지만 좀 몸이 가벼워요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.23",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.24",
          "form": "반년이라니",
          "original_form": "반년이라니",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.25",
          "form": "엄청 대단하세요",
          "original_form": "엄청 대단하세요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.26",
          "form": "해내시면 엄청 뿌듯하겠어요",
          "original_form": "해내시면 엄청 뿌듯하겠어요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.27",
          "form": "역시 운동은 꾸준히 해야하나봐요",
          "original_form": "역시 운동은 꾸준히 해야하나봐요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014896.1.28",
          "form": "꾸준히하는걸 도와주는게 링피트 같아요",
          "original_form": "꾸준히하는걸 도와주는게 링피트 같아요",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014896.1.29",
          "form": "링피트 좋은 게임이네요",
          "original_form": "링피트 좋은 게임이네요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014896.1.30",
          "form": "계속 하시니까 돈 아깝지않겠어요",
          "original_form": "계속 하시니까 돈 아깝지않겠어요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014896.1.31",
          "form": "네 헬스비라고생각하니 벌써 뽕 뽑은 기분이예요ㅋㅋㅋ",
          "original_form": "네 헬스비라고생각하니 벌써 뽕 뽑은 기분이예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014896.1.32",
          "form": "아 그러게요 헬스 비싸죠",
          "original_form": "아 그러게요 헬스 비싸죠",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014896.1.33",
          "form": "그 게임 옆에서 친구가 영업하던데",
          "original_form": "그 게임 옆에서 친구가 영업하던데",
          "speaker_id": "2",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014896.1.34",
          "form": "좀 끌리네요ㅋㅋㅋ",
          "original_form": "좀 끌리네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:27"
        }
      ]
