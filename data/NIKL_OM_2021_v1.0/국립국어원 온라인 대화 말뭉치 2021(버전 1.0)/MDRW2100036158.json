      "utterance": [
        {
          "id": "MDRW2100036158.1.1",
          "form": "수업 들을만 해?",
          "original_form": "수업 들을만 해?",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036158.1.2",
          "form": "살려줘",
          "original_form": "살려줘",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036158.1.3",
          "form": "3시간 풀강",
          "original_form": "3시간 풀강",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036158.1.4",
          "form": "실화냐?",
          "original_form": "실화냐?",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036158.1.5",
          "form": "꺄악",
          "original_form": "꺄악",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036158.1.6",
          "form": "실시간?",
          "original_form": "실시간?",
          "speaker_id": "1",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036158.1.7",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036158.1.8",
          "form": "오늘 몇 과목 들었어?",
          "original_form": "오늘 몇 과목 들었어?",
          "speaker_id": "1",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036158.1.9",
          "form": "1개",
          "original_form": "1개",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100036158.1.10",
          "form": "히히히",
          "original_form": "히히히",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100036158.1.11",
          "form": "풀강이면 진짜 힘들겠다",
          "original_form": "풀강이면 진짜 힘들겠다",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036158.1.12",
          "form": "오티하고 수업도 나갔겠네",
          "original_form": "오티하고 수업도 나갔겠네",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036158.1.13",
          "form": "엉...",
          "original_form": "엉...",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036158.1.14",
          "form": "친구랑 디엠 하면서",
          "original_form": "친구랑 디엠 하면서",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100036158.1.15",
          "form": "이건 좀 에바아니냐",
          "original_form": "이건 좀 에바아니냐",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.16",
          "form": "교수님 당장 끝내주셔도 될것같아요",
          "original_form": "교수님 당장 끝내주셔도 될것같아요",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.17",
          "form": "교수님",
          "original_form": "교수님",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.18",
          "form": "디질것 같아요",
          "original_form": "디질것 같아요",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.19",
          "form": "재발 끝내주세요",
          "original_form": "재발 끝내주세요",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.20",
          "form": "졸려요 이러고 있었음",
          "original_form": "졸려요 이러고 있었음",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100036158.1.22",
          "form": "꺄악...",
          "original_form": "꺄악...",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036158.1.23",
          "form": "우리는 풀강 거의 없걸랑",
          "original_form": "우리는 풀강 거의 없걸랑",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036158.1.24",
          "form": "1시간 15분씩 2번 나눠서 하니까",
          "original_form": "1시간 15분씩 2번 나눠서 하니까",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036158.1.25",
          "form": "첫 시간은 오티하면 거의 끝나",
          "original_form": "첫 시간은 오티하면 거의 끝나",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036158.1.26",
          "form": "우리도 나눠서 하긴 함",
          "original_form": "우리도 나눠서 하긴 함",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036158.1.27",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100036158.1.28",
          "form": "오티 30분하고",
          "original_form": "오티 30분하고",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036158.1.29",
          "form": "수업 다 나가심",
          "original_form": "수업 다 나가심",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036158.1.30",
          "form": "tmi도 많으시고",
          "original_form": "tmi도 많으시고",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100036158.1.31",
          "form": "으으 내용은 어때",
          "original_form": "으으 내용은 어때",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036158.1.32",
          "form": "할만해?",
          "original_form": "할만해?",
          "speaker_id": "1",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036158.1.33",
          "form": "별내용",
          "original_form": "별내용",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036158.1.34",
          "form": "안하긴 함",
          "original_form": "안하긴 함",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036158.1.35",
          "form": "책에 있는 내용인가 싶었음",
          "original_form": "책에 있는 내용인가 싶었음",
          "speaker_id": "2",
          "time": "20210830 17:49"
        },
        {
          "id": "MDRW2100036158.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036158.1.37",
          "form": "이게 뭔 내용일까",
          "original_form": "이게 뭔 내용일까",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036158.1.38",
          "form": "책 미리 사뒀구나",
          "original_form": "책 미리 사뒀구나",
          "speaker_id": "1",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036158.1.39",
          "form": "느낌?",
          "original_form": "느낌?",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036158.1.40",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036158.1.41",
          "form": "난 아직 하나도 안 삼ㅋㅋ",
          "original_form": "난 아직 하나도 안 삼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:51"
        },
        {
          "id": "MDRW2100036158.1.42",
          "form": "책 너무 비싸",
          "original_form": "책 너무 비싸",
          "speaker_id": "1",
          "time": "20210830 17:51"
        }
      ]
