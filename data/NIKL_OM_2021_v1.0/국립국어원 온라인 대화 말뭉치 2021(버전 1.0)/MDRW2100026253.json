      "utterance": [
        {
          "id": "MDRW2100026253.1.1",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026253.1.2",
          "form": "네네 저는근데 컴터 게임좋아해여",
          "original_form": "네네 저는근데 컴터 게임좋아해여",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026253.1.3",
          "form": "무슨 게임 좋아하시나요??",
          "original_form": "무슨 게임 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026253.1.4",
          "form": "저는 컴터가 없어서 ㅜㅜ",
          "original_form": "저는 컴터가 없어서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026253.1.5",
          "form": "스위치 샀어요",
          "original_form": "스위치 샀어요",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026253.1.6",
          "form": "저도 스위치있어요!!",
          "original_form": "저도 스위치있어요!!",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026253.1.7",
          "form": "동숲하다 땅꾸미기 싫어서 접었어요 ㅋㅋㅋㅋ",
          "original_form": "동숲하다 땅꾸미기 싫어서 접었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026253.1.8",
          "form": "헐 저돈데 ㅋㅋㅋㅋㅋㅋ 재미있긴한데",
          "original_form": "헐 저돈데 ㅋㅋㅋㅋㅋㅋ 재미있긴한데",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026253.1.9",
          "form": "너무 노동이더라구요 ㅋㅋ",
          "original_form": "너무 노동이더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026253.1.10",
          "form": "동물 모으는게 제일 재밌는것 ㅋㅋㅋ",
          "original_form": "동물 모으는게 제일 재밌는것 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026253.1.11",
          "form": "맞아요 기여운 애들로만 모으고싶어요",
          "original_form": "맞아요 기여운 애들로만 모으고싶어요",
          "speaker_id": "2",
          "time": "20210902 06:45"
        },
        {
          "id": "MDRW2100026253.1.12",
          "form": "스위치 짱구게임도 하고싶어요",
          "original_form": "스위치 짱구게임도 하고싶어요",
          "speaker_id": "1",
          "time": "20210902 08:01"
        },
        {
          "id": "MDRW2100026253.1.13",
          "form": "저도 신작게임하고싶어요!!!",
          "original_form": "저도 신작게임하고싶어요!!!",
          "speaker_id": "2",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100026253.1.14",
          "form": "근데 일본어판만 나왔데요",
          "original_form": "근데 일본어판만 나왔데요",
          "speaker_id": "1",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100026253.1.15",
          "form": "맞아요 왜 번역을 안해주는걸까요 ㅋㅋㅋ",
          "original_form": "맞아요 왜 번역을 안해주는걸까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:18"
        },
        {
          "id": "MDRW2100026253.1.16",
          "form": "한국에서도 잘 팔릴텐데 너무해요",
          "original_form": "한국에서도 잘 팔릴텐데 너무해요",
          "speaker_id": "1",
          "time": "20210902 08:19"
        },
        {
          "id": "MDRW2100026253.1.17",
          "form": "그러니까요 ㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 08:23"
        },
        {
          "id": "MDRW2100026253.1.18",
          "form": "몇월에 나오는지는 모르겟어요",
