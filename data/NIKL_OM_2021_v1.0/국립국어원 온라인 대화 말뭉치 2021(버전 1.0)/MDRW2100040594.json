      "utterance": [
        {
          "id": "MDRW2100040594.1.1",
          "form": "오 고기고기",
          "original_form": "오 고기고기",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040594.1.2",
          "form": "저는 삼겹살을 더 좋아해요 ㅋㅋ",
          "original_form": "저는 삼겹살을 더 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100040594.1.3",
          "form": "저도삼겹살좋아요",
          "original_form": "저도삼겹살좋아요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040594.1.4",
          "form": "갈비를 더좋아하능거같기두",
          "original_form": "갈비를 더좋아하능거같기두",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040594.1.5",
          "form": "오오 갈비도 좋아요 ㅠㅠ",
          "original_form": "오오 갈비도 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100040594.1.6",
          "form": "구워먹고 싶네요 ㅋㅋ",
          "original_form": "구워먹고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100040594.1.7",
          "form": "네네 그쵸ㅠㅠ",
          "original_form": "네네 그쵸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100040594.1.8",
          "form": "고기먹고싶네여",
          "original_form": "고기먹고싶네여",
          "speaker_id": "2",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100040594.1.9",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100040594.1.10",
          "form": "저는 외식했어요 ㅎㅎ",
          "original_form": "저는 외식했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100040594.1.11",
          "form": "와인더 같이 먹었는데 맛있드라구요!",
          "original_form": "와인더 같이 먹었는데 맛있드라구요!",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100040594.1.12",
          "form": "우와 메인은요?",
          "original_form": "우와 메인은요?",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100040594.1.13",
          "form": "폭립이랑 리조또요!!!!",
          "original_form": "폭립이랑 리조또요!!!!",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100040594.1.14",
          "form": "와인 좋아하세요?",
          "original_form": "와인 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100040594.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100040594.1.16",
          "form": "네네 저 와인 단거좋아해요",
          "original_form": "네네 저 와인 단거좋아해요",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100040594.1.17",
          "form": "오오 저도요 저도 아에 몰라서",
          "original_form": "오오 저도요 저도 아에 몰라서",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040594.1.18",
          "form": "단거 추천해달라해요",
          "original_form": "단거 추천해달라해요",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040594.1.19",
          "form": "와인이랑 어떤거 드세요?",
          "original_form": "와인이랑 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100040594.1.20",
          "form": "저는 와인 보통 소고기랑요",
          "original_form": "저는 와인 보통 소고기랑요",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100040594.1.21",
          "form": "무슨 빌라 어쩌고 있었는데",
          "original_form": "무슨 빌라 어쩌고 있었는데",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100040594.1.22",
          "form": "로제와인? 그런거좋아해요",
          "original_form": "로제와인? 그런거좋아해요",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100040594.1.23",
          "form": "오오 로제와인이 달달한 와인 인가용?",
          "original_form": "오오 로제와인이 달달한 와인 인가용?",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100040594.1.24",
          "form": "그런것같아요 잘 몰라요 ㅋㅋ",
          "original_form": "그런것같아요 잘 몰라요 ㅋㅋ",
