      "utterance": [
        {
          "id": "MDRW2100038827.1.1",
          "form": "유튭 즐겨보시나용?",
          "original_form": "유튭 즐겨보시나용?",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038827.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038827.1.3",
          "form": "저는 요즘 할일없으면 무조건 유튜브보네용...",
          "original_form": "저는 요즘 할일없으면 무조건 유튜브보네용...",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100038827.1.4",
          "form": "뭐보나요?",
          "original_form": "뭐보나요?",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038827.1.5",
          "form": "동물들이나 먹방이요ㅜㅋㅋ",
          "original_form": "동물들이나 먹방이요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038827.1.6",
          "form": "아ㅋㅋ 동물 뭐봐여?",
          "original_form": "아ㅋㅋ 동물 뭐봐여?",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038827.1.7",
          "form": "요즘 앵무새에 푹 빠졌어요",
          "original_form": "요즘 앵무새에 푹 빠졌어요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100038827.1.8",
          "form": "김루이라고 아세요?",
          "original_form": "김루이라고 아세요?",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100038827.1.9",
          "form": "오 아뇨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 아뇨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100038827.1.10",
          "form": "앵무새 이름인가요?",
          "original_form": "앵무새 이름인가요?",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100038827.1.11",
          "form": "네ㅋㅋ유튜브에 검색하시면 나올거에요",
          "original_form": "네ㅋㅋ유튜브에 검색하시면 나올거에요",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100038827.1.12",
          "form": "정말...사람같아요 말을 너무 잘해서",
          "original_form": "정말...사람같아요 말을 너무 잘해서",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100038827.1.13",
          "form": "ㅋㅋㅋㅋㅋ 앵무새 진짜",
          "original_form": "ㅋㅋㅋㅋㅋ 앵무새 진짜",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100038827.1.14",
          "form": "신기해여",
          "original_form": "신기해여",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100038827.1.15",
          "form": "그니까요ㅋㅋ 가족들 나갈때 배웅도 해주더라구요",
          "original_form": "그니까요ㅋㅋ 가족들 나갈때 배웅도 해주더라구요",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100038827.1.16",
          "form": "와 똑똑이네여",
          "original_form": "와 똑똑이네여",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100038827.1.17",
          "form": "배웅까지ㅋㅋㅋㅋㅋ",
          "original_form": "배웅까지ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100038827.1.18",
          "form": "잘해구와~~ 막이래용 넘 다정해 ...",
          "original_form": "잘해구와~~ 막이래용 넘 다정해 ...",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038827.1.19",
          "form": "ㅋㅋㅋㅋㅋ짐짜 신기해요",
          "original_form": "ㅋㅋㅋㅋㅋ짐짜 신기해요",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
