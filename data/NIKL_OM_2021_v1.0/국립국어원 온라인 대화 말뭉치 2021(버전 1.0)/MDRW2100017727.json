        {
          "id": "MDRW2100017727.1.1",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017727.1.2",
          "form": "치킨좋아해요",
          "original_form": "치킨좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017727.1.3",
          "form": "어떤 치킨 좋아하세요?",
          "original_form": "어떤 치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017727.1.4",
          "form": "양념치킨이요",
          "original_form": "양념치킨이요",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017727.1.5",
          "form": "뿌링클은요?",
          "original_form": "뿌링클은요?",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017727.1.6",
          "form": "뿌링클도 좋아해요",
          "original_form": "뿌링클도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017727.1.7",
          "form": "전 요즘 과일 자주 사먹고있어요",
          "original_form": "전 요즘 과일 자주 사먹고있어요",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017727.1.8",
          "form": "오 요즘 어떤 과일이 제철인가요",
          "original_form": "오 요즘 어떤 과일이 제철인가요",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017727.1.9",
          "form": "메론이요",
          "original_form": "메론이요",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017727.1.10",
          "form": "이제 가을이라 포도도 나오겠네요",
          "original_form": "이제 가을이라 포도도 나오겠네요",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017727.1.11",
          "form": "넹ㅋㅋ 메론시즌 끝나기전에 드세요",
          "original_form": "넹ㅋㅋ 메론시즌 끝나기전에 드세요",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017727.1.12",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017727.1.13",
          "form": "핫도그 먹고싶네요",
          "original_form": "핫도그 먹고싶네요",
          "speaker_id": "1",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017727.1.14",
          "form": "ㅜㅜ 명랑핫도그요?",
          "original_form": "ㅜㅜ 명랑핫도그요?",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017727.1.15",
          "form": "네...  통통한 핫도그 먹고싶어요",
          "original_form": "네...  통통한 핫도그 먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017727.1.16",
          "form": "전 갑자기 순대가 먹고싶네요",
          "original_form": "전 갑자기 순대가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017727.1.17",
          "form": "순대꼬치...",
          "original_form": "순대꼬치...",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017727.1.18",
          "form": "너무 맛있겠네요",
          "original_form": "너무 맛있겠네요",
          "speaker_id": "1",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017727.1.19",
          "form": "점심 많이먹었더니 배부르네요",
          "original_form": "점심 많이먹었더니 배부르네요",
