      "utterance": [
        {
          "id": "MDRW2100011135.1.1",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011135.1.2",
          "form": "좋아하는데 잘못해요ㅠㅠ name2님은요?",
          "original_form": "좋아하는데 잘못해요ㅠㅠ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011135.1.3",
          "form": "저도 간단한것만 해요",
          "original_form": "저도 간단한것만 해요",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011135.1.4",
          "form": "블로그보고따라서ㅎ",
          "original_form": "블로그보고따라서ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011135.1.5",
          "form": "제일 잘하는 요리 있으신가용",
          "original_form": "제일 잘하는 요리 있으신가용",
          "speaker_id": "1",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011135.1.6",
          "form": "주로 분식 좋아해요",
          "original_form": "주로 분식 좋아해요",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011135.1.7",
          "form": "떡볶이 오뎅탕ㅎ",
          "original_form": "떡볶이 오뎅탕ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011135.1.8",
          "form": "아 얼마전에 전복사서 조림해봤어요",
          "original_form": "아 얼마전에 전복사서 조림해봤어요",
          "speaker_id": "2",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011135.1.9",
          "form": "전복 손질 어렵지 않나요??!!",
          "original_form": "전복 손질 어렵지 않나요??!!",
          "speaker_id": "1",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011135.1.10",
          "form": "그냥 솔로 박박닦고 내장이랑 입떼면 끝이던데요",
          "original_form": "그냥 솔로 박박닦고 내장이랑 입떼면 끝이던데요",
          "speaker_id": "2",
          "time": "20210824 01:41"
        },
        {
          "id": "MDRW2100011135.1.11",
          "form": "편스토랑 류수영이 하는거보고 사봤거든요",
          "original_form": "편스토랑 류수영이 하는거보고 사봤거든요",
          "speaker_id": "2",
          "time": "20210824 01:41"
        },
        {
          "id": "MDRW2100011135.1.12",
          "form": "헐 그렇군용 해산물은 잘 해보질 않아서 손질이 엄두가 안났는데 유튜브 보면 되네용",
          "original_form": "헐 그렇군용 해산물은 잘 해보질 않아서 손질이 엄두가 안났는데 유튜브 보면 되네용",
          "speaker_id": "1",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011135.1.13",
          "form": "혹시 요리할때 뭐 참고하시나요?",
          "original_form": "혹시 요리할때 뭐 참고하시나요?",
          "speaker_id": "1",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011135.1.14",
          "form": "그냥 블로그 대충보고해요",
          "original_form": "그냥 블로그 대충보고해요",
          "speaker_id": "2",
          "time": "20210824 01:44"
        },
        {
          "id": "MDRW2100011135.1.15",
          "form": "어마어마한 요리는 안하니ㅎ",
          "original_form": "어마어마한 요리는 안하니ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:44"
        },
        {
          "id": "MDRW2100011135.1.16",
          "form": "요리 자주하세요?아님 배달음식 잘 시켜드시는편인가요?",
          "original_form": "요리 자주하세요?아님 배달음식 잘 시켜드시는편인가요?",
          "speaker_id": "1",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011135.1.17",
          "form": "전 인스턴트나 반조리식품이요 장봐서 하기도해요ㅎ",
          "original_form": "전 인스턴트나 반조리식품이요 장봐서 하기도해요ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011135.1.18",
          "form": "혼자라  배달은 잘  안시켜먹고요",
          "original_form": "혼자라  배달은 잘  안시켜먹고요",
          "speaker_id": "2",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011135.1.19",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011135.1.20",
          "form": "저도 배달 잘 안시켜먹는 편이에요 처리가 어려워서 ㅎㅎ",
          "original_form": "저도 배달 잘 안시켜먹는 편이에요 처리가 어려워서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011135.1.21",
          "form": "인스턴트 어떤 제품 주로 사드세요?",
          "original_form": "인스턴트 어떤 제품 주로 사드세요?",
          "speaker_id": "1",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011135.1.22",
          "form": "저 다 못먹으니 버리게되더라구요",
          "original_form": "저 다 못먹으니 버리게되더라구요",
          "speaker_id": "2",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011135.1.23",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011135.1.24",
          "form": "정원샵이나 프레시몰 요런데서 쿠폰뜰때사요ㅎ",
