      "utterance": [
        {
          "id": "MDRW2100044697.1.1",
          "form": "그렇나 ㅎㅎ 많이 마셔라",
          "original_form": "그렇나 ㅎㅎ 많이 마셔라",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044697.1.2",
          "form": "웅웅~ 몸이 따뜻해지는 느낌",
          "original_form": "웅웅~ 몸이 따뜻해지는 느낌",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.3",
          "form": "감기는 다떨어졌나",
          "original_form": "감기는 다떨어졌나",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.4",
          "form": "옹옹! 보이차 많이 마셔서 그런가??",
          "original_form": "옹옹! 보이차 많이 마셔서 그런가??",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.5",
          "form": "그럴수도 맀다",
          "original_form": "그럴수도 맀다",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.6",
          "form": "있다",
          "original_form": "있다",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.7",
          "form": "오 효능 좀 찾아봐야겠다",
          "original_form": "오 효능 좀 찾아봐야겠다",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.8",
          "form": "몸이 따뜻해지고 좋음",
          "original_form": "몸이 따뜻해지고 좋음",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.9",
          "form": "집에 아직 많이있어용?",
          "original_form": "집에 아직 많이있어용?",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.10",
          "form": "그래 ㅋㅋ",
          "original_form": "그래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.11",
          "form": "다 마시면 또 사야겠네~",
          "original_form": "다 마시면 또 사야겠네~",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044697.1.12",
          "form": "아직 많이 있다",
          "original_form": "아직 많이 있다",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044697.1.13",
          "form": "언제 이렇게 사뒀대?ㅋㅋ",
          "original_form": "언제 이렇게 사뒀대?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044697.1.14",
          "form": "이모가 사다줬었다",
          "original_form": "이모가 사다줬었다",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044697.1.15",
          "form": "아하~ 좋네 ㅎㅎ",
          "original_form": "아하~ 좋네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044697.1.16",
          "form": "다마시면 또 사달라하자",
          "original_form": "다마시면 또 사달라하자",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
