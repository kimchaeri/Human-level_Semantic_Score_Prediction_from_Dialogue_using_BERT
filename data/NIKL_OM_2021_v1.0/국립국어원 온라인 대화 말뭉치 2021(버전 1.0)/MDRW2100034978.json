      "utterance": [
        {
          "id": "MDRW2100034978.1.1",
          "form": "맥도날드 창녕마늘버거인가 드셔보셨나요",
          "original_form": "맥도날드 창녕마늘버거인가 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210828 23:16"
        },
        {
          "id": "MDRW2100034978.1.2",
          "form": "헉 그거",
          "original_form": "헉 그거",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.3",
          "form": "호불호 많이 갈리더라구요!",
          "original_form": "호불호 많이 갈리더라구요!",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.4",
          "form": "마늘이 너무 많이 들어갔다궄ㅋㅋㅋㅋㅋ",
          "original_form": "마늘이 너무 많이 들어갔다궄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.5",
          "form": "저 못먹어봣어요",
          "original_form": "저 못먹어봣어요",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.6",
          "form": "ㅜㅜㅜㅜ궁금한데ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ궁금한데ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.7",
          "form": "저 언제 약속없는날 먹어보려고 대기하고있어용",
          "original_form": "저 언제 약속없는날 먹어보려고 대기하고있어용",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.8",
          "form": "그거 시즌 끝나간대요",
          "original_form": "그거 시즌 끝나간대요",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.9",
          "form": "양치 이틀해야 냄새 없어진다고 하더라구요ㅋㅋㅋㅋ",
          "original_form": "양치 이틀해야 냄새 없어진다고 하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.10",
          "form": "ㅜㅜ안돼 ㅜ",
          "original_form": "ㅜㅜ안돼 ㅜ",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.11",
          "form": "앗ㅋㅋㅋㅋ 그정도에요?ㅋㅋㄱ",
          "original_form": "앗ㅋㅋㅋㅋ 그정도에요?ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034978.1.12",
          "form": "요청하는 사람도 있대요",
          "original_form": "요청하는 사람도 있대요",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034978.1.13",
          "form": "마늘 조금만 달라고ㅋㅋㅋ",
          "original_form": "마늘 조금만 달라고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034978.1.14",
          "form": "한국인의 맛..ㅋ 진짜 궁금하다",
          "original_form": "한국인의 맛..ㅋ 진짜 궁금하다",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034978.1.15",
          "form": "저도 갈릭 들어간건 다 좋아하는데",
          "original_form": "저도 갈릭 들어간건 다 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034978.1.16",
          "form": "이건 음...ㅋㅋㅋ",
          "original_form": "이건 음...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034978.1.17",
          "form": "아니 ㅋㅋ  음료는 콜라하구요? 왠지 마늘이니까",
          "original_form": "아니 ㅋㅋ  음료는 콜라하구요? 왠지 마늘이니까",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.18",
          "form": "소주랑먹어야할거같은",
          "original_form": "소주랑먹어야할거같은",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.20",
          "form": "소줔ㅋㅋㅋㅋㅋㅋ",
          "original_form": "소줔ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.21",
          "form": "의외로 궁합 잘 맞는거 아니에용?ㅋㅋㅋㅋ",
          "original_form": "의외로 궁합 잘 맞는거 아니에용?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.22",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.23",
          "form": "아니요 아직이요ㅠㅠㅠㅠ",
          "original_form": "아니요 아직이요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.24",
          "form": "콜라는 마늘하고..어울. 리나?ㅋㅋ",
          "original_form": "콜라는 마늘하고..어울. 리나?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.25",
          "form": "콜라는 다 잘 어울릴것같아요ㅋㅋㅋ",
          "original_form": "콜라는 다 잘 어울릴것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100034978.1.26",
          "form": "사실 저는 햄버거 먹을때",
          "original_form": "사실 저는 햄버거 먹을때",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034978.1.27",
          "form": "사이다로 바꿔서 마셔용",
          "original_form": "사이다로 바꿔서 마셔용",
          "speaker_id": "1",
          "time": "20210828 23:20"
        }
      ]
    }
