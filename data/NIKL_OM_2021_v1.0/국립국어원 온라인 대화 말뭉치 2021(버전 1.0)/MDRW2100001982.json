      },
      "utterance": [
        {
          "id": "MDRW2100001982.1.1",
          "form": "아빠 요새 책좀 보는가",
          "original_form": "아빠 요새 책좀 보는가",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001982.1.2",
          "form": "아니 ㅋㅋ",
          "original_form": "아니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001982.1.3",
          "form": "우리 딸래미는 좀 읽는가",
          "original_form": "우리 딸래미는 좀 읽는가",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001982.1.4",
          "form": "구래 ㅋㅋ 재태크책 많이 읽는다",
          "original_form": "구래 ㅋㅋ 재태크책 많이 읽는다",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001982.1.5",
          "form": "읽는건 아니고 빌려놨지 ㅋ",
          "original_form": "읽는건 아니고 빌려놨지 ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001982.1.6",
          "form": "빌리면 머해",
          "original_form": "빌리면 머해",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001982.1.7",
          "form": "읽어야지",
          "original_form": "읽어야지",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001982.1.8",
          "form": "그르게 ㅠㅋㅋ 맨날 빌리기만하고",
          "original_form": "그르게 ㅠㅋㅋ 맨날 빌리기만하고",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.9",
          "form": "좀 읽을만 하면 연체된다 ㅎ",
          "original_form": "좀 읽을만 하면 연체된다 ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.10",
          "form": "빌리자 마자 읽어라",
          "original_form": "빌리자 마자 읽어라",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.11",
          "form": "구래야겠다 ㅠㅜㅋ",
          "original_form": "구래야겠다 ㅠㅜㅋ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.12",
          "form": "재태크 책 읽을땐 좋은데 읽으려고 마음먹기까지가 넘 어렵다 ㅋ",
          "original_form": "재태크 책 읽을땐 좋은데 읽으려고 마음먹기까지가 넘 어렵다 ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.13",
          "form": "ㅋㅋ 좋은게 다 그렇다",
          "original_form": "ㅋㅋ 좋은게 다 그렇다",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.14",
          "form": "그렇치 ㅜㅜ 아빠가 역시 다 아는구만",
          "original_form": "그렇치 ㅜㅜ 아빠가 역시 다 아는구만",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001982.1.15",
          "form": "그래 다 겪어서 알지",
          "original_form": "그래 다 겪어서 알지",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001982.1.16",
          "form": "그래도 많이 읽어라",
          "original_form": "그래도 많이 읽어라",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001982.1.17",
          "form": "옹 ㅋㅋ 안 빌리고 아에 안 보는것보단 나으니~",
          "original_form": "옹 ㅋㅋ 안 빌리고 아에 안 보는것보단 나으니~",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001982.1.18",
          "form": "읽어보고 좋은책은 사놓고 자주 읽어야겠다",
          "original_form": "읽어보고 좋은책은 사놓고 자주 읽어야겠다",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001982.1.19",
          "form": "그래 같이 읽자",
          "original_form": "그래 같이 읽자",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001982.1.20",
          "form": "옹옹 ㅋㅋ",
          "original_form": "옹옹 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:41"
        }
      ]
