      "utterance": [
        {
          "id": "MDRW2100043064.1.1",
          "form": "안녕하세요 무슨 음식 좋아하세요",
          "original_form": "안녕하세요 무슨 음식 좋아하세요",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100043064.1.2",
          "form": "다 잘먹어요",
          "original_form": "다 잘먹어요",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043064.1.3",
          "form": "한식파입니다",
          "original_form": "한식파입니다",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043064.1.4",
          "form": "오 저도요 고기도 좋아하시나요",
          "original_form": "오 저도요 고기도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043064.1.5",
          "form": "당연히ㅎㅎ",
          "original_form": "당연히ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043064.1.6",
          "form": "고기는사랑이죱",
          "original_form": "고기는사랑이죱",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043064.1.7",
          "form": "무슨 고기 좋아하세요? 저는 삼겹살",
          "original_form": "무슨 고기 좋아하세요? 저는 삼겹살",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043064.1.8",
          "form": "인ㅅ",
          "original_form": "인ㅅ",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043064.1.9",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043064.1.10",
          "form": "저두 삼겹이요!!",
          "original_form": "저두 삼겹이요!!",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100043064.1.11",
          "form": "맛있죠ㅋㅋ 특히 수육이 제일 좋아요",
          "original_form": "맛있죠ㅋㅋ 특히 수육이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043064.1.12",
          "form": "헉 저번주말에 수육먹엇는데",
          "original_form": "헉 저번주말에 수육먹엇는데",
          "speaker_id": "2",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043064.1.13",
          "form": "입에서 녹앗어요",
          "original_form": "입에서 녹앗어요",
          "speaker_id": "2",
          "time": "20210907 13:54"
        },
        {
          "id": "MDRW2100043064.1.14",
          "form": "맛잘알이시네...",
          "original_form": "맛잘알이시네...",
          "speaker_id": "1",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043064.1.15",
          "form": "김치랑먹으면 천국",
          "original_form": "김치랑먹으면 천국",
          "speaker_id": "1",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043064.1.16",
          "form": "ㅋㅋ맞아유",
          "original_form": "ㅋㅋ맞아유",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043064.1.17",
          "form": "천.국",
          "original_form": "천.국",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043064.1.18",
          "form": "어유ㅋㅋㅋㅋ 고기먹고싶어요 이야기하니까",
          "original_form": "어유ㅋㅋㅋㅋ 고기먹고싶어요 이야기하니까",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043064.1.19",
          "form": "그르니까요ㅜㅜ",
          "original_form": "그르니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100043064.1.20",
          "form": "점심 뭐드셨어요",
          "original_form": "점심 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043064.1.21",
          "form": "저는 그냥 백반먹엇어욤",
          "original_form": "저는 그냥 백반먹엇어욤",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043064.1.22",
          "form": "이제",
          "original_form": "이제",
          "speaker_id": "2",
          "time": "20210907 14:20"
        },
        {
          "id": "MDRW2100043064.1.23",
          "form": "아메리카노 수혈햇어요",
          "original_form": "아메리카노 수혈햇어요",
