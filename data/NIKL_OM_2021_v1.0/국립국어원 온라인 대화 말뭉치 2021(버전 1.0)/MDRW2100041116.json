      "utterance": [
        {
          "id": "MDRW2100041116.1.1",
          "form": "카페 자주 가세요?",
          "original_form": "카페 자주 가세요?",
          "speaker_id": "1",
          "time": "20210905 17:32"
        },
        {
          "id": "MDRW2100041116.1.2",
          "form": "요즘은 코로나때문에 많이 못갔네요ㅠㅠ",
          "original_form": "요즘은 코로나때문에 많이 못갔네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 17:33"
        },
        {
          "id": "MDRW2100041116.1.3",
          "form": "스벅 기프티콘이 있어서 조만간 다녀와야할 것 같아요",
          "original_form": "스벅 기프티콘이 있어서 조만간 다녀와야할 것 같아요",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100041116.1.4",
          "form": "카페 자주 다니세요?",
          "original_form": "카페 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100041116.1.5",
          "form": "아니요 자주 가지는 않아요",
          "original_form": "아니요 자주 가지는 않아요",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100041116.1.6",
          "form": "집순이입니다",
          "original_form": "집순이입니다",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100041116.1.7",
          "form": "저두요ㅋㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100041116.1.8",
          "form": "사실 카페커피보단 캡슐커피파에요",
          "original_form": "사실 카페커피보단 캡슐커피파에요",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041116.1.9",
          "form": "커피는 자주 드시나요?",
          "original_form": "커피는 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041116.1.10",
          "form": "전화 더치 커피만 가끔씩 먹고 아이스 아메리카노 좋아해요",
          "original_form": "전화 더치 커피만 가끔씩 먹고 아이스 아메리카노 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041116.1.11",
          "form": "더치커피 한참 유행했었죠!",
          "original_form": "더치커피 한참 유행했었죠!",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041116.1.12",
          "form": "커피를 그렇게 안 좋아하다 보니까 더치커피를 더 먹게 된 거 같아요",
          "original_form": "커피를 그렇게 안 좋아하다 보니까 더치커피를 더 먹게 된 거 같아요",
          "speaker_id": "1",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041116.1.13",
          "form": "확실히 오래보관하고 먹기 편하긴 할 것 같아요ㅋㅋㅋ",
          "original_form": "확실히 오래보관하고 먹기 편하긴 할 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041116.1.14",
          "form": "더치커피 파우치로 파는 거 사서 먹으면 좋더라고요",
          "original_form": "더치커피 파우치로 파는 거 사서 먹으면 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041116.1.15",
          "form": "병에 든거 사서 물이나 우유만 타면 되니까",
          "original_form": "병에 든거 사서 물이나 우유만 타면 되니까",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041116.1.16",
          "form": "1회분으로 포장해서 팔아요",
          "original_form": "1회분으로 포장해서 팔아요",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041116.1.17",
          "form": "전 동그란 병에 다회분 든걸 먹었었는데",
          "original_form": "전 동그란 병에 다회분 든걸 먹었었는데",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041116.1.18",
          "form": "1회분이면 확실히 편하겠어요!",
          "original_form": "1회분이면 확실히 편하겠어요!",
          "speaker_id": "2",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100041116.1.19",
          "form": "네 편하고좋았어요",
          "original_form": "네 편하고좋았어요",
          "speaker_id": "1",
          "time": "20210905 17:40"
        }
      ]
