        {
          "id": "MDRW2100025100.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025100.1.2",
          "form": "네 저 강아지랑 뱀 키우고 있어요",
          "original_form": "네 저 강아지랑 뱀 키우고 있어요",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025100.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025100.1.4",
          "form": "뱀이요?",
          "original_form": "뱀이요?",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025100.1.5",
          "form": "네 애완뱀이요",
          "original_form": "네 애완뱀이요",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025100.1.6",
          "form": "작고 귀여워요",
          "original_form": "작고 귀여워요",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025100.1.7",
          "form": "겁나지 않아요?",
          "original_form": "겁나지 않아요?",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025100.1.8",
          "form": "아니요 ? 독도 없고 진짜 작고 예뻐요",
          "original_form": "아니요 ? 독도 없고 진짜 작고 예뻐요",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025100.1.9",
          "form": "눈이 정말 초롱초롱해요",
          "original_form": "눈이 정말 초롱초롱해요",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025100.1.10",
          "form": "그래도 뱀이라 전 겁나요",
          "original_form": "그래도 뱀이라 전 겁나요",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025100.1.11",
          "form": "그럴수있죠 !!",
          "original_form": "그럴수있죠 !!",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025100.1.12",
          "form": "name1도 동물 키우시나요 ?",
          "original_form": "&name1&도 동물 키우시나요 ?",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025100.1.13",
          "form": "저는 아직",
          "original_form": "저는 아직",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025100.1.14",
          "form": "오 그럼 키워보고 싶은 동물이 있으신가요 ?",
          "original_form": "오 그럼 키워보고 싶은 동물이 있으신가요 ?",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025100.1.15",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100025100.1.16",
          "form": "어떤 종이요 ?",
          "original_form": "어떤 종이요 ?",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025100.1.17",
          "form": "종은 잘 몰라서",
          "original_form": "종은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100025100.1.18",
          "form": "작거 귀여운거요",
          "original_form": "작거 귀여운거요",
