      "utterance": [
        {
          "id": "MDRW2100038454.1.1",
          "form": "푸하하",
          "original_form": "푸하하",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038454.1.2",
          "form": "가족이잖아ㅋ",
          "original_form": "가족이잖아ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038454.1.3",
          "form": "ㅋㅋㅋㅋㄱ 그래 가족이지 ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ 그래 가족이지 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038454.1.4",
          "form": "밥먹고 나니 잠오는군",
          "original_form": "밥먹고 나니 잠오는군",
          "speaker_id": "1",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038454.1.5",
          "form": "언니 토요일날 간담회 갈 거예요??",
          "original_form": "언니 토요일날 간담회 갈 거예요??",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038454.1.6",
          "form": "저번 주 갔다 왔는가",
          "original_form": "저번 주 갔다 왔는가",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038454.1.7",
          "form": "그래나도 너무 배부르다",
          "original_form": "그래나도 너무 배부르다",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038454.1.8",
          "form": "또 오라하드라",
          "original_form": "또 오라하드라",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038454.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038454.1.10",
          "form": "언니 토요일날 약속 있다 안 했어요??",
          "original_form": "언니 토요일날 약속 있다 안 했어요??",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100038454.1.11",
          "form": "현미 관심 있으면 같이 가서 해봐야겠다",
          "original_form": "현미 관심 있으면 같이 가서 해봐야겠다",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100038454.1.12",
          "form": "안그래도안갈까 고민했는데ㅜ",
          "original_form": "안그래도안갈까 고민했는데ㅜ",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100038454.1.13",
          "form": "재건이꺼만 그때 상담해서ㅡㅡ",
          "original_form": "재건이꺼만 그때 상담해서ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100038454.1.14",
          "form": "그럼 거기 과학관 안가고??",
          "original_form": "그럼 거기 과학관 안가고??",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100038454.1.15",
          "form": "2시에 오라고 하시던데",
          "original_form": "2시에 오라고 하시던데",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100038454.1.16",
          "form": "키카로바뀜",
          "original_form": "키카로바뀜",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038454.1.17",
          "form": "키카에있기로했다",
          "original_form": "키카에있기로했다",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038454.1.18",
          "form": "상담받고 날라가야지",
          "original_form": "상담받고 날라가야지",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038454.1.19",
          "form": "어 알았슈",
          "original_form": "어 알았슈",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038454.1.20",
          "form": "그럼 그때 만나서 같이 갑시다",
          "original_form": "그럼 그때 만나서 같이 갑시다",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100038454.1.21",
          "form": "그려ㅋㅋ",
          "original_form": "그려ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100038454.1.22",
          "form": "난 이제 퇴근했슈",
          "original_form": "난 이제 퇴근했슈",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038454.1.23",
          "form": "다시 육 출",
          "original_form": "다시 육 출",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038454.1.24",
          "form": "아;;;고생이 많네ㅜㅜ",
          "original_form": "아;;;고생이 많네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100038454.1.25",
          "form": "난 좀쉬야긋다",
          "original_form": "난 좀쉬야긋다",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100038454.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100038454.1.27",
          "form": "부럽다 언니 나도 쉬고 싶다",
          "original_form": "부럽다 언니 나도 쉬고 싶다",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100038454.1.28",
          "form": "잘 쉬어요 안녕",
          "original_form": "잘 쉬어요 안녕",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100038454.1.29",
          "form": "다쉬었다ㅋ",
          "original_form": "다쉬었다ㅋ",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038454.1.30",
          "form": "이제  집안일하고ㅋ",
          "original_form": "이제  집안일하고ㅋ",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100038454.1.31",
