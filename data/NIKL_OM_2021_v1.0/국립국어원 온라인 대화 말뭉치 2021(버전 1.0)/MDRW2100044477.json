      "utterance": [
        {
          "id": "MDRW2100044477.1.1",
          "form": "저 화이트하임조아해요 ㅋㅋㅋㅋ아니면 쿠키류",
          "original_form": "저 화이트하임조아해요 ㅋㅋㅋㅋ아니면 쿠키류",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044477.1.2",
          "form": "화이트하임 맛잇죠",
          "original_form": "화이트하임 맛잇죠",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044477.1.3",
          "form": "머조아하세요?",
          "original_form": "머조아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044477.1.4",
          "form": "감자칩 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "감자칩 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044477.1.5",
          "form": "전 완전 스낵류 좋아하거든요",
          "original_form": "전 완전 스낵류 좋아하거든요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044477.1.6",
          "form": "오 그렇군여 감자칩 최애가 모에요?",
          "original_form": "오 그렇군여 감자칩 최애가 모에요?",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044477.1.7",
          "form": "최애는 수미칩이에요",
          "original_form": "최애는 수미칩이에요",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044477.1.8",
          "form": "오 수미칩 한번도안먹어봣어요",
          "original_form": "오 수미칩 한번도안먹어봣어요",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044477.1.9",
          "form": "좀 다른가요?",
          "original_form": "좀 다른가요?",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044477.1.10",
          "form": "두꺼운편이에요",
          "original_form": "두꺼운편이에요",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044477.1.11",
          "form": "오 두꺼운 감자칩",
          "original_form": "오 두꺼운 감자칩",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044477.1.12",
          "form": "다른 감자칩들이 바삭 느낌이면 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다른 감자칩들이 바삭 느낌이면 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044477.1.13",
          "form": "수미칩은 와그작 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "수미칩은 와그작 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044477.1.14",
          "form": "오오 맛은 짭짤한편이에요?",
          "original_form": "오오 맛은 짭짤한편이에요?",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044477.1.15",
          "form": "다른 감자칩보다는 조금 덜 짠 편이에요",
          "original_form": "다른 감자칩보다는 조금 덜 짠 편이에요",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044477.1.16",
          "form": "짜면서 두꺼운거 좋아하시면 무뚝뚝감자칩 추천합니다",
          "original_form": "짜면서 두꺼운거 좋아하시면 무뚝뚝감자칩 추천합니다",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044477.1.17",
          "form": "오오 둘다 내일 사머겅야겟어요 ㅋㅋㅋㅋ",
          "original_form": "오오 둘다 내일 사머겅야겟어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044477.1.18",
          "form": "감자칩류 너무 오랜만이라 ㅋㅋㅋ",
          "original_form": "감자칩류 너무 오랜만이라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044477.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ화이트하임 말고는 뭐 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ화이트하임 말고는 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044477.1.20",
          "form": "버터와플 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "버터와플 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044477.1.21",
          "form": "한통 큰거사면 순삭..",
          "original_form": "한통 큰거사면 순삭..",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044477.1.22",
          "form": "이 버터와플 맛잇죠",
