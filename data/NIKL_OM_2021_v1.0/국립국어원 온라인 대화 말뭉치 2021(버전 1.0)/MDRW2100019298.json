      "utterance": [
        {
          "id": "MDRW2100019298.1.1",
          "form": "최근에 단발로 잘랐어요",
          "original_form": "최근에 단발로 잘랐어요",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019298.1.2",
          "form": "이제 거지존이라 또 고민되네요",
          "original_form": "이제 거지존이라 또 고민되네요",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019298.1.3",
          "form": "어떤 헤어스타일이세요",
          "original_form": "어떤 헤어스타일이세요",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019298.1.4",
          "form": "저는 긴머리인데",
          "original_form": "저는 긴머리인데",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019298.1.5",
          "form": "파마가다풀려서",
          "original_form": "파마가다풀려서",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019298.1.6",
          "form": "묭실가야해요ㅠㅠ",
          "original_form": "묭실가야해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019298.1.7",
          "form": "파마 하실거구나 전 더 짧게 칠꺼 고민중이ㅇ에요",
          "original_form": "파마 하실거구나 전 더 짧게 칠꺼 고민중이ㅇ에요",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019298.1.8",
          "form": "단발머리 잘어울리셔서 부러워욥",
          "original_form": "단발머리 잘어울리셔서 부러워욥",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019298.1.9",
          "form": "전 곱슬에 숱부자라서 단발은 미용사분들이 말리시더라구용ㅜ",
          "original_form": "전 곱슬에 숱부자라서 단발은 미용사분들이 말리시더라구용ㅜ",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019298.1.10",
          "form": "아정말요 저도 사실 긴머리가 더 잘어울리능데",
          "original_form": "아정말요 저도 사실 긴머리가 더 잘어울리능데",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019298.1.11",
          "form": "이번에 너무 더워서 그냥 쳐버렸어요 ㅎㅎ",
          "original_form": "이번에 너무 더워서 그냥 쳐버렸어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019298.1.12",
          "form": "긴머리.. 미용실 한번갈때마다 ㅎㄷㄷ",
          "original_form": "긴머리.. 미용실 한번갈때마다 ㅎㄷㄷ",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019298.1.13",
          "form": "정말 비싸유",
          "original_form": "정말 비싸유",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019298.1.14",
          "form": "그쵸 너무비싸",
          "original_form": "그쵸 너무비싸",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019298.1.15",
          "form": "9월에 가서 카드 긁어야겠어요ㅋㅋ",
          "original_form": "9월에 가서 카드 긁어야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019298.1.16",
          "form": "가을옷도 사고싶고...",
          "original_form": "가을옷도 사고싶고...",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019298.1.17",
          "form": "옷이없어요",
          "original_form": "옷이없어요",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019298.1.18",
          "form": "맨날 없어요...ㅎ",
          "original_form": "맨날 없어요...ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019298.1.19",
          "form": "그쵸 저도 그래요 ㅎㄹㅎ",
          "original_form": "그쵸 저도 그래요 ㅎㄹㅎ",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019298.1.20",
          "form": "저는 오늘 가울옷 하나 지르려고요 낼 머리도 잘라야지ㅠ",
          "original_form": "저는 오늘 가울옷 하나 지르려고요 낼 머리도 잘라야지ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019298.1.21",
