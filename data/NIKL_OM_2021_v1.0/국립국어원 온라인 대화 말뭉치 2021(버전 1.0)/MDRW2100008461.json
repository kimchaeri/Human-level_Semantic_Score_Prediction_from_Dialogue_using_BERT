      "utterance": [
        {
          "id": "MDRW2100008461.1.1",
          "form": "편한게 뺄수있는 방법을 알게된후로는 더 운동을 안하게 되네요",
          "original_form": "편한게 뺄수있는 방법을 알게된후로는 더 운동을 안하게 되네요",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008461.1.2",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008461.1.3",
          "form": "뭐예요 ?",
          "original_form": "뭐예요 ?",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008461.1.4",
          "form": "양약이죠ㅋㅋㅋㅋ 약의 힘을 빌려요ㅋ",
          "original_form": "양약이죠ㅋㅋㅋㅋ 약의 힘을 빌려요ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008461.1.5",
          "form": "아 전 좀 안맞아서 먹지 마래여",
          "original_form": "아 전 좀 안맞아서 먹지 마래여",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008461.1.6",
          "form": "우울감 생기지 않아요?",
          "original_form": "우울감 생기지 않아요?",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008461.1.7",
          "form": "부작용이 그거 더라구요",
          "original_form": "부작용이 그거 더라구요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008461.1.8",
          "form": "몸에 딱 맞나봐요",
          "original_form": "몸에 딱 맞나봐요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008461.1.9",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008461.1.10",
          "form": "전 의사쌤이 먹지 마래요",
          "original_form": "전 의사쌤이 먹지 마래요",
          "speaker_id": "2",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008461.1.11",
          "form": "안맞는다고",
          "original_form": "안맞는다고",
          "speaker_id": "2",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008461.1.12",
          "form": "맞아요 부작용 올수있어요",
          "original_form": "맞아요 부작용 올수있어요",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008461.1.13",
          "form": "저는 그전에도 한번씩 먹었어서..",
          "original_form": "저는 그전에도 한번씩 먹었어서..",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008461.1.14",
          "form": "특히 고혈압있으면 먹으면 안된다고 하더라구요",
          "original_form": "특히 고혈압있으면 먹으면 안된다고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008461.1.15",
          "form": "잘맞는듯해요",
          "original_form": "잘맞는듯해요",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008461.1.16",
          "form": "저 혈압도 좀있어요",
          "original_form": "저 혈압도 좀있어요",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008461.1.17",
          "form": "권하지 않으시더라구요",
          "original_form": "권하지 않으시더라구요",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008461.1.18",
          "form": "약이 맞고 넘좋겠어요",
          "original_form": "약이 맞고 넘좋겠어요",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008461.1.19",
          "form": "그쵸 안압도 높아지거든요 ,",
          "original_form": "그쵸 안압도 높아지거든요 ,",
          "speaker_id": "1",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008461.1.20",
          "form": "한약을 먹어볼까 고민도 했네요",
          "original_form": "한약을 먹어볼까 고민도 했네요",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008461.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008461.1.22",
          "form": "참 쉽게 빼는건 앖어요",
          "original_form": "참 쉽게 빼는건 앖어요",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008461.1.23",
          "form": "아무래도 약이다보니 지속적으로 먹을순 없어서 쉬어줘야하긴해요",
          "original_form": "아무래도 약이다보니 지속적으로 먹을순 없어서 쉬어줘야하긴해요",
          "speaker_id": "1",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008461.1.24",
          "form": "만약에 진짜 부작용 없는ㅌ왼벽한 다욧 약 나오면 혁명인데",
          "original_form": "만약에 진짜 부작용 없는ㅌ왼벽한 다욧 약 나오면 혁명인데",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008461.1.25",
          "form": "완전 세계 부자순위 1위 되겠죠 ㅋㅋ",
          "original_form": "완전 세계 부자순위 1위 되겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008461.1.26",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008461.1.27",
          "form": "꼭 건강 챙겨 드세요",
          "original_form": "꼭 건강 챙겨 드세요",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008461.1.28",
          "form": "뭐든 적당히가 좋죠",
          "original_form": "뭐든 적당히가 좋죠",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008461.1.29",
          "form": "네네! 감사합니다~~!!",
          "original_form": "네네! 감사합니다~~!!",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008461.1.30",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008461.1.31",
          "form": "요요없게 운동병행하면 참좋은데",
          "original_form": "요요없게 운동병행하면 참좋은데",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008461.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008461.1.33",
          "form": "그게 쉽지않네요ㅋ",
          "original_form": "그게 쉽지않네요ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:12"
        }
      ]
    }
