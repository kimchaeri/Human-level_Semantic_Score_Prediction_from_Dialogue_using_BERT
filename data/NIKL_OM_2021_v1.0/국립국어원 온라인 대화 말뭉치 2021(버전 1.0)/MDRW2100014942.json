      "utterance": [
        {
          "id": "MDRW2100014942.1.1",
          "form": "최근에 쇼핑한 물건 있으신가요?",
          "original_form": "최근에 쇼핑한 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014942.1.2",
          "form": "최근에 방향제? 룸스프레이? 구매했어요",
          "original_form": "최근에 방향제? 룸스프레이? 구매했어요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014942.1.3",
          "form": "저는 최근에 디퓨저를 구매했어요",
          "original_form": "저는 최근에 디퓨저를 구매했어요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014942.1.4",
          "form": "향이 맘에 들어서 만족 중이에요",
          "original_form": "향이 맘에 들어서 만족 중이에요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014942.1.5",
          "form": "무슨 향이에요?",
          "original_form": "무슨 향이에요?",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014942.1.6",
          "form": "약간 꽃집향이랄까요??",
          "original_form": "약간 꽃집향이랄까요??",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014942.1.7",
          "form": "구매하신 룸스프레이는 어떤 향인가요??",
          "original_form": "구매하신 룸스프레이는 어떤 향인가요??",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014942.1.8",
          "form": "와 제 취향!",
          "original_form": "와 제 취향!",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014942.1.9",
          "form": "제 건 숲향이요",
          "original_form": "제 건 숲향이요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014942.1.10",
          "form": "릴렉싱 목적으로 나왔나봐요",
          "original_form": "릴렉싱 목적으로 나왔나봐요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014942.1.11",
          "form": "향 너무 좋겠다!",
          "original_form": "향 너무 좋겠다!",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014942.1.12",
          "form": "제품을 알 수 있을까요??",
          "original_form": "제품을 알 수 있을까요??",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014942.1.13",
          "form": "록시땅 퓨리파잉 어쩌고예요",
          "original_form": "록시땅 퓨리파잉 어쩌고예요",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014942.1.14",
          "form": "오 다음에 한 번 사봐야겠어요!",
          "original_form": "오 다음에 한 번 사봐야겠어요!",
          "speaker_id": "1",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014942.1.15",
          "form": "name1님 구매하신 건 어디 제품이었어요?",
          "original_form": "&name1&님 구매하신 건 어디 제품이었어요?",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014942.1.16",
          "form": "저는 에이센트 그린에어리요!",
          "original_form": "저는 에이센트 그린에어리요!",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014942.1.17",
          "form": "추천이 가장 많아서 골랐는데 만족하고 있어요 ㅎㅎ",
          "original_form": "추천이 가장 많아서 골랐는데 만족하고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014942.1.18",
          "form": "저도 관심 가네요 꽃집향 힐링될 것 같아요",
          "original_form": "저도 관심 가네요 꽃집향 힐링될 것 같아요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014942.1.19",
          "form": "맞아요! 풀내음+꽃향 이라고 할까요",
          "original_form": "맞아요! 풀내음+꽃향 이라고 할까요",
