      "utterance": [
        {
          "id": "MDRW2100040299.1.1",
          "form": "어떤 걸로 샀나요 먹을 거인가요",
          "original_form": "어떤 걸로 샀나요 먹을 거인가요",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040299.1.2",
          "form": "어제 시켰는데 방금 배송 왔어요",
          "original_form": "어제 시켰는데 방금 배송 왔어요",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040299.1.3",
          "form": "오설록 차 세 개 샀어요",
          "original_form": "오설록 차 세 개 샀어요",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040299.1.4",
          "form": "정말 빠른 배송 이네요 오설록 차 맛있어요",
          "original_form": "정말 빠른 배송 이네요 오설록 차 맛있어요",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040299.1.5",
          "form": "보살 록 선물 세트로 요",
          "original_form": "보살 록 선물 세트로 요",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040299.1.6",
          "form": "무슨 맛이 있나요",
          "original_form": "무슨 맛이 있나요",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.7",
          "form": "쿠팡이 아닌데도 빨리 오네요",
          "original_form": "쿠팡이 아닌데도 빨리 오네요",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.8",
          "form": "안 뜯어봐서 모르겠는데",
          "original_form": "안 뜯어봐서 모르겠는데",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.9",
          "form": "빠른 배송 좋아요",
          "original_form": "빠른 배송 좋아요",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.10",
          "form": "마 아마 녹차 함 녹차 하나",
          "original_form": "마 아마 녹차 함 녹차 하나",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.11",
          "form": "유채꽃 하나",
          "original_form": "유채꽃 하나",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.12",
          "form": "귤차 하나",
          "original_form": "귤차 하나",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.13",
          "form": "제가 마실 건 안 샀나요",
          "original_form": "제가 마실 건 안 샀나요",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.14",
          "form": "그런 느낌으로 여섯 종류 일 거에요",
          "original_form": "그런 느낌으로 여섯 종류 일 거에요",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.15",
          "form": "차 좀 드릴까요",
          "original_form": "차 좀 드릴까요",
          "speaker_id": "1",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.16",
          "form": "다른 지구 분한테 연락해 보고 사진 건가요",
          "original_form": "다른 지구 분한테 연락해 보고 사진 건가요",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100040299.1.17",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.18",
          "form": "그냥 해본 말이에요 괜찮아요 저는 커피 마셔요",
          "original_form": "그냥 해본 말이에요 괜찮아요 저는 커피 마셔요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.19",
          "form": "완화해봤는데",
          "original_form": "완화해봤는데",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.20",
          "form": "본인 거 사셨고요",
          "original_form": "본인 거 사셨고요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.21",
          "form": "다리 세원 엄마",
          "original_form": "다리 세원 엄마",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.22",
          "form": "본인 거 샀다는게 뭘 샀다는 거죠 먹을 건가요",
          "original_form": "본인 거 샀다는게 뭘 샀다는 거죠 먹을 건가요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.23",
          "form": "회원",
          "original_form": "회원",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.24",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.25",
          "form": "엄마한테 지 포",
          "original_form": "엄마한테 지 포",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.26",
          "form": "사서 줄 거래요",
          "original_form": "사서 줄 거래요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040299.1.27",
          "form": "그럼 차를 사셔야겠네요",
          "original_form": "그럼 차를 사셔야겠네요",
          "speaker_id": "1",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100040299.1.28",
          "form": "눈에 좋은 차는 결명자",
          "original_form": "눈에 좋은 차는 결명자",
          "speaker_id": "1",
          "time": "20210905 23:33"
        }
      ]
    }
