      "utterance": [
        {
          "id": "MDRW2100024998.1.1",
          "form": "배달 음식 자주 드시나요?",
          "original_form": "배달 음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024998.1.2",
          "form": "네 자주 먹어요 ㅎㅎ",
          "original_form": "네 자주 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024998.1.3",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024998.1.4",
          "form": "저는 초밥좋아해요",
          "original_form": "저는 초밥좋아해요",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024998.1.5",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024998.1.6",
          "form": "초밥 맛있죠!! 저도 초밥 좋아하고 양식도 좋아해요!",
          "original_form": "초밥 맛있죠!! 저도 초밥 좋아하고 양식도 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024998.1.7",
          "form": "마끼나 카이센동 드셔보셨나요?",
          "original_form": "마끼나 카이센동 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024998.1.8",
          "form": "아 너무좋아해요ㅠㅠㅠ",
          "original_form": "아 너무좋아해요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024998.1.9",
          "form": "8ㅅ8 말만들어도 침나와요",
          "original_form": "8ㅅ8 말만들어도 침나와요",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024998.1.10",
          "form": "그쵸 ㅠㅠㅠ 가격이 좀 사악하지만",
          "original_form": "그쵸 ㅠㅠㅠ 가격이 좀 사악하지만",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024998.1.11",
          "form": "너무 맛있어요 ㅠㅠ",
          "original_form": "너무 맛있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024998.1.12",
          "form": "진짜 가격은 너무 사악해요..",
          "original_form": "진짜 가격은 너무 사악해요..",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100024998.1.13",
          "form": "그쵸 ㅠㅠㅠ 눈물을 머금고 사먹는데 넘 맛있어서 또 먹게 되는 것 같아요ㅠㅠ",
          "original_form": "그쵸 ㅠㅠㅠ 눈물을 머금고 사먹는데 넘 맛있어서 또 먹게 되는 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024998.1.14",
          "form": "후토마끼...먹고싶어요",
          "original_form": "후토마끼...먹고싶어요",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100024998.1.15",
          "form": "저도ㅜㅜ 말하니까 갑자기 먹으러 가고 싶어요!!",
          "original_form": "저도ㅜㅜ 말하니까 갑자기 먹으러 가고 싶어요!!",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100024998.1.16",
          "form": "계란초밥도 좋아하세요?",
          "original_form": "계란초밥도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100024998.1.17",
          "form": "전 일본식 계란말이를 안좋아해서 계란초밥도 안좋아해요ㅠㅠ",
          "original_form": "전 일본식 계란말이를 안좋아해서 계란초밥도 안좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100024998.1.18",
          "form": "아 저는 제가 자주 가는 라멘집 계란 초밥만 좋아해요!! 약간 이삭 토스트에 들어가는 계란마냥 달달 하면서 폭신해서요!",
          "original_form": "아 저는 제가 자주 가는 라멘집 계란 초밥만 좋아해요!! 약간 이삭 토스트에 들어가는 계란마냥 달달 하면서 폭신해서요!",
          "speaker_id": "1",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100024998.1.19",
          "form": "다른 일식 계란 초밥은 굳이 찾아 먹지는 않는 것 같아요",
          "original_form": "다른 일식 계란 초밥은 굳이 찾아 먹지는 않는 것 같아요",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024998.1.20",
          "form": "좋아하는 분들은 초밥집에서 아예 추가하시기도 하더라고요!",
          "original_form": "좋아하는 분들은 초밥집에서 아예 추가하시기도 하더라고요!",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100024998.1.21",
          "form": "아 맞아요 좋아하시는 분들은 초밥집 가서 계란 초밥 추가하는 것 저도 종종 봤어요!!",
          "original_form": "아 맞아요 좋아하시는 분들은 초밥집 가서 계란 초밥 추가하는 것 저도 종종 봤어요!!",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100024998.1.22",
          "form": "그래도 초밥은 회초밥이나 회 제외하고는 소고기 초밥이 저한텐 최고인것 같아요!",
          "original_form": "그래도 초밥은 회초밥이나 회 제외하고는 소고기 초밥이 저한텐 최고인것 같아요!",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100024998.1.23",
          "form": "사실 어제도 먹었는데 또 먹고싶네요..",
          "original_form": "사실 어제도 먹었는데 또 먹고싶네요..",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100024998.1.24",
          "form": "근데 배가 너무 빨리꺼져요ㅠㅠ",
          "original_form": "근데 배가 너무 빨리꺼져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:24"
        },
        {
          "id": "MDRW2100024998.1.25",
          "form": "아 맞아요ㅠㅠ 스시는 배가 빨리꺼지는 느낌이죠",
          "original_form": "아 맞아요ㅠㅠ 스시는 배가 빨리꺼지는 느낌이죠",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100024998.1.26",
          "form": "보통 1인에 12피스정도라 더 그러는것같아요",
          "original_form": "보통 1인에 12피스정도라 더 그러는것같아요",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024998.1.27",
          "form": "아 진짜 그런 것 같아요!",
          "original_form": "아 진짜 그런 것 같아요!",
