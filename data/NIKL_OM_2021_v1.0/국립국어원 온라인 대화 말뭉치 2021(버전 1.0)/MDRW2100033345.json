      "utterance": [
        {
          "id": "MDRW2100033345.1.1",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100033345.1.2",
          "form": "저는 롤핮니다 롤!",
          "original_form": "저는 롤핮니다 롤!",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033345.1.3",
          "form": "역시 롤 하시는군요 ㅋㅋㅋ",
          "original_form": "역시 롤 하시는군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100033345.1.4",
          "form": "ㅋㅋㅋ맞아욤",
          "original_form": "ㅋㅋㅋ맞아욤",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100033345.1.5",
          "form": "역시 롤입니다",
          "original_form": "역시 롤입니다",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100033345.1.6",
          "form": "잘 하시는 편이신가요??",
          "original_form": "잘 하시는 편이신가요??",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100033345.1.7",
          "form": "아니에욤!",
          "original_form": "아니에욤!",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100033345.1.8",
          "form": "즐기는정도..?!!",
          "original_form": "즐기는정도..?!!",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100033345.1.9",
          "form": "에헤이.. 게임도 잘하시는군요",
          "original_form": "에헤이.. 게임도 잘하시는군요",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033345.1.10",
          "form": "뭐 이런 만능이신 분이 다 있죠 ㅋㅋㅋㅋ",
          "original_form": "뭐 이런 만능이신 분이 다 있죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033345.1.11",
          "form": "등급이....?",
          "original_form": "등급이....?",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100033345.1.12",
          "form": "챌린저? 다이아?",
          "original_form": "챌린저? 다이아?",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100033345.1.13",
          "form": "어...다이아3...?이욤",
          "original_form": "어...다이아3...?이욤",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033345.1.14",
          "form": "아니.. 버스타고 싶어지는 등급인데요 ㅋㅋㅋㅋ",
          "original_form": "아니.. 버스타고 싶어지는 등급인데요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033345.1.15",
          "form": "아잌ㅋㅋㅋ여기닉넴이",
          "original_form": "아잌ㅋㅋㅋ여기닉넴이",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033345.1.16",
          "form": "롤닉넴이에염",
          "original_form": "롤닉넴이에염",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033345.1.17",
          "form": "영어로 같은 닉네임이세요?",
          "original_form": "영어로 같은 닉네임이세요?",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033345.1.18",
          "form": "네넼ㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033345.1.19",
          "form": "아.. 근데 제가 또 럭스 장인인데",
          "original_form": "아.. 근데 제가 또 럭스 장인인데",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033345.1.20",
          "form": "주로 서포터하시나봐요 ㅠ",
          "original_form": "주로 서포터하시나봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033345.1.21",
          "form": "우아아앙!!!",
          "original_form": "우아아앙!!!",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033345.1.22",
          "form": "저 숙련도 7렙이에요 ㅋㅋㅋ",
          "original_form": "저 숙련도 7렙이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033345.1.23",
          "form": "우아아아ㅏㅇㅇ 같이해요!ㅋㅋㅋ",
          "original_form": "우아아아ㅏㅇㅇ 같이해요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033345.1.24",
          "form": "킼 버스비 비쌉니다",
          "original_form": "킼 버스비 비쌉니다",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033345.1.25",
          "form": "어휴.. 제 동생이 핸드폰 가져갔다 했더니.. 이런.. 망언을..",
          "original_form": "어휴.. 제 동생이 핸드폰 가져갔다 했더니.. 이런.. 망언을..",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100033345.1.26",
          "form": "아잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100033345.1.27",
          "form": "괜찮습니다",
          "original_form": "괜찮습니다",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100033345.1.28",
          "form": "지가 버스를 타야할 판이면서 이게 무슨.. 어휴..",
          "original_form": "지가 버스를 타야할 판이면서 이게 무슨.. 어휴..",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100033345.1.29",
          "form": "제가 죄송합니다",
          "original_form": "제가 죄송합니다",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100033345.1.30",
          "form": "에이 아니옵니다아아아",
          "original_form": "에이 아니옵니다아아아",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100033345.1.31",
          "form": "제가 버스탈수고있죠!",
          "original_form": "제가 버스탈수고있죠!",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100033345.1.32",
          "form": "아마.. 하면서 저희 부모님을 지키지 못할까봐 차마 소개시켜드리기가...",
          "original_form": "아마.. 하면서 저희 부모님을 지키지 못할까봐 차마 소개시켜드리기가...",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100033345.1.33",
          "form": "떨결에 쟤가 닉네임을 알게 되서 혹시 이상하게 못하는 럭스 서포터 고르는 애가 있으면",
          "original_form": "떨결에 쟤가 닉네임을 알게 되서 혹시 이상하게 못하는 럭스 서포터 고르는 애가 있으면",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100033345.1.34",
          "form": "모르는 척 한번만 놀아주세요 ㅋㅋㅋ",
          "original_form": "모르는 척 한번만 놀아주세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100033345.1.35",
          "form": "얘가 서포터 말고 다른거 하는걸 잘 못 봤거든요",
          "original_form": "얘가 서포터 말고 다른거 하는걸 잘 못 봤거든요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100033345.1.36",
          "form": "아잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100033345.1.37",
          "form": "괜차나요오옹",
          "original_form": "괜차나요오옹",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100033345.1.38",
          "form": "시간되면 같이 즐겨요 그냥",
          "original_form": "시간되면 같이 즐겨요 그냥",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100033345.1.39",
          "form": "증말로... 조용하길래 뭐 하나 했더니만..",
          "original_form": "증말로... 조용하길래 뭐 하나 했더니만..",
