      "utterance": [
        {
          "id": "MDRW2100011378.1.1",
          "form": "최근에 사신것있나요ㅣ",
          "original_form": "최근에 사신것있나요ㅣ",
          "speaker_id": "1",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100011378.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100011378.1.3",
          "form": "매일 사는편이라",
          "original_form": "매일 사는편이라",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011378.1.4",
          "form": "어제는 주꾸미볶음을 주문했어여",
          "original_form": "어제는 주꾸미볶음을 주문했어여",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011378.1.5",
          "form": "팩으로 나와서 먹기좋아요",
          "original_form": "팩으로 나와서 먹기좋아요",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011378.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 12:10"
        },
        {
          "id": "MDRW2100011378.1.7",
          "form": "하남쭈꾸미죠???",
          "original_form": "하남쭈꾸미죠???",
          "speaker_id": "1",
          "time": "20210824 12:10"
        },
        {
          "id": "MDRW2100011378.1.8",
          "form": "앗 그거아세요?ㅋㅋㅋ",
          "original_form": "앗 그거아세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:12"
        },
        {
          "id": "MDRW2100011378.1.9",
          "form": "똥손이 볶아도 안질기고 맛있어서 자주 삽니다",
          "original_form": "똥손이 볶아도 안질기고 맛있어서 자주 삽니다",
          "speaker_id": "2",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011378.1.10",
          "form": "ㅋㅋㅋ 유명하죠 하남쭈꾸미",
          "original_form": "ㅋㅋㅋ 유명하죠 하남쭈꾸미",
          "speaker_id": "1",
          "time": "20210824 12:22"
        },
        {
          "id": "MDRW2100011378.1.11",
          "form": "맞아요 탱글하더라고요 생각보다",
          "original_form": "맞아요 탱글하더라고요 생각보다",
          "speaker_id": "1",
          "time": "20210824 12:22"
        },
        {
          "id": "MDRW2100011378.1.12",
          "form": "깻잎 쌈무 날치알은요?!",
          "original_form": "깻잎 쌈무 날치알은요?!",
          "speaker_id": "1",
          "time": "20210824 12:22"
        },
        {
          "id": "MDRW2100011378.1.13",
          "form": "같이 먹는건가요?",
          "original_form": "같이 먹는건가요?",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011378.1.14",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011378.1.15",
          "form": "마요네즈에 쭈꾸미 쌈",
          "original_form": "마요네즈에 쭈꾸미 쌈",
          "speaker_id": "1",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011378.1.16",
          "form": "마요네즈요?",
          "original_form": "마요네즈요?",
          "speaker_id": "2",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011378.1.17",
          "form": "전혀몰랐는데",
          "original_form": "전혀몰랐는데",
          "speaker_id": "2",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011378.1.18",
          "form": "마요네즈 어울리나요",
          "original_form": "마요네즈 어울리나요",
          "speaker_id": "2",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011378.1.19",
          "form": "쭈꾸미엔",
          "original_form": "쭈꾸미엔",
          "speaker_id": "1",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011378.1.20",
          "form": "마요네즈아닌가요?!!",
          "original_form": "마요네즈아닌가요?!!",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011378.1.21",
          "form": "사서 같이 먹어봐야겧네요",
          "original_form": "사서 같이 먹어봐야겧네요",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011378.1.22",
          "form": "이거저거 같이 먹어봐도 별로더라고요",
          "original_form": "이거저거 같이 먹어봐도 별로더라고요",
          "speaker_id": "2",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011378.1.23",
          "form": "그래서 전 밥에다 쭈꾸미만 비벼먹었었거든요ㅋㅋ",
          "original_form": "그래서 전 밥에다 쭈꾸미만 비벼먹었었거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011378.1.24",
          "form": "사진이 첨부되면 좋을텐데 아쉬워요",
          "original_form": "사진이 첨부되면 좋을텐데 아쉬워요",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011378.1.25",
          "form": "쌈을 보여드리고싶네요ㅋㅋㅋ",
          "original_form": "쌈을 보여드리고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011378.1.26",
          "form": "좋은팁 고맙습니다\r\n구매목록이 또 늘었네요ㅋㅋ",
          "original_form": "좋은팁 고맙습니다\r\n구매목록이 또 늘었네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011378.1.27",
          "form": "ㅋㅋㅋ 저도맛있게먹었었거든요 그쭈꾸미",
          "original_form": "ㅋㅋㅋ 저도맛있게먹었었거든요 그쭈꾸미",
          "speaker_id": "1",
