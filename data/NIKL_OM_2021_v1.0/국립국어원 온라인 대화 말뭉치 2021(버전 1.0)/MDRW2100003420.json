      "utterance": [
        {
          "id": "MDRW2100003420.1.1",
          "form": "유튜브 머 재밌는거 없나",
          "original_form": "유튜브 머 재밌는거 없나",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003420.1.2",
          "form": "피식대학 너무 재밌오",
          "original_form": "피식대학 너무 재밌오",
          "speaker_id": "1",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100003420.1.3",
          "form": "나는 공부왕찐천재 재밋도라",
          "original_form": "나는 공부왕찐천재 재밋도라",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003420.1.4",
          "form": "오 그건 뭐야 나 바로 봐바야지",
          "original_form": "오 그건 뭐야 나 바로 봐바야지",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003420.1.5",
          "form": "홍진경나오는건데 엄청재밋어 ㅋㅋㅋ",
          "original_form": "홍진경나오는건데 엄청재밋어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100003420.1.6",
          "form": "아ㅋㅋㅋㅋ홍진경 하니까 바로 생각났어 짱웃기잖아 그거",
          "original_form": "아ㅋㅋㅋㅋ홍진경 하니까 바로 생각났어 짱웃기잖아 그거",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100003420.1.7",
          "form": "어 마자 ㅋㅋㅋㅋ 이번에 워크맨도재밋드라",
          "original_form": "어 마자 ㅋㅋㅋㅋ 이번에 워크맨도재밋드라",
          "speaker_id": "2",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100003420.1.8",
          "form": "ㅋㅋㅋㅋ그에 반해 네고왕은....좀........평이 안좋더라",
          "original_form": "ㅋㅋㅋㅋ그에 반해 네고왕은....좀........평이 안좋더라",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100003420.1.9",
          "form": "저번에 배라햇을때 점주들한테 피해가서그래",
          "original_form": "저번에 배라햇을때 점주들한테 피해가서그래",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100003420.1.10",
          "form": "완전 ㅡㅡ나빴어",
          "original_form": "완전 ㅡㅡ나빴어",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100003420.1.11",
          "form": "맞아 그건 진짜 너무하더라",
          "original_form": "맞아 그건 진짜 너무하더라",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100003420.1.12",
          "form": "점주 뜯어먹기였잖아ㅜㅜ",
          "original_form": "점주 뜯어먹기였잖아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100003420.1.13",
          "form": "그니까ㅠㅠ이미예약한상태에서 그거봐가지고",
          "original_form": "그니까ㅠㅠ이미예약한상태에서 그거봐가지고",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100003420.1.14",
          "form": "그랫으면 안삿을텐뎀",
          "original_form": "그랫으면 안삿을텐뎀",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100003420.1.15",
          "form": "ㅠ마쟈 할인폭도 그렇게 크지도 않았어ㅠ",
          "original_form": "ㅠ마쟈 할인폭도 그렇게 크지도 않았어ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100003420.1.16",
          "form": "원래 복날이벤트로 하려고햇엇다더라,,",
          "original_form": "원래 복날이벤트로 하려고햇엇다더라,,",
          "speaker_id": "2",
          "time": "20210823 07:57"
        },
        {
          "id": "MDRW2100003420.1.17",
          "form": "출근하려니까 비오네ㅠ",
          "original_form": "출근하려니까 비오네ㅠ",
          "speaker_id": "2",
          "time": "20210823 07:57"
        },
        {
          "id": "MDRW2100003420.1.18",
          "form": "흥퉤 비오는날엔 드라마보면서 출근해야지",
          "original_form": "흥퉤 비오는날엔 드라마보면서 출근해야지",
          "speaker_id": "1",
          "time": "20210823 08:23"
        },
        {
          "id": "MDRW2100003420.1.19",
          "form": "대탈출",
          "original_form": "대탈출",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100003420.1.20",
          "form": "오늘 출근준비하면서 봤는데 재밌어",
          "original_form": "오늘 출근준비하면서 봤는데 재밌어",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100003420.1.21",
          "form": "아 이번에 시즌 새로 시작했지",
          "original_form": "아 이번에 시즌 새로 시작했지",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100003420.1.22",
          "form": "어때 괜찮아?",
          "original_form": "어때 괜찮아?",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100003420.1.23",
          "form": "어 재밌어! 한번봐바",
          "original_form": "어 재밌어! 한번봐바",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100003420.1.24",
          "form": "나도 한번 해보고싶더라",
          "original_form": "나도 한번 해보고싶더라",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100003420.1.25",
          "form": "시즌 1이랑 2는 챙겨봤었는데 이번꺼는 바빠서 못봤거든",
          "original_form": "시즌 1이랑 2는 챙겨봤었는데 이번꺼는 바빠서 못봤거든",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100003420.1.26",
          "form": "ㅋㅋㅋㅋ방탈출 안해봐썽?",
          "original_form": "ㅋㅋㅋㅋ방탈출 안해봐썽?",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100003420.1.27",
          "form": "해봣는데 대탈출은 스케일이다르자나 ㅋㅋㅋ",
          "original_form": "해봣는데 대탈출은 스케일이다르자나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100003420.1.28",
          "form": "막 방이바뀌고 순간이동하고 이런게재밋더라",
          "original_form": "막 방이바뀌고 순간이동하고 이런게재밋더라",
          "speaker_id": "2",
          "time": "20210823 19:00"
        },
        {
          "id": "MDRW2100003420.1.29",
          "form": "ㅋㅋㅋㅋ하긴 그렇긴 해 타임머신 짱;;",
          "original_form": "ㅋㅋㅋㅋ하긴 그렇긴 해 타임머신 짱;;",
          "speaker_id": "1",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100003420.1.30",
          "form": "이번에도 땅이 밑으로내려가서 방이바뀌고 그래 ㅋㅋㅋㅋ신기하지",
          "original_form": "이번에도 땅이 밑으로내려가서 방이바뀌고 그래 ㅋㅋㅋㅋ신기하지",
          "speaker_id": "2",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100003420.1.31",
          "form": "오ㅋㅋㅋㅋ웅 완전",
          "original_form": "오ㅋㅋㅋㅋ웅 완전",
          "speaker_id": "1",
          "time": "20210823 19:07"
        },
        {
          "id": "MDRW2100003420.1.32",
          "form": "봐야겠다",
          "original_form": "봐야겠다",
          "speaker_id": "1",
          "time": "20210823 19:07"
        }
      ]
    }
