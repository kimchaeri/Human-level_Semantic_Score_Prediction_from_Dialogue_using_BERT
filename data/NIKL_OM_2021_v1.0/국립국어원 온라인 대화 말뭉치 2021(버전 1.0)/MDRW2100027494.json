      "utterance": [
        {
          "id": "MDRW2100027494.1.1",
          "form": "지금은 안와요",
          "original_form": "지금은 안와요",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027494.1.2",
          "form": "나름 화창한 날씨예요",
          "original_form": "나름 화창한 날씨예요",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027494.1.3",
          "form": "그렇군요 저희는 하루 종일 오고 있네요 ㅎㅎ",
          "original_form": "그렇군요 저희는 하루 종일 오고 있네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027494.1.4",
          "form": "이동시간에는 비가 안오는게 최고죠!",
          "original_form": "이동시간에는 비가 안오는게 최고죠!",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027494.1.5",
          "form": "어느 지역이세요?",
          "original_form": "어느 지역이세요?",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027494.1.6",
          "form": "여긴 서울입니다",
          "original_form": "여긴 서울입니다",
          "speaker_id": "2",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027494.1.7",
          "form": "전남이에요!",
          "original_form": "전남이에요!",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027494.1.8",
          "form": "어제는 마른벼락이 계속 치더라구요",
          "original_form": "어제는 마른벼락이 계속 치더라구요",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027494.1.9",
          "form": "장관이었지만 무서웠어요",
          "original_form": "장관이었지만 무서웠어요",
          "speaker_id": "1",
          "time": "20210902 13:12"
        },
        {
          "id": "MDRW2100027494.1.10",
          "form": "헉! 마른벼락! 무섭죠",
          "original_form": "헉! 마른벼락! 무섭죠",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027494.1.11",
          "form": "그쪽지방이 지금 비가 내리는군요",
          "original_form": "그쪽지방이 지금 비가 내리는군요",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027494.1.12",
          "form": "서울은 그저께 쏟아졌으니까 비구름이 내려간걸까요?",
          "original_form": "서울은 그저께 쏟아졌으니까 비구름이 내려간걸까요?",
          "speaker_id": "2",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100027494.1.13",
          "form": "맞아요~ 비가 계속 내려요",
          "original_form": "맞아요~ 비가 계속 내려요",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027494.1.14",
          "form": "그랬나봐요!",
          "original_form": "그랬나봐요!",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027494.1.15",
          "form": "내일 아침까지 내릴 것 같아요 ㅎㅎ",
          "original_form": "내일 아침까지 내릴 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027494.1.16",
          "form": "혹시 장화 신으셨나요?",
          "original_form": "혹시 장화 신으셨나요?",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027494.1.17",
          "form": "아니요 장화 하나 구매 하고싶네요!",
          "original_form": "아니요 장화 하나 구매 하고싶네요!",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100027494.1.18",
          "form": "비올때 주로 어떤 옷차림으로 생활하시나요?",
          "original_form": "비올때 주로 어떤 옷차림으로 생활하시나요?",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027494.1.19",
          "form": "나갈때 비가 오면 장화에 반바지요",
          "original_form": "나갈때 비가 오면 장화에 반바지요",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027494.1.20",
          "form": "젖어도 수습이 쉽달까요",
          "original_form": "젖어도 수습이 쉽달까요",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027494.1.21",
          "form": "그렇죠 장화에 반바지가 정말 좋은 차림인 것 같아요",
          "original_form": "그렇죠 장화에 반바지가 정말 좋은 차림인 것 같아요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027494.1.22",
          "form": "긴바지면 물이 튀잖아요 ㅠ",
          "original_form": "긴바지면 물이 튀잖아요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027494.1.23",
          "form": "장화하나 마련해야겠어요!",
          "original_form": "장화하나 마련해야겠어요!",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027494.1.24",
          "form": "진짜 슬랙스 입었는데 비에 젖어서 종아리에 착 달라붙는거 있죠",
          "original_form": "진짜 슬랙스 입었는데 비에 젖어서 종아리에 착 달라붙는거 있죠",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027494.1.25",
          "form": "기분 진짜 찝찝하고 무겁고... ㅎ",
          "original_form": "기분 진짜 찝찝하고 무겁고... ㅎ",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027494.1.26",
          "form": "헉 그럼 그날의 기분이 좋지 못하잖아요 ㅠ",
          "original_form": "헉 그럼 그날의 기분이 좋지 못하잖아요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027494.1.27",
          "form": "역시 비올때 안나갈수있는게 최고네요",
          "original_form": "역시 비올때 안나갈수있는게 최고네요",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027494.1.28",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027494.1.29",
          "form": "한 한시간정도 불쾌하게 있는거죠",
          "original_form": "한 한시간정도 불쾌하게 있는거죠",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027494.1.30",
          "form": "그래도 한시간 정도면 마르긴합디다... ㅎ",
          "original_form": "그래도 한시간 정도면 마르긴합디다... ㅎ",
          "speaker_id": "2",
          "time": "20210902 14:55"
        }
      ]
    }
