      "utterance": [
        {
          "id": "MDRW2100042850.1.1",
          "form": "뭐가남냐그러더라",
          "original_form": "뭐가남냐그러더라",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042850.1.2",
          "form": "ㅋㅋㅋㅋ홍보비대신이라 생각하면 싸",
          "original_form": "ㅋㅋㅋㅋ홍보비대신이라 생각하면 싸",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042850.1.3",
          "form": "이제 알았으니 다들 사먹을수도..",
          "original_form": "이제 알았으니 다들 사먹을수도..",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042850.1.4",
          "form": "이히긴 너해도 괨찮다니",
          "original_form": "이히긴 너해도 괨찮다니",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042850.1.5",
          "form": "최근에 파닭먹음?",
          "original_form": "최근에 파닭먹음?",
          "speaker_id": "2",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042850.1.6",
          "form": "왕천못참지",
          "original_form": "왕천못참지",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042850.1.7",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042850.1.8",
          "form": "비오니까먹고파",
          "original_form": "비오니까먹고파",
          "speaker_id": "2",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042850.1.9",
          "form": "근데데유ㅓ먹으니가 조금 별류",
          "original_form": "근데데유ㅓ먹으니가 조금 별류",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042850.1.10",
          "form": "나 그거먹고싶어",
          "original_form": "나 그거먹고싶어",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042850.1.11",
          "form": "에후했는데?",
          "original_form": "에후했는데?",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.12",
          "form": "똥집튀김",
          "original_form": "똥집튀김",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.13",
          "form": "아 못참지",
          "original_form": "아 못참지",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.14",
          "form": "ㅇㅇ 그 특유의 향이 좀 날아감",
          "original_form": "ㅇㅇ 그 특유의 향이 좀 날아감",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.15",
          "form": "먹고싶다 겨자소스",
          "original_form": "먹고싶다 겨자소스",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.16",
          "form": "똥집튀김에 맥주",
          "original_form": "똥집튀김에 맥주",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.17",
          "form": "햐 그거지",
          "original_form": "햐 그거지",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.18",
          "form": "아니면 떡볶이에 소듀",
          "original_form": "아니면 떡볶이에 소듀",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.19",
          "form": "엽떡먹고파",
          "original_form": "엽떡먹고파",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.20",
          "form": "막걸리도먹고싶다",
          "original_form": "막걸리도먹고싶다",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.21",
          "form": "오 개조아",
          "original_form": "오 개조아",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042850.1.22",
          "form": "느린막걸리 언제먹어",
          "original_form": "느린막걸리 언제먹어",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042850.1.23",
          "form": "떡볶이에소주 쵸ㅣ됴",
          "original_form": "떡볶이에소주 쵸ㅣ됴",
          "speaker_id": "1",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042850.1.24",
          "form": "그러다없어질듯",
          "original_form": "그러다없어질듯",
