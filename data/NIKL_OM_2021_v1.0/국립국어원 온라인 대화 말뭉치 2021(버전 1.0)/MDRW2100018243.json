      "utterance": [
        {
          "id": "MDRW2100018243.1.1",
          "form": "안녕하세요 음악 장르 좋아하시나요",
          "original_form": "안녕하세요 음악 장르 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018243.1.2",
          "form": "네 힙합 좋아해요",
          "original_form": "네 힙합 좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018243.1.3",
          "form": "우와 저도 힙합 좋아하거든요",
          "original_form": "우와 저도 힙합 좋아하거든요",
          "speaker_id": "1",
          "time": "20210827 08:53"
        },
        {
          "id": "MDRW2100018243.1.4",
          "form": "어떤 노래 들으시나요",
          "original_form": "어떤 노래 들으시나요",
          "speaker_id": "1",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018243.1.5",
          "form": "재지팩트 좋아해요",
          "original_form": "재지팩트 좋아해요",
          "speaker_id": "2",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018243.1.6",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018243.1.7",
          "form": "와 재지팩트요",
          "original_form": "와 재지팩트요",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100018243.1.8",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100018243.1.9",
          "form": "저는 요즘 듣는 사람은 dpr live 들어요",
          "original_form": "저는 요즘 듣는 사람은 dpr live 들어요",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100018243.1.10",
          "form": "그렇군요! 넷플릭스도 보시나요",
          "original_form": "그렇군요! 넷플릭스도 보시나요",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100018243.1.11",
          "form": "아 넷플릭스 전에 봤어요. 님은 보시나요?",
          "original_form": "아 넷플릭스 전에 봤어요. 님은 보시나요?",
          "speaker_id": "1",
          "time": "20210828 09:51"
        },
        {
          "id": "MDRW2100018243.1.12",
          "form": "고전 보고 있어요. 고스트 버스터즈나 빅뱅이론 같은거요 ㅋㅋ",
          "original_form": "고전 보고 있어요. 고스트 버스터즈나 빅뱅이론 같은거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100018243.1.13",
          "form": "오 빅뱅이론 예전에 보다가 길어서 끝까지 못봤었는데 재밌더라고요",
          "original_form": "오 빅뱅이론 예전에 보다가 길어서 끝까지 못봤었는데 재밌더라고요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100018243.1.14",
          "form": "저는 기묘한 이야기 시리즈 봤었어요",
          "original_form": "저는 기묘한 이야기 시리즈 봤었어요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100018243.1.15",
          "form": "그것도 인기많죠 ㅋㅋㅋ 지금은 드라마 같은거 보나요??",
          "original_form": "그것도 인기많죠 ㅋㅋㅋ 지금은 드라마 같은거 보나요??",
          "speaker_id": "2",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100018243.1.16",
          "form": "왓챠로 와이우먼킬 보고있어요~",
          "original_form": "왓챠로 와이우먼킬 보고있어요~",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100018243.1.17",
          "form": "왓챠가 더 좋은가요? 넷플보다?",
          "original_form": "왓챠가 더 좋은가요? 넷플보다?",
          "speaker_id": "2",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100018243.1.18",
          "form": "아 왓챠가 우연히 2주 무료로 보게 돼서 사용 중이에요 ㅋㅋㅋ",
