      "utterance": [
        {
          "id": "MDRW2100014118.1.1",
          "form": "ㅋㅋㅋㅋㅋ고구마는 사랑입니다",
          "original_form": "ㅋㅋㅋㅋㅋ고구마는 사랑입니다",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014118.1.2",
          "form": "고구마 미워요",
          "original_form": "고구마 미워요",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014118.1.3",
          "form": "감자도 사랑이거든요? 참나",
          "original_form": "감자도 사랑이거든요? 참나",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014118.1.4",
          "form": "해외 나가보셨어요?",
          "original_form": "해외 나가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014118.1.5",
          "form": "초1때 가봤습니다 약 한달",
          "original_form": "초1때 가봤습니다 약 한달",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014118.1.6",
          "form": "어학연수군요ㅋㅋㅋ",
          "original_form": "어학연수군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014118.1.7",
          "form": "어디로갔어요?",
          "original_form": "어디로갔어요?",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014118.1.8",
          "form": "아뇨 ㅋㅋㅋ가족여행이요",
          "original_form": "아뇨 ㅋㅋㅋ가족여행이요",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014118.1.9",
          "form": "와 한달을 ㄷㄷ",
          "original_form": "와 한달을 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014118.1.10",
          "form": "캐나다 멕시코 엿나",
          "original_form": "캐나다 멕시코 엿나",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014118.1.11",
          "form": "목적지가 과테말라인데 미국비자가 안나서 경유해서갔어요",
          "original_form": "목적지가 과테말라인데 미국비자가 안나서 경유해서갔어요",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014118.1.12",
          "form": "커피보러 갔나요?",
          "original_form": "커피보러 갔나요?",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014118.1.13",
          "form": "과테말라에서 먹었던 사탕이 그렇게맛있었는데",
          "original_form": "과테말라에서 먹었던 사탕이 그렇게맛있었는데",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014118.1.14",
          "form": "아뇨 ㅋㅋㅋㅋ거기 친척이살아서 놀러갈겸 갔어요",
          "original_form": "아뇨 ㅋㅋㅋㅋ거기 친척이살아서 놀러갈겸 갔어요",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014118.1.15",
          "form": "엄마말로는 이민생각하고 가보자고해서 간건데",
          "original_form": "엄마말로는 이민생각하고 가보자고해서 간건데",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014118.1.16",
          "form": "총기소유국가라 무서워서 못살거같다고 ㅋㅋㅋㅋㅋ 그래서 쭉 한국에ㅓ서 삽니다",
          "original_form": "총기소유국가라 무서워서 못살거같다고 ㅋㅋㅋㅋㅋ 그래서 쭉 한국에ㅓ서 삽니다",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014118.1.17",
          "form": "오우 대단하시네요",
          "original_form": "오우 대단하시네요",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.18",
          "form": "이민 생각을",
          "original_form": "이민 생각을",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.19",
          "form": "친척이 과테말라에서 큰사업을해가지고 완전잘됐거든요 ㅎ",
          "original_form": "친척이 과테말라에서 큰사업을해가지고 완전잘됐거든요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.20",
          "form": "커피!?",
          "original_form": "커피!?",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.21",
          "form": "지금도 대한민국 대표시민이더라구여",
          "original_form": "지금도 대한민국 대표시민이더라구여",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.22",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.23",
          "form": "아뇨 옷장사요 ㅋㅋㅋ 지금은 식당도 해요",
          "original_form": "아뇨 옷장사요 ㅋㅋㅋ 지금은 식당도 해요",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014118.1.24",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014118.1.25",
          "form": "식모도있고 운전기사도 있어서 진짜 호화스럽게 놀다왔어요",
          "original_form": "식모도있고 운전기사도 있어서 진짜 호화스럽게 놀다왔어요",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014118.1.26",
          "form": "물가 차이가 참ㅎㅎ 어찌보면 신기해요",
          "original_form": "물가 차이가 참ㅎㅎ 어찌보면 신기해요",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014118.1.27",
          "form": "같은 돈인데 다른 값어치니",
          "original_form": "같은 돈인데 다른 값어치니",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014118.1.28",
          "form": "암튼 거기서 감자나 고구마 드셔보셨어요?",
          "original_form": "암튼 거기서 감자나 고구마 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014118.1.29",
          "form": "이거 물어볼려고 했던건데",
          "original_form": "이거 물어볼려고 했던건데",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.30",
          "form": "아 진짜 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.31",
          "form": "서두가 길었네요^^",
          "original_form": "서두가 길었네요^^",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.32",
          "form": "그냥 직구로 물어보시지",
          "original_form": "그냥 직구로 물어보시지",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.33",
          "form": "그러게요 의도하지 않았는데ㅋㅋㅋ",
          "original_form": "그러게요 의도하지 않았는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.34",
          "form": "거기서는 망고만 오질나게 먹고왔어요",
          "original_form": "거기서는 망고만 오질나게 먹고왔어요",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.35",
          "form": "아보카도랑",
          "original_form": "아보카도랑",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.36",
          "form": "그래서 제가 아보카도를 안먹어요",
          "original_form": "그래서 제가 아보카도를 안먹어요",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014118.1.37",
          "form": "아보카도가 환경 파괴범이던데요",
          "original_form": "아보카도가 환경 파괴범이던데요",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014118.1.38",
          "form": "한 알맹이가 물을 65리터 마신댔나...",
          "original_form": "한 알맹이가 물을 65리터 마신댔나...",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014118.1.39",
          "form": "전 그래서 안 사먹습니다",
          "original_form": "전 그래서 안 사먹습니다",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014118.1.40",
          "form": "맞네 멕시코 아보카도로 돈벌죠",
          "original_form": "맞네 멕시코 아보카도로 돈벌죠",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014118.1.41",
          "form": "네? 와 이거 물돼지였구나",
          "original_form": "네? 와 이거 물돼지였구나",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014118.1.42",
          "form": "네.. 환경 작살내더라구요",
          "original_form": "네.. 환경 작살내더라구요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014118.1.43",
          "form": "아보카도 농장 10년만 지나도 주변 다 가물어요",
          "original_form": "아보카도 농장 10년만 지나도 주변 다 가물어요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014118.1.44",
          "form": "뭐 제가 안 먹는다해도 크게 바뀔건 없겠지만",
          "original_form": "뭐 제가 안 먹는다해도 크게 바뀔건 없겠지만",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014118.1.45",
          "form": "물을엄청먹네요 몰랐다",
          "original_form": "물을엄청먹네요 몰랐다",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014118.1.46",
          "form": "하지만 타코먹으려면 과카몰리가 필수인걸요",
          "original_form": "하지만 타코먹으려면 과카몰리가 필수인걸요",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014118.1.47",
          "form": "아 그 네모네모하게 썰어서 올리는",
          "original_form": "아 그 네모네모하게 썰어서 올리는",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014118.1.48",
          "form": "아뇨 과카몰리는 다진다고해야하나",
          "original_form": "아뇨 과카몰리는 다진다고해야하나",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014118.1.49",
          "form": "짓이긴다고 해야할까요",
          "original_form": "짓이긴다고 해야할까요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014118.1.50",
          "form": "아?! 그 초록색 소스?",
          "original_form": "아?! 그 초록색 소스?",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014118.1.51",
          "form": "으깬다가 맞네요",
          "original_form": "으깬다가 맞네요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014118.1.52",
          "form": "였나 그건가보네요",
          "original_form": "였나 그건가보네요",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014118.1.53",
          "form": "도미노 포장예약해놨는데 타코도 먹고싶네요 환장",
          "original_form": "도미노 포장예약해놨는데 타코도 먹고싶네요 환장",
