      "utterance": [
        {
          "id": "MDRW2100026563.1.1",
          "form": "상춘재",
          "original_form": "상춘재",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026563.1.2",
          "form": "요기 깔끔하고 맛나요",
          "original_form": "요기 깔끔하고 맛나요",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026563.1.3",
          "form": "오 꼬막비빔밥?드신건가요",
          "original_form": "오 꼬막비빔밥?드신건가요",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026563.1.4",
          "form": "고등어구이 먹었던거 같아여",
          "original_form": "고등어구이 먹었던거 같아여",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026563.1.5",
          "form": "돌솥밥 고등어구이요",
          "original_form": "돌솥밥 고등어구이요",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026563.1.6",
          "form": "제주 추천 스팟이 있나요?",
          "original_form": "제주 추천 스팟이 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026563.1.7",
          "form": "아 저 추천 하나 더 해도되요?",
          "original_form": "아 저 추천 하나 더 해도되요?",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026563.1.8",
          "form": "스시애월 추천합니다",
          "original_form": "스시애월 추천합니다",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026563.1.9",
          "form": "완전되죠",
          "original_form": "완전되죠",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026563.1.10",
          "form": "환영환영",
          "original_form": "환영환영",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026563.1.11",
          "form": "감사감사 완젼",
          "original_form": "감사감사 완젼",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026563.1.12",
          "form": "초밥 좋아하시면 가성비 좋아여",
          "original_form": "초밥 좋아하시면 가성비 좋아여",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026563.1.13",
          "form": "전 시흥해녀의집 전복죽 마싯게머금",
          "original_form": "전 시흥해녀의집 전복죽 마싯게머금",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026563.1.14",
          "form": "스시애월 ! 조아요 저장",
          "original_form": "스시애월 ! 조아요 저장",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026563.1.15",
          "form": "오오 전복죽 좋져",
          "original_form": "오오 전복죽 좋져",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026563.1.16",
          "form": "성산쪽이네요",
          "original_form": "성산쪽이네요",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026563.1.17",
          "form": "넹넹 ㅋㅋㅋㅋ 뜨끈하니 아침으로..",
          "original_form": "넹넹 ㅋㅋㅋㅋ 뜨끈하니 아침으로..",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026563.1.18",
          "form": "다 아는데지만 그... 공항근ㅊㅓ",
          "original_form": "다 아는데지만 그... 공항근ㅊㅓ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026563.1.19",
          "form": "우진해장국?",
          "original_form": "우진해장국?",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026563.1.20",
          "form": "도 마싯엇는데 ㅋㅋ",
          "original_form": "도 마싯엇는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026563.1.21",
          "form": "와 거기 웨이팅 미쳤다해서",
          "original_form": "와 거기 웨이팅 미쳤다해서",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026563.1.22",
          "form": "고사리 해장국 맞죠?",
          "original_form": "고사리 해장국 맞죠?",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.23",
          "form": "못먹었어요",
          "original_form": "못먹었어요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.24",
          "form": "마자여!",
          "original_form": "마자여!",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.25",
          "form": "포장해서 먹엇어요",
          "original_form": "포장해서 먹엇어요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.26",
          "form": "맛있군요ㅜ",
          "original_form": "맛있군요ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.27",
          "form": "다들 그렇게 먹더라구요ㅎ",
          "original_form": "다들 그렇게 먹더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.28",
          "form": "포장은 글케 안걸림",
          "original_form": "포장은 글케 안걸림",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.29",
          "form": "저는 공항에서 마음이 샌든가ㅋ",
          "original_form": "저는 공항에서 마음이 샌든가ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026563.1.30",
          "form": "헐 전 그거 실패 ㅠㅠ흐잉",
          "original_form": "헐 전 그거 실패 ㅠㅠ흐잉",
          "speaker_id": "1",
          "time": "20210901 23:09"
        }
      ]
    }
  ]
