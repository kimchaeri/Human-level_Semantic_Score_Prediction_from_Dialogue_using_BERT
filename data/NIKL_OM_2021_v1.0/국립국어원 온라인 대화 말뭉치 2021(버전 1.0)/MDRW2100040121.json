      "utterance": [
        {
          "id": "MDRW2100040121.1.1",
          "form": "안녕하세요~ 가족 분들이랑 같이 사시나요~?",
          "original_form": "안녕하세요~ 가족 분들이랑 같이 사시나요~?",
          "speaker_id": "1",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100040121.1.2",
          "form": "안녕하세오",
          "original_form": "안녕하세오",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100040121.1.3",
          "form": "아니요 자취하고 있습니다!",
          "original_form": "아니요 자취하고 있습니다!",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100040121.1.4",
          "form": "아 독립하셨나요?",
          "original_form": "아 독립하셨나요?",
          "speaker_id": "1",
          "time": "20210904 10:56"
        },
        {
          "id": "MDRW2100040121.1.5",
          "form": "회사가 집에서 멀어서요~",
          "original_form": "회사가 집에서 멀어서요~",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040121.1.6",
          "form": "그래도 주말엔 본가에 갑니다",
          "original_form": "그래도 주말엔 본가에 갑니다",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100040121.1.7",
          "form": "아하~ 그러면 혼자 집에서 가사일 다 하시겠네요",
          "original_form": "아하~ 그러면 혼자 집에서 가사일 다 하시겠네요",
          "speaker_id": "1",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100040121.1.8",
          "form": "네~ 귀찮습니다 ㅠㅠ",
          "original_form": "네~ 귀찮습니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100040121.1.9",
          "form": "독립하셨나요?",
          "original_form": "독립하셨나요?",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100040121.1.10",
          "form": "아니요ㅋㅋㅋ  저는 아직 본가에서.. 캥거루족이에요ㅎㅎ",
          "original_form": "아니요ㅋㅋㅋ  저는 아직 본가에서.. 캥거루족이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 11:50"
        },
        {
          "id": "MDRW2100040121.1.11",
          "form": "부모님이랑 같이 사는게 좋죠!!!",
          "original_form": "부모님이랑 같이 사는게 좋죠!!!",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040121.1.12",
          "form": "그래도 회사는 가까우세요?",
          "original_form": "그래도 회사는 가까우세요?",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100040121.1.13",
          "form": "내 운전에서 다닐 수 있는 거리에요 ㅎㅎ",
          "original_form": "내 운전에서 다닐 수 있는 거리에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040121.1.14",
          "form": "운전해서 !",
          "original_form": "운전해서 !",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040121.1.15",
          "form": "부모님이랑 같이사는게 좋은 것 같아요 ㅠ ㅠ",
          "original_form": "부모님이랑 같이사는게 좋은 것 같아요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040121.1.16",
          "form": "회사가 가까워서 다행이네요",
          "original_form": "회사가 가까워서 다행이네요",
          "speaker_id": "2",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040121.1.17",
          "form": "그러니까요ㅠ 그런데 저는 살면서 한번도 부모님이랑 떨어져 본 적이 없아서 자취 로망도 약간 있어요 ㅎㅎㅎ",
          "original_form": "그러니까요ㅠ 그런데 저는 살면서 한번도 부모님이랑 떨어져 본 적이 없아서 자취 로망도 약간 있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100040121.1.18",
