      "utterance": [
        {
          "id": "MDRW2100005149.1.1",
          "form": "우와 진짜 재밌겠네요",
          "original_form": "우와 진짜 재밌겠네요",
          "speaker_id": "2",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005149.1.2",
          "form": "여름 느낌 너무 좋아요",
          "original_form": "여름 느낌 너무 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005149.1.3",
          "form": "한번 빌려봐야겠어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "한번 빌려봐야겠어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005149.1.4",
          "form": "그리고 뭔가 으으음?! 뭐지?",
          "original_form": "그리고 뭔가 으으음?! 뭐지?",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005149.1.5",
          "form": "하는 구간들이있는데 그마저도 재밌더라고요",
          "original_form": "하는 구간들이있는데 그마저도 재밌더라고요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005149.1.6",
          "form": "혹시 도라에몽 좋아하시나요?",
          "original_form": "혹시 도라에몽 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005149.1.7",
          "form": "오오 신기하네요!",
          "original_form": "오오 신기하네요!",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005149.1.8",
          "form": "도라에몽 너무귀엽죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "도라에몽 너무귀엽죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005149.1.9",
          "form": "전 좋아해요!! name1님도...?",
          "original_form": "전 좋아해요!! &name1&님도...?",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005149.1.10",
          "form": "저도 좋아해요 ㅋㅋ",
          "original_form": "저도 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005149.1.11",
          "form": "고등학생때는 점심시간마다 봤었어요",
          "original_form": "고등학생때는 점심시간마다 봤었어요",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005149.1.12",
          "form": "얼음고래에서 도라에몽 얘가 정말 많이나와서",
          "original_form": "얼음고래에서 도라에몽 얘가 정말 많이나와서",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005149.1.13",
          "form": "볼 때 재밌으실것같네요",
          "original_form": "볼 때 재밌으실것같네요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005149.1.14",
          "form": "와 도라에몽얘기가 나와요?? 진짜 재미있겠어요!!",
          "original_form": "와 도라에몽얘기가 나와요?? 진짜 재미있겠어요!!",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005149.1.15",
          "form": "신기신기",
          "original_form": "신기신기",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005149.1.16",
          "form": "주인공이 도라에몽을 참 좋아하거든요",
          "original_form": "주인공이 도라에몽을 참 좋아하거든요",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005149.1.17",
          "form": "처음 들어보는 도라에몽 도구들도 나오고",
          "original_form": "처음 들어보는 도라에몽 도구들도 나오고",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005149.1.18",
          "form": "흥미로웠어요!!",
          "original_form": "흥미로웠어요!!",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005149.1.19",
          "form": "오오 주인공이 덕후라니",
          "original_form": "오오 주인공이 덕후라니",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005149.1.20",
          "form": "저도 웬만한 도구는 아는데 그거 찾아보는 재미도 잇겠어요!!!",
          "original_form": "저도 웬만한 도구는 아는데 그거 찾아보는 재미도 잇겠어요!!!",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005149.1.21",
          "form": "설명도 다 해줘서",
          "original_form": "설명도 다 해줘서",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005149.1.22",
          "form": "더 좋았어요 ㅎㅎ",
          "original_form": "더 좋았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005149.1.23",
          "form": "최근 인기많은 책들은",
          "original_form": "최근 인기많은 책들은",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005149.1.24",
          "form": "읽어보셨나요?",
          "original_form": "읽어보셨나요?",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005149.1.25",
          "form": "진짜좋네요 ㅋㅋㅋㅋ 요즘은 서점을 잘 못가가지구ㅠㅠ",
          "original_form": "진짜좋네요 ㅋㅋㅋㅋ 요즘은 서점을 잘 못가가지구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005149.1.26",
          "form": "죽고싶지만 떡볶이는 먹고싶어 책도 너무 궁금해요",
          "original_form": "죽고싶지만 떡볶이는 먹고싶어 책도 너무 궁금해요",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005149.1.27",
          "form": "아 그 책 저도 들어봤어요",
          "original_form": "아 그 책 저도 들어봤어요",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005149.1.28",
          "form": "딱 현댜인들이 공감할만한 제목이죠ㅎㅎ",
          "original_form": "딱 현댜인들이 공감할만한 제목이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005149.1.29",
          "form": "맞아요 ㅋㅋㅋㅋ 또 제가 덕볶이를 사랑해가지구 ㅎㅎㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 또 제가 덕볶이를 사랑해가지구 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005149.1.30",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100005149.1.31",
          "form": "그쵸 떡볶이 너무 맛있죠",
          "original_form": "그쵸 떡볶이 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210822 00:01"
        }
      ]
