      "utterance": [
        {
          "id": "MDRW2100010483.1.1",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010483.1.2",
          "form": "할인은 정말 못참죠",
          "original_form": "할인은 정말 못참죠",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010483.1.3",
          "form": "sk가족결합 갱신하면서 상품권 9만원받았어요",
          "original_form": "sk가족결합 갱신하면서 상품권 9만원받았어요",
          "speaker_id": "1",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010483.1.4",
          "form": "엥 왠일",
          "original_form": "엥 왠일",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010483.1.5",
          "form": "전 뭔가요 ㅜㅜ고작 구글 6만원요 쓸때도 없어요",
          "original_form": "전 뭔가요 ㅜㅜ고작 구글 6만원요 쓸때도 없어요",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010483.1.6",
          "form": "장기이용거래혜택 이라서 그런가봐요",
          "original_form": "장기이용거래혜택 이라서 그런가봐요",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010483.1.7",
          "form": "웃긴게",
          "original_form": "웃긴게",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010483.1.8",
          "form": "네 좋은 제도 네요 ㅎㅎ",
          "original_form": "네 좋은 제도 네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010483.1.9",
          "form": "작년엔 무슨 6만원이상은 안된다더니 ㅋㅋ",
          "original_form": "작년엔 무슨 6만원이상은 안된다더니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010483.1.10",
          "form": "치 맨날 바껴요",
          "original_form": "치 맨날 바껴요",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.11",
          "form": "안주려고",
          "original_form": "안주려고",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.12",
          "form": "말이 코에걸면 코걸이 귀에걸면 귀걸이",
          "original_form": "말이 코에걸면 코걸이 귀에걸면 귀걸이",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.13",
          "form": "올해는 운이 좋네요 정보력도 좋으시고",
          "original_form": "올해는 운이 좋네요 정보력도 좋으시고",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.14",
          "form": "맞아요 할인에도 함정이 너무 많아요",
          "original_form": "맞아요 할인에도 함정이 너무 많아요",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.15",
          "form": "막상 사고 나면 뭐 빼먹어 안된다하고",
          "original_form": "막상 사고 나면 뭐 빼먹어 안된다하고",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.16",
          "form": "몇만 이상 사야하고",
          "original_form": "몇만 이상 사야하고",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010483.1.17",
          "form": "전 오늘 티몬 버거킹 할인 떳실해",
          "original_form": "전 오늘 티몬 버거킹 할인 떳실해",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010483.1.18",
          "form": "길래",
          "original_form": "길래",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010483.1.19",
          "form": "3200원에 불고기 버거 샀는데",
          "original_form": "3200원에 불고기 버거 샀는데",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010483.1.20",
          "form": "오 뭐 할인하던가요",
          "original_form": "오 뭐 할인하던가요",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010483.1.21",
          "form": "싼건지 모르겠어요",
          "original_form": "싼건지 모르겠어요",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010483.1.22",
          "form": "40프로래요",
          "original_form": "40프로래요",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010483.1.23",
          "form": "전 50프로해야 싼거같더라고요",
          "original_form": "전 50프로해야 싼거같더라고요",
          "speaker_id": "1",
          "time": "20210823 18:50"
        },
        {
          "id": "MDRW2100010483.1.24",
          "form": "아 49프로네요",
          "original_form": "아 49프로네요",
          "speaker_id": "2",
          "time": "20210823 18:50"
        },
        {
          "id": "MDRW2100010483.1.25",
          "form": "6300원이래요 원래",
          "original_form": "6300원이래요 원래",
          "speaker_id": "2",
          "time": "20210823 18:50"
        },
        {
          "id": "MDRW2100010483.1.26",
          "form": "그런가 싶기도하고 혹해서 샀어요",
          "original_form": "그런가 싶기도하고 혹해서 샀어요",
          "speaker_id": "2",
          "time": "20210823 18:50"
        },
        {
          "id": "MDRW2100010483.1.27",
          "form": "앱에 킹치킨버거 세트도 싸네요",
          "original_form": "앱에 킹치킨버거 세트도 싸네요",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010483.1.28",
          "form": "아전 가족결합하니 요금이 8만원 싸졌어요",
          "original_form": "아전 가족결합하니 요금이 8만원 싸졌어요",
          "speaker_id": "2",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010483.1.29",
          "form": "여태 비싸게 내고 있었다니 충격이였어요",
          "original_form": "여태 비싸게 내고 있었다니 충격이였어요",
          "speaker_id": "2",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010483.1.30",
          "form": "전 10년간 자동갱신해서",
          "original_form": "전 10년간 자동갱신해서",
          "speaker_id": "1",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100010483.1.31",
          "form": "가족결합을",
          "original_form": "가족결합을",
          "speaker_id": "1",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100010483.1.32",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210823 19:13"
        }
      ]
    }
