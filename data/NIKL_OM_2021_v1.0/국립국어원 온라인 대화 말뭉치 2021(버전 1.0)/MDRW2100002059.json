      "utterance": [
        {
          "id": "MDRW2100002059.1.1",
          "form": "이제 완전 가을이지 ㅎㅎ",
          "original_form": "이제 완전 가을이지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100002059.1.2",
          "form": "여름에 매미들는 제대로 우는거 듣지도 못했는데",
          "original_form": "여름에 매미들는 제대로 우는거 듣지도 못했는데",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100002059.1.3",
          "form": "그러게.. 시간이 참 빠르네",
          "original_form": "그러게.. 시간이 참 빠르네",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100002059.1.4",
          "form": "이렇게 시원한 날이 며칠이나 갈까",
          "original_form": "이렇게 시원한 날이 며칠이나 갈까",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002059.1.5",
          "form": "한두달은 가겠지",
          "original_form": "한두달은 가겠지",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002059.1.6",
          "form": "산책할 때 땀도 안나고 너무 좋아",
          "original_form": "산책할 때 땀도 안나고 너무 좋아",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002059.1.7",
          "form": "여름에 더워서 축 쳐져 있더니 ㅎㅎ 이제 우리 색시 기운 좀 날라나",
          "original_form": "여름에 더워서 축 쳐져 있더니 ㅎㅎ 이제 우리 색시 기운 좀 날라나",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100002059.1.8",
          "form": "ㅎㅎ 날씨만으로 기운이 좀 나는 것 같네",
          "original_form": "ㅎㅎ 날씨만으로 기운이 좀 나는 것 같네",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002059.1.9",
          "form": "일단은 뽀송하고 상쾌하니",
          "original_form": "일단은 뽀송하고 상쾌하니",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002059.1.10",
          "form": "빨래도 엄청 잘 마르지",
          "original_form": "빨래도 엄청 잘 마르지",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002059.1.11",
          "form": "응 건조해서 그런지 아주 뽀송뽀송하게 말라",
          "original_form": "응 건조해서 그런지 아주 뽀송뽀송하게 말라",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002059.1.12",
          "form": "이불 좀 널어놔야겠네 ㅋㅋ",
          "original_form": "이불 좀 널어놔야겠네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002059.1.13",
          "form": "좋지 ㅎㅎ 물먹는 하마 없어도 넘나 상쾌한 것",
          "original_form": "좋지 ㅎㅎ 물먹는 하마 없어도 넘나 상쾌한 것",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002059.1.14",
          "form": "이러다 비오면 확 추워질텐데",
          "original_form": "이러다 비오면 확 추워질텐데",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002059.1.15",
          "form": "다음주 비소식 있던데.. 제발 이대로만",
          "original_form": "다음주 비소식 있던데.. 제발 이대로만",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002059.1.16",
          "form": "비오고 나면 10도대로 내려갈듯",
          "original_form": "비오고 나면 10도대로 내려갈듯",
