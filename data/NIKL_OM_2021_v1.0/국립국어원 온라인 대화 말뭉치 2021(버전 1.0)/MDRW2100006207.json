      "utterance": [
        {
          "id": "MDRW2100006207.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006207.1.2",
          "form": "네  저는 일년에 한번은 꼭 가요",
          "original_form": "네  저는 일년에 한번은 꼭 가요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006207.1.3",
          "form": "어디 가셨나요",
          "original_form": "어디 가셨나요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006207.1.4",
          "form": "코로나 터지기직전에 스페인포르투갈 다녀왔어요",
          "original_form": "코로나 터지기직전에 스페인포르투갈 다녀왔어요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006207.1.5",
          "form": "가보신적 있으세요?",
          "original_form": "가보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006207.1.6",
          "form": "아니요ㅠㅠ",
          "original_form": "아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006207.1.7",
          "form": "아 역사를 좀공부하고 가면 더 즐길수 있을거 같더라구요",
          "original_form": "아 역사를 좀공부하고 가면 더 즐길수 있을거 같더라구요",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006207.1.8",
          "form": "아 가고싶네요",
          "original_form": "아 가고싶네요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006207.1.9",
          "form": "네 혹시 미술 좋아하세요?",
          "original_form": "네 혹시 미술 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006207.1.10",
          "form": "아니요....ㅎㅎ",
          "original_form": "아니요....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006207.1.11",
          "form": "아 저도 미술에 관심 없었는데 워낙 미술관이 많다보니",
          "original_form": "아 저도 미술에 관심 없었는데 워낙 미술관이 많다보니",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006207.1.12",
          "form": "저절로 관심이 생기더라구요",
          "original_form": "저절로 관심이 생기더라구요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006207.1.13",
          "form": "마카오가 비슷한 느낌이지 않을까요?",
          "original_form": "마카오가 비슷한 느낌이지 않을까요?",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006207.1.14",
          "form": "네 비슷하긴한데 짝퉁느낌은 좀있어요",
          "original_form": "네 비슷하긴한데 짝퉁느낌은 좀있어요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006207.1.15",
          "form": "ㅎㅎ저는 마카오만 가봐서",
          "original_form": "ㅎㅎ저는 마카오만 가봐서",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006207.1.16",
          "form": "마카오 저는 오래전에갔는데",
          "original_form": "마카오 저는 오래전에갔는데",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006207.1.17",
          "form": "베네시안?거기 좋았던거같아요",
          "original_form": "베네시안?거기 좋았던거같아요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006207.1.18",
          "form": "지금은 더 좋은곳이 많다면서요?",
          "original_form": "지금은 더 좋은곳이 많다면서요?",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006207.1.19",
          "form": "기억이 잘 ㅋㅋ 근데 되게 도시가 이뻤어요",
          "original_form": "기억이 잘 ㅋㅋ 근데 되게 도시가 이뻤어요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006207.1.20",
          "form": "그랜드레스보아?",
          "original_form": "그랜드레스보아?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006207.1.21",
          "form": "여기가 건물이 왕관같이 이뻤던듯",
          "original_form": "여기가 건물이 왕관같이 이뻤던듯",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006207.1.22",
          "form": "아 거긴첨들어요 뭔가궁금하네요",
          "original_form": "아 거긴첨들어요 뭔가궁금하네요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        }
