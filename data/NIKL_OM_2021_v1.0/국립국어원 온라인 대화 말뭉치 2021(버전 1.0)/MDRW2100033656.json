      "utterance": [
        {
          "id": "MDRW2100033656.1.1",
          "form": "떡볶이 좋아하세요????",
          "original_form": "떡볶이 좋아하세요????",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100033656.1.2",
          "form": "저는 별로 안좋아해요ㅠ",
          "original_form": "저는 별로 안좋아해요ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100033656.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100033656.1.4",
          "form": "헉 네",
          "original_form": "헉 네",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100033656.1.5",
          "form": "왜 안좋아하세요?",
          "original_form": "왜 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100033656.1.6",
          "form": "음 저 너무 한식인간인지 식사같지않아서 안좋아해요",
          "original_form": "음 저 너무 한식인간인지 식사같지않아서 안좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100033656.1.7",
          "form": "간식으로 컵볶이정도는 가끔 먹고싶을때도 있는데 식사로두고 먹으면 부담스러워요",
          "original_form": "간식으로 컵볶이정도는 가끔 먹고싶을때도 있는데 식사로두고 먹으면 부담스러워요",
          "speaker_id": "2",
          "time": "20210824 21:05"
        },
        {
          "id": "MDRW2100033656.1.8",
          "form": "아하 컵떡볶이 맛있겠네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 컵떡볶이 맛있겠네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100033656.1.9",
          "form": "한식 뭐 좋아하세요",
          "original_form": "한식 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100033656.1.10",
          "form": "찌개류도 좋아하구요 밥에 반찬이나 젓갈류도 좋아해요",
          "original_form": "찌개류도 좋아하구요 밥에 반찬이나 젓갈류도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100033656.1.11",
          "form": "찌개 뭐 제일 좋아하세요?,",
          "original_form": "찌개 뭐 제일 좋아하세요?,",
          "speaker_id": "1",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100033656.1.12",
          "form": "저는 된장찌개요",
          "original_form": "저는 된장찌개요",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033656.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033656.1.14",
          "form": "저두요 청국장도",
          "original_form": "저두요 청국장도",
          "speaker_id": "1",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033656.1.15",
          "form": "좋아해용",
          "original_form": "좋아해용",
          "speaker_id": "1",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033656.1.16",
          "form": "요즘엔 재첩국이 그렇게 먹고싶어요",
          "original_form": "요즘엔 재첩국이 그렇게 먹고싶어요",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100033656.1.17",
          "form": "국하고 먹는 밥  왜이렇게 좋은지...",
          "original_form": "국하고 먹는 밥  왜이렇게 좋은지...",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100033656.1.18",
          "form": "ㅋㅋ ㅋㅋㅋ한식이 든든하긴하죠",
          "original_form": "ㅋㅋ ㅋㅋㅋ한식이 든든하긴하죠",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100033656.1.19",
          "form": "국밥이 이분야에선 최고예요",
          "original_form": "국밥이 이분야에선 최고예요",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100033656.1.20",
          "form": "그것은 좀 인정이요 ㅎ",
          "original_form": "그것은 좀 인정이요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 21:16"
