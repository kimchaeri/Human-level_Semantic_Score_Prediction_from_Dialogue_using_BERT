      "utterance": [
        {
          "id": "MDRW2100043696.1.1",
          "form": "너무 슬퍼서ㅜㅜ많이 울었어요ㅜ",
          "original_form": "너무 슬퍼서ㅜㅜ많이 울었어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043696.1.2",
          "form": "이준기랑 아이유땜에요ㅜ",
          "original_form": "이준기랑 아이유땜에요ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043696.1.3",
          "form": "네 둘이 잘어울렷어여",
          "original_form": "네 둘이 잘어울렷어여",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043696.1.4",
          "form": "강하늘도 나오고 출연진이 화려했어요%%",
          "original_form": "강하늘도 나오고 출연진이 화려했어요%%",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043696.1.5",
          "form": "아 황자님들이 많앗죠 기억나네요ㅋㅋ",
          "original_form": "아 황자님들이 많앗죠 기억나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043696.1.6",
          "form": "네 십몇황자까지있었던~",
          "original_form": "네 십몇황자까지있었던~",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043696.1.7",
          "form": "라인업이 대단했죠",
          "original_form": "라인업이 대단했죠",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043696.1.8",
          "form": "맞아요 그래서 더 재미잇엇어여",
          "original_form": "맞아요 그래서 더 재미잇엇어여",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043696.1.9",
          "form": "보는 재미가 있었죠",
          "original_form": "보는 재미가 있었죠",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043696.1.10",
          "form": "상견니 보셨어요?",
          "original_form": "상견니 보셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043696.1.11",
          "form": "그건 안봣어요 재미잇나요",
          "original_form": "그건 안봣어요 재미잇나요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043696.1.12",
          "form": "네 너무 재밌어요~대만영화예요",
          "original_form": "네 너무 재밌어요~대만영화예요",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043696.1.13",
          "form": "타임슬립...",
          "original_form": "타임슬립...",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043696.1.14",
          "form": "꼭 추천드립니다~",
          "original_form": "꼭 추천드립니다~",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043696.1.15",
          "form": "찾아봐야겟네요",
          "original_form": "찾아봐야겟네요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043696.1.16",
          "form": "네 저는 남주가 너무 제 스탈이라서요~ㅎㅎ",
          "original_form": "네 저는 남주가 너무 제 스탈이라서요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043696.1.17",
          "form": "남주가 멋잇어야 보는 맛이 생기져ㅋㅋ",
          "original_form": "남주가 멋잇어야 보는 맛이 생기져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043696.1.18",
          "form": "네 너무 매력적이예요..여주는 외모가 조금 아쉽긴해요ㅜ",
          "original_form": "네 너무 매력적이예요..여주는 외모가 조금 아쉽긴해요ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043696.1.19",
          "form": "그래서 초반에 집중이 좀 힘들어요ㅜ",
          "original_form": "그래서 초반에 집중이 좀 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:39"
        }
      ]
