      "utterance": [
        {
          "id": "MDRW2100001462.1.1",
          "form": "안녕하세요. name1이라고합니다.",
          "original_form": "안녕하세요. &name1&이라고합니다.",
          "speaker_id": "1",
          "time": "20211014 17:49"
        },
        {
          "id": "MDRW2100001462.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211014 17:52"
        },
        {
          "id": "MDRW2100001462.1.3",
          "form": "곧 저녁시간이 다가오는데",
          "original_form": "곧 저녁시간이 다가오는데",
          "speaker_id": "2",
          "time": "20211014 17:52"
        },
        {
          "id": "MDRW2100001462.1.4",
          "form": "저녁 메뉴 정하셨나요?!",
          "original_form": "저녁 메뉴 정하셨나요?!",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.5",
          "form": "아 저는 지금 부모님 모시고 강릉 놀러와서",
          "original_form": "아 저는 지금 부모님 모시고 강릉 놀러와서",
          "speaker_id": "1",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.6",
          "form": "방금 꼬막비빔밥 먹고 나왔습니다",
          "original_form": "방금 꼬막비빔밥 먹고 나왔습니다",
          "speaker_id": "1",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.7",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.8",
          "form": "강릉이라니..",
          "original_form": "강릉이라니..",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.9",
          "form": "너무 부럽네요 ㅠㅠ",
          "original_form": "너무 부럽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.10",
          "form": "꼬막비빔밥 혹시..",
          "original_form": "꼬막비빔밥 혹시..",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.11",
          "form": "엄지네에서 드셨나요?!",
          "original_form": "엄지네에서 드셨나요?!",
          "speaker_id": "2",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.12",
          "form": "오 맞아요..!",
          "original_form": "오 맞아요..!",
          "speaker_id": "1",
          "time": "20211014 17:53"
        },
        {
          "id": "MDRW2100001462.1.13",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "1",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.14",
          "form": "오 역시..!",
          "original_form": "오 역시..!",
          "speaker_id": "2",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.15",
          "form": "꼬막비빔밥은 엄지네죠...",
          "original_form": "꼬막비빔밥은 엄지네죠...",
          "speaker_id": "2",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.16",
          "form": "조금 오지랖이긴한데",
          "original_form": "조금 오지랖이긴한데",
          "speaker_id": "2",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.17",
          "form": "혹시.. 여고시절떡볶이 드셨나요?",
          "original_form": "혹시.. 여고시절떡볶이 드셨나요?",
          "speaker_id": "2",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.18",
          "form": "제 인생 떡볶인데.. 그거 먹으러 강릉가거든요 ㅠ",
          "original_form": "제 인생 떡볶인데.. 그거 먹으러 강릉가거든요 ㅠ",
          "speaker_id": "2",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.19",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20211014 17:54"
        },
        {
          "id": "MDRW2100001462.1.20",
          "form": "거기는 못갔어요 ㅠㅠ",
          "original_form": "거기는 못갔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001462.1.21",
          "form": "다음에 기회되면 들를게요",
          "original_form": "다음에 기회되면 들를게요",
          "speaker_id": "1",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001462.1.22",
          "form": "아아 ㅠㅠ 네네 꼭 드셔보세요",
          "original_form": "아아 ㅠㅠ 네네 꼭 드셔보세요",
          "speaker_id": "2",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001462.1.23",
          "form": "뭔가 엄청난 중독성이 있는 맛이에요...",
          "original_form": "뭔가 엄청난 중독성이 있는 맛이에요...",
          "speaker_id": "2",
          "time": "20211014 17:55"
        },
        {
          "id": "MDRW2100001462.1.24",
          "form": "조만간 떡볶이 먹으러 한번 더 와야겠네요",
          "original_form": "조만간 떡볶이 먹으러 한번 더 와야겠네요",
          "speaker_id": "1",
          "time": "20211014 17:56"
        },
        {
          "id": "MDRW2100001462.1.25",
          "form": "name2님은 저녁 메뉴 정하셨나요",
          "original_form": "&name2&님은 저녁 메뉴 정하셨나요",
          "speaker_id": "1",
          "time": "20211014 17:56"
        },
        {
          "id": "MDRW2100001462.1.26",
          "form": "네네 강추에요!",
          "original_form": "네네 강추에요!",
          "speaker_id": "2",
          "time": "20211014 17:57"
        },
        {
          "id": "MDRW2100001462.1.27",
          "form": "저는 서브웨이에서 샌드위치 사먹으려구요!",
          "original_form": "저는 서브웨이에서 샌드위치 사먹으려구요!",
          "speaker_id": "2",
          "time": "20211014 17:57"
        },
        {
          "id": "MDRW2100001462.1.28",
          "form": "아 서브웨이 맛있죠",
          "original_form": "아 서브웨이 맛있죠",
          "speaker_id": "1",
          "time": "20211014 17:58"
        },
        {
          "id": "MDRW2100001462.1.29",
          "form": "무슨 맛 제일 좋아하세요",
          "original_form": "무슨 맛 제일 좋아하세요",
          "speaker_id": "1",
          "time": "20211014 17:59"
        },
        {
          "id": "MDRW2100001462.1.30",
          "form": "저는 에그마요만 먹는데",
          "original_form": "저는 에그마요만 먹는데",
          "speaker_id": "1",
          "time": "20211014 17:59"
        },
        {
          "id": "MDRW2100001462.1.31",
          "form": "에그마요라니.. 배우신 분이네요",
          "original_form": "에그마요라니.. 배우신 분이네요",
          "speaker_id": "2",
          "time": "20211014 18:00"
        },
        {
          "id": "MDRW2100001462.1.32",
          "form": "저는 비엠티에 에그마요 추가를 좋아해요 ㅎㅎ\r\n",
          "original_form": "저는 비엠티에 에그마요 추가를 좋아해요 ㅎㅎ\r\n",
