      "utterance": [
        {
          "id": "MDRW2100001873.1.1",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001873.1.2",
          "form": "게임 즐겨하진 않아요ㅜ",
          "original_form": "게임 즐겨하진 않아요ㅜ",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001873.1.3",
          "form": "게임 좋아하세요?ㅎ",
          "original_form": "게임 좋아하세요?ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001873.1.4",
          "form": "저도요~ 근데 시간많을땐 단순한거 한번씩 해요",
          "original_form": "저도요~ 근데 시간많을땐 단순한거 한번씩 해요",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001873.1.5",
          "form": "저도 단순한건 좋아해요ㅎ",
          "original_form": "저도 단순한건 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001873.1.6",
          "form": "사천성 이런것들ㅋㅋ그리고3개 한줄하면 없어지고",
          "original_form": "사천성 이런것들ㅋㅋ그리고3개 한줄하면 없어지고",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001873.1.7",
          "form": "사천성 오랜만이네요ㅎㅎ",
          "original_form": "사천성 오랜만이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001873.1.8",
          "form": "ㅋㅋ너무 오래된 게임 인가요?ㅋ",
          "original_form": "ㅋㅋ너무 오래된 게임 인가요?ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001873.1.9",
          "form": "고전게임이 더 재밌어요ㅎ",
          "original_form": "고전게임이 더 재밌어요ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100001873.1.10",
          "form": "맞죠? 저도요 시간깨기 게임",
          "original_form": "맞죠? 저도요 시간깨기 게임",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001873.1.11",
          "form": "그런게 시간도 잘가고 잠시 쉬면서 즐기기엔 딱이죠ㅎ",
          "original_form": "그런게 시간도 잘가고 잠시 쉬면서 즐기기엔 딱이죠ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001873.1.12",
          "form": "오늘도 애 낮잠 시간에 한판 해봐야 겠네요ㅋㅋ",
          "original_form": "오늘도 애 낮잠 시간에 한판 해봐야 겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100001873.1.13",
          "form": "애기 잘때는 무조건 쉬어야해요ㅎ",
          "original_form": "애기 잘때는 무조건 쉬어야해요ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100001873.1.14",
          "form": "그건그렇죠ㅎㅎ 매번 시간뽀개기 하다가 후회ㅡㅜㅠ",
          "original_form": "그건그렇죠ㅎㅎ 매번 시간뽀개기 하다가 후회ㅡㅜㅠ",
          "speaker_id": "1",
