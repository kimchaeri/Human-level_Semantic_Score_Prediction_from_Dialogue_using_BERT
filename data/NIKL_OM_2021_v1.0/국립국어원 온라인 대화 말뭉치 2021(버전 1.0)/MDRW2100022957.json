      "utterance": [
        {
          "id": "MDRW2100022957.1.1",
          "form": "오 요즘 날씨 넘 꿉꿉해",
          "original_form": "오 요즘 날씨 넘 꿉꿉해",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022957.1.2",
          "form": "그래도 미세먼지 없어서 좋지않아?",
          "original_form": "그래도 미세먼지 없어서 좋지않아?",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022957.1.3",
          "form": "ㅇㅇ그건그래",
          "original_form": "ㅇㅇ그건그래",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022957.1.4",
          "form": "그리고 좀 시원해지기도 했어",
          "original_form": "그리고 좀 시원해지기도 했어",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022957.1.5",
          "form": "입추 지나고 귀신같이 시원해졌잖아 ㅋㅋㅋ 신기해",
          "original_form": "입추 지나고 귀신같이 시원해졌잖아 ㅋㅋㅋ 신기해",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022957.1.6",
          "form": "그래서 반바지를 입을지 긴 건ㆍ 입을지 고민중이야",
          "original_form": "그래서 반바지를 입을지 긴 건ㆍ 입을지 고민중이야",
          "speaker_id": "1",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022957.1.7",
          "form": "난 바지는 긴거 티는 반팔",
          "original_form": "난 바지는 긴거 티는 반팔",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022957.1.8",
          "form": "추우면 겉옷챙기고..근데 아직 추울 정돈 아니더라",
          "original_form": "추우면 겉옷챙기고..근데 아직 추울 정돈 아니더라",
          "speaker_id": "2",
          "time": "20210829 19:36"
        },
        {
          "id": "MDRW2100022957.1.9",
          "form": "다음주도 계속 비오려나?",
          "original_form": "다음주도 계속 비오려나?",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022957.1.10",
          "form": "비온다고는 하는데 기상청을 믿을수있어야지",
          "original_form": "비온다고는 하는데 기상청을 믿을수있어야지",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022957.1.11",
          "form": "방금 봤는데 화수목 비표시야 ㅋㅋ 바뀔수도 있지만",
          "original_form": "방금 봤는데 화수목 비표시야 ㅋㅋ 바뀔수도 있지만",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022957.1.12",
          "form": "변덕이 넘 심해 요즘 ㅠ 엄청 안맞는 느낌",
          "original_form": "변덕이 넘 심해 요즘 ㅠ 엄청 안맞는 느낌",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022957.1.13",
          "form": "맞아. 국지성 호우까지 예측이 안되니까 계속 우산들구다닌당",
          "original_form": "맞아. 국지성 호우까지 예측이 안되니까 계속 우산들구다닌당",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022957.1.14",
          "form": "우산 들고 다니는 것도 짐이고 신발젖는 것도 싫고...",
          "original_form": "우산 들고 다니는 것도 짐이고 신발젖는 것도 싫고...",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022957.1.15",
          "form": "ㅠ가을장마 시렁",
          "original_form": "ㅠ가을장마 시렁",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022957.1.16",
          "form": "맞아... 그냥 날이 갑자기 추워져서 비도 안왔으면  좋겠어",
          "original_form": "맞아... 그냥 날이 갑자기 추워져서 비도 안왔으면  좋겠어",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022957.1.17",
          "form": "오 그럼 눈이올텐데",
          "original_form": "오 그럼 눈이올텐데",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022957.1.18",
          "form": "가을 건조하다고 누가그랬냐~~~",
          "original_form": "가을 건조하다고 누가그랬냐~~~",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
