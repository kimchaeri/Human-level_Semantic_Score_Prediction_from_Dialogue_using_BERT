      "utterance": [
        {
          "id": "MDRW2100034511.1.1",
          "form": "알바 어떤거해보셨어요?",
          "original_form": "알바 어떤거해보셨어요?",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034511.1.2",
          "form": "저 영화관 알바 오래했어요!",
          "original_form": "저 영화관 알바 오래했어요!",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034511.1.3",
          "form": "어떤 알바 해보셨어요?",
          "original_form": "어떤 알바 해보셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034511.1.4",
          "form": "저는 주유알바요ㅎㅎ",
          "original_form": "저는 주유알바요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100034511.1.5",
          "form": "요즘에도 하시나요??",
          "original_form": "요즘에도 하시나요??",
          "speaker_id": "1",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034511.1.6",
          "form": "야외라서 방역소독 안해도 될 것 같아요!",
          "original_form": "야외라서 방역소독 안해도 될 것 같아요!",
          "speaker_id": "1",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034511.1.7",
          "form": "요즘은 쉬어요ㅎㅎ 요즘은 어떤거하세요?",
          "original_form": "요즘은 쉬어요ㅎㅎ 요즘은 어떤거하세요?",
          "speaker_id": "2",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100034511.1.8",
          "form": "저는 그냥 회사다녀요 ㅎㅎ",
          "original_form": "저는 그냥 회사다녀요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100034511.1.9",
          "form": "회사업무 많으신가요?",
          "original_form": "회사업무 많으신가요?",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100034511.1.10",
          "form": "요즘은 재택을 많이해서 좀 괜찮아졌어요!",
          "original_form": "요즘은 재택을 많이해서 좀 괜찮아졌어요!",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100034511.1.11",
          "form": "재택해도 업무가 돌아가는거보면 대면했을때도 불필요한건 없애는게 좋을거같아요",
          "original_form": "재택해도 업무가 돌아가는거보면 대면했을때도 불필요한건 없애는게 좋을거같아요",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034511.1.12",
          "form": "맞아요 한번씩 돌아가면서 당직 서고하니까 크게 문제 될거 없고 좋아요!",
          "original_form": "맞아요 한번씩 돌아가면서 당직 서고하니까 크게 문제 될거 없고 좋아요!",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034511.1.13",
          "form": "재택문화가 널리널리 정착됐으면 좋겠어요 ㅎ",
          "original_form": "재택문화가 널리널리 정착됐으면 좋겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034511.1.14",
          "form": "맞아요! 코로나 진짜 싫지만 회식 사라진건 너무좋ㅇㄱㆍ요",
          "original_form": "맞아요! 코로나 진짜 싫지만 회식 사라진건 너무좋ㅇㄱㆍ요",
          "speaker_id": "2",
          "time": "20210830 11:02"
        },
        {
          "id": "MDRW2100034511.1.15",
          "form": "단계 내려가는 건 좋은 일인데 회식도 살아날까봐 불안하기도 해요 ㅋㅋㅋㅋ",
          "original_form": "단계 내려가는 건 좋은 일인데 회식도 살아날까봐 불안하기도 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:05"
        }
      ]
