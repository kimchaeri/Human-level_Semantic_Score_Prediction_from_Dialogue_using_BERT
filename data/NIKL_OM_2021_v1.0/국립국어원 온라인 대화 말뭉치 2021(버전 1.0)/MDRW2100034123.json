      "utterance": [
        {
          "id": "MDRW2100034123.1.1",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100034123.1.2",
          "form": "운동 좋아하진않지만 하려고 노력은해욬ㅋ",
          "original_form": "운동 좋아하진않지만 하려고 노력은해욬ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034123.1.3",
          "form": "저두ㅋㅋㅋ 좀 인간답게 살려면 해야겠더라구요ㅋㅋㅋ",
          "original_form": "저두ㅋㅋㅋ 좀 인간답게 살려면 해야겠더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034123.1.4",
          "form": "앜ㅋㅋㅋㅋ 정답이에요..",
          "original_form": "앜ㅋㅋㅋㅋ 정답이에요..",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100034123.1.5",
          "form": "그래서 필라테스 1년째 다니는 중이에요",
          "original_form": "그래서 필라테스 1년째 다니는 중이에요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100034123.1.6",
          "form": "스스로가 너무 대견한",
          "original_form": "스스로가 너무 대견한",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100034123.1.7",
          "form": "와 진짜 대단하셔요 1년동안이나다니다니!",
          "original_form": "와 진짜 대단하셔요 1년동안이나다니다니!",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034123.1.8",
          "form": "일주일에 몇회인가요?",
          "original_form": "일주일에 몇회인가요?",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034123.1.9",
          "form": "주 2회씩 가고있어요ㅎㅎㅎ",
          "original_form": "주 2회씩 가고있어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034123.1.10",
          "form": "3년 채우는 걸 목표로 하고 있엉ㅎ",
          "original_form": "3년 채우는 걸 목표로 하고 있엉ㅎ",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034123.1.11",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034123.1.12",
          "form": "와 저도 해볼까봐요ㅜ 확실히 몸매가 좋아지거나 살이빠지나요?",
          "original_form": "와 저도 해볼까봐요ㅜ 확실히 몸매가 좋아지거나 살이빠지나요?",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100034123.1.13",
          "form": "확실히 라인 정리가 좀 되더라구요",
          "original_form": "확실히 라인 정리가 좀 되더라구요",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100034123.1.14",
          "form": "살이 막 드라마틱하게 빠지는 운동은 아닌데",
          "original_form": "살이 막 드라마틱하게 빠지는 운동은 아닌데",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100034123.1.15",
          "form": "주변에서는 슬림해졌다고 알아봐줬어요",
          "original_form": "주변에서는 슬림해졌다고 알아봐줬어요",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100034123.1.16",
          "form": "오오! 고민되네요 ㅋㅋ 제트플립살까 고민했는데 정기권끊을가싶네요 ㅋㅋ",
          "original_form": "오오! 고민되네요 ㅋㅋ 제트플립살까 고민했는데 정기권끊을가싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100034123.1.17",
          "form": "아 이번 제트플립은 진짜 사고싶던데ㅋㅋㅋ 폰 뭐 쓰세요",
          "original_form": "아 이번 제트플립은 진짜 사고싶던데ㅋㅋㅋ 폰 뭐 쓰세요",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034123.1.18",
          "form": "아하! 저는 지금 엘지폰유저입니당 ㅋㅋ",
          "original_form": "아하! 저는 지금 엘지폰유저입니당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100034123.1.19",
          "form": "아하 저도 엘지폰 쓰다가 3개월전에 갤럭시s21로 왔어요 좀만 기다릴걸ㅠㅠㅋㅋ",
          "original_form": "아하 저도 엘지폰 쓰다가 3개월전에 갤럭시s21로 왔어요 좀만 기다릴걸ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100034123.1.20",
          "form": "필테학원과 갤s21의 카드값을 내며 열심히 일합니다...ㅎ..ㅎ.",
          "original_form": "필테학원과 갤s21의 카드값을 내며 열심히 일합니다...ㅎ..ㅎ.",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100034123.1.21",
          "form": "아정말요...? 이런 타이밍절묘하게안맞았네요 ㅜ",
          "original_form": "아정말요...? 이런 타이밍절묘하게안맞았네요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034123.1.22",
          "form": "엘지쓰다가 삼성쓰니까 어때요? 더좋나요??",
          "original_form": "엘지쓰다가 삼성쓰니까 어때요? 더좋나요??",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034123.1.23",
          "form": "네 삼성페이 좋아요...",
          "original_form": "네 삼성페이 좋아요...",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034123.1.24",
          "form": "전에 택시에서 엘지 페이기능 써서 계산하려고 했는데 안 받아줬어여...",
          "original_form": "전에 택시에서 엘지 페이기능 써서 계산하려고 했는데 안 받아줬어여...",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034123.1.25",
          "form": "아 맞아여.. 아직 엘지페이는 그닥 힘못쓰고있죠 ㅓ",
          "original_form": "아 맞아여.. 아직 엘지페이는 그닥 힘못쓰고있죠 ㅓ",
          "speaker_id": "1",
          "time": "20210828 09:32"
        },
