      "utterance": [
        {
          "id": "MDRW2100019337.1.1",
          "form": "여행을 가고 싶어",
          "original_form": "여행을 가고 싶어",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019337.1.2",
          "form": "코로나 때문에 여행 못간지 엄청 오래됐어 ㅠㅠ",
          "original_form": "코로나 때문에 여행 못간지 엄청 오래됐어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019337.1.3",
          "form": "원래 해외여행도 1년에 두번씩은 꼭 가고",
          "original_form": "원래 해외여행도 1년에 두번씩은 꼭 가고",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019337.1.4",
          "form": "코로나가 이렇게 길어질줄은",
          "original_form": "코로나가 이렇게 길어질줄은",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019337.1.5",
          "form": "해외여행은 2번은 가야해",
          "original_form": "해외여행은 2번은 가야해",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019337.1.6",
          "form": "열심히 비행기 마일리지도 모았는데 ㅠㅠ",
          "original_form": "열심히 비행기 마일리지도 모았는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019337.1.7",
          "form": "마일리지 비지니스 타야하능데 큰일",
          "original_form": "마일리지 비지니스 타야하능데 큰일",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019337.1.8",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.9",
          "form": "다시 해외여행 갈 수 있겠지? ㅠㅠ",
          "original_form": "다시 해외여행 갈 수 있겠지? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.10",
          "form": "내년엔 갈수 있을까",
          "original_form": "내년엔 갈수 있을까",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.11",
          "form": "가야겠지",
          "original_form": "가야겠지",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.12",
          "form": "제일 마지막으로 갔던데가 어디였더라",
          "original_form": "제일 마지막으로 갔던데가 어디였더라",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.13",
          "form": "백신도 맞고",
          "original_form": "백신도 맞고",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.14",
          "form": "태국이든가",
          "original_form": "태국이든가",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.15",
          "form": "가물가물하네",
          "original_form": "가물가물하네",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.16",
          "form": "말레이시아였나?",
          "original_form": "말레이시아였나?",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.17",
          "form": "태국 이후에 몰디브 가려다가 못 갔지",
          "original_form": "태국 이후에 몰디브 가려다가 못 갔지",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.18",
          "form": "사실 마지막 여행지는 몰디브가 될 뻔 했는데 ㅋㅋ",
          "original_form": "사실 마지막 여행지는 몰디브가 될 뻔 했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.19",
          "form": "아 태국이 마지막이었네",
          "original_form": "아 태국이 마지막이었네",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019337.1.20",
          "form": "그렇주ㅣ",
          "original_form": "그렇주ㅣ",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019337.1.21",
          "form": "방콕~ 사판 탁신~ 나나~",
          "original_form": "방콕~ 사판 탁신~ 나나~",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019337.1.22",
          "form": "태국은 물가가 비싸",
          "original_form": "태국은 물가가 비싸",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019337.1.23",
          "form": "나쁜 넘들",
          "original_form": "나쁜 넘들",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019337.1.24",
          "form": "동남아 치고 비싸긴 하지 ㅋㅋ",
          "original_form": "동남아 치고 비싸긴 하지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019337.1.25",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.26",
          "form": "몰디브 다시 예약해야지",
          "original_form": "몰디브 다시 예약해야지",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.27",
          "form": "면세점 사는 설레는 기분~",
          "original_form": "면세점 사는 설레는 기분~",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.28",
          "form": "비즈니스 타고~~",
          "original_form": "비즈니스 타고~~",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.29",
          "form": "면세점에서 술 구경하는것도 아주 재밌었는데",
          "original_form": "면세점에서 술 구경하는것도 아주 재밌었는데",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.30",
          "form": "술을 좋아하는구나",
          "original_form": "술을 좋아하는구나",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.31",
          "form": "나는 술을 잘 못먹어서",
          "original_form": "나는 술을 잘 못먹어서",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.32",
          "form": "여행가서 맨날 취하곤 해",
          "original_form": "여행가서 맨날 취하곤 해",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100019337.1.33",
          "form": "여행가서 술 마시면 더 빨리 취하는 기분이야",
          "original_form": "여행가서 술 마시면 더 빨리 취하는 기분이야",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019337.1.34",
          "form": "분위기 때문인가",
          "original_form": "분위기 때문인가",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019337.1.35",
          "form": "분위기에 취한다는 말이지",
          "original_form": "분위기에 취한다는 말이지",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019337.1.36",
          "form": "괜히 여행가는게 아니지",
          "original_form": "괜히 여행가는게 아니지",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019337.1.37",
          "form": "그렇지 여행가면 평소와 다른 장소에서 느끼는 약간의 긴장감과 들뜸 그런게 있잖아",
          "original_form": "그렇지 여행가면 평소와 다른 장소에서 느끼는 약간의 긴장감과 들뜸 그런게 있잖아",
          "speaker_id": "1",
          "time": "20210827 17:49"
        }
      ]
    }
