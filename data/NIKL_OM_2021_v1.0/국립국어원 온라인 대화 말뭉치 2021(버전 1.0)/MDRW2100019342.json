      "utterance": [
        {
          "id": "MDRW2100019342.1.1",
          "form": "알바 어떤거 해보셨어요?",
          "original_form": "알바 어떤거 해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100019342.1.2",
          "form": "저는 쿠팡이랑 포장이요",
          "original_form": "저는 쿠팡이랑 포장이요",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019342.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019342.1.4",
          "form": "저는 콘서트, 콜센터, 행사, 사무보조 별거 다 해봤어요ㅋㅋ",
          "original_form": "저는 콘서트, 콜센터, 행사, 사무보조 별거 다 해봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100019342.1.5",
          "form": "우와 콘서트나 행사는 어떻게 뽑혀요? 저 콘서트 해보고 싶은데",
          "original_form": "우와 콘서트나 행사는 어떻게 뽑혀요? 저 콘서트 해보고 싶은데",
          "speaker_id": "2",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100019342.1.6",
          "form": "단기행사 알바로 갔었어요!!!",
          "original_form": "단기행사 알바로 갔었어요!!!",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100019342.1.7",
          "form": "알바몬에 올라오는 단기행사로 갔었는데",
          "original_form": "알바몬에 올라오는 단기행사로 갔었는데",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019342.1.8",
          "form": "몇번 가서 얼굴 익혔더니 담에 또 불러주시더라구요",
          "original_form": "몇번 가서 얼굴 익혔더니 담에 또 불러주시더라구요",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019342.1.9",
          "form": "콘서트는 주로 실내체육관 쪽인가요?",
          "original_form": "콘서트는 주로 실내체육관 쪽인가요?",
          "speaker_id": "2",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100019342.1.10",
          "form": "경희대 평화의전당에서 하는 콘서트 있었고",
          "original_form": "경희대 평화의전당에서 하는 콘서트 있었고",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019342.1.11",
          "form": "시청광장에서 하는 콘서트도 있었고",
          "original_form": "시청광장에서 하는 콘서트도 있었고",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019342.1.12",
          "form": "막 음악프로 큰 콘서트같은건 아니었어요ㅋㅋ",
          "original_form": "막 음악프로 큰 콘서트같은건 아니었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019342.1.13",
          "form": "콘서트 알바면 주로 뭘 하나요?",
          "original_form": "콘서트 알바면 주로 뭘 하나요?",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019342.1.14",
          "form": "티켓팅이나 동선관리 주로 했어요",
          "original_form": "티켓팅이나 동선관리 주로 했어요",
          "speaker_id": "1",
          "time": "20210827 18:20"
        },
        {
          "id": "MDRW2100019342.1.15",
          "form": "사람구경하는거 재밌어요~",
          "original_form": "사람구경하는거 재밌어요~",
          "speaker_id": "1",
          "time": "20210827 18:20"
        },
        {
          "id": "MDRW2100019342.1.16",
          "form": "알바하다가 연예인 보신 적은 없나요?",
          "original_form": "알바하다가 연예인 보신 적은 없나요?",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019342.1.17",
          "form": "많이 봤죠! 송중기 팬미팅 알바할때 송중기랑 이광수 보고ㅋㅋ",
          "original_form": "많이 봤죠! 송중기 팬미팅 알바할때 송중기랑 이광수 보고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019342.1.18",
          "form": "뭐 여러사람 봤는데 누구였는지 기억이 안나요",
          "original_form": "뭐 여러사람 봤는데 누구였는지 기억이 안나요",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019342.1.19",
          "form": "우와 부럽네요ㅠㅠㅠ",
          "original_form": "우와 부럽네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019342.1.20",
          "form": "ㅎㅎ쿠팡 많이 힘들지 않아요?",
          "original_form": "ㅎㅎ쿠팡 많이 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100019342.1.21",
          "form": "엄청 힘들어요 저 작년 여름 한창 더울 때 했다가 죽는줄 알았어요ㅋㅋㅋ",
          "original_form": "엄청 힘들어요 저 작년 여름 한창 더울 때 했다가 죽는줄 알았어요ㅋㅋㅋ",
