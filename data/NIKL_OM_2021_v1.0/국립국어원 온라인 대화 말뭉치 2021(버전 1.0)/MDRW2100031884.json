      "utterance": [
        {
          "id": "MDRW2100031884.1.1",
          "form": "백신은 그럼 10부제 예약으로 신청하셨어요?",
          "original_form": "백신은 그럼 10부제 예약으로 신청하셨어요?",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031884.1.2",
          "form": "아뇨~전 잔여백신이요~",
          "original_form": "아뇨~전 잔여백신이요~",
          "speaker_id": "2",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100031884.1.3",
          "form": "원래는..안맞으려고했어요~",
          "original_form": "원래는..안맞으려고했어요~",
          "speaker_id": "2",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031884.1.4",
          "form": "어찌저찌 하다 잔여백신이 되서 ㅋㅋㅋㅋ",
          "original_form": "어찌저찌 하다 잔여백신이 되서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031884.1.5",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031884.1.6",
          "form": "저는 직업상 맞아야해서 10부제 전에 예약해서 맞았는데",
          "original_form": "저는 직업상 맞아야해서 10부제 전에 예약해서 맞았는데",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100031884.1.7",
          "form": "안그래도 그 질문하려고했네요 ㅋㅋㅋㅋ",
          "original_form": "안그래도 그 질문하려고했네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031884.1.8",
          "form": "12시 땡하고 예약하려고 들어갔더니 서버가 다운돼서ㅠ",
          "original_form": "12시 땡하고 예약하려고 들어갔더니 서버가 다운돼서ㅠ",
          "speaker_id": "1",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031884.1.9",
          "form": "2차까지 완료시면~",
          "original_form": "2차까지 완료시면~",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031884.1.10",
          "form": "새벽 4시에 깨서 다시 예약했어요",
          "original_form": "새벽 4시에 깨서 다시 예약했어요",
          "speaker_id": "1",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031884.1.11",
          "form": "직업상 맞아야하는분들이 많더라구요~",
          "original_form": "직업상 맞아야하는분들이 많더라구요~",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031884.1.12",
          "form": "선택의 여지가 없..ㅠㅠ",
          "original_form": "선택의 여지가 없..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 07:36"
        },
        {
          "id": "MDRW2100031884.1.13",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031884.1.14",
          "form": "그래서 맞는데 예약까지 힘드니...",
          "original_form": "그래서 맞는데 예약까지 힘드니...",
          "speaker_id": "1",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100031884.1.15",
          "form": "검사도 주1번씩 해야하는 경우도있고..",
          "original_form": "검사도 주1번씩 해야하는 경우도있고..",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031884.1.16",
          "form": "너무 힘들겠더라구요ㅠ",
          "original_form": "너무 힘들겠더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031884.1.17",
          "form": "검사도 보건소에서 줄서서 해야한다고ㅠ",
          "original_form": "검사도 보건소에서 줄서서 해야한다고ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031884.1.18",
          "form": "네 어린이집 보육교사분들은 그랬다고 들었어요",
          "original_form": "네 어린이집 보육교사분들은 그랬다고 들었어요",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100031884.1.19",
          "form": "이젠 백신접종 하고나면 검사는 안해도 되지 않을까요?",
          "original_form": "이젠 백신접종 하고나면 검사는 안해도 되지 않을까요?",
          "speaker_id": "1",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100031884.1.20",
          "form": "저두 잘...;;여튼 얘기 듣는데 너무 한다 싶었어요;",
          "original_form": "저두 잘...;;여튼 얘기 듣는데 너무 한다 싶었어요;",
          "speaker_id": "2",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100031884.1.21",
          "form": "코로나땜에 안해고 될일을 시간 돈 낭비해가면서ㅠ",
          "original_form": "코로나땜에 안해고 될일을 시간 돈 낭비해가면서ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100031884.1.22",
          "form": "네 맞아요ㅜ",
          "original_form": "네 맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:41"
        }
      ]
    }
