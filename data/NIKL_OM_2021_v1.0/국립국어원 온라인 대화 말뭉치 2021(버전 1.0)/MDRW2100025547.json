      "utterance": [
        {
          "id": "MDRW2100025547.1.1",
          "form": "아가용이었군",
          "original_form": "아가용이었군",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025547.1.2",
          "form": "하긴 초코떡볶이누가먹어",
          "original_form": "하긴 초코떡볶이누가먹어",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025547.1.3",
          "form": "와 효뜨",
          "original_form": "와 효뜨",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025547.1.4",
          "form": "마시써",
          "original_form": "마시써",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025547.1.5",
          "form": "완전",
          "original_form": "완전",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025547.1.6",
          "form": "먹음??",
          "original_form": "먹음??",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025547.1.7",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025547.1.8",
          "form": "근데 고수갖다달랬는데",
          "original_form": "근데 고수갖다달랬는데",
          "speaker_id": "1",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025547.1.9",
          "form": "단무지를더줌",
          "original_form": "단무지를더줌",
          "speaker_id": "1",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025547.1.10",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025547.1.11",
          "form": "귀가 많이 어두우시네",
          "original_form": "귀가 많이 어두우시네",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025547.1.12",
          "form": "나도 고수 엄청들어간 쌀국수",
          "original_form": "나도 고수 엄청들어간 쌀국수",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025547.1.13",
          "form": "먹고파",
          "original_form": "먹고파",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025547.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025547.1.15",
          "form": "아 고수안들머갔어",
          "original_form": "아 고수안들머갔어",
          "speaker_id": "1",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100025547.1.16",
          "form": "근데닭고기 맛있더라",
          "original_form": "근데닭고기 맛있더라",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.17",
          "form": "닭고기 쌀국수?",
          "original_form": "닭고기 쌀국수?",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.18",
          "form": "ㅇㅇ거기 메인",
          "original_form": "ㅇㅇ거기 메인",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.19",
          "form": "맛있겠다...",
          "original_form": "맛있겠다...",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.20",
          "form": "아니 닭고기 쌀국수를 붙여씀",
          "original_form": "아니 닭고기 쌀국수를 붙여씀",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.21",
          "form": "왜나쁜말미야",
          "original_form": "왜나쁜말미야",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.22",
          "form": "심심이이상해",
          "original_form": "심심이이상해",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.23",
          "form": "태국식? 베트남식?",
          "original_form": "태국식? 베트남식?",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025547.1.24",
          "form": "음 이건베트남식이야",
          "original_form": "음 이건베트남식이야",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025547.1.25",
          "form": "다리살이어서 부드럽고맛있고",
          "original_form": "다리살이어서 부드럽고맛있고",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025547.1.26",
          "form": "국물은찐해",
          "original_form": "국물은찐해",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025547.1.27",
          "form": "부추같은거들어감",
          "original_form": "부추같은거들어감",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025547.1.28",
          "form": "삼계탕인데?",
          "original_form": "삼계탕인데?",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025547.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025547.1.30",
          "form": "맛은잇겠군",
          "original_form": "맛은잇겠군",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
