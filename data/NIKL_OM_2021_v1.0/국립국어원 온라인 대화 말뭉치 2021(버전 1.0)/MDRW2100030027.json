      "utterance": [
        {
          "id": "MDRW2100030027.1.1",
          "form": "전세계의 소고기를 못 먹은 사람들을 대신해 분노합니다",
          "original_form": "전세계의 소고기를 못 먹은 사람들을 대신해 분노합니다",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100030027.1.2",
          "form": "오늘 회랑 새우튀김",
          "original_form": "오늘 회랑 새우튀김",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030027.1.3",
          "form": "드신분 손 들어보세요",
          "original_form": "드신분 손 들어보세요",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030027.1.4",
          "form": "제 꿈은 정치인입니다",
          "original_form": "제 꿈은 정치인입니다",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030027.1.5",
          "form": "기억이 나지 않죠",
          "original_form": "기억이 나지 않죠",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030027.1.6",
          "form": "낙선운동 합니다",
          "original_form": "낙선운동 합니다",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030027.1.7",
          "form": "에헴",
          "original_form": "에헴",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030027.1.8",
          "form": "벌써부터 제 출마를 기다리는 분이 있다니 뿌듯하네요",
          "original_form": "벌써부터 제 출마를 기다리는 분이 있다니 뿌듯하네요",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100030027.1.9",
          "form": "회 사주세요",
          "original_form": "회 사주세요",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030027.1.10",
          "form": "금권선거는 지양합니다",
          "original_form": "금권선거는 지양합니다",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030027.1.11",
          "form": "깨끗한 한표만 받을게요",
          "original_form": "깨끗한 한표만 받을게요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100030027.1.12",
          "form": "낙선운동합니다",
          "original_form": "낙선운동합니다",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100030027.1.13",
          "form": "이것참 이렇게 출마를 종용하시니 몸둘바를 모르겠네요ㅎ",
          "original_form": "이것참 이렇게 출마를 종용하시니 몸둘바를 모르겠네요ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100030027.1.14",
          "form": "회 사줘요",
          "original_form": "회 사줘요",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030027.1.15",
          "form": "새우튀김도 ㅋㅋ",
          "original_form": "새우튀김도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100030027.1.16",
          "form": "멍게는 차별하나요!",
          "original_form": "멍게는 차별하나요!",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030027.1.17",
          "form": "멍게는 안좋아해요",
          "original_form": "멍게는 안좋아해요",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030027.1.18",
          "form": "실망입니다",
          "original_form": "실망입니다",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030027.1.19",
          "form": "그세트의 메인은 멍게였는데",
          "original_form": "그세트의 메인은 멍게였는데",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030027.1.20",
          "form": "멍게쟁이시네요",
          "original_form": "멍게쟁이시네요",
          "speaker_id": "2",
          "time": "20210903 23:12"
        }
