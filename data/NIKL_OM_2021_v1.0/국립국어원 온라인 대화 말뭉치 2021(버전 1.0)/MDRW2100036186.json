      "utterance": [
        {
          "id": "MDRW2100036186.1.1",
          "form": "제가 한 두달만에 엄청 쪘어요ㅜ",
          "original_form": "제가 한 두달만에 엄청 쪘어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100036186.1.2",
          "form": "얼마나요?",
          "original_form": "얼마나요?",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100036186.1.3",
          "form": "무슨일 있으셨나요?",
          "original_form": "무슨일 있으셨나요?",
          "speaker_id": "2",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100036186.1.4",
          "form": "아뇨 제가 원래 1일 1식했는데",
          "original_form": "아뇨 제가 원래 1일 1식했는데",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100036186.1.5",
          "form": "너무스트레스받아서",
          "original_form": "너무스트레스받아서",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100036186.1.6",
          "form": "제맘대로 살다가 요요가 왔어요ㅜ",
          "original_form": "제맘대로 살다가 요요가 왔어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100036186.1.7",
          "form": "와 1일1식이요?",
          "original_form": "와 1일1식이요?",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100036186.1.8",
          "form": "그거 어떻게해요?",
          "original_form": "그거 어떻게해요?",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100036186.1.9",
          "form": "하루에 한끼는 어떨걸먹어요,",
          "original_form": "하루에 한끼는 어떨걸먹어요,",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100036186.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100036186.1.11",
          "form": "다이어트식으로요",
          "original_form": "다이어트식으로요",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036186.1.12",
          "form": "음 일단 최소집밥? ㅋ",
          "original_form": "음 일단 최소집밥? ㅋ",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100036186.1.13",
          "form": "몇시쯤먹나요?",
          "original_form": "몇시쯤먹나요?",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100036186.1.14",
          "form": "공복시 배고프면 어떻게해요?",
          "original_form": "공복시 배고프면 어떻게해요?",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100036186.1.15",
          "form": "음 커피나 물 마셔요",
          "original_form": "음 커피나 물 마셔요",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036186.1.16",
          "form": "저는 12시~2시3시에도 먹었어요",
          "original_form": "저는 12시~2시3시에도 먹었어요",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100036186.1.17",
          "form": "물마셔도 ㅜㅠ",
          "original_form": "물마셔도 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036186.1.18",
          "form": "손떨릴듯해요",
          "original_form": "손떨릴듯해요",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036186.1.19",
          "form": "배고파서",
          "original_form": "배고파서",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036186.1.20",
          "form": "그럴수도 있는데",
          "original_form": "그럴수도 있는데",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100036186.1.21",
          "form": "활동량에 따라 다를것같아요 ㅎㅎ",
          "original_form": "활동량에 따라 다를것같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100036186.1.22",
          "form": "약은 안드시구요?",
          "original_form": "약은 안드시구요?",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036186.1.23",
          "form": "저는 예전에 환으로된거 약 먹은적있거든요",
          "original_form": "저는 예전에 환으로된거 약 먹은적있거든요",
          "speaker_id": "2",
          "time": "20210831 06:34"
        },
        {
          "id": "MDRW2100036186.1.24",
          "form": "진짜 입맛은 뚝떨어지던데",
          "original_form": "진짜 입맛은 뚝떨어지던데",
          "speaker_id": "2",
          "time": "20210831 06:34"
        },
        {
          "id": "MDRW2100036186.1.25",
          "form": "장이 막 꼬이듯이 아파서 그만먹어요",
          "original_form": "장이 막 꼬이듯이 아파서 그만먹어요",
          "speaker_id": "2",
          "time": "20210831 06:35"
        },
        {
          "id": "MDRW2100036186.1.26",
          "form": "헉 저는 오늘부터 48시간은 레디톡인가 이거먹어요",
          "original_form": "헉 저는 오늘부터 48시간은 레디톡인가 이거먹어요",
          "speaker_id": "1",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100036186.1.27",
          "form": "이건 음 하루 5번 이거 먹는거네요 ㅋㅋㅋ",
          "original_form": "이건 음 하루 5번 이거 먹는거네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:38"
        },
        {
          "id": "MDRW2100036186.1.28",
          "form": "레디톡?",
          "original_form": "레디톡?",
          "speaker_id": "2",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036186.1.29",
          "form": "다이어트 보조제예요?",
          "original_form": "다이어트 보조제예요?",
          "speaker_id": "2",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100036186.1.30",
          "form": "네이버에 치시면 나오는데요",
          "original_form": "네이버에 치시면 나오는데요",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036186.1.31",
          "form": "알약은아니고",
          "original_form": "알약은아니고",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036186.1.32",
          "form": "주스?",
          "original_form": "주스?",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036186.1.33",
          "form": "효과는있어요?",
          "original_form": "효과는있어요?",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036186.1.34",
          "form": "식품유형이 과채주스긴하네요 ㅋ",
          "original_form": "식품유형이 과채주스긴하네요 ㅋ",
          "speaker_id": "1",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100036186.1.35",
          "form": "그 할리우드 주스? 그거같아요",
          "original_form": "그 할리우드 주스? 그거같아요",
          "speaker_id": "1",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100036186.1.36",
          "form": "그것만 하루에 5번먹는건가요?",
          "original_form": "그것만 하루에 5번먹는건가요?",
          "speaker_id": "2",
          "time": "20210831 13:57"
        },
        {
          "id": "MDRW2100036186.1.37",
          "form": "넵 이거 원액이니 물 150타서 먹으라네요",
          "original_form": "넵 이거 원액이니 물 150타서 먹으라네요",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036186.1.38",
          "form": "그럼이거가 60이니까 총210",
          "original_form": "그럼이거가 60이니까 총210",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036186.1.39",
          "form": "1리터 물마시는거나 다름없네요",
          "original_form": "1리터 물마시는거나 다름없네요",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100036186.1.40",
          "form": "맛은 있어요?",
          "original_form": "맛은 있어요?",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100036186.1.41",
          "form": "맛은 있는데",
          "original_form": "맛은 있는데",
          "speaker_id": "1",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100036186.1.42",
          "form": "진짜 그냥 자몽맛주스 맛이요",
          "original_form": "진짜 그냥 자몽맛주스 맛이요",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100036186.1.43",
          "form": "새콤달콤한",
          "original_form": "새콤달콤한",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100036186.1.44",
          "form": "저두 한번 사봐야겠어요 ㅎㅎ",
          "original_form": "저두 한번 사봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:25"
        }
      ]
