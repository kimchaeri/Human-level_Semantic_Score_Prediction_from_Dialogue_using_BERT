      "utterance": [
        {
          "id": "MDRW2100016056.1.1",
          "form": "밀키트 잘나오죠 너무",
          "original_form": "밀키트 잘나오죠 너무",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016056.1.2",
          "form": "샐러드에 삼겹살구워 올려먹으니 너무맛났어요",
          "original_form": "샐러드에 삼겹살구워 올려먹으니 너무맛났어요",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016056.1.3",
          "form": "갑자기 먹고싶네요",
          "original_form": "갑자기 먹고싶네요",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016056.1.4",
          "form": "저는 삼겹살 쫄면 사다가 같이 먹는거 짱좋아합니다!!!!!",
          "original_form": "저는 삼겹살 쫄면 사다가 같이 먹는거 짱좋아합니다!!!!!",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016056.1.5",
          "form": "땡기네요 진짜...",
          "original_form": "땡기네요 진짜...",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016056.1.6",
          "form": "그쵸...완전맛있던데",
          "original_form": "그쵸...완전맛있던데",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016056.1.7",
          "form": "샐러드에 야채골고루해서",
          "original_form": "샐러드에 야채골고루해서",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016056.1.8",
          "form": "방금 오레오 티라미스맛 레드벨벳맛 첨먹어봤는데 오레오는 오리지날이 제일 맛있는걸로!!! ㅋㅋㅋㅋ",
          "original_form": "방금 오레오 티라미스맛 레드벨벳맛 첨먹어봤는데 오레오는 오리지날이 제일 맛있는걸로!!! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100016056.1.9",
          "form": "아 저는 아직 안먹어봤는데",
          "original_form": "아 저는 아직 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016056.1.10",
          "form": "오리지날만 먹는걸루..ㅎ",
          "original_form": "오리지날만 먹는걸루..ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016056.1.11",
          "form": "입이 너무 달아졌네요ㅋㅋㅋ간식이나 디저트류도 좋아하시나용?!",
          "original_form": "입이 너무 달아졌네요ㅋㅋㅋ간식이나 디저트류도 좋아하시나용?!",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016056.1.12",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "1",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016056.1.13",
          "form": "봉지과자좋아하고 쿠키도 좋아해요",
          "original_form": "봉지과자좋아하고 쿠키도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016056.1.14",
          "form": "꼬북칩 추러스맛은 너무달더라구요 저한테는",
          "original_form": "꼬북칩 추러스맛은 너무달더라구요 저한테는",
          "speaker_id": "1",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016056.1.15",
          "form": "아 저도 그거맛있긴한데 진짜 딱 한두개먹으면 좋더라구요!!! ㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 그거맛있긴한데 진짜 딱 한두개먹으면 좋더라구요!!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016056.1.16",
          "form": "저도 단거좋아하지만 막 입이 너무 달아지는 느낌은 싫어해요ㅠㅠ",
          "original_form": "저도 단거좋아하지만 막 입이 너무 달아지는 느낌은 싫어해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016056.1.17",
          "form": "네 저도요 딱 2~3개까지만 좋아요",
          "original_form": "네 저도요 딱 2~3개까지만 좋아요",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016056.1.18",
          "form": "전 새우깡이 맛있어요\r\n옛날과자지만 ㅎ",
          "original_form": "전 새우깡이 맛있어요\r\n옛날과자지만 ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016056.1.19",
          "form": "새우깡 오징어땅콩 꼬깔콘 이런거좋아합니다 ㅋㅋㅋㅋㅋ생각해보니 봉지과자는 단거보단 고소하고 짭짤한거 좋아하는듯요ㅋㅋㅋㅋㅋ",
          "original_form": "새우깡 오징어땅콩 꼬깔콘 이런거좋아합니다 ㅋㅋㅋㅋㅋ생각해보니 봉지과자는 단거보단 고소하고 짭짤한거 좋아하는듯요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:37"
        },
        {
          "id": "MDRW2100016056.1.20",
          "form": "맞아요 제가 다 좋아하는것들이에요\r\n고소짭짤 좋아해요\r\n맥주안주로도 좋구요",
          "original_form": "맞아요 제가 다 좋아하는것들이에요\r\n고소짭짤 좋아해요\r\n맥주안주로도 좋구요",
          "speaker_id": "1",
          "time": "20210826 09:41"
        },
        {
          "id": "MDRW2100016056.1.21",
          "form": "맥주 캬...맥주 한잔 땡기네요 아침부터 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맥주 캬...맥주 한잔 땡기네요 아침부터 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016056.1.22",
          "form": "맥주vs소주 어떤쪽이 더 좋으신가요?!",
          "original_form": "맥주vs소주 어떤쪽이 더 좋으신가요?!",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016056.1.23",
          "form": "저는 맥주요 ㅎ",
          "original_form": "저는 맥주요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100016056.1.24",
          "form": "맛있어요 맥주 ㅎㅎ적당히 알딸딸해지면서요",
          "original_form": "맛있어요 맥주 ㅎㅎ적당히 알딸딸해지면서요",
          "speaker_id": "1",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100016056.1.25",
          "form": "저두 맥주요 ㅋㅋㅋㅋ소주는 아직도 무슨맛으로 먹는건지 몰겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저두 맥주요 ㅋㅋㅋㅋ소주는 아직도 무슨맛으로 먹는건지 몰겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:50"
        },
        {
          "id": "MDRW2100016056.1.26",
          "form": "저두요 너무 써요 ㅎ\r\n맥주는 얼마나 시원한지",
          "original_form": "저두요 너무 써요 ㅎ\r\n맥주는 얼마나 시원한지",
          "speaker_id": "1",
          "time": "20210826 09:51"
        }
      ]
