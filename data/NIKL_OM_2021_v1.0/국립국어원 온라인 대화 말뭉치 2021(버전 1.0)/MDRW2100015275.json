      "utterance": [
        {
          "id": "MDRW2100015275.1.1",
          "form": "오늘 뭐 드셨어요?",
          "original_form": "오늘 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015275.1.2",
          "form": "우유요",
          "original_form": "우유요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.4",
          "form": "우유만요?",
          "original_form": "우유만요?",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.5",
          "form": "우유랑 물이요",
          "original_form": "우유랑 물이요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.6",
          "form": "님 혹시 강아지인가요",
          "original_form": "님 혹시 강아지인가요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.7",
          "form": "고양인가요",
          "original_form": "고양인가요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.8",
          "form": "그거말고는 안 마신 것 같네요",
          "original_form": "그거말고는 안 마신 것 같네요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.9",
          "form": "전 쫄면이랑 김볶밥이요",
          "original_form": "전 쫄면이랑 김볶밥이요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015275.1.10",
          "form": "식음료잖아요",
          "original_form": "식음료잖아요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.11",
          "form": "음료 얘기하는 거 아닌가요",
          "original_form": "음료 얘기하는 거 아닌가요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.12",
          "form": "그리고 김볶밥이라뇨 별걸 다 줄이시네요",
          "original_form": "그리고 김볶밥이라뇨 별걸 다 줄이시네요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.13",
          "form": "식사 메뉴도 됩니다",
          "original_form": "식사 메뉴도 됩니다",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.14",
          "form": "저기요 이건 거의 표준어예요",
          "original_form": "저기요 이건 거의 표준어예요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.15",
          "form": "김볶밥은 치즈랑 먹어야하는 거 아시죠",
          "original_form": "김볶밥은 치즈랑 먹어야하는 거 아시죠",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.16",
          "form": "저희엄마는 치즈 안 줬어요",
          "original_form": "저희엄마는 치즈 안 줬어요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015275.1.17",
          "form": "주는대로 먹습니다",
          "original_form": "주는대로 먹습니다",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.18",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.19",
          "form": "직접 찾아 드셔야죠",
          "original_form": "직접 찾아 드셔야죠",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.20",
          "form": "님이야말로 강아지시네요",
          "original_form": "님이야말로 강아지시네요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.21",
          "form": "불닭볶음면 좋아하세요?",
          "original_form": "불닭볶음면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.22",
          "form": "이번에 불닭볶음당면이 새로 나왔더라고요",
          "original_form": "이번에 불닭볶음당면이 새로 나왔더라고요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.23",
          "form": "꼭 먹어볼거예요.",
          "original_form": "꼭 먹어볼거예요.",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015275.1.24",
          "form": "오 처음 들어봤어요",
          "original_form": "오 처음 들어봤어요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015275.1.25",
          "form": "당면만 들어있는 거예요?",
          "original_form": "당면만 들어있는 거예요?",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015275.1.26",
          "form": "네 당면만 들어있어요",
          "original_form": "네 당면만 들어있어요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015275.1.27",
          "form": "납작당면 좋아하세요?",
          "original_form": "납작당면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015275.1.28",
          "form": "그럼여",
          "original_form": "그럼여",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015275.1.29",
          "form": "떡볶이에 납작당면 먹는거 좋아해요",
          "original_form": "떡볶이에 납작당면 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        }
      ]
    }
  ]
