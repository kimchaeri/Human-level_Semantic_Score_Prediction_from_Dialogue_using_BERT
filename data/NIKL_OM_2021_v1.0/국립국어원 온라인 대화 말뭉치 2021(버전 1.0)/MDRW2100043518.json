      "utterance": [
        {
          "id": "MDRW2100043518.1.1",
          "form": "체인지데이즈~ 화욜",
          "original_form": "체인지데이즈~ 화욜",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043518.1.2",
          "form": "체데 지금 켰어요",
          "original_form": "체데 지금 켰어요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043518.1.3",
          "form": "ㅋㅋㅋ홍주 ㅋㅋㅋㅋ뭐가 갈팡질팡? ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ홍주 ㅋㅋㅋㅋ뭐가 갈팡질팡? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.4",
          "form": "웃기네요",
          "original_form": "웃기네요",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.5",
          "form": "휴...이자슥들",
          "original_form": "휴...이자슥들",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.6",
          "form": "ㅇ0ㅇ 오늘 한시간이네요 ㅋㅋㅋ길다",
          "original_form": "ㅇ0ㅇ 오늘 한시간이네요 ㅋㅋㅋ길다",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.7",
          "form": "그죠 먼일인지",
          "original_form": "그죠 먼일인지",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.8",
          "form": "휴 시작하자마자 열받아요",
          "original_form": "휴 시작하자마자 열받아요",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.9",
          "form": "지난주 다시버여주는데",
          "original_form": "지난주 다시버여주는데",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043518.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043518.1.11",
          "form": "아 무슨 ㅋㅋ차안에서 이미 연인임",
          "original_form": "아 무슨 ㅋㅋ차안에서 이미 연인임",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043518.1.12",
          "form": "그들 ...? 참나",
          "original_form": "그들 ...? 참나",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043518.1.13",
          "form": "하 너무 짜증나여",
          "original_form": "하 너무 짜증나여",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043518.1.14",
          "form": "그죠 근데 분위기안조음 ㅋ큐",
          "original_form": "그죠 근데 분위기안조음 ㅋ큐",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043518.1.15",
          "form": "나쁜애들이 꼭 저렇게말함 내가 너무 나쁜애 일수듀 있겠지만",
          "original_form": "나쁜애들이 꼭 저렇게말함 내가 너무 나쁜애 일수듀 있겠지만",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043518.1.16",
          "form": "제발 데이트 망해라",
          "original_form": "제발 데이트 망해라",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043518.1.17",
          "form": "성호는 신났네",
          "original_form": "성호는 신났네",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043518.1.18",
          "form": "ㅋㅋㅋㅋ 아냐 망해야댐 잘담 흥 지금에야 생각하는척 ㅡㅡ",
          "original_form": "ㅋㅋㅋㅋ 아냐 망해야댐 잘담 흥 지금에야 생각하는척 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043518.1.19",
          "form": "휴 진짜 싫어요ㅠ 민선인 그래도 진록생각하는것같은데",
          "original_form": "휴 진짜 싫어요ㅠ 민선인 그래도 진록생각하는것같은데",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043518.1.20",
          "form": "성호는 걍 ㅋㅋㅋ별생각 없어보임",
          "original_form": "성호는 걍 ㅋㅋㅋ별생각 없어보임",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043518.1.21",
          "form": "ㅋㅋㅋ 인터뷰는 또 ㅋㅋ 상미생각한대",
          "original_form": "ㅋㅋㅋ 인터뷰는 또 ㅋㅋ 상미생각한대",
