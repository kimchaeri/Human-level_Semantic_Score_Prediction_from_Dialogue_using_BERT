      "utterance": [
        {
          "id": "MDRW2100025682.1.1",
          "form": "ㅣ금 다시 볼용기는없어서",
          "original_form": "ㅣ금 다시 볼용기는없어서",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025682.1.2",
          "form": "라이브클립으로...",
          "original_form": "라이브클립으로...",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025682.1.3",
          "form": "오 노래가 되게 음산한느낌.....?",
          "original_form": "오 노래가 되게 음산한느낌.....?",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025682.1.4",
          "form": "아 그거? 저승사자 복장 라이브?",
          "original_form": "아 그거? 저승사자 복장 라이브?",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025682.1.5",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025682.1.6",
          "form": "그거봐써",
          "original_form": "그거봐써",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025682.1.7",
          "form": "저승사자 복장이",
          "original_form": "저승사자 복장이",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025682.1.8",
          "form": "특이한듯",
          "original_form": "특이한듯",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025682.1.9",
          "form": "저승사자 복장 그거",
          "original_form": "저승사자 복장 그거",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025682.1.10",
          "form": "집에 도포랑 갓이 있어서 한거라던데...",
          "original_form": "집에 도포랑 갓이 있어서 한거라던데...",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025682.1.11",
          "form": "그게 집에 있기 쉽지 않은데...",
          "original_form": "그게 집에 있기 쉽지 않은데...",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025682.1.12",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025682.1.13",
          "form": "아닠ㅋㅋㅋㄱ 집에 도포랑 갓이요...?",
          "original_form": "아닠ㅋㅋㅋㄱ 집에 도포랑 갓이요...?",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025682.1.14",
          "form": "그것도 까만색....?",
          "original_form": "그것도 까만색....?",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025682.1.15",
          "form": "그렇다던데요...?",
          "original_form": "그렇다던데요...?",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025682.1.16",
          "form": "렌탈소품 아니고 진짜 안예은거라고...",
          "original_form": "렌탈소품 아니고 진짜 안예은거라고...",
          "speaker_id": "1",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025682.1.17",
          "form": "헐ㅋㅋㄱㅋㄱㅋㅋㄱ",
          "original_form": "헐ㅋㅋㄱㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025682.1.18",
          "form": "그런게있는게 신기함",
          "original_form": "그런게있는게 신기함",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025682.1.19",
          "form": "본인이 오타쿠 성향이라 그런지..",
          "original_form": "본인이 오타쿠 성향이라 그런지..",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025682.1.20",
          "form": "조정석 찍덕이었다며....",
          "original_form": "조정석 찍덕이었다며....",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025682.1.21",
          "form": "ㅇㅇㅇ ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅇ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025682.1.22",
          "form": "엌ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025682.1.23",
          "form": "진짜??첨들먹ㅋㅋㄱㅋㄱㅋㅋ",
          "original_form": "진짜??첨들먹ㅋㅋㄱㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.24",
          "form": "저승사자옷이랑 찍덕은 좀 결이 다르긴 하지만..",
          "original_form": "저승사자옷이랑 찍덕은 좀 결이 다르긴 하지만..",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.25",
          "form": "저번주 유스케 나와서",
          "original_form": "저번주 유스케 나와서",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.26",
          "form": "말하더라고여",
          "original_form": "말하더라고여",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.28",
          "form": "관상으로 입덕해서",
          "original_form": "관상으로 입덕해서",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.29",
          "form": "뽀를 좋아하셨군",
          "original_form": "뽀를 좋아하셨군",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.30",
          "form": "경기권까진 대포 들고 다 돌았다고",
          "original_form": "경기권까진 대포 들고 다 돌았다고",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.31",
          "form": "아 조정석을좋아하셨군",
          "original_form": "아 조정석을좋아하셨군",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.33",
          "form": "뽀시절은 아니었군",
          "original_form": "뽀시절은 아니었군",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025682.1.34",
          "form": "진짜로 조아하는 것 같더라ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜로 조아하는 것 같더라ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025682.1.35",
          "form": "유희열이 조정석 얘기 꺼내니까",
          "original_form": "유희열이 조정석 얘기 꺼내니까",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025682.1.36",
          "form": "(약간 계타는 식의 얘기였음",
          "original_form": "(약간 계타는 식의 얘기였음",
          "speaker_id": "1",
          "time": "20210901 18:41"
        }
      ]
