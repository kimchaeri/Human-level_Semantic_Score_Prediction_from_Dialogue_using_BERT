      "utterance": [
        {
          "id": "MDRW2100028747.1.1",
          "form": "어 아프네",
          "original_form": "어 아프네",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028747.1.2",
          "form": "내가 주물러 준지 얼마",
          "original_form": "내가 주물러 준지 얼마",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028747.1.3",
          "form": "되지도 않았는데",
          "original_form": "되지도 않았는데",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028747.1.4",
          "form": "바로 아프네",
          "original_form": "바로 아프네",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028747.1.5",
          "form": "어깨가 별로 안 좋아서 그래",
          "original_form": "어깨가 별로 안 좋아서 그래",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028747.1.6",
          "form": "엄마도 자세가 안 좋아서 그래",
          "original_form": "엄마도 자세가 안 좋아서 그래",
          "speaker_id": "1",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028747.1.7",
          "form": "자세 잘 할려고 한다",
          "original_form": "자세 잘 할려고 한다",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028747.1.8",
          "form": "그래 좀 반듯이 있어",
          "original_form": "그래 좀 반듯이 있어",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028747.1.9",
          "form": "어깨 쫙 펴고",
          "original_form": "어깨 쫙 펴고",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028747.1.10",
          "form": "너나 잘해라",
          "original_form": "너나 잘해라",
          "speaker_id": "2",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028747.1.11",
          "form": "나는 틈틈히 스트레칭도",
          "original_form": "나는 틈틈히 스트레칭도",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028747.1.12",
          "form": "한다고",
          "original_form": "한다고",
          "speaker_id": "1",
          "time": "20210902 21:48"
        },
        {
          "id": "MDRW2100028747.1.13",
          "form": "낼 또 주물러줘",
          "original_form": "낼 또 주물러줘",
          "speaker_id": "2",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028747.1.14",
          "form": "삼일 주무르면 치킨ㄱ?",
          "original_form": "삼일 주무르면 치킨ㄱ?",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028747.1.15",
          "form": "치킨이 그렇게 먹고싶냐",
          "original_form": "치킨이 그렇게 먹고싶냐",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028747.1.16",
          "form": "아 치킨 사준다며",
          "original_form": "아 치킨 사준다며",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028747.1.17",
          "form": "그래 열심히 주물러 주면 사줄께",
          "original_form": "그래 열심히 주물러 주면 사줄께",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028747.1.18",
          "form": "헐ㅠㅜ 치킨...ㅂㄷㅂㄷ",
          "original_form": "헐ㅠㅜ 치킨...ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028747.1.19",
          "form": "대신 나 먹고 싶은 걸로 시켜죠",
