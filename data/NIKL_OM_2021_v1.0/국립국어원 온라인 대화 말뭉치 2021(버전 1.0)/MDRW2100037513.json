      "utterance": [
        {
          "id": "MDRW2100037513.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 02:22"
        },
        {
          "id": "MDRW2100037513.1.2",
          "form": "키링 마음에드세여?",
          "original_form": "키링 마음에드세여?",
          "speaker_id": "1",
          "time": "20210901 02:30"
        },
        {
          "id": "MDRW2100037513.1.3",
          "form": "뭐 그럭저럭이요",
          "original_form": "뭐 그럭저럭이요",
          "speaker_id": "2",
          "time": "20210901 02:32"
        },
        {
          "id": "MDRW2100037513.1.4",
          "form": "개인적으로는 인형만 달고다닐꺼 같아요",
          "original_form": "개인적으로는 인형만 달고다닐꺼 같아요",
          "speaker_id": "2",
          "time": "20210901 02:32"
        },
        {
          "id": "MDRW2100037513.1.5",
          "form": "오 그러시구나.. 가족들도 다 좋아해요?",
          "original_form": "오 그러시구나.. 가족들도 다 좋아해요?",
          "speaker_id": "1",
          "time": "20210901 02:33"
        },
        {
          "id": "MDRW2100037513.1.6",
          "form": "동생은 좋아하구요",
          "original_form": "동생은 좋아하구요",
          "speaker_id": "2",
          "time": "20210901 02:33"
        },
        {
          "id": "MDRW2100037513.1.7",
          "form": "엄마는 인형만 좋고 스트랩은 별로래요",
          "original_form": "엄마는 인형만 좋고 스트랩은 별로래요",
          "speaker_id": "2",
          "time": "20210901 02:33"
        },
        {
          "id": "MDRW2100037513.1.8",
          "form": "오..글쿤요.. 그건 아빠나 남자혈육드리던가 어딘가에쓰면되겟죠",
          "original_form": "오..글쿤요.. 그건 아빠나 남자혈육드리던가 어딘가에쓰면되겟죠",
          "speaker_id": "1",
          "time": "20210901 02:34"
        },
        {
          "id": "MDRW2100037513.1.9",
          "form": "그렇죠ㅎㅎ",
          "original_form": "그렇죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:34"
        },
        {
          "id": "MDRW2100037513.1.10",
          "form": "내일아침에도 뭐 할것같은데 기대되나요?",
          "original_form": "내일아침에도 뭐 할것같은데 기대되나요?",
          "speaker_id": "1",
          "time": "20210901 02:34"
        },
        {
          "id": "MDRW2100037513.1.11",
          "form": "네 기대되네요",
          "original_form": "네 기대되네요",
          "speaker_id": "2",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100037513.1.12",
          "form": "기대가 크면 실망도 크겠지만요",
          "original_form": "기대가 크면 실망도 크겠지만요",
          "speaker_id": "2",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100037513.1.13",
          "form": "아하 그러면 일단 내일보고 알려드릴게여",
          "original_form": "아하 그러면 일단 내일보고 알려드릴게여",
          "speaker_id": "1",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100037513.1.14",
          "form": "네네 감사해요",
          "original_form": "네네 감사해요",
          "speaker_id": "2",
          "time": "20210901 02:40"
        },
        {
          "id": "MDRW2100037513.1.15",
          "form": "상품 예상가는거 있으세요?",
          "original_form": "상품 예상가는거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 02:41"
        },
        {
          "id": "MDRW2100037513.1.16",
          "form": "특별한지 몰랏오요",
          "original_form": "특별한지 몰랏오요",
          "speaker_id": "1",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100037513.1.17",
          "form": "일단 봐야겟죠",
          "original_form": "일단 봐야겟죠",
          "speaker_id": "1",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100037513.1.18",
          "form": "틀별안지 어떤지",
          "original_form": "틀별안지 어떤지",
          "speaker_id": "1",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100037513.1.19",
          "form": "그쵸 일단 지켜봐야겠죠",
          "original_form": "그쵸 일단 지켜봐야겠죠",
