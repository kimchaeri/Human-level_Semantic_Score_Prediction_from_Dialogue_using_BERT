      "utterance": [
        {
          "id": "MDRW2100020178.1.1",
          "form": "아뇨 날것을 못먹어요ㅠ",
          "original_form": "아뇨 날것을 못먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020178.1.2",
          "form": "저도그렇습니다",
          "original_form": "저도그렇습니다",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020178.1.3",
          "form": "그럼 날것에 대한 것은 넘어갑시다",
          "original_form": "그럼 날것에 대한 것은 넘어갑시다",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020178.1.4",
          "form": "카페가면 뭐 마셔요",
          "original_form": "카페가면 뭐 마셔요",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020178.1.5",
          "form": "저 아아 또는 라떼",
          "original_form": "저 아아 또는 라떼",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020178.1.6",
          "form": "저는 라떼",
          "original_form": "저는 라떼",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020178.1.7",
          "form": "약간 단거좋아해서",
          "original_form": "약간 단거좋아해서",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020178.1.8",
          "form": "사럽 추가하시나요?",
          "original_form": "사럽 추가하시나요?",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020178.1.9",
          "form": "시럽",
          "original_form": "시럽",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020178.1.10",
          "form": "헤이즐넛라떼 맛잇어요",
          "original_form": "헤이즐넛라떼 맛잇어요",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020178.1.11",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020178.1.12",
          "form": "헤이즐넛 어디가 맛있어쇼",
          "original_form": "헤이즐넛 어디가 맛있어쇼",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020178.1.13",
          "form": "추천 카페 있으신가요",
          "original_form": "추천 카페 있으신가요",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020178.1.14",
          "form": "커피빈이요",
          "original_form": "커피빈이요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020178.1.15",
          "form": "한번 드셔보세요",
          "original_form": "한번 드셔보세요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020178.1.16",
          "form": "헤이즐넛 아메리카노도 맛잇어요",
          "original_form": "헤이즐넛 아메리카노도 맛잇어요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020178.1.17",
          "form": "커피빈 맛있다는 이야기 많이 듣긴했는데",
          "original_form": "커피빈 맛있다는 이야기 많이 듣긴했는데",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020178.1.18",
          "form": "꼭 한번 가서 먹어볼게요",
          "original_form": "꼭 한번 가서 먹어볼게요",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020178.1.19",
          "form": "디져트는 뭐 좋아하시나요",
          "original_form": "디져트는 뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020178.1.20",
          "form": "투썸 스초생 좋아해요 ㅎㅎ",
          "original_form": "투썸 스초생 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020178.1.21",
          "form": "전 아이스박스",
          "original_form": "전 아이스박스",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020178.1.22",
          "form": "투썸 케이크가 은근 맛있더라구요",
          "original_form": "투썸 케이크가 은근 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020178.1.23",
          "form": "맞아요 투썸 맛잇어요",
          "original_form": "맞아요 투썸 맛잇어요",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.24",
          "form": "노티드도넛",
          "original_form": "노티드도넛",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.25",
          "form": "드셔보셧나요?",
          "original_form": "드셔보셧나요?",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.26",
          "form": "헉 아직 안먹어봤어요",
          "original_form": "헉 아직 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.27",
          "form": "어떤가요",
          "original_form": "어떤가요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.28",
          "form": "아 노티드 그건가",
          "original_form": "아 노티드 그건가",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.29",
          "form": "먹어본거같은데",
          "original_form": "먹어본거같은데",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020178.1.30",
          "form": "겉에 흰가루 뭍어있고 안에 크림 잔뜩들은거",
          "original_form": "겉에 흰가루 뭍어있고 안에 크림 잔뜩들은거",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020178.1.31",
          "form": "그 스마일표시 도넛이요",
          "original_form": "그 스마일표시 도넛이요",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020178.1.32",
          "form": "먹어본거같아요",
          "original_form": "먹어본거같아요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        }
      ]
    }
  ]
