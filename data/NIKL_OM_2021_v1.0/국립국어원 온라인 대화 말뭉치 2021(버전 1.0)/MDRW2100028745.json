      "utterance": [
        {
          "id": "MDRW2100028745.1.1",
          "form": "요리 느실거에요",
          "original_form": "요리 느실거에요",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028745.1.2",
          "form": "이상형 어떻게되세요",
          "original_form": "이상형 어떻게되세요",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028745.1.3",
          "form": "연애중이신가용",
          "original_form": "연애중이신가용",
          "speaker_id": "1",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100028745.1.4",
          "form": "아니요..ㅎ",
          "original_form": "아니요..ㅎ",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100028745.1.5",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100028745.1.6",
          "form": "네네 근데 원거히에요",
          "original_form": "네네 근데 원거히에요",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100028745.1.7",
          "form": "해외 ㅡ와 한국 ㅠㅠ",
          "original_form": "해외 ㅡ와 한국 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100028745.1.8",
          "form": "없는거랑 다룸없어요ㅠㅠㅋㅋ",
          "original_form": "없는거랑 다룸없어요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100028745.1.9",
          "form": "와 엄청난 롱디시네요,,",
          "original_form": "와 엄청난 롱디시네요,,",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100028745.1.10",
          "form": "엄청 보고싶겠어요 ㅠㅠ",
          "original_form": "엄청 보고싶겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100028745.1.11",
          "form": "맞아요ㅠ 유럽 이랍니다ㅠㅠ",
          "original_form": "맞아요ㅠ 유럽 이랍니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100028745.1.12",
          "form": "ㅠㅠㅠㅠ어떡해요",
          "original_form": "ㅠㅠㅠㅠ어떡해요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100028745.1.13",
          "form": "그럼 화상통화 하세요?",
          "original_form": "그럼 화상통화 하세요?",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100028745.1.14",
          "form": "네네 매일 페이스톡 하는데",
          "original_form": "네네 매일 페이스톡 하는데",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100028745.1.15",
          "form": "귀찮아요 이제는 ㅠㅠ",
          "original_form": "귀찮아요 이제는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100028745.1.16",
          "form": "아..ㅠㅠ 실물로 보는게 좋죠ㅠㅠ",
          "original_form": "아..ㅠㅠ 실물로 보는게 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100028745.1.17",
          "form": "지치시겠어요 진짜ㅠㅠㅠ",
          "original_form": "지치시겠어요 진짜ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100028745.1.18",
          "form": "어떻게 만나게 되신거예요?",
          "original_form": "어떻게 만나게 되신거예요?",
          "speaker_id": "2",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100028745.1.19",
          "form": "저희는 고3때 학원에서 알게됐고",
          "original_form": "저희는 고3때 학원에서 알게됐고",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100028745.1.20",
          "form": "대학교때 친구다가 졸업때쯤 사귀게 됐어요 ㅋㅋㅋ",
          "original_form": "대학교때 친구다가 졸업때쯤 사귀게 됐어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100028745.1.21",
          "form": "우와 그럼 유럽으로 공부하러 가셨어요?",
          "original_form": "우와 그럼 유럽으로 공부하러 가셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100028745.1.22",
          "form": "그분은..?",
          "original_form": "그분은..?",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100028745.1.23",
          "form": "네네맞아용",
          "original_form": "네네맞아용",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100028745.1.24",
          "form": "총 3년이고 이제 1년 4개월 남았어요 ㅠㅠㅠ",
          "original_form": "총 3년이고 이제 1년 4개월 남았어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100028745.1.25",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100028745.1.26",
          "form": "험난하네요..,,",
          "original_form": "험난하네요..,,",
          "speaker_id": "2",
