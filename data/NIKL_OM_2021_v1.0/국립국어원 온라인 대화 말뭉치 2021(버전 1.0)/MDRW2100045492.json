      "utterance": [
        {
          "id": "MDRW2100045492.1.1",
          "form": "뿌리식물 아님 덩굴식물 아님 하얀색 많음",
          "original_form": "뿌리식물 아님 덩굴식물 아님 하얀색 많음",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045492.1.2",
          "form": "흐으으으음 잎사귀가 많나요?",
          "original_form": "흐으으으음 잎사귀가 많나요?",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045492.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045492.1.4",
          "form": "잎사귀라고 생각할만한 부분은 많지 않아요",
          "original_form": "잎사귀라고 생각할만한 부분은 많지 않아요",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045492.1.5",
          "form": "동글동글한가요?",
          "original_form": "동글동글한가요?",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045492.1.6",
          "form": "일부는요!",
          "original_form": "일부는요!",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045492.1.7",
          "form": "과일종류인가요??",
          "original_form": "과일종류인가요??",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045492.1.8",
          "form": "아뇨 과일은 아닙니다!",
          "original_form": "아뇨 과일은 아닙니다!",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045492.1.9",
          "form": "매운 맛이 나나욧",
          "original_form": "매운 맛이 나나욧",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045492.1.10",
          "form": "아뇨 맵지 않습니다!",
          "original_form": "아뇨 맵지 않습니다!",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045492.1.11",
          "form": "달달한 맛이 나나요?",
          "original_form": "달달한 맛이 나나요?",
          "speaker_id": "1",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045492.1.12",
          "form": "아뇨 단맛 안나요!",
          "original_form": "아뇨 단맛 안나요!",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045492.1.13",
          "form": "국내 재배가 되나요?",
          "original_form": "국내 재배가 되나요?",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045492.1.14",
          "form": "넵 당연히 됩니다!",
          "original_form": "넵 당연히 됩니다!",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045492.1.15",
          "form": "마트에서 흔히 파는 건가요?",
          "original_form": "마트에서 흔히 파는 건가요?",
          "speaker_id": "1",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045492.1.16",
          "form": "넵! 흔하게 팝니다",
          "original_form": "넵! 흔하게 팝니다",
          "speaker_id": "2",
          "time": "20210908 03:00"
        }
