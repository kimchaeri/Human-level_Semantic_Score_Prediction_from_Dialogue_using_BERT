      "utterance": [
        {
          "id": "MDRW2100013249.1.1",
          "form": "코로나만 풀리면 어디 가고 싶으신가요?",
          "original_form": "코로나만 풀리면 어디 가고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100013249.1.2",
          "form": "저 캐나다요~ 유럽도",
          "original_form": "저 캐나다요~ 유럽도",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100013249.1.3",
          "form": "오 캐나다 시원할 것같아요",
          "original_form": "오 캐나다 시원할 것같아요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100013249.1.4",
          "form": "유럽은 어느 나라쪽 가보고 싶으세영?",
          "original_form": "유럽은 어느 나라쪽 가보고 싶으세영?",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100013249.1.5",
          "form": "서유럽이나 북유럽? 스위스가 좋다던데요",
          "original_form": "서유럽이나 북유럽? 스위스가 좋다던데요",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100013249.1.6",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100013249.1.7",
          "form": "스위스는 경치 구경하기 정말 좋을 것같아요",
          "original_form": "스위스는 경치 구경하기 정말 좋을 것같아요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100013249.1.8",
          "form": "깨끗하다는 느낌도 있구요",
          "original_form": "깨끗하다는 느낌도 있구요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100013249.1.9",
          "form": "어디가보고 싶으세요?",
          "original_form": "어디가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100013249.1.10",
          "form": "저는 남미쪽이요!",
          "original_form": "저는 남미쪽이요!",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100013249.1.11",
          "form": "브라질 이런 곳 가보고 싶어요",
          "original_form": "브라질 이런 곳 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100013249.1.12",
          "form": "가보고싶은데 위험할거같아요",
          "original_form": "가보고싶은데 위험할거같아요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100013249.1.13",
          "form": "우유니사막 가보곤싶더라구요",
          "original_form": "우유니사막 가보곤싶더라구요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100013249.1.14",
          "form": "오왕 거기 물에 비쳐서",
          "original_form": "오왕 거기 물에 비쳐서",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100013249.1.15",
          "form": "거의 평행이론....느낌나는",
          "original_form": "거의 평행이론....느낌나는",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100013249.1.16",
          "form": "네네  거울같이",
          "original_form": "네네  거울같이",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100013249.1.17",
          "form": "맞죠맞죠? 거기도 가보고 싶네요",
          "original_form": "맞죠맞죠? 거기도 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100013249.1.18",
          "form": "멋있더라구요",
          "original_form": "멋있더라구요",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100013249.1.19",
          "form": "네 맞아요ㅠ",
          "original_form": "네 맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:51"
        }
      ]
