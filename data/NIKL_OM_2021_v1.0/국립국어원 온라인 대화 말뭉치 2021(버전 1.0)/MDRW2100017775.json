      "utterance": [
        {
          "id": "MDRW2100017775.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100017775.1.2",
          "form": "지금은 안오는 것 같아요",
          "original_form": "지금은 안오는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100017775.1.3",
          "form": "아깐 비가 엄청 내리던데 말이에요",
          "original_form": "아깐 비가 엄청 내리던데 말이에요",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100017775.1.4",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100017775.1.5",
          "form": "그대신 습한게 오래가네요",
          "original_form": "그대신 습한게 오래가네요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100017775.1.6",
          "form": "제습기 돌리고 있어요",
          "original_form": "제습기 돌리고 있어요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100017775.1.7",
          "form": "그래야겠어오 저도",
          "original_form": "그래야겠어오 저도",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100017775.1.8",
          "form": "이제 가을인데 오히려 지금이 더 여름같네요",
          "original_form": "이제 가을인데 오히려 지금이 더 여름같네요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100017775.1.9",
          "form": "덥지 않아서 다행이긴 하지만요",
          "original_form": "덥지 않아서 다행이긴 하지만요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100017775.1.10",
          "form": "추워요",
          "original_form": "추워요",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100017775.1.11",
          "form": "추운 정도인가요?",
          "original_form": "추운 정도인가요?",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100017775.1.12",
          "form": "새벽에 창문 열고 자면요",
          "original_form": "새벽에 창문 열고 자면요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100017775.1.13",
          "form": "새벽에",
          "original_form": "새벽에",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100017775.1.14",
          "form": "아 그렇소",
          "original_form": "아 그렇소",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100017775.1.15",
          "form": "죠",
          "original_form": "죠",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100017775.1.16",
          "form": "새벽에는 기온이 많이 떨어지더라구요",
          "original_form": "새벽에는 기온이 많이 떨어지더라구요",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100017775.1.17",
          "form": "저도 그래서 창문을 조금만 열고 자요",
          "original_form": "저도 그래서 창문을 조금만 열고 자요",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017775.1.18",
          "form": "창문닫거 자야 해여",
          "original_form": "창문닫거 자야 해여",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017775.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
