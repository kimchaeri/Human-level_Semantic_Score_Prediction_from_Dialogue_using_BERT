          "speaker_id": "1",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025471.1.4",
          "form": "아마 못할거같아요ㅋㅋ",
          "original_form": "아마 못할거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025471.1.5",
          "form": "비혼이라서요?",
          "original_form": "비혼이라서요?",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025471.1.6",
          "form": "지금이 편해서 생각이 없네요",
          "original_form": "지금이 편해서 생각이 없네요",
          "speaker_id": "2",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025471.1.7",
          "form": "그럼 된거죠",
          "original_form": "그럼 된거죠",
          "speaker_id": "1",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100025471.1.8",
          "form": "기혼이신가요",
          "original_form": "기혼이신가요",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
