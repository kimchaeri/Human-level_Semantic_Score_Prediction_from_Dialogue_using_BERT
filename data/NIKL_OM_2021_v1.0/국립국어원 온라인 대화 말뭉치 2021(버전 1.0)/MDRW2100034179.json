      "utterance": [
        {
          "id": "MDRW2100034179.1.1",
          "form": "이제 좀있음 고구마의 계절이네요",
          "original_form": "이제 좀있음 고구마의 계절이네요",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100034179.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100034179.1.3",
          "form": "전 밤고구마파요",
          "original_form": "전 밤고구마파요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100034179.1.4",
          "form": "전 호박고구마파요 더 단 느낌이라서요",
          "original_form": "전 호박고구마파요 더 단 느낌이라서요",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100034179.1.5",
          "form": "오 요새 많이들 호박파예요",
          "original_form": "오 요새 많이들 호박파예요",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034179.1.6",
          "form": "전 오렸을때부터 밤요 ㅋㅋ",
          "original_form": "전 오렸을때부터 밤요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034179.1.7",
          "form": "호박이 나중에 유행한 품종이라 밤고구마가 더 전통이있죠",
          "original_form": "호박이 나중에 유행한 품종이라 밤고구마가 더 전통이있죠",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034179.1.8",
          "form": "그렇죠 호",
          "original_form": "그렇죠 호",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034179.1.9",
          "form": "전 쭉 같아요",
          "original_form": "전 쭉 같아요",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034179.1.10",
          "form": "포도 드셨어요?",
          "original_form": "포도 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034179.1.11",
          "form": "올해 좀 먹었죠 자주는 아니구요",
          "original_form": "올해 좀 먹었죠 자주는 아니구요",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034179.1.12",
          "form": "씨를 뱉기 귀찮아서요",
          "original_form": "씨를 뱉기 귀찮아서요",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034179.1.13",
          "form": "네 전 씨먹어요",
          "original_form": "네 전 씨먹어요",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034179.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034179.1.15",
          "form": "다들 먹는법도 다르죠 ㅎㅎ",
          "original_form": "다들 먹는법도 다르죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034179.1.16",
          "form": "아 전 씨를 뱉는 파라 이게 먹다보면 피곤해요",
          "original_form": "아 전 씨를 뱉는 파라 이게 먹다보면 피곤해요",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034179.1.17",
          "form": "검정 캠벨포도 철이라 낼사오게요",
          "original_form": "검정 캠벨포도 철이라 낼사오게요",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.18",
          "form": "반송이 먹는데 한시간 걸려요",
          "original_form": "반송이 먹는데 한시간 걸려요",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.20",
          "form": "시간이 두배 ㅎㅎ",
          "original_form": "시간이 두배 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.21",
          "form": "요즘 포도가 달아요 저도 좋아하는데,,, 먹기가 귀찮아서 좀 덜먹어요 ㅋㅋㅋ",
          "original_form": "요즘 포도가 달아요 저도 좋아하는데,,, 먹기가 귀찮아서 좀 덜먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.22",
          "form": "귀찮음 덜먹죠",
          "original_form": "귀찮음 덜먹죠",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.23",
          "form": "꽃게도 남편이 귀찮다고 안먹어요",
          "original_form": "꽃게도 남편이 귀찮다고 안먹어요",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.24",
          "form": "덩달아 저도 못먹네요",
          "original_form": "덩달아 저도 못먹네요",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034179.1.25",
          "form": "꽃게 맛난데 ㅋㅋ",
          "original_form": "꽃게 맛난데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034179.1.26",
          "form": "꽃게 좋아 하세요?",
          "original_form": "꽃게 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034179.1.27",
          "form": "아 껍질가기가 귀찮죠 그치만 다리엔 어차피 살이 없어서",
          "original_form": "아 껍질가기가 귀찮죠 그치만 다리엔 어차피 살이 없어서",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034179.1.28",
          "form": "씹어서 쭉  발라 먹음 되는데",
          "original_form": "씹어서 쭉  발라 먹음 되는데",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034179.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034179.1.30",
          "form": "것도 귀찮나봐요 ㅋㅋ",
          "original_form": "것도 귀찮나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:31"
        }
      ]
    }
