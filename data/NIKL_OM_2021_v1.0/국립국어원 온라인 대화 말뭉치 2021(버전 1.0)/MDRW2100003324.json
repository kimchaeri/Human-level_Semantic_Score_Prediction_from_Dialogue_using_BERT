      "utterance": [
        {
          "id": "MDRW2100003324.1.1",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003324.1.2",
          "form": "딱한번 가봤어요! 대만이요~\r\n지인님은요?",
          "original_form": "딱한번 가봤어요! 대만이요~\r\n지인님은요?",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003324.1.3",
          "form": "저는 아직 안가봤는데",
          "original_form": "저는 아직 안가봤는데",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003324.1.4",
          "form": "1~2년후에 일본 가보고 싶네요",
          "original_form": "1~2년후에 일본 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003324.1.5",
          "form": "코로나가 빨리 끝나야 해외여행도 갈수 있을텐데요",
          "original_form": "코로나가 빨리 끝나야 해외여행도 갈수 있을텐데요",
          "speaker_id": "2",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003324.1.6",
          "form": "코로나 끝나면 여행계획 있으세요?",
          "original_form": "코로나 끝나면 여행계획 있으세요?",
          "speaker_id": "1",
          "time": "20210820 21:56"
        },
        {
          "id": "MDRW2100003324.1.7",
          "form": "캠핑을 좋아해서 캠핑을 더 자주 다닐것 같아요!",
          "original_form": "캠핑을 좋아해서 캠핑을 더 자주 다닐것 같아요!",
          "speaker_id": "2",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003324.1.8",
          "form": "오 캠핑 좋아하세요?",
          "original_form": "오 캠핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003324.1.9",
          "form": "저도 캠핑같은거 가서 밤에 별보고 싶네요",
          "original_form": "저도 캠핑같은거 가서 밤에 별보고 싶네요",
          "speaker_id": "1",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003324.1.10",
          "form": "요즘은 갬성 캠핑이라고 해서 다들 많이 다니시더라구요",
          "original_form": "요즘은 갬성 캠핑이라고 해서 다들 많이 다니시더라구요",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003324.1.11",
          "form": "근데 캠핑은 우리나라가 좁다보니까 사람 많지않아요?",
          "original_form": "근데 캠핑은 우리나라가 좁다보니까 사람 많지않아요?",
          "speaker_id": "1",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003324.1.12",
          "form": "코로나 때문에 캠핑자리도 한군데씩 건너서 예약잡아주더라구요",
          "original_form": "코로나 때문에 캠핑자리도 한군데씩 건너서 예약잡아주더라구요",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003324.1.13",
          "form": "저는 어디서 본것처럼 숲같은데서 혼자 불피우고 그런게 좋은거같아요",
          "original_form": "저는 어디서 본것처럼 숲같은데서 혼자 불피우고 그런게 좋은거같아요",
          "speaker_id": "1",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003324.1.14",
          "form": "은하수 실제로 한번 보고싶네요",
          "original_form": "은하수 실제로 한번 보고싶네요",
          "speaker_id": "1",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003324.1.15",
          "form": "불멍이라고 하죠ㅋㅋ\r\n은하수는 저도 본적없는거 같은데 산골에 가면 볼수 있나요?",
          "original_form": "불멍이라고 하죠ㅋㅋ\r\n은하수는 저도 본적없는거 같은데 산골에 가면 볼수 있나요?",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003324.1.16",
          "form": "주변에 빛없는곳이 잘보인다던데",
          "original_form": "주변에 빛없는곳이 잘보인다던데",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003324.1.17",
          "form": "산쪽이면 아무래도 잘보이지 않을까요?",
          "original_form": "산쪽이면 아무래도 잘보이지 않을까요?",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003324.1.18",
          "form": "아! 나중에 한번 자세히 봐야겠네요",
          "original_form": "아! 나중에 한번 자세히 봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 22:14"
        }
      ]
