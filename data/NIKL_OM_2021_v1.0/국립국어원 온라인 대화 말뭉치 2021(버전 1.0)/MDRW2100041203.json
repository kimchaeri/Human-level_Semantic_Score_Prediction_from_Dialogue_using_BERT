      "utterance": [
        {
          "id": "MDRW2100041203.1.1",
          "form": "가을에 어디로 나들이 하고 싶으세요?",
          "original_form": "가을에 어디로 나들이 하고 싶으세요?",
          "speaker_id": "1",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100041203.1.2",
          "form": "저는 놀이동산 가고싶어요 안간지 너무 오래됐네요",
          "original_form": "저는 놀이동산 가고싶어요 안간지 너무 오래됐네요",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100041203.1.3",
          "form": "에버랜드요?",
          "original_form": "에버랜드요?",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100041203.1.4",
          "form": "어디든 좋을거같아요! 놀이기구만 있으면",
          "original_form": "어디든 좋을거같아요! 놀이기구만 있으면",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100041203.1.5",
          "form": "근데 에버랜드 진짜 좋겠네요 동물들도 보고",
          "original_form": "근데 에버랜드 진짜 좋겠네요 동물들도 보고",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100041203.1.6",
          "form": "네 지금 날씨엔 다니기도 딱 좋고",
          "original_form": "네 지금 날씨엔 다니기도 딱 좋고",
          "speaker_id": "1",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100041203.1.7",
          "form": "에버랜드 조경도 잘 해놨을텐데",
          "original_form": "에버랜드 조경도 잘 해놨을텐데",
          "speaker_id": "1",
          "time": "20210905 20:10"
        },
        {
          "id": "MDRW2100041203.1.8",
          "form": "이야기하는새에 에버랜드 가고싶어졌네요 ㅋㅋ",
          "original_form": "이야기하는새에 에버랜드 가고싶어졌네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100041203.1.9",
          "form": "여행 가고싶은곳 있으신가요",
          "original_form": "여행 가고싶은곳 있으신가요",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100041203.1.10",
          "form": "ㅎㅎ에버랜드와 가까운 곳에 사시나요?",
          "original_form": "ㅎㅎ에버랜드와 가까운 곳에 사시나요?",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100041203.1.11",
          "form": "저는 어디든 나들이하면 좋을 것 같아요",
          "original_form": "저는 어디든 나들이하면 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100041203.1.12",
          "form": "이번 여름 휴가도 집콕하고",
          "original_form": "이번 여름 휴가도 집콕하고",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100041203.1.13",
          "form": "주말에도 백신 접종해서 집콕했더니",
          "original_form": "주말에도 백신 접종해서 집콕했더니",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100041203.1.14",
          "form": "나가고 싶네요ㅜ",
          "original_form": "나가고 싶네요ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100041203.1.15",
          "form": "저도 코로나 심해진 뒤로 계속 집콕이네요ㅜㅜ",
          "original_form": "저도 코로나 심해진 뒤로 계속 집콕이네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 20:13"
        },
        {
          "id": "MDRW2100041203.1.16",
          "form": "나들이하기 좋은 날씨라",
          "original_form": "나들이하기 좋은 날씨라",
          "speaker_id": "1",
          "time": "20210905 20:13"
        },
        {
          "id": "MDRW2100041203.1.17",
          "form": "더 아쉬워요",
          "original_form": "더 아쉬워요",
          "speaker_id": "1",
          "time": "20210905 20:13"
        },
        {
          "id": "MDRW2100041203.1.18",
          "form": "그러게요 그래도 내일은 부산갈일 있어 콧바람이라도 넣으려구요",
          "original_form": "그러게요 그래도 내일은 부산갈일 있어 콧바람이라도 넣으려구요",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041203.1.19",
          "form": "간김에 드라이브",
          "original_form": "간김에 드라이브",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041203.1.20",
          "form": "와 부산이라니",
          "original_form": "와 부산이라니",
          "speaker_id": "1",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041203.1.21",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041203.1.22",
          "form": "간 김에 여행도 좀 하시나요?",
          "original_form": "간 김에 여행도 좀 하시나요?",
          "speaker_id": "1",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100041203.1.23",
          "form": "부산 가까워서 여행까진 아니고 해변가 드라이브나 하고올까싶어요 ㅎㅎ",
          "original_form": "부산 가까워서 여행까진 아니고 해변가 드라이브나 하고올까싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100041203.1.24",
          "form": "아 부산 가까이 사시는구나",
          "original_form": "아 부산 가까이 사시는구나",
          "speaker_id": "1",
          "time": "20210905 20:15"
        }
      ]
    }
