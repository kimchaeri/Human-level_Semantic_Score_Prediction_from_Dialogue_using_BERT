      "utterance": [
        {
          "id": "MDRW2100005465.1.1",
          "form": "당근마켓 자주 써?",
          "original_form": "당근마켓 자주 써?",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100005465.1.2",
          "form": "아직 올린 적은 없고",
          "original_form": "아직 올린 적은 없고",
          "speaker_id": "2",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100005465.1.3",
          "form": "키워드 알림은 설정해뒀어",
          "original_form": "키워드 알림은 설정해뒀어",
          "speaker_id": "2",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100005465.1.4",
          "form": "너는 자주 쓰던 거 같던데, 어땠어?",
          "original_form": "너는 자주 쓰던 거 같던데, 어땠어?",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100005465.1.5",
          "form": "나는 완전 많이 쓰지",
          "original_form": "나는 완전 많이 쓰지",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100005465.1.6",
          "form": "인터넷으로 거래하면 배송포장 해야되는데",
          "original_form": "인터넷으로 거래하면 배송포장 해야되는데",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100005465.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005465.1.8",
          "form": "이건 그런 게 없어서 좋아",
          "original_form": "이건 그런 게 없어서 좋아",
          "speaker_id": "1",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005465.1.9",
          "form": "최근에 거래한 물건 있어?",
          "original_form": "최근에 거래한 물건 있어?",
          "speaker_id": "2",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005465.1.10",
          "form": "나 인피니트 굿즈...ㅋㅋ",
          "original_form": "나 인피니트 굿즈...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100005465.1.11",
          "form": "인피니트 굿즈 좋지",
          "original_form": "인피니트 굿즈 좋지",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005465.1.12",
          "form": "주로 어떤 키워드를 설정해?",
          "original_form": "주로 어떤 키워드를 설정해?",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005465.1.13",
          "form": "키워드는 설정 안 하고",
          "original_form": "키워드는 설정 안 하고",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005465.1.14",
          "form": "내가 팔고 싶은 것만 팔아",
          "original_form": "내가 팔고 싶은 것만 팔아",
          "speaker_id": "1",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100005465.1.15",
          "form": "오호 좋다",
          "original_form": "오호 좋다",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100005465.1.16",
          "form": "나는 키워드 설정하고 올라오는 거 보는 중",
          "original_form": "나는 키워드 설정하고 올라오는 거 보는 중",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100005465.1.17",
          "form": "아직 사거나 판 적은 없어",
          "original_form": "아직 사거나 판 적은 없어",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100005465.1.18",
          "form": "무슨 설정해놨어?",
          "original_form": "무슨 설정해놨어?",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100005465.1.19",
          "form": "카메라(회사별로), 이북리더기",
          "original_form": "카메라(회사별로), 이북리더기",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100005465.1.20",
          "form": "이렇게 설정했어",
          "original_form": "이렇게 설정했어",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100005465.1.21",
          "form": "다 비싼거네",
          "original_form": "다 비싼거네",
          "speaker_id": "1",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100005465.1.22",
          "form": "카메라는 그래도..",
          "original_form": "카메라는 그래도..",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100005465.1.23",
          "form": "원가보다 싸게 올라오는 편",
          "original_form": "원가보다 싸게 올라오는 편",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100005465.1.24",
          "form": "원가가 대부분 100만원 이상인데",
          "original_form": "원가가 대부분 100만원 이상인데",
          "speaker_id": "2",
          "time": "20210822 20:44"
        }
      ]
