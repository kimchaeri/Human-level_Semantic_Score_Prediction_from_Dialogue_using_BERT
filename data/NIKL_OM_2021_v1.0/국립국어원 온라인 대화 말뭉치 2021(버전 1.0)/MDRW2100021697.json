        {
          "id": "MDRW2100021697.1.1",
          "form": "옛날에 야구 몇번 직관해보긴했어요!",
          "original_form": "옛날에 야구 몇번 직관해보긴했어요!",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021697.1.2",
          "form": "오 어디구단이요?",
          "original_form": "오 어디구단이요?",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021697.1.3",
          "form": "서울이랑 어디였지..",
          "original_form": "서울이랑 어디였지..",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021697.1.4",
          "form": "이승엽 현역시절때 간거라 가물하네요",
          "original_form": "이승엽 현역시절때 간거라 가물하네요",
          "speaker_id": "2",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021697.1.5",
          "form": "악 엄청 옛날이시군요",
          "original_form": "악 엄청 옛날이시군요",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021697.1.6",
          "form": "네 그이후론 직관을 잘 안해서 ㅎㅎ",
          "original_form": "네 그이후론 직관을 잘 안해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021697.1.7",
          "form": "요즘 그리고 보는게 해외쪽인지라",
          "original_form": "요즘 그리고 보는게 해외쪽인지라",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021697.1.8",
          "form": "직관도 힘들구요 ㅠ",
          "original_form": "직관도 힘들구요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021697.1.9",
          "form": "저는 호날두 내한 직관.. 급빡치네요ㅋㅋㅋ",
          "original_form": "저는 호날두 내한 직관.. 급빡치네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021697.1.10",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021697.1.11",
          "form": "날아간 10만원~ ㅠㅠ",
          "original_form": "날아간 10만원~ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021697.1.12",
          "form": "ㅜㅠ... 안타깝네요",
          "original_form": "ㅜㅠ... 안타깝네요",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021697.1.13",
          "form": "해축보시는건가요",
          "original_form": "해축보시는건가요",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021697.1.14",
          "form": "넹 해축보고 있어요",
          "original_form": "넹 해축보고 있어요",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021697.1.15",
          "form": "EPL이랑 라리가 주로봐요",
          "original_form": "EPL이랑 라리가 주로봐요",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021697.1.16",
          "form": "오역시 축구좋아하시는군요",
          "original_form": "오역시 축구좋아하시는군요",
