      "utterance": [
        {
          "id": "MDRW2100033024.1.1",
          "form": "제주 이야기 금지",
          "original_form": "제주 이야기 금지",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033024.1.2",
          "form": "우왕 인도에서 뭐 하셨어요",
          "original_form": "우왕 인도에서 뭐 하셨어요",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033024.1.3",
          "form": "투머치했어요",
          "original_form": "투머치했어요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033024.1.4",
          "form": "인도궁금해여",
          "original_form": "인도궁금해여",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033024.1.5",
          "form": "한 번도 안가 봤거든요",
          "original_form": "한 번도 안가 봤거든요",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033024.1.6",
          "form": "대기업주제원 아파트 관리여",
          "original_form": "대기업주제원 아파트 관리여",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100033024.1.7",
          "form": "인도 한번은 가봐야져",
          "original_form": "인도 한번은 가봐야져",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.8",
          "form": "아파트 관리 엄청 특이하네요",
          "original_form": "아파트 관리 엄청 특이하네요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.9",
          "form": "코로나 때문에 돌아오신 건가요",
          "original_form": "코로나 때문에 돌아오신 건가요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.10",
          "form": "고오급 게스트하우스",
          "original_form": "고오급 게스트하우스",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.12",
          "form": "그러게요 인도도 난리였죠 베트남처럼",
          "original_form": "그러게요 인도도 난리였죠 베트남처럼",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.13",
          "form": "저는 베트남에서 쭉 살줄 알았거든요",
          "original_form": "저는 베트남에서 쭉 살줄 알았거든요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100033024.1.14",
          "form": "네 직전에 구호단체 일헸는데",
          "original_form": "네 직전에 구호단체 일헸는데",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033024.1.15",
          "form": "그러나 아니었다면 계속 있었을 거예요",
          "original_form": "그러나 아니었다면 계속 있었을 거예요",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033024.1.16",
          "form": "그만두고 간곳이",
          "original_form": "그만두고 간곳이",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033024.1.17",
          "form": "우와 멋있어요",
          "original_form": "우와 멋있어요",
          "speaker_id": "1",
          "time": "20210906 14:06"
        },
        {
          "id": "MDRW2100033024.1.18",
          "form": "재난현장이 될줄이야",
          "original_form": "재난현장이 될줄이야",
          "speaker_id": "2",
          "time": "20210906 14:06"
        },
