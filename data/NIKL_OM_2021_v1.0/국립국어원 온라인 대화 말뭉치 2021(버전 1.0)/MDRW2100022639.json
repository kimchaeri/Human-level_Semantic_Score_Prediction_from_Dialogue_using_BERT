      "utterance": [
        {
          "id": "MDRW2100022639.1.1",
          "form": "하남쭈꾸미 밀키트가 맛있어요?",
          "original_form": "하남쭈꾸미 밀키트가 맛있어요?",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022639.1.2",
          "form": "밀키트 맛있더라구요 !",
          "original_form": "밀키트 맛있더라구요 !",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022639.1.3",
          "form": "유명해서 매번 할인할때마다 품절이구",
          "original_form": "유명해서 매번 할인할때마다 품절이구",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022639.1.4",
          "form": "거기엔 쭈꾸미만 들어있는거에요?",
          "original_form": "거기엔 쭈꾸미만 들어있는거에요?",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022639.1.5",
          "form": "매콤항거 좋아하시면 좋아할거같아요",
          "original_form": "매콤항거 좋아하시면 좋아할거같아요",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022639.1.6",
          "form": "대패삼겹살 같은거 사서 같이 먹으면",
          "original_form": "대패삼겹살 같은거 사서 같이 먹으면",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022639.1.7",
          "form": "더 맛있겠다",
          "original_form": "더 맛있겠다",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.8",
          "form": "크 완전 먹잘알이시군요",
          "original_form": "크 완전 먹잘알이시군요",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.9",
          "form": "ㅋㅋㅋ그럼요 이게 바로 쭈삼 아니겠습니까",
          "original_form": "ㅋㅋㅋ그럼요 이게 바로 쭈삼 아니겠습니까",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.10",
          "form": "담에한번드셔보세요!",
          "original_form": "담에한번드셔보세요!",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.11",
          "form": "ㅋㅋ 얼마 정도 해용?",
          "original_form": "ㅋㅋ 얼마 정도 해용?",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.12",
          "form": "삼백그람도 팔아서 혼자먹기좋아요",
          "original_form": "삼백그람도 팔아서 혼자먹기좋아요",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.13",
          "form": "밀키트는 좀 비싼 느낌이에요 왠지",
          "original_form": "밀키트는 좀 비싼 느낌이에요 왠지",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.14",
          "form": "저는 삼백그람 세개에 만오천원정도 삿어요",
          "original_form": "저는 삼백그람 세개에 만오천원정도 삿어요",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.15",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.16",
          "form": "물론 할인한가격인듯해요",
          "original_form": "물론 할인한가격인듯해요",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.17",
          "form": "저는 냉동쭈꾸미 사서 먹어야겠어요 ㅜㅜ",
          "original_form": "저는 냉동쭈꾸미 사서 먹어야겠어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022639.1.18",
          "form": "갑자기 쭈삼 확 당기네",
          "original_form": "갑자기 쭈삼 확 당기네",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022639.1.19",
          "form": "매운건 잘 드세요?",
          "original_form": "매운건 잘 드세요?",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022639.1.20",
          "form": "ㅋㅋㅋㅋㅋ삼겹살 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ삼겹살 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022639.1.21",
          "form": "잘 먹지 못해서 안먹으려고 해요",
          "original_form": "잘 먹지 못해서 안먹으려고 해요",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022639.1.22",
          "form": "아아 그렇군요 저는 매운거 좋아해요 ㅋㅋㅋ",
          "original_form": "아아 그렇군요 저는 매운거 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:41"
        }
      ]
