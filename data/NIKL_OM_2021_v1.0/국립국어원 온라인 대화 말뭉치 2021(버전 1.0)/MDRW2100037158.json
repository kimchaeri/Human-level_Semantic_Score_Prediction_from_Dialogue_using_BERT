        },
        {
          "id": "MDRW2100037158.1.2",
          "form": "필링기요",
          "original_form": "필링기요",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100037158.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100037158.1.4",
          "form": "저는 오늘 베라 아이스크림을 샀어요ㅋㅋ",
          "original_form": "저는 오늘 베라 아이스크림을 샀어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037158.1.5",
          "form": "아 민초좋아파시군요",
          "original_form": "아 민초좋아파시군요",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037158.1.6",
          "form": "민초의 매력은 뭔가요?",
          "original_form": "민초의 매력은 뭔가요?",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037158.1.7",
          "form": "앗 대화 다른분과 헷갈렸어요ㅜ",
          "original_form": "앗 대화 다른분과 헷갈렸어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100037158.1.8",
          "form": "베라 맛있죠",
          "original_form": "베라 맛있죠",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100037158.1.9",
          "form": "ㅋㅋㅋㅋ네 오늘 31데이라 하프갤런 사왔네여ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ네 오늘 31데이라 하프갤런 사왔네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100037158.1.10",
          "form": "아 오늘 31일이었군요",
          "original_form": "아 오늘 31일이었군요",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100037158.1.11",
