          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015228.1.4",
          "form": "강아지키워요",
          "original_form": "강아지키워요",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015228.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015228.1.6",
          "form": "키워요",
          "original_form": "키워요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015228.1.7",
          "form": "아 저 뱀도 키워요",
          "original_form": "아 저 뱀도 키워요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015228.1.8",
          "form": "뭐 키우세요?",
          "original_form": "뭐 키우세요?",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015228.1.9",
          "form": "name3라는 이름의 콩이요",
          "original_form": "&name3&라는 이름의 콩이요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015228.1.10",
          "form": "콩을 키우신다고요?",
          "original_form": "콩을 키우신다고요?",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015228.1.11",
          "form": "콩은 동물이 아니잖아요.",
          "original_form": "콩은 동물이 아니잖아요.",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015228.1.12",
          "form": "그래도 제 반려식물이라",
          "original_form": "그래도 제 반려식물이라",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.13",
          "form": "말해봤어요",
          "original_form": "말해봤어요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.14",
          "form": "또 어디가셨죠?",
          "original_form": "또 어디가셨죠?",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.15",
          "form": "뱀은 잘 자라나요?",
          "original_form": "뱀은 잘 자라나요?",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.16",
          "form": "반려동물 말하는건데요",
          "original_form": "반려동물 말하는건데요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.17",
          "form": "저 바쁜데 자꾸 재촉하지 마세요",
          "original_form": "저 바쁜데 자꾸 재촉하지 마세요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.18",
          "form": "뱀 요즘에 성장이 멈춘 것 같아요",
          "original_form": "뱀 요즘에 성장이 멈춘 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.19",
          "form": "다 컸나보죠",
          "original_form": "다 컸나보죠",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015228.1.20",
