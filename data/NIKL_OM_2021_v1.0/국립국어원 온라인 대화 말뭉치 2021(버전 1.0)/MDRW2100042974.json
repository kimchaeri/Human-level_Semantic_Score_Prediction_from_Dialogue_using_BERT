      "utterance": [
        {
          "id": "MDRW2100042974.1.1",
          "form": "저는동유럽이요",
          "original_form": "저는동유럽이요",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042974.1.2",
          "form": "크로아티아 같은",
          "original_form": "크로아티아 같은",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042974.1.3",
          "form": "와 저도 체코 너무 가고파요",
          "original_form": "와 저도 체코 너무 가고파요",
          "speaker_id": "1",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042974.1.4",
          "form": "크로아티아ㅠㅠㅠ 이전에 무슨 연애프로그램에 한번 나왓오서",
          "original_form": "크로아티아ㅠㅠㅠ 이전에 무슨 연애프로그램에 한번 나왓오서",
          "speaker_id": "1",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042974.1.5",
          "form": "뮤비에 진쩌 이쁘게 덤겼거근요",
          "original_form": "뮤비에 진쩌 이쁘게 덤겼거근요",
          "speaker_id": "1",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042974.1.6",
          "form": "버킷리스트에요",
          "original_form": "버킷리스트에요",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042974.1.7",
          "form": "물가도좋대서ㅎㅎㅎ",
          "original_form": "물가도좋대서ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100042974.1.8",
          "form": "오오 그쵸ㅠㅠ  저도 가고시퍼요",
          "original_form": "오오 그쵸ㅠㅠ  저도 가고시퍼요",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042974.1.9",
          "form": "남친 북유럽있는데 동유럽에서 만나기로 했어",
          "original_form": "남친 북유럽있는데 동유럽에서 만나기로 했어",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042974.1.10",
          "form": "홀동유럽어디?",
          "original_form": "홀동유럽어디?",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100042974.1.11",
          "form": "나는 체코가 너무 가고싶어",
          "original_form": "나는 체코가 너무 가고싶어",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100042974.1.12",
          "form": "프라하",
          "original_form": "프라하",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100042974.1.13",
          "form": "체코좋다고하드라구",
          "original_form": "체코좋다고하드라구",
          "speaker_id": "2",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100042974.1.14",
          "form": "나도가보고싶어ㅎㅎ",
          "original_form": "나도가보고싶어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100042974.1.15",
          "form": "그치ㅠㅠ 너무 가고싶어",
          "original_form": "그치ㅠㅠ 너무 가고싶어",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100042974.1.16",
          "form": "코로나 물러가라ㅠㅠ",
          "original_form": "코로나 물러가라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100042974.1.17",
          "form": "그르게ㅜ물러가라ㅜㅜ",
          "original_form": "그르게ㅜ물러가라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100042974.1.18",
          "form": "여행많이다녀봤어?",
          "original_form": "여행많이다녀봤어?",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100042974.1.19",
          "form": "나 남친 좀 만나러가자ㅠㅠ",
          "original_form": "나 남친 좀 만나러가자ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100042974.1.20",
          "form": "ㅜㅜ보고싶겠다ㅠ",
          "original_form": "ㅜㅜ보고싶겠다ㅠ",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100042974.1.21",
          "form": "그러게 얼굴 기억도 안나 사실",
          "original_form": "그러게 얼굴 기억도 안나 사실",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100042974.1.22",
          "form": "오래됬구나ㅠ",
          "original_form": "오래됬구나ㅠ",
          "speaker_id": "2",
