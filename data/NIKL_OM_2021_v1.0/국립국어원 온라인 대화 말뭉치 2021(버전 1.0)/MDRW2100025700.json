        },
        {
          "id": "MDRW2100025700.1.2",
          "form": "필라테스 최근에 했다며",
          "original_form": "필라테스 최근에 했다며",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025700.1.3",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025700.1.4",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025700.1.5",
          "form": "월요일이 첫수업이었어!",
          "original_form": "월요일이 첫수업이었어!",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025700.1.6",
          "form": "정말 힘들어서 죽는 줄 알았어~",
          "original_form": "정말 힘들어서 죽는 줄 알았어~",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025700.1.7",
          "form": "name2이는 필라테스 해봤어?!",
          "original_form": "&name2&이는 필라테스 해봤어?!",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025700.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025700.1.9",
          "form": "한 2년전에",
          "original_form": "한 2년전에",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025700.1.10",
          "form": "했었어",
          "original_form": "했었어",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025700.1.11",
          "form": "백신맞았어 방그음",
          "original_form": "백신맞았어 방그음",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025700.1.12",
          "form": "오!!!!",
          "original_form": "오!!!!",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025700.1.13",
          "form": "세상에 이제 2차구나!!!",
          "original_form": "세상에 이제 2차구나!!!",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025700.1.14",
          "form": "백신 선배야 name2이는!!!",
          "original_form": "백신 선배야 &name2&이는!!!",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025700.1.15",
          "form": "무탈하면 좋겠다! 그리고 무탈할거야 ",
          "original_form": "무탈하면 좋겠다! 그리고 무탈할거야 ",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025700.1.16",
          "form": "ㅎㅎ고마워요",
          "original_form": "ㅎㅎ고마워요",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025700.1.17",
          "form": "부도 부작용없고 안아프길,,",
          "original_form": "부도 부작용없고 안아프길,,",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025700.1.18",
          "form": "하루하루 떨리겠다아ㅜ",
          "original_form": "하루하루 떨리겠다아ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025700.1.19",
          "form": "남의 일이 아닌걸?! 나는 10월 8일에 맞아!!",
          "original_form": "남의 일이 아닌걸?! 나는 10월 8일에 맞아!!",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025700.1.20",
          "form": "지금은 컨디션 어때??",
          "original_form": "지금은 컨디션 어때??",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025700.1.21",
          "form": "팔이 엄청 아프고 피곤한거말곤",
          "original_form": "팔이 엄청 아프고 피곤한거말곤",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100025700.1.22",
          "form": "아직 괜찮아요..ㅎㅎ",
          "original_form": "아직 괜찮아요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100025700.1.23",
          "form": "앗 피곤하구나 8ㅅ8",
          "original_form": "앗 피곤하구나 8ㅅ8",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100025700.1.24",
          "form": "근데 왜 은은하게 안괜찮아보이지?!",
          "original_form": "근데 왜 은은하게 안괜찮아보이지?!",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100025700.1.25",
          "form": "ㅋㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100025700.1.26",
          "form": "센스있는 name1",
          "original_form": "센스있는 &name1&",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100025700.1.27",
          "form": "약간 히마리가 없어지고있어",
          "original_form": "약간 히마리가 없어지고있어",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100025700.1.28",
          "form": "히마리 경상도 방언인가;0;",
          "original_form": "히마리 경상도 방언인가;0;",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100025700.1.29",
          "form": "오 히마리?! 사실 처음 들어보는 말이야",
          "original_form": "오 히마리?! 사실 처음 들어보는 말이야",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025700.1.30",
          "form": "멕아리 같은 건가?!",
          "original_form": "멕아리 같은 건가?!",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025700.1.31",
          "form": "맥아리 인가?!",
          "original_form": "맥아리 인가?!",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025700.1.32",
          "form": "확실히 2차가 1차보다 약효가 세긴 한가보다....",
          "original_form": "확실히 2차가 1차보다 약효가 세긴 한가보다....",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100025700.1.33",
          "form": "오 맞아맞아",
          "original_form": "오 맞아맞아",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100025700.1.34",
          "form": "힘 ㅎㅎ",
          "original_form": "힘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025700.1.35",
          "form": "name1는 몸좀 어때?",
          "original_form": "&name1&는 몸좀 어때?",
          "speaker_id": "2",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025700.1.36",
          "form": "나는 괜찮아!!",
          "original_form": "나는 괜찮아!!",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025700.1.37",
          "form": "오늘은 필라테스 가려다가 그냥 말았어,,",
          "original_form": "오늘은 필라테스 가려다가 그냥 말았어,,",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025700.1.38",
          "form": "내일 교수님한테 뭐 물어볼 겸 공부를 해야할 것 같아서ㅠ",
          "original_form": "내일 교수님한테 뭐 물어볼 겸 공부를 해야할 것 같아서ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025700.1.39",
          "form": "오늘 일찍 자야겠다 name2아ㅠ",
          "original_form": "오늘 일찍 자야겠다 &name2&아ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025700.1.40",
          "form": "응응 잘했어~",
          "original_form": "응응 잘했어~",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025700.1.41",
