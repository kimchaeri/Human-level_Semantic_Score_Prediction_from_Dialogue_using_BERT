      "utterance": [
        {
          "id": "MDRW2100045308.1.1",
          "form": "언니는 제일 좋아하는 계절이 뭐야?",
          "original_form": "언니는 제일 좋아하는 계절이 뭐야?",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045308.1.2",
          "form": "나는 놀랍게도 여름!",
          "original_form": "나는 놀랍게도 여름!",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045308.1.3",
          "form": "사실 봄인데 분위기때문에 여름을좋아하게됐어",
          "original_form": "사실 봄인데 분위기때문에 여름을좋아하게됐어",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.5",
          "form": "오!! 나는 가을!",
          "original_form": "오!! 나는 가을!",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.6",
          "form": "시원해서 좋아 ㅎㅎ",
          "original_form": "시원해서 좋아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.7",
          "form": "날씨로는 가을이 최고지 ㅎㅎ",
          "original_form": "날씨로는 가을이 최고지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.8",
          "form": "태어난것도 가을이구",
          "original_form": "태어난것도 가을이구",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.9",
          "form": "나는 여름 청춘을 좋아해서 여름이 좋나봐",
          "original_form": "나는 여름 청춘을 좋아해서 여름이 좋나봐",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045308.1.10",
          "form": "맞아 그러고보면 곧 언니 생일이네! 선선한 날씨에 태어난거 잘어울려",
          "original_form": "맞아 그러고보면 곧 언니 생일이네! 선선한 날씨에 태어난거 잘어울려",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045308.1.11",
          "form": "여름은 여름만의 그 분위기가 진짜 좋은거같애ㅠ",
          "original_form": "여름은 여름만의 그 분위기가 진짜 좋은거같애ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045308.1.12",
          "form": "맞아 그 그늘진 이미지랑 쨍한 하늘색이 최고야",
          "original_form": "맞아 그 그늘진 이미지랑 쨍한 하늘색이 최고야",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045308.1.13",
          "form": "물론 날씨는 최악이야",
          "original_form": "물론 날씨는 최악이야",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045308.1.14",
          "form": "그래도 이제 슬슬 가을이되가는거같아서 좋다",
          "original_form": "그래도 이제 슬슬 가을이되가는거같아서 좋다",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045308.1.15",
          "form": "여름은 덥긴한데 추억미화되는것같아",
          "original_form": "여름은 덥긴한데 추억미화되는것같아",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045308.1.16",
          "form": "작년 가을에 언니랑 같이 벤치에서 수다떨었던거 좋았는데~",
          "original_form": "작년 가을에 언니랑 같이 벤치에서 수다떨었던거 좋았는데~",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045308.1.17",
          "form": "아 그게 작면 가을이었어?",
          "original_form": "아 그게 작면 가을이었어?",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045308.1.18",
          "form": "시간이 왤케빨라.... 하긴그때 날씨가 가을이었구나",
          "original_form": "시간이 왤케빨라.... 하긴그때 날씨가 가을이었구나",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045308.1.19",
          "form": "시간 진짜 빠르지ㅠㅠ 밖에 앉아있기에 기온도 좋았는데",
          "original_form": "시간 진짜 빠르지ㅠㅠ 밖에 앉아있기에 기온도 좋았는데",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045308.1.20",
          "form": "맞아 지금도 문열어놓으니까 시원하고좋다",
          "original_form": "맞아 지금도 문열어놓으니까 시원하고좋다",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045308.1.21",
          "form": "요새는 미세먼지도 좋은거같더라고",
          "original_form": "요새는 미세먼지도 좋은거같더라고",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045308.1.22",
          "form": "나도 퇴근하고 집 올때 날씨 선선하니까 좋더라고 ㅎㅎ",
          "original_form": "나도 퇴근하고 집 올때 날씨 선선하니까 좋더라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045308.1.23",
          "form": "비만 안왔으면 좋겠어ㅠㅠ",
          "original_form": "비만 안왔으면 좋겠어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045308.1.24",
          "form": "그러게 요새 왜 자꾸 비가 오지?",
          "original_form": "그러게 요새 왜 자꾸 비가 오지?",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045308.1.25",
          "form": "가을비인가? 뭔가 매일매일 비오는기분",
          "original_form": "가을비인가? 뭔가 매일매일 비오는기분",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045308.1.26",
          "form": "비오는 소리는 듣기좋은데 나가면 불편해ㅜㅜ",
          "original_form": "비오는 소리는 듣기좋은데 나가면 불편해ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045308.1.27",
          "form": "바지 끝 젖는거 최악..",
          "original_form": "바지 끝 젖는거 최악..",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045308.1.28",
          "form": "그래도 비오면 시원해져서 좋긴해",
          "original_form": "그래도 비오면 시원해져서 좋긴해",
          "speaker_id": "2",
          "time": "20210908 01:54"
        }
      ]
    }
