      "utterance": [
        {
          "id": "MDRW2100020728.1.1",
          "form": "땅콩소스도 인터넷으로 샀는데 제가 자주가는 집 그맛이 안나더라구요",
          "original_form": "땅콩소스도 인터넷으로 샀는데 제가 자주가는 집 그맛이 안나더라구요",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020728.1.2",
          "form": "그럼 밀푀유나베좋아하시나요",
          "original_form": "그럼 밀푀유나베좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.3",
          "form": "앗 한번도 안먹어봤는데 맛있을거 같아요",
          "original_form": "앗 한번도 안먹어봤는데 맛있을거 같아요",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.4",
          "form": "앗 안드셔보셨구나",
          "original_form": "앗 안드셔보셨구나",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.5",
          "form": "밀푀유 나베는 뭔가 밀키트로 해야할거 같은데",
          "original_form": "밀푀유 나베는 뭔가 밀키트로 해야할거 같은데",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.6",
          "form": "맞아요 그게싸요",
          "original_form": "맞아요 그게싸요",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.7",
          "form": "귀찮을거같아서 미뤄뒀거든요ㅋㅋㅋ",
          "original_form": "귀찮을거같아서 미뤄뒀거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.8",
          "form": "밀푀유는 마이셰프입니다",
          "original_form": "밀푀유는 마이셰프입니다",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.9",
          "form": "맛있나여",
          "original_form": "맛있나여",
          "speaker_id": "2",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.10",
          "form": "기억하세요",
          "original_form": "기억하세요",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020728.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.12",
          "form": "알겠습니다",
          "original_form": "알겠습니다",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.13",
          "form": "진짜 밀키트는 저기가 가장ㅈ괜찮아요",
          "original_form": "진짜 밀키트는 저기가 가장ㅈ괜찮아요",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.14",
          "form": "비추하는 곳도 있으신가요",
          "original_form": "비추하는 곳도 있으신가요",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.15",
          "form": "프레시지.. 별로..",
          "original_form": "프레시지.. 별로..",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.16",
          "form": "ㅋㅋㅋㅋ 개인적 의견인데",
          "original_form": "ㅋㅋㅋㅋ 개인적 의견인데",
          "speaker_id": "1",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.17",
          "form": "적어둬야겠어요",
          "original_form": "적어둬야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.18",
          "form": "제친구도 마이셰프 종종 사먹더라구요",
          "original_form": "제친구도 마이셰프 종종 사먹더라구요",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020728.1.19",
          "form": "양이나 맛이나 마이셰프가 제일 괜찮아요",
          "original_form": "양이나 맛이나 마이셰프가 제일 괜찮아요",
          "speaker_id": "1",
          "time": "20210828 11:45"
        }
      ]
