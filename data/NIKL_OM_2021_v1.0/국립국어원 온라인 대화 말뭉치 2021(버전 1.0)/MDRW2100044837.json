      "utterance": [
        {
          "id": "MDRW2100044837.1.1",
          "form": "제발 데려가주라...",
          "original_form": "제발 데려가주라...",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.2",
          "form": "둘기들 면상 더이상 보고싶지않으니까",
          "original_form": "둘기들 면상 더이상 보고싶지않으니까",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.3",
          "form": "아닌 웨이션 걔네는",
          "original_form": "아닌 웨이션 걔네는",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.4",
          "form": "진짜 뭔데",
          "original_form": "진짜 뭔데",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.5",
          "form": "드림캐쳐 한동이는 남겨둬도 ㅇㅋ",
          "original_form": "드림캐쳐 한동이는 남겨둬도 ㅇㅋ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.6",
          "form": "루카스?",
          "original_form": "루카스?",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.7",
          "form": "중국노래 부를거면 중극으로 갓요",
          "original_form": "중국노래 부를거면 중극으로 갓요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.8",
          "form": "가세요",
          "original_form": "가세요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.9",
          "form": "루키스이전부터 노답",
          "original_form": "루키스이전부터 노답",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044837.1.10",
          "form": "루카스는 더노답이었지만",
          "original_form": "루카스는 더노답이었지만",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044837.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
