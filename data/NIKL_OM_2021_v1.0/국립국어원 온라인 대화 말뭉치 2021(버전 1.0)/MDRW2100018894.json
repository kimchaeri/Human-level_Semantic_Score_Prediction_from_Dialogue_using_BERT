      "utterance": [
        {
          "id": "MDRW2100018894.1.1",
          "form": "안녕하세요! 오늘 점심 뭐 드실거에요?",
          "original_form": "안녕하세요! 오늘 점심 뭐 드실거에요?",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018894.1.2",
          "form": "아 전 먹었어요ㅎㅎ",
          "original_form": "아 전 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018894.1.3",
          "form": "점심 드셨어요??",
          "original_form": "점심 드셨어요??",
          "speaker_id": "2",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018894.1.4",
          "form": "저는 아직 고민중이에요ㅠ",
          "original_form": "저는 아직 고민중이에요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018894.1.5",
          "form": "비 와서 전이라도 부칠까 하다가",
          "original_form": "비 와서 전이라도 부칠까 하다가",
          "speaker_id": "1",
          "time": "20210827 13:26"
        },
        {
          "id": "MDRW2100018894.1.6",
          "form": "귀찮아서 그냥 밥 간단하게 먹을지 고민이네요",
          "original_form": "귀찮아서 그냥 밥 간단하게 먹을지 고민이네요",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018894.1.7",
          "form": "오 전 좋죠",
          "original_form": "오 전 좋죠",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018894.1.8",
          "form": "전 귀찮아서 대충 먹었어요ㅋㅋ",
          "original_form": "전 귀찮아서 대충 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018894.1.9",
          "form": "맘만같아선 그냥 마라탕 배달시켜먹고싶어요ㅋㅋㅋ",
          "original_form": "맘만같아선 그냥 마라탕 배달시켜먹고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018894.1.10",
          "form": "ㅋㅋㅋㅋ배달음식이 편하죠",
          "original_form": "ㅋㅋㅋㅋ배달음식이 편하죠",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018894.1.11",
          "form": "마라탕 좋아하세요??",
          "original_form": "마라탕 좋아하세요??",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018894.1.12",
          "form": "네! 먹고 탈나는 경우가 많아서 자주 먹진 않는데 종종 생각나네요ㅠ",
          "original_form": "네! 먹고 탈나는 경우가 많아서 자주 먹진 않는데 종종 생각나네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018894.1.13",
          "form": "저도 매운거 잘 못먹어서 마라탕 먹으면 탈나더라구요ㅠㅠ",
          "original_form": "저도 매운거 잘 못먹어서 마라탕 먹으면 탈나더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018894.1.14",
          "form": "전 불닭도 잘 못먹어요..ㅎㅎ",
          "original_form": "전 불닭도 잘 못먹어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018894.1.15",
          "form": "저도요ㅠ 불닭 먹으면 탈나는데 가끔 너무 먹고싶어서 아플서 알면서도 한번씩 먹게되더라구요",
          "original_form": "저도요ㅠ 불닭 먹으면 탈나는데 가끔 너무 먹고싶어서 아플서 알면서도 한번씩 먹게되더라구요",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018894.1.16",
          "form": "저도요...그래서 소스 다 안 넣고 먹긴 해요ㅋㅋ",
          "original_form": "저도요...그래서 소스 다 안 넣고 먹긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018894.1.17",
          "form": "마라탕에 어떤 재료 넣는거 좋아하세요??",
          "original_form": "마라탕에 어떤 재료 넣는거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018894.1.18",
          "form": "저는 두부랑 버섯은 꼭 넣어요!!",
          "original_form": "저는 두부랑 버섯은 꼭 넣어요!!",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018894.1.19",
          "form": "고기는 없어도 저건 없음 아쉽더라구요ㅠ",
          "original_form": "고기는 없어도 저건 없음 아쉽더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018894.1.20",
          "form": "오 저도 푸주 넣는거 좋아해요ㅎㅎ",
          "original_form": "오 저도 푸주 넣는거 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018894.1.21",
          "form": "숙주랑 청경채 많이 넣는거 좋아하고요ㅎㅎ",
          "original_form": "숙주랑 청경채 많이 넣는거 좋아하고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018894.1.22",
          "form": "아 숙주랑 청경채도 진짜 맛있죠ㅠㅜ",
          "original_form": "아 숙주랑 청경채도 진짜 맛있죠ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018894.1.23",
          "form": "마라탕에 넣는 야채는 다 맛있는거같아요",
          "original_form": "마라탕에 넣는 야채는 다 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018894.1.24",
          "form": "그니까요 ㅎㅎ",
