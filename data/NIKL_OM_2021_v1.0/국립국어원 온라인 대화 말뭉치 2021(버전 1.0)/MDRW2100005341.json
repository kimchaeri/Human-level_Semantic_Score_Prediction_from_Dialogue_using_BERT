      "utterance": [
        {
          "id": "MDRW2100005341.1.1",
          "form": "아ㅎ 그렇군요ㅎ 그래도 수습기간은 있죠?",
          "original_form": "아ㅎ 그렇군요ㅎ 그래도 수습기간은 있죠?",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005341.1.2",
          "form": "아니요 그것도 없어요 ㅋㅋ",
          "original_form": "아니요 그것도 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005341.1.3",
          "form": "수습 아무 의미도 없는거ㅎㅎ어짜피 본인의사가 아니면 뽑을꺼면서ㅎㅎ",
          "original_form": "수습 아무 의미도 없는거ㅎㅎ어짜피 본인의사가 아니면 뽑을꺼면서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005341.1.4",
          "form": "중간에 나간사람들많아요?",
          "original_form": "중간에 나간사람들많아요?",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005341.1.5",
          "form": "있죠",
          "original_form": "있죠",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005341.1.6",
          "form": "3개월도 안되서 나간애는 1명이고 거진 1년은 채우고 다가드라구요",
          "original_form": "3개월도 안되서 나간애는 1명이고 거진 1년은 채우고 다가드라구요",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.7",
          "form": "일이 힘들어요?",
          "original_form": "일이 힘들어요?",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.8",
          "form": "업체와의 스트레스... 상사와 스트레스",
          "original_form": "업체와의 스트레스... 상사와 스트레스",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.9",
          "form": "저희는 안나가요....",
          "original_form": "저희는 안나가요....",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.10",
          "form": "업무도 많을텐데",
          "original_form": "업무도 많을텐데",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.11",
          "form": "꼭 버티고 안나가서ㅎㅎ",
          "original_form": "꼭 버티고 안나가서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.12",
          "form": "역삼각형이되ㅛ어요",
          "original_form": "역삼각형이되ㅛ어요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005341.1.13",
          "form": "아 고참들이 많은거네요... 신입이 적고",
          "original_form": "아 고참들이 많은거네요... 신입이 적고",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005341.1.14",
          "form": "저희 이직이 잦은 팀은 ... 스케줄이",
          "original_form": "저희 이직이 잦은 팀은 ... 스케줄이",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005341.1.15",
          "form": "시간단위에요 ㅋㅋㅋ",
          "original_form": "시간단위에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005341.1.16",
          "form": "아... 그러면 엄청 힘듥ㅅ네여",
          "original_form": "아... 그러면 엄청 힘듥ㅅ네여",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005341.1.17",
          "form": "사원, 대리보다 경영진이 많으면 이해하시겟죠?ㅎ",
          "original_form": "사원, 대리보다 경영진이 많으면 이해하시겟죠?ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005341.1.18",
          "form": "ㅋㅋㅋㅋ 엄청 불균형인 회사이네요",
          "original_form": "ㅋㅋㅋㅋ 엄청 불균형인 회사이네요",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005341.1.19",
          "form": "안나가서그런가봐요ㅎ",
          "original_form": "안나가서그런가봐요ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005341.1.20",
          "form": "경영진이 많으면 그분들 노하우로 영업하시나?",
          "original_form": "경영진이 많으면 그분들 노하우로 영업하시나?",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005341.1.21",
          "form": "좋은회사도아닌데 짜르질않아요",
          "original_form": "좋은회사도아닌데 짜르질않아요",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005341.1.22",
          "form": "영업도 못하는데",
          "original_form": "영업도 못하는데",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005341.1.23",
          "form": "장기계약이 많다보니 그런것같애요ㅎ",
          "original_form": "장기계약이 많다보니 그런것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005341.1.24",
          "form": "장기계약이면 정년보장 그런거에요?",
          "original_form": "장기계약이면 정년보장 그런거에요?",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005341.1.25",
          "form": "주어를 생략했네요ㅎㅎ COA 장기계약이 많아서 회사가 돈을 안정적으로 벌어요ㅎ 그래서 짜르질 않아요ㅎ 정년보장된 회사였었죠",
          "original_form": "주어를 생략했네요ㅎㅎ COA 장기계약이 많아서 회사가 돈을 안정적으로 벌어요ㅎ 그래서 짜르질 않아요ㅎ 정년보장된 회사였었죠",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005341.1.26",
          "form": "뭔가 검색해봣어요 .",
          "original_form": "뭔가 검색해봣어요 .",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005341.1.27",
          "form": "회사였었죠면 이젠 바뀌고 있는건가요?",
          "original_form": "회사였었죠면 이젠 바뀌고 있는건가요?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005341.1.28",
          "form": "이제 바뀌려나봐요ㅎ 나도 바꿔줬으면좋겟네요ㅎㅎ",
          "original_form": "이제 바뀌려나봐요ㅎ 나도 바꿔줬으면좋겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005341.1.29",
          "form": "이름도 바꾸고ㅎ 사람은안바뀌는데ㅠㅠ 힘드네요ㅎ",
          "original_form": "이름도 바꾸고ㅎ 사람은안바뀌는데ㅠㅠ 힘드네요ㅎ",
          "speaker_id": "2",
