        },
        {
          "id": "MDRW2100020413.1.2",
          "form": "내일풀죠 졸리니까",
          "original_form": "내일풀죠 졸리니까",
          "speaker_id": "2",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020413.1.3",
          "form": "대학교 생활 어땠어요",
          "original_form": "대학교 생활 어땠어요",
          "speaker_id": "1",
          "time": "20210828 03:00"
        },
        {
          "id": "MDRW2100020413.1.4",
          "form": "엥 이제 시작인데요",
          "original_form": "엥 이제 시작인데요",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020413.1.5",
          "form": "해뜨는거봐야죠",
          "original_form": "해뜨는거봐야죠",
          "speaker_id": "1",
          "time": "20210828 03:01"
        },
        {
          "id": "MDRW2100020413.1.6",
          "form": "제대학생활",
          "original_form": "제대학생활",
          "speaker_id": "2",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020413.1.7",
          "form": "딱히 기억안나네여",
          "original_form": "딱히 기억안나네여",
          "speaker_id": "2",
          "time": "20210828 09:07"
        },
        {
          "id": "MDRW2100020413.1.8",
          "form": "ㅋㅋ얼마 안 됐잖아요",
          "original_form": "ㅋㅋ얼마 안 됐잖아요",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100020413.1.9",
          "form": "아 저희는 학교 군기 빡세서",
          "original_form": "아 저희는 학교 군기 빡세서",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100020413.1.10",
          "form": "입학초에 선배님이라고부름ㅋㅋ",
          "original_form": "입학초에 선배님이라고부름ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100020413.1.11",
          "form": "재수생들도 14랑 동갑인데 선배님해야함;;꼴값",
          "original_form": "재수생들도 14랑 동갑인데 선배님해야함;;꼴값",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100020413.1.12",
          "form": "ㅋㅋㅋ선배정도는 뭐",
          "original_form": "ㅋㅋㅋ선배정도는 뭐",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100020413.1.13",
          "form": "집들어갔나여",
          "original_form": "집들어갔나여",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100020413.1.14",
          "form": "넹 집들어온지 오래됨",
          "original_form": "넹 집들어온지 오래됨",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100020413.1.15",
          "form": "저 궁금한거있는데",
          "original_form": "저 궁금한거있는데",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100020413.1.16",
          "form": "디엠보낼테니 받아보세요",
          "original_form": "디엠보낼테니 받아보세요",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100020413.1.17",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100020413.1.18",
          "form": "성적괜찮앗나요",
          "original_form": "성적괜찮앗나요",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100020413.1.19",
          "form": "제성적 개판이였죠",
          "original_form": "제성적 개판이였죠",
          "speaker_id": "2",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100020413.1.20",
          "form": "개판이요?",
          "original_form": "개판이요?",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100020413.1.21",
          "form": "에프잇엇나요",
          "original_form": "에프잇엇나요",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100020413.1.22",
          "form": "앗그정돈아니고",
          "original_form": "앗그정돈아니고",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100020413.1.23",
          "form": "씨밭이였어요",
          "original_form": "씨밭이였어요",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100020413.1.24",
          "form": "이게욕을 피했네",
          "original_form": "이게욕을 피했네",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100020413.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100020413.1.26",
          "form": "졸업했으면되죠",
          "original_form": "졸업했으면되죠",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100020413.1.27",
          "form": "박씨는 성적어땠나요",
          "original_form": "박씨는 성적어땠나요",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100020413.1.28",
          "form": "저는 롤코였죠",
          "original_form": "저는 롤코였죠",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100020413.1.29",
          "form": "바닥이다가 2학년2학ㅋ때부터 확오름",
          "original_form": "바닥이다가 2학년2학ㅋ때부터 확오름",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100020413.1.30",
          "form": "저도 3학년때부터 확오름",
          "original_form": "저도 3학년때부터 확오름",
          "speaker_id": "2",
