      "utterance": [
        {
          "id": "MDRW2100009576.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 00:54"
        },
        {
          "id": "MDRW2100009576.1.2",
          "form": "네 저 게임 엄청 좋아해요ㅋㅋ",
          "original_form": "네 저 게임 엄청 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009576.1.3",
          "form": "무슨게임 좋아하세요??",
          "original_form": "무슨게임 좋아하세요??",
          "speaker_id": "2",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009576.1.4",
          "form": "인터넷 게임으로는 롤 좋아해요 name2님은요?",
          "original_form": "인터넷 게임으로는 롤 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 00:55"
        },
        {
          "id": "MDRW2100009576.1.5",
          "form": "저는 컴게임은 잘 안해용..",
          "original_form": "저는 컴게임은 잘 안해용..",
          "speaker_id": "2",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009576.1.6",
          "form": "폰게임은 해요",
          "original_form": "폰게임은 해요",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009576.1.7",
          "form": "로드 오브 히어로즈라는 게임 아세요?",
          "original_form": "로드 오브 히어로즈라는 게임 아세요?",
          "speaker_id": "2",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009576.1.8",
          "form": "오 알아요!",
          "original_form": "오 알아요!",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009576.1.9",
          "form": "친구들이 하는 거 본 적 있어요",
          "original_form": "친구들이 하는 거 본 적 있어요",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009576.1.10",
          "form": "정말요?! 진짜 재밌어요~",
          "original_form": "정말요?! 진짜 재밌어요~",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009576.1.11",
          "form": "롤은 중학교 다닐 때 잠깐 했었는데",
          "original_form": "롤은 중학교 다닐 때 잠깐 했었는데",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009576.1.12",
          "form": "어떤 게임인가요?",
          "original_form": "어떤 게임인가요?",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009576.1.13",
          "form": "오ㅋㅋㅋ 그렇군요",
          "original_form": "오ㅋㅋㅋ 그렇군요",
          "speaker_id": "1",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009576.1.14",
          "form": "그..전략형 스토리 게임?",
          "original_form": "그..전략형 스토리 게임?",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009576.1.15",
          "form": "스토리가 엄청 큰 비중을 차지해요",
          "original_form": "스토리가 엄청 큰 비중을 차지해요",
          "speaker_id": "2",
          "time": "20210823 00:58"
        },
        {
          "id": "MDRW2100009576.1.16",
          "form": "스토리가 재밌어서 팬들이 스포일러를 엄청 조김해요",
          "original_form": "스토리가 재밌어서 팬들이 스포일러를 엄청 조김해요",
          "speaker_id": "2",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009576.1.17",
          "form": "엄청 흥미로운데요?",
          "original_form": "엄청 흥미로운데요?",
          "speaker_id": "1",
          "time": "20210823 00:59"
        },
        {
          "id": "MDRW2100009576.1.18",
          "form": "저는 쿠키런이나 롤 같은 스토리 비중 적은 게임을 좋아하지만 한 번 해보고 싶네요",
          "original_form": "저는 쿠키런이나 롤 같은 스토리 비중 적은 게임을 좋아하지만 한 번 해보고 싶네요",
          "speaker_id": "1",
          "time": "20210823 01:00"
        },
        {
          "id": "MDRW2100009576.1.19",
          "form": "좋아요..! 친구들한테 해보고 싶다고 얘기라면 친구분들이 기뻐하시겠네요",
          "original_form": "좋아요..! 친구들한테 해보고 싶다고 얘기라면 친구분들이 기뻐하시겠네요",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009576.1.20",
          "form": "롤은 기억이 잘 안나는데 저는 애니비아라는 캐릭터를 썼었어요 ㅋㅋㅋㅋ",
          "original_form": "롤은 기억이 잘 안나는데 저는 애니비아라는 캐릭터를 썼었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009576.1.21",
          "form": "오ㅋㅋㅋ 미드러였군요 전 주로 탑을 갑니다!",
          "original_form": "오ㅋㅋㅋ 미드러였군요 전 주로 탑을 갑니다!",
          "speaker_id": "1",
          "time": "20210823 01:02"
        }
      ]
    }
