      "utterance": [
        {
          "id": "MDRW2100028744.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028744.1.2",
          "form": "네 여행 너무 좋죠",
          "original_form": "네 여행 너무 좋죠",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028744.1.3",
          "form": "여행지 어디가 제일 좋았나요?",
          "original_form": "여행지 어디가 제일 좋았나요?",
          "speaker_id": "2",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028744.1.4",
          "form": "최근에 여행 간곳 있으신가요?",
          "original_form": "최근에 여행 간곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028744.1.5",
          "form": "저는 스페인이요",
          "original_form": "저는 스페인이요",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100028744.1.6",
          "form": "스페인 멋지다고 들었어요",
          "original_form": "스페인 멋지다고 들었어요",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028744.1.7",
          "form": "저는 부산이요 여행이라기보다 드라이브지만요",
          "original_form": "저는 부산이요 여행이라기보다 드라이브지만요",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028744.1.8",
          "form": "부산 좋져ㅠ",
          "original_form": "부산 좋져ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028744.1.9",
          "form": "고향이 부산이라 부산 넘 좋아해요",
          "original_form": "고향이 부산이라 부산 넘 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028744.1.10",
          "form": "아 고향이 부산이시군요 저는 부산근처 살아서 자주 가는편이에요",
          "original_form": "아 고향이 부산이시군요 저는 부산근처 살아서 자주 가는편이에요",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028744.1.11",
          "form": "스페인 어디가 젤 좋았나요?",
          "original_form": "스페인 어디가 젤 좋았나요?",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100028744.1.12",
          "form": "그렇군요ㅎㅎ 겨울 바다 좋아해서 가고 싶어요",
          "original_form": "그렇군요ㅎㅎ 겨울 바다 좋아해서 가고 싶어요",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028744.1.13",
          "form": "스페인은 남부가 좋았어요",
          "original_form": "스페인은 남부가 좋았어요",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028744.1.14",
          "form": "세비야나 말라가쪽!",
          "original_form": "세비야나 말라가쪽!",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028744.1.15",
          "form": "스페인 음식이 맛있다고 들었어요 맛있게 드신거 있으신가요?",
          "original_form": "스페인 음식이 맛있다고 들었어요 맛있게 드신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100028744.1.16",
          "form": "해산물이 맛있었던거 같아요 한국에서는 잘 안먹던 문어를 많이 먹은 기억이ㅎㅎ...",
          "original_form": "해산물이 맛있었던거 같아요 한국에서는 잘 안먹던 문어를 많이 먹은 기억이ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028744.1.17",
          "form": "아 문어샐러드 티비에서 본것같아요",
          "original_form": "아 문어샐러드 티비에서 본것같아요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028744.1.18",
          "form": "네 오래전인데 문어 맛나게 먹은 기억은 아직 나네요",
          "original_form": "네 오래전인데 문어 맛나게 먹은 기억은 아직 나네요",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028744.1.19",
          "form": "저도 스페인 가고싶어지네요 ㅎㅎ 거기는 날씨가 진짜 좋아보이더라구요",
          "original_form": "저도 스페인 가고싶어지네요 ㅎㅎ 거기는 날씨가 진짜 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028744.1.20",
          "form": "네 더울때 가도 습하지 않아서 좋았던거 같아요 꼭 강추입니다!",
          "original_form": "네 더울때 가도 습하지 않아서 좋았던거 같아요 꼭 강추입니다!",
          "speaker_id": "1",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028744.1.21",
          "form": "사람들도 다 친절했어요",
          "original_form": "사람들도 다 친절했어요",
          "speaker_id": "1",
          "time": "20210902 22:31"
        }
      ]
    }
