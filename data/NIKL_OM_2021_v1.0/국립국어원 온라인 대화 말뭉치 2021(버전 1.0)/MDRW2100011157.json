      "utterance": [
        {
          "id": "MDRW2100011157.1.1",
          "form": "요즘 단발병이 와서 고민이야",
          "original_form": "요즘 단발병이 와서 고민이야",
          "speaker_id": "1",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100011157.1.2",
          "form": "단발병 멈춰!",
          "original_form": "단발병 멈춰!",
          "speaker_id": "2",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100011157.1.3",
          "form": "앜ㅋㅋㅋㅋ 알겠어 ㅠㅠㅠㅠㅠ 언니는 머리 길러?",
          "original_form": "앜ㅋㅋㅋㅋ 알겠어 ㅠㅠㅠㅠㅠ 언니는 머리 길러?",
          "speaker_id": "1",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100011157.1.4",
          "form": "나는 그냥 머리가 길어지고 있어",
          "original_form": "나는 그냥 머리가 길어지고 있어",
          "speaker_id": "2",
          "time": "20210824 01:59"
        },
        {
          "id": "MDRW2100011157.1.5",
          "form": "너는 왜 단발병 왔어?",
          "original_form": "너는 왜 단발병 왔어?",
          "speaker_id": "2",
          "time": "20210824 01:59"
        },
        {
          "id": "MDRW2100011157.1.6",
          "form": "나는 날씨가 선선해 지니 좀 센치해져서?",
          "original_form": "나는 날씨가 선선해 지니 좀 센치해져서?",
          "speaker_id": "1",
          "time": "20210824 01:59"
        },
        {
          "id": "MDRW2100011157.1.7",
          "form": "아하 기분전환이구나",
          "original_form": "아하 기분전환이구나",
          "speaker_id": "2",
          "time": "20210824 02:00"
        },
        {
          "id": "MDRW2100011157.1.8",
          "form": "근데 너는 긴 머리가 워낙 잘 어울려서!",
          "original_form": "근데 너는 긴 머리가 워낙 잘 어울려서!",
          "speaker_id": "2",
          "time": "20210824 02:00"
        },
        {
          "id": "MDRW2100011157.1.9",
          "form": "그래서 그냥 긴머리 하려고 ㅎㅎㅎ",
          "original_form": "그래서 그냥 긴머리 하려고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:00"
        },
        {
          "id": "MDRW2100011157.1.10",
          "form": "머리가 상한 것도 있어ㅠㅠㅠ",
          "original_form": "머리가 상한 것도 있어ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 02:00"
        },
        {
          "id": "MDRW2100011157.1.11",
          "form": "나도 머리 길었을때 상해서 자르게 되더라",
          "original_form": "나도 머리 길었을때 상해서 자르게 되더라",
          "speaker_id": "2",
          "time": "20210824 02:01"
        },
        {
          "id": "MDRW2100011157.1.12",
          "form": "맞아 아마 상해서 조만간 조금 자를 것 같아 ㅎㅎ",
          "original_form": "맞아 아마 상해서 조만간 조금 자를 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:02"
        },
        {
          "id": "MDRW2100011157.1.13",
          "form": "응응 조금만 잘라보자",
          "original_form": "응응 조금만 잘라보자",
          "speaker_id": "2",
          "time": "20210824 02:02"
        },
        {
          "id": "MDRW2100011157.1.14",
          "form": "3센티미터 정도만 잘라보려고~",
          "original_form": "3센티미터 정도만 잘라보려고~",
          "speaker_id": "1",
          "time": "20210824 02:03"
        },
        {
          "id": "MDRW2100011157.1.15",
          "form": "언니는 머리 기를거야?",
          "original_form": "언니는 머리 기를거야?",
          "speaker_id": "1",
          "time": "20210824 02:03"
        },
        {
          "id": "MDRW2100011157.1.16",
          "form": "그정도면 괜찮겠네",
          "original_form": "그정도면 괜찮겠네",
          "speaker_id": "2",
          "time": "20210824 02:03"
        },
        {
          "id": "MDRW2100011157.1.17",
          "form": "나는 지금은 일단 기를것같이",
          "original_form": "나는 지금은 일단 기를것같이",
          "speaker_id": "2",
          "time": "20210824 02:03"
        },
        {
          "id": "MDRW2100011157.1.18",
          "form": "오 좋은 선택이야!! 길러도 넘 예쁠 것 같아 ><",
          "original_form": "오 좋은 선택이야!! 길러도 넘 예쁠 것 같아 ><",
