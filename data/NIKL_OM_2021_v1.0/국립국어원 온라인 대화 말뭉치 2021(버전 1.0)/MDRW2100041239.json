      "utterance": [
        {
          "id": "MDRW2100041239.1.1",
          "form": "전 라이너는 잘 안그려요 안어울리더라고요ㅠ",
          "original_form": "전 라이너는 잘 안그려요 안어울리더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041239.1.2",
          "form": "이젠진짜안그려요..자꾸다크로 내려와서",
          "original_form": "이젠진짜안그려요..자꾸다크로 내려와서",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041239.1.3",
          "form": "어떤걸사도번지더라구요",
          "original_form": "어떤걸사도번지더라구요",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041239.1.4",
          "form": "그렇습니다ㅠㅜ",
          "original_form": "그렇습니다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041239.1.5",
          "form": "갈색으로 그려보세요 번져도 이쁘더라고요",
          "original_form": "갈색으로 그려보세요 번져도 이쁘더라고요",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041239.1.6",
          "form": "맞아요 갈색이 그나마 ㅎㅎ",
          "original_form": "맞아요 갈색이 그나마 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041239.1.7",
          "form": "언더도그리구싶은데 번져서 도저히 안돼요오",
          "original_form": "언더도그리구싶은데 번져서 도저히 안돼요오",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041239.1.8",
          "form": "오늘 당근거래나가면서",
          "original_form": "오늘 당근거래나가면서",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041239.1.9",
          "form": "진짜간만에 화장하구나갔는데",
          "original_form": "진짜간만에 화장하구나갔는데",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041239.1.10",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041239.1.11",
          "form": "옷꼬라지가진짜",
          "original_form": "옷꼬라지가진짜",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041239.1.12",
          "form": "그친구가 반하지않았니?",
          "original_form": "그친구가 반하지않았니?",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041239.1.13",
          "form": "가관이어서빨리들어왓어요",
          "original_form": "가관이어서빨리들어왓어요",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041239.1.14",
          "form": "여자분이셧어여ㅋㄱㅈㅋㅈㄱㅈㄱ",
          "original_form": "여자분이셧어여ㅋㄱㅈㅋㅈㄱㅈㄱ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041239.1.15",
          "form": "그랬군요ㅠㅠ",
          "original_form": "그랬군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041239.1.16",
          "form": "이쁜 셔츠 입고나가지ㅜㅜ 무슨 옷 입었나요",
          "original_form": "이쁜 셔츠 입고나가지ㅜㅜ 무슨 옷 입었나요",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041239.1.17",
          "form": "물든티와",
          "original_form": "물든티와",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.18",
          "form": "보풀이 잔뜩일어난",
          "original_form": "보풀이 잔뜩일어난",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.19",
          "form": "이ㅠㅠㅠ",
          "original_form": "이ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.20",
          "form": "추리닝",
          "original_form": "추리닝",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.21",
          "form": "ㄱㅋㄱㅈㅋ",
          "original_form": "ㄱㅋㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.22",
          "form": "보풀 난건 버려야하나 어쩌죠",
          "original_form": "보풀 난건 버려야하나 어쩌죠",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.23",
          "form": "원래집바진데 입고나갔어여  귀찮아가지구",
          "original_form": "원래집바진데 입고나갔어여  귀찮아가지구",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041239.1.24",
          "form": "영상에 이어",
          "original_form": "영상에 이어",
          "speaker_id": "2",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041239.1.25",
