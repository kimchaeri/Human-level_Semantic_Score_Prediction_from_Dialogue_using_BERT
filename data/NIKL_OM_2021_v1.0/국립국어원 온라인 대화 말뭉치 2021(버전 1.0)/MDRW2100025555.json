      "utterance": [
        {
          "id": "MDRW2100025555.1.1",
          "form": "최근 차를 사고 싶어졌어요",
          "original_form": "최근 차를 사고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100025555.1.2",
          "form": "어떤차요?",
          "original_form": "어떤차요?",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100025555.1.3",
          "form": "외제차를 갖고싶지만..돈없어서 포기했어요",
          "original_form": "외제차를 갖고싶지만..돈없어서 포기했어요",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100025555.1.4",
          "form": "회사 출근용차를 원하셨던 건가요?",
          "original_form": "회사 출근용차를 원하셨던 건가요?",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100025555.1.5",
          "form": "아 따로 살 돈은 없어요 ㅎ",
          "original_form": "아 따로 살 돈은 없어요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100025555.1.6",
          "form": "그냥 제소유 차를 갖고싶은",
          "original_form": "그냥 제소유 차를 갖고싶은",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100025555.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100025555.1.8",
          "form": "갖고싶은거있으세요?",
          "original_form": "갖고싶은거있으세요?",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100025555.1.9",
          "form": "저는 태블릿이요",
          "original_form": "저는 태블릿이요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100025555.1.10",
          "form": "오~ 아이패드요?",
          "original_form": "오~ 아이패드요?",
          "speaker_id": "1",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100025555.1.11",
          "form": "아니요 탭이요 패드는 너무 비싸서....",
          "original_form": "아니요 탭이요 패드는 너무 비싸서....",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100025555.1.12",
          "form": "아하~ 저도 예~~~~전 모델 있는데 전 잘 안써지더라고요",
          "original_form": "아하~ 저도 예~~~~전 모델 있는데 전 잘 안써지더라고요",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100025555.1.13",
          "form": "아이패드 있으신가요?",
          "original_form": "아이패드 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100025555.1.14",
          "form": "갤럭시 탭 있어요!",
          "original_form": "갤럭시 탭 있어요!",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100025555.1.15",
          "form": "그렇군요 탭 몇 라인인가요?",
          "original_form": "그렇군요 탭 몇 라인인가요?",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100025555.1.16",
          "form": "A였던거같아요 ㅎ 진짜 오래전에 샀던거라",
          "original_form": "A였던거같아요 ㅎ 진짜 오래전에 샀던거라",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100025555.1.17",
          "form": "잘 켜보지도 않았다죠 ㅎ충동적으로 산거라",
