      "utterance": [
        {
          "id": "MDRW2100035051.1.1",
          "form": "나 최근에 귀여운 동물을 봤어",
          "original_form": "나 최근에 귀여운 동물을 봤어",
          "speaker_id": "2",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100035051.1.2",
          "form": "반려동물로 키울 수 있는 거니",
          "original_form": "반려동물로 키울 수 있는 거니",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100035051.1.3",
          "form": "맞아 분명 그럴 거야",
          "original_form": "맞아 분명 그럴 거야",
          "speaker_id": "2",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100035051.1.4",
          "form": "어떤 거 길래 그래",
          "original_form": "어떤 거 길래 그래",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100035051.1.5",
          "form": "라콘 알아?",
          "original_form": "라콘 알아?",
          "speaker_id": "2",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100035051.1.6",
          "form": "라쿤 알지 너구리 닮았어",
          "original_form": "라쿤 알지 너구리 닮았어",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100035051.1.7",
          "form": "라콘과 너구리는 아예 다르게 생겼는데",
          "original_form": "라콘과 너구리는 아예 다르게 생겼는데",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100035051.1.8",
          "form": "말도 안 되는 소리",
          "original_form": "말도 안 되는 소리",
          "speaker_id": "1",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100035051.1.9",
          "form": "라쿤은 눈이 까맣고 행동이 귀여워",
          "original_form": "라쿤은 눈이 까맣고 행동이 귀여워",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100035051.1.10",
          "form": "아 그러면 라쿠는 레서팬더랑 닮았어",
          "original_form": "아 그러면 라쿠는 레서팬더랑 닮았어",
          "speaker_id": "1",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100035051.1.11",
          "form": "그건 자세히 보면 아니라는 것을 알게 될 걸",
          "original_form": "그건 자세히 보면 아니라는 것을 알게 될 걸",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100035051.1.12",
          "form": "라쿤을 반려 동물로 삼을 수 있구나",
          "original_form": "라쿤을 반려 동물로 삼을 수 있구나",
          "speaker_id": "1",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100035051.1.13",
          "form": "의외로 많이들 하던데",
          "original_form": "의외로 많이들 하던데",
          "speaker_id": "2",
          "time": "20210829 01:12"
        }
