      "utterance": [
        {
          "id": "MDRW2100043692.1.1",
          "form": "엄마 오늘 열이 좀 나네ㅜ",
          "original_form": "엄마 오늘 열이 좀 나네ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043692.1.2",
          "form": "언제맞았는데",
          "original_form": "언제맞았는데",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043692.1.3",
          "form": "한 일주일정도",
          "original_form": "한 일주일정도",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043692.1.4",
          "form": "아빠는 2주되었고",
          "original_form": "아빠는 2주되었고",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043692.1.5",
          "form": "일주일됐는데 열이지금나나 ㅜ",
          "original_form": "일주일됐는데 열이지금나나 ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043692.1.6",
          "form": "너거형부는 이번주금요일 1차 맞는다",
          "original_form": "너거형부는 이번주금요일 1차 맞는다",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043692.1.7",
          "form": "아빠는 참...건강한거같다",
          "original_form": "아빠는 참...건강한거같다",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043692.1.8",
          "form": "그러게 그래도 전립선때문에 다음주 서울대간다",
          "original_form": "그러게 그래도 전립선때문에 다음주 서울대간다",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043692.1.9",
          "form": "내가 엄빠때문에 서울대를 엄청 자주간다~ㅋㅋ",
          "original_form": "내가 엄빠때문에 서울대를 엄청 자주간다~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043692.1.10",
          "form": "형부는 맞지마라하지",
          "original_form": "형부는 맞지마라하지",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043692.1.11",
          "form": "그렇네 ㅋ",
          "original_form": "그렇네 ㅋ",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043692.1.12",
          "form": "팀장이 넉거형부 뭐 실수하기만 지키고 있어서",
          "original_form": "팀장이 넉거형부 뭐 실수하기만 지키고 있어서",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043692.1.13",
          "form": "그래도 당뇨라하고 ㅠ",
          "original_form": "그래도 당뇨라하고 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043692.1.14",
          "form": "말이 안 통한다..맞으라고 하는게 아니라 자꾸 사람 만나라고하니깐ㅜ",
          "original_form": "말이 안 통한다..맞으라고 하는게 아니라 자꾸 사람 만나라고하니깐ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043692.1.15",
          "form": "너거형부도 위험에 노출되니ㅜ",
          "original_form": "너거형부도 위험에 노출되니ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043692.1.16",
          "form": "차라리 걸리는게나을수도있지않나",
          "original_form": "차라리 걸리는게나을수도있지않나",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043692.1.17",
          "form": "걸리면 치명적이지ㅜ당뇨라서",
          "original_form": "걸리면 치명적이지ㅜ당뇨라서",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043692.1.18",
          "form": "그리고 뉴스나온다ㅜ정부청사 코로나 환자 나왔다고",
          "original_form": "그리고 뉴스나온다ㅜ정부청사 코로나 환자 나왔다고",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043692.1.19",
          "form": "팀장한테 집중공격 당한다",
          "original_form": "팀장한테 집중공격 당한다",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
