      "utterance": [
        {
          "id": "MDRW2100027354.1.1",
          "form": "점심 배달하시나요",
          "original_form": "점심 배달하시나요",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027354.1.2",
          "form": "아무래도 탄수화물이^^",
          "original_form": "아무래도 탄수화물이^^",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.3",
          "form": "떡볶이도 좋구요",
          "original_form": "떡볶이도 좋구요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.4",
          "form": "저는 나가서 먹으려구요",
          "original_form": "저는 나가서 먹으려구요",
          "speaker_id": "1",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.5",
          "form": "요즘 로제떡볶이 좋고",
          "original_form": "요즘 로제떡볶이 좋고",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.6",
          "form": "순두부찌개나 찌개류도요",
          "original_form": "순두부찌개나 찌개류도요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.7",
          "form": "정식세트라거나",
          "original_form": "정식세트라거나",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.8",
          "form": "외식은 선택권이 넓지요",
          "original_form": "외식은 선택권이 넓지요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.9",
          "form": "제가 다 너무 좋아하는 메뉴들인데 요즘 뭔가 부담스러운 음식들이 잘 안땡기더라구요",
          "original_form": "제가 다 너무 좋아하는 메뉴들인데 요즘 뭔가 부담스러운 음식들이 잘 안땡기더라구요",
          "speaker_id": "1",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.10",
          "form": "후식은 커피로요",
          "original_form": "후식은 커피로요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.11",
          "form": "어제도 그래서 샐러드 먹고왔어요",
          "original_form": "어제도 그래서 샐러드 먹고왔어요",
          "speaker_id": "1",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027354.1.12",
          "form": "샐러드가 속 편해요",
          "original_form": "샐러드가 속 편해요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027354.1.13",
          "form": "고명을 조금 든든하게 하심되겠어요",
          "original_form": "고명을 조금 든든하게 하심되겠어요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027354.1.14",
          "form": "서브웨이도 빵없이 샐러드 맛있구요",
          "original_form": "서브웨이도 빵없이 샐러드 맛있구요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027354.1.15",
          "form": "단호박이 들어가도 포만감 있겠어요",
          "original_form": "단호박이 들어가도 포만감 있겠어요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027354.1.16",
          "form": "헉 뭔가 쉐프같으세요 ㅎㅎ!!",
          "original_form": "헉 뭔가 쉐프같으세요 ㅎㅎ!!",
          "speaker_id": "1",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027354.1.17",
          "form": "음식에 관심많아요",
          "original_form": "음식에 관심많아요",
          "speaker_id": "2",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100027354.1.18",
          "form": "건강하게 먹는것이욥",
          "original_form": "건강하게 먹는것이욥",
          "speaker_id": "2",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027354.1.19",
          "form": "베이글샌드위치도 좋아요",
          "original_form": "베이글샌드위치도 좋아요",
          "speaker_id": "2",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027354.1.20",
          "form": "베이글 맛있죠ㅠㅠ",
          "original_form": "베이글 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027354.1.21",
          "form": "따끈따끈 막 나왔을때 먹으면 캬",
          "original_form": "따끈따끈 막 나왔을때 먹으면 캬",
          "speaker_id": "1",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027354.1.22",
          "form": "네 빵도 모자란듯 끼니로 좋아요",
          "original_form": "네 빵도 모자란듯 끼니로 좋아요",
          "speaker_id": "2",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027354.1.23",
          "form": "후식으로 라떼류 먹으면 든든할거에요",
          "original_form": "후식으로 라떼류 먹으면 든든할거에요",
          "speaker_id": "2",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100027354.1.24",
          "form": "고구마라떼",
          "original_form": "고구마라떼",
