      "utterance": [
        {
          "id": "MDRW2100011166.1.1",
          "form": "요즘 집에서 돈 벌 고 싶은데 좋은 방법이 없을까?",
          "original_form": "요즘 집에서 돈 벌 고 싶은데 좋은 방법이 없을까?",
          "speaker_id": "1",
          "time": "20210824 02:04"
        },
        {
          "id": "MDRW2100011166.1.2",
          "form": "나는 화상과외 하고 있어",
          "original_form": "나는 화상과외 하고 있어",
          "speaker_id": "2",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100011166.1.3",
          "form": "나도 화상 멘토링 하고 있어 ㅎㅎ",
          "original_form": "나도 화상 멘토링 하고 있어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 02:29"
        },
        {
          "id": "MDRW2100011166.1.4",
          "form": "코로나의 좋은 점 이랄까?",
          "original_form": "코로나의 좋은 점 이랄까?",
          "speaker_id": "1",
          "time": "20210824 02:29"
        },
        {
          "id": "MDRW2100011166.1.5",
          "form": "정말 그러네",
          "original_form": "정말 그러네",
          "speaker_id": "2",
          "time": "20210824 02:57"
        },
        {
          "id": "MDRW2100011166.1.6",
          "form": "코로나 덕에 화상 과외도 하고",
          "original_form": "코로나 덕에 화상 과외도 하고",
          "speaker_id": "2",
          "time": "20210824 02:57"
        },
        {
          "id": "MDRW2100011166.1.7",
          "form": "또 다른 재택근무는 있어?",
          "original_form": "또 다른 재택근무는 있어?",
          "speaker_id": "1",
          "time": "20210825 04:10"
        },
        {
          "id": "MDRW2100011166.1.8",
          "form": "나는 요새 카카오 페이 눈치게임 하는 중이야",
          "original_form": "나는 요새 카카오 페이 눈치게임 하는 중이야",
          "speaker_id": "1",
          "time": "20210825 04:10"
        },
        {
          "id": "MDRW2100011166.1.9",
          "form": "요즘에는 어떤 일자리 해?",
          "original_form": "요즘에는 어떤 일자리 해?",
          "speaker_id": "1",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100011166.1.10",
          "form": "나는 번역가 알아보고 있어",
          "original_form": "나는 번역가 알아보고 있어",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100011166.1.11",
          "form": "우와 번역도 재택근무로 하는거야?",
          "original_form": "우와 번역도 재택근무로 하는거야?",
          "speaker_id": "1",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100011166.1.12",
          "form": "응 그런 셈이지",
          "original_form": "응 그런 셈이지",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100011166.1.13",
          "form": "오 프리랜서 그런건가?",
          "original_form": "오 프리랜서 그런건가?",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100011166.1.14",
          "form": "그거는 실력만 좋으면 할 수 있는거야?",
          "original_form": "그거는 실력만 좋으면 할 수 있는거야?",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100011166.1.15",
          "form": "그런셈이지",
          "original_form": "그런셈이지",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100011166.1.16",
          "form": "그렇구나 멋있어",
          "original_form": "그렇구나 멋있어",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100011166.1.17",
          "form": "그거는 겸직허가 가능한가?",
          "original_form": "그거는 겸직허가 가능한가?",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100011166.1.18",
          "form": "너는 하는거 있어?",
          "original_form": "너는 하는거 있어?",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100011166.1.19",
          "form": "나는 띡히 하는거 없어!",
          "original_form": "나는 띡히 하는거 없어!",
          "speaker_id": "1",
          "time": "20210906 15:29"
        }
      ]
