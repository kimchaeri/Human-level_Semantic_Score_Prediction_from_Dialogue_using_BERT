      "utterance": [
        {
          "id": "MDRW2100041731.1.1",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210906 15:10"
        },
        {
          "id": "MDRW2100041731.1.2",
          "form": "즐겁지 못한 월요일이지만.. 네네 ㅎㅎ",
          "original_form": "즐겁지 못한 월요일이지만.. 네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 15:10"
        },
        {
          "id": "MDRW2100041731.1.3",
          "form": "ㅋㅋㅋㅋㅋ 점심 드셨아요?",
          "original_form": "ㅋㅋㅋㅋㅋ 점심 드셨아요?",
          "speaker_id": "1",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041731.1.4",
          "form": "저는 점심 먹었어요! 식사하셨나요?",
          "original_form": "저는 점심 먹었어요! 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041731.1.5",
          "form": "넵!!! 그리고 지금은 백신 맞으러 왔어요ㅋㅋㅋㅋ",
          "original_form": "넵!!! 그리고 지금은 백신 맞으러 왔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041731.1.6",
          "form": "헉 오늘 백신맞는 날이시군요",
          "original_form": "헉 오늘 백신맞는 날이시군요",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041731.1.7",
          "form": "넵넵 집가는길에 빵 잔뜩 사가려고요",
          "original_form": "넵넵 집가는길에 빵 잔뜩 사가려고요",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041731.1.8",
          "form": "어떤 종류 좋아하세요?",
          "original_form": "어떤 종류 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041731.1.9",
          "form": "저는 진짜 완전",
          "original_form": "저는 진짜 완전",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.10",
          "form": "빵 맛 빵",
          "original_form": "빵 맛 빵",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋아해요",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.12",
          "form": "무슨느낌인지... 아시겠...나요..??",
          "original_form": "무슨느낌인지... 아시겠...나요..??",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.13",
          "form": "앗..음...",
          "original_form": "앗..음...",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.14",
          "form": "으음. ..",
          "original_form": "으음. ..",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.15",
          "form": "커스타드빵 같은 느낌이려나여",
          "original_form": "커스타드빵 같은 느낌이려나여",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.16",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.17",
          "form": "으음",
          "original_form": "으음",
          "speaker_id": "1",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041731.1.18",
          "form": "스콘 베이글 크로와상",
          "original_form": "스콘 베이글 크로와상",
          "speaker_id": "1",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100041731.1.19",
          "form": "이런 느낌?!",
          "original_form": "이런 느낌?!",
          "speaker_id": "1",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100041731.1.20",
          "form": "뭔 느낌인지 알 것 같아요!",
          "original_form": "뭔 느낌인지 알 것 같아요!",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041731.1.21",
          "form": "다른 맛을 빌리지 않고..",
          "original_form": "다른 맛을 빌리지 않고..",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041731.1.22",
          "form": "빵 자체의 맛..!!",
          "original_form": "빵 자체의 맛..!!",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041731.1.23",
          "form": "맞아요...! 밀가루와 버터와... 발효된 그 맛...!",
          "original_form": "맞아요...! 밀가루와 버터와... 발효된 그 맛...!",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041731.1.24",
          "form": "저는 쫌 짭짤한 빵을 좋아해요",
          "original_form": "저는 쫌 짭짤한 빵을 좋아해요",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041731.1.25",
          "form": "피자빵 소세지빵 이런..",
          "original_form": "피자빵 소세지빵 이런..",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041731.1.26",
          "form": "아이유 피셜 진지한 맛",
          "original_form": "아이유 피셜 진지한 맛",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041731.1.27",
          "form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041731.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아이유씨가 그런 말씀을 하셨어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아이유씨가 그런 말씀을 하셨어요?",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041731.1.29",
          "form": "음 그런데 베이글 같은 빵이 더 진지한 빵 아닌가요...?! ",
          "original_form": "음 그런데 베이글 같은 빵이 더 진지한 빵 아닌가요...?! ",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041731.1.30",
          "form": "단빵(재밌는 맛)을 좋아한대요!",
          "original_form": "단빵(재밌는 맛)을 좋아한대요!",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041731.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041731.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ음! 관점의 차이일까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음! 관점의 차이일까요",
          "speaker_id": "1",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041731.1.33",
          "form": "어우 저는 그 베이글 뻑뻑한 그 맛이 넘 좋아요",
          "original_form": "어우 저는 그 베이글 뻑뻑한 그 맛이 넘 좋아요",
          "speaker_id": "1",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041731.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041731.1.35",
          "form": "전 베이글 막 데워져서 나왔을때...",
          "original_form": "전 베이글 막 데워져서 나왔을때...",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041731.1.36",
          "form": "크림치즈 사악 발라서먹능걸 좋아해요ㅠㅠㅠ",
          "original_form": "크림치즈 사악 발라서먹능걸 좋아해요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041731.1.37",
          "form": "베이글... 뿌드득 소리날것 같지만 딱딱하진 않은 그 맛...",
