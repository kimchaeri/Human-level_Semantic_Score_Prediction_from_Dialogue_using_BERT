      "utterance": [
        {
          "id": "MDRW2100026708.1.1",
          "form": "주변에 군대간사람 있나요?",
          "original_form": "주변에 군대간사람 있나요?",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026708.1.2",
          "form": "다 제대했네요ㅎㅎ군대간 사람 있으세요?",
          "original_form": "다 제대했네요ㅎㅎ군대간 사람 있으세요?",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026708.1.3",
          "form": "저는 사촌이 이번 10월에 가요ㅎ.ㅎ",
          "original_form": "저는 사촌이 이번 10월에 가요ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026708.1.4",
          "form": "워낙 숫기없는 애라 걱정이에요",
          "original_form": "워낙 숫기없는 애라 걱정이에요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026708.1.5",
          "form": "아 육군으로 가나요?",
          "original_form": "아 육군으로 가나요?",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026708.1.6",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026708.1.7",
          "form": "저는 사실 여자라서 잘 모르거든요...",
          "original_form": "저는 사실 여자라서 잘 모르거든요...",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026708.1.8",
          "form": "저도 그래요ㅎㅎ그런데 군대 갔다와서 오히려 긍정적으로 변하는 사람들도 많더라구요",
          "original_form": "저도 그래요ㅎㅎ그런데 군대 갔다와서 오히려 긍정적으로 변하는 사람들도 많더라구요",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026708.1.9",
          "form": "체력도 키우고!",
          "original_form": "체력도 키우고!",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026708.1.10",
          "form": "ㅎ.ㅎ 그런가요!? 아무래도 그렇겠죠?? 제눈엔 아직 20살 애기라ㅠ",
          "original_form": "ㅎ.ㅎ 그런가요!? 아무래도 그렇겠죠?? 제눈엔 아직 20살 애기라ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026708.1.11",
          "form": "여자둘이서 군대얘기라니ㅋㅋ신기하네요",
          "original_form": "여자둘이서 군대얘기라니ㅋㅋ신기하네요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026708.1.12",
          "form": "그러게요 ㅋㅋ주제가 ..ㅋㅋ",
          "original_form": "그러게요 ㅋㅋ주제가 ..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026708.1.13",
          "form": "시간이 많아서 생각할 시간도 가지고 좋았다고..",
          "original_form": "시간이 많아서 생각할 시간도 가지고 좋았다고..",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026708.1.14",
          "form": "물론 힘들겠지만ㅜㅜ",
          "original_form": "물론 힘들겠지만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026708.1.15",
          "form": "아들분께서요??",
          "original_form": "아들분께서요??",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026708.1.16",
          "form": "음음 아니라면 죄송해요",
          "original_form": "음음 아니라면 죄송해요",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026708.1.17",
          "form": "아들은 의경 가서. 제대하더니 경찰시험봐서 지금 경찰하고 있어요",
          "original_form": "아들은 의경 가서. 제대하더니 경찰시험봐서 지금 경찰하고 있어요",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026708.1.18",
          "form": "와 되게 뿌듯하시겠어요",
          "original_form": "와 되게 뿌듯하시겠어요",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026708.1.19",
          "form": "경시 되게 어렵다던데",
          "original_form": "경시 되게 어렵다던데",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026708.1.20",
          "form": "경찰시험이 힘들어서 다 소방으로 돌리구 그러더라구요!!",
          "original_form": "경찰시험이 힘들어서 다 소방으로 돌리구 그러더라구요!!",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026708.1.21",
          "form": "그쵸 참 잘 됐어요",
          "original_form": "그쵸 참 잘 됐어요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026708.1.22",
          "form": "부러워요~!!!",
          "original_form": "부러워요~!!!",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026708.1.23",
          "form": "대학생인가요?",
          "original_form": "대학생인가요?",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026708.1.24",
          "form": "사촌분한테 편지 자주 써주세요ㅎㅎ",
          "original_form": "사촌분한테 편지 자주 써주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026708.1.25",
          "form": "저는 이제 막 졸업해서 시험준비하네요ㅠ",
          "original_form": "저는 이제 막 졸업해서 시험준비하네요ㅠ",
