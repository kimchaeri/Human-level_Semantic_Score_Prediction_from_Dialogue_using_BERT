        },
        {
          "id": "MDRW2100027408.1.2",
          "form": "여행가자",
          "original_form": "여행가자",
          "speaker_id": "1",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027408.1.3",
          "form": "여행가기 좋은 날씨는 뭘까",
          "original_form": "여행가기 좋은 날씨는 뭘까",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027408.1.4",
          "form": "맑고 바람많이 부는 날",
          "original_form": "맑고 바람많이 부는 날",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027408.1.5",
          "form": "여행가기 좋은 날씨지",
          "original_form": "여행가기 좋은 날씨지",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027408.1.6",
          "form": "계절은 지금이지",
          "original_form": "계절은 지금이지",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027408.1.7",
          "form": "가을 좋지",
          "original_form": "가을 좋지",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027408.1.8",
          "form": "봄엔 꽃이펴서 예쁘고",
          "original_form": "봄엔 꽃이펴서 예쁘고",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027408.1.9",
          "form": "울긋불긋 낙엽도 이쁘지",
          "original_form": "울긋불긋 낙엽도 이쁘지",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027408.1.10",
          "form": "한라산 예쁘겠다",
          "original_form": "한라산 예쁘겠다",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027408.1.11",
          "form": "단풍 구경가기 딱이야",
          "original_form": "단풍 구경가기 딱이야",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027408.1.12",
          "form": "비오면 뭐할까",
          "original_form": "비오면 뭐할까",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027408.1.13",
          "form": "비오면 아무것도 못하지",
          "original_form": "비오면 아무것도 못하지",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027408.1.14",
          "form": "집에 있어야지",
          "original_form": "집에 있어야지",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027408.1.15",
          "form": "눈오는건 어때",
          "original_form": "눈오는건 어때",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027408.1.16",
          "form": "첫눈은 예쁘지",
          "original_form": "첫눈은 예쁘지",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027408.1.17",
          "form": "세상이 깨끗해",
          "original_form": "세상이 깨끗해",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027408.1.18",
          "form": "눈이 젤 이뻐",
          "original_form": "눈이 젤 이뻐",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027408.1.19",
          "form": "태풍안오믄 좋겠다",
          "original_form": "태풍안오믄 좋겠다",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027408.1.20",
          "form": "바람부는건 좋아",
          "original_form": "바람부는건 좋아",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
