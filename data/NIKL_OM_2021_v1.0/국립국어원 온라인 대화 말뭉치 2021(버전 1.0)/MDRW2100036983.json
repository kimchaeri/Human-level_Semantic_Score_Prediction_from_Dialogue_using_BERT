      "utterance": [
        {
          "id": "MDRW2100036983.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 16:19"
        },
        {
          "id": "MDRW2100036983.1.2",
          "form": "네 근데 최근엔 못가고있어요",
          "original_form": "네 근데 최근엔 못가고있어요",
          "speaker_id": "2",
          "time": "20210831 16:20"
        },
        {
          "id": "MDRW2100036983.1.3",
          "form": "저도요ㅜㅜ어디가가장 좋으셨어요",
          "original_form": "저도요ㅜㅜ어디가가장 좋으셨어요",
          "speaker_id": "1",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036983.1.4",
          "form": "부산에서 재밌게 놀았어요",
          "original_form": "부산에서 재밌게 놀았어요",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036983.1.5",
          "form": "기억에 남는 여행지 있으신가요?",
          "original_form": "기억에 남는 여행지 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036983.1.6",
          "form": "국내는 제주도요ㅜㅜ부산도너무좋죠",
          "original_form": "국내는 제주도요ㅜㅜ부산도너무좋죠",
          "speaker_id": "1",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100036983.1.7",
          "form": "부산어디가 젤좋으셨어요",
          "original_form": "부산어디가 젤좋으셨어요",
          "speaker_id": "1",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100036983.1.8",
          "form": "서면이랑 해운대 갔는데 둘 다 좋았어요",
          "original_form": "서면이랑 해운대 갔는데 둘 다 좋았어요",
          "speaker_id": "2",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100036983.1.9",
          "form": "저도 해운대 광안리너무좋아해여",
          "original_form": "저도 해운대 광안리너무좋아해여",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100036983.1.10",
          "form": "맛집은어디가셨어요",
          "original_form": "맛집은어디가셨어요",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100036983.1.11",
          "form": "서면에 마라탕집 갔었어요",
          "original_form": "서면에 마라탕집 갔었어요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036983.1.12",
          "form": "해운대 맛집 있나요?",
          "original_form": "해운대 맛집 있나요?",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036983.1.13",
          "form": "저도 맛집은 거의 서면에 가는것같아요",
          "original_form": "저도 맛집은 거의 서면에 가는것같아요",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036983.1.14",
          "form": "마라탕맛있던가여?",
          "original_form": "마라탕맛있던가여?",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036983.1.15",
          "form": "네 맛있었어요",
          "original_form": "네 맛있었어요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036983.1.16",
          "form": "라공방이라고 유명하더라구요",
          "original_form": "라공방이라고 유명하더라구요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036983.1.17",
          "form": "오오 라공방 한번찾아봐야겠네요",
          "original_form": "오오 라공방 한번찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036983.1.18",
          "form": "해외는 가보고싶은 여행지있으시나요",
          "original_form": "해외는 가보고싶은 여행지있으시나요",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036983.1.19",
          "form": "전 스페인 가보고싶어요",
          "original_form": "전 스페인 가보고싶어요",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036983.1.20",
          "form": "저도요ㅜㅜ미식의나라 스페인ㅜㅜㅜ",
          "original_form": "저도요ㅜㅜ미식의나라 스페인ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100036983.1.21",
          "form": "스페인가보고싶은이유 뭐에요",
          "original_form": "스페인가보고싶은이유 뭐에요",
          "speaker_id": "1",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036983.1.22",
          "form": "주변에 유럽 갔다온 친구들한테 평이 제일 좋더라구요 ㅋㅋㅋ",
          "original_form": "주변에 유럽 갔다온 친구들한테 평이 제일 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036983.1.23",
          "form": "날씨도 좋고 풍경도 예쁘고 음식도 맛있대요",
          "original_form": "날씨도 좋고 풍경도 예쁘고 음식도 맛있대요",
          "speaker_id": "2",
          "time": "20210831 16:34"
        }
      ]
    }
