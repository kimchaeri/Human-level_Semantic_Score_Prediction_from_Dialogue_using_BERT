      "utterance": [
        {
          "id": "MDRW2100033033.1.1",
          "form": "안녕하세요 점심드셨나요??",
          "original_form": "안녕하세요 점심드셨나요??",
          "speaker_id": "1",
          "time": "20210906 14:17"
        },
        {
          "id": "MDRW2100033033.1.2",
          "form": "네, 닭갈비 먹었어요",
          "original_form": "네, 닭갈비 먹었어요",
          "speaker_id": "2",
          "time": "20210906 14:17"
        },
        {
          "id": "MDRW2100033033.1.3",
          "form": "와 마싯겟다..",
          "original_form": "와 마싯겟다..",
          "speaker_id": "1",
          "time": "20210906 14:17"
        },
        {
          "id": "MDRW2100033033.1.4",
          "form": "전 오늘 초밥을 먹었어요",
          "original_form": "전 오늘 초밥을 먹었어요",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033033.1.5",
          "form": "오, 모둠초밥?",
          "original_form": "오, 모둠초밥?",
          "speaker_id": "2",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033033.1.6",
          "form": "은행골아시나요??",
          "original_form": "은행골아시나요??",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033033.1.7",
          "form": "어, 들어본 것 같아요",
          "original_form": "어, 들어본 것 같아요",
          "speaker_id": "2",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033033.1.8",
          "form": "넵 거기서 특진초밥인가 먹었어요ㅎ",
          "original_form": "넵 거기서 특진초밥인가 먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033033.1.9",
          "form": "오, 맛있던가요??",
          "original_form": "오, 맛있던가요??",
          "speaker_id": "2",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100033033.1.10",
          "form": "넵 맛있어요! 근데 밥알이 잘 부서저요",
          "original_form": "넵 맛있어요! 근데 밥알이 잘 부서저요",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100033033.1.11",
          "form": "아하, 그랬군요",
          "original_form": "아하, 그랬군요",
          "speaker_id": "2",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100033033.1.12",
          "form": "그래서 먹기가 좀 불편한점말고는 없어요!",
          "original_form": "그래서 먹기가 좀 불편한점말고는 없어요!",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033033.1.13",
          "form": "오, 네네\n초밥 자주 드시나요?",
          "original_form": "오, 네네\n초밥 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033033.1.14",
          "form": "넵 전 초밥좋아해요!!저번주에도 먹었어요 ㅎㅎ",
          "original_form": "넵 전 초밥좋아해요!!저번주에도 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033033.1.15",
          "form": "해산물을 좋아하시는구나",
          "original_form": "해산물을 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033033.1.16",
          "form": "해산물 안좋아하시나요??",
          "original_form": "해산물 안좋아하시나요??",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100033033.1.17",
          "form": "자주 먹는 편은 아니에요ㅎㅎ",
          "original_form": "자주 먹는 편은 아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100033033.1.18",
          "form": "어떤음식을 좋아하세요??",
          "original_form": "어떤음식을 좋아하세요??",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100033033.1.19",
          "form": "떡볶이 김밥 오므라이스 냉면 좋아해요ㅎㅎ",
          "original_form": "떡볶이 김밥 오므라이스 냉면 좋아해요ㅎㅎ",
          "speaker_id": "2",
