      "utterance": [
        {
          "id": "MDRW2100036848.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036848.1.2",
          "form": "오늘 점심식사하셨나요",
          "original_form": "오늘 점심식사하셨나요",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036848.1.3",
          "form": "아직이요\r\nname1님은요?",
          "original_form": "아직이요\r\n&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036848.1.4",
          "form": "전 지금먹고있어요",
          "original_form": "전 지금먹고있어요",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036848.1.5",
          "form": "짜장밥으로 ㅎㅎ",
          "original_form": "짜장밥으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036848.1.6",
          "form": "name2님뭐드실거에요",
          "original_form": "&name2&님뭐드실거에요",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036848.1.7",
          "form": "점심이 늦어서 간단하게 먹어야 할 것 같아요",
          "original_form": "점심이 늦어서 간단하게 먹어야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036848.1.8",
          "form": "지금먹으면 저녁이 애매하긴하네요",
          "original_form": "지금먹으면 저녁이 애매하긴하네요",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036848.1.9",
          "form": "배달음식좋아하시나요",
          "original_form": "배달음식좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036848.1.10",
          "form": "네 그렇죠\r\n배달음식 가끔 먹지만 즐기는편은 아니에요",
          "original_form": "네 그렇죠\r\n배달음식 가끔 먹지만 즐기는편은 아니에요",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100036848.1.11",
          "form": "전 배달너무좋아해서 탈이에요ㅜ",
          "original_form": "전 배달너무좋아해서 탈이에요ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100036848.1.12",
          "form": "가끔어떤거 시켜드세요",
          "original_form": "가끔어떤거 시켜드세요",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036848.1.13",
          "form": "음 최근에는 치킨 먹었어요",
          "original_form": "음 최근에는 치킨 먹었어요",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036848.1.14",
          "form": "어디 치킨드셨어요",
          "original_form": "어디 치킨드셨어요",
          "speaker_id": "1",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036848.1.15",
          "form": "굽네치킨이요",
          "original_form": "굽네치킨이요",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036848.1.16",
          "form": "굽네맛있져ㅜㅜ굽네뭐드셨어요",
          "original_form": "굽네맛있져ㅜㅜ굽네뭐드셨어요",
