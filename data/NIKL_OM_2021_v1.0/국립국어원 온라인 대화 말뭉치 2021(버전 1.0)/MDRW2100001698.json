      "utterance": [
        {
          "id": "MDRW2100001698.1.1",
          "form": "학교에서 요즘에 시험쳐???",
          "original_form": "학교에서 요즘에 시험쳐???",
          "speaker_id": "2",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100001698.1.2",
          "form": "시험치지",
          "original_form": "시험치지",
          "speaker_id": "1",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001698.1.3",
          "form": "일주일남았어",
          "original_form": "일주일남았어",
          "speaker_id": "1",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001698.1.4",
          "form": "언니야는 수강신청 다함??",
          "original_form": "언니야는 수강신청 다함??",
          "speaker_id": "1",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001698.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100001698.1.6",
          "form": "난 이미 올클했다 ㅋㅋㅋ",
          "original_form": "난 이미 올클했다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100001698.1.7",
          "form": "이번엔 다행히 잘 일어나서",
          "original_form": "이번엔 다행히 잘 일어나서",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100001698.1.8",
          "form": "전부다 잡았어",
          "original_form": "전부다 잡았어",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100001698.1.9",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001698.1.10",
          "form": "언니야 동아리같은거는 안해?",
          "original_form": "언니야 동아리같은거는 안해?",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001698.1.11",
          "form": "나 안해",
          "original_form": "나 안해",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001698.1.12",
          "form": "동아리도 전부 경기쪽에서 모여서",
          "original_form": "동아리도 전부 경기쪽에서 모여서",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001698.1.13",
          "form": "나는 지금 여기 사니깐",
          "original_form": "나는 지금 여기 사니깐",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001698.1.14",
          "form": "너무 멀어서, 코로나때문에 못가",
          "original_form": "너무 멀어서, 코로나때문에 못가",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001698.1.15",
          "form": "모임도 지금 제한인데",
          "original_form": "모임도 지금 제한인데",
          "speaker_id": "2",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001698.1.16",
          "form": "그건 그렇다",
          "original_form": "그건 그렇다",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001698.1.17",
          "form": "그러면 학교 다니는거같지도 않겠다",
          "original_form": "그러면 학교 다니는거같지도 않겠다",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001698.1.18",
          "form": "그냥... 온라인 학교 다니는거같아",
          "original_form": "그냥... 온라인 학교 다니는거같아",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001698.1.19",
          "form": "너는???",
          "original_form": "너는???",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001698.1.20",
          "form": "그래도 개학해서 학교 가니 어때?",
          "original_form": "그래도 개학해서 학교 가니 어때?",
          "speaker_id": "2",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001698.1.21",
          "form": "애들 만나서 얘기하는건 재밌지",
          "original_form": "애들 만나서 얘기하는건 재밌지",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001698.1.22",
          "form": "근데 요새 한창 대입준비한다고",
          "original_form": "근데 요새 한창 대입준비한다고",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001698.1.23",
          "form": "그러는거는",
          "original_form": "그러는거는",
          "speaker_id": "1",
          "time": "20210819 18:16"
        },
        {
          "id": "MDRW2100001698.1.24",
          "form": "좀 싫지",
          "original_form": "좀 싫지",
          "speaker_id": "1",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001698.1.25",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001698.1.26",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001698.1.27",
          "form": "애들 만나는게좋지???",
          "original_form": "애들 만나는게좋지???",
          "speaker_id": "2",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001698.1.28",
          "form": "우리는 누군지도 몰러~",
          "original_form": "우리는 누군지도 몰러~",
          "speaker_id": "2",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001698.1.29",
          "form": "이게 무슨 학교냐",
          "original_form": "이게 무슨 학교냐",
          "speaker_id": "1",
          "time": "20210819 18:18"
        },
        {
          "id": "MDRW2100001698.1.30",
          "form": "비대면 너무 싫다",
          "original_form": "비대면 너무 싫다",
          "speaker_id": "1",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001698.1.31",
          "form": "ㅠㅜ그니까",
          "original_form": "ㅠㅜ그니까",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001698.1.32",
          "form": "근데 또 편해서",
          "original_form": "근데 또 편해서",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001698.1.33",
          "form": "비대면 하고싶긴함",
          "original_form": "비대면 하고싶긴함",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001698.1.34",
          "form": "ㅋㅋㅋ모순이지?",
          "original_form": "ㅋㅋㅋ모순이지?",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001698.1.35",
          "form": "엉 완전 모순순",
          "original_form": "엉 완전 모순순",
          "speaker_id": "1",
          "time": "20210819 18:19"
        }
      ]
