        },
        {
          "id": "MDRW2100020097.1.2",
          "form": "저오늘 퇴근 30분 일찍햇습니다",
          "original_form": "저오늘 퇴근 30분 일찍햇습니다",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020097.1.3",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020097.1.4",
          "form": "승자시네요ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "승자시네요ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020097.1.5",
          "form": "재택근무는 왜 안하는걸까요...ㅜ",
          "original_form": "재택근무는 왜 안하는걸까요...ㅜ",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다들 외근에 휴가에 그래서 5명밖에안남은거에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 다들 외근에 휴가에 그래서 5명밖에안남은거에요",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.7",
          "form": "팀장님 허락 하에 30분일찍퇴근햇습니다 ㅎㅎㅎㅎ",
          "original_form": "팀장님 허락 하에 30분일찍퇴근햇습니다 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.8",
          "form": "ㅋㅋㅋㅋㅋ오와...",
          "original_form": "ㅋㅋㅋㅋㅋ오와...",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.9",
          "form": "그니까요 !!!!",
          "original_form": "그니까요 !!!!",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.10",
          "form": "어차피 일 많아서 재택한다고 놀지도 못할텐데",
          "original_form": "어차피 일 많아서 재택한다고 놀지도 못할텐데",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.11",
          "form": "좀 시켜주지",
          "original_form": "좀 시켜주지",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.12",
          "form": "역시 금욜에는 칼퇴 해야해요!!",
          "original_form": "역시 금욜에는 칼퇴 해야해요!!",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.13",
          "form": "금욜에 붙잡는거 너무 시러요!!",
          "original_form": "금욜에 붙잡는거 너무 시러요!!",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020097.1.14",
          "form": "아니 다른회사들은",
          "original_form": "아니 다른회사들은",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020097.1.15",
          "form": "재택근무 다들 하던데",
          "original_form": "재택근무 다들 하던데",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020097.1.16",
          "form": "맞아요 금욜엔 저 5시부터 퇴근준비합니다",
          "original_form": "맞아요 금욜엔 저 5시부터 퇴근준비합니다",
          "speaker_id": "1",
          "time": "20210827 23:43"
