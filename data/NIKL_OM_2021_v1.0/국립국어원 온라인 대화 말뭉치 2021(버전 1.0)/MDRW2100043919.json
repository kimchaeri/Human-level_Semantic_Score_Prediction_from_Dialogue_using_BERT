      "utterance": [
        {
          "id": "MDRW2100043919.1.1",
          "form": "냉장보관하고 달콤한데 아이스크림은 아닌 디저트..!",
          "original_form": "냉장보관하고 달콤한데 아이스크림은 아닌 디저트..!",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043919.1.2",
          "form": "크기가 큰 디저트인가요?",
          "original_form": "크기가 큰 디저트인가요?",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043919.1.3",
          "form": "크기가 약간 사과랑 비슷해요!!",
          "original_form": "크기가 약간 사과랑 비슷해요!!",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043919.1.4",
          "form": "헉 사과.. 꽤 크군요!",
          "original_form": "헉 사과.. 꽤 크군요!",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043919.1.5",
          "form": "크림이 들어가나요?",
          "original_form": "크림이 들어가나요?",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043919.1.6",
          "form": "아뇨 크림은 들어가지 않습니다!",
          "original_form": "아뇨 크림은 들어가지 않습니다!",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043919.1.7",
          "form": "아 뭐지 뭐지 뭐지",
          "original_form": "아 뭐지 뭐지 뭐지",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043919.1.8",
          "form": "딱딱한 음식이예요?",
          "original_form": "딱딱한 음식이예요?",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043919.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아뇨!! 제가 먹고 싶은 건 말랑해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아뇨!! 제가 먹고 싶은 건 말랑해요",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043919.1.10",
          "form": "하지만 딱딱한 것도 있죠ㅎㅎ",
          "original_form": "하지만 딱딱한 것도 있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043919.1.11",
          "form": "다 드렸다",
          "original_form": "다 드렸다",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043919.1.12",
          "form": "아앗 혹시 과일인가요?",
          "original_form": "아앗 혹시 과일인가요?",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043919.1.13",
          "form": "과일입니다 >_ㅇ",
          "original_form": "과일입니다 >_ㅇ",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043919.1.14",
          "form": "정답!!!!!!! 감!!!",
          "original_form": "정답!!!!!!! 감!!!",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043919.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
