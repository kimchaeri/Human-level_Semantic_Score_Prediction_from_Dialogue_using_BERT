      "utterance": [
        {
          "id": "MDRW2100006066.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006066.1.2",
          "form": "스포츠 좋아하시나요?",
          "original_form": "스포츠 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006066.1.3",
          "form": "넵. 좋아해요.\n지금 축구하네요.",
          "original_form": "넵. 좋아해요.\n지금 축구하네요.",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006066.1.4",
          "form": "해외축구 보시는건가요?",
          "original_form": "해외축구 보시는건가요?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006066.1.5",
          "form": "아. 지금 월드컵 예선하잖아요.",
          "original_form": "아. 지금 월드컵 예선하잖아요.",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100006066.1.6",
          "form": "아 2022월드컵 예선이요?",
          "original_form": "아 2022월드컵 예선이요?",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100006066.1.7",
          "form": "넵. 카타르 월드컵요.",
          "original_form": "넵. 카타르 월드컵요.",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006066.1.8",
          "form": "잘 하나요??",
          "original_form": "잘 하나요??",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006066.1.9",
          "form": "레바논과 하는데 레바논이 약팀이에요.\n근데 날씨가 변수네요.",
          "original_form": "레바논과 하는데 레바논이 약팀이에요.\n근데 날씨가 변수네요.",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006066.1.10",
          "form": "아 카타르면 날씨가 많이 덥겠어요",
          "original_form": "아 카타르면 날씨가 많이 덥겠어요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100006066.1.11",
          "form": "지금은 우리나라 수원에서 경기하고 있어요.",
          "original_form": "지금은 우리나라 수원에서 경기하고 있어요.",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100006066.1.12",
          "form": "아 우리나라에서 하나요??",
          "original_form": "아 우리나라에서 하나요??",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006066.1.13",
          "form": "월드컵에 관심은 없는데",
          "original_form": "월드컵에 관심은 없는데",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006066.1.14",
          "form": "최근에 레바논이 잘해서",
          "original_form": "최근에 레바논이 잘해서",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006066.1.15",
          "form": "일본 이겼다는 얘기는 들은거같아요",
          "original_form": "일본 이겼다는 얘기는 들은거같아요",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006066.1.16",
          "form": "일본은 다른 조인데요.\n오늘 0시에 경기를 한다네요.",
          "original_form": "일본은 다른 조인데요.\n오늘 0시에 경기를 한다네요.",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006066.1.17",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006066.1.18",
          "form": "완전 다른 얘기였나보네요 ㅋㅋㅋ",
          "original_form": "완전 다른 얘기였나보네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006066.1.19",
          "form": "수원에 비가 많이 오나봐요.\n축구하는데 비가 많이 오네요.\n미끄러울텐데요.",
          "original_form": "수원에 비가 많이 오나봐요.\n축구하는데 비가 많이 오네요.\n미끄러울텐데요.",
          "speaker_id": "2",
          "time": "20210907 20:20"
        }
      ]
