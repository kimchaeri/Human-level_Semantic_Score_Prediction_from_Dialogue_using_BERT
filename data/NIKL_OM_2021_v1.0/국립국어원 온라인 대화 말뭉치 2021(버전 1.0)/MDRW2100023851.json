      "utterance": [
        {
          "id": "MDRW2100023851.1.1",
          "form": "안녕하세용 저녁 식사는 하셨어요~?",
          "original_form": "안녕하세용 저녁 식사는 하셨어요~?",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023851.1.2",
          "form": "네 일찍 먹었어요~~^^\n식사하셨어요?",
          "original_form": "네 일찍 먹었어요~~^^\n식사하셨어요?",
          "speaker_id": "2",
          "time": "20210830 19:09"
        },
        {
          "id": "MDRW2100023851.1.3",
          "form": "네네 저는 방금 카레 먹었어요ㅎㅎ",
          "original_form": "네네 저는 방금 카레 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:09"
        },
        {
          "id": "MDRW2100023851.1.4",
          "form": "어떤거 드셨나요~?",
          "original_form": "어떤거 드셨나요~?",
          "speaker_id": "1",
          "time": "20210830 19:09"
        },
        {
          "id": "MDRW2100023851.1.5",
          "form": "전 비빔밥이요.\n나물이 맛있어서 비빔밥이 생각나더라구요~^^",
          "original_form": "전 비빔밥이요.\n나물이 맛있어서 비빔밥이 생각나더라구요~^^",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023851.1.6",
          "form": "우와 비빔밥!",
          "original_form": "우와 비빔밥!",
          "speaker_id": "1",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023851.1.7",
          "form": "참기름파이신가요",
          "original_form": "참기름파이신가요",
          "speaker_id": "1",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023851.1.8",
          "form": "들기름파이신가요?",
          "original_form": "들기름파이신가요?",
          "speaker_id": "1",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023851.1.9",
          "form": "전 들기름이요!",
          "original_form": "전 들기름이요!",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023851.1.10",
          "form": "그 고소한 냄새는 정말 한그릇 뚝딱 비울 수 있더라구요!",
          "original_form": "그 고소한 냄새는 정말 한그릇 뚝딱 비울 수 있더라구요!",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023851.1.11",
          "form": "저도 들기름파에용!ㅎㅎ",
          "original_form": "저도 들기름파에용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100023851.1.12",
          "form": "무생채 먹으면 맛있어요ㅎㅎ",
          "original_form": "무생채 먹으면 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100023851.1.13",
          "form": "앗 반가워요!",
          "original_form": "앗 반가워요!",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100023851.1.14",
          "form": "아 무생채!\n무생채도 시도해봐야겠네요~!",
          "original_form": "아 무생채!\n무생채도 시도해봐야겠네요~!",
          "speaker_id": "2",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100023851.1.15",
          "form": "김가루는 넣어드시나용?",
          "original_form": "김가루는 넣어드시나용?",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100023851.1.16",
          "form": "전 조금 넣어먹는뎅!",
          "original_form": "전 조금 넣어먹는뎅!",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100023851.1.17",
          "form": "네 김가루 조금 깨소금도 넣어서 먹어요~!",
          "original_form": "네 김가루 조금 깨소금도 넣어서 먹어요~!",
          "speaker_id": "2",
          "time": "20210830 19:27"
        },
        {
          "id": "MDRW2100023851.1.18",
          "form": "저랑 입맛이 비슷하신것같아요~ㅎㅎㅎ",
          "original_form": "저랑 입맛이 비슷하신것같아요~ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:28"
        },
        {
          "id": "MDRW2100023851.1.19",
          "form": "비빔밥 땡기네요 갑자기ㅋㅋ",
          "original_form": "비빔밥 땡기네요 갑자기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:28"
        },
        {
          "id": "MDRW2100023851.1.20",
          "form": "내일은 비빔밥으로 드세요 \n들기름 넣어서~~^^",
          "original_form": "내일은 비빔밥으로 드세요 \n들기름 넣어서~~^^",
          "speaker_id": "2",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100023851.1.21",
          "form": "네넵ㅋㅋㅋ 나물하려면",
          "original_form": "네넵ㅋㅋㅋ 나물하려면",
          "speaker_id": "1",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100023851.1.22",
          "form": "살짝 일이겠네요ㅠㅜㅋㅋ",
          "original_form": "살짝 일이겠네요ㅠㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100023851.1.23",
          "form": "불닭볶음면 좋아하세요?",
          "original_form": "불닭볶음면 좋아하세요?",
