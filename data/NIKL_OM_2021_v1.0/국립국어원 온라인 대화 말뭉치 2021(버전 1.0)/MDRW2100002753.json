      "utterance": [
        {
          "id": "MDRW2100002753.1.1",
          "form": "커피도 즐겨 드시나요?",
          "original_form": "커피도 즐겨 드시나요?",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002753.1.2",
          "form": "커피 좋아해요ㅋ",
          "original_form": "커피 좋아해요ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002753.1.3",
          "form": "약간신맛 나는 커피좋아해요~",
          "original_form": "약간신맛 나는 커피좋아해요~",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002753.1.4",
          "form": "저는 탄맛 강한거 빼곤 다 잘먹어요~",
          "original_form": "저는 탄맛 강한거 빼곤 다 잘먹어요~",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002753.1.5",
          "form": "커피는 원두부터 너무 다양하더라구요",
          "original_form": "커피는 원두부터 너무 다양하더라구요",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002753.1.6",
          "form": "네 저는근데 막입이라 그냥 다 먹어요ㅋㅋ딱히 가리지는. 않는",
          "original_form": "네 저는근데 막입이라 그냥 다 먹어요ㅋㅋ딱히 가리지는. 않는",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002753.1.7",
          "form": "저도 잘 구분할줄 몰라서 막 먹어요ㅋ",
          "original_form": "저도 잘 구분할줄 몰라서 막 먹어요ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002753.1.8",
          "form": "ㅋㅋㅋㅋ 그저 먹을게 있음에 감사",
          "original_form": "ㅋㅋㅋㅋ 그저 먹을게 있음에 감사",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002753.1.9",
          "form": "먹는거에 행복합니다ㅋㅋㅋ",
          "original_form": "먹는거에 행복합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002753.1.10",
          "form": "졸려서 달달구리로 커피 한잔 먹어야겠어요ㅠㅋㅋ",
          "original_form": "졸려서 달달구리로 커피 한잔 먹어야겠어요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002753.1.11",
          "form": "오후엔 단거죠ㅋㅋ",
          "original_form": "오후엔 단거죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002753.1.12",
          "form": "맞아요~ 아침에는 아메ㅋㅋ",
          "original_form": "맞아요~ 아침에는 아메ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002753.1.13",
          "form": "아침은 아메진하게먹어요ㅋㅋ",
          "original_form": "아침은 아메진하게먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002753.1.14",
          "form": "맞아요 잠이 깨야하니ㅋㅋㅋ",
          "original_form": "맞아요 잠이 깨야하니ㅋㅋㅋ",
