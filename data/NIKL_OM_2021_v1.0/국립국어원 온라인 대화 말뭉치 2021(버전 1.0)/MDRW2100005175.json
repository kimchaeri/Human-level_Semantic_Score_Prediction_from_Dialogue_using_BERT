      "utterance": [
        {
          "id": "MDRW2100005175.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005175.1.2",
          "form": "안녕하세요~ 오늘 name1님이 사시는 곳의 날씨는 어땠나요?",
          "original_form": "안녕하세요~ 오늘 &name1&님이 사시는 곳의 날씨는 어땠나요?",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005175.1.3",
          "form": "오늘 비가 엄청 왔어요",
          "original_form": "오늘 비가 엄청 왔어요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005175.1.4",
          "form": "지금은 선선해서 좋은 날씨네요",
          "original_form": "지금은 선선해서 좋은 날씨네요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005175.1.5",
          "form": "그쪽은 어떤가요?",
          "original_form": "그쪽은 어떤가요?",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005175.1.6",
          "form": "저도 비 많이 왔었어요!",
          "original_form": "저도 비 많이 왔었어요!",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005175.1.7",
          "form": "지금은 비 그쳐서 시원해요ㅎㅎ",
          "original_form": "지금은 비 그쳐서 시원해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005175.1.8",
          "form": "오늘 되게 전국적으로 비가 온 것 같더라구요",
          "original_form": "오늘 되게 전국적으로 비가 온 것 같더라구요",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005175.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005175.1.10",
          "form": "여름에 안온 비가 지금 내리는 것 같았어요",
          "original_form": "여름에 안온 비가 지금 내리는 것 같았어요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005175.1.11",
          "form": "비오는 날을 좋아하세요?",
          "original_form": "비오는 날을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005175.1.12",
          "form": "집에 있을 때 빗소리 듣는 건 괜찮은데, 제가 나가야하는 날에 오는 건 싫어요ㅠㅠ",
          "original_form": "집에 있을 때 빗소리 듣는 건 괜찮은데, 제가 나가야하는 날에 오는 건 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005175.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005175.1.14",
          "form": "저도 그래요 ㅠㅠ",
          "original_form": "저도 그래요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005175.1.15",
          "form": "책읽을때 듣는 빗소리는 너무 좋은데",
          "original_form": "책읽을때 듣는 빗소리는 너무 좋은데",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005175.1.16",
          "form": "너무 불퍈했어요 ㅠ",
          "original_form": "너무 불퍈했어요 ㅠ",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005175.1.17",
          "form": "우산 들고 다니는 것도 귀찮고 옷 젖는 것도 싫어서요ㅠㅠ",
          "original_form": "우산 들고 다니는 것도 귀찮고 옷 젖는 것도 싫어서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005175.1.18",
          "form": "공감해요 ㅠㅠ",
          "original_form": "공감해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005175.1.19",
          "form": "우산 이게 은근히 무겁죠",
          "original_form": "우산 이게 은근히 무겁죠",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005175.1.20",
          "form": "그럼 좋아하는 계절 따로 있나요?",
          "original_form": "그럼 좋아하는 계절 따로 있나요?",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005175.1.21",
          "form": "저는 가을 제일 좋아해요! 선선해서 좋더라구요~",
          "original_form": "저는 가을 제일 좋아해요! 선선해서 좋더라구요~",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005175.1.22",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005175.1.23",
          "form": "저는 봄이랑 가을이요!",
          "original_form": "저는 봄이랑 가을이요!",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005175.1.24",
          "form": "원래는 가을을 더 좋아했는데",
          "original_form": "원래는 가을을 더 좋아했는데",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005175.1.25",
          "form": "요즘은 봄애 꽃보는 것도 너무 매력적인것같아서요",
          "original_form": "요즘은 봄애 꽃보는 것도 너무 매력적인것같아서요",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005175.1.26",
          "form": "맞아요 봄에 꽃보는 거 참 좋죠ㅠㅠ",
          "original_form": "맞아요 봄에 꽃보는 거 참 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005175.1.27",
          "form": "요즘은 코로나때문에 꽃구경도 못갔어요",
          "original_form": "요즘은 코로나때문에 꽃구경도 못갔어요",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005175.1.28",
          "form": "어떤 꽃 좋아하세요?",
          "original_form": "어떤 꽃 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005175.1.29",
          "form": "저는 벚꽃구경이 참 좋더라고요",
          "original_form": "저는 벚꽃구경이 참 좋더라고요",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005175.1.30",
          "form": "프리지아도 좋아하긴 하지만 흔히 보이는 꽃은 아니니까요",
          "original_form": "프리지아도 좋아하긴 하지만 흔히 보이는 꽃은 아니니까요",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005175.1.31",
          "form": "아 나중에 겨울에는 동백꽃 보러도 가고싶어요",
          "original_form": "아 나중에 겨울에는 동백꽃 보러도 가고싶어요",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005175.1.32",
          "form": "프리지아 꽃말도 예뻐서 저도 좋아해요!",
          "original_form": "프리지아 꽃말도 예뻐서 저도 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005175.1.33",
          "form": "동백꽃도 진짜 좋죠ㅠㅠ 겨울에 다른 꽃들은 져있는데, 동백꽃은 푸르고 빨개서 더 눈에 띄는 것 같아요",
          "original_form": "동백꽃도 진짜 좋죠ㅠㅠ 겨울에 다른 꽃들은 져있는데, 동백꽃은 푸르고 빨개서 더 눈에 띄는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005175.1.34",
          "form": "오 꽃말은 기억안나는대",
          "original_form": "오 꽃말은 기억안나는대",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005175.1.35",
          "form": "뭐였죠?",
          "original_form": "뭐였죠?",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005175.1.36",
          "form": "저는 동백을 실제로 본적은 없거든요",
          "original_form": "저는 동백을 실제로 본적은 없거든요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005175.1.37",
          "form": "말씀들으니 보러 더 가야겠네요 ㅠㅠ",
          "original_form": "말씀들으니 보러 더 가야겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005175.1.38",
          "form": "여러가지 있는데 당신의 시작을 응원합니다요!",
          "original_form": "여러가지 있는데 당신의 시작을 응원합니다요!",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005175.1.39",
          "form": "오늘 일기예보보니까 다음주에 비올것 같대요ㅠㅠ",
          "original_form": "오늘 일기예보보니까 다음주에 비올것 같대요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005175.1.40",
          "form": "뜻이 너무 예쁘네요ㅠㅠ",
          "original_form": "뜻이 너무 예쁘네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005175.1.41",
          "form": "또 오려나여 ㅠ 산책이 힘들어지갰어료",
          "original_form": "또 오려나여 ㅠ 산책이 힘들어지갰어료",
          "speaker_id": "1",
          "time": "20210821 22:39"
        }
      ]
    }
