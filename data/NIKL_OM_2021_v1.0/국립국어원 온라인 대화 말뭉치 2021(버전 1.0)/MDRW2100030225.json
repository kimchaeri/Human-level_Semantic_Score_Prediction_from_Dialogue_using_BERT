      "utterance": [
        {
          "id": "MDRW2100030225.1.1",
          "form": "뮤지컬 보고싶어!!",
          "original_form": "뮤지컬 보고싶어!!",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030225.1.2",
          "form": "뮤지컬!!!!!",
          "original_form": "뮤지컬!!!!!",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030225.1.3",
          "form": "나두 ㅎㅎ",
          "original_form": "나두 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030225.1.4",
          "form": "뮤지컬 보러간적 잇어?",
          "original_form": "뮤지컬 보러간적 잇어?",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030225.1.5",
          "form": "재밌겠다..",
          "original_form": "재밌겠다..",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.6",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.7",
          "form": "몇살때?",
          "original_form": "몇살때?",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.8",
          "form": "학교에서 한 5번은본거같아",
          "original_form": "학교에서 한 5번은본거같아",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.9",
          "form": "중딩때만 5번",
          "original_form": "중딩때만 5번",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.10",
          "form": "중학교때 선생님들이 뮤지컬을 참 좋아하셨지",
          "original_form": "중학교때 선생님들이 뮤지컬을 참 좋아하셨지",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.11",
          "form": "뮤지컬 공연장같은데 가봣어?",
          "original_form": "뮤지컬 공연장같은데 가봣어?",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.12",
          "form": "당근이지",
          "original_form": "당근이지",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.13",
          "form": "오 ㅎㅎ",
          "original_form": "오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.14",
          "form": "뒤에 세트같은거있는데",
          "original_form": "뒤에 세트같은거있는데",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.15",
          "form": "신기했어",
          "original_form": "신기했어",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.16",
          "form": "뮤지컬은 확실히 영상으로 보는거랑",
          "original_form": "뮤지컬은 확실히 영상으로 보는거랑",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.17",
          "form": "그리고 끝나고 막 사진찍어주시구",
          "original_form": "그리고 끝나고 막 사진찍어주시구",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.18",
          "form": "실물로 보는거랑 다르더라",
          "original_form": "실물로 보는거랑 다르더라",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.19",
          "form": "얘기같은거 했는데 되게 재밌었어",
          "original_form": "얘기같은거 했는데 되게 재밌었어",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.20",
          "form": "오 ㅋㅋ 좋왓겟다",
          "original_form": "오 ㅋㅋ 좋왓겟다",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.21",
          "form": "그렇더라구",
          "original_form": "그렇더라구",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030225.1.22",
          "form": "그리고 참여도가 되게 높더라",
          "original_form": "그리고 참여도가 되게 높더라",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.23",
          "form": "어땟는데?",
          "original_form": "어땟는데?",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.24",
          "form": "랜덤으로 한명씩 불러서",
          "original_form": "랜덤으로 한명씩 불러서",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.25",
          "form": "나와서 같이 춤춰",
          "original_form": "나와서 같이 춤춰",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.26",
          "form": "춤추거나 퀴즈같은거 풀고",
          "original_form": "춤추거나 퀴즈같은거 풀고",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.27",
          "form": "그랬어",
          "original_form": "그랬어",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.28",
          "form": "헐 ㅋㅋ 신기한 뮤지컬을 봣네",
          "original_form": "헐 ㅋㅋ 신기한 뮤지컬을 봣네",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030225.1.29",
          "form": "특이하당",
          "original_form": "특이하당",
          "speaker_id": "2",
          "time": "20210903 23:04"
        }
      ]
    }
