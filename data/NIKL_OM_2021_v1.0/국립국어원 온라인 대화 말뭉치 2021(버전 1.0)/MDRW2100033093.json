      "utterance": [
        {
          "id": "MDRW2100033093.1.1",
          "form": "요즘... 추억의 게임들이 빠짐",
          "original_form": "요즘... 추억의 게임들이 빠짐",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100033093.1.2",
          "form": "게임들에 빠짐",
          "original_form": "게임들에 빠짐",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100033093.1.3",
          "form": "슈게임 함",
          "original_form": "슈게임 함",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100033093.1.4",
          "form": "아... 좋지",
          "original_form": "아... 좋지",
          "speaker_id": "1",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100033093.1.5",
          "form": "어릴때는 많이 하지말라고 한 시간씩만 했는데",
          "original_form": "어릴때는 많이 하지말라고 한 시간씩만 했는데",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033093.1.6",
          "form": "옛날엔 컴퓨터 게임이 많이 나왔었는데",
          "original_form": "옛날엔 컴퓨터 게임이 많이 나왔었는데",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033093.1.7",
          "form": "요즘 대세는 모바일인듯?",
          "original_form": "요즘 대세는 모바일인듯?",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033093.1.8",
          "form": "맞아.... 근데 내가 너무 과거에 머물러 있어서 할 게 없음 ㅋ",
          "original_form": "맞아.... 근데 내가 너무 과거에 머물러 있어서 할 게 없음 ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033093.1.9",
          "form": "인기 게임... 오버워치가 아직 인기 게임이라는 것이 놀라운",
          "original_form": "인기 게임... 오버워치가 아직 인기 게임이라는 것이 놀라운",
          "speaker_id": "2",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033093.1.10",
          "form": "뭔가 적응이 안 된다고 해야하나",
          "original_form": "뭔가 적응이 안 된다고 해야하나",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100033093.1.11",
          "form": "그렇게 다들... 과거의 망령이 돼 버린 거지",
          "original_form": "그렇게 다들... 과거의 망령이 돼 버린 거지",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100033093.1.12",
          "form": "한국 모바일 게임은 과금 유도가 심해서 패치 때마다 적응 안 됨..",
          "original_form": "한국 모바일 게임은 과금 유도가 심해서 패치 때마다 적응 안 됨..",
          "speaker_id": "1",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100033093.1.13",
          "form": "아 진짜임 나 쿠키런 오븐브레이크 패치 넘 노골적이라서 놀랐음",
          "original_form": "아 진짜임 나 쿠키런 오븐브레이크 패치 넘 노골적이라서 놀랐음",
          "speaker_id": "2",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100033093.1.14",
          "form": "그런 팀전 경쟁 게임이 인기긴 하지",
          "original_form": "그런 팀전 경쟁 게임이 인기긴 하지",
          "speaker_id": "1",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100033093.1.15",
          "form": "그래서 애들 부모님카드 결제 막으려고 환불 규정까지 있고",
          "original_form": "그래서 애들 부모님카드 결제 막으려고 환불 규정까지 있고",
          "speaker_id": "1",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100033093.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 애기들....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 애기들....",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100033093.1.17",
          "form": "배그 아직 살아 있나?",
          "original_form": "배그 아직 살아 있나?",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100033093.1.18",
          "form": "아직 팔팔한데 핵이 많지",
          "original_form": "아직 팔팔한데 핵이 많지",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100033093.1.19",
          "form": "배그도 컴이랑 모바일 다르잖아",
          "original_form": "배그도 컴이랑 모바일 다르잖아",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100033093.1.20",
          "form": "맞아 모바일 배그 다시 시작할까 생각 중",
          "original_form": "맞아 모바일 배그 다시 시작할까 생각 중",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100033093.1.21",
          "form": "요즘 시간 죽일 게 넘 없음 ㅋ",
          "original_form": "요즘 시간 죽일 게 넘 없음 ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033093.1.22",
          "form": "베이컨 게임이나 하고....",
          "original_form": "베이컨 게임이나 하고....",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033093.1.23",
          "form": "나는 뭐 만드는 게임... 좀 평화로운",
          "original_form": "나는 뭐 만드는 게임... 좀 평화로운",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033093.1.24",
          "form": "피자 만들기 같은 그런 종류",
          "original_form": "피자 만들기 같은 그런 종류",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033093.1.25",
          "form": "ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ",
          "original_form": "ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033093.1.26",
          "form": "나도 그거 할까 피자 게임",
          "original_form": "나도 그거 할까 피자 게임",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033093.1.27",
          "form": "설치해야겠다",
          "original_form": "설치해야겠다",
          "speaker_id": "2",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033093.1.28",
          "form": "멸치까지 나왔더라 토핑이",
          "original_form": "멸치까지 나왔더라 토핑이",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033093.1.29",
          "form": "완전 웃겨 정어리 피자냐고",
          "original_form": "완전 웃겨 정어리 피자냐고",
          "speaker_id": "2",
