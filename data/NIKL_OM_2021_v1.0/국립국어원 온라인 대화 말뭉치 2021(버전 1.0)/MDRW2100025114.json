      "utterance": [
        {
          "id": "MDRW2100025114.1.1",
          "form": "아니근데 웨하스는 로아커를말하는건가?",
          "original_form": "아니근데 웨하스는 로아커를말하는건가?",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025114.1.2",
          "form": "응응!ㅋㅋ로아커ㅋㅋㅋㅋ",
          "original_form": "응응!ㅋㅋ로아커ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025114.1.3",
          "form": "로아커 거의 다먹음ㅋㅋㅋㅋ....",
          "original_form": "로아커 거의 다먹음ㅋㅋㅋㅋ....",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100025114.1.4",
          "form": "나도ㅋㅋ나 빨간색 아껴먹는중이야..",
          "original_form": "나도ㅋㅋ나 빨간색 아껴먹는중이야..",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100025114.1.5",
          "form": "너 원래 남색이 최애라 그러지 않았음? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너 원래 남색이 최애라 그러지 않았음? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100025114.1.6",
          "form": "아니아니 빨간색!ㅋㅋ",
          "original_form": "아니아니 빨간색!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100025114.1.7",
          "form": "아니야 너 남색이라그랬어!",
          "original_form": "아니야 너 남색이라그랬어!",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100025114.1.8",
          "form": "초코맛이 찐하다고 좋다며!",
          "original_form": "초코맛이 찐하다고 좋다며!",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025114.1.9",
          "form": "남색을 배반한거야?",
          "original_form": "남색을 배반한거야?",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025114.1.10",
          "form": "아니그게 빨간맛이었어....",
          "original_form": "아니그게 빨간맛이었어....",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100025114.1.11",
          "form": "남색먹었는데 그맛이아녔어ㅜㅜ",
          "original_form": "남색먹었는데 그맛이아녔어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100025114.1.12",
          "form": "뭐여 ㅋㅋㅋㅋ 역시 빨강이 최고지ㅡ",
          "original_form": "뭐여 ㅋㅋㅋㅋ 역시 빨강이 최고지ㅡ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100025114.1.13",
          "form": "이거 약간 그거잖아 누텔라",
          "original_form": "이거 약간 그거잖아 누텔라",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100025114.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100025114.1.15",
          "form": "ㅇㅇ맞아ㅋㅋ돗자리가 생각보다 커ㅋㅋ",
          "original_form": "ㅇㅇ맞아ㅋㅋ돗자리가 생각보다 커ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100025114.1.16",
          "form": "ㅋㅋㅋㅋ사은품이라 받긴햇능데 쓸데있나",
          "original_form": "ㅋㅋㅋㅋ사은품이라 받긴햇능데 쓸데있나",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100025114.1.17",
          "form": "우린 차에 뒀다 산소가면 쓸라고 ㅋㅋ",
          "original_form": "우린 차에 뒀다 산소가면 쓸라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100025114.1.18",
          "form": "야 나 갑자기 그거먹고싶음 샐러드빵 ㅋㅋㅋㅋㅋㅋ뭔지아나 옛날 매점에 팔던거",
          "original_form": "야 나 갑자기 그거먹고싶음 샐러드빵 ㅋㅋㅋㅋㅋㅋ뭔지아나 옛날 매점에 팔던거",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100025114.1.19",
          "form": "신세계 푸드몰에 쿠폰준다 만원이상구매시 만원 할인",
          "original_form": "신세계 푸드몰에 쿠폰준다 만원이상구매시 만원 할인",
          "speaker_id": "1",
