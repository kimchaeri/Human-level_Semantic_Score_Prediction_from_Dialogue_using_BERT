      "utterance": [
        {
          "id": "MDRW2100028795.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028795.1.2",
          "form": "좋아하는데 많이는 못 다녀봤어요",
          "original_form": "좋아하는데 많이는 못 다녀봤어요",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028795.1.3",
          "form": "기억에남는여행은요?",
          "original_form": "기억에남는여행은요?",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028795.1.4",
          "form": "저는 하와이죠!!",
          "original_form": "저는 하와이죠!!",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028795.1.5",
          "form": "신혼여행인가요? ㅎ",
          "original_form": "신혼여행인가요? ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028795.1.6",
          "form": "언니가있어서 자주 갔는데 갈때마다 돌아오고 싶지않았답니다",
          "original_form": "언니가있어서 자주 갔는데 갈때마다 돌아오고 싶지않았답니다",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028795.1.7",
          "form": "쇼핑과 날씨가 환상이예요",
          "original_form": "쇼핑과 날씨가 환상이예요",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028795.1.8",
          "form": "와 부럽네요 언젠가 가고프네요\r\n주로 무슨 쇼핑하세요?",
          "original_form": "와 부럽네요 언젠가 가고프네요\r\n주로 무슨 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028795.1.9",
          "form": "아울렛이죠!!",
          "original_form": "아울렛이죠!!",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028795.1.10",
          "form": "코치나 토리버치 나이키등",
          "original_form": "코치나 토리버치 나이키등",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028795.1.11",
          "form": "큰아울렛이 유명하죠",
          "original_form": "큰아울렛이 유명하죠",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028795.1.12",
          "form": "여행의 또 다른즐거움은 맛난음식이지요",
          "original_form": "여행의 또 다른즐거움은 맛난음식이지요",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028795.1.13",
          "form": "마자요!! 저는 내일 제주도 떠나려고 짐싸고 있어요 ㅎ",
          "original_form": "마자요!! 저는 내일 제주도 떠나려고 짐싸고 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028795.1.14",
          "form": "맛집아는곳 있으세요?",
          "original_form": "맛집아는곳 있으세요?",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028795.1.15",
          "form": "자유로운 영혼이시군요\r\n십오년전이라 많이 바뀌었겠네요\r\n흑돼지구이 신선한 회 등",
          "original_form": "자유로운 영혼이시군요\r\n십오년전이라 많이 바뀌었겠네요\r\n흑돼지구이 신선한 회 등",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028795.1.16",
          "form": "요즘은 정말 외국다녀온것처럼 좋답니다",
          "original_form": "요즘은 정말 외국다녀온것처럼 좋답니다",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028795.1.17",
          "form": "회와 흑돼지는 변함없는 메뉴네요!",
          "original_form": "회와 흑돼지는 변함없는 메뉴네요!",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028795.1.18",
          "form": "즐건추억 많이만들고 오세요\r\n음식 맛있게 드시고 건강한여행요 ㅎ",
          "original_form": "즐건추억 많이만들고 오세요\r\n음식 맛있게 드시고 건강한여행요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100028795.1.19",
          "form": "감사합니다!! 제주도 다녀오시길 꼭 추천합니다",
