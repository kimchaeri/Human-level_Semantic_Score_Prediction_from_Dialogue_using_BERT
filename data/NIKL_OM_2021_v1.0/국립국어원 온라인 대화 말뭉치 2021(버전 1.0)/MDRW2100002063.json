        {
          "id": "MDRW2100002063.1.1",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002063.1.2",
          "form": "네! 여행 좋아해요",
          "original_form": "네! 여행 좋아해요",
          "speaker_id": "2",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002063.1.3",
          "form": "코로나끝나면 당장해야죠",
          "original_form": "코로나끝나면 당장해야죠",
          "speaker_id": "2",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002063.1.4",
          "form": "거리두기 4단계 연장이라는 뉴스속보ㅠㅠ",
          "original_form": "거리두기 4단계 연장이라는 뉴스속보ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002063.1.5",
          "form": "예전 여행은 어디가셨나요(?)",
          "original_form": "예전 여행은 어디가셨나요(?)",
          "speaker_id": "1",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002063.1.6",
          "form": "부산놀러갔어요",
          "original_form": "부산놀러갔어요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100002063.1.7",
          "form": "부산 해운대(?)",
          "original_form": "부산 해운대(?)",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002063.1.8",
          "form": "네맞아요!!",
          "original_form": "네맞아요!!",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100002063.1.9",
          "form": "근처에 아쿠아리움도 놀러갔죠",
          "original_form": "근처에 아쿠아리움도 놀러갔죠",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100002063.1.10",
          "form": "아 ~ 맞다 거기 아쿠아리움저도 한번가봤네요",
          "original_form": "아 ~ 맞다 거기 아쿠아리움저도 한번가봤네요",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100002063.1.11",
          "form": "맞아요! 거기 좋아요",
          "original_form": "맞아요! 거기 좋아요",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100002063.1.12",
          "form": "부산은 꼼장어",
          "original_form": "부산은 꼼장어",
          "speaker_id": "1",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100002063.1.13",
          "form": "부산은 바다죠!!",
          "original_form": "부산은 바다죠!!",
          "speaker_id": "2",
          "time": "20210824 07:57"
        },
        {
          "id": "MDRW2100002063.1.14",
          "form": "꼼장어먹으면서 바다구경",
          "original_form": "꼼장어먹으면서 바다구경",
          "speaker_id": "2",
          "time": "20210824 07:58"
        },
        {
          "id": "MDRW2100002063.1.15",
          "form": "오~~ 부산바다 가고싶다",
          "original_form": "오~~ 부산바다 가고싶다",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100002063.1.16",
          "form": "부산바다 좋죠!!!",
          "original_form": "부산바다 좋죠!!!",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100002063.1.17",
          "form": "해운대놀러가봤어요",
          "original_form": "해운대놀러가봤어요",
