      "utterance": [
        {
          "id": "MDRW2100008217.1.1",
          "form": "군대는",
          "original_form": "군대는",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008217.1.2",
          "form": "무슨이야기를 해야할까요 ㅋㅋㅋ",
          "original_form": "무슨이야기를 해야할까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008217.1.3",
          "form": "아는게 없는데 음",
          "original_form": "아는게 없는데 음",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008217.1.4",
          "form": "군대는..",
          "original_form": "군대는..",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008217.1.5",
          "form": "제 신랑은 해병대나왓대요ㅋ",
          "original_form": "제 신랑은 해병대나왓대요ㅋ",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008217.1.6",
          "form": "저희 아버지도 해병대 나오셨다고하셨었죠 ㅋㅋ빨리 다음이야기 물어봐주세요 ㅋㅋㅋ",
          "original_form": "저희 아버지도 해병대 나오셨다고하셨었죠 ㅋㅋ빨리 다음이야기 물어봐주세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008217.1.7",
          "form": "ㅋㅋ그런데요?",
          "original_form": "ㅋㅋ그런데요?",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008217.1.8",
          "form": "엄마는 실감나는 이야기를 믿고 그든든함에 반하셨다고",
          "original_form": "엄마는 실감나는 이야기를 믿고 그든든함에 반하셨다고",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008217.1.9",
          "form": "그런데 반전이 있었다는 ㅋㅋㅋ",
          "original_form": "그런데 반전이 있었다는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100008217.1.10",
          "form": "ㅋㅋ반전은 뭐죠?",
          "original_form": "ㅋㅋ반전은 뭐죠?",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100008217.1.11",
          "form": "사실 방위셨대요 흔히 말하는 4급이요 ㅋㅋㅋ",
          "original_form": "사실 방위셨대요 흔히 말하는 4급이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100008217.1.12",
          "form": "ㅋㅋ아 해병대는 어디로..",
          "original_form": "ㅋㅋ아 해병대는 어디로..",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100008217.1.13",
          "form": "제 신랑도 해병대 별거아닌거처럼 얘기하는데 부심이 잇더라구요",
          "original_form": "제 신랑도 해병대 별거아닌거처럼 얘기하는데 부심이 잇더라구요",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.14",
          "form": "아버지 집안 모든분들은 해병대출신이세요",
          "original_form": "아버지 집안 모든분들은 해병대출신이세요",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.15",
          "form": "은근 해병대 따짐ㅋㅋ",
          "original_form": "은근 해병대 따짐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.16",
          "form": "할아버지 큰아버지",
          "original_form": "할아버지 큰아버지",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.17",
          "form": "저희 시아버지도 해병대시래여",
          "original_form": "저희 시아버지도 해병대시래여",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.18",
          "form": "심지어 제 친가사촌들도",
          "original_form": "심지어 제 친가사촌들도",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.19",
          "form": "생각보다 많은가봐요",
          "original_form": "생각보다 많은가봐요",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.20",
          "form": "그 부심 알거같아요 ㅋㅋ",
          "original_form": "그 부심 알거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.21",
          "form": "근데 수영은 잘 못하는거같던데",
          "original_form": "근데 수영은 잘 못하는거같던데",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100008217.1.22",
          "form": "자존심은 높고",
          "original_form": "자존심은 높고",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100008217.1.23",
          "form": "근데 아빤 눈이 안좋으셔서 면제가되신거죠",
          "original_form": "근데 아빤 눈이 안좋으셔서 면제가되신거죠",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100008217.1.24",
          "form": "대박인 시절이죠? 인력이남아서 평발이나 눈나빠도 4급 뜨던시절 ㅋㅋ",
