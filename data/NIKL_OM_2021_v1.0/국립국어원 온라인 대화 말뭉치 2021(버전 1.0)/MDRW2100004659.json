          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004659.1.7",
          "form": "유명 관광명소 다 돌고싶네요",
          "original_form": "유명 관광명소 다 돌고싶네요",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004659.1.8",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004659.1.9",
          "form": "국내 유명지도 가고 싶어요",
          "original_form": "국내 유명지도 가고 싶어요",
          "speaker_id": "2",
          "time": "20210821 18:36"
        },
        {
