        },
        {
          "id": "MDRW2100004659.1.2",
          "form": "여행 별로 안좋아했는데 코로나되니 못가서",
          "original_form": "여행 별로 안좋아했는데 코로나되니 못가서",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004659.1.3",
          "form": "이젠 여행 가고싶어요",
          "original_form": "이젠 여행 가고싶어요",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004659.1.4",
          "form": "저도 그러고 싶은데",
          "original_form": "저도 그러고 싶은데",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004659.1.5",
          "form": "해외여행 가고싶어요",
          "original_form": "해외여행 가고싶어요",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004659.1.6",
          "form": "저도 유럽",
