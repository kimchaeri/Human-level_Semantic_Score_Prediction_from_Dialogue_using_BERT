        {
          "id": "MDRW2100046755.1.1",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20211104 12:25"
        },
        {
          "id": "MDRW2100046755.1.2",
          "form": "안녕하세요! 좋은 아침이네요 ",
          "original_form": "안녕하세요! 좋은 아침이네요 {emoji:🌞}",
          "speaker_id": "2",
          "time": "20211104 08:55"
        },
        {
          "id": "MDRW2100046755.1.3",
          "form": "반갑네요",
          "original_form": "반갑네요",
          "speaker_id": "1",
          "time": "20211104 11:44"
        },
        {
          "id": "MDRW2100046755.1.4",
          "form": "점심은 드셨나요~?",
          "original_form": "점심은 드셨나요~?",
          "speaker_id": "2",
          "time": "20211104 11:44"
        },
        {
          "id": "MDRW2100046755.1.5",
          "form": "거를려고요",
          "original_form": "거를려고요",
          "speaker_id": "1",
          "time": "20211104 11:44"
        },
        {
          "id": "MDRW2100046755.1.6",
          "form": "아침을 늦게 먹어서요",
          "original_form": "아침을 늦게 먹어서요",
          "speaker_id": "1",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.7",
          "form": "헉 다이어트는 아니죠?",
          "original_form": "헉 다이어트는 아니죠?",
          "speaker_id": "2",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.8",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.9",
          "form": "많이 먹었어요.",
          "original_form": "많이 먹었어요.",
          "speaker_id": "1",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.10",
          "form": "너무 아침을",
          "original_form": "너무 아침을",
          "speaker_id": "1",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.11",
          "form": "아 ㅎㅎ 원래 아침을 드시나요?",
          "original_form": "아 ㅎㅎ 원래 아침을 드시나요?",
          "speaker_id": "2",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.12",
          "form": "요새들어서 먹고 있어요",
          "original_form": "요새들어서 먹고 있어요",
          "speaker_id": "1",
          "time": "20211104 11:45"
        },
        {
          "id": "MDRW2100046755.1.13",
          "form": "와 아침 먹는 게 건강한 습관이래요!",
          "original_form": "와 아침 먹는 게 건강한 습관이래요!",
          "speaker_id": "2",
          "time": "20211104 11:46"
        },
        {
          "id": "MDRW2100046755.1.14",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20211104 11:46"
        },
        {
          "id": "MDRW2100046755.1.15",
          "form": "떡볶이 먹었어요",
          "original_form": "떡볶이 먹었어요",
          "speaker_id": "1",
          "time": "20211104 11:46"
        },
        {
          "id": "MDRW2100046755.1.16",
          "form": "튀김하고요",
          "original_form": "튀김하고요",
          "speaker_id": "1",
          "time": "20211104 11:46"
        },
        {
          "id": "MDRW2100046755.1.17",
          "form": "어재 먹다가 남은거 냉동실어 얼려",
          "original_form": "어재 먹다가 남은거 냉동실어 얼려",
          "speaker_id": "1",
          "time": "20211104 11:46"
        },
        {
          "id": "MDRW2100046755.1.18",
          "form": "두었다가요",
          "original_form": "두었다가요",
          "speaker_id": "1",
          "time": "20211104 11:46"
        },
        {
          "id": "MDRW2100046755.1.19",
          "form": "해동해서 먹었어요",
          "original_form": "해동해서 먹었어요",
          "speaker_id": "1",
          "time": "20211104 11:47"
        },
        {
          "id": "MDRW2100046755.1.20",
          "form": "헐 아침부터 떡볶이!",
          "original_form": "헐 아침부터 떡볶이!",
          "speaker_id": "2",
          "time": "20211104 11:49"
        },
        {
