      "utterance": [
        {
          "id": "MDRW2100044554.1.1",
          "form": "회 조아하세요?",
          "original_form": "회 조아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044554.1.2",
          "form": "녜 없어서못먹져",
          "original_form": "녜 없어서못먹져",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044554.1.3",
          "form": "어떤회 저아하세여",
          "original_form": "어떤회 저아하세여",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044554.1.4",
          "form": "연어도 좋고요",
          "original_form": "연어도 좋고요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044554.1.5",
          "form": "크 광어는요",
          "original_form": "크 광어는요",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044554.1.6",
          "form": "그건 기본으로 좋고요ㅋㄱㄲ",
          "original_form": "그건 기본으로 좋고요ㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044554.1.7",
          "form": "ㅋㅋㅋㅋ 먹고싶네요 당장",
          "original_form": "ㅋㅋㅋㅋ 먹고싶네요 당장",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044554.1.8",
          "form": "전어 드시나요?",
          "original_form": "전어 드시나요?",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044554.1.9",
          "form": "네 좋아해요ㅋㅋ",
          "original_form": "네 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044554.1.10",
          "form": "참치는 먹긴하는데 그닥",
          "original_form": "참치는 먹긴하는데 그닥",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044554.1.11",
          "form": "ㅋㅋㅋ 전어 구이랑 회 다 드시나요",
          "original_form": "ㅋㅋㅋ 전어 구이랑 회 다 드시나요",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044554.1.12",
          "form": "회는 세꼬시라 고난도죠",
          "original_form": "회는 세꼬시라 고난도죠",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044554.1.13",
          "form": "ㅋㅋㅋ 그게또 맛잇지않나요",
          "original_form": "ㅋㅋㅋ 그게또 맛잇지않나요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044554.1.14",
          "form": "전 그건아직 극복못햇어요ㅋㅋ",
          "original_form": "전 그건아직 극복못햇어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044554.1.15",
          "form": "아쉽군요 ㅋㅋ 곧 대하도 철이에요",
          "original_form": "아쉽군요 ㅋㅋ 곧 대하도 철이에요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044554.1.16",
          "form": "네 한번 먹어야겠네요",
          "original_form": "네 한번 먹어야겠네요",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044554.1.17",
          "form": "대하도 맛있죠",
          "original_form": "대하도 맛있죠",
          "speaker_id": "1",
          "time": "20210907 23:21"
        }
      ]
