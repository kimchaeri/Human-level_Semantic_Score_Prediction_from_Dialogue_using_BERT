      "utterance": [
        {
          "id": "MDRW2100022280.1.1",
          "form": "그정도로 작진 않았어요 새우가...",
          "original_form": "그정도로 작진 않았어요 새우가...",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022280.1.2",
          "form": "새우중에 탈각새우있는데 다이몬드 새우라고",
          "original_form": "새우중에 탈각새우있는데 다이몬드 새우라고",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.3",
          "form": "아 그거 알아요",
          "original_form": "아 그거 알아요",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.4",
          "form": "그게 가장 가성비넘칩니다",
          "original_form": "그게 가장 가성비넘칩니다",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.5",
          "form": "아시는구나",
          "original_form": "아시는구나",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.6",
          "form": "맞아요 저 그거 자주먹었어요 ㅋㅋ",
          "original_form": "맞아요 저 그거 자주먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.7",
          "form": "그 무슨 상자에 들어있는거 맞죠",
          "original_form": "그 무슨 상자에 들어있는거 맞죠",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.8",
          "form": "뭔가 맛잘알의 냄새가나시네",
          "original_form": "뭔가 맛잘알의 냄새가나시네",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.9",
          "form": "그거 진짜 최고죠",
          "original_form": "그거 진짜 최고죠",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.10",
          "form": "아니요 봉지에 들어있어요",
          "original_form": "아니요 봉지에 들어있어요",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.11",
          "form": "봉지도 있어요?",
          "original_form": "봉지도 있어요?",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.12",
          "form": "ㅋㅋㅋ 우리서로 다른 새우를 말하고있는건가",
          "original_form": "ㅋㅋㅋ 우리서로 다른 새우를 말하고있는건가",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.13",
          "form": "아 봉지로된것도 최고에요",
          "original_form": "아 봉지로된것도 최고에요",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.14",
          "form": "다이아몬드에서 상자로 된것도 팔거든요",
          "original_form": "다이아몬드에서 상자로 된것도 팔거든요",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.15",
          "form": "사이즈 34/26",
          "original_form": "사이즈 34/26",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.16",
          "form": "이게 제일..",
          "original_form": "이게 제일..",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022280.1.17",
          "form": "ㅠㅠ저희 동네는 왜 안팔까요 그런거",
          "original_form": "ㅠㅠ저희 동네는 왜 안팔까요 그런거",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.18",
          "form": "상자로된것도 팔아요?",
          "original_form": "상자로된것도 팔아요?",
          "speaker_id": "1",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.19",
          "form": "인터넷은 맨날 녹아서 와요",
          "original_form": "인터넷은 맨날 녹아서 와요",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.20",
          "form": "네 근데 상자는 탈각아니고 껍질있어요",
          "original_form": "네 근데 상자는 탈각아니고 껍질있어요",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.21",
          "form": "저 이거 인터넷으로 샀..",
          "original_form": "저 이거 인터넷으로 샀..",
          "speaker_id": "1",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.22",
          "form": "드라이아이스 넣어줘요?",
          "original_form": "드라이아이스 넣어줘요?",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.23",
          "form": "아 껍질이 있구나",
          "original_form": "아 껍질이 있구나",
          "speaker_id": "1",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022280.1.24",
          "form": "아니요 깡깡 얼어서 오던데",
          "original_form": "아니요 깡깡 얼어서 오던데",
          "speaker_id": "1",
          "time": "20210829 12:01"
        }
      ]
