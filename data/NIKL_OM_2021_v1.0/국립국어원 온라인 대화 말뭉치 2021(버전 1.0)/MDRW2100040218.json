      "utterance": [
        {
          "id": "MDRW2100040218.1.1",
          "form": "연애하세요?",
          "original_form": "연애하세요?",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100040218.1.2",
          "form": "아뇨ㅎ",
          "original_form": "아뇨ㅎ",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040218.1.3",
          "form": "ㅜㅜㅋㅋㅋ맘이없으신가요",
          "original_form": "ㅜㅜㅋㅋㅋ맘이없으신가요",
          "speaker_id": "2",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040218.1.4",
          "form": "아 그건 아니지만",
          "original_form": "아 그건 아니지만",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100040218.1.5",
          "form": "최근에 헤어지기도 했고",
          "original_form": "최근에 헤어지기도 했고",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040218.1.6",
          "form": "당분간 저와 제 일에 집중하고 싶어서요ㅎㅎ",
          "original_form": "당분간 저와 제 일에 집중하고 싶어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040218.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040218.1.8",
          "form": "아하 안힘드시나요",
          "original_form": "아하 안힘드시나요",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040218.1.9",
          "form": "네 생각보단 잘지내고 있어요!",
          "original_form": "네 생각보단 잘지내고 있어요!",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100040218.1.10",
          "form": "다행이네요 저도 안해용 ㅎ",
          "original_form": "다행이네요 저도 안해용 ㅎ",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040218.1.11",
          "form": "집중해야죠 저도 저한테 ㅎㅎㅎㅎㅎㅎ",
          "original_form": "집중해야죠 저도 저한테 ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100040218.1.12",
          "form": "ㅎㅎㅎ좋은 생각이예요!",
          "original_form": "ㅎㅎㅎ좋은 생각이예요!",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040218.1.13",
          "form": "연애와 결혼이 꼭 필수는 아니니까요",
          "original_form": "연애와 결혼이 꼭 필수는 아니니까요",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100040218.1.14",
          "form": "마자요  얼마나 만나고 헤어지셧나요",
          "original_form": "마자요  얼마나 만나고 헤어지셧나요",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100040218.1.15",
          "form": "최근 연애는",
          "original_form": "최근 연애는",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040218.1.16",
          "form": "한 6개월 정도요",
          "original_form": "한 6개월 정도요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040218.1.17",
          "form": "아항 자만추?",
          "original_form": "아항 자만추?",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040218.1.18",
          "form": "그렇긴한데",
          "original_form": "그렇긴한데",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040218.1.19",
          "form": "요즘은 자연스러운 만남 자체가 힘들어서",
          "original_form": "요즘은 자연스러운 만남 자체가 힘들어서",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040218.1.20",
          "form": "ㅋㅋㅋ 글쵸 점점 만날사람이 없네요",
          "original_form": "ㅋㅋㅋ 글쵸 점점 만날사람이 없네요",
