      "utterance": [
        {
          "id": "MDRW2100045374.1.1",
          "form": "음 뚜레쥬르?",
          "original_form": "음 뚜레쥬르?",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045374.1.2",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045374.1.3",
          "form": "우리집밑에빵집와봤나",
          "original_form": "우리집밑에빵집와봤나",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045374.1.4",
          "form": "너무머나 ㅋㅋㅋㅋ",
          "original_form": "너무머나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045374.1.5",
          "form": "빵야도있다",
          "original_form": "빵야도있다",
          "speaker_id": "1",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045374.1.6",
          "form": "아 빵야!",
          "original_form": "아 빵야!",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045374.1.7",
          "form": "너느집밑은 안가봄ㅋㅋ",
          "original_form": "너느집밑은 안가봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045374.1.8",
          "form": "ㅋㅋㅋ 투어해라 빵집투어",
          "original_form": "ㅋㅋㅋ 투어해라 빵집투어",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045374.1.9",
          "form": "우리집밑에도 ㄱㅊ하다",
          "original_form": "우리집밑에도 ㄱㅊ하다",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045374.1.10",
          "form": "난사실 빵야보다 맛있다",
          "original_form": "난사실 빵야보다 맛있다",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045374.1.11",
          "form": "아그래??! 거기 빵종류많아?",
          "original_form": "아그래??! 거기 빵종류많아?",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045374.1.12",
          "form": "ㅇㅇ 빵야나 뚜쥬이런데랑 별로안겹쳐",
          "original_form": "ㅇㅇ 빵야나 뚜쥬이런데랑 별로안겹쳐",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045374.1.13",
          "form": "근데최근에는안가봐서모르겠네",
          "original_form": "근데최근에는안가봐서모르겠네",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045374.1.14",
          "form": "슈있어??",
          "original_form": "슈있어??",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045374.1.15",
          "form": "어 있어 ㅋㅋ베이비슈",
          "original_form": "어 있어 ㅋㅋ베이비슈",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045374.1.16",
          "form": "ㄱㅊ",
          "original_form": "ㄱㅊ",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045374.1.17",
          "form": "앗 좋아ㅋㅋㅋㅋ근데 너무 멀다",
          "original_form": "앗 좋아ㅋㅋㅋㅋ근데 너무 멀다",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045374.1.18",
          "form": "뭐가멀어 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐가멀어 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045374.1.19",
          "form": "진심 5분도안걸림ㅋㅋㅋ",
          "original_form": "진심 5분도안걸림ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045374.1.20",
          "form": "너무멀다...헐거짓말",
          "original_form": "너무멀다...헐거짓말",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.21",
          "form": "20분은걸림",
          "original_form": "20분은걸림",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.22",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.23",
          "form": "아뭔소리여 이십분이면 현대백화점도갈수있어",
          "original_form": "아뭔소리여 이십분이면 현대백화점도갈수있어",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.24",
          "form": "로티보이번먹고싶어 ㅜ",
          "original_form": "로티보이번먹고싶어 ㅜ",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.25",
          "form": "차타고?!ㅋㅋㅋ",
          "original_form": "차타고?!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.26",
          "form": "겉은달달 속은 짭쪼음",
          "original_form": "겉은달달 속은 짭쪼음",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045374.1.27",
          "form": "로티보이 이제없이?",
          "original_form": "로티보이 이제없이?",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.28",
          "form": "뛰어서 ㅋㅋㅋ",
          "original_form": "뛰어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.29",
          "form": "없어...",
          "original_form": "없어...",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.30",
          "form": "전국에 얼마안남음",
          "original_form": "전국에 얼마안남음",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.31",
          "form": "몇군데없을걸",
          "original_form": "몇군데없을걸",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.32",
          "form": "ㅜㅜㅜ제일가까운게 부산이넼ㅋ",
          "original_form": "ㅜㅜㅜ제일가까운게 부산이넼ㅋ",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.33",
          "form": "안퍽퍽해",
          "original_form": "안퍽퍽해",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045374.1.34",
          "form": "베이비슈 빵야에도 있을걸",
          "original_form": "베이비슈 빵야에도 있을걸",
          "speaker_id": "1",
          "time": "20210908 02:20"
        }
      ]
