      "utterance": [
        {
          "id": "MDRW2100020740.1.1",
          "form": "빵이 너무많아서ㅋㅋ",
          "original_form": "빵이 너무많아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020740.1.2",
          "form": "먹어야해요",
          "original_form": "먹어야해요",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020740.1.3",
          "form": "제가...",
          "original_form": "제가...",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.4",
          "form": "벌써..",
          "original_form": "벌써..",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.5",
          "form": "세번이나 물었군요..",
          "original_form": "세번이나 물었군요..",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.6",
          "form": "아닙니다",
          "original_form": "아닙니다",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.7",
          "form": "개의치않아요",
          "original_form": "개의치않아요",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.8",
          "form": "이 단기기억상실증..",
          "original_form": "이 단기기억상실증..",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.9",
          "form": "사람의 기억력은 단기적인거니까요",
          "original_form": "사람의 기억력은 단기적인거니까요",
          "speaker_id": "2",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.10",
          "form": "점심메뉴가 정해져있는 기분은",
          "original_form": "점심메뉴가 정해져있는 기분은",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.11",
          "form": "어떤기분인가요",
          "original_form": "어떤기분인가요",
          "speaker_id": "1",
          "time": "20210828 11:55"
        },
        {
          "id": "MDRW2100020740.1.12",
          "form": "안정감있나요",
          "original_form": "안정감있나요",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.13",
          "form": "어떤기분이냐면",
          "original_form": "어떤기분이냐면",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.14",
          "form": "머릿속으로 그 맛이 상상되는",
          "original_form": "머릿속으로 그 맛이 상상되는",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.15",
          "form": "기분입니다",
          "original_form": "기분입니다",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ먹기 전부터",
          "original_form": "ㅋㅋㅋㅋㅋㅋ먹기 전부터",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.18",
          "form": "먹는 기분이 나겠군요",
          "original_form": "먹는 기분이 나겠군요",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.19",
          "form": "비빔밥 간장 vs 고추장",
          "original_form": "비빔밥 간장 vs 고추장",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.20",
          "form": "이미지트레이닝 겁나해요",
          "original_form": "이미지트레이닝 겁나해요",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.22",
          "form": "와 참기름만 넣으려 했는데..",
          "original_form": "와 참기름만 넣으려 했는데..",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020740.1.23",
          "form": "아 양념하는거",
          "original_form": "아 양념하는거",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.24",
          "form": "사실 집에 수제 비빔장이 있어요",
          "original_form": "사실 집에 수제 비빔장이 있어요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.25",
          "form": "안좋아하시는구나",
          "original_form": "안좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.26",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.27",
          "form": "수제라니",
          "original_form": "수제라니",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.28",
          "form": "대박적",
          "original_form": "대박적",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.29",
          "form": "잊고 있었는데 넣어볼까 싶군요",
          "original_form": "잊고 있었는데 넣어볼까 싶군요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.30",
          "form": "잊고있었는데... 저희집 비빔밥집도 하네요",
          "original_form": "잊고있었는데... 저희집 비빔밥집도 하네요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.31",
          "form": "앗 전 이만 점심을 먹으러가야겠습니니다..",
          "original_form": "앗 전 이만 점심을 먹으러가야겠습니니다..",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.32",
          "form": "너무부르네요",
          "original_form": "너무부르네요",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.33",
          "form": "맛저하십셔",
          "original_form": "맛저하십셔",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020740.1.34",
          "form": "앗ㅋㅋㅋㅋㅋㅋ 어서가보세요",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ 어서가보세요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        }
      ]
