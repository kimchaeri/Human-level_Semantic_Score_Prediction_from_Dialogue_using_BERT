      "utterance": [
        {
          "id": "MDRW2100010771.1.1",
          "form": "어느피자집인지 말해주세요",
          "original_form": "어느피자집인지 말해주세요",
          "speaker_id": "1",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010771.1.2",
          "form": "강남요?",
          "original_form": "강남요?",
          "speaker_id": "2",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010771.1.3",
          "form": "아뇨 지금먹는 피자집이요 ㅋㅋㅋㅋ",
          "original_form": "아뇨 지금먹는 피자집이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010771.1.4",
          "form": "앜ㅋㅋㅋㅋ 피자스톰이에요 여기 치즈가 맛있어요",
          "original_form": "앜ㅋㅋㅋㅋ 피자스톰이에요 여기 치즈가 맛있어요",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010771.1.5",
          "form": "새우튀김은 첨으로 추가해봤는데 괜찮네요",
          "original_form": "새우튀김은 첨으로 추가해봤는데 괜찮네요",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010771.1.6",
          "form": "대전은 없는거같네요ㅠㅋㅋㅋ 체인인가",
          "original_form": "대전은 없는거같네요ㅠㅋㅋㅋ 체인인가",
          "speaker_id": "1",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010771.1.7",
          "form": "네네 체인인데ㅠ",
          "original_form": "네네 체인인데ㅠ",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010771.1.8",
          "form": "대전 사시는구나",
          "original_form": "대전 사시는구나",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010771.1.9",
          "form": "강남 피자집은 브릭오븐 << 여기 였던 것 같아요",
          "original_form": "강남 피자집은 브릭오븐 << 여기 였던 것 같아요",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010771.1.10",
          "form": "오 손수 찾아주시고 ㅋㅋㅋ 꼭가볼게요",
          "original_form": "오 손수 찾아주시고 ㅋㅋㅋ 꼭가볼게요",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010771.1.11",
          "form": "네 대전살아요 대전은 맛집이 단한개도없음 ㅠ",
          "original_form": "네 대전살아요 대전은 맛집이 단한개도없음 ㅠ",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010771.1.12",
          "form": "대전은 성심당 아닌가요?",
          "original_form": "대전은 성심당 아닌가요?",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010771.1.13",
          "form": "대전사람은 성심당 안가요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대전사람은 성심당 안가요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010771.1.14",
          "form": "성심당가보면 다 타지사람들이에요",
          "original_form": "성심당가보면 다 타지사람들이에요",
          "speaker_id": "1",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010771.1.15",
          "form": "ㅋㅋㅋㅋ가봤어요",
          "original_form": "ㅋㅋㅋㅋ가봤어요",
          "speaker_id": "2",
          "time": "20210823 21:55"
        },
        {
          "id": "MDRW2100010771.1.16",
          "form": "근처에 큰 성당도 있었는것 같은데",
          "original_form": "근처에 큰 성당도 있었는것 같은데",
          "speaker_id": "2",
          "time": "20210823 21:55"
        },
        {
          "id": "MDRW2100010771.1.17",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210823 21:55"
        },
        {
          "id": "MDRW2100010771.1.18",
          "form": "대흥동성당 말하는건가 ㅋㅋㅋㅋ",
          "original_form": "대흥동성당 말하는건가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010771.1.19",
          "form": "제가그쪽동네를 잘안가가지구",
          "original_form": "제가그쪽동네를 잘안가가지구",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010771.1.20",
          "form": "성심당 샌드위치 진짜맛잇어요 인정",
          "original_form": "성심당 샌드위치 진짜맛잇어요 인정",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010771.1.21",
          "form": "아 맞아요! 그런 이름이었어요",
          "original_form": "아 맞아요! 그런 이름이었어요",
          "speaker_id": "2",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010771.1.22",
          "form": "샌드위치 맛집이었구나...",
          "original_form": "샌드위치 맛집이었구나...",
          "speaker_id": "2",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010771.1.23",
          "form": "부추빵 유명하대서 먹었는데 제스탈은 아니었던",
          "original_form": "부추빵 유명하대서 먹었는데 제스탈은 아니었던",
          "speaker_id": "2",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010771.1.24",
          "form": "부추빵줘도안먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "부추빵줘도안먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010771.1.25",
          "form": "튀소가 맛잇어요 근데? 그냥 기름많은 소보루맛인데",
          "original_form": "튀소가 맛잇어요 근데? 그냥 기름많은 소보루맛인데",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010771.1.26",
          "form": "아 그거 먹었어요",
          "original_form": "아 그거 먹었어요",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010771.1.27",
          "form": "괜찮던데요?",
          "original_form": "괜찮던데요?",
          "speaker_id": "2",
          "time": "20210823 22:14"
        },
        {
          "id": "MDRW2100010771.1.28",
          "form": "부추빵에 충격받아서 그런가",
          "original_form": "부추빵에 충격받아서 그런가",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010771.1.29",
          "form": "부추빵진짜 노맛이죠 ㅋㅋㅋ만두생각하면 오산이에요",
          "original_form": "부추빵진짜 노맛이죠 ㅋㅋㅋ만두생각하면 오산이에요",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010771.1.30",
          "form": "요즘엔 튀소구마라고 고구마맛도나왓는데",
          "original_form": "요즘엔 튀소구마라고 고구마맛도나왓는데",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010771.1.31",
          "form": "초코도잇고",
          "original_form": "초코도잇고",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010771.1.32",
          "form": "튀소가 젤나아요",
          "original_form": "튀소가 젤나아요",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010771.1.33",
          "form": "진짜 첨에 충격 먹었어요 이게 뭔... 빵에 부추계란국 넣은 느낌",
          "original_form": "진짜 첨에 충격 먹었어요 이게 뭔... 빵에 부추계란국 넣은 느낌",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010771.1.34",
          "form": "오 저 고구마 좋아합니다",
          "original_form": "오 저 고구마 좋아합니다",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010771.1.35",
          "form": "근데 튀소 느끼하긴 하던데요",
          "original_form": "근데 튀소 느끼하긴 하던데요",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010771.1.36",
          "form": "순수우유롤잇는데 그거맛잇어요 ㅋㅋㅋ",
          "original_form": "순수우유롤잇는데 그거맛잇어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010771.1.37",
          "form": "담에 꼭 먹어보세여",
          "original_form": "담에 꼭 먹어보세여",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010771.1.38",
          "form": "다음이 언제일진 모르겠지만",
          "original_form": "다음이 언제일진 모르겠지만",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010771.1.39",
          "form": "아! 맞다 대전에 그거 지금 할텐데",
          "original_form": "아! 맞다 대전에 그거 지금 할텐데",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010771.1.40",
          "form": "퓰리쳐사진전",
          "original_form": "퓰리쳐사진전",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010771.1.41",
          "form": "어....모르겟는데",
          "original_form": "어....모르겟는데",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010771.1.42",
          "form": "어디서한대요? ㅋㅋㅋㅋㅋ",
          "original_form": "어디서한대요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010771.1.43",
          "form": "대전시립미술관이래요",
          "original_form": "대전시립미술관이래요",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010771.1.44",
          "form": "저 대신 가주세요..",
          "original_form": "저 대신 가주세요..",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010771.1.45",
          "form": "원래 서울에서 하던건데",
          "original_form": "원래 서울에서 하던건데",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010771.1.46",
          "form": "아 시립미술관",
          "original_form": "아 시립미술관",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010771.1.47",
          "form": "코로나 확 커져가지고 못 갔거든요 휴관하고",
          "original_form": "코로나 확 커져가지고 못 갔거든요 휴관하고",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010771.1.48",
          "form": "시간되면 가보도록할게요",
          "original_form": "시간되면 가보도록할게요",
          "speaker_id": "1",
          "time": "20210823 22:39"
        }
      ]
    }
