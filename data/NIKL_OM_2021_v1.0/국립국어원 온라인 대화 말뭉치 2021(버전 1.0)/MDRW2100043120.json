      "utterance": [
        {
          "id": "MDRW2100043120.1.1",
          "form": "마라탕 기회되면 한번 먹어봐야겠네용",
          "original_form": "마라탕 기회되면 한번 먹어봐야겠네용",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043120.1.2",
          "form": "네ㅋㅋ배달로도 괜찮으니 매운거땡기실때",
          "original_form": "네ㅋㅋ배달로도 괜찮으니 매운거땡기실때",
          "speaker_id": "1",
          "time": "20210907 14:35"
        },
        {
          "id": "MDRW2100043120.1.3",
          "form": "시도해보세욥",
          "original_form": "시도해보세욥",
          "speaker_id": "1",
          "time": "20210907 14:35"
        },
        {
          "id": "MDRW2100043120.1.4",
          "form": "어디 추천하실 브랜드있으신가영 체인이면",
          "original_form": "어디 추천하실 브랜드있으신가영 체인이면",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043120.1.5",
          "form": "이게 지역마다",
          "original_form": "이게 지역마다",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043120.1.6",
          "form": "다른거같더라구요",
          "original_form": "다른거같더라구요",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043120.1.7",
          "form": "저희지역은 라쿵푸마라탕 유명해요",
          "original_form": "저희지역은 라쿵푸마라탕 유명해요",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043120.1.8",
          "form": "체인이네요 ㅋㅋ",
          "original_form": "체인이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043120.1.9",
          "form": "네ㅋㅋ점바점 심해유",
          "original_form": "네ㅋㅋ점바점 심해유",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043120.1.10",
          "form": "점바점....아..ㅠ 후기 꼭 보고 먹어볼깨여...",
          "original_form": "점바점....아..ㅠ 후기 꼭 보고 먹어볼깨여...",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043120.1.11",
          "form": "ㅎㅎ매운궁물 야채 잡탕이지만 맛잇어유",
          "original_form": "ㅎㅎ매운궁물 야채 잡탕이지만 맛잇어유",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043120.1.12",
          "form": "오 국물맛 기대되네여",
          "original_form": "오 국물맛 기대되네여",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043120.1.13",
          "form": "ㅎㅎ향신료 거부감없으시다면 추천!",
          "original_form": "ㅎㅎ향신료 거부감없으시다면 추천!",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043120.1.14",
          "form": "어떤향신료일지..ㅎㅎ",
          "original_form": "어떤향신료일지..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043120.1.15",
          "form": "ㅋㅋ후기가 궁금하네욤 히히",
          "original_form": "ㅋㅋ후기가 궁금하네욤 히히",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043120.1.16",
          "form": "아 근데 신랑이 새로운맛에 도전하는건 안 좋아해서 먹자고 하면 어떤반응일지.ㅋ",
          "original_form": "아 근데 신랑이 새로운맛에 도전하는건 안 좋아해서 먹자고 하면 어떤반응일지.ㅋ",
          "speaker_id": "2",
          "time": "20210907 15:14"
        }
      ]
