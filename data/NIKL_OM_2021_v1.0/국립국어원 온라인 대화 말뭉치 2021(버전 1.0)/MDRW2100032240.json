      "utterance": [
        {
          "id": "MDRW2100032240.1.1",
          "form": "시간표?",
          "original_form": "시간표?",
          "speaker_id": "2",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032240.1.2",
          "form": "뭐 나름 괜찮아",
          "original_form": "뭐 나름 괜찮아",
          "speaker_id": "2",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032240.1.3",
          "form": "화수목금은 1교시 수업이고",
          "original_form": "화수목금은 1교시 수업이고",
          "speaker_id": "2",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032240.1.4",
          "form": "월요일은 2교시에 전부다 실시간 강의에 화면을 켜야하지만 괜찮아^^",
          "original_form": "월요일은 2교시에 전부다 실시간 강의에 화면을 켜야하지만 괜찮아^^",
          "speaker_id": "2",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032240.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032240.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저런",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저런",
          "speaker_id": "1",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032240.1.7",
          "form": "난 월화우목 1교시",
          "original_form": "난 월화우목 1교시",
          "speaker_id": "1",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032240.1.8",
          "form": "월수는 대면 화목은 실강",
          "original_form": "월수는 대면 화목은 실강",
          "speaker_id": "1",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032240.1.9",
          "form": "우리 둘다 파이팅이다...",
          "original_form": "우리 둘다 파이팅이다...",
          "speaker_id": "1",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032240.1.10",
          "form": "월요일은 진짜 오전 수업 없어야되는거 아니냐",
          "original_form": "월요일은 진짜 오전 수업 없어야되는거 아니냐",
          "speaker_id": "2",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032240.1.11",
          "form": "월요일은 오전없고 금요일은 오후에 수업없고",
          "original_form": "월요일은 오전없고 금요일은 오후에 수업없고",
          "speaker_id": "2",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032240.1.12",
          "form": "그래도 난 추석전까진 대면없다..",
          "original_form": "그래도 난 추석전까진 대면없다..",
          "speaker_id": "2",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100032240.1.13",
          "form": "어우 나같으면 다시 집에 돌아온다",
          "original_form": "어우 나같으면 다시 집에 돌아온다",
          "speaker_id": "1",
          "time": "20210905 15:44"
        },
        {
          "id": "MDRW2100032240.1.14",
          "form": "금여일만 교양 1개",
          "original_form": "금여일만 교양 1개",
          "speaker_id": "1",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032240.1.15",
          "form": "또생겻네 뭘 잘목눌러야 저게 뜨냐",
          "original_form": "또생겻네 뭘 잘목눌러야 저게 뜨냐",
          "speaker_id": "1",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032240.1.16",
          "form": "뭐가 떠?",
          "original_form": "뭐가 떠?",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032240.1.17",
          "form": "너한텐 안보이너보네",
          "original_form": "너한텐 안보이너보네",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032240.1.18",
          "form": "그럼 돽어",
          "original_form": "그럼 돽어",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032240.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.20",
          "form": "흠 그래",
          "original_form": "흠 그래",
          "speaker_id": "2",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.21",
          "form": "근데 너 동아리해?",
          "original_form": "근데 너 동아리해?",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.22",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "2",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.23",
          "form": "제빵동아리",
          "original_form": "제빵동아리",
          "speaker_id": "2",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.24",
          "form": "이번주부터 나간다",
          "original_form": "이번주부터 나간다",
          "speaker_id": "2",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.25",
          "form": "아맞다",
          "original_form": "아맞다",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.26",
          "form": "부럼네",
          "original_form": "부럼네",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.27",
          "form": "나더 잇는지 살펴봐야지",
          "original_form": "나더 잇는지 살펴봐야지",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032240.1.28",
          "form": "이제 대먄이니깐 할 수 잇겟지",
          "original_form": "이제 대먄이니깐 할 수 잇겟지",
          "speaker_id": "1",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100032240.1.29",
          "form": "동아리해",
          "original_form": "동아리해",
          "speaker_id": "2",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100032240.1.30",
          "form": "회식 때 선배들이 꼰대거나 술먹이는거 아니면 진짜 재밌어",
          "original_form": "회식 때 선배들이 꼰대거나 술먹이는거 아니면 진짜 재밌어",
          "speaker_id": "2",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100032240.1.31",
          "form": "ㅋㅋㅋㅋ1핟년때 봉사동아리 나빼고 서로 친하더라고",
          "original_form": "ㅋㅋㅋㅋ1핟년때 봉사동아리 나빼고 서로 친하더라고",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032240.1.32",
          "form": "코로나때문에 봉사도 거의 못해서 관둠ㅋ",
          "original_form": "코로나때문에 봉사도 거의 못해서 관둠ㅋ",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032240.1.33",
          "form": "벽화그리는거 진짜 재밌었는데",
          "original_form": "벽화그리는거 진짜 재밌었는데",
          "speaker_id": "1",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032240.1.34",
          "form": "벽화그리기 은근 어렵더라..",
          "original_form": "벽화그리기 은근 어렵더라..",
          "speaker_id": "2",
          "time": "20210905 15:57"
        },
        {
          "id": "MDRW2100032240.1.35",
          "form": "마자 근데 재밌더라고",
          "original_form": "마자 근데 재밌더라고",
          "speaker_id": "1",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100032240.1.36",
          "form": "담엔 풍경화말고 캐릭터 그리고 싶어",
          "original_form": "담엔 풍경화말고 캐릭터 그리고 싶어",
          "speaker_id": "1",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100032240.1.37",
          "form": "그럼 더 주체적?으로 할 수 있을텐데",
          "original_form": "그럼 더 주체적?으로 할 수 있을텐데",
          "speaker_id": "1",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100032240.1.38",
          "form": "너가 그리고 싶은거 그릴 수 있다는 말인가?",
          "original_form": "너가 그리고 싶은거 그릴 수 있다는 말인가?",
          "speaker_id": "2",
          "time": "20210905 16:08"
        },
        {
          "id": "MDRW2100032240.1.39",
          "form": "기본도안은 주지 않을까 싶은데",
          "original_form": "기본도안은 주지 않을까 싶은데",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032240.1.40",
          "form": "픙경화그리니깐 우리가 할 수 있는게 없어",
          "original_form": "픙경화그리니깐 우리가 할 수 있는게 없어",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032240.1.41",
          "form": "난 풍경화가 좋던데..",
          "original_form": "난 풍경화가 좋던데..",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032240.1.42",
          "form": "너는 애니쪽이 전문(?)이니께",
          "original_form": "너는 애니쪽이 전문(?)이니께",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032240.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ작업하시는 분들도 풍경화면 시키지도 못한다면서 엄청 불만이더라고 캐릭터쪽이 시킬 수 있다고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ작업하시는 분들도 풍경화면 시키지도 못한다면서 엄청 불만이더라고 캐릭터쪽이 시킬 수 있다고",
          "speaker_id": "1",
          "time": "20210905 20:57"
        }
      ]
