      "utterance": [
        {
          "id": "MDRW2100027201.1.1",
          "form": "윌리스랑 좋아하는 음식 말해줘...",
          "original_form": "윌리스랑 좋아하는 음식 말해줘...",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027201.1.2",
          "form": "코끼리랑 젤리 사탕",
          "original_form": "코끼리랑 젤리 사탕",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027201.1.3",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027201.1.4",
          "form": "난 좋아하는 음식 젤리랑 사탕!!!!",
          "original_form": "난 좋아하는 음식 젤리랑 사탕!!!!",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027201.1.5",
          "form": "크레이프 케이크도 좋아해!!",
          "original_form": "크레이프 케이크도 좋아해!!",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027201.1.6",
          "form": "크림치즈 말고 생크림",
          "original_form": "크림치즈 말고 생크림",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027201.1.7",
          "form": "우유크림이면 더 좋아",
          "original_form": "우유크림이면 더 좋아",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027201.1.8",
          "form": "다음에 대전 오면 맛있는 차랑 디저트집 소개시켜줄게",
          "original_form": "다음에 대전 오면 맛있는 차랑 디저트집 소개시켜줄게",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027201.1.9",
          "form": "어머!!! 나는 요즘 집밥이 너무 먹고 싶어...",
          "original_form": "어머!!! 나는 요즘 집밥이 너무 먹고 싶어...",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027201.1.10",
          "form": "김치찌개에 현미밥 비벼서 먹고 싶고",
          "original_form": "김치찌개에 현미밥 비벼서 먹고 싶고",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.11",
          "form": "반찬으로 데친 다시마 초고추장에 찍어먹고 싶다...",
          "original_form": "반찬으로 데친 다시마 초고추장에 찍어먹고 싶다...",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.12",
          "form": "난 부대찌개가 좋아...",
          "original_form": "난 부대찌개가 좋아...",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.13",
          "form": "나 사워젤리도 좋아해!!",
          "original_form": "나 사워젤리도 좋아해!!",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.14",
          "form": "난 갑상선때문에 해조류 금지당했어...ㅠ",
          "original_form": "난 갑상선때문에 해조류 금지당했어...ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.15",
          "form": "부대찌개? 오오... 우리 동생도 부대찌개 좋아하는데",
          "original_form": "부대찌개? 오오... 우리 동생도 부대찌개 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.16",
          "form": "샤워젤리 맛있지!!!!!!!!!!!",
          "original_form": "샤워젤리 맛있지!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027201.1.17",
          "form": "아놔...... 슬프다 나 해산물 해조류 너무 좋아하는데",
          "original_form": "아놔...... 슬프다 나 해산물 해조류 너무 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.18",
          "form": "맞어 샤워젤리 너무 맛있어!!!",
          "original_form": "맞어 샤워젤리 너무 맛있어!!!",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.19",
          "form": "김도 못 먹어 이젠..",
          "original_form": "김도 못 먹어 이젠..",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.20",
          "form": "새콤한 맛 나서 너무 좋아",
          "original_form": "새콤한 맛 나서 너무 좋아",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.21",
          "form": "말도안돼는이상황,",
          "original_form": "말도안돼는이상황,",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.22",
          "form": "ㅠㅠ슬프다... 나 맥주에 김 같은 안주 먹는 거 좋아하는데",
          "original_form": "ㅠㅠ슬프다... 나 맥주에 김 같은 안주 먹는 거 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.23",
          "form": "그치!!!! 진짜 맛있는 공룡 샤워젤리 있는데",
          "original_form": "그치!!!! 진짜 맛있는 공룡 샤워젤리 있는데",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.24",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.25",
          "form": "나중에 보내줄게",
          "original_form": "나중에 보내줄게",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.26",
          "form": "헐!!!!!",
          "original_form": "헐!!!!!",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.27",
          "form": "광주는 국밥이 맛있어",
          "original_form": "광주는 국밥이 맛있어",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027201.1.28",
          "form": "헐 국밥 조아",
          "original_form": "헐 국밥 조아",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027201.1.29",
          "form": "나 진짜 사택 근처에 드디어 그나마 맛있는 국밥집 발견해서",
          "original_form": "나 진짜 사택 근처에 드디어 그나마 맛있는 국밥집 발견해서",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027201.1.30",
          "form": "맨날 그 국밥집만 가... 순대국이 메인인데",
          "original_form": "맨날 그 국밥집만 가... 순대국이 메인인데",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027201.1.31",
          "form": "내가 순대를 별로 안 좋아해서",
          "original_form": "내가 순대를 별로 안 좋아해서",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027201.1.32",
          "form": "헐!!!!!!!!",
          "original_form": "헐!!!!!!!!",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027201.1.33",
          "form": "맨날 다른 국밥만 시켜",
          "original_form": "맨날 다른 국밥만 시켜",
          "speaker_id": "1",
          "time": "20210902 10:35"
        }
      ]
