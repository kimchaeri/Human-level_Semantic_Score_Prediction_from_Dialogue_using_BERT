      "utterance": [
        {
          "id": "MDRW2100008882.1.1",
          "form": "채소 가지요?!",
          "original_form": "채소 가지요?!",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008882.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008882.1.3",
          "form": "맨날 안먹는데 사요",
          "original_form": "맨날 안먹는데 사요",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008882.1.4",
          "form": "항상 버려요",
          "original_form": "항상 버려요",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008882.1.5",
          "form": "지금 부추겉절이 하려고요",
          "original_form": "지금 부추겉절이 하려고요",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008882.1.6",
          "form": "막창에 먹게 레시피 찾았어요",
          "original_form": "막창에 먹게 레시피 찾았어요",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008882.1.7",
          "form": "오오 나중에 저도 레시피 공유해주세요!",
          "original_form": "오오 나중에 저도 레시피 공유해주세요!",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008882.1.8",
          "form": "저보다 언니신거죠?!",
          "original_form": "저보다 언니신거죠?!",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008882.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008882.1.10",
          "form": "저 매실을 부었네요",
          "original_form": "저 매실을 부었네요",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008882.1.11",
          "form": "저 좀 많아요 ㅋㅋ",
          "original_form": "저 좀 많아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008882.1.12",
          "form": "이래서 망해요 ㅋㅋ",
          "original_form": "이래서 망해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008882.1.13",
          "form": "근데 막을만 하네요",
          "original_form": "근데 막을만 하네요",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008882.1.14",
          "form": "간장 멸치젓 매실액 마늘 고추가루 식초요",
          "original_form": "간장 멸치젓 매실액 마늘 고추가루 식초요",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008882.1.15",
          "form": "된장소스를 못찾겠어요",
          "original_form": "된장소스를 못찾겠어요",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008882.1.16",
          "form": "아 막장말고 다른소스 만드시는거예요?",
          "original_form": "아 막장말고 다른소스 만드시는거예요?",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008882.1.17",
          "form": "막장 찍어먹는 소스 된장으로 하던데",
          "original_form": "막장 찍어먹는 소스 된장으로 하던데",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008882.1.18",
          "form": "기먕 제맘대로 했어요",
          "original_form": "기먕 제맘대로 했어요",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008882.1.19",
          "form": "레시피가 없네요",
          "original_form": "레시피가 없네요",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008882.1.20",
          "form": "나름 괜찮네요 ㅎㅎ맛났어요 잘먹었네요",
          "original_form": "나름 괜찮네요 ㅎㅎ맛났어요 잘먹었네요",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008882.1.21",
          "form": "와 맛있었겠어요~~ ㅎㅎ\r\n특히 저녁에 먹는 막창이 최고죠^^",
          "original_form": "와 맛있었겠어요~~ ㅎㅎ\r\n특히 저녁에 먹는 막창이 최고죠^^",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100008882.1.22",
          "form": "맞아요 고소 합니다",
          "original_form": "맞아요 고소 합니다",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100008882.1.23",
          "form": "디저트 복숭아 먹을라 했더니",
          "original_form": "디저트 복숭아 먹을라 했더니",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100008882.1.24",
          "form": "배불러 못 먹겠네요 ㅋㅋ",
          "original_form": "배불러 못 먹겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100008882.1.25",
          "form": "아까 스타벅스에서 쿠키블루베리 머핀인가 먹었는데 정말 맛있었어요",
          "original_form": "아까 스타벅스에서 쿠키블루베리 머핀인가 먹었는데 정말 맛있었어요",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100008882.1.26",
          "form": "카페서 어떤 디저트 좋아해요?",
          "original_form": "카페서 어떤 디저트 좋아해요?",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100008882.1.27",
          "form": "아! 저는 커피를 딱히 안좋아하는 데 가면 케잌 종류로 먹어요 ㅋ 치즈케이크 같은!",
          "original_form": "아! 저는 커피를 딱히 안좋아하는 데 가면 케잌 종류로 먹어요 ㅋ 치즈케이크 같은!",
          "speaker_id": "1",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100008882.1.28",
          "form": "저도 디저트만 엄청 먹어요",
          "original_form": "저도 디저트만 엄청 먹어요",
          "speaker_id": "2",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100008882.1.29",
          "form": "케이크 종류가 짱이죠",
          "original_form": "케이크 종류가 짱이죠",
          "speaker_id": "2",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100008882.1.30",
          "form": "가서 치즈빵도 있는데 꿀이랑 주는데 그것도 맛나요",
          "original_form": "가서 치즈빵도 있는데 꿀이랑 주는데 그것도 맛나요",
          "speaker_id": "2",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100008882.1.31",
          "form": "요자들은 아무래도 디저트에 약해요 ㅋㅋ",
          "original_form": "요자들은 아무래도 디저트에 약해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100008882.1.32",
          "form": "요샌 코로나로 맛집 투어는 자주 못가서 별로 아는데가 없네요",
          "original_form": "요샌 코로나로 맛집 투어는 자주 못가서 별로 아는데가 없네요",
          "speaker_id": "2",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100008882.1.33",
          "form": "치즈빵을 꿀에 찍어먹나요? 특이하네요",
          "original_form": "치즈빵을 꿀에 찍어먹나요? 특이하네요",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100008882.1.34",
          "form": "네 치즈피자 비슷해요",
          "original_form": "네 치즈피자 비슷해요",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100008882.1.35",
          "form": "꿀찍어 먹잖아요 ㅎㅎ",
          "original_form": "꿀찍어 먹잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100008882.1.36",
          "form": "아 그러네요 ㅎㅎ 파자 꿀찍어먹는건 안이상하고 ㅋㅋㅋㅋ빵찍어먹는건 이상하게 봤네요 ㅋㅋ",
          "original_form": "아 그러네요 ㅎㅎ 파자 꿀찍어먹는건 안이상하고 ㅋㅋㅋㅋ빵찍어먹는건 이상하게 봤네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100008882.1.37",
          "form": "고르곤졸라피자 처럼요",
          "original_form": "고르곤졸라피자 처럼요",
