      "utterance": [
        {
          "id": "MDRW2100042526.1.1",
          "form": "name1님 순대 좋아하세요?",
          "original_form": "&name1&님 순대 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042526.1.2",
          "form": "너무 뜬금없죠 ㅋㅋㅋㅋ",
          "original_form": "너무 뜬금없죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042526.1.3",
          "form": "ㅋㅋㅋㅋ 아뇨 저는 순대는 좋아하는 편이 아닌데 있으면 먹고 사실 부산물을 더 좋아해요 간이나 염통같은거요",
          "original_form": "ㅋㅋㅋㅋ 아뇨 저는 순대는 좋아하는 편이 아닌데 있으면 먹고 사실 부산물을 더 좋아해요 간이나 염통같은거요",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042526.1.4",
          "form": "내장 좋아하시는구나",
          "original_form": "내장 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042526.1.5",
          "form": "저는 다 좋아요 ㅎㅎㅎ",
          "original_form": "저는 다 좋아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042526.1.6",
          "form": "그럼 순대국 별로 안좋아하세요,",
          "original_form": "그럼 순대국 별로 안좋아하세요,",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042526.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042526.1.8",
          "form": "네 순대국보단 돼지국밥을 먹는 편이에요",
          "original_form": "네 순대국보단 돼지국밥을 먹는 편이에요",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042526.1.9",
          "form": "아아 돼지국밥 맛있어요ㅜ 저는 순대국도 돼지국밥도 다 좋아요",
          "original_form": "아아 돼지국밥 맛있어요ㅜ 저는 순대국도 돼지국밥도 다 좋아요",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042526.1.10",
          "form": "돼지국밥이 부산에 유명하다고 하잖아요",
          "original_form": "돼지국밥이 부산에 유명하다고 하잖아요",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042526.1.11",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042526.1.12",
          "form": "맞아요!! 저도 그렇게 들었어요",
          "original_form": "맞아요!! 저도 그렇게 들었어요",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042526.1.13",
          "form": "그래서 부산 갔을 때 돼지국밥을 먹고 오긴 했는데 그냥 근처에 있는 걸 가서 그런지 특별함은 없더라구요",
          "original_form": "그래서 부산 갔을 때 돼지국밥을 먹고 오긴 했는데 그냥 근처에 있는 걸 가서 그런지 특별함은 없더라구요",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042526.1.14",
          "form": "맛집으로 유명한 곳을 갔어야했나..",
          "original_form": "맛집으로 유명한 곳을 갔어야했나..",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042526.1.15",
          "form": "name1님 제 머릿속에 들어갔다 나오셨나요",
          "original_form": "&name1&님 제 머릿속에 들어갔다 나오셨나요",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042526.1.16",
          "form": "제가 하려는 말을 똑같이 하셨어요",
          "original_form": "제가 하려는 말을 똑같이 하셨어요",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042526.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042526.1.18",
          "form": "그냥 우리집 역 근처 국밥집이 더 맛있는데??",
          "original_form": "그냥 우리집 역 근처 국밥집이 더 맛있는데??",
          "speaker_id": "2",
          "time": "20210906 23:13"
        },
        {
          "id": "MDRW2100042526.1.19",
          "form": "라고 생각했어요",
          "original_form": "라고 생각했어요",
          "speaker_id": "2",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042526.1.20",
          "form": "ㅋㅋㄱㄱname2님도 같은 경험하셨구나",
          "original_form": "ㅋㅋㄱㄱ&name2&님도 같은 경험하셨구나",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042526.1.21",
          "form": "저는 오히려 부산에서 먹은 곱창이 그렇게 맛있더라구요",
          "original_form": "저는 오히려 부산에서 먹은 곱창이 그렇게 맛있더라구요",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042526.1.22",
          "form": "무슨 곱창이죠!",
          "original_form": "무슨 곱창이죠!",
          "speaker_id": "2",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042526.1.23",
          "form": "광안리인가 서면인가",
          "original_form": "광안리인가 서면인가",
          "speaker_id": "1",
          "time": "20210906 23:25"
        },
        {
          "id": "MDRW2100042526.1.24",
          "form": "곱창구이에요?아니면 볶음인가요?",
          "original_form": "곱창구이에요?아니면 볶음인가요?",
          "speaker_id": "2",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042526.1.25",
          "form": "곱창구이였어요",
          "original_form": "곱창구이였어요",
          "speaker_id": "1",
          "time": "20210906 23:51"
        },
        {
          "id": "MDRW2100042526.1.26",
          "form": "기본찬으로 육회도 주고",
          "original_form": "기본찬으로 육회도 주고",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042526.1.27",
          "form": "좋은 곳이었어요",
          "original_form": "좋은 곳이었어요",
          "speaker_id": "1",
          "time": "20210906 23:52"
        },
        {
          "id": "MDRW2100042526.1.28",
          "form": "육회!!",
          "original_form": "육회!!",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042526.1.29",
          "form": "좋은 곳이에요!!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋은 곳이에요!!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042526.1.30",
          "form": "육회 먹고 싶어요ㅠ",
          "original_form": "육회 먹고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210906 23:55"
        },
        {
          "id": "MDRW2100042526.1.31",
          "form": "육회도 진짜 맛있죠 아 이 밤에 먹을 거 얘기 너무 고통스럽네요",
          "original_form": "육회도 진짜 맛있죠 아 이 밤에 먹을 거 얘기 너무 고통스럽네요",
