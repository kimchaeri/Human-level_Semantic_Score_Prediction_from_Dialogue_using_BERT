      "utterance": [
        {
          "id": "MDRW2100015457.1.1",
          "form": "제주도 여행 가본 경험 있어요?",
          "original_form": "제주도 여행 가본 경험 있어요?",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015457.1.2",
          "form": "제주도 여행 진짜 애기때가서 기억에 남는게 ㅂㄹ없네요",
          "original_form": "제주도 여행 진짜 애기때가서 기억에 남는게 ㅂㄹ없네요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015457.1.3",
          "form": "성인때 가보고싶어요",
          "original_form": "성인때 가보고싶어요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.4",
          "form": "악 ㅋㅋㅋ 그렇구나..",
          "original_form": "악 ㅋㅋㅋ 그렇구나..",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.5",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.6",
          "form": "네ㅜㅜ 저 거기 고기국수 진짜 맛있게 먹었어요",
          "original_form": "네ㅜㅜ 저 거기 고기국수 진짜 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.7",
          "form": "맛있는데 많데요 ㅋㅋ",
          "original_form": "맛있는데 많데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.8",
          "form": "오 고기국수",
          "original_form": "오 고기국수",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.9",
          "form": "흑돼지도맛있어요?",
          "original_form": "흑돼지도맛있어요?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.10",
          "form": "흑돼지 진짜 미쳐요",
          "original_form": "흑돼지 진짜 미쳐요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.11",
          "form": "제주도에서 현지인 추천 식당 갔는데 와....",
          "original_form": "제주도에서 현지인 추천 식당 갔는데 와....",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.12",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015457.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015457.1.14",
          "form": "침고인다",
          "original_form": "침고인다",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015457.1.15",
          "form": "진짜 와....",
          "original_form": "진짜 와....",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015457.1.16",
          "form": "제주도에서 먹은 거 다 맛있게 먹었어요. 단 막걸리 빼고 ㅠ",
          "original_form": "제주도에서 먹은 거 다 맛있게 먹었어요. 단 막걸리 빼고 ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015457.1.17",
          "form": "고기국수랑 흑돼지중 뭐가 더맛있나요? ㅋㅋ",
          "original_form": "고기국수랑 흑돼지중 뭐가 더맛있나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015457.1.18",
          "form": "제주도 단막걸리는 제외ㅋㅋㅋ",
          "original_form": "제주도 단막걸리는 제외ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015457.1.19",
          "form": "둘 다 진짜 맛있어서 우위를 가릴 수가 없어요",
          "original_form": "둘 다 진짜 맛있어서 우위를 가릴 수가 없어요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015457.1.20",
          "form": "밤막걸리 땅콩막걸리 등등 다 별로였어요 제겐..",
          "original_form": "밤막걸리 땅콩막걸리 등등 다 별로였어요 제겐..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015457.1.21",
          "form": "와 둘다 꼭 먹어볼게요 ㅋㅋㅋ",
          "original_form": "와 둘다 꼭 먹어볼게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015457.1.22",
          "form": "제주도 카페도 핫플레이스 많다는데 가봤나요?",
          "original_form": "제주도 카페도 핫플레이스 많다는데 가봤나요?",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015457.1.23",
          "form": "애월 카페~",
          "original_form": "애월 카페~",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015457.1.24",
          "form": "제주도 가면 인스타용 카페 ㅋㅋㅋ꼭 필수코스라고..",
          "original_form": "제주도 가면 인스타용 카페 ㅋㅋㅋ꼭 필수코스라고..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015457.1.25",
          "form": "오 들어본거같아요 완전 명소 ㅋㅋ",
          "original_form": "오 들어본거같아요 완전 명소 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015457.1.26",
          "form": "맞아요 인스타용 카페 ㅋㅋㅋ",
          "original_form": "맞아요 인스타용 카페 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015457.1.27",
          "form": "카페가 예쁘긴 했어요 막 배경이 바다고 그래서.. 근데 주차가 진짜 헬이였어요",
          "original_form": "카페가 예쁘긴 했어요 막 배경이 바다고 그래서.. 근데 주차가 진짜 헬이였어요",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015457.1.28",
          "form": "아 주차헬인것도 어디서 들은거같은",
          "original_form": "아 주차헬인것도 어디서 들은거같은",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015457.1.29",
          "form": "뷰가 최고라고",
          "original_form": "뷰가 최고라고",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015457.1.30",
          "form": "맛은 그닥이죠?",
          "original_form": "맛은 그닥이죠?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.31",
          "form": "진입로가 두군데인데 그 두군데에서 차가 들어오면 가운데에서 오도가도 못하게 막혀요 ㅋㅋㅋㅋ",
          "original_form": "진입로가 두군데인데 그 두군데에서 차가 들어오면 가운데에서 오도가도 못하게 막혀요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.32",
          "form": "1차선 도로 헬..",
          "original_form": "1차선 도로 헬..",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.33",
          "form": "헐 주차가 불편하네요",
          "original_form": "헐 주차가 불편하네요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.34",
          "form": "뷰는 진짜 예쁘긴 했어요...",
          "original_form": "뷰는 진짜 예쁘긴 했어요...",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.35",
          "form": "와 그거 어디서 읽은거같은 그래서 싸움난다고 주차하다가",
          "original_form": "와 그거 어디서 읽은거같은 그래서 싸움난다고 주차하다가",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.36",
          "form": "바로 옆에 바다가 진짜 에메랄드 빛..",
          "original_form": "바로 옆에 바다가 진짜 에메랄드 빛..",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.37",
          "form": "와 그래도 가고싶네요",
          "original_form": "와 그래도 가고싶네요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015457.1.38",
          "form": "아마 아기때의 제주도보다 지금의 제주도가 훨씬 좋을거에요",
          "original_form": "아마 아기때의 제주도보다 지금의 제주도가 훨씬 좋을거에요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015457.1.39",
          "form": "요즘 비행기값도 싸니 꼭 가보길 바라요!",
          "original_form": "요즘 비행기값도 싸니 꼭 가보길 바라요!",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015457.1.40",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210825 23:12"
        }
      ]
    }
