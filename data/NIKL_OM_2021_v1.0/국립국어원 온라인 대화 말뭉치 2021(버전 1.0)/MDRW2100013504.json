      "utterance": [
        {
          "id": "MDRW2100013504.1.1",
          "form": "시간이 너무 길어서요 ㅜ",
          "original_form": "시간이 너무 길어서요 ㅜ",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013504.1.2",
          "form": "아하 뮤지컬은 집중력이 필요하긴하더라고요ㅋㅋㅋ",
          "original_form": "아하 뮤지컬은 집중력이 필요하긴하더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013504.1.3",
          "form": "맞아요 ㅜ 2시간 정도 집중하는게 힘들어요",
          "original_form": "맞아요 ㅜ 2시간 정도 집중하는게 힘들어요",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013504.1.4",
          "form": "동감해요 ㅜ 대부분 2시간 넘게하시도 하고",
          "original_form": "동감해요 ㅜ 대부분 2시간 넘게하시도 하고",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013504.1.5",
          "form": "ㅋㅋㅋ 글구 좀 좁은 공간에 오래있는게 힘들어서",
          "original_form": "ㅋㅋㅋ 글구 좀 좁은 공간에 오래있는게 힘들어서",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013504.1.6",
          "form": "아하 뮤지컬은 그런게 힘들긴하죠",
          "original_form": "아하 뮤지컬은 그런게 힘들긴하죠",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013504.1.7",
          "form": "네 ㅜ ㅋㅋㅋㅋㅋㅋ 그거만 빼면 뮤지컬 좋죠",
          "original_form": "네 ㅜ ㅋㅋㅋㅋㅋㅋ 그거만 빼면 뮤지컬 좋죠",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013504.1.8",
          "form": "ㅋㅋㅋ 보고 싶은 뮤지컬 잇나요?",
          "original_form": "ㅋㅋㅋ 보고 싶은 뮤지컬 잇나요?",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013504.1.9",
          "form": "저는 지킬앤하이드?",
          "original_form": "저는 지킬앤하이드?",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013504.1.10",
          "form": "유명한거네요 저도 아직 안봤지만",
          "original_form": "유명한거네요 저도 아직 안봤지만",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013504.1.11",
          "form": "티켓킹 어려울까요?",
          "original_form": "티켓킹 어려울까요?",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013504.1.12",
          "form": "배우에 따라 뮤지컬 티켓팅은 다를것같아요 ㅜ",
          "original_form": "배우에 따라 뮤지컬 티켓팅은 다를것같아요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013504.1.13",
          "form": "유명 배우면",
          "original_form": "유명 배우면",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013504.1.14",
          "form": "좀 어렵겟죠",
          "original_form": "좀 어렵겟죠",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013504.1.15",
          "form": "잘 아시는그나 ㅜㅜ 그렇기야 하네요",
          "original_form": "잘 아시는그나 ㅜㅜ 그렇기야 하네요",
