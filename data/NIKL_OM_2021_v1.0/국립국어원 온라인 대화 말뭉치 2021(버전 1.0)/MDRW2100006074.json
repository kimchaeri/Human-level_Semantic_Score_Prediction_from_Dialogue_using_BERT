          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100006074.1.17",
          "form": "크 ㅜㅜ",
          "original_form": "크 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006074.1.18",
