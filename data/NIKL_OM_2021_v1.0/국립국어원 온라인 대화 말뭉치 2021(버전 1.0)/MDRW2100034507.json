      "utterance": [
        {
          "id": "MDRW2100034507.1.1",
          "form": "뭐하고 계십니꺼??ㅋㅋㅋㅋ",
          "original_form": "뭐하고 계십니꺼??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034507.1.2",
          "form": "콩나물밥 기다리고있습니다ㅋㅋ",
          "original_form": "콩나물밥 기다리고있습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034507.1.3",
          "form": "청소는 다했구요.ㅋ",
          "original_form": "청소는 다했구요.ㅋ",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034507.1.4",
          "form": "니는? 뭐해?",
          "original_form": "니는? 뭐해?",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034507.1.5",
          "form": "이야~우린 첫찌 라면 끓여달래서 하면 먹고",
          "original_form": "이야~우린 첫찌 라면 끓여달래서 하면 먹고",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034507.1.6",
          "form": "배두드리는 중 ㅋㅋㅋ",
          "original_form": "배두드리는 중 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100034507.1.7",
          "form": "집한번 싹 엎고싶은데 ㅋㅋㅋ",
          "original_form": "집한번 싹 엎고싶은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034507.1.8",
          "form": "팔이 아파가 요번주는 앙되겠다 캄서 ㅋㅋㅋㅋ",
          "original_form": "팔이 아파가 요번주는 앙되겠다 캄서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034507.1.9",
          "form": "ㅋㅋㅋ  그래 무리하지마라",
          "original_form": "ㅋㅋㅋ  그래 무리하지마라",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034507.1.10",
          "form": "이번주는 안되지ㅋㅋ",
          "original_form": "이번주는 안되지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034507.1.11",
          "form": "나도 확 뒤집고싶은데 조금씩 정리중ㅋ",
          "original_form": "나도 확 뒤집고싶은데 조금씩 정리중ㅋ",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034507.1.12",
          "form": "첫찌 또 혼자 자겠다고..방 옮겨달라고..",
          "original_form": "첫찌 또 혼자 자겠다고..방 옮겨달라고..",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034507.1.13",
          "form": "몇번을 옮기는지 모린다ㅠ",
          "original_form": "몇번을 옮기는지 모린다ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034507.1.14",
          "form": "헉ㅋㅋㅋㅋㅋ",
          "original_form": "헉ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034507.1.15",
          "form": "웃긴다ㅋㅋ",
          "original_form": "웃긴다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034507.1.16",
          "form": "어떻게해ㅜㅜ또 옮겨야되나ㅜ",
          "original_form": "어떻게해ㅜㅜ또 옮겨야되나ㅜ",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034507.1.17",
          "form": "나 올리모델링한집 댕겨왔는데..",
          "original_form": "나 올리모델링한집 댕겨왔는데..",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034507.1.18",
          "form": "그거보고나니 와이래 하곱노 ㅋㅋㅋㅋㅋㅂ",
          "original_form": "그거보고나니 와이래 하곱노 ㅋㅋㅋㅋㅋㅂ",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034507.1.19",
          "form": "돈도 없는데 ㅋㅋㅋ",
          "original_form": "돈도 없는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034507.1.20",
          "form": "싹다 뜯어가 4천 줬다던데 ㅋㅋㅋ",
          "original_form": "싹다 뜯어가 4천 줬다던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034507.1.21",
          "form": "ㅋㅋㅋ 아하",
          "original_form": "ㅋㅋㅋ 아하",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034507.1.22",
          "form": "거기는 왜갔노?ㅋ",
          "original_form": "거기는 왜갔노?ㅋ",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034507.1.23",
          "form": "너희집도 좋다 ㅋㄱㅋㅋㅋㄱㅋㄱㅋㅋㅋ왜그래ㅋㅋㅋㅋ",
          "original_form": "너희집도 좋다 ㅋㄱㅋㅋㅋㄱㅋㄱㅋㅋㅋ왜그래ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034507.1.24",
          "form": "신랑 회사동생 너거아팟으로 이사왔는데",
          "original_form": "신랑 회사동생 너거아팟으로 이사왔는데",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034507.1.25",
          "form": "자기도 인테리어했으니 자랑하고싶어가~",
          "original_form": "자기도 인테리어했으니 자랑하고싶어가~",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034507.1.26",
          "form": "응응 싹 다 고쳤대?",
          "original_form": "응응 싹 다 고쳤대?",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034507.1.27",
          "form": "아아 ㅋㅋ대박이네ㅋㅋ",
          "original_form": "아아 ㅋㅋ대박이네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034507.1.28",
          "form": "4천? 34평?",
          "original_form": "4천? 34평?",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034507.1.29",
          "form": "너네집이랑 같은데",
          "original_form": "너네집이랑 같은데",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.30",
          "form": "이일랜드 다떼고 바닥 대리석에 ㅋㅋㅋㅋ",
          "original_form": "이일랜드 다떼고 바닥 대리석에 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.31",
          "form": "헐 28평을 4천줬다말이가?",
          "original_form": "헐 28평을 4천줬다말이가?",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.32",
          "form": "대박이네 겁니 많이들었구먼",
          "original_form": "대박이네 겁니 많이들었구먼",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.33",
          "form": "응 진짜 싹 ㅋㅋㅋㅋㅋ",
          "original_form": "응 진짜 싹 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.34",
          "form": "와 바닥 대리석..",
          "original_form": "와 바닥 대리석..",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.35",
          "form": "애없어?ㅋㅋ",
          "original_form": "애없어?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.36",
          "form": "아니 애있음 불안해가 바닥대리석은못하겠던데",
          "original_form": "아니 애있음 불안해가 바닥대리석은못하겠던데",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034507.1.37",
          "form": "요즘 유행하는건 다 해놨데~",
          "original_form": "요즘 유행하는건 다 해놨데~",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100034507.1.38",
          "form": "마누라가 애 안낳는다고 ㅋㅋㅋㅋ",
          "original_form": "마누라가 애 안낳는다고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100034507.1.39",
          "form": "우리도 그얘기함 ㅋㅋㅋㅋ애있음 못한다고 ㅋㅋ",
          "original_form": "우리도 그얘기함 ㅋㅋㅋㅋ애있음 못한다고 ㅋㅋ",
          "speaker_id": "2",
