      "utterance": [
        {
          "id": "MDRW2100047680.1.1",
          "form": "유투브는없는게없군요",
          "original_form": "유투브는없는게없군요",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047680.1.2",
          "form": "유툽이진짜",
          "original_form": "유툽이진짜",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047680.1.3",
          "form": "별게별게",
          "original_form": "별게별게",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047680.1.4",
          "form": "다있네요",
          "original_form": "다있네요",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047680.1.5",
          "form": "글게요",
          "original_form": "글게요",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047680.1.6",
          "form": "애들 장래희망이 유투버라자나용",
          "original_form": "애들 장래희망이 유투버라자나용",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047680.1.7",
          "form": "많이벌긴하죠ㅋㅋㅋ",
          "original_form": "많이벌긴하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100047680.1.8",
          "form": "잘된사람들은",
          "original_form": "잘된사람들은",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100047680.1.9",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100047680.1.10",
          "form": "ㅇ네",
          "original_form": "ㅇ네",
          "speaker_id": "2",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100047680.1.11",
          "form": "잘나가는 유투버중에 남을돕는 컨텐츠면  부러워요",
          "original_form": "잘나가는 유투버중에 남을돕는 컨텐츠면  부러워요",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100047680.1.12",
          "form": "요새 나쁜걸로 어그로끌어서",
          "original_form": "요새 나쁜걸로 어그로끌어서",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100047680.1.13",
          "form": "클릭수늘리는유투버도많더래요",
          "original_form": "클릭수늘리는유투버도많더래요",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100047680.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100047680.1.15",
          "form": "그런거도많나요?",
          "original_form": "그런거도많나요?",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100047680.1.16",
          "form": "왜 한강에서 사망한 의대생 의문의 죽음",
          "original_form": "왜 한강에서 사망한 의대생 의문의 죽음",
          "speaker_id": "2",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100047680.1.17",
          "form": "친구의 살인인가 이러면서",
          "original_form": "친구의 살인인가 이러면서",
          "speaker_id": "2",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100047680.1.18",
          "form": "근거없이 의혹스러운영상만들어 돈벌어가서 고소당했자나용",
          "original_form": "근거없이 의혹스러운영상만들어 돈벌어가서 고소당했자나용",
          "speaker_id": "2",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100047680.1.19",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047680.1.20",
          "form": "그건 너무하네요...",
          "original_form": "그건 너무하네요...",
          "speaker_id": "1",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047680.1.21",
          "form": "돈이뭔지",
          "original_form": "돈이뭔지",
          "speaker_id": "1",
          "time": "20210827 01:20"
        },
        {
          "id": "MDRW2100047680.1.22",
          "form": "돈이 많이 벌린다나봐욧",
          "original_form": "돈이 많이 벌린다나봐욧",
          "speaker_id": "2",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100047680.1.23",
          "form": "동물학대하면서 구조하는척하는 계정두 있대요",
          "original_form": "동물학대하면서 구조하는척하는 계정두 있대요",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100047680.1.24",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047680.1.25",
          "form": "너무 충격적이네요",
          "original_form": "너무 충격적이네요",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047680.1.26",
          "form": "그런거넘싫어요",
          "original_form": "그런거넘싫어요",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047680.1.27",
          "form": "좋아도",
          "original_form": "좋아도",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047680.1.28",
          "form": "문제고",
          "original_form": "문제고",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047680.1.29",
          "form": "악용하는사례가없음좋겠어용",
          "original_form": "악용하는사례가없음좋겠어용",
          "speaker_id": "2",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100047680.1.30",
          "form": "맞아요ㅠ 그런건",
          "original_form": "맞아요ㅠ 그런건",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100047680.1.31",
          "form": "넘싫네요 흑흑",
          "original_form": "넘싫네요 흑흑",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100047680.1.32",
          "form": "먹방이",
          "original_form": "먹방이",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100047680.1.33",
          "form": "평화로워요",
          "original_form": "평화로워요",
          "speaker_id": "1",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100047680.1.34",
          "form": "ㅋㅋ 글표",
          "original_form": "ㅋㅋ 글표",
          "speaker_id": "2",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100047680.1.35",
          "form": "쵸",
          "original_form": "쵸",
          "speaker_id": "2",
          "time": "20210827 01:32"
        },
        {
          "id": "MDRW2100047680.1.36",
          "form": "글쵸 전 고양이먹방최고ㅋㅋ",
