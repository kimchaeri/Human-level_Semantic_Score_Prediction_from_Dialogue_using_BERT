      "utterance": [
        {
          "id": "MDRW2100041084.1.1",
          "form": "맥도날드랑 버거킹 중에서 뭐가 좋으세요?",
          "original_form": "맥도날드랑 버거킹 중에서 뭐가 좋으세요?",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100041084.1.2",
          "form": "맥도날드요!",
          "original_form": "맥도날드요!",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100041084.1.3",
          "form": "상하이스파이스치킨 좋아하는데",
          "original_form": "상하이스파이스치킨 좋아하는데",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100041084.1.4",
          "form": "조금 양이 줄어서 아쉬워요ㅠㅠ",
          "original_form": "조금 양이 줄어서 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100041084.1.5",
          "form": "네 다른 분도 그 얘기 하시더라고요 양이 줄었대요",
          "original_form": "네 다른 분도 그 얘기 하시더라고요 양이 줄었대요",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100041084.1.6",
          "form": "저는 요즘 수비 버거에 맛들였어요",
          "original_form": "저는 요즘 수비 버거에 맛들였어요",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100041084.1.7",
          "form": "슈.비.",
          "original_form": "슈.비.",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041084.1.8",
          "form": "앗 슈.비!",
          "original_form": "앗 슈.비!",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041084.1.9",
          "form": "새우버거 였던가요?",
          "original_form": "새우버거 였던가요?",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041084.1.10",
          "form": "새우랑 소고기요",
          "original_form": "새우랑 소고기요",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041084.1.11",
          "form": "두개가 들어있어요",
          "original_form": "두개가 들어있어요",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100041084.1.12",
          "form": "맛있겠어요!",
          "original_form": "맛있겠어요!",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041084.1.13",
          "form": "저 슈슈는 먹어본 것 같은데",
          "original_form": "저 슈슈는 먹어본 것 같은데",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041084.1.14",
          "form": "이게 새우단독이었죠?",
          "original_form": "이게 새우단독이었죠?",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041084.1.15",
          "form": "예 저는 베토디가 좋았는데",
          "original_form": "예 저는 베토디가 좋았는데",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041084.1.16",
          "form": "바뀌었어요",
          "original_form": "바뀌었어요",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041084.1.17",
          "form": "네 단독이요",
          "original_form": "네 단독이요",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041084.1.18",
          "form": "베이컨토마토디럭스? 맞죠ㅋㅋㅋ?",
          "original_form": "베이컨토마토디럭스? 맞죠ㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041084.1.19",
          "form": "네네 그거 맞아요",
          "original_form": "네네 그거 맞아요",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041084.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041084.1.21",
          "form": "롯데리아도 드시나요?",
          "original_form": "롯데리아도 드시나요?",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041084.1.22",
          "form": "요새 롯데리아 많이 맛있어졌ㄷ고 하더라구요!!",
          "original_form": "요새 롯데리아 많이 맛있어졌ㄷ고 하더라구요!!",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041084.1.23",
          "form": "전 양념감자 진짜 좋아해요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 양념감자 진짜 좋아해요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041084.1.24",
          "form": "네 저도 얼마 전에 롯데리아 먹어 봤다가 맛있더라",
          "original_form": "네 저도 얼마 전에 롯데리아 먹어 봤다가 맛있더라",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041084.1.25",
          "form": "구요",
          "original_form": "구요",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041084.1.26",
          "form": "뭐 드셔보셨나요! ㅋㅋㅋ 롯데리아도 은근 메뉴가 휙휙바뀌더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "뭐 드셔보셨나요! ㅋㅋㅋ 롯데리아도 은근 메뉴가 휙휙바뀌더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041084.1.27",
          "form": "불새버거 아재버거 이런 새로운거 내다가도 금방 단종시켜서...",
          "original_form": "불새버거 아재버거 이런 새로운거 내다가도 금방 단종시켜서...",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041084.1.28",
          "form": "예전에 무슨 유러피안프리코 버거??",
          "original_form": "예전에 무슨 유러피안프리코 버거??",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041084.1.29",
          "form": "그거 맛있었는데 없어졌더라구요",
          "original_form": "그거 맛있었는데 없어졌더라구요",
          "speaker_id": "1",
          "time": "20210905 16:56"
        }
      ]
    }
