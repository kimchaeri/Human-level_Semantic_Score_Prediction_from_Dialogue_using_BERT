      "utterance": [
        {
          "id": "MDRW2100009805.1.1",
          "form": "서럽네요 ㅋㅋㅋ 시원한음료 좋아하는데",
          "original_form": "서럽네요 ㅋㅋㅋ 시원한음료 좋아하는데",
          "speaker_id": "1",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009805.1.2",
          "form": "건강생각하면 당줄여야하는데 그런게 너무 맛있어요ㅎㅎ",
          "original_form": "건강생각하면 당줄여야하는데 그런게 너무 맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:37"
        },
        {
          "id": "MDRW2100009805.1.3",
          "form": "세치혀와 몸이 따로놀아요ㅋ",
          "original_form": "세치혀와 몸이 따로놀아요ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:37"
        },
        {
          "id": "MDRW2100009805.1.4",
          "form": "추방해야할듯",
          "original_form": "추방해야할듯",
          "speaker_id": "1",
          "time": "20210823 10:38"
        },
        {
          "id": "MDRW2100009805.1.5",
          "form": "ㅎㅎ 오늘까지마 먹구 낼부터는 물마셔야지 하구ㅜㅜ",
          "original_form": "ㅎㅎ 오늘까지마 먹구 낼부터는 물마셔야지 하구ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 10:38"
        },
        {
          "id": "MDRW2100009805.1.6",
          "form": "저지금 물떠놨는데",
          "original_form": "저지금 물떠놨는데",
          "speaker_id": "1",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009805.1.7",
          "form": "점심에 먹을 초코음료생각하고있어요ㅋ",
          "original_form": "점심에 먹을 초코음료생각하고있어요ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009805.1.8",
          "form": "저도 건강생각한다고 토마토간거 들고왔는데 맛없어요ㅠㅠㅋㅋ",
          "original_form": "저도 건강생각한다고 토마토간거 들고왔는데 맛없어요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009805.1.9",
          "form": "아침식사로는 제격이네요",
          "original_form": "아침식사로는 제격이네요",
          "speaker_id": "1",
          "time": "20210823 10:40"
        },
        {
          "id": "MDRW2100009805.1.10",
          "form": "설탕은요?",
          "original_form": "설탕은요?",
          "speaker_id": "1",
          "time": "20210823 10:40"
        },
        {
          "id": "MDRW2100009805.1.11",
          "form": "당근 뿌렸죠ㅎㅎㅎ 그냥은 아직 힘드네요..",
          "original_form": "당근 뿌렸죠ㅎㅎㅎ 그냥은 아직 힘드네요..",
          "speaker_id": "2",
          "time": "20210823 10:40"
        },
        {
          "id": "MDRW2100009805.1.12",
          "form": "ㅋㅋ 토마토 맛있는데",
          "original_form": "ㅋㅋ 토마토 맛있는데",
          "speaker_id": "1",
          "time": "20210823 10:41"
        },
        {
          "id": "MDRW2100009805.1.13",
          "form": "습관이 필요한거겠죠",
          "original_form": "습관이 필요한거겠죠",
          "speaker_id": "1",
          "time": "20210823 10:41"
        },
        {
          "id": "MDRW2100009805.1.14",
          "form": "맞아요ㅎㅎ 샐러드에 있는건 먹는데..습관들여야겠어요",
          "original_form": "맞아요ㅎㅎ 샐러드에 있는건 먹는데..습관들여야겠어요",
          "speaker_id": "2",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009805.1.15",
          "form": "조금씩 건강식으로 바꿀필요가있어요",
          "original_form": "조금씩 건강식으로 바꿀필요가있어요",
          "speaker_id": "1",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009805.1.16",
          "form": "어렵지만ㅋ",
          "original_form": "어렵지만ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009805.1.17",
          "form": "실패의 쓴맛!",
          "original_form": "실패의 쓴맛!",
          "speaker_id": "1",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009805.1.18",
          "form": "맞아요 실행이 필요한데ㅎㅎㅎ",
          "original_form": "맞아요 실행이 필요한데ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009805.1.19",
          "form": "스벅가서 나름 건강한거 골라마시면",
          "original_form": "스벅가서 나름 건강한거 골라마시면",
          "speaker_id": "1",
          "time": "20210823 10:43"
        },
        {
          "id": "MDRW2100009805.1.20",
          "form": "위로는돼요ㅋ",
          "original_form": "위로는돼요ㅋ",
          "speaker_id": "1",
          "time": "20210823 10:43"
        },
        {
          "id": "MDRW2100009805.1.21",
          "form": "차종류로 마셔버릇해야겠어요ㅎㅎ",
          "original_form": "차종류로 마셔버릇해야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 10:43"
        }
      ]
