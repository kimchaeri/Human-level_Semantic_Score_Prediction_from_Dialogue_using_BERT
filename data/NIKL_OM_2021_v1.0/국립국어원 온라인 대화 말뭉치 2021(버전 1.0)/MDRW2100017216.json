        },
        {
          "id": "MDRW2100017216.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017216.1.3",
          "form": "운동좋아하시나요",
          "original_form": "운동좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017216.1.4",
          "form": "운동 즐기진않는데 요즘 관심갖고있어요",
          "original_form": "운동 즐기진않는데 요즘 관심갖고있어요",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017216.1.5",
          "form": "건강에 관심이 많아졌거든요",
          "original_form": "건강에 관심이 많아졌거든요",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017216.1.6",
          "form": "name1님은 운동 좋아하시나요?",
          "original_form": "&name1&님은 운동 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017216.1.7",
          "form": "네 홈트해보려구요",
          "original_form": "네 홈트해보려구요",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017216.1.8",
          "form": "홈트 저는 김연경선수가 찍은 홈트해요",
          "original_form": "홈트 저는 김연경선수가 찍은 홈트해요",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017216.1.9",
          "form": "뭔가 믿음직스럽고",
          "original_form": "뭔가 믿음직스럽고",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017216.1.10",
          "form": "같이 열심히 하게되더라구요",
          "original_form": "같이 열심히 하게되더라구요",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017216.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017216.1.12",
          "form": "헉 식빵언니ㅎㅎ",
          "original_form": "헉 식빵언니ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100017216.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100017216.1.14",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100017216.1.15",
          "form": "편집도 괜찮던데요ㅎㅎ",
          "original_form": "편집도 괜찮던데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100017216.1.16",
          "form": "맞아요 지금 안하고 있던데",
          "original_form": "맞아요 지금 안하고 있던데",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100017216.1.17",
          "form": "다시보고싶어요",
          "original_form": "다시보고싶어요",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100017216.1.18",
          "form": "선수 일상 보는게 은근 재밌더라구요",
          "original_form": "선수 일상 보는게 은근 재밌더라구요",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100017216.1.19",
          "form": "곧 다시올라오지않을까요ㅎㅎ",
          "original_form": "곧 다시올라오지않을까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100017216.1.20",
          "form": "그럴것 같더라구요!",
          "original_form": "그럴것 같더라구요!",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100017216.1.21",
          "form": "역시 존버는 승리",
          "original_form": "역시 존버는 승리",
          "speaker_id": "2",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100017216.1.22",
          "form": "빨리올라왔음 좋겠네여",
          "original_form": "빨리올라왔음 좋겠네여",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100017216.1.23",
          "form": "네 올림픽 후기라든지",
          "original_form": "네 올림픽 후기라든지",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100017216.1.24",
          "form": "연습연상이라든지",
          "original_form": "연습연상이라든지",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100017216.1.25",
          "form": "운동하는 모습 보고싶어요",
          "original_form": "운동하는 모습 보고싶어요",
          "speaker_id": "2",
          "time": "20210828 15:53"
        }
