      "utterance": [
        {
          "id": "MDRW2100022827.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022827.1.2",
          "form": "전 요즘 굴소스볶음밥에 꽂혔어요",
          "original_form": "전 요즘 굴소스볶음밥에 꽂혔어요",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022827.1.3",
          "form": "게으름뱅이가 만들 수 있는",
          "original_form": "게으름뱅이가 만들 수 있는",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022827.1.4",
          "form": "효율적인 음식",
          "original_form": "효율적인 음식",
          "speaker_id": "2",
          "time": "20210829 18:13"
        },
        {
          "id": "MDRW2100022827.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022827.1.6",
          "form": "냉장고에 있는 아유 왜이렇게 렉이 걸려",
          "original_form": "냉장고에 있는 아유 왜이렇게 렉이 걸려",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022827.1.7",
          "form": "냉장고 야채 다 털어넣고",
          "original_form": "냉장고 야채 다 털어넣고",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022827.1.8",
          "form": "저 굴소스 들어가는건 따로 안먹어봤어요",
          "original_form": "저 굴소스 들어가는건 따로 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022827.1.9",
          "form": "밥한주걱 굴소스 한 숟갈 넣으면",
          "original_form": "밥한주걱 굴소스 한 숟갈 넣으면",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022827.1.10",
          "form": "땡이여유",
          "original_form": "땡이여유",
          "speaker_id": "2",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100022827.1.11",
          "form": "와 꼭 해드셔보세요",
          "original_form": "와 꼭 해드셔보세요",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022827.1.12",
          "form": "아니 또 상상볶음밥 해 드실거죠",
          "original_form": "아니 또 상상볶음밥 해 드실거죠",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
          "id": "MDRW2100022827.1.13",
          "form": "다 알아요",
          "original_form": "다 알아요",
          "speaker_id": "2",
          "time": "20210829 18:15"
        },
        {
