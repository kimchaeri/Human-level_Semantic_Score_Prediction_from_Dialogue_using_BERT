      "utterance": [
        {
          "id": "MDRW2100014498.1.1",
          "form": "매달해요",
          "original_form": "매달해요",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014498.1.2",
          "form": "저도 해보게요 ㅎㅎ",
          "original_form": "저도 해보게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.3",
          "form": "근뎊요번에 많이주네요",
          "original_form": "근뎊요번에 많이주네요",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.4",
          "form": "요게 적용상품을 잘골라서 사야해요 ㅋㅋ",
          "original_form": "요게 적용상품을 잘골라서 사야해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.5",
          "form": "첫가입시 막커피주고",
          "original_form": "첫가입시 막커피주고",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.6",
          "form": "아...담달에도 하면 도전해봐야겠어요",
          "original_form": "아...담달에도 하면 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.7",
          "form": "쿠폰주고",
          "original_form": "쿠폰주고",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.8",
          "form": "혜택 잘보고 들어가야해요",
          "original_form": "혜택 잘보고 들어가야해요",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.9",
          "form": "아..그런것도 있어요?",
          "original_form": "아..그런것도 있어요?",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.10",
          "form": "이미 가입은 되어있지싶어요 ㅜㅠ",
          "original_form": "이미 가입은 되어있지싶어요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014498.1.11",
          "form": "벌써 신발도 싸게 샀어요",
          "original_form": "벌써 신발도 싸게 샀어요",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014498.1.12",
          "form": "생각없이 가입은 맨날 해두고 ㅋ",
          "original_form": "생각없이 가입은 맨날 해두고 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014498.1.13",
          "form": "아 아쉽다 ㅜㅜ",
          "original_form": "아 아쉽다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014498.1.14",
          "form": "요샌 가입 막하면 혜택이 없어요",
          "original_form": "요샌 가입 막하면 혜택이 없어요",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014498.1.15",
          "form": "다잡은 물고기 밥을안줘요 ㅡㅡ",
          "original_form": "다잡은 물고기 밥을안줘요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014498.1.16",
          "form": "그래도 룰렛도 있고 출석도 하면 좋아요",
          "original_form": "그래도 룰렛도 있고 출석도 하면 좋아요",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014498.1.17",
          "form": "나빠요",
          "original_form": "나빠요",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014498.1.18",
          "form": "기존회원도 우대좀 해주징",
          "original_form": "기존회원도 우대좀 해주징",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014498.1.19",
          "form": "네 상술이죠 ㅡㅡ",
          "original_form": "네 상술이죠 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014498.1.20",
          "form": "그러니꺼요갈수록 혜택이 줄죠",
          "original_form": "그러니꺼요갈수록 혜택이 줄죠",
          "speaker_id": "2",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.21",
          "form": "돌아서면 더 할인하고",
          "original_form": "돌아서면 더 할인하고",
          "speaker_id": "2",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.22",
          "form": "이러니 제돈주고 머사기 불안해요",
          "original_form": "이러니 제돈주고 머사기 불안해요",
          "speaker_id": "1",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.23",
          "form": "싸게산줄알았는데",
          "original_form": "싸게산줄알았는데",
          "speaker_id": "1",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.24",
          "form": "맞아요 바가지 같고",
          "original_form": "맞아요 바가지 같고",
          "speaker_id": "2",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.25",
          "form": "그담주에 할인들어가고 ㅋ",
          "original_form": "그담주에 할인들어가고 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.26",
          "form": "쿠폰먼저 봐야해요",
          "original_form": "쿠폰먼저 봐야해요",
          "speaker_id": "2",
          "time": "20210825 19:26"
        },
        {
          "id": "MDRW2100014498.1.27",
          "form": "오늘 위메프",
          "original_form": "오늘 위메프",
          "speaker_id": "2",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014498.1.28",
          "form": "수박쥬스인가도 100원에주고",
          "original_form": "수박쥬스인가도 100원에주고",
          "speaker_id": "2",
