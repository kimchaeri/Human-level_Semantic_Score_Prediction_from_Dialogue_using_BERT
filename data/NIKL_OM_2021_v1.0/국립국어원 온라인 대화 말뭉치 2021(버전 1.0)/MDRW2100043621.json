      "utterance": [
        {
          "id": "MDRW2100043621.1.1",
          "form": "나 아직 안정했는데",
          "original_form": "나 아직 안정했는데",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.2",
          "form": "고민중이야",
          "original_form": "고민중이야",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.3",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.4",
          "form": "거기 비와?",
          "original_form": "거기 비와?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.5",
          "form": "일단 컵라면과 만두가 가장",
          "original_form": "일단 컵라면과 만두가 가장",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.6",
          "form": "아니 안와",
          "original_form": "아니 안와",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.7",
          "form": "오 괜찮은 조합이군",
          "original_form": "오 괜찮은 조합이군",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.8",
          "form": "오전에 잠깐 오고 말았어",
          "original_form": "오전에 잠깐 오고 말았어",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.9",
          "form": "언니는 뭐먹어?",
          "original_form": "언니는 뭐먹어?",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.10",
          "form": "여긴 지금도 비와서 국물이 땡김",
          "original_form": "여긴 지금도 비와서 국물이 땡김",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043621.1.12",
          "form": "국물 뭐 먹을거야?",
          "original_form": "국물 뭐 먹을거야?",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.13",
          "form": "수제비?",
          "original_form": "수제비?",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.14",
          "form": "아직 확정은 아냐",
          "original_form": "아직 확정은 아냐",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.15",
          "form": "오 수제비",
          "original_form": "오 수제비",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.16",
          "form": "수제비 좋지",
          "original_form": "수제비 좋지",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.17",
          "form": "나도 수제비 좋아하는데",
          "original_form": "나도 수제비 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.18",
          "form": "내가 하면 맨날 망해",
          "original_form": "내가 하면 맨날 망해",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.19",
          "form": "아님 메밀 잔치국수를 먹을까",
          "original_form": "아님 메밀 잔치국수를 먹을까",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043621.1.20",
          "form": "아닠 수제비도 직접 해??",
          "original_form": "아닠 수제비도 직접 해??",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.21",
          "form": "아닠 걍 파는 면 사다",
          "original_form": "아닠 걍 파는 면 사다",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.22",
          "form": "국물에 투하하는정도?",
          "original_form": "국물에 투하하는정도?",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.23",
          "form": "그래도 대단하다 ㅋㅋㅋ",
          "original_form": "그래도 대단하다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.24",
          "form": "난 바지락 칼국수 수제비 추천",
          "original_form": "난 바지락 칼국수 수제비 추천",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.25",
          "form": "바지락국물 먹고싶다",
          "original_form": "바지락국물 먹고싶다",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.26",
          "form": "헐 그러게",
          "original_form": "헐 그러게",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043621.1.27",
          "form": "이 근처에 파는데가 있나",
          "original_form": "이 근처에 파는데가 있나",
          "speaker_id": "1",
          "time": "20210907 19:47"
        }
      ]
    }
