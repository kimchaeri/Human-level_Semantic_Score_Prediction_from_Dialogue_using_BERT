      },
      "utterance": [
        {
          "id": "MDRW2100044756.1.1",
          "form": "응 아빠가 딤섬 사와서 그거 먹었어",
          "original_form": "응 아빠가 딤섬 사와서 그거 먹었어",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044756.1.2",
          "form": "와 집 주변에 딤섬가게가 있었어?",
          "original_form": "와 집 주변에 딤섬가게가 있었어?",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044756.1.3",
          "form": "나 한번도 못봤는데",
          "original_form": "나 한번도 못봤는데",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044756.1.4",
          "form": "집 주변 말고 다른 동네에서 사옴. 유명한 집이래",
          "original_form": "집 주변 말고 다른 동네에서 사옴. 유명한 집이래",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044756.1.5",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044756.1.6",
          "form": "나도 담섬 땡기네",
          "original_form": "나도 담섬 땡기네",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044756.1.7",
          "form": "넌 저녁 먹었어?",
          "original_form": "넌 저녁 먹었어?",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044756.1.8",
          "form": "응 왕어혈교 먹었어 엄마가 준거",
          "original_form": "응 왕어혈교 먹었어 엄마가 준거",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044756.1.9",
          "form": "그거 맛 괜찮아?",
          "original_form": "그거 맛 괜찮아?",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044756.1.10",
          "form": "어 새우도 많이 들었고 엄청 탱글탱글해",
          "original_form": "어 새우도 많이 들었고 엄청 탱글탱글해",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044756.1.11",
          "form": "막내도 맛있다고 하더라. 더 시켜야겠네",
          "original_form": "막내도 맛있다고 하더라. 더 시켜야겠네",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044756.1.12",
          "form": "ㅋㅋㅋ근데 좀 많이 먹으면 질리는 맛이라서 아쉬워",
          "original_form": "ㅋㅋㅋ근데 좀 많이 먹으면 질리는 맛이라서 아쉬워",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044756.1.13",
          "form": "양이 너무 많으면 한번에 다 먹지 말고 두번에 나눠 먹어",
          "original_form": "양이 너무 많으면 한번에 다 먹지 말고 두번에 나눠 먹어",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044756.1.14",
          "form": "다음에 먹을 땐 그래야겠다",
          "original_form": "다음에 먹을 땐 그래야겠다",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044756.1.15",
          "form": "레몬 간장에 찍어먹으면 더 맛있을 거 같아",
          "original_form": "레몬 간장에 찍어먹으면 더 맛있을 거 같아",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044756.1.16",
          "form": "참고하겠음. 느끼하면 탄산음료랑 같이 먹는 것도 좋아",
          "original_form": "참고하겠음. 느끼하면 탄산음료랑 같이 먹는 것도 좋아",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044756.1.17",
          "form": "아이스크림 먹고 싶어",
          "original_form": "아이스크림 먹고 싶어",
          "speaker_id": "2",
          "time": "20210908 00:03"
        }
      ]
