      "utterance": [
        {
          "id": "MDRW2100017004.1.1",
          "form": "아 나도 여행 가고 싶다",
          "original_form": "아 나도 여행 가고 싶다",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100017004.1.2",
          "form": "어디가고 싶음",
          "original_form": "어디가고 싶음",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100017004.1.3",
          "form": "방콕",
          "original_form": "방콕",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100017004.1.4",
          "form": "태국 방콕",
          "original_form": "태국 방콕",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100017004.1.5",
          "form": "아님 미국",
          "original_form": "아님 미국",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100017004.1.6",
          "form": "방콕 가봤음?",
          "original_form": "방콕 가봤음?",
          "speaker_id": "1",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017004.1.7",
          "form": "안 가봐서 가고 싶어",
          "original_form": "안 가봐서 가고 싶어",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017004.1.8",
          "form": "두번이나 말하길래",
          "original_form": "두번이나 말하길래",
          "speaker_id": "1",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017004.1.9",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017004.1.10",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017004.1.11",
          "form": "강조용법?",
          "original_form": "강조용법?",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017004.1.12",
          "form": "태국 뭐가 끌려서 가고싶은데",
          "original_form": "태국 뭐가 끌려서 가고싶은데",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100017004.1.13",
          "form": "동남아 한 번 더 가보고 싶은데",
          "original_form": "동남아 한 번 더 가보고 싶은데",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.14",
          "form": "같이 갈만한 친구인",
          "original_form": "같이 갈만한 친구인",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.15",
          "form": "동남아 뭐가 맘에들어",
          "original_form": "동남아 뭐가 맘에들어",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.16",
          "form": "키큰애가 가고 싶어하니",
          "original_form": "키큰애가 가고 싶어하니",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.17",
          "form": "동료가 있다는 점",
          "original_form": "동료가 있다는 점",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.18",
          "form": "동남아 따듯해",
          "original_form": "동남아 따듯해",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.19",
          "form": "덥던데",
          "original_form": "덥던데",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.20",
          "form": "난 오키나와도 힘들었어",
          "original_form": "난 오키나와도 힘들었어",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017004.1.21",
          "form": "1월에 가면 따듯할거야",
          "original_form": "1월에 가면 따듯할거야",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017004.1.22",
          "form": "바다도 못 들어가",
          "original_form": "바다도 못 들어가",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017004.1.23",
          "form": "뭐 한국도 7월지나면",
          "original_form": "뭐 한국도 7월지나면",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017004.1.24",
          "form": "밖은더운데 바다 못들어가잖아",
          "original_form": "밖은더운데 바다 못들어가잖아",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017004.1.25",
          "form": "그정도면 온난하지",
          "original_form": "그정도면 온난하지",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017004.1.26",
          "form": "가서 호캉스 하자",
          "original_form": "가서 호캉스 하자",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017004.1.27",
          "form": "싸다",
          "original_form": "싸다",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017004.1.28",
          "form": "섭씨 35도가 넘으면 사람은 일사병에 걸릴수도 있음...",
          "original_form": "섭씨 35도가 넘으면 사람은 일사병에 걸릴수도 있음...",
          "speaker_id": "1",
          "time": "20210826 16:56"
        }
      ]
    }
