      "utterance": [
        {
          "id": "MDRW2100005311.1.1",
          "form": "밖이 안보여서 ㅎ",
          "original_form": "밖이 안보여서 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005311.1.2",
          "form": "태풍온다네요",
          "original_form": "태풍온다네요",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005311.1.3",
          "form": "이번주내내 비",
          "original_form": "이번주내내 비",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005311.1.4",
          "form": "아까뉴스보니 부산 난리났던데",
          "original_form": "아까뉴스보니 부산 난리났던데",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005311.1.5",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005311.1.6",
          "form": "일기예보 함봐보세요",
          "original_form": "일기예보 함봐보세요",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005311.1.7",
          "form": "아 엄청나게",
          "original_form": "아 엄청나게",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005311.1.8",
          "form": "뉴스보셨어요? 비피해가 좀있는거같던데",
          "original_form": "뉴스보셨어요? 비피해가 좀있는거같던데",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005311.1.9",
          "form": "네 봤어요",
          "original_form": "네 봤어요",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005311.1.10",
          "form": "대구는 괜찮대요?",
          "original_form": "대구는 괜찮대요?",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005311.1.11",
          "form": "아직",
          "original_form": "아직",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005311.1.12",
          "form": "맑아요",
          "original_form": "맑아요",
          "speaker_id": "1",
          "time": "20210822 11:53"
        },
        {
          "id": "MDRW2100005311.1.13",
          "form": "오늘은 비안오나보네요",
          "original_form": "오늘은 비안오나보네요",
          "speaker_id": "2",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100005311.1.14",
          "form": "네 그럴수도",
          "original_form": "네 그럴수도",
          "speaker_id": "1",
          "time": "20210822 11:55"
        },
        {
          "id": "MDRW2100005311.1.15",
          "form": "여기도 안오는듯",
          "original_form": "여기도 안오는듯",
