      "utterance": [
        {
          "id": "MDRW2100003531.1.1",
          "form": "엄마~ 케이스 살거에요?",
          "original_form": "엄마~ 케이스 살거에요?",
          "speaker_id": "1",
          "time": "20210821 00:35"
        },
        {
          "id": "MDRW2100003531.1.2",
          "form": "글쎄.. 얼만데?",
          "original_form": "글쎄.. 얼만데?",
          "speaker_id": "2",
          "time": "20210821 00:35"
        },
        {
          "id": "MDRW2100003531.1.3",
          "form": "어떤 디자인을 사느냐에 따라 다를 듯 한데~",
          "original_form": "어떤 디자인을 사느냐에 따라 다를 듯 한데~",
          "speaker_id": "1",
          "time": "20210821 00:35"
        },
        {
          "id": "MDRW2100003531.1.4",
          "form": "무난한게 좋아요? 화려한 게 좋아요?",
          "original_form": "무난한게 좋아요? 화려한 게 좋아요?",
          "speaker_id": "1",
          "time": "20210821 00:36"
        },
        {
          "id": "MDRW2100003531.1.5",
          "form": "무난한 거~",
          "original_form": "무난한 거~",
          "speaker_id": "2",
          "time": "20210821 00:36"
        },
        {
          "id": "MDRW2100003531.1.6",
          "form": "색깔은?",
          "original_form": "색깔은?",
          "speaker_id": "1",
          "time": "20210821 00:36"
        },
        {
          "id": "MDRW2100003531.1.7",
          "form": "무슨 색깔 있는데?",
          "original_form": "무슨 색깔 있는데?",
          "speaker_id": "2",
          "time": "20210821 00:36"
        },
        {
          "id": "MDRW2100003531.1.8",
          "form": "검정색, 파랑색, 회색, 빨간색, 노란색",
          "original_form": "검정색, 파랑색, 회색, 빨간색, 노란색",
          "speaker_id": "1",
          "time": "20210821 00:36"
        },
        {
          "id": "MDRW2100003531.1.9",
          "form": "무난한 건 검정이 제일 무난할 거 같은데",
          "original_form": "무난한 건 검정이 제일 무난할 거 같은데",
          "speaker_id": "1",
          "time": "20210821 00:36"
        },
        {
          "id": "MDRW2100003531.1.10",
          "form": "그런데 검정은 너무 어둡지 않나?",
          "original_form": "그런데 검정은 너무 어둡지 않나?",
          "speaker_id": "2",
          "time": "20210821 00:37"
        },
        {
          "id": "MDRW2100003531.1.11",
          "form": "검정이니까 어둡긴 하죠...",
          "original_form": "검정이니까 어둡긴 하죠...",
          "speaker_id": "1",
          "time": "20210821 00:37"
        },
        {
          "id": "MDRW2100003531.1.12",
          "form": "그럼 파랑색?",
          "original_form": "그럼 파랑색?",
          "speaker_id": "1",
          "time": "20210821 00:37"
        },
        {
          "id": "MDRW2100003531.1.13",
          "form": "파랑이 막 쨍한 파랑이 아니라 약간 네이비여서 괜찮을듯 한데",
          "original_form": "파랑이 막 쨍한 파랑이 아니라 약간 네이비여서 괜찮을듯 한데",
          "speaker_id": "1",
          "time": "20210821 00:37"
        },
        {
          "id": "MDRW2100003531.1.14",
          "form": "네이비?? 네이비가 무슨 색이야?",
          "original_form": "네이비?? 네이비가 무슨 색이야?",
          "speaker_id": "2",
          "time": "20210821 00:37"
        },
        {
          "id": "MDRW2100003531.1.15",
          "form": "군색? 어두운 파랑? 권색!",
          "original_form": "군색? 어두운 파랑? 권색!",
          "speaker_id": "1",
          "time": "20210821 00:37"
        },
        {
          "id": "MDRW2100003531.1.16",
          "form": "그럼 그게 좋겠네",
          "original_form": "그럼 그게 좋겠네",
          "speaker_id": "2",
          "time": "20210821 00:38"
        },
        {
          "id": "MDRW2100003531.1.17",
          "form": "그럼 파랑으로 시킬게요~",
          "original_form": "그럼 파랑으로 시킬게요~",
          "speaker_id": "1",
          "time": "20210821 00:38"
        },
        {
          "id": "MDRW2100003531.1.18",
          "form": "그래~",
          "original_form": "그래~",
          "speaker_id": "2",
          "time": "20210821 00:38"
        },
        {
          "id": "MDRW2100003531.1.19",
          "form": "엄마가 봤던 50만원대 폰은",
          "original_form": "엄마가 봤던 50만원대 폰은",
          "speaker_id": "1",
          "time": "20210821 00:54"
        },
        {
          "id": "MDRW2100003531.1.20",
          "form": "가개통 된 거래요~",
          "original_form": "가개통 된 거래요~",
          "speaker_id": "1",
          "time": "20210821 00:54"
        }
      ]
