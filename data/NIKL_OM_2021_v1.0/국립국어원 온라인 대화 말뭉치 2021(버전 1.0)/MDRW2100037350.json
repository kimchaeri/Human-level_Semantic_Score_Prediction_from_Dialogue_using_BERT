      "utterance": [
        {
          "id": "MDRW2100037350.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037350.1.2",
          "form": "안녕하세요 저녁 드셨나요?",
          "original_form": "안녕하세요 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100037350.1.3",
          "form": "네~ 저녁으로 불고기덮밥먹었어요ㅎㅎ",
          "original_form": "네~ 저녁으로 불고기덮밥먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037350.1.4",
          "form": "사신건가요?",
          "original_form": "사신건가요?",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037350.1.5",
          "form": "아뇨 해먹었어요 ㅋㅋㅋ",
          "original_form": "아뇨 해먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037350.1.6",
          "form": "직접 만드신건가요?",
          "original_form": "직접 만드신건가요?",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037350.1.7",
          "form": "네~ㅋㅋㅋ 근데양념장비율 약간 실패한거같긴해요 ㅋㅋ",
          "original_form": "네~ㅋㅋㅋ 근데양념장비율 약간 실패한거같긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037350.1.8",
          "form": "그래도 맛있을 것 같은데요? \r\n레시피는 어디서 보고 하셨나요?",
          "original_form": "그래도 맛있을 것 같은데요? \r\n레시피는 어디서 보고 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037350.1.9",
          "form": "레시피 없이 그냥 주워들은내용토대로 감으로했어요 ㅋㅋㅋ",
          "original_form": "레시피 없이 그냥 주워들은내용토대로 감으로했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037350.1.10",
          "form": "레시피보고할걸그랬나봐여",
          "original_form": "레시피보고할걸그랬나봐여",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037350.1.11",
          "form": "그래도 요리를 잘하시나봐요 불고기 양념장 만들기 쉽지 않은데",
          "original_form": "그래도 요리를 잘하시나봐요 불고기 양념장 만들기 쉽지 않은데",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037350.1.12",
          "form": "음 잘하진않지만 요리하는거 좋아해서 이것저것 많이해봐여~ㅋㅋㅋ",
          "original_form": "음 잘하진않지만 요리하는거 좋아해서 이것저것 많이해봐여~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037350.1.13",
          "form": "그럼 잘하시는 요리 있으신가요?",
          "original_form": "그럼 잘하시는 요리 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100037350.1.14",
          "form": "저는 바질페스토파스타 잘한다고 자부(?)합니다 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 바질페스토파스타 잘한다고 자부(?)합니다 ㅋㅋㅋㅋㅋ",
