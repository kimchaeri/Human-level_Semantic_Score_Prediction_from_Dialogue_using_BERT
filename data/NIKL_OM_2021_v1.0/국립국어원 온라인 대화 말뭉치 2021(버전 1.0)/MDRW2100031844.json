      "utterance": [
        {
          "id": "MDRW2100031844.1.1",
          "form": "너무 재밌네요 이런대홬ㅋㅋ",
          "original_form": "너무 재밌네요 이런대홬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.2",
          "form": "진짜 너무 좋습니다.",
          "original_form": "진짜 너무 좋습니다.",
          "speaker_id": "1",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.3",
          "form": "아니 저 이 이벤트도",
          "original_form": "아니 저 이 이벤트도",
          "speaker_id": "1",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.4",
          "form": "블로그 이웃님 덕분에 알았거든요",
          "original_form": "블로그 이웃님 덕분에 알았거든요",
          "speaker_id": "1",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.5",
          "form": "그래서 여기저기 말하고 다니고...",
          "original_form": "그래서 여기저기 말하고 다니고...",
          "speaker_id": "1",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.6",
          "form": "오오 저는 트위터에서...",
          "original_form": "오오 저는 트위터에서...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.7",
          "form": "오오 트위터",
          "original_form": "오오 트위터",
          "speaker_id": "1",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.8",
          "form": "저는 아이디 만들어두기만 하고",
          "original_form": "저는 아이디 만들어두기만 하고",
          "speaker_id": "1",
          "time": "20210905 02:45"
        },
        {
          "id": "MDRW2100031844.1.9",
          "form": "글이 잘 안써지더라구요",
          "original_form": "글이 잘 안써지더라구요",
          "speaker_id": "1",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.10",
          "form": "오오...",
          "original_form": "오오...",
          "speaker_id": "2",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.11",
          "form": "저는 진짜 그냥 똥마렵다 이런말써요",
          "original_form": "저는 진짜 그냥 똥마렵다 이런말써요",
          "speaker_id": "2",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.12",
          "form": "블로그는 제가 많이 쓰면 그만큼 많이들 봐주시는데",
          "original_form": "블로그는 제가 많이 쓰면 그만큼 많이들 봐주시는데",
          "speaker_id": "1",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.13",
          "form": "아 넘웃겨",
          "original_form": "아 넘웃겨",
          "speaker_id": "1",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.14",
          "form": "아 그런걸 쓰는거군요",
          "original_form": "아 그런걸 쓰는거군요",
          "speaker_id": "1",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.15",
          "form": "ㅋㅋ....",
          "original_form": "ㅋㅋ....",
          "speaker_id": "2",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.16",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.17",
          "form": "그럼 저도 열심히 써볼게요",
          "original_form": "그럼 저도 열심히 써볼게요",
          "speaker_id": "1",
          "time": "20210905 02:46"
        },
        {
          "id": "MDRW2100031844.1.18",
