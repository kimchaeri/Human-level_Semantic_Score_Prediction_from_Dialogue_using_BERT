      "utterance": [
        {
          "id": "MDRW2100046368.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 16:50"
        },
        {
          "id": "MDRW2100046368.1.2",
          "form": "안녕하세요~ 날이 많이 추워졌어요!",
          "original_form": "안녕하세요~ 날이 많이 추워졌어요!",
          "speaker_id": "2",
          "time": "20211019 16:50"
        },
        {
          "id": "MDRW2100046368.1.3",
          "form": "가을이 사라졌어요",
          "original_form": "가을이 사라졌어요",
          "speaker_id": "1",
          "time": "20211019 16:50"
        },
        {
          "id": "MDRW2100046368.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20211019 16:50"
        },
        {
          "id": "MDRW2100046368.1.5",
          "form": "그러게요 정말 순식간에 추워져서 가을을 즐기지도 못해서 너무 아쉽네요..",
          "original_form": "그러게요 정말 순식간에 추워져서 가을을 즐기지도 못해서 너무 아쉽네요..",
          "speaker_id": "2",
          "time": "20211019 16:51"
        },
        {
          "id": "MDRW2100046368.1.6",
          "form": "추위 많이 타시나요?",
          "original_form": "추위 많이 타시나요?",
          "speaker_id": "2",
          "time": "20211019 16:51"
        },
        {
          "id": "MDRW2100046368.1.7",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20211019 16:51"
        },
        {
          "id": "MDRW2100046368.1.8",
          "form": "네.. 추운거 너무 힘들어요",
          "original_form": "네.. 추운거 너무 힘들어요",
          "speaker_id": "1",
          "time": "20211019 16:52"
        },
        {
          "id": "MDRW2100046368.1.9",
          "form": "빨리 봄이 오면 좋겠어요",
          "original_form": "빨리 봄이 오면 좋겠어요",
          "speaker_id": "1",
          "time": "20211019 16:52"
        },
        {
          "id": "MDRW2100046368.1.10",
          "form": "저는 더운것보다는 추운게 좋아서 조금 설레요 ㅋㅋ",
          "original_form": "저는 더운것보다는 추운게 좋아서 조금 설레요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 16:52"
        },
        {
          "id": "MDRW2100046368.1.11",
          "form": "그러시군요.. 전 차라리 더운게 나아요.",
          "original_form": "그러시군요.. 전 차라리 더운게 나아요.",
          "speaker_id": "1",
          "time": "20211019 16:53"
        },
        {
          "id": "MDRW2100046368.1.12",
          "form": "오호.. 하긴 더우나 추우나 밖에 안나가고 실내에만 있으면 그게 천국이죠 ㅎㅎ",
          "original_form": "오호.. 하긴 더우나 추우나 밖에 안나가고 실내에만 있으면 그게 천국이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 16:54"
        },
        {
          "id": "MDRW2100046368.1.13",
          "form": "그쵸 방콕이 최고에요",
          "original_form": "그쵸 방콕이 최고에요",
          "speaker_id": "1",
          "time": "20211019 16:55"
        },
        {
          "id": "MDRW2100046368.1.14",
          "form": "겨울에 침대 이불 속에서 아이스크림 파 먹으며 티비 보는거 좋아합니다 ㅋㅋ",
          "original_form": "겨울에 침대 이불 속에서 아이스크림 파 먹으며 티비 보는거 좋아합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 16:56"
        },
        {
          "id": "MDRW2100046368.1.15",
          "form": "ㅋㅋㅋ그건 만국 공통의 천국 아닌가요",
          "original_form": "ㅋㅋㅋ그건 만국 공통의 천국 아닌가요",
          "speaker_id": "2",
          "time": "20211019 16:56"
        },
        {
          "id": "MDRW2100046368.1.16",
          "form": "앗... 그런가요? ㅋㅋ",
          "original_form": "앗... 그런가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 16:57"
        },
        {
          "id": "MDRW2100046368.1.17",
          "form": "저도 겨울에 전기장판 위에서 귤까먹으면서 티비 보는거 좋아합니다 ㅎㅎ",
          "original_form": "저도 겨울에 전기장판 위에서 귤까먹으면서 티비 보는거 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 16:57"
        }
      ]
