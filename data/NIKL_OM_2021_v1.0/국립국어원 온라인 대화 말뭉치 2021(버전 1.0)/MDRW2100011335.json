      "utterance": [
        {
          "id": "MDRW2100011335.1.1",
          "form": "점심 뭐드실건가용?",
          "original_form": "점심 뭐드실건가용?",
          "speaker_id": "2",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100011335.1.2",
          "form": "아직 정해진 게 없어요 리틀 송님은 정해진 거 있으신요",
          "original_form": "아직 정해진 게 없어요 리틀 송님은 정해진 거 있으신요",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100011335.1.3",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "1",
          "time": "20210824 11:15"
        },
        {
          "id": "MDRW2100011335.1.4",
          "form": "저는 아마 그냥 집밥먹을것같아요~~ㅎㅎ",
          "original_form": "저는 아마 그냥 집밥먹을것같아요~~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011335.1.5",
          "form": "요즘 꽃힌 음식 있으세요?",
          "original_form": "요즘 꽃힌 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011335.1.6",
          "form": "저 요즘 꽈배기 집에서 파는 핫도그에 꽂혔어요",
          "original_form": "저 요즘 꽈배기 집에서 파는 핫도그에 꽂혔어요",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011335.1.7",
          "form": "핫도그 좋아하시나요",
          "original_form": "핫도그 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011335.1.8",
          "form": "오 너무 맛있죠",
          "original_form": "오 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011335.1.9",
          "form": "설탕 묻혀서ㅎㅎㅎ",
          "original_form": "설탕 묻혀서ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011335.1.10",
          "form": "설탕 묻히면 너무 맛있죠",
          "original_form": "설탕 묻히면 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011335.1.11",
          "form": "소스는 뿌려 드시는 편인가요",
          "original_form": "소스는 뿌려 드시는 편인가요",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011335.1.12",
          "form": "저는 케찹이랑 머스타드 뿌려 먹어요",
          "original_form": "저는 케찹이랑 머스타드 뿌려 먹어요",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011335.1.13",
          "form": "네!! 저도 케챱이랑 머스타드요~~설탕에 소스뿌려 먹는거 누가 생각했을까요...천재같아요ㅎㅎ",
          "original_form": "네!! 저도 케챱이랑 머스타드요~~설탕에 소스뿌려 먹는거 누가 생각했을까요...천재같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011335.1.14",
          "form": "한동안 명랑핫도그만 먹었는데 ㅎㅎ",
          "original_form": "한동안 명랑핫도그만 먹었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011335.1.15",
          "form": "진짜요 어떤 맛잘알 이 생각해 냈는지 천재같아요",
          "original_form": "진짜요 어떤 맛잘알 이 생각해 냈는지 천재같아요",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011335.1.16",
          "form": "명랑에서 어떤 핫도그 자주 드시나요",
          "original_form": "명랑에서 어떤 핫도그 자주 드시나요",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011335.1.17",
          "form": "전 통모짜나 점보 자주먹어요!!ㅎㅎㅎ",
          "original_form": "전 통모짜나 점보 자주먹어요!!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100011335.1.18",
          "form": "가끔 감자나 고구마 묻힌것도 먹는데 그건 헤비해서 가끔 식사대용으로도 좋아요ㅎㅎ",
          "original_form": "가끔 감자나 고구마 묻힌것도 먹는데 그건 헤비해서 가끔 식사대용으로도 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100011335.1.19",
          "form": "맞아요 통모짜 진짜 맛있어요 명랑 핫도그의 시그니처 같은 느낌",
          "original_form": "맞아요 통모짜 진짜 맛있어요 명랑 핫도그의 시그니처 같은 느낌",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011335.1.20",
          "form": "치즈가 쭉 늘어나는 게 매력인 거 같아요",
          "original_form": "치즈가 쭉 늘어나는 게 매력인 거 같아요",
