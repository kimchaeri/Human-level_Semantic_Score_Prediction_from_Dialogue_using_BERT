      "utterance": [
        {
          "id": "MDRW2100039618.1.1",
          "form": "뭐 좀 드셨나요?ㅎ",
          "original_form": "뭐 좀 드셨나요?ㅎ",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039618.1.2",
          "form": "저는 아직안먹었어요ㅠ",
          "original_form": "저는 아직안먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039618.1.3",
          "form": "배고프지 않나요ㅜ",
          "original_form": "배고프지 않나요ㅜ",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039618.1.4",
          "form": "9시에 꿔바로우 먹기로 해서",
          "original_form": "9시에 꿔바로우 먹기로 해서",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039618.1.5",
          "form": "잠자코기다리고있습니다ㅎㅎ",
          "original_form": "잠자코기다리고있습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039618.1.6",
          "form": "아 배달요?ㅎㅎ",
          "original_form": "아 배달요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039618.1.7",
          "form": "포장이요!",
          "original_form": "포장이요!",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039618.1.8",
          "form": "오오ㅎㅎ저도 탕수육보단 꿔바로우가ㅋㅋㅋ",
          "original_form": "오오ㅎㅎ저도 탕수육보단 꿔바로우가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039618.1.9",
          "form": "콜라 사이다중에는",
          "original_form": "콜라 사이다중에는",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039618.1.10",
          "form": "뭐좋아하세요?!",
          "original_form": "뭐좋아하세요?!",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039618.1.11",
          "form": "ㅎㅎㅎ딱히ㅋㅋㅋ둘다 잘먹어요",
          "original_form": "ㅎㅎㅎ딱히ㅋㅋㅋ둘다 잘먹어요",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039618.1.12",
          "form": "앗 저도ㅋㅋㅋ",
          "original_form": "앗 저도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039618.1.13",
          "form": "탄산정말좋아해요",
          "original_form": "탄산정말좋아해요",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100039618.1.14",
          "form": "치아에 안좋대서ㅠ",
          "original_form": "치아에 안좋대서ㅠ",
          "speaker_id": "2",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039618.1.15",
          "form": "탄산수로 바꿀까봐요",
          "original_form": "탄산수로 바꿀까봐요",
          "speaker_id": "2",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100039618.1.16",
          "form": "저도 요즘 탄산수 노력중이에요",
          "original_form": "저도 요즘 탄산수 노력중이에요",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039618.1.17",
          "form": "씨그램 맛있더라고요",
          "original_form": "씨그램 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039618.1.18",
          "form": "ㅎㅎㅎ네네 그냥 먹어야는데ㅜ",
          "original_form": "ㅎㅎㅎ네네 그냥 먹어야는데ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039618.1.19",
          "form": "저는 자꾸 단게좋아서 청을 타게됩니다ㅜ",
          "original_form": "저는 자꾸 단게좋아서 청을 타게됩니다ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100039618.1.20",
          "form": "ㅋㅋㅋㅋㅋㄱ에이드되는거죠",
          "original_form": "ㅋㅋㅋㅋㅋㄱ에이드되는거죠",
          "speaker_id": "1",
