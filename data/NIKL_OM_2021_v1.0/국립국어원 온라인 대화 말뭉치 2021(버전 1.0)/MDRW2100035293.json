      "utterance": [
        {
          "id": "MDRW2100035293.1.1",
          "form": "오븐이 갖고 싶어졌어요.ㅈ",
          "original_form": "오븐이 갖고 싶어졌어요.ㅈ",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100035293.1.2",
          "form": "베이킹 하시나요",
          "original_form": "베이킹 하시나요",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100035293.1.3",
          "form": "아뇹!",
          "original_form": "아뇹!",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035293.1.4",
          "form": "치즈오븐스파게티를 집에서 먹고싶어욥",
          "original_form": "치즈오븐스파게티를 집에서 먹고싶어욥",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100035293.1.5",
          "form": "오오 집에서 오븐스파케티 왕 고급지네요",
          "original_form": "오오 집에서 오븐스파케티 왕 고급지네요",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100035293.1.6",
          "form": "ㅋㅋㅋㅋㅋ한참 오븐스파게티에 환장했었는데",
          "original_form": "ㅋㅋㅋㅋㅋ한참 오븐스파게티에 환장했었는데",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100035293.1.7",
          "form": "그춍!",
          "original_form": "그춍!",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100035293.1.8",
          "form": "크으 좋아하셨군요",
          "original_form": "크으 좋아하셨군요",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100035293.1.9",
          "form": "진짜 너무 맛있지 않나요?!",
          "original_form": "진짜 너무 맛있지 않나요?!",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100035293.1.10",
          "form": "넹 대학때 최애메뉴여서 맨날 먹으러갔어요ㅋㅋㅋㅋ",
          "original_form": "넹 대학때 최애메뉴여서 맨날 먹으러갔어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100035293.1.11",
          "form": "갑자기 그 맛이 떠오르네요 오븐스파게티 땡긴다요...",
          "original_form": "갑자기 그 맛이 떠오르네요 오븐스파게티 땡긴다요...",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100035293.1.12",
          "form": "오븐 작은 거 생각하시나요?",
          "original_form": "오븐 작은 거 생각하시나요?",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035293.1.13",
          "form": "크으.. 다음엔 오븐스파게티를!",
          "original_form": "크으.. 다음엔 오븐스파게티를!",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100035293.1.14",
          "form": "작은거 생각했는데 작은거 사지말고 큰거 사라더라구요",
          "original_form": "작은거 생각했는데 작은거 사지말고 큰거 사라더라구요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035293.1.15",
          "form": "그래서 고민돼요ㅠ",
          "original_form": "그래서 고민돼요ㅠ",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100035293.1.16",
          "form": "그러게여ㅋㅋㅋㅋ이왕 사는거 커야 쓰임새그 클테니까",
          "original_form": "그러게여ㅋㅋㅋㅋ이왕 사는거 커야 쓰임새그 클테니까",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100035293.1.17",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035293.1.18",
          "form": "그렇다더라구요",
          "original_form": "그렇다더라구요",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035293.1.19",
          "form": "사면 또 이것저것 하고싶을거 같아요ㅋㅋ",
          "original_form": "사면 또 이것저것 하고싶을거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035293.1.20",
          "form": "마자여 사놓으면 다 쓰게 되어있단 말이져",
          "original_form": "마자여 사놓으면 다 쓰게 되어있단 말이져",
          "speaker_id": "2",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100035293.1.21",
          "form": "마자용!!",
          "original_form": "마자용!!",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100035293.1.22",
          "form": "근데 생각보다 비싸더라구요?ㅜㅠ",
          "original_form": "근데 생각보다 비싸더라구요?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100035293.1.23",
          "form": "앗 얼마나 해요",
          "original_form": "앗 얼마나 해요",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100035293.1.24",
          "form": "욕심은 끝이없다고 찾다보니 2ㅡ30정도는 되야할거같더라구요ㅋㅋㄲ",
          "original_form": "욕심은 끝이없다고 찾다보니 2ㅡ30정도는 되야할거같더라구요ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100035293.1.25",
          "form": "오 진짜 급 사기엔 좀 부담되는 금액이네요 ㅋㅋㅋ",
          "original_form": "오 진짜 급 사기엔 좀 부담되는 금액이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100035293.1.26",
          "form": "그쵸ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100035293.1.27",
          "form": "ㅋㅋㅋㅋㅋ그냥 오븐스파게티 식당 찾아가는게 낫겠는데여",
          "original_form": "ㅋㅋㅋㅋㅋ그냥 오븐스파게티 식당 찾아가는게 낫겠는데여",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100035293.1.28",
          "form": "그럴 것 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "그럴 것 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
