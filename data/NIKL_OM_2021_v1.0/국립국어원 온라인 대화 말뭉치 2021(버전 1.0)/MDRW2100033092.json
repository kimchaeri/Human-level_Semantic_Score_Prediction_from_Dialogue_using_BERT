          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033092.1.4",
          "form": "근데 날씨 진짜많이풀려서 이젠 1도안더움",
          "original_form": "근데 날씨 진짜많이풀려서 이젠 1도안더움",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033092.1.5",
          "form": "밤엔 추워요",
          "original_form": "밤엔 추워요",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100033092.1.6",
          "form": "저 여름 이불 넣.고",
          "original_form": "저 여름 이불 넣.고",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033092.1.7",
          "form": "극세사 꺼냄ㅋㅋㅋ",
          "original_form": "극세사 꺼냄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100033092.1.8",
          "form": "극세사 너무갔다",
          "original_form": "극세사 너무갔다",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033092.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033092.1.10",
          "form": "추위 잘 버티나봐요?",
          "original_form": "추위 잘 버티나봐요?",
          "speaker_id": "2",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100033092.1.11",
          "form": "더운거 vs 추운거",
          "original_form": "더운거 vs 추운거",
          "speaker_id": "2",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100033092.1.12",
          "form": "추운게낫죠",
          "original_form": "추운게낫죠",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100033092.1.13",
          "form": "입으면되는데",
          "original_form": "입으면되는데",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100033092.1.14",
          "form": "문다닫고 극세사덮고잔다구여?",
          "original_form": "문다닫고 극세사덮고잔다구여?",
          "speaker_id": "1",
          "time": "20210906 16:32"
        },
        {
          "id": "MDRW2100033092.1.15",
          "form": "수족냉증있어서 그런가ㅋㅋ",
          "original_form": "수족냉증있어서 그런가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100033092.1.16",
          "form": "추운걸 더 못버팀",
          "original_form": "추운걸 더 못버팀",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100033092.1.17",
          "form": "아 더운건 벗어도 끝나는데",
          "original_form": "아 더운건 벗어도 끝나는데",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033092.1.18",
          "form": "겨울은 핫팩도잇고 버티기 괜찮음",
          "original_form": "겨울은 핫팩도잇고 버티기 괜찮음",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100033092.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
