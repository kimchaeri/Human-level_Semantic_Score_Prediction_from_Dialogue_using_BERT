      "utterance": [
        {
          "id": "MDRW2100042471.1.1",
          "form": "가긴했는데",
          "original_form": "가긴했는데",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.2",
          "form": "낚시 자체는 별거 없어",
          "original_form": "낚시 자체는 별거 없어",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.3",
          "form": "ㄹㅇ 딱 받고",
          "original_form": "ㄹㅇ 딱 받고",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.4",
          "form": "시작도 안했어",
          "original_form": "시작도 안했어",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.5",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.6",
          "form": "아니 딴거 할게 너무 많아",
          "original_form": "아니 딴거 할게 너무 많아",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.7",
          "form": "이거 물고기 있는데 찾기가",
          "original_form": "이거 물고기 있는데 찾기가",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.8",
          "form": "너무 힘들어",
          "original_form": "너무 힘들어",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042471.1.9",
          "form": "글킨 하지...",
          "original_form": "글킨 하지...",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042471.1.10",
          "form": "이건 좀 고인물용으로 낸 거같긴 함",
          "original_form": "이건 좀 고인물용으로 낸 거같긴 함",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042471.1.11",
          "form": "나 이제 페보니우스창이랑 라이덴 특성강화도 해야대서",
          "original_form": "나 이제 페보니우스창이랑 라이덴 특성강화도 해야대서",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042471.1.12",
          "form": "코등이 쓸데없다고",
          "original_form": "코등이 쓸데없다고",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042471.1.13",
          "form": "안갈아버린거 ㄹㅇ",
          "original_form": "안갈아버린거 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042471.1.14",
          "form": "천만다행이야.....",
          "original_form": "천만다행이야.....",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042471.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 여기는 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 여기는 진짜",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.16",
          "form": "신규템은 엥간해선 갈면 안돼",
          "original_form": "신규템은 엥간해선 갈면 안돼",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.17",
          "form": "코등이가 산처럼 쌓여있었거든",
          "original_form": "코등이가 산처럼 쌓여있었거든",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.18",
          "form": "앞으로도",
          "original_form": "앞으로도",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.19",
          "form": "무랑 닭다리나",
          "original_form": "무랑 닭다리나",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.20",
          "form": "갈아야겠다",
          "original_form": "갈아야겠다",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 부럽다...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 부럽다...",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.22",
          "form": "난 주인공 특성 좀 올렷더니",
          "original_form": "난 주인공 특성 좀 올렷더니",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.23",
          "form": "고갈상태임",
          "original_form": "고갈상태임",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042471.1.24",
          "form": "아니 월렙 6인데 왜째서 오래된 코등이만 나오는지...?",
          "original_form": "아니 월렙 6인데 왜째서 오래된 코등이만 나오는지...?",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.25",
          "form": "아 번개속성에 코등이 들어?",
          "original_form": "아 번개속성에 코등이 들어?",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.26",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.27",
          "form": "나 번개는 ㄹㅇ 손도 안대서",
          "original_form": "나 번개는 ㄹㅇ 손도 안대서",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.28",
          "form": "근데 앞으로도 안대도 될듯",
          "original_form": "근데 앞으로도 안대도 될듯",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.29",
          "form": "코등이랑 이나즈마 비경에서 나오는 특성책",
          "original_form": "코등이랑 이나즈마 비경에서 나오는 특성책",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.30",
          "form": "첫 5성이 라이덴이라",
          "original_form": "첫 5성이 라이덴이라",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042471.1.31",
          "form": "번갈아가면서 나옴",
          "original_form": "번갈아가면서 나옴",
          "speaker_id": "1",
          "time": "20210906 22:23"
        }
      ]
    }
  ]
