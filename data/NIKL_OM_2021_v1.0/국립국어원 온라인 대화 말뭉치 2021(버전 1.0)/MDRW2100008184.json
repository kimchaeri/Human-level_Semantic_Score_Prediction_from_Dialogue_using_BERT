      "utterance": [
        {
          "id": "MDRW2100008184.1.1",
          "form": "요즘은 재택하는 회사가 늘어났음",
          "original_form": "요즘은 재택하는 회사가 늘어났음",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008184.1.2",
          "form": "재택해봤니",
          "original_form": "재택해봤니",
          "speaker_id": "1",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008184.1.3",
          "form": "난안해봄",
          "original_form": "난안해봄",
          "speaker_id": "1",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008184.1.4",
          "form": "재택해봤는데 장점도 있고 단점도 있음",
          "original_form": "재택해봤는데 장점도 있고 단점도 있음",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008184.1.5",
          "form": "재택언제해봄",
          "original_form": "재택언제해봄",
          "speaker_id": "1",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008184.1.6",
          "form": "코로나 시작즈음에 해봄",
          "original_form": "코로나 시작즈음에 해봄",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008184.1.7",
          "form": "장점은 출근 안해도 되는 점",
          "original_form": "장점은 출근 안해도 되는 점",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008184.1.8",
          "form": "단점은 일을 안한다는 점",
          "original_form": "단점은 일을 안한다는 점",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008184.1.9",
          "form": "장점밖에없네요",
          "original_form": "장점밖에없네요",
          "speaker_id": "1",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008184.1.10",
          "form": "다음날 출근하면 2배의 일이 기다리고 있지",
          "original_form": "다음날 출근하면 2배의 일이 기다리고 있지",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008184.1.11",
          "form": "그래도 재택해면 시간을 유용하게 쓸 수 있어서 좋음",
          "original_form": "그래도 재택해면 시간을 유용하게 쓸 수 있어서 좋음",
          "speaker_id": "2",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008184.1.12",
          "form": "재택하고싶다",
          "original_form": "재택하고싶다",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008184.1.13",
          "form": "휴직은해봤어요",
          "original_form": "휴직은해봤어요",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008184.1.14",
          "form": "짱임",
          "original_form": "짱임",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008184.1.15",
          "form": "휴직도 좋지",
          "original_form": "휴직도 좋지",
          "speaker_id": "2",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008184.1.16",
          "form": "하지만 공식적으로 일하면서 월루하는 재택도 좋음",
          "original_form": "하지만 공식적으로 일하면서 월루하는 재택도 좋음",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008184.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008184.1.18",
          "form": "재택소취",
          "original_form": "재택소취",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008184.1.19",
          "form": "재택은 일주일에 한 두번 정도가 좋은 것 같음",
          "original_form": "재택은 일주일에 한 두번 정도가 좋은 것 같음",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008184.1.20",
          "form": "실제 회사에 나가서 처리해야하는 업무도 많으니까",
          "original_form": "실제 회사에 나가서 처리해야하는 업무도 많으니까",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008184.1.21",
          "form": "풀재택하고싶어",
          "original_form": "풀재택하고싶어",
