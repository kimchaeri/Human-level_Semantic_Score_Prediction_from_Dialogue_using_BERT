      "utterance": [
        {
          "id": "MDRW2100010769.1.1",
          "form": "혹시 취업하신 분이신가요?",
          "original_form": "혹시 취업하신 분이신가요?",
          "speaker_id": "1",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010769.1.2",
          "form": "저는 아직 준비중이에요!!ㅎㅎ\r\n취뽀하셨나용ㅎㅎ",
          "original_form": "저는 아직 준비중이에요!!ㅎㅎ\r\n취뽀하셨나용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010769.1.3",
          "form": "앗 저도 취준생입니다!!!",
          "original_form": "앗 저도 취준생입니다!!!",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010769.1.4",
          "form": "알바하면서 취준하고 있어요",
          "original_form": "알바하면서 취준하고 있어요",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010769.1.5",
          "form": "알바하시나요?!",
          "original_form": "알바하시나요?!",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010769.1.6",
          "form": "앗 저는 감사하게도 부모님이 용돈 지원해주셔서 알바는 안하고있어요!!ㅎㅎ",
          "original_form": "앗 저는 감사하게도 부모님이 용돈 지원해주셔서 알바는 안하고있어요!!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010769.1.7",
          "form": "알바 뭐하시나요?",
          "original_form": "알바 뭐하시나요?",
          "speaker_id": "2",
          "time": "20210823 21:51"
        },
        {
          "id": "MDRW2100010769.1.8",
          "form": "헉 좋다좋다",
          "original_form": "헉 좋다좋다",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010769.1.9",
          "form": "저 알바 파바알바하고있어요!!!",
          "original_form": "저 알바 파바알바하고있어요!!!",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010769.1.10",
          "form": "저번에 장학생으로 뽑혔답니다",
          "original_form": "저번에 장학생으로 뽑혔답니다",
          "speaker_id": "1",
          "time": "20210823 21:52"
        },
        {
          "id": "MDRW2100010769.1.11",
          "form": "와 완전 대단해요!!!",
          "original_form": "와 완전 대단해요!!!",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.12",
          "form": "일도 하시면서 취준하시는데",
          "original_form": "일도 하시면서 취준하시는데",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.13",
          "form": "장학생까지!!",
          "original_form": "장학생까지!!",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.14",
          "form": "크크 파바 오전에 하세요?",
          "original_form": "크크 파바 오전에 하세요?",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.15",
          "form": "오픈조는 엄청 일찍부터 하던데",
          "original_form": "오픈조는 엄청 일찍부터 하던데",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.16",
          "form": "엇 맞아요!!",
          "original_form": "엇 맞아요!!",
          "speaker_id": "1",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.17",
          "form": "7시부터 해요",
          "original_form": "7시부터 해요",
          "speaker_id": "1",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010769.1.18",
          "form": "근데 그게 좋은게 어차피 안일어나는 시간에 하는거라",
          "original_form": "근데 그게 좋은게 어차피 안일어나는 시간에 하는거라",
          "speaker_id": "1",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010769.1.19",
          "form": "완전 아침일찍이네요ㅎㅎㅎ",
          "original_form": "완전 아침일찍이네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010769.1.20",
          "form": "뭔가 오후가 있는 삶 체험 가능합니다",
          "original_form": "뭔가 오후가 있는 삶 체험 가능합니다",
          "speaker_id": "1",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010769.1.21",
          "form": "혹시 호옥시 공시준비하시나요?",
          "original_form": "혹시 호옥시 공시준비하시나요?",
          "speaker_id": "1",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010769.1.22",
