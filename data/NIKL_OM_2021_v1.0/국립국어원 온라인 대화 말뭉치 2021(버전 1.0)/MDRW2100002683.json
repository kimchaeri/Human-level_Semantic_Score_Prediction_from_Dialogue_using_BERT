      "utterance": [
        {
          "id": "MDRW2100002683.1.1",
          "form": "오늘 친구 졸업식인데 뭘 살까",
          "original_form": "오늘 친구 졸업식인데 뭘 살까",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.2",
          "form": "음. 꽃다발은 어때?",
          "original_form": "음. 꽃다발은 어때?",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.3",
          "form": "학교 앞에서 뭘 판매하고 있는거야?",
          "original_form": "학교 앞에서 뭘 판매하고 있는거야?",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.4",
          "form": "그런데 학교 근처에서 파는것들은",
          "original_form": "그런데 학교 근처에서 파는것들은",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.5",
          "form": "상태가 별로야",
          "original_form": "상태가 별로야",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.6",
          "form": "아니라면 인터넷으로 배송받을 생각이니?",
          "original_form": "아니라면 인터넷으로 배송받을 생각이니?",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.7",
          "form": "그리고 조환데도 비싸게 팔아",
          "original_form": "그리고 조환데도 비싸게 팔아",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.8",
          "form": "아 아쉽구나. 상태가 어떤데?",
          "original_form": "아 아쉽구나. 상태가 어떤데?",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.9",
          "form": "한철장사가 너무 심해",
          "original_form": "한철장사가 너무 심해",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002683.1.10",
          "form": "조화인데 비싸다니 악랄하구나.",
          "original_form": "조화인데 비싸다니 악랄하구나.",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002683.1.11",
          "form": "좀 시들시들하더라 꽃들이",
          "original_form": "좀 시들시들하더라 꽃들이",
          "speaker_id": "2",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002683.1.12",
          "form": "편지같은건 산 거야?",
          "original_form": "편지같은건 산 거야?",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002683.1.13",
          "form": "아쉽다. 다른 것들이 좋겠어.",
          "original_form": "아쉽다. 다른 것들이 좋겠어.",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002683.1.14",
          "form": "선물하고도 머쓱해질 상품이구나.",
          "original_form": "선물하고도 머쓱해질 상품이구나.",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002683.1.15",
          "form": "편지는 못샀어ㅠㅠ",
          "original_form": "편지는 못샀어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002683.1.16",
          "form": "다음에 카톡으로 마음을 전해야겠어",
          "original_form": "다음에 카톡으로 마음을 전해야겠어",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002683.1.17",
          "form": "좋은 생각이야",
          "original_form": "좋은 생각이야",
          "speaker_id": "1",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002683.1.18",
          "form": "마카롱을 선물하는 건 어때?",
          "original_form": "마카롱을 선물하는 건 어때?",
          "speaker_id": "1",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002683.1.19",
          "form": "요즘 마카롱도 유행이더라.",
          "original_form": "요즘 마카롱도 유행이더라.",
          "speaker_id": "1",
          "time": "20210820 14:49"
        }
      ]
