      "utterance": [
        {
          "id": "MDRW2100015416.1.1",
          "form": "오늘저녁은 뭐였나요!!!",
          "original_form": "오늘저녁은 뭐였나요!!!",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015416.1.2",
          "form": "어제 도미노할인하길래 먹엇습니다 ㅎ",
          "original_form": "어제 도미노할인하길래 먹엇습니다 ㅎ",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100015416.1.3",
          "form": "맛잇드라구여",
          "original_form": "맛잇드라구여",
          "speaker_id": "2",
          "time": "20210826 08:14"
        },
        {
          "id": "MDRW2100015416.1.4",
          "form": "오 피자 맛있었겠당",
          "original_form": "오 피자 맛있었겠당",
          "speaker_id": "1",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100015416.1.5",
          "form": "저는 어제 곱창순두부찌개 먹었어요",
          "original_form": "저는 어제 곱창순두부찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210826 08:22"
        },
        {
          "id": "MDRW2100015416.1.6",
          "form": "우와 순두부찌개에 곱창들어가잇는거에요?",
          "original_form": "우와 순두부찌개에 곱창들어가잇는거에요?",
          "speaker_id": "2",
          "time": "20210826 08:29"
        },
        {
          "id": "MDRW2100015416.1.7",
          "form": "밥한그릇 뚝딱이네요",
          "original_form": "밥한그릇 뚝딱이네요",
          "speaker_id": "2",
          "time": "20210826 08:29"
        },
        {
          "id": "MDRW2100015416.1.8",
          "form": "네네 생각보다 맛있더라구요",
          "original_form": "네네 생각보다 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100015416.1.9",
          "form": "말로만들어도 맛있을거같은데요 ㅋㅋㅋ",
          "original_form": "말로만들어도 맛있을거같은데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100015416.1.10",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100015416.1.11",
          "form": "점심 서브웨이 먹었어요!!",
          "original_form": "점심 서브웨이 먹었어요!!",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100015416.1.12",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100015416.1.13",
          "form": "서브웨이 맛잇겟다ㅠ 저는 치킨데리야끼좋아해요",
          "original_form": "서브웨이 맛잇겟다ㅠ 저는 치킨데리야끼좋아해요",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015416.1.14",
          "form": "전 분식먹엇어요 김밥 쫄면 돈까스요 ㄹ",
          "original_form": "전 분식먹엇어요 김밥 쫄면 돈까스요 ㄹ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015416.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015416.1.16",
          "form": "서브웨이 로스트치킨 드셔보셨어요?",
          "original_form": "서브웨이 로스트치킨 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015416.1.17",
          "form": "생각보다 안퍽퍽하고 맛있어요!!",
          "original_form": "생각보다 안퍽퍽하고 맛있어요!!",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015416.1.18",
          "form": "분식도 맛잌ㅅ겠다",
          "original_form": "분식도 맛잌ㅅ겠다",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100015416.1.19",
          "form": "퍽퍽할거같아서 안먹어봣어요 ㅋㅋㅋ",
          "original_form": "퍽퍽할거같아서 안먹어봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100015416.1.20",
          "form": "배고파요ㅠ저녁안먹엇더니",
          "original_form": "배고파요ㅠ저녁안먹엇더니",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100015416.1.21",
          "form": "왜 저녁 안먹었어요ㅠㅠ",
          "original_form": "왜 저녁 안먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100015416.1.22",
          "form": "어제 운동가가지고 운동갈땐 안먹게되드라구요 ㅋㅋㅋ무거운몸으로 하기가",
          "original_form": "어제 운동가가지고 운동갈땐 안먹게되드라구요 ㅋㅋㅋ무거운몸으로 하기가",
          "speaker_id": "2",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100015416.1.23",
          "form": "아아 하긴 운동가려면 많이 먹기 좀 그렇죠",
          "original_form": "아아 하긴 운동가려면 많이 먹기 좀 그렇죠",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100015416.1.24",
          "form": "평일에안먹고 주말에뿌시네요 ㅋㅋㅋㅋ",
          "original_form": "평일에안먹고 주말에뿌시네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:25"
        }
      ]
    }
