      "utterance": [
        {
          "id": "MDRW2100003180.1.1",
          "form": "안녕하세요! 날씨 괜찮으신가요?",
          "original_form": "안녕하세요! 날씨 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100003180.1.2",
          "form": "아침에 비오더니 지금은 그쳤네요ㅎ",
          "original_form": "아침에 비오더니 지금은 그쳤네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100003180.1.3",
          "form": "거기도 그런가요?",
          "original_form": "거기도 그런가요?",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100003180.1.4",
          "form": "비가 오고 그치고 반복이네요!",
          "original_form": "비가 오고 그치고 반복이네요!",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100003180.1.5",
          "form": "뉴스보니까 태풍이 온다고 하더라고요",
          "original_form": "뉴스보니까 태풍이 온다고 하더라고요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100003180.1.6",
          "form": "아마 그 영향인듯 싶어요",
          "original_form": "아마 그 영향인듯 싶어요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100003180.1.7",
          "form": "태풍이 온대요?",
          "original_form": "태풍이 온대요?",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100003180.1.8",
          "form": "네 지금 오고 있다고하네요",
          "original_form": "네 지금 오고 있다고하네요",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100003180.1.9",
          "form": "그래서 제 친구가 경남사는데",
          "original_form": "그래서 제 친구가 경남사는데",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100003180.1.10",
          "form": "비바람 엄청 분다고..",
          "original_form": "비바람 엄청 분다고..",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100003180.1.11",
          "form": "난리도 아니라구 하네요",
          "original_form": "난리도 아니라구 하네요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100003180.1.12",
          "form": "아 진짜요? 그냥 가을장마라고만 들었는데",
          "original_form": "아 진짜요? 그냥 가을장마라고만 들었는데",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100003180.1.13",
          "form": "찾아보니 태풍 오마이스 라는 애가 오네요",
          "original_form": "찾아보니 태풍 오마이스 라는 애가 오네요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100003180.1.14",
          "form": "대한민국에 직격으로 오는 모양이에요",
          "original_form": "대한민국에 직격으로 오는 모양이에요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100003180.1.15",
          "form": "뉴스 함 봐야겠네요 태풍오는건 첨들어서",
          "original_form": "뉴스 함 봐야겠네요 태풍오는건 첨들어서",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100003180.1.16",
          "form": "우선은 크기는 작아서 큰 피해는 없을거같다는 생각인데 아직은 모르겠네요 ㅠㅠ",
          "original_form": "우선은 크기는 작아서 큰 피해는 없을거같다는 생각인데 아직은 모르겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100003180.1.17",
          "form": "네네 한번 보시는거 좋아요",
          "original_form": "네네 한번 보시는거 좋아요",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100003180.1.18",
          "form": "네이버에 검색해도 나오니 참고하세요!",
          "original_form": "네이버에 검색해도 나오니 참고하세요!",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100003180.1.19",
          "form": "올해 태풍없이 조용히 지나가는거같더니",
          "original_form": "올해 태풍없이 조용히 지나가는거같더니",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100003180.1.20",
          "form": "그러게요 ㅠㅠ",
          "original_form": "그러게요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003180.1.21",
          "form": "조용히 지나갔으면 좋겠는데",
          "original_form": "조용히 지나갔으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003180.1.22",
          "form": "피해없이 지나가길 바래야죠ㅠㅠ",
          "original_form": "피해없이 지나가길 바래야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003180.1.23",
          "form": "그렇죠..ㅠㅠ",
          "original_form": "그렇죠..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003180.1.24",
          "form": "피해없길 바랄뿐",
          "original_form": "피해없길 바랄뿐",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100003180.1.25",
          "form": "아니면 노선이 조금이라도 다른곳으로 튀어서 피해가 적어진다던지..",
          "original_form": "아니면 노선이 조금이라도 다른곳으로 튀어서 피해가 적어진다던지..",
          "speaker_id": "1",
          "time": "20210821 17:05"
        }
      ]
    }
