      "utterance": [
        {
          "id": "MDRW2100044824.1.1",
          "form": "미쳐따미쳐따 고양이 자랑해주세요!",
          "original_form": "미쳐따미쳐따 고양이 자랑해주세요!",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044824.1.2",
          "form": "ㅋㅋㅋ고양이 두마리 본가에서 키워요!",
          "original_form": "ㅋㅋㅋ고양이 두마리 본가에서 키워요!",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044824.1.3",
          "form": "두마리나!!",
          "original_form": "두마리나!!",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044824.1.4",
          "form": "어떤 친구들이에여?",
          "original_form": "어떤 친구들이에여?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044824.1.5",
          "form": "첫째는 코숏인데 보호소에서 데려왔구 삼색냥이예요 ㅋㅋ",
          "original_form": "첫째는 코숏인데 보호소에서 데려왔구 삼색냥이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044824.1.6",
          "form": "둘째는 샵에서 온 페르시안친칠라요",
          "original_form": "둘째는 샵에서 온 페르시안친칠라요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044824.1.7",
          "form": "삼색냥이! 친칠라!",
          "original_form": "삼색냥이! 친칠라!",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044824.1.8",
          "form": "아 저 쓰러져요",
          "original_form": "아 저 쓰러져요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044824.1.9",
          "form": "이런건 실물 봐야하는데",
          "original_form": "이런건 실물 봐야하는데",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044824.1.10",
          "form": "여긴 왜 사진을 안 보여주는지 ㅠ",
          "original_form": "여긴 왜 사진을 안 보여주는지 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044824.1.11",
          "form": "ㅋㅋㅋ앗 사진도 듀ㅣ나옄ㅋㅋ",
          "original_form": "ㅋㅋㅋ앗 사진도 듀ㅣ나옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044824.1.12",
          "form": "제가 이거 쓰는 거 잘 몰라서여...ㅋㅋㅋㅋ",
          "original_form": "제가 이거 쓰는 거 잘 몰라서여...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044824.1.13",
          "form": "사진은 이게 보낼 수가 없거든요 ㅠ",
          "original_form": "사진은 이게 보낼 수가 없거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044824.1.14",
          "form": "넹...제가 뭐 눌러봐도 어렵네여...",
          "original_form": "넹...제가 뭐 눌러봐도 어렵네여...",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044824.1.15",
          "form": "고양이 좋아하시나봐여!",
          "original_form": "고양이 좋아하시나봐여!",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044824.1.16",
          "form": "키우는 건 어떠신가여 ㅎㅎ",
          "original_form": "키우는 건 어떠신가여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044824.1.17",
          "form": "고양이 좋아해요",
          "original_form": "고양이 좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044824.1.18",
          "form": "하지만 여건상 책임지지 못할 것 같아서 랜선 이모하고 있어요 ㅋㅋ",
          "original_form": "하지만 여건상 책임지지 못할 것 같아서 랜선 이모하고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044824.1.19",
          "form": "아하 저도 저 혼자면 어려웠을듯...ㅠ",
          "original_form": "아하 저도 저 혼자면 어려웠을듯...ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044824.1.20",
          "form": "주로 유튜브 랜선 이모 인가여",
          "original_form": "주로 유튜브 랜선 이모 인가여",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044824.1.21",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044824.1.22",
          "form": "그렇기도 하고 친구가 키우는 고양이를 자주 사진으로 보고 있어요",
          "original_form": "그렇기도 하고 친구가 키우는 고양이를 자주 사진으로 보고 있어요",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044824.1.23",
          "form": "저도 친구들 자주 보여줘요ㅋㅋㅋ",
          "original_form": "저도 친구들 자주 보여줘요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044824.1.24",
          "form": "그래서 키우는 분들 너무 부러워요 ㅠ",
          "original_form": "그래서 키우는 분들 너무 부러워요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044824.1.25",
          "form": "애기들 몇살인가요?",
          "original_form": "애기들 몇살인가요?",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044824.1.26",
          "form": "3살 2살이요 ㅋㅋ",
          "original_form": "3살 2살이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044824.1.27",
          "form": "아 저 진짜 넘어갑니다 ㅠ",
          "original_form": "아 저 진짜 넘어갑니다 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044824.1.28",
          "form": "젤리 핑크빛인가요?",
          "original_form": "젤리 핑크빛인가요?",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044824.1.29",
          "form": "첫째는 믹스고 둘째는 포도젤리예여!",
          "original_form": "첫째는 믹스고 둘째는 포도젤리예여!",
          "speaker_id": "2",
          "time": "20210908 00:17"
        }
      ]
    }
