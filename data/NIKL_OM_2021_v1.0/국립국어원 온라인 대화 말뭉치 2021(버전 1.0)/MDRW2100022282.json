      "utterance": [
        {
          "id": "MDRW2100022282.1.1",
          "form": "저는 인터넷 냉동배송 잘 안시켜요",
          "original_form": "저는 인터넷 냉동배송 잘 안시켜요",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022282.1.2",
          "form": "근데 중요한건 전 새우 못먹습니다",
          "original_form": "근데 중요한건 전 새우 못먹습니다",
          "speaker_id": "1",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022282.1.3",
          "form": "이 구역이 좀 느린가 늦게와요",
          "original_form": "이 구역이 좀 느린가 늦게와요",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022282.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100022282.1.5",
          "form": "ㅋㅋ근데 왜 쟁여두세요",
          "original_form": "ㅋㅋ근데 왜 쟁여두세요",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.6",
          "form": "누나가 먹어서",
          "original_form": "누나가 먹어서",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.7",
          "form": "아 누나를 위해 사두는거에요?",
          "original_form": "아 누나를 위해 사두는거에요?",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.8",
          "form": "저 새우알러지가있어서",
          "original_form": "저 새우알러지가있어서",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.9",
          "form": "대박이다 ㅜㅜ 그럼 밥도 같이 못먹겠네요",
          "original_form": "대박이다 ㅜㅜ 그럼 밥도 같이 못먹겠네요",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.10",
          "form": "누나가 사먹더라구요 그거 봤어요",
          "original_form": "누나가 사먹더라구요 그거 봤어요",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.11",
          "form": "밥 뭐에다가요",
          "original_form": "밥 뭐에다가요",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.12",
          "form": "본인 먹는것도 아닌데 사이즈도 외우시구....",
          "original_form": "본인 먹는것도 아닌데 사이즈도 외우시구....",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.13",
          "form": "그 새우요리요 ㅋㅋ",
          "original_form": "그 새우요리요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.14",
          "form": "누나가..",
          "original_form": "누나가..",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.15",
          "form": "귀에 딱지에앉도록",
          "original_form": "귀에 딱지에앉도록",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.16",
          "form": "심부름 시키던가요 ㅋㅋ",
          "original_form": "심부름 시키던가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.17",
          "form": "설명을해서ㅋㅋ",
          "original_form": "설명을해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.18",
          "form": "아이구 ㅋㅋㅋ ㅜㄴ나가 맛잘알 이시네",
          "original_form": "아이구 ㅋㅋㅋ ㅜㄴ나가 맛잘알 이시네",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100022282.1.19",
          "form": "제가 배워갑니다",
          "original_form": "제가 배워갑니다",
          "speaker_id": "2",
          "time": "20210829 12:03"
        }
      ]
