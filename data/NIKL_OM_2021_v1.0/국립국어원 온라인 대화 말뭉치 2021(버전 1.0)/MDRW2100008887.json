      "utterance": [
        {
          "id": "MDRW2100008887.1.1",
          "form": "저녁식사는 하셨어요?",
          "original_form": "저녁식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008887.1.2",
          "form": "스파게티해서 간단히 먹었어요",
          "original_form": "스파게티해서 간단히 먹었어요",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008887.1.3",
          "form": "밥하기 귀찮네요~",
          "original_form": "밥하기 귀찮네요~",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008887.1.4",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008887.1.5",
          "form": "저는 신랑더러 떡볶이 셔틀시켜서",
          "original_form": "저는 신랑더러 떡볶이 셔틀시켜서",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008887.1.6",
          "form": "떡볶이 먹었어요 ㅎ",
          "original_form": "떡볶이 먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008887.1.7",
          "form": "아이들은 떡국 먹고싶다고해서 후다닥 끓여주고요 ㅎ",
          "original_form": "아이들은 떡국 먹고싶다고해서 후다닥 끓여주고요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008887.1.8",
          "form": "떡볶이는 언제먹어도 맛있죠~~~ 전 어제 먹었어요^^",
          "original_form": "떡볶이는 언제먹어도 맛있죠~~~ 전 어제 먹었어요^^",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008887.1.9",
          "form": "떡국도 좋네요.",
          "original_form": "떡국도 좋네요.",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008887.1.10",
          "form": "오랫만에 떡국이야기를 하더라구요",
          "original_form": "오랫만에 떡국이야기를 하더라구요",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008887.1.11",
          "form": "첫째가 떡국을 좋아해요",
          "original_form": "첫째가 떡국을 좋아해요",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008887.1.12",
          "form": "애들 좋아하죠. 저번에 해달라했는데 떡사러가기 귀찮아서 패스~",
          "original_form": "애들 좋아하죠. 저번에 해달라했는데 떡사러가기 귀찮아서 패스~",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008887.1.13",
          "form": "저는 항시 냉동실에 준비해놓는데",
          "original_form": "저는 항시 냉동실에 준비해놓는데",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008887.1.14",
          "form": "시원해졌으니 뜨끈하게 한번해야겠네요",
          "original_form": "시원해졌으니 뜨끈하게 한번해야겠네요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008887.1.15",
          "form": "오늘은 조금만 있더라구요",
          "original_form": "오늘은 조금만 있더라구요",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008887.1.16",
          "form": "모지랄까걱정했는데 다행이 다먹고 더달라안하더라구요 ㅎ",
          "original_form": "모지랄까걱정했는데 다행이 다먹고 더달라안하더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008887.1.17",
          "form": "떡국떡은 얼렸다 녹이면 풀어져서 전 그때 사다해요. ㅋ 잘안해먹는거죠~",
          "original_form": "떡국떡은 얼렸다 녹이면 풀어져서 전 그때 사다해요. ㅋ 잘안해먹는거죠~",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008887.1.18",
          "form": "다행이네요. 없을때 더달라함 난감한데",
          "original_form": "다행이네요. 없을때 더달라함 난감한데",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008887.1.19",
          "form": "ㅎㅎ 저희는 식감은 잘안따져서 ㅋㅋㅋ\r\n그냥 먹어요 ㅎㅎ",
          "original_form": "ㅎㅎ 저희는 식감은 잘안따져서 ㅋㅋㅋ\r\n그냥 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008887.1.20",
          "form": "ㅎㅎ 울애들은 눈으로 음식을먹어서ㅠ",
          "original_form": "ㅎㅎ 울애들은 눈으로 음식을먹어서ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008887.1.21",
          "form": "막둥이가 그렇게 눈으로보고 따져요. 먹어보지도 않고요",
          "original_form": "막둥이가 그렇게 눈으로보고 따져요. 먹어보지도 않고요",
          "speaker_id": "2",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008887.1.22",
          "form": "눈으로요,,",
          "original_form": "눈으로요,,",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008887.1.23",
          "form": "대단하네요",
          "original_form": "대단하네요",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008887.1.24",
          "form": "ㅎ 뭐들어갔는지 둘러보고 이상하다싶음 밥만~ 쫌 편식이 심해요",
          "original_form": "ㅎ 뭐들어갔는지 둘러보고 이상하다싶음 밥만~ 쫌 편식이 심해요",
          "speaker_id": "2",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008887.1.25",
          "form": "고기는 좋아하는데 떡갈비같이 갈은고기는 잘안먹어요",
          "original_form": "고기는 좋아하는데 떡갈비같이 갈은고기는 잘안먹어요",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008887.1.26",
          "form": "와 벌써 눈썰미가 대단한데요",
          "original_form": "와 벌써 눈썰미가 대단한데요",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008887.1.27",
          "form": "떡갈비 맛나는데",
          "original_form": "떡갈비 맛나는데",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008887.1.28",
          "form": "그니깐요.  대부분이 없어서 못먹지 싫어하진 않는데 이상해요",
          "original_form": "그니깐요.  대부분이 없어서 못먹지 싫어하진 않는데 이상해요",
          "speaker_id": "2",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008887.1.29",
          "form": "스파게티는 소스 어떤거 드셨어요?",
          "original_form": "스파게티는 소스 어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:41"
        },
        {
          "id": "MDRW2100008887.1.30",
          "form": "로제요",
          "original_form": "로제요",
          "speaker_id": "2",
          "time": "20210822 19:43"
        }
      ]
    }
