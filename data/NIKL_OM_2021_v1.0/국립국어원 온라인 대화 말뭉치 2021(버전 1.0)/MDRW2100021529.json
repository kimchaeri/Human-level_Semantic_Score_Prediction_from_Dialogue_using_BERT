          "id": "MDRW2100021529.1.1",
          "form": "떡볶이 먹고 김치볶음밥 해먹었어요",
          "original_form": "떡볶이 먹고 김치볶음밥 해먹었어요",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021529.1.2",
          "form": "맛나겠네녀",
          "original_form": "맛나겠네녀",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021529.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021529.1.4",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021529.1.5",
          "form": "맛있으셨겠어여",
          "original_form": "맛있으셨겠어여",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021529.1.6",
          "form": "네 좋더라구요",
          "original_form": "네 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100021529.1.7",
          "form": "요즘 카페 가시나요?",
          "original_form": "요즘 카페 가시나요?",
          "speaker_id": "2",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021529.1.8",
          "form": "못가죠",
          "original_form": "못가죠",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021529.1.9",
          "form": "저는 테이크아웃으로 종종 가요ㅠ",
          "original_form": "저는 테이크아웃으로 종종 가요ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021529.1.10",
          "form": "그렇게라도",
          "original_form": "그렇게라도",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021529.1.11",
          "form": "집에서 내려먹긴 귀찮으니까요",
          "original_form": "집에서 내려먹긴 귀찮으니까요",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021529.1.12",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021529.1.13",
          "form": "빨리 잠잠해져야 카페에 자주 갈텐데여",
          "original_form": "빨리 잠잠해져야 카페에 자주 갈텐데여",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021529.1.14",
