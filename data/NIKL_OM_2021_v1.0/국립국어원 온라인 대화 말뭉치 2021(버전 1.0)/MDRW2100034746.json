      "utterance": [
        {
          "id": "MDRW2100034746.1.1",
          "form": "이제 주말 저녁인데 뭐 하세여",
          "original_form": "이제 주말 저녁인데 뭐 하세여",
          "speaker_id": "2",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034746.1.2",
          "form": "추천받은 영화가 있어서 그거볼까 생각중이에요",
          "original_form": "추천받은 영화가 있어서 그거볼까 생각중이에요",
          "speaker_id": "1",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034746.1.3",
          "form": "오 어떤 영화요",
          "original_form": "오 어떤 영화요",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034746.1.4",
          "form": "기묘한이야기요!",
          "original_form": "기묘한이야기요!",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034746.1.5",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034746.1.6",
          "form": "제목만 들어봤어요 ㅋㅋ",
          "original_form": "제목만 들어봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034746.1.7",
          "form": "무서운거 아니에요?",
          "original_form": "무서운거 아니에요?",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034746.1.8",
          "form": "헉 무서운거에요?",
          "original_form": "헉 무서운거에요?",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100034746.1.9",
          "form": "판타지인줄 알았어요ㅋㅋ",
          "original_form": "판타지인줄 알았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100034746.1.10",
          "form": "아니 저도 잘 몰라요 ㅋㅋㅋ",
          "original_form": "아니 저도 잘 몰라요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:51"
        },
        {
          "id": "MDRW2100034746.1.11",
          "form": "드라마 기묘한 이야기면 판타지 맞는데",
          "original_form": "드라마 기묘한 이야기면 판타지 맞는데",
          "speaker_id": "2",
          "time": "20210828 18:51"
        },
        {
          "id": "MDRW2100034746.1.12",
          "form": "영환 공포영화인걸로 ㅋㅋㅋ",
          "original_form": "영환 공포영화인걸로 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:51"
        },
        {
          "id": "MDRW2100034746.1.13",
          "form": "허억ㅋㅋㅋ",
          "original_form": "허억ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034746.1.14",
          "form": "반대로 알아들었었나봐요ㅋㅋ",
          "original_form": "반대로 알아들었었나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034746.1.15",
          "form": "으으 공포 영화 좋아하세요?",
          "original_form": "으으 공포 영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034746.1.16",
          "form": "앜ㅋ 드라마 기묘한 이야기고 사실 살짝 공포끼있어요",
          "original_form": "앜ㅋ 드라마 기묘한 이야기고 사실 살짝 공포끼있어요",
          "speaker_id": "2",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100034746.1.17",
          "form": "저 옛날엔 즐겨봤는데 요즘은 잘 못보겠어요 ㅋㅋ",
          "original_form": "저 옛날엔 즐겨봤는데 요즘은 잘 못보겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100034746.1.18",
          "form": "세상에나",
          "original_form": "세상에나",
          "speaker_id": "1",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100034746.1.19",
          "form": "여름에 영화관에서 매년 한편씩 보긴했어요",
          "original_form": "여름에 영화관에서 매년 한편씩 보긴했어요",
          "speaker_id": "2",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100034746.1.20",
          "form": "컨저링이나 애나벨같은거",
          "original_form": "컨저링이나 애나벨같은거",
          "speaker_id": "2",
          "time": "20210828 18:54"
        },
        {
          "id": "MDRW2100034746.1.21",
          "form": "여름에 개봉 많이 하죠",
          "original_form": "여름에 개봉 많이 하죠",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100034746.1.22",
          "form": "컨저링은 개인적으로 기분이 묘하게 나빴어요ㅜ",
          "original_form": "컨저링은 개인적으로 기분이 묘하게 나빴어요ㅜ",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100034746.1.23",
          "form": "네 사실 외국 귀신이라 별로 막 와닿진 않았지만ㅋㅋㅋ무섭게 생기긴 하더라구요ㅋㅋㅋ",
          "original_form": "네 사실 외국 귀신이라 별로 막 와닿진 않았지만ㅋㅋㅋ무섭게 생기긴 하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100034746.1.24",
          "form": "아 그건그래요ㅋㅋㅋ",
          "original_form": "아 그건그래요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:10"
        }
      ]
    }
