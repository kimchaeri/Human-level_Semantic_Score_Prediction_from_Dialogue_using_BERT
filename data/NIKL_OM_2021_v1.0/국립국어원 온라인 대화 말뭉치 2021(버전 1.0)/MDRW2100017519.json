      "utterance": [
        {
          "id": "MDRW2100017519.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100017519.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017519.1.3",
          "form": "백신예약을 하셨나요?",
          "original_form": "백신예약을 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017519.1.4",
          "form": "전 9월로 했어요",
          "original_form": "전 9월로 했어요",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017519.1.5",
          "form": "아앗... 백신... 저는 얼마 전 잔여백신으로 1차 접종을 했습니다.",
          "original_form": "아앗... 백신... 저는 얼마 전 잔여백신으로 1차 접종을 했습니다.",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017519.1.6",
          "form": "9월에 1차 맞으시는 건가요?",
          "original_form": "9월에 1차 맞으시는 건가요?",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100017519.1.7",
          "form": "ㄴㅔ 지금 일차 맞아요 맞으셨나요?",
          "original_form": "ㄴㅔ 지금 일차 맞아요 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017519.1.8",
          "form": "네 저는 아스트라제네케 맞았어요.",
          "original_form": "네 저는 아스트라제네케 맞았어요.",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017519.1.9",
          "form": "name1은 화이자 맞으시는 거죠?",
          "original_form": "&name1&은 화이자 맞으시는 거죠?",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017519.1.10",
          "form": "전 화이자 호 ㄱ은 모더나라고 써있어요 아마 재고있는걸로 맞을거에요",
          "original_form": "전 화이자 호 ㄱ은 모더나라고 써있어요 아마 재고있는걸로 맞을거에요",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017519.1.11",
          "form": "님은 일이차 둘다 아재인가요?",
          "original_form": "님은 일이차 둘다 아재인가요?",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017519.1.12",
          "form": "아마 화이자일 거 같아요.",
          "original_form": "아마 화이자일 거 같아요.",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017519.1.13",
          "form": "접종 때 의사분이 2차는 아제 말구 아마 화이자일 거고 8주 뒤에 접종이라고 했거든요.",
          "original_form": "접종 때 의사분이 2차는 아제 말구 아마 화이자일 거고 8주 뒤에 접종이라고 했거든요.",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017519.1.14",
          "form": "아 그럼 2차도아플거에요",
          "original_form": "아 그럼 2차도아플거에요",
          "speaker_id": "1",
          "time": "20210826 20:01"
        },
        {
          "id": "MDRW2100017519.1.15",
          "form": "아닌가 제가 의사가 아니니 확언은 못하지만 아재가 1차가 아프고 화이자가 2차가 아프다니",
          "original_form": "아닌가 제가 의사가 아니니 확언은 못하지만 아재가 1차가 아프고 화이자가 2차가 아프다니",
          "speaker_id": "1",
          "time": "20210826 20:01"
        },
        {
          "id": "MDRW2100017519.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 1차도 부작용이 꽤 컸는데 2차도 고통 당첨이군요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 1차도 부작용이 꽤 컸는데 2차도 고통 당첨이군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017519.1.17",
          "form": "화이자는 2차 접종 간격이 6주던가요?",
          "original_form": "화이자는 2차 접종 간격이 6주던가요?",
          "speaker_id": "2",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017519.1.18",
          "form": "모르겠습니다 하하하 국가가 알아서 알려주겠죠",
          "original_form": "모르겠습니다 하하하 국가가 알아서 알려주겠죠",
          "speaker_id": "1",
          "time": "20210826 20:04"
        },
        {
          "id": "MDRW2100017519.1.19",
          "form": "물량없으면 어차피 간격을 못지킬테니 안알아봤는데 딱 한 6주긴 할거에요",
          "original_form": "물량없으면 어차피 간격을 못지킬테니 안알아봤는데 딱 한 6주긴 할거에요",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017519.1.20",
          "form": "ㅋㅋㅋㅋㅋ 믿음직스럽지 않은 국가지만 어쩔 수 없네여...",
          "original_form": "ㅋㅋㅋㅋㅋ 믿음직스럽지 않은 국가지만 어쩔 수 없네여...",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017519.1.21",
          "form": "그래도 신을 믿는다며 백신을 거부한 탈레반 보단 나은걸로 해요",
          "original_form": "그래도 신을 믿는다며 백신을 거부한 탈레반 보단 나은걸로 해요",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017519.1.22",
          "form": "탈레반이랑 비교하면 더 나쁜 나라는 거의 없고 아이티 정도가 있겠지만",
          "original_form": "탈레반이랑 비교하면 더 나쁜 나라는 거의 없고 아이티 정도가 있겠지만",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017519.1.23",
          "form": "ㅋㅋㅋㅋㅋ 탈레반하고 비교할 정도면 이미 망한 나라 아닌가옄ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 탈레반하고 비교할 정도면 이미 망한 나라 아닌가옄ㅋㅋ",
          "speaker_id": "2",
