      "utterance": [
        {
          "id": "MDRW2100028226.1.1",
          "form": "전 카티비요",
          "original_form": "전 카티비요",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028226.1.2",
          "form": "삼프로티비라고 경제채널이에요",
          "original_form": "삼프로티비라고 경제채널이에요",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028226.1.3",
          "form": "유투브로 음악 들어요",
          "original_form": "유투브로 음악 들어요",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028226.1.4",
          "form": "방송이 엄청나요 과함",
          "original_form": "방송이 엄청나요 과함",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028226.1.5",
          "form": "팟캐도 들으시나요",
          "original_form": "팟캐도 들으시나요",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028226.1.6",
          "form": "유튭은 영상보는건데..",
          "original_form": "유튭은 영상보는건데..",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100028226.1.7",
          "form": "콘텐츠가 많아 힝ㅅ들죠",
          "original_form": "콘텐츠가 많아 힝ㅅ들죠",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028226.1.8",
          "form": "팟케는 세개 다운받앗어요",
          "original_form": "팟케는 세개 다운받앗어요",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028226.1.9",
          "form": "전 구독 하고 몇개 채널만 봐요",
          "original_form": "전 구독 하고 몇개 채널만 봐요",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028226.1.10",
          "form": "네이버오디오 팥빵 페이지투 일케세개요",
          "original_form": "네이버오디오 팥빵 페이지투 일케세개요",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028226.1.11",
          "form": "지니 이용해요 전",
          "original_form": "지니 이용해요 전",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028226.1.12",
          "form": "뭔채널보시나여",
          "original_form": "뭔채널보시나여",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100028226.1.13",
          "form": "케이티거든욪통신사가",
          "original_form": "케이티거든욪통신사가",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.14",
          "form": "갑자기통신사는왜요",
          "original_form": "갑자기통신사는왜요",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.15",
          "form": "활명수요 십오야랑",
          "original_form": "활명수요 십오야랑",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.16",
          "form": "전 유튭 보는데 그런건첨들어요",
          "original_form": "전 유튭 보는데 그런건첨들어요",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.17",
          "form": "재미위주로 봐요",
          "original_form": "재미위주로 봐요",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.18",
          "form": "그러실거같아요",
          "original_form": "그러실거같아요",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.19",
          "form": "정보보다",
          "original_form": "정보보다",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100028226.1.20",
          "form": "정보가중요한데..",
          "original_form": "정보가중요한데..",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100028226.1.21",
