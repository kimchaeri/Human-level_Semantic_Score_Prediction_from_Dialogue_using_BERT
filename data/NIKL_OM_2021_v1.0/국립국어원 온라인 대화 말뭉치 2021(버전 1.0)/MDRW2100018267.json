      "utterance": [
        {
          "id": "MDRW2100018267.1.1",
          "form": "나도 맥주 먹으면서 야구 보고 싶다",
          "original_form": "나도 맥주 먹으면서 야구 보고 싶다",
          "speaker_id": "2",
