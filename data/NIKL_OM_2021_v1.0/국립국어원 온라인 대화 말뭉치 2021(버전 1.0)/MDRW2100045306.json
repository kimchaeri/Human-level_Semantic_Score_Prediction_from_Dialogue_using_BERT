      "utterance": [
        {
          "id": "MDRW2100045306.1.1",
          "form": "내가알기로는 대학병원에있었음!",
          "original_form": "내가알기로는 대학병원에있었음!",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045306.1.2",
          "form": "내가아는 아마데우스는 예술회관 카페임ㅋㅋㅋ",
          "original_form": "내가아는 아마데우스는 예술회관 카페임ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045306.1.3",
          "form": "빵집은모르겠다",
          "original_form": "빵집은모르겠다",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045306.1.4",
          "form": "예술관에있는건 아직있음",
          "original_form": "예술관에있는건 아직있음",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045306.1.5",
          "form": "그래?!! 거기맞았음좋겠다ㅜㅜ",
          "original_form": "그래?!! 거기맞았음좋겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045306.1.6",
          "form": "근데 그냥 카페야",
          "original_form": "근데 그냥 카페야",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045306.1.7",
          "form": "무슨빵을 찾는겨",
          "original_form": "무슨빵을 찾는겨",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045306.1.8",
          "form": "나 거기 파운드케익너무먹고싶어",
          "original_form": "나 거기 파운드케익너무먹고싶어",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045306.1.9",
          "form": "파운드케익을 팔던가...",
          "original_form": "파운드케익을 팔던가...",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045306.1.10",
          "form": "난왜 대화통과가안되징",
          "original_form": "난왜 대화통과가안되징",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045306.1.11",
          "form": "안파는듯",
          "original_form": "안파는듯",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045306.1.12",
          "form": "그냥 카페인듯",
          "original_form": "그냥 카페인듯",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045306.1.13",
          "form": "이름만같은 ㅋㅋㅋㅋ",
          "original_form": "이름만같은 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045306.1.14",
          "form": "앗....아쉽네ㅜㅜㅜ살면서 언젠가 한번은 다시 먹을수있겠지..",
          "original_form": "앗....아쉽네ㅜㅜㅜ살면서 언젠가 한번은 다시 먹을수있겠지..",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045306.1.15",
          "form": "없어졌다면 불가능하지않을까....",
          "original_form": "없어졌다면 불가능하지않을까....",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045306.1.16",
          "form": "무슨맛이길래 ㅋㅋㅋ",
          "original_form": "무슨맛이길래 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045306.1.17",
          "form": "다른지역은 있더라ㅋㅋㅋ",
          "original_form": "다른지역은 있더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045306.1.18",
          "form": "엥 체인점이야???",
          "original_form": "엥 체인점이야???",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045306.1.19",
          "form": "그런듯!ㅋㅋ",
          "original_form": "그런듯!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045306.1.20",
          "form": "아뭐야 ㅋㅋㅋㅋ",
          "original_form": "아뭐야 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045306.1.21",
          "form": "난 로티보이먹고싶어",
          "original_form": "난 로티보이먹고싶어",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045306.1.22",
          "form": "로티보이?!",
          "original_form": "로티보이?!",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045306.1.23",
          "form": "로티보이를몰라?",
          "original_form": "로티보이를몰라?",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045306.1.24",
