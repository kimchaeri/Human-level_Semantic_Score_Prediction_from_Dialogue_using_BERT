      "utterance": [
        {
          "id": "MDRW2100012386.1.1",
          "form": "헉 엄청 아프신 편이신가봐요 ㅜㅜ",
          "original_form": "헉 엄청 아프신 편이신가봐요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012386.1.2",
          "form": "네ㅜ",
          "original_form": "네ㅜ",
          "speaker_id": "2",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012386.1.3",
          "form": "그래도 약 먹으면 괜찮아요",
          "original_form": "그래도 약 먹으면 괜찮아요",
          "speaker_id": "2",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012386.1.4",
          "form": "인류의 절반이 고통받는데..",
          "original_form": "인류의 절반이 고통받는데..",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012386.1.5",
          "form": "아직 의학적 해결이 ㅇ안되었다는게 의아해요",
          "original_form": "아직 의학적 해결이 ㅇ안되었다는게 의아해요",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012386.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012386.1.7",
          "form": "진짜 초경 이후로",
          "original_form": "진짜 초경 이후로",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012386.1.8",
          "form": "계속 의문이었어요",
          "original_form": "계속 의문이었어요",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012386.1.9",
          "form": "거대한 사기극이라는 결론을..",
          "original_form": "거대한 사기극이라는 결론을..",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012386.1.10",
          "form": "뭔가 이래서 결혼하기 더 싫은것도 있어요",
          "original_form": "뭔가 이래서 결혼하기 더 싫은것도 있어요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012386.1.11",
          "form": "사기극에 놀아나기 싫은 기분ㅋㅋㅋ",
          "original_form": "사기극에 놀아나기 싫은 기분ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012386.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012386.1.13",
          "form": "사회가 원하는대로 살기 싫어요",
          "original_form": "사회가 원하는대로 살기 싫어요",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012386.1.14",
          "form": "어디 니들이 해봐라 내가 말을 듣나..",
          "original_form": "어디 니들이 해봐라 내가 말을 듣나..",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012386.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012386.1.16",
          "form": "그래도 비혼이 늘어서",
          "original_form": "그래도 비혼이 늘어서",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012386.1.17",
          "form": "내 생각이 유난인건 아니구나 싶어요",
          "original_form": "내 생각이 유난인건 아니구나 싶어요",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012386.1.18",
          "form": "사회적인 제도도 이쪽으로 많이 생길테고",
          "original_form": "사회적인 제도도 이쪽으로 많이 생길테고",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012386.1.19",
          "form": "저번에 어떤분이 비혼마을 만들고 싶다 하셨는데",
          "original_form": "저번에 어떤분이 비혼마을 만들고 싶다 하셨는데",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012386.1.20",
          "form": "가능할 것 같아요",
          "original_form": "가능할 것 같아요",
          "speaker_id": "2",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100012386.1.21",
          "form": "비혼마을 생기면 범죄율 0퍼일듯요",
          "original_form": "비혼마을 생기면 범죄율 0퍼일듯요",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012386.1.22",
          "form": "법없는 마을..",
          "original_form": "법없는 마을..",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012386.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012386.1.24",
          "form": "자전거 도둑도 외부인일 것 같아요",
          "original_form": "자전거 도둑도 외부인일 것 같아요",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012386.1.25",
          "form": "자전겈ㅋㅋㅋㅋㅋ",
          "original_form": "자전겈ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012386.1.26",
          "form": "혼자 살 생각 하니까 막 형광등가는법 이런거 배워두고싶어요",
          "original_form": "혼자 살 생각 하니까 막 형광등가는법 이런거 배워두고싶어요",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012386.1.27",
          "form": "아 형광등 맞아요",
          "original_form": "아 형광등 맞아요",
          "speaker_id": "2",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012386.1.28",
          "form": "그런걸 생각 못했네요",
          "original_form": "그런걸 생각 못했네요",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012386.1.29",
          "form": "열쇠 따는 법이나..",
          "original_form": "열쇠 따는 법이나..",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012386.1.30",
          "form": "벌레 잡는거나 청소도",
          "original_form": "벌레 잡는거나 청소도",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012386.1.31",
          "form": "열쇠요?",
          "original_form": "열쇠요?",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012386.1.32",
          "form": "배워두면 쓸모있긴 하겠네요",
          "original_form": "배워두면 쓸모있긴 하겠네요",
          "speaker_id": "2",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012386.1.33",
          "form": "네 ㅜㅜ 아 요즘 도어락이죠",
          "original_form": "네 ㅜㅜ 아 요즘 도어락이죠",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100012386.1.34",
          "form": "예전에 집 문 잠겨서 열쇠공 불렀는데 다 남자더라구요",
          "original_form": "예전에 집 문 잠겨서 열쇠공 불렀는데 다 남자더라구요",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012386.1.35",
          "form": "새벽에 열쇠공이랑 둘이 있으려니 무서웠어요",
          "original_form": "새벽에 열쇠공이랑 둘이 있으려니 무서웠어요",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012386.1.36",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012386.1.37",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012386.1.38",
          "form": "열쇠를 못열어도 문제고",
          "original_form": "열쇠를 못열어도 문제고",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012386.1.39",
          "form": "열쇠공을 기다려도 문제고",
          "original_form": "열쇠공을 기다려도 문제고",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012386.1.40",
          "form": "살기 힘드네요",
          "original_form": "살기 힘드네요",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012386.1.41",
          "form": "새벽에 열쇠때문에 놀라셨겠어요",
          "original_form": "새벽에 열쇠때문에 놀라셨겠어요",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012386.1.42",
          "form": "맞아욬ㅋㅋㅋ 그래서 비혼마을 만드려면 열쇠따는거 배워야지 생각햇는데 요새 도어락이네요",
          "original_form": "맞아욬ㅋㅋㅋ 그래서 비혼마을 만드려면 열쇠따는거 배워야지 생각햇는데 요새 도어락이네요",
          "speaker_id": "1",
          "time": "20210824 22:27"
        }
      ]
