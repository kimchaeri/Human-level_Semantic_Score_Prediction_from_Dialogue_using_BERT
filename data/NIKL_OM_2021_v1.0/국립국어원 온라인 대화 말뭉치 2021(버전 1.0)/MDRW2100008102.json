      "utterance": [
        {
          "id": "MDRW2100008102.1.1",
          "form": "스포츠 좋아하세요??",
          "original_form": "스포츠 좋아하세요??",
          "speaker_id": "1",
          "time": "20210822 13:20"
        },
        {
          "id": "MDRW2100008102.1.2",
          "form": "아 전 야구 좋아해서 한때 많이 봤어요 ㅋㅋㅋ",
          "original_form": "아 전 야구 좋아해서 한때 많이 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008102.1.3",
          "form": "저두요ㅎㅎ 야구 좋아했죠ㅜㅜ",
          "original_form": "저두요ㅎㅎ 야구 좋아했죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008102.1.4",
          "form": "직관도 가시고 그랬나요?",
          "original_form": "직관도 가시고 그랬나요?",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008102.1.5",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008102.1.6",
          "form": "한번은 제가 좋아하는 선수가 연타석 홈런쳤는데 짜릿하더라구요ㅎ",
          "original_form": "한번은 제가 좋아하는 선수가 연타석 홈런쳤는데 짜릿하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008102.1.7",
          "form": "오 엄청 좋으셨겠다 생각만 해도 ㅋㅋ",
          "original_form": "오 엄청 좋으셨겠다 생각만 해도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008102.1.8",
          "form": "이용규 아세요???",
          "original_form": "이용규 아세요???",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008102.1.9",
          "form": "1년에 10개 칠까말까했던ㅋㅋㅋ",
          "original_form": "1년에 10개 칠까말까했던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008102.1.10",
          "form": "알죠 ㅋㅋㅋ",
          "original_form": "알죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008102.1.11",
          "form": "대박난 순간이었겠네요 ㅋㅋㅋ",
          "original_form": "대박난 순간이었겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008102.1.12",
          "form": "네ㅋㅋㅋ 그것도 꽤 3점 인가 만루인가 되게 큰걸로 기억하는데ㅋㅋ",
          "original_form": "네ㅋㅋㅋ 그것도 꽤 3점 인가 만루인가 되게 큰걸로 기억하는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008102.1.13",
          "form": "와 소리 엄청 지르셨겠네요 ㅋㅋㅋ",
          "original_form": "와 소리 엄청 지르셨겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008102.1.14",
          "form": "네ㅋㅋㅋㅋㅋㅋ\r\n친구 안고 난리였지요ㅎㅎㅎㅎ",
          "original_form": "네ㅋㅋㅋㅋㅋㅋ\r\n친구 안고 난리였지요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008102.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 사실 해외야구 위주로 봐서 그 직관의 짜릿함을 잘 몰라요 ㅋㅋㅋ 너무 부러워요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 사실 해외야구 위주로 봐서 그 직관의 짜릿함을 잘 몰라요 ㅋㅋㅋ 너무 부러워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008102.1.16",
          "form": "저도 요즘은 안 봐요",
          "original_form": "저도 요즘은 안 봐요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008102.1.17",
          "form": "올림픽 이후",
          "original_form": "올림픽 이후",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008102.1.18",
          "form": "다시 관심 가지고 싶은데ㅋㅋㅋ",
          "original_form": "다시 관심 가지고 싶은데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008102.1.19",
          "form": "직관을 못가서...ㅋㅋㅋ",
          "original_form": "직관을 못가서...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008102.1.20",
          "form": "그게 또 확 꽂힐 때가 있는 거 같아요 ㅋㅋㅋㅋ",
          "original_form": "그게 또 확 꽂힐 때가 있는 거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008102.1.21",
          "form": "하루종일 관련영상만 보고 ㅋㅋㅋ",
          "original_form": "하루종일 관련영상만 보고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008102.1.22",
          "form": "네ㅜ 이번에 이의리한테 꽂혔는데",
          "original_form": "네ㅜ 이번에 이의리한테 꽂혔는데",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008102.1.23",
          "form": "아쉽습니다ㅎㅎㅎㅋㅋㅋ",
          "original_form": "아쉽습니다ㅎㅎㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008102.1.24",
          "form": "그 선수 이름 진짜 특이한거 같아요 ㅋㅋㅋ 한번 들으면 안 잊혀요",
          "original_form": "그 선수 이름 진짜 특이한거 같아요 ㅋㅋㅋ 한번 들으면 안 잊혀요",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008102.1.25",
          "form": "네ㅜ",
          "original_form": "네ㅜ",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008102.1.26",
          "form": "이번에 졸업한 20살인데너",
          "original_form": "이번에 졸업한 20살인데너",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008102.1.27",
          "form": "너무 잘 던져서ㅋㅋㅋㅋ",
          "original_form": "너무 잘 던져서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008102.1.28",
          "form": "헐 스무살이면 완전 창창하네요",
          "original_form": "헐 스무살이면 완전 창창하네요",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008102.1.29",
          "form": "아마 이번 신인왕 타지 않을까 싶어요!!",
          "original_form": "아마 이번 신인왕 타지 않을까 싶어요!!",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008102.1.30",
          "form": "오 그래서 사람들 입에 많이 올랐나봐요",
          "original_form": "오 그래서 사람들 입에 많이 올랐나봐요",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008102.1.31",
          "form": "진짜 미래가 기대되는 선수입니다ㅋㅋㅋ",
          "original_form": "진짜 미래가 기대되는 선수입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008102.1.32",
          "form": "김광현이나 류현진 선수 처럼 컸으면 좋겠어요ㅋㅋ",
          "original_form": "김광현이나 류현진 선수 처럼 컸으면 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008102.1.33",
          "form": "크 좋죠 저도 류현진 때문에 mlb 보게 된거라.. 잘되면 진짜 좋겠네요",
          "original_form": "크 좋죠 저도 류현진 때문에 mlb 보게 된거라.. 잘되면 진짜 좋겠네요",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008102.1.34",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008102.1.35",
          "form": "류현진 너무 멋있어요..",
          "original_form": "류현진 너무 멋있어요..",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008102.1.36",
          "form": "그쵸 첨에 다저스 갔을때 너무 재밌게 봤는데 지금도 캐나다에서 잘하더라고요 ㅎㅎ",
          "original_form": "그쵸 첨에 다저스 갔을때 너무 재밌게 봤는데 지금도 캐나다에서 잘하더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008102.1.37",
          "form": "진짜 어딜가도 잘하니까 팬 입장에선 너무 좋을거같아요ㅜ",
          "original_form": "진짜 어딜가도 잘하니까 팬 입장에선 너무 좋을거같아요ㅜ",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008102.1.38",
          "form": "전 양현종도 응원하는데ㅜ",
          "original_form": "전 양현종도 응원하는데ㅜ",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008102.1.39",
          "form": "아 양현종은 어떤가요 요새",
          "original_form": "아 양현종은 어떤가요 요새",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008102.1.40",
          "form": "자세히는 모르지만 불펜으로 전환되서 아직은 조금 힘든거같더라구요",
          "original_form": "자세히는 모르지만 불펜으로 전환되서 아직은 조금 힘든거같더라구요",
          "speaker_id": "1",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008102.1.41",
          "form": "에구 ㅠㅠㅠ 몰랐는데 다시 잘 올라오면 좋겠네요 ㅠㅠ",
          "original_form": "에구 ㅠㅠㅠ 몰랐는데 다시 잘 올라오면 좋겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:45"
        },
        {
          "id": "MDRW2100008102.1.42",
          "form": "아마 의지가 있으니 끝까지는 해볼거 같아요ㅎㅎㅎ",
          "original_form": "아마 의지가 있으니 끝까지는 해볼거 같아요ㅎㅎㅎ",
