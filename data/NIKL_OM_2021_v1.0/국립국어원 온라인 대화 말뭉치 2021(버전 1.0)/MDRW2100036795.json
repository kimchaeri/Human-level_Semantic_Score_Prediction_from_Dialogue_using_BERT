      "utterance": [
        {
          "id": "MDRW2100036795.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036795.1.2",
          "form": "혹시 앓고잇는 병이 잇으세요?",
          "original_form": "혹시 앓고잇는 병이 잇으세요?",
          "speaker_id": "2",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036795.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210831 12:19"
        },
        {
          "id": "MDRW2100036795.1.4",
          "form": "건강이최고죠. 특별히 성형하고싶은 부위가 잇으세요?",
          "original_form": "건강이최고죠. 특별히 성형하고싶은 부위가 잇으세요?",
          "speaker_id": "2",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.5",
          "form": "보톡스용",
          "original_form": "보톡스용",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.6",
          "form": "아니다 콧대",
          "original_form": "아니다 콧대",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.7",
          "form": "어디요??",
          "original_form": "어디요??",
          "speaker_id": "2",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.8",
          "form": "콧대에 보톡스??",
          "original_form": "콧대에 보톡스??",
          "speaker_id": "2",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.9",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.10",
          "form": "위험한데요",
          "original_form": "위험한데요",
          "speaker_id": "2",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.11",
          "form": "사실 필러가 낫겠죠",
          "original_form": "사실 필러가 낫겠죠",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.12",
          "form": "필러하면 콧대 퍼진대요",
          "original_form": "필러하면 콧대 퍼진대요",
          "speaker_id": "2",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036795.1.13",
          "form": "코끝은 높은데 콧대는 낮아요ㅠ",
          "original_form": "코끝은 높은데 콧대는 낮아요ㅠ",
          "speaker_id": "1",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036795.1.14",
          "form": "그래서 안하는만도 못하대요",
          "original_form": "그래서 안하는만도 못하대요",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036795.1.15",
          "form": "하지만 저 그거하면 눈 하나되요",
          "original_form": "하지만 저 그거하면 눈 하나되요",
          "speaker_id": "1",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036795.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 이런 눈이 몰렷어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 이런 눈이 몰렷어요?",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036795.1.17",
          "form": "눈이 몰려서",
          "original_form": "눈이 몰려서",
