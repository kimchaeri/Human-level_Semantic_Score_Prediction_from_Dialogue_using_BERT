        },
        {
          "id": "MDRW2100021493.1.5",
          "form": "운동은 딱히 하고 있지 않아요ㅠㅠ",
          "original_form": "운동은 딱히 하고 있지 않아요ㅠㅠ",
          "speaker_id": "2",
