      "utterance": [
        {
          "id": "MDRW2100034991.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034991.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034991.1.3",
          "form": "혹시 이차돌이라는",
          "original_form": "혹시 이차돌이라는",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034991.1.4",
          "form": "식당 아세요?",
          "original_form": "식당 아세요?",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034991.1.5",
          "form": "아니요 어떤 식당이죠?",
          "original_form": "아니요 어떤 식당이죠?",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034991.1.6",
          "form": "수입산 차돌박이 구이나 고기 등등 파는식당이래요",
          "original_form": "수입산 차돌박이 구이나 고기 등등 파는식당이래요",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034991.1.7",
          "form": "그렇군요 가보셨나요?",
          "original_form": "그렇군요 가보셨나요?",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034991.1.8",
          "form": "아뇨. 저희집에서 많이 멀어서 안가봣어요",
          "original_form": "아뇨. 저희집에서 많이 멀어서 안가봣어요",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100034991.1.9",
          "form": "그런데 좋은소식이 하나잇더라고오.",
          "original_form": "그런데 좋은소식이 하나잇더라고오.",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034991.1.10",
          "form": "어떤 소식이죠?\r\n행사하나요?",
          "original_form": "어떤 소식이죠?\r\n행사하나요?",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034991.1.11",
          "form": "이번에 네고왕에 그 음식점이 떳나보더라고요.\r\n방문고객한정으로 할인쿠폰을 제시하면 할인을해준다고하더라고요.\r\n\r\n근데 완전 끝과 끝이라서 못갈것같아요 ㅋㅋ",
          "original_form": "이번에 네고왕에 그 음식점이 떳나보더라고요.\r\n방문고객한정으로 할인쿠폰을 제시하면 할인을해준다고하더라고요.\r\n\r\n근데 완전 끝과 끝이라서 못갈것같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034991.1.12",
          "form": "아 너무 멀면 가기가 힘들죠ㅠㅠ",
          "original_form": "아 너무 멀면 가기가 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034991.1.13",
          "form": "넹.. 그리고 혹시 마스크 필오하신가요?",
          "original_form": "넹.. 그리고 혹시 마스크 필오하신가요?",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100034991.1.14",
          "form": "어떤 마스크요?",
          "original_form": "어떤 마스크요?",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034991.1.15",
          "form": "덴탈마스크에 프린팅되어잇는 디자인이요",
          "original_form": "덴탈마스크에 프린팅되어잇는 디자인이요",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100034991.1.16",
          "form": "만약에 공짜라도 괜찮으시면\r\n유투브에서 9시내고향- 마스크편 보고\r\n구독 및 기대평+ 댓글작성 후 \r\n상세페이지 들어가셔서 폼 작성하시면되요",
          "original_form": "만약에 공짜라도 괜찮으시면\r\n유투브에서 9시내고향- 마스크편 보고\r\n구독 및 기대평+ 댓글작성 후 \r\n상세페이지 들어가셔서 폼 작성하시면되요",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100034991.1.17",
          "form": "오 디자인되어있는 마스크라니",
          "original_form": "오 디자인되어있는 마스크라니",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100034991.1.18",
          "form": "조금 복잡하네요\r\n관심은 생기지만 아직은 집에 마스크가 여유 있어서요",
          "original_form": "조금 복잡하네요\r\n관심은 생기지만 아직은 집에 마스크가 여유 있어서요",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100034991.1.19",
          "form": "ㅋㅋㅋㅋ 저는 그냥 해둿어요 ㅋㅋㅋ 막쓰려고요~~",
          "original_form": "ㅋㅋㅋㅋ 저는 그냥 해둿어요 ㅋㅋㅋ 막쓰려고요~~",
