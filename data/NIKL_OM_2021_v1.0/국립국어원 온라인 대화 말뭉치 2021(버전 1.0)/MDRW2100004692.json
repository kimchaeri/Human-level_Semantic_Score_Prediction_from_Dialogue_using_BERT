        {
          "id": "MDRW2100004692.1.1",
          "form": "일해여?",
          "original_form": "일해여?",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004692.1.2",
          "form": "일해요?",
          "original_form": "일해요?",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004692.1.3",
          "form": "아뇨 아직 학생이에요",
          "original_form": "아뇨 아직 학생이에요",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004692.1.4",
          "form": "직장인 이신가요?",
          "original_form": "직장인 이신가요?",
          "speaker_id": "2",
          "time": "20210821 18:01"
        },
        {
          "id": "MDRW2100004692.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004692.1.6",
          "form": "코로나인데 재택근무 할 수 있게 하나요?",
          "original_form": "코로나인데 재택근무 할 수 있게 하나요?",
          "speaker_id": "2",
          "time": "20210821 18:05"
        },
        {
          "id": "MDRW2100004692.1.7",
          "form": "아니요 사무실요",
          "original_form": "아니요 사무실요",
          "speaker_id": "1",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004692.1.8",
          "form": "마스크 쓰고 일할려면 불편하겠어요",
          "original_form": "마스크 쓰고 일할려면 불편하겠어요",
          "speaker_id": "2",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004692.1.9",
          "form": "퇴근은 언제 하나요?",
          "original_form": "퇴근은 언제 하나요?",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004692.1.10",
          "form": "오늘은 주말이라",
          "original_form": "오늘은 주말이라",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004692.1.11",
          "form": "쉬죠",
          "original_form": "쉬죠",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004692.1.12",
          "form": "주중이 문제죠",
          "original_form": "주중이 문제죠",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004692.1.13",
          "form": "한국이 근로시간이 많기로 악명이 자자하죠",
          "original_form": "한국이 근로시간이 많기로 악명이 자자하죠",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004692.1.14",
          "form": "휴가는 자유로운 편인가요?",
          "original_form": "휴가는 자유로운 편인가요?",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004692.1.15",
          "form": "눈치 보이죠",
          "original_form": "눈치 보이죠",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004692.1.16",
          "form": "직장 사람들은 어떤가요?",
          "original_form": "직장 사람들은 어떤가요?",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004692.1.17",
          "form": "다양하죠",
          "original_form": "다양하죠",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004692.1.18",
          "form": "회사에서 회식 자주 가나요?",
          "original_form": "회사에서 회식 자주 가나요?",
          "speaker_id": "2",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004692.1.19",
          "form": "전혀요",
          "original_form": "전혀요",
          "speaker_id": "1",
          "time": "20210821 18:55"
        }
      ]
