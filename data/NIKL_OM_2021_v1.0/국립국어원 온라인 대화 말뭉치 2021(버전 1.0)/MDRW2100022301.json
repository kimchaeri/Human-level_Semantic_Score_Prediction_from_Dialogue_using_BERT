      "utterance": [
        {
          "id": "MDRW2100022301.1.1",
          "form": "삼겹살하고 오겹살의 차이 알고 있나요 ㅎㅎ",
          "original_form": "삼겹살하고 오겹살의 차이 알고 있나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.2",
          "form": "치킨좋아하세요",
          "original_form": "치킨좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.3",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.4",
          "form": "차이에요",
          "original_form": "차이에요",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.5",
          "form": "넹 말해봐요!",
          "original_form": "넹 말해봐요!",
          "speaker_id": "2",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.6",
          "form": "어떤 차이요 ㅋㅋ",
          "original_form": "어떤 차이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 12:17"
        },
        {
          "id": "MDRW2100022301.1.8",
          "form": "치킨 좋아하시나요",
          "original_form": "치킨 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 12:18"
        },
        {
          "id": "MDRW2100022301.1.9",
          "form": "네 치킨 좋아합니다",
          "original_form": "네 치킨 좋아합니다",
          "speaker_id": "2",
          "time": "20210829 12:18"
        },
        {
          "id": "MDRW2100022301.1.10",
          "form": "치킨 부위중 안쳐지는 것도 있어요 ㅠ",
          "original_form": "치킨 부위중 안쳐지는 것도 있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:18"
        },
        {
          "id": "MDRW2100022301.1.11",
          "form": "후라이드 vs 양념",
          "original_form": "후라이드 vs 양념",
          "speaker_id": "1",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.12",
          "form": "반반입니다",
          "original_form": "반반입니다",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.13",
          "form": "그 둘은 싸움 붙이면 안돼요",
          "original_form": "그 둘은 싸움 붙이면 안돼요",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.14",
          "form": "와 반반 처음 나온 답변이다",
          "original_form": "와 반반 처음 나온 답변이다",
          "speaker_id": "1",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그치만 양념을 더 선호해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그치만 양념을 더 선호해요",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.16",
          "form": "저 물어보면 8할이 후라이드라 그랬거든요",
          "original_form": "저 물어보면 8할이 후라이드라 그랬거든요",
          "speaker_id": "1",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.17",
          "form": "양념도 처음이에요",
          "original_form": "양념도 처음이에요",
          "speaker_id": "1",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.18",
          "form": "어 ㅋㅋ 진짜요?",
          "original_form": "어 ㅋㅋ 진짜요?",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.19",
          "form": "네ㅋㅋ 대부분 후라이드 말하세요",
          "original_form": "네ㅋㅋ 대부분 후라이드 말하세요",
          "speaker_id": "1",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.20",
          "form": "후라이드는 뭔가 먹다보면 물려요",
          "original_form": "후라이드는 뭔가 먹다보면 물려요",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022301.1.21",
          "form": "안그래요?",
