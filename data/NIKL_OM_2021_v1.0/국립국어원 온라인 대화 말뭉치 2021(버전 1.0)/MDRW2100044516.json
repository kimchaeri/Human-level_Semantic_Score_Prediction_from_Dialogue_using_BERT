      "utterance": [
        {
          "id": "MDRW2100044516.1.1",
          "form": "뭐라 하지 첨에",
          "original_form": "뭐라 하지 첨에",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.2",
          "form": "햄튜버랑 지컨님이 플레이만 하면",
          "original_form": "햄튜버랑 지컨님이 플레이만 하면",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.3",
          "form": "얼레벌레 부재료 구하고",
          "original_form": "얼레벌레 부재료 구하고",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.4",
          "form": "내가 플레이 할때도 너무 웃겨서 디집어짐ㅠ",
          "original_form": "내가 플레이 할때도 너무 웃겨서 디집어짐ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.5",
          "form": "사슴 죽이면 고기를 얻었습니다! 하고 뜨잖아",
          "original_form": "사슴 죽이면 고기를 얻었습니다! 하고 뜨잖아",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.6",
          "form": "그게너무어이없는거야",
          "original_form": "그게너무어이없는거야",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.7",
          "form": "그리고 뭣보다 그들은 진엔딩에 집착하지 않음",
          "original_form": "그리고 뭣보다 그들은 진엔딩에 집착하지 않음",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.8",
          "form": "왤케 헤맑냐고",
          "original_form": "왤케 헤맑냐고",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.9",
          "form": "줜아 얼레벌레ㅠㅠㅠ",
          "original_form": "줜아 얼레벌레ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.10",
          "form": "마자마자ㅠㅠㅠ",
          "original_form": "마자마자ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.11",
          "form": "사슴고기 스테이크 먹기",
          "original_form": "사슴고기 스테이크 먹기",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.12",
          "form": "아 ㅁㅈㅁㅈ 그들은 그저............즐겜을 추구할뿐",
          "original_form": "아 ㅁㅈㅁㅈ 그들은 그저............즐겜을 추구할뿐",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.13",
          "form": "결국은 근데 다 스테이크 됏을뿐",
          "original_form": "결국은 근데 다 스테이크 됏을뿐",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.14",
          "form": "머 요리를 한거 같지는 않는데ㅎ",
          "original_form": "머 요리를 한거 같지는 않는데ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044516.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아.. 아니 손님들을 그냥 고기취급하는게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아.. 아니 손님들을 그냥 고기취급하는게",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044516.1.16",
          "form": "개구리 캐릭터가",
          "original_form": "개구리 캐릭터가",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044516.1.17",
          "form": "진짜 잇는척 다해",
          "original_form": "진짜 잇는척 다해",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044516.1.18",
          "form": "나중에는 좀...엽기로 느껴졌음 뭐랄까 나는 잔인한 행위에 대해 주변 사람들이 지나치게 초연하게 대응하면 어느 순간 그걸 엽기로 느끼는 것 같아..",
          "original_form": "나중에는 좀...엽기로 느껴졌음 뭐랄까 나는 잔인한 행위에 대해 주변 사람들이 지나치게 초연하게 대응하면 어느 순간 그걸 엽기로 느끼는 것 같아..",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044516.1.19",
          "form": "냠냠 쩝쩝 스테이크 걍 신선한 고기를 구웟어요",
          "original_form": "냠냠 쩝쩝 스테이크 걍 신선한 고기를 구웟어요",
