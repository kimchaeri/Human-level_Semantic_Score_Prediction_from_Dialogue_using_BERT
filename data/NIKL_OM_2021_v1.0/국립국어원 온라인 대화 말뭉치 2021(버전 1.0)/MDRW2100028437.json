      "utterance": [
        {
          "id": "MDRW2100028437.1.1",
          "form": "보고있어요 재밌네요",
          "original_form": "보고있어요 재밌네요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028437.1.2",
          "form": "저는 소리만 듣고있어요",
          "original_form": "저는 소리만 듣고있어요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028437.1.3",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028437.1.4",
          "form": "먼가 심심한 경기에요",
          "original_form": "먼가 심심한 경기에요",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028437.1.5",
          "form": "잘하나요?",
          "original_form": "잘하나요?",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028437.1.6",
          "form": "지금까지는 아쉬운 경기이긴 하네요",
          "original_form": "지금까지는 아쉬운 경기이긴 하네요",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028437.1.7",
          "form": "옆에서 name3가 그렇다고 우네요",
          "original_form": "옆에서 &name3&가 그렇다고 우네요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028437.1.8",
          "form": "듣기만했는데 알다니 영특하네요ㅋ",
          "original_form": "듣기만했는데 알다니 영특하네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028437.1.9",
          "form": "말이 많거든요",
          "original_form": "말이 많거든요",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028437.1.10",
          "form": "지금도 쫑알거려요",
          "original_form": "지금도 쫑알거려요",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028437.1.11",
          "form": "귀에 피나는중",
          "original_form": "귀에 피나는중",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028437.1.12",
          "form": "고양이 영상틀어주고 대화시키세요ㅋ",
          "original_form": "고양이 영상틀어주고 대화시키세요ㅋ",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028437.1.13",
          "form": "저희애들은",
          "original_form": "저희애들은",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028437.1.14",
          "form": "고양이 관심도없어요",
          "original_form": "고양이 관심도없어요",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028437.1.15",
          "form": "새나 쥐 영상좋아해요",
          "original_form": "새나 쥐 영상좋아해요",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028437.1.16",
          "form": "그래요? 그거 틀어주고 관심을 돌려야겠네요",
          "original_form": "그래요? 그거 틀어주고 관심을 돌려야겠네요",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028437.1.17",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028437.1.18",
          "form": "지금 닭고기를 삶고 있눈걸료",
          "original_form": "지금 닭고기를 삶고 있눈걸료",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028437.1.19",
          "form": "입을 막아야해요",
          "original_form": "입을 막아야해요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028437.1.20",
          "form": "잘하셨네요",
          "original_form": "잘하셨네요",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100028437.1.21",
          "form": "다먹고",
          "original_form": "다먹고",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028437.1.22",
          "form": "힘이 넘쳐서 또 떠드네요",
          "original_form": "힘이 넘쳐서 또 떠드네요",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028437.1.23",
          "form": "축구는 동점이네요",
          "original_form": "축구는 동점이네요",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100028437.1.24",
          "form": "그러게요 화요일에 경기있다던데 그때는 이겼으면 좋겠네요",
          "original_form": "그러게요 화요일에 경기있다던데 그때는 이겼으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028437.1.25",
          "form": "또있나봐요",
          "original_form": "또있나봐요",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028437.1.26",
          "form": "날씨가 우중충한게 공포 보고싶어요",
          "original_form": "날씨가 우중충한게 공포 보고싶어요",
          "speaker_id": "1",
          "time": "20210903 08:08"
        },
        {
          "id": "MDRW2100028437.1.27",
          "form": "맞아요 공포영화보기좋은 날씨죠",
          "original_form": "맞아요 공포영화보기좋은 날씨죠",
          "speaker_id": "2",
          "time": "20210903 09:00"
        },
        {
          "id": "MDRW2100028437.1.28",
          "form": "티비에 뭐 재밌능거 하나요?",
          "original_form": "티비에 뭐 재밌능거 하나요?",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100028437.1.29",
          "form": "모르겠네요? 금요일에 딱히 볼 게 없어보이네요",
          "original_form": "모르겠네요? 금요일에 딱히 볼 게 없어보이네요",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100028437.1.30",
          "form": "지금은 뭐하시는데요?",
          "original_form": "지금은 뭐하시는데요?",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100028437.1.31",
          "form": "노래들어요 발라드 틀어놓고 강아지 발만지고있어요",
          "original_form": "노래들어요 발라드 틀어놓고 강아지 발만지고있어요",
          "speaker_id": "2",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100028437.1.32",
          "form": "이어폰으로 듣고계신가요?",
          "original_form": "이어폰으로 듣고계신가요?",
          "speaker_id": "1",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100028437.1.33",
          "form": "아니요 이어폰꼈더니 귀에 염증나서 자주 안끼려고요",
          "original_form": "아니요 이어폰꼈더니 귀에 염증나서 자주 안끼려고요",
          "speaker_id": "2",
          "time": "20210903 09:39"
        },
        {
          "id": "MDRW2100028437.1.34",
          "form": "헉 염증 이슈가 있더라구요",
          "original_form": "헉 염증 이슈가 있더라구요",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100028437.1.35",
          "form": "그러게요 제가 겪을줄은 몰랐네요",
          "original_form": "그러게요 제가 겪을줄은 몰랐네요",
          "speaker_id": "2",
          "time": "20210903 09:46"
        }
      ]
    }
