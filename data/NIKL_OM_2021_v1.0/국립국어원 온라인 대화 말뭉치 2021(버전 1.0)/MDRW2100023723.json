      "utterance": [
        {
          "id": "MDRW2100023723.1.1",
          "form": "줌은 하는데 재택은 아닌거야?",
          "original_form": "줌은 하는데 재택은 아닌거야?",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100023723.1.2",
          "form": "응 각자교실에 앉아서 줌해ㅋㅋ",
          "original_form": "응 각자교실에 앉아서 줌해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100023723.1.3",
          "form": "집에서 재택하는 꼴은 못보겠나봥",
          "original_form": "집에서 재택하는 꼴은 못보겠나봥",
          "speaker_id": "1",
          "time": "20210830 19:11"
        },
        {
          "id": "MDRW2100023723.1.4",
          "form": "그런가베 부들부들",
          "original_form": "그런가베 부들부들",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023723.1.5",
          "form": "출근해서 줌이라니..ㅋㅋ",
          "original_form": "출근해서 줌이라니..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100023723.1.6",
          "form": "장소이동줌ㅋㅋ 계속 줌인거야?",
          "original_form": "장소이동줌ㅋㅋ 계속 줌인거야?",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023723.1.7",
          "form": "아니 단계낮아지면 줌3일 대면2일인거같아",
          "original_form": "아니 단계낮아지면 줌3일 대면2일인거같아",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023723.1.8",
          "form": "전면등교할수도있고ㅋ",
          "original_form": "전면등교할수도있고ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023723.1.9",
          "form": "일반회사 다니는 친구는 재택한지 1년도 넘은듯!",
          "original_form": "일반회사 다니는 친구는 재택한지 1년도 넘은듯!",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100023723.1.10",
          "form": "그래도 회사가 다잘굴러가는거보면",
          "original_form": "그래도 회사가 다잘굴러가는거보면",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100023723.1.11",
          "form": "주4일해돋늴듯",
          "original_form": "주4일해돋늴듯",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100023723.1.12",
          "form": "ㅋㅋ 주4 좋다",
          "original_form": "ㅋㅋ 주4 좋다",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100023723.1.13",
          "form": "그치? 주4회 출근 1회 재택도 좋지..ㅋ",
          "original_form": "그치? 주4회 출근 1회 재택도 좋지..ㅋ",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100023723.1.14",
          "form": "재택으로도 세상 잘 돌아가는거보니 진짜 주4일제 해야겠다싶어 ㅋ",
          "original_form": "재택으로도 세상 잘 돌아가는거보니 진짜 주4일제 해야겠다싶어 ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
