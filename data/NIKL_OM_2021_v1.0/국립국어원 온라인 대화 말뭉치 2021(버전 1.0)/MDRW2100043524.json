      "utterance": [
        {
          "id": "MDRW2100043524.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043524.1.2",
          "form": "무슨 음료 좋아하세요?",
          "original_form": "무슨 음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043524.1.3",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043524.1.4",
          "form": "저는 바닐라라떼요!!",
          "original_form": "저는 바닐라라떼요!!",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043524.1.5",
          "form": "자몽에이드도 좋아해요",
          "original_form": "자몽에이드도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043524.1.6",
          "form": "와 ㅎㅎ 저도 자몽에이드 좋아해요!",
          "original_form": "와 ㅎㅎ 저도 자몽에이드 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043524.1.7",
          "form": "바닐라라떼는 어느 브랜드꺼 좋아하세요?",
          "original_form": "바닐라라떼는 어느 브랜드꺼 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043524.1.8",
          "form": "전 집 앞에 컴포즈 있어서 주로 컴포즈에서 사마셔요",
          "original_form": "전 집 앞에 컴포즈 있어서 주로 컴포즈에서 사마셔요",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043524.1.9",
          "form": "아하 컴포즈가 맛있나요?",
          "original_form": "아하 컴포즈가 맛있나요?",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043524.1.10",
          "form": "저는 라떼!! 컴포즈 엄청 좋아해요",
          "original_form": "저는 라떼!! 컴포즈 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043524.1.11",
          "form": "집근처에 하삼동이랑 컴포즈 있는데 하삼동보단 컴포즈가 저는 더 괜찮더라구요!",
          "original_form": "집근처에 하삼동이랑 컴포즈 있는데 하삼동보단 컴포즈가 저는 더 괜찮더라구요!",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043524.1.12",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043524.1.13",
          "form": "저도저도 컴포즈를 더 좋아해요",
          "original_form": "저도저도 컴포즈를 더 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043524.1.14",
          "form": "네 금방 ㅎㅎ",
          "original_form": "네 금방 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043524.1.15",
          "form": "메뉴 뭐였어요 ㅋㅋ",
          "original_form": "메뉴 뭐였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043524.1.16",
          "form": "저는 삼겹살 먹었어요!",
          "original_form": "저는 삼겹살 먹었어요!",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043524.1.17",
          "form": "와....삼겹살... 맛있겠어요",
          "original_form": "와....삼겹살... 맛있겠어요",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043524.1.18",
          "form": "저는 고등어랑",
          "original_form": "저는 고등어랑",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043524.1.19",
          "form": "오늘 배달온 조선호텔 김치에",
          "original_form": "오늘 배달온 조선호텔 김치에",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043524.1.20",
          "form": "김이요!",
          "original_form": "김이요!",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043524.1.21",
          "form": "고등어는 구워서 드셨나요?",
          "original_form": "고등어는 구워서 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043524.1.22",
          "form": "조림해드셨나요!?",
          "original_form": "조림해드셨나요!?",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043524.1.23",
          "form": "둘 다 맛있긴하지만ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "둘 다 맛있긴하지만ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043524.1.24",
          "form": "구이로 먹었어요!!",
          "original_form": "구이로 먹었어요!!",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043524.1.25",
          "form": "전 구이가 좋습니다 ㅎㅎ",
          "original_form": "전 구이가 좋습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043524.1.26",
          "form": "그러시구나 ㅎㅎㅎ 요리는주로 직접 해드시나요?",
          "original_form": "그러시구나 ㅎㅎㅎ 요리는주로 직접 해드시나요?",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043524.1.27",
          "form": "아님 배달해서 드시나요??",
          "original_form": "아님 배달해서 드시나요??",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043524.1.28",
          "form": "주로 직접해먹어요 ㅋㅋ 배달을 잘 안해요",
          "original_form": "주로 직접해먹어요 ㅋㅋ 배달을 잘 안해요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043524.1.29",
          "form": "저도 거의 해먹는긴 하는데 그래도 일주일에 한 번 정도는 배달해서 먹어요.....ㅋㅋㅋ",
          "original_form": "저도 거의 해먹는긴 하는데 그래도 일주일에 한 번 정도는 배달해서 먹어요.....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043524.1.30",
          "form": "메뉴는 어떤걸루 해드세요?",
          "original_form": "메뉴는 어떤걸루 해드세요?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043524.1.31",
          "form": "음 배달말고 사다먹기도하고 ㅎㅎ",
          "original_form": "음 배달말고 사다먹기도하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043524.1.32",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043524.1.33",
          "form": "매번 비슷한 음식만 해먹으니 좀 질려서요ㅠㅠ",
          "original_form": "매번 비슷한 음식만 해먹으니 좀 질려서요ㅠㅠ",
