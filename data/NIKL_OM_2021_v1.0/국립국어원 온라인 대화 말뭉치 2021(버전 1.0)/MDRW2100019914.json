        {
          "id": "MDRW2100019914.1.1",
          "form": "오ㅎ 쇼핑ㅎ",
          "original_form": "오ㅎ 쇼핑ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019914.1.2",
          "form": "이제 쇼핑은 자제를 해야겟네요ㅎㅎㅎ",
          "original_form": "이제 쇼핑은 자제를 해야겟네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019914.1.3",
          "form": "ㅎㅎㅎ 쇼핑 으로 탕진 많이 해요?",
          "original_form": "ㅎㅎㅎ 쇼핑 으로 탕진 많이 해요?",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019914.1.4",
          "form": "아뇨ㅎㅎ 지금까지 거의 안샀는데ㅎ",
          "original_form": "아뇨ㅎㅎ 지금까지 거의 안샀는데ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.5",
          "form": "앱테크하면서 쇼핑중독이 된것같아요ㅎㅎ",
          "original_form": "앱테크하면서 쇼핑중독이 된것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.6",
          "form": "엌 전 왠만하면 안사요",
          "original_form": "엌 전 왠만하면 안사요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.7",
          "form": "전 시작한지 얼마안됬잖아요ㅎ 기껏해야 6개월ㅎㅎ",
          "original_form": "전 시작한지 얼마안됬잖아요ㅎ 기껏해야 6개월ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.8",
          "form": "딱 맞춰서 ㅎㅎ",
          "original_form": "딱 맞춰서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.9",
          "form": "그래서 요즘 많이 자제 하고있는데ㅎㅎ",
          "original_form": "그래서 요즘 많이 자제 하고있는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.10",
          "form": "이게 딱 그렇데요",
          "original_form": "이게 딱 그렇데요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019914.1.11",
          "form": "저렴하지 더사자 와",
          "original_form": "저렴하지 더사자 와",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019914.1.12",
          "form": "너무 재밌긴해요ㅎㅎ",
          "original_form": "너무 재밌긴해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019914.1.13",
          "form": "그래도 적립금에서 딱 해결하자",
          "original_form": "그래도 적립금에서 딱 해결하자",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019914.1.14",
          "form": "근데 진짜 고수는 적립금 날라가도 살거 없음 안산데요 ㅎㅎ",
          "original_form": "근데 진짜 고수는 적립금 날라가도 살거 없음 안산데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019914.1.15",
          "form": "필여한게 아니라 ㅡㅁ ㅡ",
          "original_form": "필여한게 아니라 ㅡㅁ ㅡ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019914.1.16",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019914.1.17",
          "form": "전 아직 초보네요ㅎ",
          "original_form": "전 아직 초보네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019914.1.18",
          "form": "오늘 스벅에 눈이멀어...ㅎㅎㅎㅎ",
          "original_form": "오늘 스벅에 눈이멀어...ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019914.1.19",
          "form": "저도 고수는 아님여 ㅋㅋ",
          "original_form": "저도 고수는 아님여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019914.1.20",
          "form": "카팅온 바로 질렀네요ㅎㅎ",
