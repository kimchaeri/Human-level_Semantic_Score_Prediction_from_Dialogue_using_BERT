      "utterance": [
        {
          "id": "MDRW2100014255.1.1",
          "form": "책상 산다고 하더니",
          "original_form": "책상 산다고 하더니",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014255.1.2",
          "form": "책상이 좋긴하데 방안 자리를 너무 차지할거같아서",
          "original_form": "책상이 좋긴하데 방안 자리를 너무 차지할거같아서",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014255.1.3",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014255.1.4",
          "form": "엉 침대테이블은 공간활용도 되니 방을 넓게 쓸수 있을거 같아서",
          "original_form": "엉 침대테이블은 공간활용도 되니 방을 넓게 쓸수 있을거 같아서",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014255.1.5",
          "form": "어 어디서 살려고",
          "original_form": "어 어디서 살려고",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014255.1.6",
          "form": "바데스코에 팔더라 높이 조절도 젤 높고",
          "original_form": "바데스코에 팔더라 높이 조절도 젤 높고",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014255.1.7",
          "form": "원목으로 된거 안사고",
          "original_form": "원목으로 된거 안사고",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014255.1.8",
          "form": "나도 그러고 싶은데 원목은 높이에서 침대에 안됨",
          "original_form": "나도 그러고 싶은데 원목은 높이에서 침대에 안됨",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014255.1.9",
          "form": "아 너 침대 높이가 있지",
          "original_form": "아 너 침대 높이가 있지",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014255.1.10",
          "form": "엉 그래서 높이 조절 가능할걸로 살려고",
          "original_form": "엉 그래서 높이 조절 가능할걸로 살려고",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014255.1.11",
          "form": "원목보다는 싸겠다 그럼",
          "original_form": "원목보다는 싸겠다 그럼",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014255.1.12",
          "form": "엉 5만원 좀 넘는데 배송비 주고 하면 거의 7만원 가까이",
          "original_form": "엉 5만원 좀 넘는데 배송비 주고 하면 거의 7만원 가까이",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014255.1.13",
          "form": "그 가격이면 싸다",
          "original_form": "그 가격이면 싸다",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
