      "utterance": [
        {
          "id": "MDRW2100042134.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042134.1.2",
          "form": "네 먹었어요. 드셨어요?",
          "original_form": "네 먹었어요. 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042134.1.3",
          "form": "네네 김치찌개먹었어요 뭐 드셨어요",
          "original_form": "네네 김치찌개먹었어요 뭐 드셨어요",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042134.1.4",
          "form": "목살 조금 구워먹었어요",
          "original_form": "목살 조금 구워먹었어요",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042134.1.5",
          "form": "우와 목살",
          "original_form": "우와 목살",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042134.1.6",
          "form": "월요일 든든하게 시작하셨네요",
          "original_form": "월요일 든든하게 시작하셨네요",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042134.1.7",
          "form": "네ㅋㅋ 월요병엔 꽤 효과 좋아요ㅋㅋㅋ",
          "original_form": "네ㅋㅋ 월요병엔 꽤 효과 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042134.1.8",
          "form": "오 저도 고기 먹을걸 ㅋㅋ 낼 먹어야져",
          "original_form": "오 저도 고기 먹을걸 ㅋㅋ 낼 먹어야져",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042134.1.9",
          "form": "김치도 볶아서 같이 먹었는데 맛있어요",
          "original_form": "김치도 볶아서 같이 먹었는데 맛있어요",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042134.1.10",
          "form": "우와 ... 진짜 맛나게 드셨다",
          "original_form": "우와 ... 진짜 맛나게 드셨다",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042134.1.11",
          "form": "ㅋㅋ좀 간단하게 먹었어요. 요샌 고기집을 못 가서 아쉽네요.",
          "original_form": "ㅋㅋ좀 간단하게 먹었어요. 요샌 고기집을 못 가서 아쉽네요.",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100042134.1.12",
          "form": "맞아요ㅠㅠ 쌈두 싸먹고 김치도 굽고 해야하는데",
          "original_form": "맞아요ㅠㅠ 쌈두 싸먹고 김치도 굽고 해야하는데",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042134.1.13",
          "form": "볶음밥도 해먹어야되는데ㅠㅠ",
          "original_form": "볶음밥도 해먹어야되는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:48"
        },
        {
          "id": "MDRW2100042134.1.14",
          "form": "집에서 굽는건 또 번거롭더라구요",
          "original_form": "집에서 굽는건 또 번거롭더라구요",
          "speaker_id": "2",
          "time": "20210906 20:49"
        },
        {
          "id": "MDRW2100042134.1.15",
          "form": "그니까요ㅠㅠ오 볶음밥 ㅠㅠ",
          "original_form": "그니까요ㅠㅠ오 볶음밥 ㅠㅠ",
          "speaker_id": "1",
