      "utterance": [
        {
          "id": "MDRW2100000988.1.1",
          "form": "반려동물은 키우시나요?",
          "original_form": "반려동물은 키우시나요?",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100000988.1.2",
          "form": "반려동물 있으실까요?",
          "original_form": "반려동물 있으실까요?",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100000988.1.3",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000988.1.4",
          "form": "강아지 한미리가 있습니다",
          "original_form": "강아지 한미리가 있습니다",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000988.1.5",
          "form": "아주 귀여우어요",
          "original_form": "아주 귀여우어요",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000988.1.6",
          "form": "앗 저두요 강아지 한마리있어요",
          "original_form": "앗 저두요 강아지 한마리있어요",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000988.1.7",
          "form": "넘 귀여워요",
          "original_form": "넘 귀여워요",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000988.1.8",
          "form": "그쵸 강아지는 너무 귀여워요",
          "original_form": "그쵸 강아지는 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000988.1.9",
          "form": "저는 요크셔테리어에요!",
          "original_form": "저는 요크셔테리어에요!",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000988.1.10",
          "form": "애기는 어떤 간식을 제일 좋아하나요?",
          "original_form": "애기는 어떤 간식을 제일 좋아하나요?",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000988.1.11",
          "form": "다좋아해요... 없어서 못먹을지경이에요",
          "original_form": "다좋아해요... 없어서 못먹을지경이에요",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000988.1.12",
          "form": "근데 나이가있다보니까 말랑말랑한걸 좋아해요",
          "original_form": "근데 나이가있다보니까 말랑말랑한걸 좋아해요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000988.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ귀여워라 알러지는 없어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀여워라 알러지는 없어요?",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000988.1.14",
          "form": "딱히 알러지는 없는데",
          "original_form": "딱히 알러지는 없는데",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000988.1.15",
          "form": "매운걸 좋아해요",
          "original_form": "매운걸 좋아해요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000988.1.16",
          "form": "이상해요",
          "original_form": "이상해요",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000988.1.17",
          "form": "강아지가요?",
          "original_form": "강아지가요?",
          "speaker_id": "1",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000988.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ앜 특이해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앜 특이해요",
          "speaker_id": "1",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000988.1.19",
          "form": "입맛에 맞았나봐요",
          "original_form": "입맛에 맞았나봐요",
          "speaker_id": "2",
          "time": "20210820 11:29"
        },
        {
          "id": "MDRW2100000988.1.20",
          "form": "저희강아지는 그냥 요즘 눈물이 많이 생겼어요",
          "original_form": "저희강아지는 그냥 요즘 눈물이 많이 생겼어요",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000988.1.21",
          "form": "그게 걱정이예요",
          "original_form": "그게 걱정이예요",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000988.1.22",
          "form": "세상에 ㅠㅠㅠㅠ 애기 사료 바꿔보셨어요??ㅠㅠ",
          "original_form": "세상에 ㅠㅠㅠㅠ 애기 사료 바꿔보셨어요??ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000988.1.23",
          "form": "네 사료는 바꿔봤는데 효과가 미비하더라구요ㅠㅠ",
          "original_form": "네 사료는 바꿔봤는데 효과가 미비하더라구요ㅠㅠ",
