      "utterance": [
        {
          "id": "MDRW2100032756.1.1",
          "form": "담주수욜빼고다가능",
          "original_form": "담주수욜빼고다가능",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032756.1.2",
          "form": "50개 이상 해치운듯ㅋㅋ",
          "original_form": "50개 이상 해치운듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032756.1.3",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032756.1.4",
          "form": "물론오셔도공부를하셔야하지만요",
          "original_form": "물론오셔도공부를하셔야하지만요",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032756.1.5",
          "form": "담주가 수욜빼고 다 가능???",
          "original_form": "담주가 수욜빼고 다 가능???",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032756.1.6",
          "form": "공부?ㅋㅋㅋ뭐 늘 하는게 공부긴 하지",
          "original_form": "공부?ㅋㅋㅋ뭐 늘 하는게 공부긴 하지",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032756.1.7",
          "form": "맞슴돠",
          "original_form": "맞슴돠",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032756.1.8",
          "form": "동생없을때오는게젤좋쥐",
          "original_form": "동생없을때오는게젤좋쥐",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032756.1.9",
          "form": "담주에 동생이 없음?",
          "original_form": "담주에 동생이 없음?",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032756.1.10",
          "form": "낼은확실히동생이없긴함",
          "original_form": "낼은확실히동생이없긴함",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032756.1.11",
          "form": "야우리집 추석에 아무도 안내려감",
          "original_form": "야우리집 추석에 아무도 안내려감",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
