        },
        {
          "id": "MDRW2100002002.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002002.1.5",
          "form": "해외여행 가보셨어요?",
          "original_form": "해외여행 가보셨어요?",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002002.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002002.1.7",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100002002.1.8",
          "form": "파리 괌 호주 등이요",
          "original_form": "파리 괌 호주 등이요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100002002.1.9",
          "form": "오 되게 재미있었을거 같아요",
          "original_form": "오 되게 재미있었을거 같아요",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002002.1.10",
          "form": "네 호주가 정말 젛았아요",
          "original_form": "네 호주가 정말 젛았아요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100002002.1.11",
          "form": "호주가서 캥거루 보셨어요?",
          "original_form": "호주가서 캥거루 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100002002.1.12",
          "form": "호주는 벌레든 뭐든 다 크다던데",
          "original_form": "호주는 벌레든 뭐든 다 크다던데",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100002002.1.13",
          "form": "캥거루 코알라 코앞에서 봤어요",
          "original_form": "캥거루 코알라 코앞에서 봤어요",
          "speaker_id": "1",
          "time": "20210819 22:59"
        },
        {
          "id": "MDRW2100002002.1.14",
          "form": "캥거루 실제로 보면 커요?",
