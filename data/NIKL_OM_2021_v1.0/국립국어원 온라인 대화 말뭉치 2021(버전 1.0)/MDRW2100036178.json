      "utterance": [
        {
          "id": "MDRW2100036178.1.1",
          "form": "시장 닭강정 진짜 맛있죠!",
          "original_form": "시장 닭강정 진짜 맛있죠!",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100036178.1.2",
          "form": "맞아요 근데 시장이 일찍 닫아서 잘 못 먹어ㅠㅠ",
          "original_form": "맞아요 근데 시장이 일찍 닫아서 잘 못 먹어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100036178.1.3",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100036178.1.4",
          "form": "가끔 보이면 유혹에 넘어가서 꼭 사와요ㅋㅋㅋㅋ",
          "original_form": "가끔 보이면 유혹에 넘어가서 꼭 사와요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100036178.1.5",
          "form": "넘 맛있으니까 어쩔 수 없이 백전백패...ㅋㅋㅋㅋ",
          "original_form": "넘 맛있으니까 어쩔 수 없이 백전백패...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100036178.1.6",
          "form": "닭강정에 들어있는 떡 좋아하시나요?",
          "original_form": "닭강정에 들어있는 떡 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100036178.1.7",
          "form": "떡 없어서 못 먹어요 ㅋㅋㅋ",
          "original_form": "떡 없어서 못 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:12"
        },
        {
          "id": "MDRW2100036178.1.8",
          "form": "떡 튀긴 거 너무 맛있어요ㅋㅋㅋ 떡꼬치도 너무 맛있고",
          "original_form": "떡 튀긴 거 너무 맛있어요ㅋㅋㅋ 떡꼬치도 너무 맛있고",
          "speaker_id": "1",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100036178.1.9",
          "form": "학교 다닐 때 떡꼬치 엄청 먹었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "학교 다닐 때 떡꼬치 엄청 먹었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:14"
        },
        {
          "id": "MDRW2100036178.1.10",
          "form": "피카츄돈까스 기억 나세요?",
          "original_form": "피카츄돈까스 기억 나세요?",
          "speaker_id": "2",
          "time": "20210830 18:15"
        },
        {
          "id": "MDRW2100036178.1.11",
          "form": "네네! 학원 끝나고 자주 사먹었어요",
          "original_form": "네네! 학원 끝나고 자주 사먹었어요",
          "speaker_id": "1",
          "time": "20210830 18:15"
        },
        {
          "id": "MDRW2100036178.1.12",
          "form": "맨날 500원씩 챙겨 가서",
          "original_form": "맨날 500원씩 챙겨 가서",
          "speaker_id": "1",
          "time": "20210830 18:15"
        },
        {
          "id": "MDRW2100036178.1.13",
          "form": "그게 왤케 맛있었울까요 ㅋㅋㅋㅋㅋ",
          "original_form": "그게 왤케 맛있었울까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100036178.1.14",
          "form": "그 위에 뿌려주는 빨간 소스가 너무 맛있었어요ㅋㅋ",
          "original_form": "그 위에 뿌려주는 빨간 소스가 너무 맛있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100036178.1.15",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ근데 요즘은 학교앞 분식점 많이 없어졌더라구요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ근데 요즘은 학교앞 분식점 많이 없어졌더라구요",
          "speaker_id": "2",
          "time": "20210830 18:33"
        },
        {
          "id": "MDRW2100036178.1.16",
          "form": "맞아요...ㅠㅠ 아예 근처에 분식집이 없는 곳도 있더라고요... 코로나 때문에 문 닫는 곳도 많고",
          "original_form": "맞아요...ㅠㅠ 아예 근처에 분식집이 없는 곳도 있더라고요... 코로나 때문에 문 닫는 곳도 많고",
          "speaker_id": "1",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036178.1.17",
          "form": "코로나 때문에 다들 많이 힘들어요ㅠㅠ",
          "original_form": "코로나 때문에 다들 많이 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 18:39"
        }
      ]
