      "utterance": [
        {
          "id": "MDRW2100006049.1.1",
          "form": "축구조아해?",
          "original_form": "축구조아해?",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006049.1.2",
          "form": "아니 너는?",
          "original_form": "아니 너는?",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006049.1.3",
          "form": "조아 아는선수있음?",
          "original_form": "조아 아는선수있음?",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006049.1.4",
          "form": "손흥민",
          "original_form": "손흥민",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006049.1.5",
          "form": "박지성",
          "original_form": "박지성",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.6",
          "form": "이천수",
          "original_form": "이천수",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.7",
          "form": "박지성은퇴함",
          "original_form": "박지성은퇴함",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.8",
          "form": "감독하나",
          "original_form": "감독하나",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.9",
          "form": "이천수래 웃기는 거지 지금...",
          "original_form": "이천수래 웃기는 거지 지금...",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.10",
          "form": "맞잖아",
          "original_form": "맞잖아",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.11",
          "form": "ㄴㄴ 지도자 코스밟고 코치같은거할거야 전북",
          "original_form": "ㄴㄴ 지도자 코스밟고 코치같은거할거야 전북",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.12",
          "form": "이천수는 먼 이천수야.. 은퇴하셨어..",
          "original_form": "이천수는 먼 이천수야.. 은퇴하셨어..",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100006049.1.13",
          "form": "아득하다.",
          "original_form": "아득하다.",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.14",
          "form": "오 전북가는구나",
          "original_form": "오 전북가는구나",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.15",
          "form": "이청용",
          "original_form": "이청용",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.16",
          "form": "그래서 말많았지",
          "original_form": "그래서 말많았지",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.17",
          "form": "이청용조아",
          "original_form": "이청용조아",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.18",
          "form": "손흥민전에 조아했어",
          "original_form": "손흥민전에 조아했어",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.19",
          "form": "걔누구지",
          "original_form": "걔누구지",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.20",
          "form": "기성용",
          "original_form": "기성용",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006049.1.21",
          "form": "답답하면 니들이 뛰던가",
          "original_form": "답답하면 니들이 뛰던가",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
