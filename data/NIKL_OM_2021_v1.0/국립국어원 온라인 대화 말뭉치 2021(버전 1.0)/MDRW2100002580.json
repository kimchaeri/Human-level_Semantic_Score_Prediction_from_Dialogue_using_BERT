      "utterance": [
        {
          "id": "MDRW2100002580.1.1",
          "form": "중고거래 하세요?",
          "original_form": "중고거래 하세요?",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002580.1.2",
          "form": "중고거래 해본적은 있어요",
          "original_form": "중고거래 해본적은 있어요",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002580.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002580.1.4",
          "form": "중고거래할때는 신중하게 해야죠",
          "original_form": "중고거래할때는 신중하게 해야죠",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002580.1.5",
          "form": "물건 판매도 해봤고 사보기도 해봤죠",
          "original_form": "물건 판매도 해봤고 사보기도 해봤죠",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002580.1.6",
          "form": "저는 판매만요",
          "original_form": "저는 판매만요",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002580.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002580.1.8",
          "form": "중간에 사기도 당해본적도 있어요ㅠㅠ",
          "original_form": "중간에 사기도 당해본적도 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002580.1.9",
          "form": "신중하게 했는데도",
          "original_form": "신중하게 했는데도",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002580.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002580.1.11",
          "form": "조심해야합니다.",
          "original_form": "조심해야합니다.",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002580.1.12",
          "form": "중고거래할때는 항상 조심해야죠ㅠㅠ",
          "original_form": "중고거래할때는 항상 조심해야죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002580.1.13",
          "form": "네 불안하죠",
          "original_form": "네 불안하죠",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002580.1.14",
          "form": "안그래도 며칠전에 머지포인트 회사부도나서",
          "original_form": "안그래도 며칠전에 머지포인트 회사부도나서",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002580.1.15",
          "form": "마음이 심난해요ㅠㅠ",
          "original_form": "마음이 심난해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002580.1.16",
          "form": "머지 하셨구나",
          "original_form": "머지 하셨구나",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002580.1.17",
          "form": "머지포인트아시죠? 할인해주는거ㅠㅠ",
          "original_form": "머지포인트아시죠? 할인해주는거ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002580.1.18",
          "form": "내용기사보고 충격받아서ㅠㅠ",
          "original_form": "내용기사보고 충격받아서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002580.1.19",
          "form": "멘탈붕괴중이에요ㅠㅠ",
          "original_form": "멘탈붕괴중이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002580.1.20",
          "form": "못돌려받아요ㅠㅠ",
          "original_form": "못돌려받아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002580.1.21",
          "form": "아그런가요?",
          "original_form": "아그런가요?",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002580.1.22",
          "form": "수많은사람이 그머지를 충전했으니깐요ㅜㅜ",
          "original_form": "수많은사람이 그머지를 충전했으니깐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002580.1.23",
          "form": "하긴",
          "original_form": "하긴",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002580.1.24",
          "form": "그회사가 2년동안 10000억? 넘게 벌었데요",
