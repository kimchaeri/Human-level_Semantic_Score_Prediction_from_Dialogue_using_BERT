      "utterance": [
        {
          "id": "MDRW2100023061.1.1",
          "form": "안녕하세요 저녁 뭐 드셨나요!",
          "original_form": "안녕하세요 저녁 뭐 드셨나요!",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023061.1.2",
          "form": "안녕하세용!",
          "original_form": "안녕하세용!",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100023061.1.3",
          "form": "저는 빵 먹구",
          "original_form": "저는 빵 먹구",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023061.1.4",
          "form": "라면도 먹었어용!",
          "original_form": "라면도 먹었어용!",
          "speaker_id": "2",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023061.1.5",
          "form": "우왕 많이 드셨네여",
          "original_form": "우왕 많이 드셨네여",
          "speaker_id": "1",
          "time": "20210829 21:20"
        },
        {
          "id": "MDRW2100023061.1.6",
          "form": "name1은 뭐 드셨어용?",
          "original_form": "&name1&은 뭐 드셨어용?",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100023061.1.7",
          "form": "아직 안 먹었어요",
          "original_form": "아직 안 먹었어요",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023061.1.8",
          "form": "오일파스타 해먹을까 하는 중이에요!",
          "original_form": "오일파스타 해먹을까 하는 중이에요!",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023061.1.9",
          "form": "헐헐",
          "original_form": "헐헐",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023061.1.10",
          "form": "오일 소스는 시판 소스 사드시는거 있으세요?",
          "original_form": "오일 소스는 시판 소스 사드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023061.1.11",
          "form": "아뇨! 제가 해먹어요!!",
          "original_form": "아뇨! 제가 해먹어요!!",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023061.1.12",
          "form": "그냥 오일 두르고 재료넣고 볶고",
          "original_form": "그냥 오일 두르고 재료넣고 볶고",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100023061.1.13",
          "form": "소금후추 갈릭파우더 넣고 챱챱하면 끝이에요!!",
          "original_form": "소금후추 갈릭파우더 넣고 챱챱하면 끝이에요!!",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023061.1.14",
          "form": "와 요리 잘하시나봐요!",
          "original_form": "와 요리 잘하시나봐요!",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023061.1.15",
          "form": "저는 청정원 파스타 소스 맨날 사먹는데ㅠㅠㅋㅋ",
          "original_form": "저는 청정원 파스타 소스 맨날 사먹는데ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023061.1.16",
          "form": "ㅋㅋㅋ청정원도 맛있죠!",
          "original_form": "ㅋㅋㅋ청정원도 맛있죠!",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023061.1.17",
          "form": "청정원에서 어떤 거 제일 좋아하세요?",
          "original_form": "청정원에서 어떤 거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023061.1.18",
          "form": "청정원 파스타 소스 여러가지 있는데 그중에서 로제파스타가 제일 맛있어용!",
          "original_form": "청정원 파스타 소스 여러가지 있는데 그중에서 로제파스타가 제일 맛있어용!",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023061.1.19",
          "form": "드셔보셨나유?",
          "original_form": "드셔보셨나유?",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023061.1.20",
          "form": "앗 저 청정원에서 로제랑 볼로네제 먹어봤어요!",
          "original_form": "앗 저 청정원에서 로제랑 볼로네제 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023061.1.21",
          "form": "로제 맛있더라구요...",
          "original_form": "로제 맛있더라구요...",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023061.1.22",
          "form": "그춍ㅋㅋㅋ 로제가 제일 인기가 많은것같아여",
          "original_form": "그춍ㅋㅋㅋ 로제가 제일 인기가 많은것같아여",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023061.1.23",
          "form": "볼로네제는 폰타나가 맛있다던데 드셔보셨나여?",
          "original_form": "볼로네제는 폰타나가 맛있다던데 드셔보셨나여?",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023061.1.24",
          "form": "앗 폰타나꺼는",
          "original_form": "앗 폰타나꺼는",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023061.1.25",
          "form": "크림?",
          "original_form": "크림?",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023061.1.26",
          "form": "그것만 먹어봤는데",
          "original_form": "그것만 먹어봤는데",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023061.1.27",
          "form": "되게 진하고 맛있었어요!",
          "original_form": "되게 진하고 맛있었어요!",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023061.1.28",
          "form": "아아 맞아요 폰타나 크림!",
          "original_form": "아아 맞아요 폰타나 크림!",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023061.1.29",
          "form": "하 배고파지네요..",
          "original_form": "하 배고파지네요..",
          "speaker_id": "1",
          "time": "20210829 21:32"
        }
      ]
    }
