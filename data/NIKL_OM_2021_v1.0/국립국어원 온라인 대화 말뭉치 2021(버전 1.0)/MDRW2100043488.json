      "utterance": [
        {
          "id": "MDRW2100043488.1.1",
          "form": "무럭무럭 크고 있습니다 ㅋㅋ",
          "original_form": "무럭무럭 크고 있습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043488.1.2",
          "form": "힘들진 않으시고요?",
          "original_form": "힘들진 않으시고요?",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043488.1.3",
          "form": "둘째가 이런줄 몰랐어요",
          "original_form": "둘째가 이런줄 몰랐어요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043488.1.4",
          "form": "배도 더 빨리 나오고...",
          "original_form": "배도 더 빨리 나오고...",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043488.1.5",
          "form": "더 힘들어요",
          "original_form": "더 힘들어요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043488.1.6",
          "form": "첫째는 어떤가요?",
          "original_form": "첫째는 어떤가요?",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043488.1.7",
          "form": "첫째는 이제 나름 사람 구실을 하려고 하네요 ㅋㅋ",
          "original_form": "첫째는 이제 나름 사람 구실을 하려고 하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043488.1.8",
          "form": "하지만 자기 주장이 뚜렷해지고 ㅋㅋㅋ",
          "original_form": "하지만 자기 주장이 뚜렷해지고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043488.1.9",
          "form": "샘내거나 하진 않고요?",
          "original_form": "샘내거나 하진 않고요?",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043488.1.10",
          "form": "아직은 눈에 보이지 않아서 그런지 ㅋㅋㅋ",
          "original_form": "아직은 눈에 보이지 않아서 그런지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043488.1.11",
          "form": "괜찮아요 ㅋㅋㅋ",
          "original_form": "괜찮아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043488.1.12",
          "form": "우리 큰애는 배 나오면서부터 질투를 했거든요",
          "original_form": "우리 큰애는 배 나오면서부터 질투를 했거든요",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043488.1.13",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043488.1.14",
          "form": "지금은 잘 지내죠?",
          "original_form": "지금은 잘 지내죠?",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043488.1.15",
          "form": "그럼요. 둘째가 나온 직후가 전쟁통이었엉ㅅ",
          "original_form": "그럼요. 둘째가 나온 직후가 전쟁통이었엉ㅅ",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043488.1.16",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043488.1.17",
          "form": "아...걱정된다...",
          "original_form": "아...걱정된다...",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043488.1.18",
          "form": "어느새 키우려나",
          "original_form": "어느새 키우려나",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043488.1.19",
          "form": "우리 큰애는 막 소리를 미친듯이 지르면서",
          "original_form": "우리 큰애는 막 소리를 미친듯이 지르면서",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043488.1.20",
          "form": "장난감을통을 집어 던지고",
          "original_form": "장난감을통을 집어 던지고",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043488.1.21",
          "form": "아...ㅠㅠ 힘드셨겠어요",
          "original_form": "아...ㅠㅠ 힘드셨겠어요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043488.1.22",
          "form": "책장을 쓰러뜨리고 그랬어요 ㅡ.ㅡ",
          "original_form": "책장을 쓰러뜨리고 그랬어요 ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043488.1.23",
          "form": "진짜 둘 챙기는거 만만치 않을듯요",
          "original_form": "진짜 둘 챙기는거 만만치 않을듯요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043488.1.24",
          "form": "그래서 둘째를 괜히 낳았구나 싶었는데",
          "original_form": "그래서 둘째를 괜히 낳았구나 싶었는데",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043488.1.25",
          "form": "둘째가 너무 예쁘고요",
          "original_form": "둘째가 너무 예쁘고요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043488.1.26",
          "form": "둘째는 사랑이라고",
          "original_form": "둘째는 사랑이라고",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043488.1.27",
          "form": "둘이 잘 놀아서 제 자유시간이 점점 길어져요",
          "original_form": "둘이 잘 놀아서 제 자유시간이 점점 길어져요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043488.1.28",
          "form": "부럽습니다~",
          "original_form": "부럽습니다~",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043488.1.29",
          "form": "맞아요 둘째는 뭘 해도 예쁩니다",
          "original_form": "맞아요 둘째는 뭘 해도 예쁩니다",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043488.1.30",
          "form": "그리고 우리가 첫째 키우며 이미 단련이 됐잖아요",
          "original_form": "그리고 우리가 첫째 키우며 이미 단련이 됐잖아요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043488.1.31",
          "form": "둘찌는 뭘 해도 예상 가능해서 좀 나아요",
          "original_form": "둘찌는 뭘 해도 예상 가능해서 좀 나아요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        }
      ]
    }
