      "utterance": [
        {
          "id": "MDRW2100004830.1.1",
          "form": "저번에반고흐 전시회 어땟엇니?",
          "original_form": "저번에반고흐 전시회 어땟엇니?",
          "speaker_id": "1",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004830.1.2",
          "form": "반고흐? 피카소 아니고?",
          "original_form": "반고흐? 피카소 아니고?",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004830.1.3",
          "form": "피카소였었나? 헷갈렸네",
          "original_form": "피카소였었나? 헷갈렸네",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004830.1.4",
          "form": "ㅎ피카소였지. 나름 재미있었지",
          "original_form": "ㅎ피카소였지. 나름 재미있었지",
          "speaker_id": "2",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004830.1.5",
          "form": "피카소전시회 뭐 기억나?",
          "original_form": "피카소전시회 뭐 기억나?",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004830.1.6",
          "form": "피카소 바람둥이ㅋ",
          "original_form": "피카소 바람둥이ㅋ",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004830.1.7",
          "form": "그렇기는 하지 몇 여자를 만난건지",
          "original_form": "그렇기는 하지 몇 여자를 만난건지",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004830.1.8",
          "form": "근데 시대를앞서서 감각이 좋긴해.",
          "original_form": "근데 시대를앞서서 감각이 좋긴해.",
          "speaker_id": "2",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004830.1.9",
          "form": "응 볼만은 했어.",
          "original_form": "응 볼만은 했어.",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004830.1.10",
          "form": "유명한게 없어서 아쉽고",
          "original_form": "유명한게 없어서 아쉽고",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004830.1.11",
          "form": "집그림 그려진거 맘에들어서 엽서 살까 하다가 말았징",
          "original_form": "집그림 그려진거 맘에들어서 엽서 살까 하다가 말았징",
          "speaker_id": "2",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004830.1.12",
          "form": "엽서 그때 전시회에서 구매했으면 집 인테리어로 좋아겟다",
          "original_form": "엽서 그때 전시회에서 구매했으면 집 인테리어로 좋아겟다",
          "speaker_id": "1",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004830.1.13",
          "form": "그러게. 근데 그 그림 뭐였는지 모르겠어 ㅎ",
          "original_form": "그러게. 근데 그 그림 뭐였는지 모르겠어 ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004830.1.14",
          "form": "인터넷에 피카소 전시회로 검색하면 나올려나?",
          "original_form": "인터넷에 피카소 전시회로 검색하면 나올려나?",
          "speaker_id": "1",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004830.1.15",
          "form": "사진 찍어온거는 있는데 한번 검색해보고 그 그림으로 유화그리기 dIY있으면 사고싶다",
          "original_form": "사진 찍어온거는 있는데 한번 검색해보고 그 그림으로 유화그리기 dIY있으면 사고싶다",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004830.1.16",
          "form": "뭐였는지 나도 궁금하네",
          "original_form": "뭐였는지 나도 궁금하네",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004830.1.17",
          "form": "여기선 사진같은건 못보내네잉ㅎ",
          "original_form": "여기선 사진같은건 못보내네잉ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004830.1.18",
          "form": "전시회 다른거도 봐보고 싶다",
          "original_form": "전시회 다른거도 봐보고 싶다",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004830.1.19",
          "form": "ㅎ난 전시회 말구 너랑 전에갔던 뮤지컬",
