      "utterance": [
        {
          "id": "MDRW2100023195.1.1",
          "form": "동물좋아하세요~",
          "original_form": "동물좋아하세요~",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023195.1.2",
          "form": "네!! 털 달린동물 안달린동물 다 좋아해요 ㅎㅎ",
          "original_form": "네!! 털 달린동물 안달린동물 다 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023195.1.3",
          "form": "키우는동물있으세요!?",
          "original_form": "키우는동물있으세요!?",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023195.1.4",
          "form": "저는 고양이 키워요~",
          "original_form": "저는 고양이 키워요~",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023195.1.5",
          "form": "와 ~ 전 고양이키워본적은없는데",
          "original_form": "와 ~ 전 고양이키워본적은없는데",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023195.1.6",
          "form": "뭐 키워보셨어요?",
          "original_form": "뭐 키워보셨어요?",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023195.1.7",
          "form": "강아지요!",
          "original_form": "강아지요!",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023195.1.8",
          "form": "지금도 키우고 계신가요?",
          "original_form": "지금도 키우고 계신가요?",
          "speaker_id": "1",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023195.1.9",
          "form": "아뇨ㅠ 부모님이랑 같이 살때 키웠던건데 독립후엔 안키워여ㅠ",
          "original_form": "아뇨ㅠ 부모님이랑 같이 살때 키웠던건데 독립후엔 안키워여ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100023195.1.10",
          "form": "지금은 부모님께서 키우시고요?",
          "original_form": "지금은 부모님께서 키우시고요?",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023195.1.11",
          "form": "지금은 동생이 출가해서 데려가서 키워여!",
          "original_form": "지금은 동생이 출가해서 데려가서 키워여!",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023195.1.12",
          "form": "아이구 정 많이 들어서 보고싶겠어요 ㅎㅎ",
          "original_form": "아이구 정 많이 들어서 보고싶겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023195.1.13",
          "form": "한번씩만나면 내무릎에 올라와서 안떨어져요 ㅋㅋ",
          "original_form": "한번씩만나면 내무릎에 올라와서 안떨어져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100023195.1.14",
          "form": "독립후엔 왜 안기르세요?",
          "original_form": "독립후엔 왜 안기르세요?",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100023195.1.15",
          "form": "솔직히 좋아하지만 그만큼 책임질자신은없어서요ㅠ",
          "original_form": "솔직히 좋아하지만 그만큼 책임질자신은없어서요ㅠ",
          "speaker_id": "2",
