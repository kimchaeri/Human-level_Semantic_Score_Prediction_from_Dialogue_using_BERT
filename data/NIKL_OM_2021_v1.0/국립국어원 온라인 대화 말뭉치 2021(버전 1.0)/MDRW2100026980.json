      "utterance": [
        {
          "id": "MDRW2100026980.1.1",
          "form": "오늘 날씨 넘 좋다",
          "original_form": "오늘 날씨 넘 좋다",
          "speaker_id": "2",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100026980.1.2",
          "form": "그러게  진짜 가을 날씨야",
          "original_form": "그러게  진짜 가을 날씨야",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100026980.1.3",
          "form": "살랑살랑 바람도부니 걸어서 머리좀 짜르고 오지?",
          "original_form": "살랑살랑 바람도부니 걸어서 머리좀 짜르고 오지?",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100026980.1.4",
          "form": "자르긴 해야하는데",
          "original_form": "자르긴 해야하는데",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100026980.1.5",
          "form": "추석때쯤 자르려고 했지",
          "original_form": "추석때쯤 자르려고 했지",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100026980.1.6",
          "form": "그때까지 왜기다려",
          "original_form": "그때까지 왜기다려",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026980.1.7",
          "form": "큰집도 안갈껀데",
          "original_form": "큰집도 안갈껀데",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026980.1.8",
          "form": "그런가? 이따 그럼 갔다와야겠다",
          "original_form": "그런가? 이따 그럼 갔다와야겠다",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026980.1.9",
          "form": "응.  뱃살도 뺄겸 걸어가",
          "original_form": "응.  뱃살도 뺄겸 걸어가",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100026980.1.10",
          "form": "같이가면 갈께",
          "original_form": "같이가면 갈께",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100026980.1.11",
          "form": "귀찮게 혼자가지...알았어 이따 해쫌 넘어가면 그때가자",
          "original_form": "귀찮게 혼자가지...알았어 이따 해쫌 넘어가면 그때가자",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100026980.1.12",
          "form": "살빼야하는데 큰일이다. 걷기가ㅈ힘들어",
          "original_form": "살빼야하는데 큰일이다. 걷기가ㅈ힘들어",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100026980.1.13",
          "form": "ㅎ 갑자기 몸무게가 늘어서 그렇지",
          "original_form": "ㅎ 갑자기 몸무게가 늘어서 그렇지",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100026980.1.14",
          "form": "이제 저녁때 쫌씩만 걷자~",
          "original_form": "이제 저녁때 쫌씩만 걷자~",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100026980.1.15",
          "form": "응. 진짜 몸이 너무 안좋아진거같아.",
          "original_form": "응. 진짜 몸이 너무 안좋아진거같아.",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100026980.1.16",
          "form": "나이가 있으니 이제 관리좀해",
          "original_form": "나이가 있으니 이제 관리좀해",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100026980.1.17",
          "form": "그래야겠어. 무리하지않게 조금씩 해야지",
          "original_form": "그래야겠어. 무리하지않게 조금씩 해야지",
          "speaker_id": "2",
          "time": "20210902 13:21"
        }
      ]
