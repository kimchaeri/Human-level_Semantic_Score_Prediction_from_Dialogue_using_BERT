      "utterance": [
        {
          "id": "MDRW2100042388.1.1",
          "form": "공부하기 싫대요",
          "original_form": "공부하기 싫대요",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.2",
          "form": "제일 좋아하는 시간 점심 시간",
          "original_form": "제일 좋아하는 시간 점심 시간",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.3",
          "form": "아 공부",
          "original_form": "아 공부",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.4",
          "form": "밥은 안 먹잖아",
          "original_form": "밥은 안 먹잖아",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.5",
          "form": "밥 먹으면 집에 가니까ㅋㅋㅋ",
          "original_form": "밥 먹으면 집에 가니까ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.7",
          "form": "아 미치겠노 ㅋㅋㅋㅋㄱㅋ",
          "original_form": "아 미치겠노 ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.8",
          "form": "집에 가기 위해",
          "original_form": "집에 가기 위해",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.9",
          "form": "애들은 우리보다 똑똑해요ㅋㅋㅋㅋㅋ",
          "original_form": "애들은 우리보다 똑똑해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.10",
          "form": "먹기 싫은 밥을 먹고",
          "original_form": "먹기 싫은 밥을 먹고",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.11",
          "form": "거쳐 가는 시간이지요ㅋㅋㅋㅋ",
          "original_form": "거쳐 가는 시간이지요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.12",
          "form": "즐거운 하교만을 기다리는",
          "original_form": "즐거운 하교만을 기다리는",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042388.1.13",
          "form": "우리 힐데스",
          "original_form": "우리 힐데스",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.14",
          "form": "피아노 갈 때 제일 신남ㅋㅋㅋㅋ",
          "original_form": "피아노 갈 때 제일 신남ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.16",
          "form": "그래 맞다",
          "original_form": "그래 맞다",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.17",
          "form": "우리 name3도 학교 가서 제일 좋은 건",
          "original_form": "우리 &name3&도 학교 가서 제일 좋은 건",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.18",
          "form": "친구들이랑 노는거",
          "original_form": "친구들이랑 노는거",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.19",
          "form": "제일 싫은 건",
          "original_form": "제일 싫은 건",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.20",
          "form": "점심 시간",
          "original_form": "점심 시간",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.22",
          "form": "밥 먹기 싫어",
          "original_form": "밥 먹기 싫어",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.23",
          "form": "언니 name3에게도 알려줘요",
          "original_form": "언니 &name3&에게도 알려줘요",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.24",
          "form": "어 그래 그래 먹기 싫단다",
          "original_form": "어 그래 그래 먹기 싫단다",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.25",
          "form": "밥 먹고 나면 집에 갈 수 있다는 거",
          "original_form": "밥 먹고 나면 집에 갈 수 있다는 거",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.26",
          "form": "점심 시간을 좀 좋아 하라고ㅋㅋㅋ",
          "original_form": "점심 시간을 좀 좋아 하라고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.27",
          "form": "그러니까 대충 안 먹고 버리고 그냥 나가지",
          "original_form": "그러니까 대충 안 먹고 버리고 그냥 나가지",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042388.1.28",
          "form": "밥을 먹다 안 먹던ㅋㅋㅋㅋ",
          "original_form": "밥을 먹다 안 먹던ㅋㅋㅋㅋ",
