      "utterance": [
        {
          "id": "MDRW2100036331.1.1",
          "form": "저도 2번인가",
          "original_form": "저도 2번인가",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.2",
          "form": "해먹어봤어요 ㅎㅎ 토마토넣고 라면끓여먹는거에요 ㅋㅋ",
          "original_form": "해먹어봤어요 ㅎㅎ 토마토넣고 라면끓여먹는거에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.3",
          "form": "계란풀고 !",
          "original_form": "계란풀고 !",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.4",
          "form": "라면이 갑자기",
          "original_form": "라면이 갑자기",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.5",
          "form": "건강식으로 보여욬ㅋㅋㅋㅋ",
          "original_form": "건강식으로 보여욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.6",
          "form": "나중에 나쁘지않다면 드셔보세요 ㅋㅋㅋ",
          "original_form": "나중에 나쁘지않다면 드셔보세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.7",
          "form": "다음날 덜 붓겠네요",
          "original_form": "다음날 덜 붓겠네요",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.8",
          "form": "전 새벽에 2시에 츌출하면....토마토라면을 먹어요",
          "original_form": "전 새벽에 2시에 츌출하면....토마토라면을 먹어요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036331.1.9",
          "form": "토마토버프로ㅋㅋㅋ",
          "original_form": "토마토버프로ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.10",
          "form": "라면은 반개만넣고 ㅋㅋㅋ",
          "original_form": "라면은 반개만넣고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.11",
          "form": "아아 좋다좋다",
          "original_form": "아아 좋다좋다",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.12",
          "form": "딱좋다",
          "original_form": "딱좋다",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.13",
          "form": "토마토 갈아마셔야겠어요ㅋㅋㅋㅋ",
          "original_form": "토마토 갈아마셔야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.14",
          "form": "토마토를 4개정도 넣어서",
          "original_form": "토마토를 4개정도 넣어서",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.15",
          "form": "갑자기 신나는것",
          "original_form": "갑자기 신나는것",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.16",
          "form": "양이꽤되요 ㅋㅋㅋ",
          "original_form": "양이꽤되요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.17",
          "form": "전 꿀을 넣어요 ㅋㅋㅋ 토마토갈때",
          "original_form": "전 꿀을 넣어요 ㅋㅋㅋ 토마토갈때",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036331.1.18",
          "form": "설탕 말고 꿀 넣으면",
          "original_form": "설탕 말고 꿀 넣으면",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036331.1.19",
          "form": "더 맛있겠네요",
          "original_form": "더 맛있겠네요",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036331.1.20",
          "form": "역시 쩝쩝박사이신것같아요ㅋㅋㅋㅋ",
          "original_form": "역시 쩝쩝박사이신것같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036331.1.21",
          "form": "뭔가 설탕보다 낫겟지 ㅋㅋㅋ싶어서",
          "original_form": "뭔가 설탕보다 낫겟지 ㅋㅋㅋ싶어서",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036331.1.22",
          "form": "저 홈런볼도 지금 있는데 에어프라이어에 돌려서 드셔보셨어요??",
          "original_form": "저 홈런볼도 지금 있는데 에어프라이어에 돌려서 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210830 21:33"
        }
      ]
