      "utterance": [
        {
          "id": "MDRW2100025378.1.1",
          "form": "안녕하세요 쌀쌀한 가을이네요",
          "original_form": "안녕하세요 쌀쌀한 가을이네요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100025378.1.2",
          "form": "네 이제 정말 가을이 온거 같아요",
          "original_form": "네 이제 정말 가을이 온거 같아요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100025378.1.3",
          "form": "저는 쌀쌀해지면 왠지 모르게 챡이 생각나더라고요",
          "original_form": "저는 쌀쌀해지면 왠지 모르게 챡이 생각나더라고요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100025378.1.4",
          "form": "책이 생각나도라고요",
          "original_form": "책이 생각나도라고요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100025378.1.5",
          "form": "저도요 책을 읽으며 여운과 감상에 빠지고 싶은 느낌!",
          "original_form": "저도요 책을 읽으며 여운과 감상에 빠지고 싶은 느낌!",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100025378.1.6",
          "form": "그쵸ㅠㅠ 요즘 책 뭐 읽으세요?",
          "original_form": "그쵸ㅠㅠ 요즘 책 뭐 읽으세요?",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100025378.1.7",
          "form": "전 용의자X의 헌신 이라는 책 읽고 있어요",
          "original_form": "전 용의자X의 헌신 이라는 책 읽고 있어요",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100025378.1.8",
          "form": "아 책 제목 들어봤어요",
          "original_form": "아 책 제목 들어봤어요",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100025378.1.9",
          "form": "일본 소설이지요?",
          "original_form": "일본 소설이지요?",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100025378.1.10",
          "form": "네 맞아요 ㅎㅎ 재밌더라구요",
          "original_form": "네 맞아요 ㅎㅎ 재밌더라구요",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100025378.1.11",
          "form": "요즘 어떤 도서 읽으세요?",
          "original_form": "요즘 어떤 도서 읽으세요?",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100025378.1.12",
          "form": "저는 한강의 채식주의자 읽고 있어요",
          "original_form": "저는 한강의 채식주의자 읽고 있어요",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100025378.1.13",
          "form": "아 한강~ 유명하더라구요 그 작가분",
          "original_form": "아 한강~ 유명하더라구요 그 작가분",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100025378.1.14",
          "form": "무슨 상도 받았다던데",
          "original_form": "무슨 상도 받았다던데",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100025378.1.15",
          "form": "네 맞아요 베스트셀러에요",
          "original_form": "네 맞아요 베스트셀러에요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100025378.1.16",
          "form": "근데 책이 약간 난해한 부분도 있고",
          "original_form": "근데 책이 약간 난해한 부분도 있고",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100025378.1.17",
          "form": "어려운 부분도 많은거 같아요",
          "original_form": "어려운 부분도 많은거 같아요",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100025378.1.18",
          "form": "아 뭔지 알거 같아요ㅎ",
          "original_form": "아 뭔지 알거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100025378.1.19",
          "form": "한강 작가 작품이 대부분 그렇다고 들었어오",
          "original_form": "한강 작가 작품이 대부분 그렇다고 들었어오",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100025378.1.20",
          "form": "네 맞아요ㅎㅎ",
          "original_form": "네 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100025378.1.21",
          "form": "아 그리고 전 요즘 잡지도 보는 중이에요",
          "original_form": "아 그리고 전 요즘 잡지도 보는 중이에요",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100025378.1.22",
          "form": "어떤 잡지 보세요? 우와 저도 잡지 보는데",
          "original_form": "어떤 잡지 보세요? 우와 저도 잡지 보는데",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100025378.1.23",
          "form": "전 보그 구독해요~!",
          "original_form": "전 보그 구독해요~!",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100025378.1.24",
          "form": "헐 저도 보그 봐요",
          "original_form": "헐 저도 보그 봐요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100025378.1.25",
          "form": "대박 이렇게 겹치네요 보그가 제일 접근하기도 쉽고 내용도 볼만해서요",
          "original_form": "대박 이렇게 겹치네요 보그가 제일 접근하기도 쉽고 내용도 볼만해서요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100025378.1.26",
          "form": "맞아요 전 책도 좋은데 잡지도 매력이 있어서 즐겨봐요",
          "original_form": "맞아요 전 책도 좋은데 잡지도 매력이 있어서 즐겨봐요",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100025378.1.27",
          "form": "잡지는 표지 보는 재미도 있고 여러 내용을 골고루 다양히 본다는 장점이 있는 거 같아요ㅎ",
          "original_form": "잡지는 표지 보는 재미도 있고 여러 내용을 골고루 다양히 본다는 장점이 있는 거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:05"
        }
      ]
    }
