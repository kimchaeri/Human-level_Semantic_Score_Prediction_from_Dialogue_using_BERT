      "utterance": [
        {
          "id": "MDRW2100025562.1.1",
          "form": "배달 음식 고르는게 고민이에요",
          "original_form": "배달 음식 고르는게 고민이에요",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025562.1.2",
          "form": "맞아요 맨날 머먹을지 정하는거부터가 ㅠㅠ",
          "original_form": "맞아요 맨날 머먹을지 정하는거부터가 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025562.1.3",
          "form": "그래서 맨날 같은거만 시켜요",
          "original_form": "그래서 맨날 같은거만 시켜요",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100025562.1.4",
          "form": "익숙한게 좋긴하죠!!!",
          "original_form": "익숙한게 좋긴하죠!!!",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025562.1.5",
          "form": "저는 그날그날땡기는걸로 시키긴해요!",
          "original_form": "저는 그날그날땡기는걸로 시키긴해요!",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025562.1.6",
          "form": "순위 애들 안시키니",
          "original_form": "순위 애들 안시키니",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025562.1.7",
          "form": "맨날 실패해요",
          "original_form": "맨날 실패해요",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025562.1.8",
          "form": "맛없어요 ㅜㅜ",
          "original_form": "맛없어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025562.1.9",
          "form": "아 맞아요 잘못 걸리면 최악이죠 ㅠ",
          "original_form": "아 맞아요 잘못 걸리면 최악이죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025562.1.10",
          "form": "그런집은 리뷰도 안냄겨요",
          "original_form": "그런집은 리뷰도 안냄겨요",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025562.1.11",
          "form": "리뷰이벤트 받아도요??",
          "original_form": "리뷰이벤트 받아도요??",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025562.1.12",
          "form": "다신안먹을꺼면 그래도 되긴하죠!!ㄴㅋㅋ",
          "original_form": "다신안먹을꺼면 그래도 되긴하죠!!ㄴㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025562.1.13",
          "form": "그럼 그짓말로 맛있다고 써주긴해요 ㅜㅜㅋ",
          "original_form": "그럼 그짓말로 맛있다고 써주긴해요 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100025562.1.14",
          "form": "화나지만",
          "original_form": "화나지만",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025562.1.15",
          "form": "팥빙수 시켜먹고싶네요",
          "original_form": "팥빙수 시켜먹고싶네요",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025562.1.16",
          "form": "맛있겠어요...",
          "original_form": "맛있겠어요...",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025562.1.17",
          "form": "저도아스크림이 땡기네요 ㅠ",
          "original_form": "저도아스크림이 땡기네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025562.1.18",
          "form": "어떤 아이스크림 좋아하시는데요?",
          "original_form": "어떤 아이스크림 좋아하시는데요?",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025562.1.19",
          "form": "붕어 싸만코나 민트 좋아해요",
          "original_form": "붕어 싸만코나 민트 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025562.1.20",
          "form": "저는 호두 아이스크림 ㅎㅎ",
          "original_form": "저는 호두 아이스크림 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025562.1.21",
          "form": "오늘 저녁 메뉴는 뭔가욤",
