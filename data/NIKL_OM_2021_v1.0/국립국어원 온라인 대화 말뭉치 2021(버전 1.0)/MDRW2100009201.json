      "utterance": [
        {
          "id": "MDRW2100009201.1.1",
          "form": "그... 그 자리에서요? :)",
          "original_form": "그... 그 자리에서요? :)",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009201.1.2",
          "form": "그럴수있죠ㅋㅋㅋ\r\n멈출수 없는 맛",
          "original_form": "그럴수있죠ㅋㅋㅋ\r\n멈출수 없는 맛{emoji:♡}",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009201.1.3",
          "form": "커피랑 같이 먹으니까",
          "original_form": "커피랑 같이 먹으니까",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009201.1.4",
          "form": "그냥 술술 넘어갔어요ㅜㅜ",
          "original_form": "그냥 술술 넘어갔어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009201.1.5",
          "form": "거기 아메리카노 맛집인가요? ㅋㅋ",
          "original_form": "거기 아메리카노 맛집인가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009201.1.6",
          "form": "아님 아메랑 마카롱의 조화가\r\n원래 그렇게 위험한건가요?",
          "original_form": "아님 아메랑 마카롱의 조화가\r\n원래 그렇게 위험한건가요?",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009201.1.7",
          "form": "그냥 제식욕이넘쳤던거같아요 ㅋㅋ",
          "original_form": "그냥 제식욕이넘쳤던거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009201.1.8",
          "form": "그러고 비좀 나아져서",
          "original_form": "그러고 비좀 나아져서",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009201.1.9",
          "form": "여전히 많이 왔지만ㅈ",
          "original_form": "여전히 많이 왔지만ㅈ",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009201.1.10",
          "form": "왠지 부끄러워져서 후다닥나옴ㅜ",
          "original_form": "왠지 부끄러워져서 후다닥나옴ㅜ",
          "speaker_id": "1",
          "time": "20210822 22:43"
        },
        {
          "id": "MDRW2100009201.1.11",
          "form": "술술 드시는거 보면서\r\n사장님 엄청 뿌듯하셨을거 같은데용",
          "original_form": "술술 드시는거 보면서\r\n사장님 엄청 뿌듯하셨을거 같은데용",
          "speaker_id": "2",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009201.1.12",
          "form": "그렇게 생각해주심 다행이지만",
          "original_form": "그렇게 생각해주심 다행이지만",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009201.1.13",
          "form": "근데 웃긴건",
          "original_form": "근데 웃긴건",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009201.1.14",
          "form": "왜 배가 안불렀을까요 ㅋㅋ",
          "original_form": "왜 배가 안불렀을까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009201.1.15",
          "form": "마카롱은 배부르려고 먹는게 아니라서?ㅋㅋㅈ",
          "original_form": "마카롱은 배부르려고 먹는게 아니라서?ㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009201.1.16",
          "form": "먹고나서 현타와서 그걸 하고 왔어요 ㅋㅋ",
          "original_form": "먹고나서 현타와서 그걸 하고 왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009201.1.17",
          "form": "뭔지 물어봐 주세욥 ㅋㅋㅋ",
          "original_form": "뭔지 물어봐 주세욥 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009201.1.18",
          "form": "ㅋㅋㅋㅋㅋ뭐하고 오셨어요",
          "original_form": "ㅋㅋㅋㅋㅋ뭐하고 오셨어요",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009201.1.19",
          "form": "찐 궁금해지네요 ㅋㅋㅈ",
          "original_form": "찐 궁금해지네요 ㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009201.1.20",
          "form": "헌혈요 ㅋㅋ",
          "original_form": "헌혈요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009201.1.21",
          "form": "저도 제가웃김 ㅋㅋㅋ",
          "original_form": "저도 제가웃김 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009201.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이욜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이욜",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009201.1.23",
          "form": "이건 어디 컬투쇼 같은데\r\n사연 보내셔야 되는거 아닌가요? ㅋㅋ",
          "original_form": "이건 어디 컬투쇼 같은데\r\n사연 보내셔야 되는거 아닌가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009201.1.24",
          "form": "전 영양보충을 한거에요",
          "original_form": "전 영양보충을 한거에요",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009201.1.25",
          "form": "헌혈을 하기위해",
          "original_form": "헌혈을 하기위해",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009201.1.26",
          "form": "에네르기파를 모은거죠 ㅋㅋㅋㅋㅋ",
          "original_form": "에네르기파를 모은거죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009201.1.27",
          "form": "완전 큰그림이었는데요?ㅋㅋ",
          "original_form": "완전 큰그림이었는데요?ㅋㅋ",
