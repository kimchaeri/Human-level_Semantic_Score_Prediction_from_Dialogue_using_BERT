      "utterance": [
        {
          "id": "MDRW2100020171.1.1",
          "form": "안녕하세요! 점심 드셨나요?!?",
          "original_form": "안녕하세요! 점심 드셨나요?!?",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100020171.1.2",
          "form": "네! 바빠서 컵라면 먹었네요",
          "original_form": "네! 바빠서 컵라면 먹었네요",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100020171.1.3",
          "form": "점심드셨어요?",
          "original_form": "점심드셨어요?",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100020171.1.4",
          "form": "아니요 아직요!",
          "original_form": "아니요 아직요!",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100020171.1.5",
          "form": "어떤 컵라면 드셨어요?!",
          "original_form": "어떤 컵라면 드셨어요?!",
          "speaker_id": "2",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100020171.1.6",
          "form": "저는 육계장 작은컵이요!",
          "original_form": "저는 육계장 작은컵이요!",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100020171.1.7",
          "form": "컵라면 자주드세요?",
          "original_form": "컵라면 자주드세요?",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100020171.1.8",
          "form": "아 역시 컵라면은 육개장이죠ㅋㅋ",
          "original_form": "아 역시 컵라면은 육개장이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100020171.1.9",
          "form": "아니요ㅋㅋㅋ 라면을 그렇게 자주 먹진 않아요",
          "original_form": "아니요ㅋㅋㅋ 라면을 그렇게 자주 먹진 않아요",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100020171.1.10",
          "form": "가끔 먹어야 맛있어요!",
          "original_form": "가끔 먹어야 맛있어요!",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100020171.1.11",
          "form": "그렇긴하죠!",
          "original_form": "그렇긴하죠!",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100020171.1.12",
          "form": "점심 뭐드실 예정이세요?",
          "original_form": "점심 뭐드실 예정이세요?",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100020171.1.13",
          "form": "저두 귀찮아서 씨리얼 말아먹었어요ㅋㅋ",
          "original_form": "저두 귀찮아서 씨리얼 말아먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100020171.1.14",
          "form": "씨리얼 저는 초코쪽이 좋던데",
          "original_form": "씨리얼 저는 초코쪽이 좋던데",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100020171.1.15",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100020171.1.16",
          "form": "저는 초코도 좋고 기본 콘푸로스트도 좋아요!!",
          "original_form": "저는 초코도 좋고 기본 콘푸로스트도 좋아요!!",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100020171.1.17",
          "form": "코코볼도 자주 사먹어요ㅋㅋㅋ",
          "original_form": "코코볼도 자주 사먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100020171.1.18",
          "form": "코코볼 좋죠ㅋㅋㅋㅋ",
          "original_form": "코코볼 좋죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100020171.1.19",
          "form": "시리얼은 역시조금 달아야",
          "original_form": "시리얼은 역시조금 달아야",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100020171.1.20",
          "form": "맛있는거 같아요",
          "original_form": "맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100020171.1.21",
          "form": "맞아요ㅋㅋㅋ 우유 없어도 간식으로 먹기도 좋구요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ 우유 없어도 간식으로 먹기도 좋구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100020171.1.22",
          "form": "요플레랑 먹기도 좋구요",
          "original_form": "요플레랑 먹기도 좋구요",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100020171.1.23",
          "form": "아주 만능이예요",
          "original_form": "아주 만능이예요",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100020171.1.24",
          "form": "맞아요ㅋㅋㅋ 다른분한테 들었는데 크로와상 안에 요거트 과일이랑 시리얼 넣고 먹으면 맛있대요!!",
          "original_form": "맞아요ㅋㅋㅋ 다른분한테 들었는데 크로와상 안에 요거트 과일이랑 시리얼 넣고 먹으면 맛있대요!!",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100020171.1.25",
          "form": "빵이랑 시리얼 동시에 먹는거 생각해본적 없는데 너무 맛있을거같지 않나요?ㅋㅋ",
          "original_form": "빵이랑 시리얼 동시에 먹는거 생각해본적 없는데 너무 맛있을거같지 않나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100020171.1.26",
          "form": "대박 맛잘알이시다...",
