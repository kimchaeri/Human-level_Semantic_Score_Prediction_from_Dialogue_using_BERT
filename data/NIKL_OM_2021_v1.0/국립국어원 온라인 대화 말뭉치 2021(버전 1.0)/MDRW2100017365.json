        },
        {
          "id": "MDRW2100017365.1.2",
          "form": "전 파워집수니라서 해본적없어요",
          "original_form": "전 파워집수니라서 해본적없어요",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017365.1.3",
          "form": "저ㄴ도 상품권땜에",
          "original_form": "저ㄴ도 상품권땜에",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017365.1.4",
          "form": "회사동호회 가입했어요 ㅋㅋ",
          "original_form": "회사동호회 가입했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017365.1.5",
          "form": "헉ㅋㅋ 회사동호회",
          "original_form": "헉ㅋㅋ 회사동호회",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017365.1.6",
          "form": "등산동호회인데 한번도 안가봤어요",
          "original_form": "등산동호회인데 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017365.1.7",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017365.1.8",
          "form": "저는 모임 싫어해서 코시국에 회식없어서 너무 좋아요ㅜㅜ",
          "original_form": "저는 모임 싫어해서 코시국에 회식없어서 너무 좋아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017365.1.9",
          "form": "헐 회식 없는건 신의한수에요 너무 좋아요",
          "original_form": "헐 회식 없는건 신의한수에요 너무 좋아요",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017365.1.10",
          "form": "회식 영원히 없어라ㅠㅠ",
          "original_form": "회식 영원히 없어라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017365.1.11",
          "form": "그쳐ㅠㅠ 최고",
          "original_form": "그쳐ㅠㅠ 최고",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017365.1.12",
          "form": "다음에 하자는데 영원히 안하고싶어요",
          "original_form": "다음에 하자는데 영원히 안하고싶어요",
