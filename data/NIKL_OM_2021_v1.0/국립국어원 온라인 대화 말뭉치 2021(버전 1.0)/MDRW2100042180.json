      "utterance": [
        {
          "id": "MDRW2100042180.1.1",
          "form": "어제 텀블벅 오랜만에 구경하는데",
          "original_form": "어제 텀블벅 오랜만에 구경하는데",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042180.1.2",
          "form": "통장 없어질 뻔 했다...",
          "original_form": "통장 없어질 뻔 했다...",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042180.1.3",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042180.1.4",
          "form": "텀블벅... 아무래도 그런 편이지",
          "original_form": "텀블벅... 아무래도 그런 편이지",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042180.1.5",
          "form": "mz세대 잘알",
          "original_form": "mz세대 잘알",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042180.1.6",
          "form": "고양이 오르골 봄?",
          "original_form": "고양이 오르골 봄?",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042180.1.7",
          "form": "님이 탐라에 가져와서 봄",
          "original_form": "님이 탐라에 가져와서 봄",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042180.1.8",
          "form": "왕귀여움",
          "original_form": "왕귀여움",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042180.1.9",
          "form": "노래도 지브리 음악 들어가서",
          "original_form": "노래도 지브리 음악 들어가서",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042180.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042180.1.11",
          "form": "어제 밤에 구경하다가 눈물흘림",
          "original_form": "어제 밤에 구경하다가 눈물흘림",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.12",
          "form": "커마 엄청 다양하더라",
          "original_form": "커마 엄청 다양하더라",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.13",
          "form": "고영이들... 행복해...",
          "original_form": "고영이들... 행복해...",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.14",
          "form": "귀여움...",
          "original_form": "귀여움...",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.15",
          "form": "하... 고영좋아",
          "original_form": "하... 고영좋아",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.16",
          "form": "살까 말까 고민했는데",
          "original_form": "살까 말까 고민했는데",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.17",
          "form": "참았다...",
          "original_form": "참았다...",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042180.1.18",
          "form": "고여이...",
          "original_form": "고여이...",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.19",
          "form": "텀블벅...",
          "original_form": "텀블벅...",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.20",
          "form": "구경하면 정말 재밌는데",
          "original_form": "구경하면 정말 재밌는데",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.21",
          "form": "통장은 안 재밌음",
          "original_form": "통장은 안 재밌음",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.22",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.23",
          "form": "솔직히 살 거 없을 줄 알고",
          "original_form": "솔직히 살 거 없을 줄 알고",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.24",
          "form": "대충 쭉 내리고 있었는데",
          "original_form": "대충 쭉 내리고 있었는데",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.25",
          "form": "준비되지 않은 집사에게 훌쩍 다가온 어쩌고...",
          "original_form": "준비되지 않은 집사에게 훌쩍 다가온 어쩌고...",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042180.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042180.1.27",
          "form": "근데진짜...",
          "original_form": "근데진짜...",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042180.1.28",
          "form": "귀여웠어",
          "original_form": "귀여웠어",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042180.1.29",
          "form": "ㄹㅇ...",
          "original_form": "ㄹㅇ...",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042180.1.30",
          "form": "님네고영 커마해보고싶었음",
          "original_form": "님네고영 커마해보고싶었음",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042180.1.31",
          "form": "가격도... 그정도면",
          "original_form": "가격도... 그정도면",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042180.1.32",
          "form": "제법 혹한",
          "original_form": "제법 혹한",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042180.1.33",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042180.1.34",
          "form": "#가시나요",
          "original_form": "#가시나요",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042180.1.35",
          "form": "...그래도 그러기엔 큰 돈이라",
          "original_form": "...그래도 그러기엔 큰 돈이라",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042180.1.36",
          "form": "재고 남아있긴 한가 어제 봤을 때 별로 안 남았던데",
          "original_form": "재고 남아있긴 한가 어제 봤을 때 별로 안 남았던데",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042180.1.37",
          "form": "그렇군요...",
          "original_form": "그렇군요...",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042180.1.38",
          "form": "오만원짜리는 품절이네",
          "original_form": "오만원짜리는 품절이네",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042180.1.39",
