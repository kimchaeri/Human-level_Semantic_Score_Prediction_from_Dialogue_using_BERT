      "utterance": [
        {
          "id": "MDRW2100040821.1.1",
          "form": "아앗 서로 다른 데서 배달음식 시키시는거?ㅎㅎ",
          "original_form": "아앗 서로 다른 데서 배달음식 시키시는거?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040821.1.2",
          "form": "넵 ㅋㅋㅋ 아님",
          "original_form": "넵 ㅋㅋㅋ 아님",
          "speaker_id": "2",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040821.1.3",
          "form": "요샌 나가서 한잔못하니",
          "original_form": "요샌 나가서 한잔못하니",
          "speaker_id": "2",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040821.1.4",
          "form": "집에서 1차로 시켜먹고",
          "original_form": "집에서 1차로 시켜먹고",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040821.1.5",
          "form": "2차로 시켜먹고...ㅠㅠ",
          "original_form": "2차로 시켜먹고...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040821.1.6",
          "form": "아 배달음식으로 1차 2차ㅎㅎ",
          "original_form": "아 배달음식으로 1차 2차ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040821.1.7",
          "form": "둘다 음주도 좋아해서ㅠㅠ",
          "original_form": "둘다 음주도 좋아해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040821.1.8",
          "form": "요즘 근데 정말 웬만한 음식은 다 배달되더라고요",
          "original_form": "요즘 근데 정말 웬만한 음식은 다 배달되더라고요",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040821.1.9",
          "form": "맞아요ㅠ 좋아하는 꼬치집이있는데 좀오래걸려서 ㅋㅋ 미리시켜놓으면",
          "original_form": "맞아요ㅠ 좋아하는 꼬치집이있는데 좀오래걸려서 ㅋㅋ 미리시켜놓으면",
          "speaker_id": "2",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040821.1.10",
          "form": "2차에..딱이에요ㅠㅠ하하ㅏㅠㅠ",
          "original_form": "2차에..딱이에요ㅠㅠ하하ㅏㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040821.1.11",
          "form": "오 꼬치집!",
          "original_form": "오 꼬치집!",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040821.1.12",
          "form": "어떤 꼬치 좋아하세요?ㅎㅎ",
          "original_form": "어떤 꼬치 좋아하세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040821.1.13",
          "form": "그냥 주방장 맘대로 랜덤꼬치시켜요 ㅎ",
          "original_form": "그냥 주방장 맘대로 랜덤꼬치시켜요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100040821.1.14",
          "form": "오호 그렇군요",
          "original_form": "오호 그렇군요",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040821.1.15",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100040821.1.16",
          "form": "네~ 동네에서 가장좋아하는집이에요 ㅎㅎ",
          "original_form": "네~ 동네에서 가장좋아하는집이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100040821.1.17",
          "form": "요즘 맛집들도 배달이나 택배서비스 하는 건 좋더라고요",
          "original_form": "요즘 맛집들도 배달이나 택배서비스 하는 건 좋더라고요",
          "speaker_id": "1",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040821.1.18",
          "form": "맞아요~ 전 지방이지만 ㅠㅠ 서울경기권은 어플에서도 더 다양하고!",
          "original_form": "맞아요~ 전 지방이지만 ㅠㅠ 서울경기권은 어플에서도 더 다양하고!",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100040821.1.19",
          "form": "네 배달음식 어플에 진짜 다양한 맛집들이 있어서",
          "original_form": "네 배달음식 어플에 진짜 다양한 맛집들이 있어서",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040821.1.20",
          "form": "안시키고 구경만 해도 재밌어요",
          "original_form": "안시키고 구경만 해도 재밌어요",
          "speaker_id": "1",
          "time": "20210905 09:06"
        }
      ]
