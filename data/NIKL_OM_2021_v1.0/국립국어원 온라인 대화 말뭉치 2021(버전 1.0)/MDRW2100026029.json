      "utterance": [
        {
          "id": "MDRW2100026029.1.1",
          "form": "안녕하세요  좋아하는 음식이 뭐에요??",
          "original_form": "안녕하세요  좋아하는 음식이 뭐에요??",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100026029.1.2",
          "form": "안녕하세요 저는 고기요!!!",
          "original_form": "안녕하세요 저는 고기요!!!",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100026029.1.3",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100026029.1.4",
          "form": "저도 고기 좋아해요 스테이크도 삼겹살도 치킨도 좋아요",
          "original_form": "저도 고기 좋아해요 스테이크도 삼겹살도 치킨도 좋아요",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100026029.1.5",
          "form": "고기는 늘 옳죠",
          "original_form": "고기는 늘 옳죠",
          "speaker_id": "2",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100026029.1.6",
          "form": "배달 음식으로 늘 함께해요",
          "original_form": "배달 음식으로 늘 함께해요",
          "speaker_id": "1",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100026029.1.7",
          "form": "배달 좋죠",
          "original_form": "배달 좋죠",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100026029.1.8",
          "form": "진짜 배달없음 살수가 없어요",
          "original_form": "진짜 배달없음 살수가 없어요",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100026029.1.9",
          "form": "맞아요 커피도 배달시키기 시작하니까 거의 매일 먹네요",
          "original_form": "맞아요 커피도 배달시키기 시작하니까 거의 매일 먹네요",
          "speaker_id": "1",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100026029.1.10",
          "form": "커피 배달 진짜 넘 편해요!!",
          "original_form": "커피 배달 진짜 넘 편해요!!",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100026029.1.11",
          "form": "저녁으로 치킨 배달 시키려구요",
          "original_form": "저녁으로 치킨 배달 시키려구요",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026029.1.12",
          "form": "헐 치킨...",
          "original_form": "헐 치킨...",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026029.1.13",
          "form": "무슨 치킨 좋아하세요?",
          "original_form": "무슨 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026029.1.14",
          "form": "굽네 고추바사삭 제일 좋아해요",
          "original_form": "굽네 고추바사삭 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026029.1.15",
          "form": "넘 맛있어요",
          "original_form": "넘 맛있어요",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026029.1.16",
          "form": "전 맵찔이라ㅠㅠㅠ",
          "original_form": "전 맵찔이라ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100026029.1.17",
          "form": "매운 거 좋아하세요?",
          "original_form": "매운 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100026029.1.18",
          "form": "아니요 저도 맵찔이라 요즘 떡볶이도 못먹어요",
          "original_form": "아니요 저도 맵찔이라 요즘 떡볶이도 못먹어요",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026029.1.19",
          "form": "안매운 떡볶이 어디 없을까요",
          "original_form": "안매운 떡볶이 어디 없을까요",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026029.1.20",
          "form": "앜ㅋㅋㅋㅋ 저두요...",
          "original_form": "앜ㅋㅋㅋㅋ 저두요...",
          "speaker_id": "2",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100026029.1.21",
          "form": "그래서 떡볶이는 잘 안 먹어요...",
          "original_form": "그래서 떡볶이는 잘 안 먹어요...",
          "speaker_id": "2",
          "time": "20210901 19:21"
        },
        {
          "id": "MDRW2100026029.1.22",
          "form": "맞아요...그래서 로제떡볶이가 유행해서 좀 행복해요",
          "original_form": "맞아요...그래서 로제떡볶이가 유행해서 좀 행복해요",
          "speaker_id": "1",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100026029.1.23",
          "form": "오 그러네요!! 로제가 있었죠",
          "original_form": "오 그러네요!! 로제가 있었죠",
