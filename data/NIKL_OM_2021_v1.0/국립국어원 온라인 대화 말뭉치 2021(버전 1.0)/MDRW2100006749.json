      "utterance": [
        {
          "id": "MDRW2100006749.1.1",
          "form": "오늘 무척 춥네요",
          "original_form": "오늘 무척 춥네요",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100006749.1.2",
          "form": "네 비는 안내리는 꿉꿉하네요",
          "original_form": "네 비는 안내리는 꿉꿉하네요",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006749.1.3",
          "form": "안내리는데ㅎㅎ",
          "original_form": "안내리는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006749.1.4",
          "form": "맞아요 해는 떴는데 습도는 높은듯해요",
          "original_form": "맞아요 해는 떴는데 습도는 높은듯해요",
          "speaker_id": "1",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100006749.1.5",
          "form": "맞아요. 낮엔 후덥지근할거 같네요",
          "original_form": "맞아요. 낮엔 후덥지근할거 같네요",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006749.1.6",
          "form": "가을날씨가 아닌거같죠??",
          "original_form": "가을날씨가 아닌거같죠??",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100006749.1.7",
          "form": "네ㅋㅋ 좀 더 선선해지거나 해야 할거 같은데요",
          "original_form": "네ㅋㅋ 좀 더 선선해지거나 해야 할거 같은데요",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006749.1.8",
          "form": "비가 좀 물러갔으면 싶네요",
          "original_form": "비가 좀 물러갔으면 싶네요",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100006749.1.9",
          "form": "가끔 오면 운치있는데 계속 오니까 지겹네요ㅋㅋ",
          "original_form": "가끔 오면 운치있는데 계속 오니까 지겹네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100006749.1.10",
          "form": "네 넘 지겨워요 몇주째인지..",
          "original_form": "네 넘 지겨워요 몇주째인지..",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006749.1.11",
          "form": "오늘은 그친다고 하니깐... 내일은 맑지 않을까요?",
          "original_form": "오늘은 그친다고 하니깐... 내일은 맑지 않을까요?",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100006749.1.12",
          "form": "여긴 오늘은 해가 뜨긴 했어요",
          "original_form": "여긴 오늘은 해가 뜨긴 했어요",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100006749.1.13",
          "form": "그래요? 여긴 구름 때문에 안보여요ㅋㅋ",
          "original_form": "그래요? 여긴 구름 때문에 안보여요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100006749.1.14",
          "form": "쨍했으면 좋겠어요 ㅎㅎ",
          "original_form": "쨍했으면 좋겠어요 ㅎㅎ",
          "speaker_id": "1",
