      "utterance": [
        {
          "id": "MDRW2100002449.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002449.1.2",
          "form": "키우고 있는 반려동물 있나요?",
          "original_form": "키우고 있는 반려동물 있나요?",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002449.1.3",
          "form": "저는 지금은 없어요^^",
          "original_form": "저는 지금은 없어요^^",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002449.1.4",
          "form": "저도 없어요ㅋㅋ",
          "original_form": "저도 없어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002449.1.5",
          "form": "책임질 자신이 없어서",
          "original_form": "책임질 자신이 없어서",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002449.1.6",
          "form": "강아지  키웠었는데\r\n사고로 죽고, 한 마리는\r\n아파서 안락사 했었는데\r\nㅠㅠ 그 이후로 못키우겠더라구요",
          "original_form": "강아지  키웠었는데\r\n사고로 죽고, 한 마리는\r\n아파서 안락사 했었는데\r\nㅠㅠ 그 이후로 못키우겠더라구요",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002449.1.7",
          "form": "아이구ㅜㅜ",
          "original_form": "아이구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002449.1.8",
          "form": "마음아프셨겠어요",
          "original_form": "마음아프셨겠어요",
          "speaker_id": "1",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002449.1.9",
          "form": "네ㅠㅠ 많이 아프고 힘들었쥬",
          "original_form": "네ㅠㅠ 많이 아프고 힘들었쥬",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002449.1.10",
          "form": "혹시 알러지 때문에\r\n못 키우신건 아니죠?",
          "original_form": "혹시 알러지 때문에\r\n못 키우신건 아니죠?",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002449.1.11",
          "form": "그건 아니고....금전적으로도 부담스럽고",
          "original_form": "그건 아니고....금전적으로도 부담스럽고",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002449.1.12",
          "form": "나중에 헤어지면",
          "original_form": "나중에 헤어지면",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002449.1.13",
          "form": "너무힘들거같아서",
          "original_form": "너무힘들거같아서",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002449.1.14",
          "form": "겁도 나고 그러더라구요...",
          "original_form": "겁도 나고 그러더라구요...",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002449.1.15",
          "form": "맞아요ㅠㅠ\r\n헤어짐은 언제나 아프고 힘들죠",
          "original_form": "맞아요ㅠㅠ\r\n헤어짐은 언제나 아프고 힘들죠",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002449.1.16",
          "form": "ㅜㅜ 예전에 길냥이를 좀 돌봐준적이 있는데",
          "original_form": "ㅜㅜ 예전에 길냥이를 좀 돌봐준적이 있는데",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002449.1.17",
          "form": "오 길냥이 ㅠㅠ",
          "original_form": "오 길냥이 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002449.1.18",
          "form": "그아이는 좋은집에 입양가서 헤어진건데도",
          "original_form": "그아이는 좋은집에 입양가서 헤어진건데도",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002449.1.19",
          "form": "아직도 걔 생각하면 넘 그립고 그래요",
          "original_form": "아직도 걔 생각하면 넘 그립고 그래요",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002449.1.20",
          "form": "진지하게 입양고민했었는데ㅜㅜ",
          "original_form": "진지하게 입양고민했었는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002449.1.21",
          "form": "도저히 엄두가 안나서...",
          "original_form": "도저히 엄두가 안나서...",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002449.1.22",
          "form": "에고ㅠㅠ 민초님 마음이 넘 좋으시네요",
          "original_form": "에고ㅠㅠ 민초님 마음이 넘 좋으시네요",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002449.1.23",
          "form": "다들 이런 마음으로 반려동물을\r\n들였으면 좋겠어요ㅠ",
          "original_form": "다들 이런 마음으로 반려동물을\r\n들였으면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002449.1.24",
          "form": "충분히 생각하고 고민하고 쫌 ㅠ",
          "original_form": "충분히 생각하고 고민하고 쫌 ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002449.1.25",
          "form": "ㅠ 돈 여유가 있었으면 입양했을텐데",
          "original_form": "ㅠ 돈 여유가 있었으면 입양했을텐데",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002449.1.26",
          "form": "아프면 돈이 많이 들 수도 있대서...",
          "original_form": "아프면 돈이 많이 들 수도 있대서...",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002449.1.27",
          "form": "제 코가 석자라...",
          "original_form": "제 코가 석자라...",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002449.1.28",
          "form": "힝ㅠㅠ 그래도 좋은 곳으로 갔다니\r\n너무 마음 아파하지 마세요ㅠ",
          "original_form": "힝ㅠㅠ 그래도 좋은 곳으로 갔다니\r\n너무 마음 아파하지 마세요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:11"
        }
      ]
    }
