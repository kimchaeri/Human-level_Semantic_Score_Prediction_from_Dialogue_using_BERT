      "utterance": [
        {
          "id": "MDRW2100011916.1.1",
          "form": "난 나중에 맛칼럼리스트하고 싶어",
          "original_form": "난 나중에 맛칼럼리스트하고 싶어",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011916.1.2",
          "form": "그거 딱 맞을듯",
          "original_form": "그거 딱 맞을듯",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011916.1.3",
          "form": "솔직히 천직이라고 본다",
          "original_form": "솔직히 천직이라고 본다",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011916.1.4",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011916.1.5",
          "form": "나만의 조합을 소개하고 싶어",
          "original_form": "나만의 조합을 소개하고 싶어",
          "speaker_id": "2",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011916.1.6",
          "form": "블로그 같은거 운영해",
          "original_form": "블로그 같은거 운영해",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011916.1.7",
          "form": "나중에 브런치를 해볼려고",
          "original_form": "나중에 브런치를 해볼려고",
          "speaker_id": "2",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011916.1.8",
          "form": "브런치 만드는거?",
          "original_form": "브런치 만드는거?",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011916.1.9",
          "form": "아니 브런치라고 연재 플랫폼이 있어",
          "original_form": "아니 브런치라고 연재 플랫폼이 있어",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011916.1.10",
          "form": "아 그거 좋은거 같아",
          "original_form": "아 그거 좋은거 같아",
          "speaker_id": "1",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011916.1.11",
          "form": "들어 본 적 있어?",
          "original_form": "들어 본 적 있어?",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011916.1.12",
          "form": "아니 없어",
          "original_form": "아니 없어",
          "speaker_id": "1",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011916.1.13",
