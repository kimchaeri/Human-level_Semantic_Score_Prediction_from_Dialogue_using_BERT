      "utterance": [
        {
          "id": "MDRW2100039584.1.1",
          "form": "단종되었나? 시중에 파는거 본적이 없음",
          "original_form": "단종되었나? 시중에 파는거 본적이 없음",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.3",
          "form": "ㅇㅇ그런거 같음",
          "original_form": "ㅇㅇ그런거 같음",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.4",
          "form": "아니 이럴수가",
          "original_form": "아니 이럴수가",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.5",
          "form": "칼피스맛 맛있었는디",
          "original_form": "칼피스맛 맛있었는디",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.6",
          "form": "일본가면 살 수 있을 줄 알았는데",
          "original_form": "일본가면 살 수 있을 줄 알았는데",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.7",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.8",
          "form": "아니 근데 그걸 기억하고 있어ㅋㅋㅋ",
          "original_form": "아니 근데 그걸 기억하고 있어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.9",
          "form": "포도 칼피스였나....",
          "original_form": "포도 칼피스였나....",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.10",
          "form": "존맛템....소중해....",
          "original_form": "존맛템....소중해....",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039584.1.11",
          "form": "아 님님",
          "original_form": "아 님님",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039584.1.12",
          "form": "올리브영 세일한대매요",
          "original_form": "올리브영 세일한대매요",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039584.1.13",
          "form": "올리브영 세일 봤으여",
          "original_form": "올리브영 세일 봤으여",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039584.1.14",
          "form": "님 살거 있??",
          "original_form": "님 살거 있??",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039584.1.15",
          "form": "님 바이린샵 알아? 여기 패밀리 세일 때리면 좋더라...",
          "original_form": "님 바이린샵 알아? 여기 패밀리 세일 때리면 좋더라...",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039584.1.16",
          "form": "헐 바이린샵은 어댜???",
          "original_form": "헐 바이린샵은 어댜???",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039584.1.17",
          "form": "올영.....아직 모르겠어",
          "original_form": "올영.....아직 모르겠어",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039584.1.18",
          "form": "오 린 컴퍼니??",
          "original_form": "오 린 컴퍼니??",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039584.1.19",
          "form": "그 케네스 레이디 있고 라인 브랜드 있는 통합브랜드샵?",
          "original_form": "그 케네스 레이디 있고 라인 브랜드 있는 통합브랜드샵?",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039584.1.20",
          "form": "님 거기 세일 하면 눈 돌아 갑니다",
          "original_form": "님 거기 세일 하면 눈 돌아 갑니다",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039584.1.21",
          "form": "내년 1월 말에 지릅시댜",
          "original_form": "내년 1월 말에 지릅시댜",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039584.1.22",
          "form": "님 나 냥냥이 봉사하고 올게오",
          "original_form": "님 나 냥냥이 봉사하고 올게오",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100039584.1.23",
          "form": "부업하고 있으세오..",
          "original_form": "부업하고 있으세오..",
          "speaker_id": "2",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039584.1.24",
          "form": "헐 빨리 돌아오세오 바이린샵 얘기하자ㅏㅏ",
          "original_form": "헐 빨리 돌아오세오 바이린샵 얘기하자ㅏㅏ",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039584.1.25",
          "form": "내년 1월에 바이린샵에서 뭐살거야??",
          "original_form": "내년 1월에 바이린샵에서 뭐살거야??",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039584.1.26",
          "form": "ㅎ ㅏ 저 왔슴니댜",
          "original_form": "ㅎ ㅏ 저 왔슴니댜",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039584.1.27",
          "form": "내년에 저 원피스랑 블라우스랑 아우터 사고 있습니다",
          "original_form": "내년에 저 원피스랑 블라우스랑 아우터 사고 있습니다",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100039584.1.28",
          "form": "오 아우터는 코트인가요??",
          "original_form": "오 아우터는 코트인가요??",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039584.1.29",
          "form": "고생많으셨어요ㅠㅇ0ㅇ",
          "original_form": "고생많으셨어요ㅠㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039584.1.30",
          "form": "코트는...아 이번에 산 사람들 보니깐 울100 40만원 까지 반값에 산사람도 있더라고",
          "original_form": "코트는...아 이번에 산 사람들 보니깐 울100 40만원 까지 반값에 산사람도 있더라고",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039584.1.31",
          "form": "와 가성비가",
          "original_form": "와 가성비가",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039584.1.32",
          "form": "갓성비 ㄷㄷㄷㄷ",
          "original_form": "갓성비 ㄷㄷㄷㄷ",
