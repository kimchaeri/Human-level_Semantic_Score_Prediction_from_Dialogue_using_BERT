        },
        {
          "id": "MDRW2100015605.1.6",
          "form": "마쟈여",
          "original_form": "마쟈여",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015605.1.7",
          "form": "ㅋㅋ다행이에요",
          "original_form": "ㅋㅋ다행이에요",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015605.1.8",
          "form": "sf좋아하세요?",
          "original_form": "sf좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015605.1.9",
          "form": "인셉션 맞죠??",
          "original_form": "인셉션 맞죠??",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015605.1.10",
          "form": "저 아직도 그거 이해못했어여",
          "original_form": "저 아직도 그거 이해못했어여",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015605.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015605.1.12",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015605.1.13",
          "form": "그거..팽이는 이해하셨어요",
          "original_form": "그거..팽이는 이해하셨어요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015605.1.14",
          "form": "꿈속의 꿈",
          "original_form": "꿈속의 꿈",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015605.1.15",
          "form": "넹!ㅋㅋㅋㅋㅋ",
          "original_form": "넹!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015605.1.16",
          "form": "그게 꿈에서 깼는데 꿈이고 그것도 꿈이고",
          "original_form": "그게 꿈에서 깼는데 꿈이고 그것도 꿈이고",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015605.1.17",
          "form": "꿈인지 알아낼라구 팽이 돌리는거 맞져",
          "original_form": "꿈인지 알아낼라구 팽이 돌리는거 맞져",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015605.1.18",
          "form": "맞아요 맞사",
          "original_form": "맞아요 맞사",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015605.1.19",
          "form": "그거아시면 됐네요.",
          "original_form": "그거아시면 됐네요.",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015605.1.20",
          "form": "근데 꿈중의  꿈은 꿈이 나인지 내가 꾸는게 꿈인지 헛갈리는데",
          "original_form": "근데 꿈중의  꿈은 꿈이 나인지 내가 꾸는게 꿈인지 헛갈리는데",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015605.1.21",
          "form": "팽이가 계속도니까 마지막은 꿈을 의미하는 긧 같아요",
          "original_form": "팽이가 계속도니까 마지막은 꿈을 의미하는 긧 같아요",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015605.1.22",
          "form": "오홍 잘아신당 전 가상세계하면",
          "original_form": "오홍 잘아신당 전 가상세계하면",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015605.1.23",
          "form": "에스파 생각나는데",
          "original_form": "에스파 생각나는데",
          "speaker_id": "2",
          "time": "20210826 00:10"
