      "utterance": [
        {
          "id": "MDRW2100015166.1.1",
          "form": "음 전 좀 확확 바뀌는 편인데",
          "original_form": "음 전 좀 확확 바뀌는 편인데",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015166.1.2",
          "form": "최근엔 와이드 연청에 꽂혀서",
          "original_form": "최근엔 와이드 연청에 꽂혀서",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015166.1.3",
          "form": "흰 반팔이나 크롭?",
          "original_form": "흰 반팔이나 크롭?",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015166.1.4",
          "form": "반팔 니트도 많이 입어요",
          "original_form": "반팔 니트도 많이 입어요",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015166.1.5",
          "form": "니트류를 좋아해가지구",
          "original_form": "니트류를 좋아해가지구",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015166.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015166.1.7",
          "form": "자신있으신가봐요",
          "original_form": "자신있으신가봐요",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015166.1.8",
          "form": "체형 별로면 입어도 태가 잘 안사는데",
          "original_form": "체형 별로면 입어도 태가 잘 안사는데",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015166.1.9",
          "form": "와이드청바지요???",
          "original_form": "와이드청바지요???",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015166.1.10",
          "form": "크롭티랑 반팔니트요",
          "original_form": "크롭티랑 반팔니트요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015166.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015166.1.12",
          "form": "니트는 모르겠지만",
          "original_form": "니트는 모르겠지만",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015166.1.13",
          "form": "크롭 입을 땐 배에 힘주고... 걸어야져..",
          "original_form": "크롭 입을 땐 배에 힘주고... 걸어야져..",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015166.1.14",
          "form": "죽을 것 같아요",
          "original_form": "죽을 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015166.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015166.1.16",
          "form": "ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015166.1.17",
          "form": "예전엔 저도 옷좀 힘주고 다닌다고",
          "original_form": "예전엔 저도 옷좀 힘주고 다닌다고",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015166.1.18",
          "form": "맨날 코트에 블루종 오버니트 엄청 사서",
          "original_form": "맨날 코트에 블루종 오버니트 엄청 사서",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015166.1.19",
          "form": "입고다녔는데",
          "original_form": "입고다녔는데",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015166.1.20",
          "form": "취준생되고 몸관리 안되니까",
          "original_form": "취준생되고 몸관리 안되니까",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015166.1.21",
          "form": "무조건 검정색ㅋㅋㅋㅋㅋ",
          "original_form": "무조건 검정색ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015166.1.22",
          "form": "무조건 트레이닝복",
          "original_form": "무조건 트레이닝복",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015166.1.23",
          "form": "맞아요 그리고 요즘 코로나때문에 ㅠㅠ",
          "original_form": "맞아요 그리고 요즘 코로나때문에 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015166.1.24",
          "form": "셔츠 예전꺼 입었는데 팔이 터졌어요ㅋㅋㅋㅋ",
          "original_form": "셔츠 예전꺼 입었는데 팔이 터졌어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015166.1.25",
          "form": "혹시 근육이 붙으신 건 아닐까 생각해봅니다",
          "original_form": "혹시 근육이 붙으신 건 아닐까 생각해봅니다",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015166.1.26",
          "form": "반팔니트도 이게 체형 별로인 사람이 입으면",
          "original_form": "반팔니트도 이게 체형 별로인 사람이 입으면",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015166.1.27",
          "form": "약간 뭐라해야되지",
          "original_form": "약간 뭐라해야되지",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015166.1.28",
          "form": "가방이 걸어다니는것같은느낌?",
          "original_form": "가방이 걸어다니는것같은느낌?",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015166.1.29",
          "form": "2년사이에 10키로가 쪄버렸어요...",
          "original_form": "2년사이에 10키로가 쪄버렸어요...",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015166.1.30",
          "form": "제가 바로 그 가방입니다",
          "original_form": "제가 바로 그 가방입니다",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015166.1.31",
          "form": "전 고3때만 7키로 쪘었는데..",
          "original_form": "전 고3때만 7키로 쪘었는데..",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015166.1.32",
          "form": "뭔가 살찌니깐 옷 사기가 싫어요",
          "original_form": "뭔가 살찌니깐 옷 사기가 싫어요",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015166.1.33",
          "form": "난 살 빼고 옷살건데",
          "original_form": "난 살 빼고 옷살건데",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015166.1.34",
          "form": "이건 진정한 내가 아닌데",
          "original_form": "이건 진정한 내가 아닌데",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015166.1.35",
          "form": "이러면서ㅋㅋㅋㅋ",
          "original_form": "이러면서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015166.1.36",
          "form": "저랑 똑같네요..",
          "original_form": "저랑 똑같네요..",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015166.1.37",
          "form": "지금 학교 졸업하셨어요??",
          "original_form": "지금 학교 졸업하셨어요??",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015166.1.38",
          "form": "네 작년에 졸업하고 취준하는척 놀먹하고있네요",
          "original_form": "네 작년에 졸업하고 취준하는척 놀먹하고있네요",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015166.1.39",
          "form": "전 곧 복학인데",
          "original_form": "전 곧 복학인데",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015166.1.40",
          "form": "속이 울렁거려요",
          "original_form": "속이 울렁거려요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015166.1.41",
          "form": "어이구",
          "original_form": "어이구",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015166.1.42",
          "form": "학교는 멀어요?",
          "original_form": "학교는 멀어요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015166.1.43",
          "form": "학교 좀 멀어요..ㅠㅜ",
          "original_form": "학교 좀 멀어요..ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015166.1.44",
          "form": "근데 비대면이라",
          "original_form": "근데 비대면이라",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015166.1.45",
          "form": "오 서울권이신가봐요",
          "original_form": "오 서울권이신가봐요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015166.1.46",
          "form": "학교 가기 싫기도 한데",
          "original_form": "학교 가기 싫기도 한데",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015166.1.47",
          "form": "가야될 것 같기도 하고",
          "original_form": "가야될 것 같기도 하고",
          "speaker_id": "2",
          "time": "20210825 22:26"
        }
      ]
    }
