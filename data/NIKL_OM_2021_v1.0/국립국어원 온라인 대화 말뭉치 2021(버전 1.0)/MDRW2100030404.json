        },
        {
          "id": "MDRW2100030404.1.2",
          "form": "육포에 치즈 얹어서 맥주 한잔 합니다!",
          "original_form": "육포에 치즈 얹어서 맥주 한잔 합니다!",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030404.1.3",
          "form": "야식메뉴는 정하셨나요",
          "original_form": "야식메뉴는 정하셨나요",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030404.1.4",
          "form": "거 술 안좋아한다고",
          "original_form": "거 술 안좋아한다고",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030404.1.5",
          "form": "하지 않으셨어요?",
          "original_form": "하지 않으셨어요?",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030404.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030404.1.7",
          "form": "소주 싫어합니다",
          "original_form": "소주 싫어합니다",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030404.1.8",
          "form": "맥주도 사실 맛 잘 몰라요",
          "original_form": "맥주도 사실 맛 잘 몰라요",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030404.1.9",
          "form": "하지만 육포엔 맥주잖아요?",
          "original_form": "하지만 육포엔 맥주잖아요?",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030404.1.10",
          "form": "우유에 식빵 먹는 것처럼",
          "original_form": "우유에 식빵 먹는 것처럼",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030404.1.11",
          "form": "우유에 콘푸라이트인 것처럼",
          "original_form": "우유에 콘푸라이트인 것처럼",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030404.1.12",
          "form": "그래서 뭐 드시기로 했나요",
          "original_form": "그래서 뭐 드시기로 했나요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030404.1.13",
          "form": "저는 그냥 잡니다 ㅎㅎ",
          "original_form": "저는 그냥 잡니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030404.1.14",
