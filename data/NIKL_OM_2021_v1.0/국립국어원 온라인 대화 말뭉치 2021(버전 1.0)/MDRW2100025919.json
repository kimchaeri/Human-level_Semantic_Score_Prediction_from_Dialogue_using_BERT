      "utterance": [
        {
          "id": "MDRW2100025919.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025919.1.2",
          "form": "무슨 음료 좋아하세요?",
          "original_form": "무슨 음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025919.1.3",
          "form": "역시 아메리카노죠! name1님은요?",
          "original_form": "역시 아메리카노죠! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025919.1.4",
          "form": "저도 아메리카노 좋아요! 그래도 피지오가 더 존맛인거같아요!",
          "original_form": "저도 아메리카노 좋아요! 그래도 피지오가 더 존맛인거같아요!",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025919.1.5",
          "form": "피지오가 뭐에요??",
          "original_form": "피지오가 뭐에요??",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025919.1.6",
          "form": "스타벅스 피지오요",
          "original_form": "스타벅스 피지오요",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025919.1.7",
          "form": "에이드...?",
          "original_form": "에이드...?",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025919.1.8",
          "form": "앜ㅋㅋㅋ그건 안먹어봤어요",
          "original_form": "앜ㅋㅋㅋ그건 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025919.1.9",
          "form": "스벅은 가면 자허블만 먹어섴ㅋㅋㅋ",
          "original_form": "스벅은 가면 자허블만 먹어섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025919.1.10",
          "form": "아 ㅋㅋㅋ 자몽 ㅋㅋㅋㅋ 저는 그걸 안먹어봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ 자몽 ㅋㅋㅋㅋ 저는 그걸 안먹어봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025919.1.11",
          "form": "다음에  피지오 먹어봐요!",
          "original_form": "다음에  피지오 먹어봐요!",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025919.1.12",
          "form": "저는 패션후르츠 피지오 먹어요!",
          "original_form": "저는 패션후르츠 피지오 먹어요!",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025919.1.13",
          "form": "탱고티..",
          "original_form": "탱고티..",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025919.1.14",
          "form": "오 추천 감사해요 ㅋㅋㅋ 패션후르츠면 진짜 맛 보장",
          "original_form": "오 추천 감사해요 ㅋㅋㅋ 패션후르츠면 진짜 맛 보장",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025919.1.15",
          "form": "그쵸오 ㅎㅎ",
          "original_form": "그쵸오 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:06"
        },
        {
          "id": "MDRW2100025919.1.16",
          "form": "자허블도드셔보세요",
          "original_form": "자허블도드셔보세요",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100025919.1.17",
          "form": "네 ㅎㅎㅎ 저도 먹어볼께요 ㅎㅎ",
          "original_form": "네 ㅎㅎㅎ 저도 먹어볼께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100025919.1.18",
          "form": "아메리카노는 어느 브랜드 좋아하세요?",
          "original_form": "아메리카노는 어느 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100025919.1.19",
          "form": "네스프레소요! 사실 별로 안가려요 개인카페아니면",
          "original_form": "네스프레소요! 사실 별로 안가려요 개인카페아니면",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025919.1.20",
          "form": "아항",
