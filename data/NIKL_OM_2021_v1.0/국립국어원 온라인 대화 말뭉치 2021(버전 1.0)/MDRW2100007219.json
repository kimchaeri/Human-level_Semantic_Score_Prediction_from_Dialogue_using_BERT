      "utterance": [
        {
          "id": "MDRW2100007219.1.1",
          "form": "안녕하세요 쿼카 좋아하세요??",
          "original_form": "안녕하세요 쿼카 좋아하세요??",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007219.1.2",
          "form": "안녕하세요 ! 쿼카 귀여워서 좋아해요",
          "original_form": "안녕하세요 ! 쿼카 귀여워서 좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007219.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007219.1.4",
          "form": "네! 저는 고양이 키우고있어요 쿼카도 좋아해요ㅋㅋ",
          "original_form": "네! 저는 고양이 키우고있어요 쿼카도 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007219.1.5",
          "form": "고양이 너무 귀엽죠",
          "original_form": "고양이 너무 귀엽죠",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007219.1.6",
          "form": "고양이 이름이 뭐예요?",
          "original_form": "고양이 이름이 뭐예요?",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007219.1.7",
          "form": "name3예요ㅎㅎ 치즈태비라서ㅋㅋ",
          "original_form": "&name3&예요ㅎㅎ 치즈태비라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007219.1.8",
          "form": "이름 너무 귀엽네요ㅋㅋ",
          "original_form": "이름 너무 귀엽네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007219.1.9",
          "form": "어떤 동물 제일 좋아하세요? 반려동물 키우시나요?!",
          "original_form": "어떤 동물 제일 좋아하세요? 반려동물 키우시나요?!",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007219.1.10",
          "form": "저는 예전에 햄스터를 키웠어요 !",
          "original_form": "저는 예전에 햄스터를 키웠어요 !",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007219.1.11",
          "form": "지금은 여건이 안되서 다른 동물은 못키우고 있어요ㅠㅠ",
          "original_form": "지금은 여건이 안되서 다른 동물은 못키우고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007219.1.12",
          "form": "헉 저도 햄스터 키운 적 있어요!",
          "original_form": "헉 저도 햄스터 키운 적 있어요!",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007219.1.13",
          "form": "펄햄스터였어요",
          "original_form": "펄햄스터였어요",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007219.1.14",
          "form": "저도 펄햄스터였는데 이름은 name4였어요 !",
          "original_form": "저도 펄햄스터였는데 이름은 &name4&였어요 !",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007219.1.15",
          "form": "펄햄스터 너무 귀엽지 않나요 ㅠㅠ",
          "original_form": "펄햄스터 너무 귀엽지 않나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007219.1.16",
          "form": "헉헉 그렇군요ㅠㅠ 네 하얗고...ㅋㅋㅋ",
          "original_form": "헉헉 그렇군요ㅠㅠ 네 하얗고...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007219.1.17",
          "form": "저희애는 이름 name5였어요!",
          "original_form": "저희애는 이름 &name5&였어요!",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007219.1.18",
          "form": "name5도 엄청 귀여웠을 것 같아요 !",
          "original_form": "&name5&도 엄청 귀여웠을 것 같아요 !",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007219.1.19",
          "form": "name5도 해바라기씨를 좋아했나요?",
          "original_form": "&name5&도 해바라기씨를 좋아했나요?",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007219.1.20",
          "form": "헉 네ㅋㅋㅋ햄스터들 해바라기씨를 제일 좋아하나봐요 역시",
          "original_form": "헉 네ㅋㅋㅋ햄스터들 해바라기씨를 제일 좋아하나봐요 역시",
