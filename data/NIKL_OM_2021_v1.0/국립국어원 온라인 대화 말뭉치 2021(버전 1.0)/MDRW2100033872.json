      "utterance": [
        {
          "id": "MDRW2100033872.1.1",
          "form": "장기투자를 차라리 하고싶은데 보는 눈이 없어서",
          "original_form": "장기투자를 차라리 하고싶은데 보는 눈이 없어서",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100033872.1.2",
          "form": "그것도 힘들고..",
          "original_form": "그것도 힘들고..",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100033872.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100033872.1.4",
          "form": "안됩니다 단기로하고 소소하게 먹읍시다",
          "original_form": "안됩니다 단기로하고 소소하게 먹읍시다",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100033872.1.5",
          "form": "다이소 처리해야죠",
          "original_form": "다이소 처리해야죠",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100033872.1.6",
          "form": "가정이 흔들려요",
          "original_form": "가정이 흔들려요",
          "speaker_id": "2",
          "time": "20210825 13:58"
        },
        {
          "id": "MDRW2100033872.1.7",
          "form": "벌이가 되여하는데 마이너스 ㅜㅜ",
          "original_form": "벌이가 되여하는데 마이너스 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100033872.1.8",
          "form": "ㄱ나마 공모주가 벌어요",
          "original_form": "ㄱ나마 공모주가 벌어요",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100033872.1.9",
          "form": "용돈도 그걸로줘요",
          "original_form": "용돈도 그걸로줘요",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100033872.1.10",
          "form": "저도.. 아시는분이 공모주만 하래요 저는 성격급해서",
          "original_form": "저도.. 아시는분이 공모주만 하래요 저는 성격급해서",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100033872.1.11",
          "form": "안된다고ㅋㅋㄱㅋ",
          "original_form": "안된다고ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100033872.1.12",
          "form": "저는 거으ㅣ 백화점수준이예요ㅠㅡㅠ",
          "original_form": "저는 거으ㅣ 백화점수준이예요ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100033872.1.13",
          "form": "저도 급해요 ㅋㅋ",
          "original_form": "저도 급해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100033872.1.14",
          "form": "카뱅 후딱 ㅋㅋ 팔았어요",
          "original_form": "카뱅 후딱 ㅋㅋ 팔았어요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100033872.1.15",
          "form": "저도 그냥 빨리팔고 빨리벌고 그럴래요",
          "original_form": "저도 그냥 빨리팔고 빨리벌고 그럴래요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100033872.1.16",
          "form": "저도 둘째날팔았는데...",
          "original_form": "저도 둘째날팔았는데...",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100033872.1.17",
          "form": "욕심에 한번들어갔다가 수입 반토막내고 나왔어요",
          "original_form": "욕심에 한번들어갔다가 수입 반토막내고 나왔어요",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100033872.1.18",
          "form": "차라리 들고있었음 벌었는데",
          "original_form": "차라리 들고있었음 벌었는데",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100033872.1.19",
          "form": "어휴 어째요",
          "original_form": "어휴 어째요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100033872.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100033872.1.21",
          "form": "이게 진짜 아무나 돈못벌어요",
          "original_form": "이게 진짜 아무나 돈못벌어요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100033872.1.22",
          "form": "그냥전 30분 안에팔고 소소수익 얻고 소고기 사먹을해요 ㅋㅋ",
          "original_form": "그냥전 30분 안에팔고 소소수익 얻고 소고기 사먹을해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100033872.1.23",
          "form": "맞아요 공모주는",
          "original_form": "맞아요 공모주는",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100033872.1.24",
          "form": "초반에 던져야더큰수입이더라구요ㅋ",
          "original_form": "초반에 던져야더큰수입이더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100033872.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100033872.1.26",
          "form": "요샌 ㅉㅎㅁ 미궁일때도 있어요",
          "original_form": "요샌 ㅉㅎㅁ 미궁일때도 있어요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100033872.1.27",
          "form": "갑자기 나중레 오르고",
          "original_form": "갑자기 나중레 오르고",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100033872.1.28",
          "form": "하지만 전 그냥 일찍팔고",
          "original_form": "하지만 전 그냥 일찍팔고",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100033872.1.29",
          "form": "조금벌자 예요",
          "original_form": "조금벌자 예요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100033872.1.30",
          "form": "딴 알바도 하긴 해야겠어요",
          "original_form": "딴 알바도 하긴 해야겠어요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100033872.1.31",
          "form": "갖고있음..  스트레스받긴하더라구요",
          "original_form": "갖고있음..  스트레스받긴하더라구요",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100033872.1.32",
          "form": "오르는지 떨어지는지",
          "original_form": "오르는지 떨어지는지",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100033872.1.33",
          "form": "자꾸 봐야하니",
          "original_form": "자꾸 봐야하니",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100033872.1.34",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100033872.1.35",
          "form": "소소한 알바에 스트레스받으며 할필요없어요",
          "original_form": "소소한 알바에 스트레스받으며 할필요없어요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100033872.1.36",
          "form": "저도 초반에 그냥 던져야겠어요ㅋㅋㅋ",
          "original_form": "저도 초반에 그냥 던져야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.37",
          "form": "어차피 마이너스 안나면 욕심버리고",
          "original_form": "어차피 마이너스 안나면 욕심버리고",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.38",
          "form": "내려놔야죠 ㅎㅎ",
          "original_form": "내려놔야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.39",
          "form": "전 원래던져요 ㅋㅋ",
          "original_form": "전 원래던져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.40",
          "form": "그쵸 욕심이.,  화를 부르고..",
          "original_form": "그쵸 욕심이.,  화를 부르고..",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.41",
          "form": "아 요새 이틀까진 갖고있기도해요",
          "original_form": "아 요새 이틀까진 갖고있기도해요",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.42",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100033872.1.43",
          "form": "스펙도 전확던져요 ㅋㅋ",
          "original_form": "스펙도 전확던져요 ㅋㅋ",
