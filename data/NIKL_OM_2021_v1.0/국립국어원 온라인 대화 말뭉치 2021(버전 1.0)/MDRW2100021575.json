      "utterance": [
        {
          "id": "MDRW2100021575.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021575.1.2",
          "form": "안녕하세요 ㅎㅎ 스포츠 좋아하세요?",
          "original_form": "안녕하세요 ㅎㅎ 스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021575.1.3",
          "form": "올림픽이후로 여자배구에 빠져있어요!",
          "original_form": "올림픽이후로 여자배구에 빠져있어요!",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021575.1.4",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021575.1.5",
          "form": "저도 좋아해요 !!",
          "original_form": "저도 좋아해요 !!",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021575.1.6",
          "form": "반가워요! 요즘 경기 챙겨 보시나요?",
          "original_form": "반가워요! 요즘 경기 챙겨 보시나요?",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021575.1.7",
          "form": "저는 배구는 올림픽때 열심히 봤구 ㅎㅎ 원래 야구 좋아해요 !",
          "original_form": "저는 배구는 올림픽때 열심히 봤구 ㅎㅎ 원래 야구 좋아해요 !",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021575.1.8",
          "form": "아 야구 지금 시즌이죠??",
          "original_form": "아 야구 지금 시즌이죠??",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021575.1.9",
          "form": "응원하는 팀 잘 하고 있나요 ㅎㅎ",
          "original_form": "응원하는 팀 잘 하고 있나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021575.1.10",
          "form": "아니요 ㅎㅎ ... 9등입니다 !",
          "original_form": "아니요 ㅎㅎ ... 9등입니다 !",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021575.1.11",
          "form": "꼴찌 싸움중이네욬ㅋㅋ 아 8등이네요 ,,,",
          "original_form": "꼴찌 싸움중이네욬ㅋㅋ 아 8등이네요 ,,,",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021575.1.12",
          "form": "아.. 저도 한때 그 팀을 응원했... ㅠㅠ",
          "original_form": "아.. 저도 한때 그 팀을 응원했... ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100021575.1.13",
          "form": "승리의... ㅠㅠ",
          "original_form": "승리의... ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021575.1.14",
          "form": "하하 ,,, 동지였군요 ,,,",
          "original_form": "하하 ,,, 동지였군요 ,,,",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021575.1.15",
          "form": "잘하실때 응원하셨어요 ??!",
          "original_form": "잘하실때 응원하셨어요 ??!",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021575.1.16",
          "form": "잘할때 ㅎㅎ",
          "original_form": "잘할때 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021575.1.17",
          "form": "제 기억에 2007년인가 2008년부터 쭉 응원하다가 바빠져서 잠깐 스포츠를 끊고 살았는데",
          "original_form": "제 기억에 2007년인가 2008년부터 쭉 응원하다가 바빠져서 잠깐 스포츠를 끊고 살았는데",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021575.1.18",
          "form": "최근에 보니까 왜 이러는거지싶은 등수가 되어있더라구요ㅠㅠ",
          "original_form": "최근에 보니까 왜 이러는거지싶은 등수가 되어있더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021575.1.19",
          "form": "제 마지막 기억은 5위였나 4였나그랬는데ㅠㅠ",
          "original_form": "제 마지막 기억은 5위였나 4였나그랬는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021575.1.20",
          "form": "아 그때 한창 잘했었죠 ㅋㅋㄱ 로이스터시절 ㅠㅠ",
          "original_form": "아 그때 한창 잘했었죠 ㅋㅋㄱ 로이스터시절 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021575.1.21",
          "form": "17년도에 잠깐 잘했다가 거의 뭐 항상 7-8등 유지중이에요ㄱㄱㅋㅋ",
          "original_form": "17년도에 잠깐 잘했다가 거의 뭐 항상 7-8등 유지중이에요ㄱㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021575.1.22",
          "form": "제가 마침 그때 응원했던 거였네요... ㅎㅎ",
          "original_form": "제가 마침 그때 응원했던 거였네요... ㅎㅎ",
