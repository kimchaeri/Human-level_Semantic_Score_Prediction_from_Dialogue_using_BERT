      "utterance": [
        {
          "id": "MDRW2100041988.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100041988.1.2",
          "form": "네 밥먹엇어요\n모드셨나요",
          "original_form": "네 밥먹엇어요\n모드셨나요",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100041988.1.3",
          "form": "저는 돈까스와 카레요",
          "original_form": "저는 돈까스와 카레요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100041988.1.4",
          "form": "와 돈까스 맛있겠네요",
          "original_form": "와 돈까스 맛있겠네요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100041988.1.5",
          "form": "냉동 남산돈까스 사서 에어프라이어에 돌려먹었는데 아주 괜찬더라구요",
          "original_form": "냉동 남산돈까스 사서 에어프라이어에 돌려먹었는데 아주 괜찬더라구요",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100041988.1.6",
          "form": "바삭하게 되나요",
          "original_form": "바삭하게 되나요",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100041988.1.7",
          "form": "기름 안두르고 바삭하게 되서 기름들어간건 그걸로해서먹어요",
          "original_form": "기름 안두르고 바삭하게 되서 기름들어간건 그걸로해서먹어요",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100041988.1.8",
          "form": "오 저도 해봐야겠네요",
          "original_form": "오 저도 해봐야겠네요",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041988.1.9",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100041988.1.10",
          "form": "비가오길래 전을 했는데 망했어여ㅋㅋ",
          "original_form": "비가오길래 전을 했는데 망했어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100041988.1.11",
          "form": "무슨전요?",
          "original_form": "무슨전요?",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100041988.1.12",
          "form": "야채잇는거 이거저거넣고요ㅋ",
          "original_form": "야채잇는거 이거저거넣고요ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100041988.1.13",
          "form": "오징어넣고 부추고추전도 저는 잘해먹어요",
          "original_form": "오징어넣고 부추고추전도 저는 잘해먹어요",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100041988.1.14",
          "form": "비슷한가요?",
          "original_form": "비슷한가요?",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100041988.1.15",
          "form": "네 근데 모가잘못댓는지 다 부서져서ㅋㅋ",
          "original_form": "네 근데 모가잘못댓는지 다 부서져서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100041988.1.16",
          "form": "농도가 안맞았나보네요",
          "original_form": "농도가 안맞았나보네요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100041988.1.17",
          "form": "듣다보니 저도 낼 해먹어야겠네요~^",
          "original_form": "듣다보니 저도 낼 해먹어야겠네요~^",
          "speaker_id": "2",
          "time": "20210906 22:08"
        }
      ]
