      "utterance": [
        {
          "id": "MDRW2100031974.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210905 10:06"
        },
        {
          "id": "MDRW2100031974.1.2",
          "form": "네.완전요",
          "original_form": "네.완전요",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031974.1.3",
          "form": "다 좋아하지않나요?",
          "original_form": "다 좋아하지않나요?",
          "speaker_id": "2",
          "time": "20210905 10:07"
        },
        {
          "id": "MDRW2100031974.1.4",
          "form": "간혹 안좋아하는 사람도 있더라구요 ㅎㅎ",
          "original_form": "간혹 안좋아하는 사람도 있더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.5",
          "form": "해외파세요? 국내?",
          "original_form": "해외파세요? 국내?",
          "speaker_id": "1",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.6",
          "form": "맞아요..아주드물게",
          "original_form": "맞아요..아주드물게",
          "speaker_id": "2",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.7",
          "form": "코로나전엔 해외요",
          "original_form": "코로나전엔 해외요",
          "speaker_id": "2",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.8",
          "form": "ㅎㅎ그쵸",
          "original_form": "ㅎㅎ그쵸",
          "speaker_id": "1",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.9",
          "form": "해외 못간지",
          "original_form": "해외 못간지",
          "speaker_id": "1",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.10",
          "form": "너무 오래됐네요",
          "original_form": "너무 오래됐네요",
          "speaker_id": "1",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.11",
          "form": "아이가지전에열심히다니세요",
          "original_form": "아이가지전에열심히다니세요",
          "speaker_id": "2",
          "time": "20210905 10:08"
        },
        {
          "id": "MDRW2100031974.1.12",
          "form": "아이낳고는 몇년동안은 거의못가요",
          "original_form": "아이낳고는 몇년동안은 거의못가요",
          "speaker_id": "2",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031974.1.13",
          "form": "ㅋㅋ 지금 나갈 수가....",
          "original_form": "ㅋㅋ 지금 나갈 수가....",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031974.1.14",
          "form": "그러게요..코로나가없어져야할텐데",
          "original_form": "그러게요..코로나가없어져야할텐데",
          "speaker_id": "2",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031974.1.15",
          "form": "ㅋㅋ포기했어요",
          "original_form": "ㅋㅋ포기했어요",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031974.1.16",
          "form": "올해는 갈 수 있을까 했는데",
          "original_form": "올해는 갈 수 있을까 했는데",
          "speaker_id": "1",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100031974.1.17",
          "form": "내년까지도 힘들겠죠?",
          "original_form": "내년까지도 힘들겠죠?",
          "speaker_id": "2",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100031974.1.18",
          "form": "네 어려워 보여요ㅠ",
          "original_form": "네 어려워 보여요ㅠ",
          "speaker_id": "1",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031974.1.19",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031974.1.20",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100031974.1.21",
          "form": "아쉬운대로 국내라도...ㅎㅎ",
          "original_form": "아쉬운대로 국내라도...ㅎㅎ",
