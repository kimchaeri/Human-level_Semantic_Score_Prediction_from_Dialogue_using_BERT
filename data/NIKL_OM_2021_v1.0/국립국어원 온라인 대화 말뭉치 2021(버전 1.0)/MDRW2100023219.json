      "utterance": [
        {
          "id": "MDRW2100023219.1.1",
          "form": "지금 딱히 재밌는거 안해",
          "original_form": "지금 딱히 재밌는거 안해",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023219.1.2",
          "form": "그냥 뭐 보고 있는지 궁금해서",
          "original_form": "그냥 뭐 보고 있는지 궁금해서",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023219.1.3",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023219.1.4",
          "form": "뭔 프로 해?",
          "original_form": "뭔 프로 해?",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023219.1.5",
          "form": "미우새 끝나고 광고해",
          "original_form": "미우새 끝나고 광고해",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023219.1.6",
          "form": "미우새 누구나왔어?",
          "original_form": "미우새 누구나왔어?",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100023219.1.7",
          "form": "게스트?",
          "original_form": "게스트?",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100023219.1.8",
          "form": "응 게스트 아니어도 그냥 출연짘",
          "original_form": "응 게스트 아니어도 그냥 출연짘",
          "speaker_id": "1",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100023219.1.9",
          "form": "소녀시대 유리 나왔어",
          "original_form": "소녀시대 유리 나왔어",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100023219.1.10",
          "form": "의외의 인물이 나왔네",
          "original_form": "의외의 인물이 나왔네",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100023219.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100023219.1.12",
          "form": "또 누구 나왔어?",
          "original_form": "또 누구 나왔어?",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100023219.1.13",
          "form": "신동엽이랑 서장훈 나왔어",
          "original_form": "신동엽이랑 서장훈 나왔어",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023219.1.14",
          "form": "아니 그 사람들은 매번 나오잖아",
          "original_form": "아니 그 사람들은 매번 나오잖아",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023219.1.15",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023219.1.16",
          "form": "축구 관련된 거 말고는 보는 유튜브 없어?",
          "original_form": "축구 관련된 거 말고는 보는 유튜브 없어?",
