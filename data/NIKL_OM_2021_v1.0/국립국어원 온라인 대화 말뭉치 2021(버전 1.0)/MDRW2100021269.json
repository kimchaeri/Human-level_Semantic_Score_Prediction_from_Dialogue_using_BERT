      "utterance": [
        {
          "id": "MDRW2100021269.1.1",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021269.1.2",
          "form": "어떤거 보셨었나요",
          "original_form": "어떤거 보셨었나요",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021269.1.3",
          "form": "본건 좀 있었는데",
          "original_form": "본건 좀 있었는데",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021269.1.4",
          "form": "기억이 남는건 레미제라블!",
          "original_form": "기억이 남는건 레미제라블!",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021269.1.5",
          "form": "오페라의 유령이랑 캣츠도 기억에 남네요",
          "original_form": "오페라의 유령이랑 캣츠도 기억에 남네요",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021269.1.6",
          "form": "맘마미아도요!",
          "original_form": "맘마미아도요!",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021269.1.7",
          "form": "와 다 명작들이네요~",
          "original_form": "와 다 명작들이네요~",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021269.1.8",
          "form": "네! 명작들 엄청 많이봤어요 ㅎㅎ",
          "original_form": "네! 명작들 엄청 많이봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021269.1.9",
          "form": "뮤지컬은 좀 비싸죠?",
          "original_form": "뮤지컬은 좀 비싸죠?",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021269.1.10",
          "form": "문화생활하면 이정돈 줘도된다..? 란 느낌이에요 저는 ㅎㅎ",
          "original_form": "문화생활하면 이정돈 줘도된다..? 란 느낌이에요 저는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021269.1.11",
          "form": "조금비싼편이긴하죠",
          "original_form": "조금비싼편이긴하죠",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021269.1.12",
          "form": "저는아직 한번도못봐서ㅠ",
          "original_form": "저는아직 한번도못봐서ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021269.1.13",
          "form": "지금 코로나 시국이라 좀 힘들긴하구",
          "original_form": "지금 코로나 시국이라 좀 힘들긴하구",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021269.1.14",
          "form": "나중에 잠잠해질때 한번 가세요 ㅠㅠ",
          "original_form": "나중에 잠잠해질때 한번 가세요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021269.1.15",
          "form": "그날들 한번 보고싶네요",
          "original_form": "그날들 한번 보고싶네요",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021269.1.16",
          "form": "아 그것도 보고싶네요 ㅠㅠ 그날들..",
          "original_form": "아 그것도 보고싶네요 ㅠㅠ 그날들..",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021269.1.17",
          "form": "그날들도 유명하죠?",
          "original_form": "그날들도 유명하죠?",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021269.1.18",
          "form": "네 유명하죵",
          "original_form": "네 유명하죵",
          "speaker_id": "2",
