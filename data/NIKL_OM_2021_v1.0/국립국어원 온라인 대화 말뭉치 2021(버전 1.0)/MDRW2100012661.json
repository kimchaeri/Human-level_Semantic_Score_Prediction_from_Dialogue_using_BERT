      "utterance": [
        {
          "id": "MDRW2100012661.1.1",
          "form": "너의 동생 군인이지?",
          "original_form": "너의 동생 군인이지?",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.2",
          "form": "군대에 간 지 한참 된 거 같은데..",
          "original_form": "군대에 간 지 한참 된 거 같은데..",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.3",
          "form": "전역이 멀었구나 아직.",
          "original_form": "전역이 멀었구나 아직.",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.4",
          "form": "응 이제 일년가까이 된 것 같아",
          "original_form": "응 이제 일년가까이 된 것 같아",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.5",
          "form": "동생 머리 밀어줄 때 어땠는지 궁금해.",
          "original_form": "동생 머리 밀어줄 때 어땠는지 궁금해.",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.6",
          "form": "아무래도 해병대라서",
          "original_form": "아무래도 해병대라서",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.7",
          "form": "좀 더 꼼꼼히 밀어줬어",
          "original_form": "좀 더 꼼꼼히 밀어줬어",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.8",
          "form": "사람 머리 밀어본 적 있어?",
          "original_form": "사람 머리 밀어본 적 있어?",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.9",
          "form": "동생이전에",
          "original_form": "동생이전에",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.10",
          "form": "머리를 조금 짜른 것 같기도하고",
          "original_form": "머리를 조금 짜른 것 같기도하고",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.11",
          "form": "머리 밀어줄 생각을 하다니 대단하다",
          "original_form": "머리 밀어줄 생각을 하다니 대단하다",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012661.1.12",
          "form": "머리는 머리카락이아니라 머리를 말하는거야",
          "original_form": "머리는 머리카락이아니라 머리를 말하는거야",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.13",
          "form": "아아 순간 헷갈릴 뻔 했어",
          "original_form": "아아 순간 헷갈릴 뻔 했어",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.14",
          "form": "그런데 내머리가 아니니까 알게뭐니",
          "original_form": "그런데 내머리가 아니니까 알게뭐니",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.15",
          "form": "무서울 뻔 했잖니.",
          "original_form": "무서울 뻔 했잖니.",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.16",
          "form": "잔인하구나",
          "original_form": "잔인하구나",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.17",
          "form": "동생은 훈련 잘 받고있대?",
          "original_form": "동생은 훈련 잘 받고있대?",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.18",
          "form": "요즘 군대는 살만한가봐",
          "original_form": "요즘 군대는 살만한가봐",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.19",
          "form": "나랑 아까까지 카톡도했어",
          "original_form": "나랑 아까까지 카톡도했어",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.20",
          "form": "아그치. 연락도 많이 와?",
          "original_form": "아그치. 연락도 많이 와?",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.21",
          "form": "와.",
          "original_form": "와.",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012661.1.22",
          "form": "휴가 나오면 뿌링클 먹고싶다고 난리야",
          "original_form": "휴가 나오면 뿌링클 먹고싶다고 난리야",
          "speaker_id": "2",
          "time": "20210824 23:49"
        }
      ]
