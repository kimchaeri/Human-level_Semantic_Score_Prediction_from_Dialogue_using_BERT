        },
        {
          "id": "MDRW2100034842.1.2",
          "form": "끝임",
          "original_form": "끝임",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100034842.1.3",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100034842.1.4",
          "form": "나도 집에만 있었어",
          "original_form": "나도 집에만 있었어",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100034842.1.5",
          "form": "오랜만에 날이 안 흐려서 좋더라",
          "original_form": "오랜만에 날이 안 흐려서 좋더라",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034842.1.6",
          "form": "마저...",
          "original_form": "마저...",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034842.1.7",
          "form": "하지만 못나갔다~",
          "original_form": "하지만 못나갔다~",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034842.1.8",
          "form": "1시 넘어서까지 잤다~",
          "original_form": "1시 넘어서까지 잤다~",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034842.1.9",
          "form": "내일도 비 안오겠지..?",
          "original_form": "내일도 비 안오겠지..?",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034842.1.10",
          "form": "내일은 안 올듯?",
          "original_form": "내일은 안 올듯?",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100034842.1.11",
          "form": "근데 다음주에 또 비온대",
          "original_form": "근데 다음주에 또 비온대",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100034842.1.12",
          "form": "아아ㅏㄱ",
          "original_form": "아아ㅏㄱ",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100034842.1.13",
          "form": "엉엉ㅠㅠ",
          "original_form": "엉엉ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034842.1.14",
          "form": "지겨워...그만 왔으면 좋겠어",
          "original_form": "지겨워...그만 왔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034842.1.15",
          "form": "마자...",
          "original_form": "마자...",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034842.1.16",
          "form": "비만 오다가 가을 끝날까봐 무섭다",
          "original_form": "비만 오다가 가을 끝날까봐 무섭다",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034842.1.17",
          "form": "아아ㅏㄱ",
          "original_form": "아아ㅏㄱ",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034842.1.18",
          "form": "싫어!!",
          "original_form": "싫어!!",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034842.1.19",
          "form": "그냥 시원한 바람만 부는 가을이 좋단 말이야...",
          "original_form": "그냥 시원한 바람만 부는 가을이 좋단 말이야...",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100034842.1.20",
          "form": "비는 그만와야 돼...",
          "original_form": "비는 그만와야 돼...",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100034842.1.21",
          "form": "마자...",
          "original_form": "마자...",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034842.1.22",
          "form": "가을에 소풍 갈래",
          "original_form": "가을에 소풍 갈래",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034842.1.23",
          "form": "소풍...",
          "original_form": "소풍...",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034842.1.24",
          "form": "아 비 눈치없네~ 그만 와라~",
          "original_form": "아 비 눈치없네~ 그만 와라~",
          "speaker_id": "1",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100034842.1.25",
          "form": "아 증말 눈치좀 챙겨라",
          "original_form": "아 증말 눈치좀 챙겨라",
