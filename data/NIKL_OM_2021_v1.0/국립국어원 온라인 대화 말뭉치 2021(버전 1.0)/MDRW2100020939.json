        },
        {
          "id": "MDRW2100020939.1.3",
          "form": "게임 하는거 있으세요?",
          "original_form": "게임 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020939.1.4",
          "form": "아니요 ㅜㅜ name1은요?",
          "original_form": "아니요 ㅜㅜ &name1&은요?",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020939.1.5",
          "form": "저는 요즘에는 마인크래프트를..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 요즘에는 마인크래프트를..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020939.1.6",
          "form": "오랜만에 하니까 재밌더라구요",
          "original_form": "오랜만에 하니까 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.7",
          "form": "오 친구분들이랑 하시는거군요??",
          "original_form": "오 친구분들이랑 하시는거군요??",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.8",
          "form": "아뇨 혼자 해요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 혼자 해요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.9",
          "form": "같이 하는게 재밌는데",
          "original_form": "같이 하는게 재밌는데",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.10",
          "form": "주변에 하는애가 없어서",
          "original_form": "주변에 하는애가 없어서",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.11",
          "form": "오 혼자해도되는굼요",
          "original_form": "오 혼자해도되는굼요",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.12",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.13",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.14",
          "form": "저는 좀 무섭더라고요",
          "original_form": "저는 좀 무섭더라고요",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.15",
          "form": "아아 몬스터요?",
          "original_form": "아아 몬스터요?",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020939.1.16",
          "form": "네. 그렇게 확 오는걸 별로 안좋아해서요 ㅋㅋ",
          "original_form": "네. 그렇게 확 오는걸 별로 안좋아해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020939.1.17",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020939.1.18",
          "form": "저도 몬스터 싫어해서",
          "original_form": "저도 몬스터 싫어해서",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020939.1.19",
          "form": "심장이 막 놀래여",
          "original_form": "심장이 막 놀래여",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
