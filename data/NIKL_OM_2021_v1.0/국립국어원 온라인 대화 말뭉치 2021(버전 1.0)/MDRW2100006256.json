      "utterance": [
        {
          "id": "MDRW2100006256.1.1",
          "form": "솔직히 넷플이 좀 더 많은 거 같아요",
          "original_form": "솔직히 넷플이 좀 더 많은 거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100006256.1.2",
          "form": "근데 왓챠에서만 볼 수 있는 것들도 있어서...",
          "original_form": "근데 왓챠에서만 볼 수 있는 것들도 있어서...",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100006256.1.3",
          "form": "아항 어떤거있어요?!?!?",
          "original_form": "아항 어떤거있어요?!?!?",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006256.1.4",
          "form": "저는 그냥 둘 다 쓰는 중이에요!",
          "original_form": "저는 그냥 둘 다 쓰는 중이에요!",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006256.1.5",
          "form": "오피스랑 시트크릭 보고 있고",
          "original_form": "오피스랑 시트크릭 보고 있고",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006256.1.6",
          "form": "덱스터 닥터후 이것도 조금 봤고요",
          "original_form": "덱스터 닥터후 이것도 조금 봤고요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006256.1.7",
          "form": "덱스터는 무슨내용이에요?!",
          "original_form": "덱스터는 무슨내용이에요?!",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006256.1.8",
          "form": "본적은 없지만 와이 우먼 킬 이것도 추천 많이 하시더라고요",
          "original_form": "본적은 없지만 와이 우먼 킬 이것도 추천 많이 하시더라고요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006256.1.9",
          "form": "덱스터는 범죄물이에요",
          "original_form": "덱스터는 범죄물이에요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006256.1.10",
          "form": "연쇄살인마 잡는 연쇄살인마 이야기",
          "original_form": "연쇄살인마 잡는 연쇄살인마 이야기",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006256.1.11",
          "form": "와 겁나 재밋을것같아요 ㅋㅋㅋㅋ",
          "original_form": "와 겁나 재밋을것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006256.1.12",
          "form": "연쇄살인마라니 ㅋㅋㅋㅋㅋ",
          "original_form": "연쇄살인마라니 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006256.1.13",
          "form": "재밌더라고요",
          "original_form": "재밌더라고요",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006256.1.14",
          "form": "근데 뒤로갈수록 평이 안 좋아지는 거 같아서",
          "original_form": "근데 뒤로갈수록 평이 안 좋아지는 거 같아서",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006256.1.15",
          "form": "중간에 적당히 하차했어요",
          "original_form": "중간에 적당히 하차했어요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006256.1.16",
          "form": "아항ㅋㅋㅋ ㅠ 흠 잔인한가요??",
          "original_form": "아항ㅋㅋㅋ ㅠ 흠 잔인한가요??",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006256.1.17",
          "form": "앗 네 청불이에요",
          "original_form": "앗 네 청불이에요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006256.1.18",
          "form": "잘하셨어요 ㅋㅋ 하차타이밍을 잘 잡으셨군요!!",
          "original_form": "잘하셨어요 ㅋㅋ 하차타이밍을 잘 잡으셨군요!!",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006256.1.19",
          "form": "헐 그렇게나 잔인한가요? ㅠㅠ 전 잔인한거 또 못보거든요 ㅠㅠ",
          "original_form": "헐 그렇게나 잔인한가요? ㅠㅠ 전 잔인한거 또 못보거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006256.1.20",
          "form": "피가 좀 많이 나왔던 거 같아요",
          "original_form": "피가 좀 많이 나왔던 거 같아요",
