      "utterance": [
        {
          "id": "MDRW2100028272.1.1",
          "form": "괘씸한 피자가게 입니다",
          "original_form": "괘씸한 피자가게 입니다",
          "speaker_id": "1",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028272.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028272.1.3",
          "form": "치킨 주문하셨나요?",
          "original_form": "치킨 주문하셨나요?",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028272.1.4",
          "form": "당근이죠",
          "original_form": "당근이죠",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028272.1.5",
          "form": "이제 먹어요",
          "original_form": "이제 먹어요",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028272.1.6",
          "form": "현기증나요",
          "original_form": "현기증나요",
          "speaker_id": "1",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100028272.1.7",
          "form": "맛있겠네요 애들은 달라고 안하나요?",
          "original_form": "맛있겠네요 애들은 달라고 안하나요?",
          "speaker_id": "2",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100028272.1.8",
          "form": "벌써 종이가방에 얼굴 넣었어요",
          "original_form": "벌써 종이가방에 얼굴 넣었어요",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028272.1.9",
          "form": "귀엽네요ㅋㅋ 머리들이미는건 고양이나 개나 똑같네요",
          "original_form": "귀엽네요ㅋㅋ 머리들이미는건 고양이나 개나 똑같네요",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028272.1.10",
          "form": "울고불고 난리낫어요",
          "original_form": "울고불고 난리낫어요",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028272.1.11",
          "form": "무시중",
          "original_form": "무시중",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028272.1.12",
          "form": "ㅋㅋ 뒷감당 어떻게하려구요?",
          "original_form": "ㅋㅋ 뒷감당 어떻게하려구요?",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028272.1.13",
          "form": "지쳐서",
          "original_form": "지쳐서",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028272.1.14",
          "form": "자빠링 해있는걸요",
          "original_form": "자빠링 해있는걸요",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028272.1.15",
          "form": "그렇군요 다른거라도 줘야겠네요ㅋ",
          "original_form": "그렇군요 다른거라도 줘야겠네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028272.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028272.1.17",
          "form": "지금 입 사이렌 울리는중이에요",
          "original_form": "지금 입 사이렌 울리는중이에요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028272.1.18",
          "form": "살려주세요",
          "original_form": "살려주세요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028272.1.19",
          "form": "ㅋㅋㅋ 간식이라도 입에 물려줘야겠네요",
          "original_form": "ㅋㅋㅋ 간식이라도 입에 물려줘야겠네요",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028272.1.20",
          "form": "예삐는 뭐 먹나요",
          "original_form": "예삐는 뭐 먹나요",
          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028272.1.21",
          "form": "사료에 시저섞어주려구요",
          "original_form": "사료에 시저섞어주려구요",
          "speaker_id": "2",
          "time": "20210902 19:52"
        }
