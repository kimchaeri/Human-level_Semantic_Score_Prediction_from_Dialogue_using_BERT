      "utterance": [
        {
          "id": "MDRW2100006113.1.1",
          "form": "책 자주 읽으시나요?",
          "original_form": "책 자주 읽으시나요?",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006113.1.2",
          "form": "요즘은 이북이나 웹소설 위주로 보게 되네요",
          "original_form": "요즘은 이북이나 웹소설 위주로 보게 되네요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006113.1.3",
          "form": "웹소설도 보시는군여",
          "original_form": "웹소설도 보시는군여",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006113.1.4",
          "form": "전 거의 만화책....",
          "original_form": "전 거의 만화책....",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006113.1.5",
          "form": "웹툰 말고 책으로도 보시는군요. 요즘은 어떤 거 보세요?",
          "original_form": "웹툰 말고 책으로도 보시는군요. 요즘은 어떤 거 보세요?",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006113.1.6",
          "form": "원피스랑",
          "original_form": "원피스랑",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006113.1.7",
          "form": "요즘 도라에몽에 빠졌어요 ㅋㅋ",
          "original_form": "요즘 도라에몽에 빠졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006113.1.8",
          "form": "원피스는 아주 초기에 좀 보다 말았었는데 아직도 완결 나려면 멀었나요?ㅋㅋ",
          "original_form": "원피스는 아주 초기에 좀 보다 말았었는데 아직도 완결 나려면 멀었나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006113.1.9",
          "form": "작가의 의도를 모르겠어요",
          "original_form": "작가의 의도를 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006113.1.10",
          "form": "언제까지 연재하려고 하는건지",
          "original_form": "언제까지 연재하려고 하는건지",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006113.1.11",
          "form": "편집부에서 원하는 걸까요?",
          "original_form": "편집부에서 원하는 걸까요?",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006113.1.12",
          "form": "아니면 아직도 그려놓은게 많아서",
          "original_form": "아니면 아직도 그려놓은게 많아서",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006113.1.13",
          "form": "편집해서 계속 내는걸수도 있구요",
          "original_form": "편집해서 계속 내는걸수도 있구요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006113.1.14",
          "form": "만화책은 사서 보시나요? 아니면 디지털플랫폼?",
          "original_form": "만화책은 사서 보시나요? 아니면 디지털플랫폼?",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006113.1.15",
          "form": "디지털플랫폼이요",
          "original_form": "디지털플랫폼이요",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006113.1.16",
          "form": "보고싶을때마다 빌려서 ㅋ",
          "original_form": "보고싶을때마다 빌려서 ㅋ",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006113.1.17",
          "form": "어디가 좋나요?",
          "original_form": "어디가 좋나요?",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006113.1.18",
          "form": "네이버요 ㅋㅋ",
          "original_form": "네이버요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006113.1.19",
          "form": "다른데 좋은곳을 모르겠어요",
          "original_form": "다른데 좋은곳을 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006113.1.20",
          "form": "시리즈요?",
          "original_form": "시리즈요?",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100006113.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006113.1.22",
          "form": "시리즈요",
          "original_form": "시리즈요",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006113.1.23",
          "form": "구경 좀 해봐야겠네요 ㅋㅋ",
