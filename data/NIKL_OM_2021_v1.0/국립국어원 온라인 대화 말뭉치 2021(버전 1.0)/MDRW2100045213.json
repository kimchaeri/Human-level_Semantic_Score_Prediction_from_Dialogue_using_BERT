      "utterance": [
        {
          "id": "MDRW2100045213.1.1",
          "form": "저는 뭉텅이로 있는거 조금 별로애요",
          "original_form": "저는 뭉텅이로 있는거 조금 별로애요",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045213.1.2",
          "form": "name2님은 괜찮으세요?",
          "original_form": "&name2&님은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045213.1.3",
          "form": "팽이버섯요",
          "original_form": "팽이버섯요",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045213.1.4",
          "form": "마라탕이라 샤브샤브는 괜찮은데",
          "original_form": "마라탕이라 샤브샤브는 괜찮은데",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045213.1.5",
          "form": "대부분 요리에는 좀 잘게 잘려있는게 좋긴하져",
          "original_form": "대부분 요리에는 좀 잘게 잘려있는게 좋긴하져",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045213.1.6",
          "form": "버섯 좋아하세요?",
          "original_form": "버섯 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045213.1.7",
          "form": "네 좋아해요 근데 표고만 빼구요...ㅋㅋ",
          "original_form": "네 좋아해요 근데 표고만 빼구요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045213.1.8",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045213.1.9",
          "form": "네저도 좋아해요 표고는 쪼끔 덜 좋아요",
          "original_form": "네저도 좋아해요 표고는 쪼끔 덜 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045213.1.10",
          "form": "향이 너무 쎄요 ㅋㅋㅋ",
          "original_form": "향이 너무 쎄요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045213.1.11",
          "form": "맞아여 .. 그 향이.. 너무 쎄서 잘안먹게돼요",
          "original_form": "맞아여 .. 그 향이.. 너무 쎄서 잘안먹게돼요",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045213.1.12",
          "form": "그 라면에 보면 표고있잖아요.. 그것도 별로...입니다..ㅎ",
          "original_form": "그 라면에 보면 표고있잖아요.. 그것도 별로...입니다..ㅎ",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045213.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045213.1.14",
          "form": "근데 또 예쁘기는 예뻐서",
          "original_form": "근데 또 예쁘기는 예뻐서",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045213.1.15",
          "form": "뭔가 대접하는 음식 해야할때면 어쩔수없이 해요",
          "original_form": "뭔가 대접하는 음식 해야할때면 어쩔수없이 해요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045213.1.16",
          "form": "맞아요 .. 그 표고 윗부분 * 이모양으로 잘라놓으면 예쁘더라구요",
          "original_form": "맞아요 .. 그 표고 윗부분 * 이모양으로 잘라놓으면 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045213.1.17",
          "form": "앜ㅋㅋㅋㅋ 귀여워요",
          "original_form": "앜ㅋㅋㅋㅋ 귀여워요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045213.1.18",
          "form": "눈꽃같기도 하네요",
          "original_form": "눈꽃같기도 하네요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045213.1.19",
          "form": "그쵸 ㅋㅋ 데코하기 괜찮아요",
          "original_form": "그쵸 ㅋㅋ 데코하기 괜찮아요",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045213.1.20",
          "form": "전 버섯 중에서는 양송이 제일 좋아해요",
          "original_form": "전 버섯 중에서는 양송이 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045213.1.21",
          "form": "양송이 너무 귀여워요!!",
          "original_form": "양송이 너무 귀여워요!!",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045213.1.22",
          "form": "저는 느타리도 좋아요",
          "original_form": "저는 느타리도 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045213.1.23",
