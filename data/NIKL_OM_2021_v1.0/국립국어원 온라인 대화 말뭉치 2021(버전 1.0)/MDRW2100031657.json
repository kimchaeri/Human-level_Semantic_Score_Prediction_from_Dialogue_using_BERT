      "utterance": [
        {
          "id": "MDRW2100031657.1.1",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.2",
          "form": "근데 솔직히",
          "original_form": "근데 솔직히",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.3",
          "form": "우리나라는",
          "original_form": "우리나라는",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.4",
          "form": "뭐라해야하지",
          "original_form": "뭐라해야하지",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.6",
          "form": "도시 미관? 거리 미관?",
          "original_form": "도시 미관? 거리 미관?",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.7",
          "form": "그런게 너무 못난거같아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런게 너무 못난거같아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.8",
          "form": "아 어어",
          "original_form": "아 어어",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031657.1.9",
          "form": "자연이 예쁜곳은 많은데",
          "original_form": "자연이 예쁜곳은 많은데",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.10",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.11",
          "form": "걍 길거리는 진짜 ㄱ ㅐ못생김",
          "original_form": "걍 길거리는 진짜 ㄱ ㅐ못생김",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.12",
          "form": "약간 계획보시가 아니라",
          "original_form": "약간 계획보시가 아니라",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.13",
          "form": "중구난방으로",
          "original_form": "중구난방으로",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.14",
          "form": "돈 되는 것들은",
          "original_form": "돈 되는 것들은",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.15",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.16",
          "form": "다 집어넣은 느낌임",
          "original_form": "다 집어넣은 느낌임",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.17",
          "form": "빠르게 발전해서 그런거같기도함",
          "original_form": "빠르게 발전해서 그런거같기도함",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.18",
          "form": "유럽쪽처럼",
          "original_form": "유럽쪽처럼",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031657.1.19",
          "form": "그냥 길거리만 예뻐도",
          "original_form": "그냥 길거리만 예뻐도",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031657.1.20",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031657.1.21",
          "form": "스트레스 훨씬 덜받을듯",
          "original_form": "스트레스 훨씬 덜받을듯",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031657.1.22",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031657.1.23",
          "form": "쓰레기를 너무 많이 버림",
          "original_form": "쓰레기를 너무 많이 버림",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031657.1.24",
          "form": "아 맞아....",
          "original_form": "아 맞아....",
          "speaker_id": "1",
          "time": "20210904 23:27"
        }
      ]
