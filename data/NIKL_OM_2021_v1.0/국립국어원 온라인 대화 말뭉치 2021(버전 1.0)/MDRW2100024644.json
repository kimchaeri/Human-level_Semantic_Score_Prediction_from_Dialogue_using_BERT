      "utterance": [
        {
          "id": "MDRW2100024644.1.1",
          "form": "한국에서 못 본 음식들을 먹어봤어",
          "original_form": "한국에서 못 본 음식들을 먹어봤어",
          "speaker_id": "2",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100024644.1.2",
          "form": "당고 처음 먹어봄",
          "original_form": "당고 처음 먹어봄",
          "speaker_id": "2",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100024644.1.3",
          "form": "당고 많이  싸던데",
          "original_form": "당고 많이  싸던데",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024644.1.4",
          "form": "계란말이 달다더라",
          "original_form": "계란말이 달다더라",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024644.1.5",
          "form": "맞아 3개들이 팩에 2천원 안팎이었어",
          "original_form": "맞아 3개들이 팩에 2천원 안팎이었어",
          "speaker_id": "2",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024644.1.6",
          "form": "계란말이 안 먹어봤어 먹어볼걸!",
          "original_form": "계란말이 안 먹어봤어 먹어볼걸!",
          "speaker_id": "2",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024644.1.7",
          "form": "넌 당고 먹어봤니?",
          "original_form": "넌 당고 먹어봤니?",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024644.1.8",
          "form": "일본 숙소는 어때?",
          "original_form": "일본 숙소는 어때?",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024644.1.9",
          "form": "난 만들어서   먹어봤어",
          "original_form": "난 만들어서   먹어봤어",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024644.1.10",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024644.1.11",
          "form": "와 금손이네ㅋㅋㅋㅋㅋ",
          "original_form": "와 금손이네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024644.1.12",
          "form": "일본숙소는 에어비앤비로 예약해서 갔었어",
          "original_form": "일본숙소는 에어비앤비로 예약해서 갔었어",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100024644.1.13",
          "form": "하루에 얼마정도야?",
          "original_form": "하루에 얼마정도야?",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024644.1.14",
          "form": "여행시즌에 잡은 거라 1박에 9만원 정도 했었어",
          "original_form": "여행시즌에 잡은 거라 1박에 9만원 정도 했었어",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024644.1.15",
          "form": "꽤   비싸다",
          "original_form": "꽤   비싸다",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024644.1.16",
          "form": "친구랑 같이  간거야?",
          "original_form": "친구랑 같이  간거야?",
          "speaker_id": "1",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100024644.1.17",
          "form": "응 친구랑 같이 갔는데 방이 사진보다 아담해서 놀랐어",
          "original_form": "응 친구랑 같이 갔는데 방이 사진보다 아담해서 놀랐어",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024644.1.18",
          "form": "ㅋㅋㅋㅋ참나 웃기는 숙소네",
          "original_form": "ㅋㅋㅋㅋ참나 웃기는 숙소네",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024644.1.19",
          "form": "ㅋㅋㅋㅋㅋ방도 사진빨이더라 잘 골라야해",
          "original_form": "ㅋㅋㅋㅋㅋ방도 사진빨이더라 잘 골라야해",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024644.1.20",
          "form": "잠은 잘왔니",
          "original_form": "잠은 잘왔니",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024644.1.21",
          "form": "깨끗  하고?",
          "original_form": "깨끗  하고?",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024644.1.22",
          "form": "응 다행히 벌레 같은 것도 안 나왔어",
          "original_form": "응 다행히 벌레 같은 것도 안 나왔어",
          "speaker_id": "2",
          "time": "20210831 16:37"
        }
      ]
