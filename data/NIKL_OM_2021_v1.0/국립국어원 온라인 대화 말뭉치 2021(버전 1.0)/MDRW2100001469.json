      "utterance": [
        {
          "id": "MDRW2100001469.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20211014 20:52"
        },
        {
          "id": "MDRW2100001469.1.2",
          "form": "안녕~",
          "original_form": "안녕~",
          "speaker_id": "2",
          "time": "20211014 20:52"
        },
        {
          "id": "MDRW2100001469.1.3",
          "form": "혹시 백신 맞았어?",
          "original_form": "혹시 백신 맞았어?",
          "speaker_id": "2",
          "time": "20211014 20:52"
        },
        {
          "id": "MDRW2100001469.1.4",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20211014 20:52"
        },
        {
          "id": "MDRW2100001469.1.5",
          "form": "6월 쯤인거",
          "original_form": "6월 쯤인거",
          "speaker_id": "1",
          "time": "20211014 20:52"
        },
        {
          "id": "MDRW2100001469.1.6",
          "form": "오 되게 일찍 맞았네!",
          "original_form": "오 되게 일찍 맞았네!",
          "speaker_id": "2",
          "time": "20211014 20:53"
        },
        {
          "id": "MDRW2100001469.1.7",
          "form": "군인이라서...",
          "original_form": "군인이라서...",
          "speaker_id": "1",
          "time": "20211014 20:53"
        },
        {
          "id": "MDRW2100001469.1.8",
          "form": "아하... 2차까지 다 맞았겠구나 그럼",
          "original_form": "아하... 2차까지 다 맞았겠구나 그럼",
          "speaker_id": "2",
          "time": "20211014 20:53"
        },
        {
          "id": "MDRW2100001469.1.9",
          "form": "맞고나서 아프진 않았어?",
          "original_form": "맞고나서 아프진 않았어?",
          "speaker_id": "2",
          "time": "20211014 20:53"
        },
        {
          "id": "MDRW2100001469.1.10",
          "form": "괜찮았어",
          "original_form": "괜찮았어",
          "speaker_id": "1",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.11",
          "form": "근육통 정ㄷㅎ",
          "original_form": "근육통 정ㄷㅎ",
          "speaker_id": "1",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.12",
          "form": "오 원래도 되게 건강한가보당",
          "original_form": "오 원래도 되게 건강한가보당",
          "speaker_id": "2",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.13",
          "form": "내 주변 친구 중 한명은",
          "original_form": "내 주변 친구 중 한명은",
          "speaker_id": "2",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.14",
          "form": "심장 아파서 대학병원가서 검사받고왔었는데 ㅠㅠ",
          "original_form": "심장 아파서 대학병원가서 검사받고왔었는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.15",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "1",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.16",
          "form": "어후 사실 방금도",
          "original_form": "어후 사실 방금도",
          "speaker_id": "2",
          "time": "20211014 20:54"
        },
        {
          "id": "MDRW2100001469.1.17",
          "form": "심근염이네",
          "original_form": "심근염이네",
          "speaker_id": "1",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.18",
          "form": "뉴스에서 20대 여성이 백신 맞고 사망했다는거 보고",
          "original_form": "뉴스에서 20대 여성이 백신 맞고 사망했다는거 보고",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.19",
          "form": "맘이 안좋더라 ㅠㅠ",
          "original_form": "맘이 안좋더라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.20",
          "form": "혈전 의심된다고..",
          "original_form": "혈전 의심된다고..",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.21",
          "form": "안타깝네",
          "original_form": "안타깝네",
          "speaker_id": "1",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.22",
          "form": "너는",
          "original_form": "너는",
          "speaker_id": "1",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.23",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.24",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.25",
          "form": "백신 맞았어?",
          "original_form": "백신 맞았어?",
          "speaker_id": "1",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.26",
          "form": "아니..ㅠㅠ",
          "original_form": "아니..ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.27",
          "form": "맞아야되는데 솔직히",
          "original_form": "맞아야되는데 솔직히",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.28",
          "form": "부작용때문에 겁나서 ㅠㅠ",
          "original_form": "부작용때문에 겁나서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211014 20:55"
        },
        {
          "id": "MDRW2100001469.1.29",
          "form": "나도 조금은 무섭더라",
          "original_form": "나도 조금은 무섭더라",
          "speaker_id": "1",
          "time": "20211014 20:56"
        },
        {
          "id": "MDRW2100001469.1.30",
          "form": "나는 맞은 사람들 대단하다고 생각해..",
          "original_form": "나는 맞은 사람들 대단하다고 생각해..",
          "speaker_id": "2",
          "time": "20211014 20:56"
        },
        {
          "id": "MDRW2100001469.1.31",
          "form": "여튼 빨리 치료제 나와서",
          "original_form": "여튼 빨리 치료제 나와서",
          "speaker_id": "2",
          "time": "20211014 20:56"
        },
        {
          "id": "MDRW2100001469.1.32",
          "form": "다들 좀 한시름 놨으면 좋겠다 ㅠ\r\n",
          "original_form": "다들 좀 한시름 놨으면 좋겠다 ㅠ\r\n",
          "speaker_id": "2",
          "time": "20211014 20:57"
        }
      ]
