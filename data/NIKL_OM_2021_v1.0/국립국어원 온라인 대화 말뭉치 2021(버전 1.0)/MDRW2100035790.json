      "utterance": [
        {
          "id": "MDRW2100035790.1.1",
          "form": "안녕라게요",
          "original_form": "안녕라게요",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100035790.1.2",
          "form": "어느계절이 제일 싫어여?",
          "original_form": "어느계절이 제일 싫어여?",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100035790.1.3",
          "form": "여름이욘",
          "original_form": "여름이욘",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035790.1.4",
          "form": "오.. 그러시구나.. 더위 많이타세여?",
          "original_form": "오.. 그러시구나.. 더위 많이타세여?",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100035790.1.5",
          "form": "많이타요",
          "original_form": "많이타요",
          "speaker_id": "2",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100035790.1.6",
          "form": "선풍기가 금방 고장나겟네여",
          "original_form": "선풍기가 금방 고장나겟네여",
          "speaker_id": "1",
          "time": "20210830 02:07"
        },
        {
          "id": "MDRW2100035790.1.7",
          "form": "더워서 오래키실텐데",
          "original_form": "더워서 오래키실텐데",
          "speaker_id": "1",
          "time": "20210830 02:07"
        },
        {
          "id": "MDRW2100035790.1.8",
          "form": "그럼 바꾸면 돼요",
          "original_form": "그럼 바꾸면 돼요",
          "speaker_id": "2",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100035790.1.9",
          "form": "오... 여유넘치시네여",
          "original_form": "오... 여유넘치시네여",
          "speaker_id": "1",
          "time": "20210830 02:10"
        },
        {
          "id": "MDRW2100035790.1.10",
          "form": "네 집에 선풍기 많아요",
          "original_form": "네 집에 선풍기 많아요",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100035790.1.11",
          "form": "오.. 저희집에 하나만 기부해주심안되나여",
          "original_form": "오.. 저희집에 하나만 기부해주심안되나여",
          "speaker_id": "1",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100035790.1.12",
          "form": "선풍기 안쓰는 사람이 있어서 뺏어오면 되요",
          "original_form": "선풍기 안쓰는 사람이 있어서 뺏어오면 되요",
          "speaker_id": "2",
          "time": "20210830 02:14"
        },
        {
          "id": "MDRW2100035790.1.13",
          "form": "와 사악하시다..",
          "original_form": "와 사악하시다..",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100035790.1.14",
          "form": "마음이 어두우시네여",
          "original_form": "마음이 어두우시네여",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100035790.1.15",
          "form": "안쓰는데 놀게두면 선풍기가 불쌍해요",
          "original_form": "안쓰는데 놀게두면 선풍기가 불쌍해요",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100035790.1.16",
          "form": "그러시구나.. 집에있는 선풍기가 너무 안고장나는것도 문제가아닐까여?",
          "original_form": "그러시구나.. 집에있는 선풍기가 너무 안고장나는것도 문제가아닐까여?",
          "speaker_id": "1",
          "time": "20210830 02:17"
        }
