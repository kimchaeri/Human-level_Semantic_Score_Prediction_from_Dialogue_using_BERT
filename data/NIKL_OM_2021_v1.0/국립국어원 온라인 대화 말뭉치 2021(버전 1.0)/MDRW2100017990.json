      "utterance": [
        {
          "id": "MDRW2100017990.1.1",
          "form": "넌 언제 백신 맞니?",
          "original_form": "넌 언제 백신 맞니?",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017990.1.2",
          "form": "담달이요",
          "original_form": "담달이요",
          "speaker_id": "1",
          "time": "20210827 06:03"
        },
        {
          "id": "MDRW2100017990.1.3",
          "form": "화이자?",
          "original_form": "화이자?",
          "speaker_id": "2",
          "time": "20210827 06:04"
        },
        {
          "id": "MDRW2100017990.1.4",
          "form": "네 엄마는 아제 맞으셨죠?",
          "original_form": "네 엄마는 아제 맞으셨죠?",
          "speaker_id": "1",
          "time": "20210827 06:04"
        },
        {
          "id": "MDRW2100017990.1.5",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210827 06:04"
        },
        {
          "id": "MDRW2100017990.1.6",
          "form": "지금 괜찮으세오?",
          "original_form": "지금 괜찮으세오?",
          "speaker_id": "1",
          "time": "20210827 06:04"
        },
        {
          "id": "MDRW2100017990.1.7",
          "form": "다행이도",
          "original_form": "다행이도",
          "speaker_id": "2",
          "time": "20210827 06:04"
        },
        {
          "id": "MDRW2100017990.1.8",
          "form": "아빠도요?",
          "original_form": "아빠도요?",
          "speaker_id": "1",
          "time": "20210827 06:05"
        },
        {
          "id": "MDRW2100017990.1.9",
          "form": "컨디션 좋고",
          "original_form": "컨디션 좋고",
          "speaker_id": "2",
          "time": "20210827 06:05"
        },
        {
          "id": "MDRW2100017990.1.10",
          "form": "이번 백신이  급하게 만들어진거라 이래저래 걱정이 돼요",
          "original_form": "이번 백신이  급하게 만들어진거라 이래저래 걱정이 돼요",
          "speaker_id": "1",
          "time": "20210827 06:06"
        },
        {
          "id": "MDRW2100017990.1.11",
          "form": "우리도 그랬지만 지금은 정말 괜찮아",
          "original_form": "우리도 그랬지만 지금은 정말 괜찮아",
          "speaker_id": "2",
          "time": "20210827 06:06"
        },
        {
          "id": "MDRW2100017990.1.12",
          "form": "그럼 두분은 2차까지 맞으셨으니까 한시름 놨네요",
          "original_form": "그럼 두분은 2차까지 맞으셨으니까 한시름 놨네요",
          "speaker_id": "1",
          "time": "20210827 06:07"
        },
        {
          "id": "MDRW2100017990.1.13",
          "form": "그래 너만 잘 맞으면 된다",
          "original_form": "그래 너만 잘 맞으면 된다",
          "speaker_id": "2",
          "time": "20210827 06:07"
        },
        {
          "id": "MDRW2100017990.1.14",
          "form": "네 백신 맞기 전에 몸 관리 좀 하려구요",
          "original_form": "네 백신 맞기 전에 몸 관리 좀 하려구요",
          "speaker_id": "1",
          "time": "20210827 06:08"
        },
        {
          "id": "MDRW2100017990.1.15",
          "form": "백신 맞은 곳은 좀 어떠니?",
          "original_form": "백신 맞은 곳은 좀 어떠니?",
          "speaker_id": "2",
