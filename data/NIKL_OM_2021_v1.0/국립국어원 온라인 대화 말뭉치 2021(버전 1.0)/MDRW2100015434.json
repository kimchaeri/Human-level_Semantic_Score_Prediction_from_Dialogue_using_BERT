      "utterance": [
        {
          "id": "MDRW2100015434.1.1",
          "form": "종교있어요",
          "original_form": "종교있어요",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015434.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015434.1.3",
          "form": "아뇨 무종교에요!",
          "original_form": "아뇨 무종교에요!",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015434.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015434.1.5",
          "form": "딱히 종굘 믿지않아서",
          "original_form": "딱히 종굘 믿지않아서",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015434.1.6",
          "form": "무신론자기도 하고요",
          "original_form": "무신론자기도 하고요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015434.1.7",
          "form": "부모님은불교요",
          "original_form": "부모님은불교요",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015434.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015434.1.9",
          "form": "저희쪽 부모님은 다 무교에요",
          "original_form": "저희쪽 부모님은 다 무교에요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015434.1.10",
          "form": "어른들은 불교 많이 가시던데 신기해요",
          "original_form": "어른들은 불교 많이 가시던데 신기해요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015434.1.11",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015434.1.12",
          "form": "절을 가본게 딱 1번..?",
          "original_form": "절을 가본게 딱 1번..?",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015434.1.13",
          "form": "가본거같아요",
          "original_form": "가본거같아요",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015434.1.14",
          "form": "전 교회",
          "original_form": "전 교회",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015434.1.15",
          "form": "교회는 좀 가본게",
          "original_form": "교회는 좀 가본게",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015434.1.16",
          "form": "중, 고등학교가 기독교 재단이라",
          "original_form": "중, 고등학교가 기독교 재단이라",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015434.1.17",
          "form": "교회는 좀 가봤어요",
          "original_form": "교회는 좀 가봤어요",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015434.1.18",
          "form": "전 친구랑",
          "original_form": "전 친구랑",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015434.1.19",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015434.1.20",
          "form": "외울거 있지 않았나요?",
          "original_form": "외울거 있지 않았나요?",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015434.1.21",
          "form": "네 성경이나",
          "original_form": "네 성경이나",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015434.1.22",
          "form": "찬송가 외워야하죠",
          "original_form": "찬송가 외워야하죠",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015434.1.23",
          "form": "그렇게많이",
          "original_form": "그렇게많이",
          "speaker_id": "1",
          "time": "20210825 23:31"
        }
      ]
