      "utterance": [
        {
          "id": "MDRW2100016379.1.1",
          "form": "군대....ㅋㅋㅋ",
          "original_form": "군대....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016379.1.2",
          "form": "저는 여자라서 듣기만할게요 ㅋㅋㅋㄱ",
          "original_form": "저는 여자라서 듣기만할게요 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016379.1.3",
          "form": "저도 여자예요",
          "original_form": "저도 여자예요",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016379.1.4",
          "form": "군대하면 뭐가 떠오르세요?",
          "original_form": "군대하면 뭐가 떠오르세요?",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016379.1.5",
          "form": "창살없는감옥?  남자들 안쓰럽 불쌍이요 ㅠㅠ",
          "original_form": "창살없는감옥?  남자들 안쓰럽 불쌍이요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016379.1.6",
          "form": "요즘엔 기간도 많이줄고 휴대폰 소지도된다하고",
          "original_form": "요즘엔 기간도 많이줄고 휴대폰 소지도된다하고",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016379.1.7",
          "form": "훨씬 좋아진거같지만용",
          "original_form": "훨씬 좋아진거같지만용",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016379.1.8",
          "form": "맞아요 월급도 오르고ㅋㅋ",
          "original_form": "맞아요 월급도 오르고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016379.1.9",
          "form": "오늘 가사에 군장병들",
          "original_form": "오늘 가사에 군장병들",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016379.1.10",
          "form": "마스크 안쓰고 집단면역",
          "original_form": "마스크 안쓰고 집단면역",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016379.1.11",
          "form": "실험한다구요",
          "original_form": "실험한다구요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016379.1.12",
          "form": "그런일도 있었군요",
          "original_form": "그런일도 있었군요",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016379.1.13",
          "form": "군생활 하다보면 인간관계가 끈끈해지는거같아요",
          "original_form": "군생활 하다보면 인간관계가 끈끈해지는거같아요",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016379.1.14",
          "form": "중요한건 사회로 나오면",
          "original_form": "중요한건 사회로 나오면",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016379.1.15",
          "form": "연락을 잘안하게 된다지만요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "연락을 잘안하게 된다지만요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016379.1.16",
          "form": "ㅋㅋㅋㅋ 끈끈한거맞나요?ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 끈끈한거맞나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016379.1.17",
          "form": "저는 군대하면 부조리의 집합체가 떠올라요",
          "original_form": "저는 군대하면 부조리의 집합체가 떠올라요",
          "speaker_id": "1",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016379.1.18",
          "form": "그안에 있을때만 끈끈한걸로...ㅋㄱㅋ",
          "original_form": "그안에 있을때만 끈끈한걸로...ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016379.1.19",
          "form": "맞아요 그건 좀 심하죠",
          "original_form": "맞아요 그건 좀 심하죠",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016379.1.20",
          "form": "특히나 요즘에  터지는 사건들보면",
          "original_form": "특히나 요즘에  터지는 사건들보면",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016379.1.21",
          "form": "곪을데로 곪아서 터질게 터진느낌이랄까요",
          "original_form": "곪을데로 곪아서 터질게 터진느낌이랄까요",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016379.1.22",
          "form": "이러는데 여자도 군대가라 함부로 하면 안되죠ㅜ",
          "original_form": "이러는데 여자도 군대가라 함부로 하면 안되죠ㅜ",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100016379.1.23",
          "form": "공감해요 여군 대우 개떡같이 하면서",
          "original_form": "공감해요 여군 대우 개떡같이 하면서",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016379.1.24",
          "form": "가라마라하는게 웃겨요",
          "original_form": "가라마라하는게 웃겨요",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016379.1.25",
          "form": "성.폭.행 사건 뉴스 보셨나요?",
          "original_form": "성.폭.행 사건 뉴스 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016379.1.26",
          "form": "네 제말이요",
          "original_form": "네 제말이요",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016379.1.27",
          "form": "꼭 사람죽어나가야지  그때서야 조금 움직일까말까고",
          "original_form": "꼭 사람죽어나가야지  그때서야 조금 움직일까말까고",
