      "utterance": [
        {
          "id": "MDRW2100027401.1.1",
          "form": "좋아하는 영화 장르 있으세요?",
          "original_form": "좋아하는 영화 장르 있으세요?",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027401.1.2",
          "form": "코미디 영화나 액션이요",
          "original_form": "코미디 영화나 액션이요",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027401.1.3",
          "form": "저는 좀비영화 좋아해요",
          "original_form": "저는 좀비영화 좋아해요",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027401.1.4",
          "form": "최근에 뭐 재밌게 보셨어요",
          "original_form": "최근에 뭐 재밌게 보셨어요",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027401.1.5",
          "form": "최근에는 딱히 본게 없네요",
          "original_form": "최근에는 딱히 본게 없네요",
          "speaker_id": "2",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027401.1.6",
          "form": "넷플에서 범죄영화 하나 봤어요",
          "original_form": "넷플에서 범죄영화 하나 봤어요",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027401.1.7",
          "form": "전 지난주에 넷플에서 헝거게임 봤어요",
          "original_form": "전 지난주에 넷플에서 헝거게임 봤어요",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027401.1.8",
          "form": "범죄영화 이름이 뭔가요",
          "original_form": "범죄영화 이름이 뭔가요",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027401.1.9",
          "form": "나를 믿어줘 에요",
          "original_form": "나를 믿어줘 에요",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027401.1.10",
          "form": "외국영환데 실화기반이라고 하더라구요",
          "original_form": "외국영환데 실화기반이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027401.1.11",
          "form": "오 재밌어보여요ㅋㅋ",
          "original_form": "오 재밌어보여요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027401.1.12",
          "form": "찜해놔야겠네요",
          "original_form": "찜해놔야겠네요",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027401.1.13",
          "form": "앞부분은 노골적인 장면들이 있어서 별론데 범죄자 추적하는건 재밌더라구요",
          "original_form": "앞부분은 노골적인 장면들이 있어서 별론데 범죄자 추적하는건 재밌더라구요",
          "speaker_id": "2",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027401.1.14",
          "form": "범죄 스릴러물이 볼때마다 쫄깃하고 좋은 것 같아요",
          "original_form": "범죄 스릴러물이 볼때마다 쫄깃하고 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100027401.1.15",
          "form": "저는 드라마 잘 안보는 편인데 보면 비밀의숲이나 시그널 같은 장르물 많이 봐요",
          "original_form": "저는 드라마 잘 안보는 편인데 보면 비밀의숲이나 시그널 같은 장르물 많이 봐요",
          "speaker_id": "1",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027401.1.16",
          "form": "요새 장르물 인기 좋져",
          "original_form": "요새 장르물 인기 좋져",
          "speaker_id": "2",
          "time": "20210902 13:03"
        },
        {
          "id": "MDRW2100027401.1.17",
          "form": "비밀의 숲도 정주행 다 못해서ㅋㅋ 다시한번 보려구요",
          "original_form": "비밀의 숲도 정주행 다 못해서ㅋㅋ 다시한번 보려구요",
          "speaker_id": "2",
          "time": "20210902 13:03"
        },
        {
          "id": "MDRW2100027401.1.18",
          "form": "저도 시즌2는 못봤는데",
          "original_form": "저도 시즌2는 못봤는데",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027401.1.19",
          "form": "친구가 보지말라고ㅡ..망작이라고 하더라구요ㅠㅋㅋㅋㅋ",
          "original_form": "친구가 보지말라고ㅡ..망작이라고 하더라구요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027401.1.20",
          "form": "ㅋㅋㅋ시즌1이 너무 잘되서 그런거 같아요",
          "original_form": "ㅋㅋㅋ시즌1이 너무 잘되서 그런거 같아요",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027401.1.21",
          "form": "요새는 저는 빅뱅이론 정주행중이에요 ㅋㅋ",
          "original_form": "요새는 저는 빅뱅이론 정주행중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:28"
        }
      ]
