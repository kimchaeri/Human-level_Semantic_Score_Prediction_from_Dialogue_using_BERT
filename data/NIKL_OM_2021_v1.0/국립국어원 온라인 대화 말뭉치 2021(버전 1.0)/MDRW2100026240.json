      "utterance": [
        {
          "id": "MDRW2100026240.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026240.1.2",
          "form": "집안일 직접 하세요?",
          "original_form": "집안일 직접 하세요?",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026240.1.3",
          "form": "안녕하세요~ 집안일은 직접하는데 가족끼리 분담해요!",
          "original_form": "안녕하세요~ 집안일은 직접하는데 가족끼리 분담해요!",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026240.1.4",
          "form": "저도 분담해요 ㅎㅎ 같이 살거든요",
          "original_form": "저도 분담해요 ㅎㅎ 같이 살거든요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026240.1.5",
          "form": "어떻게 분담하세요?",
          "original_form": "어떻게 분담하세요?",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026240.1.6",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026240.1.7",
          "form": "격일 출근자가 있어서 요일? 날짜 맞춰서 분담해요!",
          "original_form": "격일 출근자가 있어서 요일? 날짜 맞춰서 분담해요!",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026240.1.8",
          "form": "오오 그게 좋죠! 저는 요리랑 설거지 주로 하는 것 같아요",
          "original_form": "오오 그게 좋죠! 저는 요리랑 설거지 주로 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100026240.1.9",
          "form": "오늘은 피곤해서 사먹었어요 ㅋㅋㅋ",
          "original_form": "오늘은 피곤해서 사먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026240.1.10",
          "form": "그럴때 있죠. 집안일 너무 피곤한 것 같아요. 사먹는게 편한데ㅠㅠ",
          "original_form": "그럴때 있죠. 집안일 너무 피곤한 것 같아요. 사먹는게 편한데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026240.1.11",
          "form": "집안일에도 누가 돈 주면 좋겠어요",
          "original_form": "집안일에도 누가 돈 주면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026240.1.12",
          "form": "저도요ㅠㅠ 해도 뭔가 끝이 없어요",
          "original_form": "저도요ㅠㅠ 해도 뭔가 끝이 없어요",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026240.1.13",
          "form": "해서 완료되면 좋은데 또 생기고ㅠㅠ",
          "original_form": "해서 완료되면 좋은데 또 생기고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026240.1.14",
          "form": "티가 안나요ㅠㅜ",
          "original_form": "티가 안나요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026240.1.15",
          "form": "그리고 한거 티도 잘 안나요ㅠㅠ 근데 안하면 티나고ㅠㅠ",
          "original_form": "그리고 한거 티도 잘 안나요ㅠㅠ 근데 안하면 티나고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026240.1.16",
          "form": "자꾸 생기고 진짜 노동이 따로없어요ㅠㅠ",
          "original_form": "자꾸 생기고 진짜 노동이 따로없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026240.1.17",
          "form": "진짜 우렁이각시가 필요해요 ㅋㅋㅋ 어디 우리집에는 안 오나",
          "original_form": "진짜 우렁이각시가 필요해요 ㅋㅋㅋ 어디 우리집에는 안 오나",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026240.1.18",
          "form": "저는 그래도 로봇청소기 덕분에 많이 한시름 놨어요",
          "original_form": "저는 그래도 로봇청소기 덕분에 많이 한시름 놨어요",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026240.1.19",
          "form": "진짜로요 ㅋㅋㅋㅋㅋ가사 도와주는 요정같은게 오면 좋을텐데",
          "original_form": "진짜로요 ㅋㅋㅋㅋㅋ가사 도와주는 요정같은게 오면 좋을텐데",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026240.1.20",
          "form": "로봇청소기 좋은가요? 살까말까 고민중인데",
          "original_form": "로봇청소기 좋은가요? 살까말까 고민중인데",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026240.1.21",
          "form": "가끔 탈출하는 녀석도 있다고해서 ㅋㅋㅋㅋㅋ",
          "original_form": "가끔 탈출하는 녀석도 있다고해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026240.1.22",
          "form": "아 ㅋㅋ 맞아요 보면서 약간 조절해줘야해요",
          "original_form": "아 ㅋㅋ 맞아요 보면서 약간 조절해줘야해요",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026240.1.23",
          "form": "한눈팔면 구석에서 뱅뱅 돌아요",
          "original_form": "한눈팔면 구석에서 뱅뱅 돌아요",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026240.1.24",
          "form": "기계여도 다소...사람의 손길이 필요하긴 한가봐요",
          "original_form": "기계여도 다소...사람의 손길이 필요하긴 한가봐요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026240.1.25",
          "form": "그래도 있으면 편할 것 같긴하네요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래도 있으면 편할 것 같긴하네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026240.1.26",
          "form": "저는 첨엔 진짜 다 알아서 되는 줄 알았는데",
          "original_form": "저는 첨엔 진짜 다 알아서 되는 줄 알았는데",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026240.1.27",
          "form": "좀 옮겨주긴 해야해요 ㅋㅋㅋㅋ",
          "original_form": "좀 옮겨주긴 해야해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:15"
        }
      ]
