      "utterance": [
        {
          "id": "MDRW2100000490.1.1",
          "form": "좋아하는 게임 있으세요?",
          "original_form": "좋아하는 게임 있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000490.1.2",
          "form": "저 쿠키런이랑 메이플스토리요ㅋㅋㅋ",
          "original_form": "저 쿠키런이랑 메이플스토리요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000490.1.3",
          "form": "앗 저도 쿠키런 좋아하는데!",
          "original_form": "앗 저도 쿠키런 좋아하는데!",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000490.1.4",
          "form": "오ㅋㅋㅋㅋ 이번에 업데이트 했던데",
          "original_form": "오ㅋㅋㅋㅋ 이번에 업데이트 했던데",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000490.1.5",
          "form": "새로운 쿠키가 나왔더라구요",
          "original_form": "새로운 쿠키가 나왔더라구요",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000490.1.6",
          "form": "오 맞아요 무슨 여자아이돌 같이 생긴 ㅎㅎ",
          "original_form": "오 맞아요 무슨 여자아이돌 같이 생긴 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000490.1.7",
          "form": "분홍색쿠키ㅋㅋㅋ",
          "original_form": "분홍색쿠키ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000490.1.8",
          "form": "뽑으셨나요ㅋㅋ",
          "original_form": "뽑으셨나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000490.1.9",
          "form": "네 저 연뽑 한번에 나왔어요!ㅎㅎ",
          "original_form": "네 저 연뽑 한번에 나왔어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000490.1.10",
          "form": "와우... 전왜안나올까요ㅋㅋㅋ",
          "original_form": "와우... 전왜안나올까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000490.1.11",
          "form": "저도 나왔으면좋겠네용",
          "original_form": "저도 나왔으면좋겠네용",
          "speaker_id": "2",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000490.1.12",
          "form": "앗 ㅠㅠㅋ 제 기운 나눠드릴게요!",
          "original_form": "앗 ㅠㅠㅋ 제 기운 나눠드릴게요!",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000490.1.13",
          "form": "ㅋㅋㅋ 감사해욬ㅋㅋ 다음 뽑기때는 꼭 나오길ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 감사해욬ㅋㅋ 다음 뽑기때는 꼭 나오길ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000490.1.14",
          "form": "올른 크리스탈 모아야겠어요",
          "original_form": "올른 크리스탈 모아야겠어요",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000490.1.15",
          "form": "네 꼭 나오길 바래요!ㅎㅎ",
          "original_form": "네 꼭 나오길 바래요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000490.1.16",
          "form": "크리스탈 좀 많이 생겼으면 좋겠어요",
          "original_form": "크리스탈 좀 많이 생겼으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000490.1.17",
          "form": "그이까요ㅋㅋㅋ 저는 무과금으로 하는거랔ㅋ",
          "original_form": "그이까요ㅋㅋㅋ 저는 무과금으로 하는거랔ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000490.1.18",
          "form": "크리스탈 한참 부족...ㅠㅠ",
          "original_form": "크리스탈 한참 부족...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000490.1.19",
          "form": "저두요 ㅠ저두 돈좀쓰다가 요새는 무과금이에요 ㅠㅠ",
          "original_form": "저두요 ㅠ저두 돈좀쓰다가 요새는 무과금이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:32"
        }
      ]
