      "utterance": [
        {
          "id": "MDRW2100023846.1.1",
          "form": "안녕하세요, 학생이세요?",
          "original_form": "안녕하세요, 학생이세요?",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023846.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023846.1.3",
          "form": "직장인이에요 ㅎㅎ",
          "original_form": "직장인이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023846.1.4",
          "form": "아하! ㅋㅋㅋ",
          "original_form": "아하! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023846.1.5",
          "form": "아 그럼 학창시절 생각 나시겠어요",
          "original_form": "아 그럼 학창시절 생각 나시겠어요",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023846.1.6",
          "form": "음... 사실 요즘엔 잘 안들어요.  애들이 안타깝기만 하고",
          "original_form": "음... 사실 요즘엔 잘 안들어요.  애들이 안타깝기만 하고",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023846.1.7",
          "form": "학교가 영 활기가 없네요",
          "original_form": "학교가 영 활기가 없네요",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023846.1.8",
          "form": "아 그러네요",
          "original_form": "아 그러네요",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023846.1.9",
          "form": "지금도 등교가 가능한가요?",
          "original_form": "지금도 등교가 가능한가요?",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023846.1.10",
          "form": "등교는 해요.",
          "original_form": "등교는 해요.",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023846.1.11",
          "form": "근데 그러니까 라떼는요",
          "original_form": "근데 그러니까 라떼는요",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023846.1.12",
          "form": "쉬는 시간에 왁자지껄 매점도 가고",
          "original_form": "쉬는 시간에 왁자지껄 매점도 가고",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023846.1.13",
          "form": "운동장에서 뛰고 삼삼오오 모여서 이거봤어? 이거 알아? 하는 그런 낙으로 학교를 다녔던거 같은데",
          "original_form": "운동장에서 뛰고 삼삼오오 모여서 이거봤어? 이거 알아? 하는 그런 낙으로 학교를 다녔던거 같은데",
          "speaker_id": "1",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100023846.1.14",
          "form": "아무래도 힘들죠",
          "original_form": "아무래도 힘들죠",
          "speaker_id": "1",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100023846.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023846.1.16",
          "form": "수업시간 몰래 친구들이랑 떠들고",
          "original_form": "수업시간 몰래 친구들이랑 떠들고",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023846.1.17",
          "form": "쉬는시간에 나가서 놀다가 좀 늦게들어오고 하는 맛인데",
          "original_form": "쉬는시간에 나가서 놀다가 좀 늦게들어오고 하는 맛인데",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023846.1.18",
          "form": "요즘은 좀 안쓰러워보이더라구요",
          "original_form": "요즘은 좀 안쓰러워보이더라구요",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100023846.1.19",
          "form": "그쵸, 꼭 국영수 배우려고만 학교가는게 아닌데...",
          "original_form": "그쵸, 꼭 국영수 배우려고만 학교가는게 아닌데...",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100023846.1.20",
          "form": "아직도 나눠서 반씩 등교하나요?",
          "original_form": "아직도 나눠서 반씩 등교하나요?",
          "speaker_id": "2",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023846.1.21",
          "form": "아직은요",
          "original_form": "아직은요",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100023846.1.22",
          "form": "근데 곧 전면등교한데요",
          "original_form": "근데 곧 전면등교한데요",
          "speaker_id": "1",
          "time": "20210830 19:08"
        },
        {
          "id": "MDRW2100023846.1.23",
          "form": "뭐 이러다 또 못할수도 있지만요",
          "original_form": "뭐 이러다 또 못할수도 있지만요",
          "speaker_id": "1",
          "time": "20210830 19:08"
        },
        {
          "id": "MDRW2100023846.1.24",
          "form": "그쵸 전면등교 얘기는 작년부터 했던거같아요 ㅋㅋㅋ..",
          "original_form": "그쵸 전면등교 얘기는 작년부터 했던거같아요 ㅋㅋㅋ..",
          "speaker_id": "2",
          "time": "20210830 19:12"
        },
        {
          "id": "MDRW2100023846.1.25",
          "form": "근데 전면등교해도 왁자지껄은 못하겠죠",
          "original_form": "근데 전면등교해도 왁자지껄은 못하겠죠",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023846.1.26",
          "form": "네 자리랑 가림판같은거 다 두겠죠",
          "original_form": "네 자리랑 가림판같은거 다 두겠죠",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023846.1.27",
          "form": "그래도 같은 반 친구 얼굴도 잘 모르는 지금보다는 좀 나아질거같아요",
          "original_form": "그래도 같은 반 친구 얼굴도 잘 모르는 지금보다는 좀 나아질거같아요",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023846.1.28",
          "form": "그렇겠죠?",
          "original_form": "그렇겠죠?",
          "speaker_id": "1",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100023846.1.29",
          "form": "이런 시기에도 우정이 꽃피기를 바라요 ㅎㅎ",
          "original_form": "이런 시기에도 우정이 꽃피기를 바라요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:19"
        }
      ]
    }
