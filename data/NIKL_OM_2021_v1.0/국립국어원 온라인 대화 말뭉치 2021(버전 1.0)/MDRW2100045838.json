      "utterance": [
        {
          "id": "MDRW2100045838.1.1",
          "form": "전 쌀국수 아직도전을 ㅠㅠ",
          "original_form": "전 쌀국수 아직도전을 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100045838.1.2",
          "form": "헐 안드셔보셨어요?",
          "original_form": "헐 안드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045838.1.3",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045838.1.4",
          "form": "옛날에먹고",
          "original_form": "옛날에먹고",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045838.1.5",
          "form": "맛이 이상해서",
          "original_form": "맛이 이상해서",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045838.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045838.1.7",
          "form": "헐 ㅠㅠ 그러시구나",
          "original_form": "헐 ㅠㅠ 그러시구나",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045838.1.8",
          "form": "저는 쌀국수 좋아해요",
          "original_form": "저는 쌀국수 좋아해요",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045838.1.9",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045838.1.10",
          "form": "전떡볶이!",
          "original_form": "전떡볶이!",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045838.1.11",
          "form": "최애음식이신가요",
          "original_form": "최애음식이신가요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045838.1.12",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045838.1.13",
          "form": "어디떡볶이 제일 좋어하새요?",
          "original_form": "어디떡볶이 제일 좋어하새요?",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045838.1.14",
          "form": "저는 음... 시장이나 신불??",
          "original_form": "저는 음... 시장이나 신불??",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045838.1.15",
          "form": "두끼도 좋아햐요",
          "original_form": "두끼도 좋아햐요",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045838.1.16",
          "form": "시장이나 신불?",
          "original_form": "시장이나 신불?",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045838.1.17",
          "form": "시장은 그냥 시장인가툐?",
          "original_form": "시장은 그냥 시장인가툐?",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045838.1.18",
          "form": "심뷸은 뭐됴?",
          "original_form": "심뷸은 뭐됴?",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045838.1.19",
          "form": "신불떡볶이라고 있어요!!",
          "original_form": "신불떡볶이라고 있어요!!",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045838.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045838.1.21",
          "form": "삼첩분식도 맛있더라규오!!",
          "original_form": "삼첩분식도 맛있더라규오!!",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045838.1.22",
          "form": "정말 떡볶이 전문가시군요!",
          "original_form": "정말 떡볶이 전문가시군요!",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045838.1.23",
          "form": "먹는거에는 진심입니뎡",
          "original_form": "먹는거에는 진심입니뎡",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045838.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
