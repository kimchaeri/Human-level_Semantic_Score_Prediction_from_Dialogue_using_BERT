      "utterance": [
        {
          "id": "MDRW2100026727.1.1",
          "form": "왜이렇게 어렵냐",
          "original_form": "왜이렇게 어렵냐",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026727.1.2",
          "form": "뭐가?",
          "original_form": "뭐가?",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026727.1.3",
          "form": "알바?",
          "original_form": "알바?",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026727.1.4",
          "form": "사장 잘 만나는거",
          "original_form": "사장 잘 만나는거",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.5",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.6",
          "form": "그니까...이정도면",
          "original_form": "그니까...이정도면",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.7",
          "form": "카페사장에 선입견생길듯",
          "original_form": "카페사장에 선입견생길듯",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.9",
          "form": "진짜 ㅇㅈ....",
          "original_form": "진짜 ㅇㅈ....",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.10",
          "form": "근데 카페는",
          "original_form": "근데 카페는",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026727.1.11",
          "form": "어는 업종으로 들어가는 걸까",
          "original_form": "어는 업종으로 들어가는 걸까",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026727.1.12",
          "form": "음식점업이겠지?",
          "original_form": "음식점업이겠지?",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026727.1.13",
          "form": "카페도 요식업 일부이지않을까?",
          "original_form": "카페도 요식업 일부이지않을까?",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026727.1.14",
          "form": "ㅇㅇ그럴거같애",
          "original_form": "ㅇㅇ그럴거같애",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026727.1.15",
          "form": "근데 빵 만들고 그런건 있는데",
          "original_form": "근데 빵 만들고 그런건 있는데",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026727.1.16",
          "form": "커피는 안보이네",
          "original_form": "커피는 안보이네",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026727.1.17",
          "form": "근로장려금 하는데",
          "original_form": "근로장려금 하는데",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026727.1.18",
          "form": "아아 그거 신청 알아보고있어?",
          "original_form": "아아 그거 신청 알아보고있어?",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026727.1.19",
          "form": "신청이라기보다는",
          "original_form": "신청이라기보다는",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.20",
          "form": "커피는 왜없지",
          "original_form": "커피는 왜없지",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.21",
          "form": "근로장려금 계산하는게 있길래",
          "original_form": "근로장려금 계산하는게 있길래",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.22",
          "form": "함 해보려고",
          "original_form": "함 해보려고",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.24",
          "form": "커피 검색해서 찾았다 ㅋㅋㅋㅋㅋ",
          "original_form": "커피 검색해서 찾았다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.25",
          "form": "몇십만원씩 받는사람도 있던데",
          "original_form": "몇십만원씩 받는사람도 있던데",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.26",
          "form": "글자도 작아서 안보이네",
          "original_form": "글자도 작아서 안보이네",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.27",
          "form": "두근",
          "original_form": "두근",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026727.1.29",
          "form": "아 근데 이거",
          "original_form": "아 근데 이거",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026727.1.30",
          "form": "짠~",
          "original_form": "짠~",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100026727.1.31",
          "form": "아 맞다 그",
          "original_form": "아 맞다 그",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100026727.1.32",
          "form": "근로장려금은 들어가봤어?",
          "original_form": "근로장려금은 들어가봤어?",
          "speaker_id": "1",
          "time": "20210904 23:15"
        }
      ]
    }
