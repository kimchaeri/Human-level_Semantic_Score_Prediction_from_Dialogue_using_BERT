      "utterance": [
        {
          "id": "MDRW2100006306.1.1",
          "form": "그럼 죽음에 대하여 보셨어요?",
          "original_form": "그럼 죽음에 대하여 보셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100006306.1.2",
          "form": "봤어요",
          "original_form": "봤어요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006306.1.3",
          "form": "오 어떠셨어요?",
          "original_form": "오 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006306.1.4",
          "form": "별로였나요?",
          "original_form": "별로였나요?",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100006306.1.5",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006306.1.6",
          "form": "그림체도특이해요",
          "original_form": "그림체도특이해요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006306.1.7",
          "form": "연의 편지는요?",
          "original_form": "연의 편지는요?",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006306.1.8",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006306.1.9",
          "form": "웹툰 얘기하니 흥분되네욬ㅋㅋㅋ",
          "original_form": "웹툰 얘기하니 흥분되네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006306.1.10",
          "form": "정말 좋아하거든요 ㅋㅋ",
          "original_form": "정말 좋아하거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006306.1.11",
          "form": "그건못봤어요",
          "original_form": "그건못봤어요",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006306.1.12",
          "form": "보세요 추천 ㅎㅎ",
          "original_form": "보세요 추천 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006306.1.13",
          "form": "장르가몬가요",
          "original_form": "장르가몬가요",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006306.1.14",
          "form": "혹시 어떤장르 좋아하세요?",
          "original_form": "혹시 어떤장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006306.1.15",
          "form": "저는일상툰이나연애용ㅎㅎ",
          "original_form": "저는일상툰이나연애용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006306.1.16",
          "form": "연의편지 좋아하실거같아요 엄청짧아서",
          "original_form": "연의편지 좋아하실거같아요 엄청짧아서",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006306.1.17",
          "form": "금방봐요 ㅎㅎ",
          "original_form": "금방봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006306.1.18",
          "form": "몽환적인 느낌인데",
          "original_form": "몽환적인 느낌인데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006306.1.19",
          "form": "너무 예뻐요",
          "original_form": "너무 예뻐요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006306.1.20",
          "form": "ㅎㅎㅅㅅ꼭볼게요",
          "original_form": "ㅎㅎㅅㅅ꼭볼게요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006306.1.21",
          "form": "요즘 연재하는것도",
          "original_form": "요즘 연재하는것도",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006306.1.22",
          "form": "저도 거의 로맨스위주로보는데",
          "original_form": "저도 거의 로맨스위주로보는데",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006306.1.23",
          "form": "다음도보세요?",
          "original_form": "다음도보세요?",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006306.1.24",
          "form": "별이삼샵 아는여자애",
          "original_form": "별이삼샵 아는여자애",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006306.1.25",
          "form": "마지막말못봤어요ㅜ",
          "original_form": "마지막말못봤어요ㅜ",
