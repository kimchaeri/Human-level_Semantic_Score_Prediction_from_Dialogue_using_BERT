      "utterance": [
        {
          "id": "MDRW2100043517.1.1",
          "form": "크림소스 떡볶이인가요?",
          "original_form": "크림소스 떡볶이인가요?",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043517.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043517.1.3",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043517.1.4",
          "form": "파스타로 한거랑 비슷했어요 맛있어요",
          "original_form": "파스타로 한거랑 비슷했어요 맛있어요",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100043517.1.5",
          "form": "근데 왜 떡으로 한거죠?",
          "original_form": "근데 왜 떡으로 한거죠?",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.6",
          "form": "예전에 뷔페에서 먹은게 생각나서 해봤는데 맛있었어요",
          "original_form": "예전에 뷔페에서 먹은게 생각나서 해봤는데 맛있었어요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.7",
          "form": "저도 크림파스타 좋아해요",
          "original_form": "저도 크림파스타 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.8",
          "form": "저는 집에서 하면 좋은게 새우잔뜩 넣고해서 ㅎㅎ",
          "original_form": "저는 집에서 하면 좋은게 새우잔뜩 넣고해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.9",
          "form": "집에서 하는 장점이죠",
          "original_form": "집에서 하는 장점이죠",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.10",
          "form": "네네 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.11",
          "form": "크림소스랑 새우가 참 잘어울리죠",
          "original_form": "크림소스랑 새우가 참 잘어울리죠",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.12",
          "form": "맞아요 새우랑 베이컨 잔뜩넣고 해먹으면...",
          "original_form": "맞아요 새우랑 베이컨 잔뜩넣고 해먹으면...",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043517.1.13",
          "form": "너무 맛있겠어요",
          "original_form": "너무 맛있겠어요",
          "speaker_id": "2",
