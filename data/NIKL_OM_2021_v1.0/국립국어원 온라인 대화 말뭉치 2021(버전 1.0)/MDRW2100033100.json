      "utterance": [
        {
          "id": "MDRW2100033100.1.1",
          "form": "저녁메뉴는 뭐드셨나요",
          "original_form": "저녁메뉴는 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100033100.1.2",
          "form": "비 와서",
          "original_form": "비 와서",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100033100.1.3",
          "form": "김치전 해먹고 있어요",
          "original_form": "김치전 해먹고 있어요",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100033100.1.4",
          "form": "아...비오는 날의 부침개..좋네요",
          "original_form": "아...비오는 날의 부침개..좋네요",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100033100.1.5",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033100.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033100.1.7",
          "form": "김치전/부추전 어느쪽이세요?",
          "original_form": "김치전/부추전 어느쪽이세요?",
          "speaker_id": "2",
          "time": "20210907 04:24"
        },
        {
          "id": "MDRW2100033100.1.8",
          "form": "어제는 김치전이었네요",
          "original_form": "어제는 김치전이었네요",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100033100.1.9",
          "form": "물론 부추전이나",
          "original_form": "물론 부추전이나",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100033100.1.10",
          "form": "파전도 좋아합니다",
          "original_form": "파전도 좋아합니다",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100033100.1.11",
          "form": "저는 김치전보단 파전..부추전쪽이네요..ㅎ 사실 기름두르면 다 맛있죠...",
          "original_form": "저는 김치전보단 파전..부추전쪽이네요..ㅎ 사실 기름두르면 다 맛있죠...",
          "speaker_id": "2",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100033100.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100033100.1.13",
          "form": "사실 탄수화물+기름+적당한 야채",
          "original_form": "사실 탄수화물+기름+적당한 야채",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100033100.1.14",
          "form": "맛없기 힘든 조합이죠",
          "original_form": "맛없기 힘든 조합이죠",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100033100.1.15",
          "form": "한장으론 멈추기 힘들고...",
          "original_form": "한장으론 멈추기 힘들고...",
          "speaker_id": "2",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100033100.1.16",
          "form": "흠흠...",
          "original_form": "흠흠...",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100033100.1.17",
          "form": "사실 어제도...",
          "original_form": "사실 어제도...",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100033100.1.18",
          "form": "ㅋㅋㅋ 맛있게 먹었으면 됐죠 아이들도 잘 먹더라구요 부침개",
          "original_form": "ㅋㅋㅋ 맛있게 먹었으면 됐죠 아이들도 잘 먹더라구요 부침개",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100033100.1.19",
          "form": "하기야",
          "original_form": "하기야",
          "speaker_id": "1",
          "time": "20210907 10:56"
        },
        {
          "id": "MDRW2100033100.1.20",
          "form": "맛있으면",
          "original_form": "맛있으면",
          "speaker_id": "1",
          "time": "20210907 10:56"
        },
        {
          "id": "MDRW2100033100.1.21",
          "form": "0칼로리라잖아요",
          "original_form": "0칼로리라잖아요",
          "speaker_id": "1",
          "time": "20210907 10:56"
        },
        {
          "id": "MDRW2100033100.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 10:56"
        },
        {
          "id": "MDRW2100033100.1.23",
          "form": "오늘은 뭐 드셨나요",
          "original_form": "오늘은 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100033100.1.24",
          "form": "점심은",
          "original_form": "점심은",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100033100.1.25",
          "form": "간단하게 빵 먹었어요",
          "original_form": "간단하게 빵 먹었어요",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100033100.1.26",
          "form": "코로나 이후로",
          "original_form": "코로나 이후로",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100033100.1.27",
          "form": "식사를 각자 자리에서",
          "original_form": "식사를 각자 자리에서",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100033100.1.28",
          "form": "도시락으로 먹다보니",
          "original_form": "도시락으로 먹다보니",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100033100.1.29",
          "form": "점심은 빵 먹었어요",
          "original_form": "점심은 빵 먹었어요",
