      "utterance": [
        {
          "id": "MDRW2100024631.1.1",
          "form": "넘 귀여워요 택이..",
          "original_form": "넘 귀여워요 택이..",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024631.1.2",
          "form": "그런 드라마 하나 더 하면 보고파요",
          "original_form": "그런 드라마 하나 더 하면 보고파요",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024631.1.3",
          "form": "펜트하우스 보세요?",
          "original_form": "펜트하우스 보세요?",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024631.1.4",
          "form": "네네!!!! 저 매화 충격받으며 보고 있습니닷",
          "original_form": "네네!!!! 저 매화 충격받으며 보고 있습니닷",
          "speaker_id": "1",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100024631.1.5",
          "form": "이번주 거 보셨나요?!",
          "original_form": "이번주 거 보셨나요?!",
          "speaker_id": "1",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100024631.1.6",
          "form": "네 이번주거 넘 충격..",
          "original_form": "네 이번주거 넘 충격..",
          "speaker_id": "2",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100024631.1.7",
          "form": "댜체 다음주엔 무슨 내용을.. 보여주려고.ㅡ?",
          "original_form": "댜체 다음주엔 무슨 내용을.. 보여주려고.ㅡ?",
          "speaker_id": "2",
          "time": "20210831 19:21"
        },
        {
          "id": "MDRW2100024631.1.8",
          "form": "아 그니까요!!! 누구 말로는 이번화가 아예 주단태 꿈이다라는 말도 있고",
          "original_form": "아 그니까요!!! 누구 말로는 이번화가 아예 주단태 꿈이다라는 말도 있고",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024631.1.9",
          "form": "넘넘 혼란스러워요",
          "original_form": "넘넘 혼란스러워요",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024631.1.10",
          "form": "이번주가 마지막 아닌가요..?",
          "original_form": "이번주가 마지막 아닌가요..?",
          "speaker_id": "1",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024631.1.11",
          "form": "다음주가 마지막인걸루 알고있어요..!!",
          "original_form": "다음주가 마지막인걸루 알고있어요..!!",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024631.1.12",
          "form": "14화가 마지막인가요?",
          "original_form": "14화가 마지막인가요?",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024631.1.13",
          "form": "저도 확실히 모르겠어요 ㅠㅠ",
          "original_form": "저도 확실히 모르겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024631.1.14",
          "form": "전 천서진 연기력이..",
          "original_form": "전 천서진 연기력이..",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024631.1.15",
          "form": "헛 저도 확실히 모르겠네요 ..^^",
          "original_form": "헛 저도 확실히 모르겠네요 ..^^",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024631.1.16",
          "form": "천서진 진짜 치매 연기라는 말 있는 거 듣고",
          "original_form": "천서진 진짜 치매 연기라는 말 있는 거 듣고",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024631.1.17",
          "form": "소름 돋았잖아요 진짜 그럴까봐",
          "original_form": "소름 돋았잖아요 진짜 그럴까봐",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024631.1.18",
          "form": "진쩌",
          "original_form": "진쩌",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100024631.1.19",
          "form": "김소연이 연말에 대상받을거같아요...",
          "original_form": "김소연이 연말에 대상받을거같아요...",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100024631.1.20",
          "form": "안 받을 슈가 없어요!!!!",
          "original_form": "안 받을 슈가 없어요!!!!",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100024631.1.21",
          "form": "그 아빠 죽었을 때 우는 연기도 너무 소름",
          "original_form": "그 아빠 죽었을 때 우는 연기도 너무 소름",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100024631.1.22",
          "form": "그러니까요 진짜 연기천재..",
          "original_form": "그러니까요 진짜 연기천재..",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100024631.1.23",
          "form": "어쫌 그렇게 연기를 하는지 대박이에요",
          "original_form": "어쫌 그렇게 연기를 하는지 대박이에요",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100024631.1.24",
          "form": "다른 사람도 다 잘하는데",
          "original_form": "다른 사람도 다 잘하는데",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100024631.1.25",
          "form": "김소연은 진짜 소름돋는거 같아요",
          "original_form": "김소연은 진짜 소름돋는거 같아요",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100024631.1.26",
          "form": "아 그니까요!!!!!!! 저는 개인적으로 이지아보다",
          "original_form": "아 그니까요!!!!!!! 저는 개인적으로 이지아보다",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100024631.1.27",
          "form": "더 몰입도 높은 캐릭터가탕요",
          "original_form": "더 몰입도 높은 캐릭터가탕요",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100024631.1.28",
          "form": "저도요!! 이지아는 제기쥰..무매력캐..ㅠ 로건이랑 엮이고나서부터",
          "original_form": "저도요!! 이지아는 제기쥰..무매력캐..ㅠ 로건이랑 엮이고나서부터",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100024631.1.29",
          "form": "진짜 캐붕인거같아여..ㅠㅠ",
          "original_form": "진짜 캐붕인거같아여..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100024631.1.30",
          "form": "헐 헐 저도요",
          "original_form": "헐 헐 저도요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100024631.1.31",
          "form": "저는.. 그 둘이 이해가.. 안 가거든요",
          "original_form": "저는.. 그 둘이 이해가.. 안 가거든요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100024631.1.32",
          "form": "암만 피 안 섞인 남매지만..",
          "original_form": "암만 피 안 섞인 남매지만..",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100024631.1.33",
          "form": "양동생의 친엄마랑 사랑이라니요..",
          "original_form": "양동생의 친엄마랑 사랑이라니요..",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100024631.1.34",
          "form": "ㅋㅋㅋㅋㅋ역시 저만 그런게 아니였군요!!!!",
          "original_form": "ㅋㅋㅋㅋㅋ역시 저만 그런게 아니였군요!!!!",
          "speaker_id": "2",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100024631.1.35",
          "form": "심지어 내 친딸을.. 이용해먹고 버린.. ㅠ",
          "original_form": "심지어 내 친딸을.. 이용해먹고 버린.. ㅠ",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100024631.1.36",
          "form": "머 그런 서사들을 생각해보명 전혀 이해가 안가는..? ㅠㅠ",
          "original_form": "머 그런 서사들을 생각해보명 전혀 이해가 안가는..? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100024631.1.37",
          "form": "천서진 돌려내~!~!!",
          "original_form": "천서진 돌려내~!~!!",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100024631.1.38",
          "form": "아 그르니까요!! 저는 솔직히 오윤희 용서한 것도",
          "original_form": "아 그르니까요!! 저는 솔직히 오윤희 용서한 것도",
          "speaker_id": "1",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100024631.1.39",
          "form": "잘 이해가 안 가요 ㅠ.ㅠ",
          "original_form": "잘 이해가 안 가요 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100024631.1.40",
          "form": "그렇게 설아를 부르짖었으면서..",
