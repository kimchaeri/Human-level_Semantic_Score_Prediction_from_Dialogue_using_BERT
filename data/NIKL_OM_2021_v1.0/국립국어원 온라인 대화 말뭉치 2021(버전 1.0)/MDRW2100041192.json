      "utterance": [
        {
          "id": "MDRW2100041192.1.1",
          "form": "이젠 밤공기가 제법 쌀쌀하네요",
          "original_form": "이젠 밤공기가 제법 쌀쌀하네요",
          "speaker_id": "1",
          "time": "20210905 19:52"
        },
        {
          "id": "MDRW2100041192.1.2",
          "form": "네 요즘 낮에도 별로 안 덥구 밤에는 춥더라구요",
          "original_form": "네 요즘 낮에도 별로 안 덥구 밤에는 춥더라구요",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041192.1.3",
          "form": "네 가을인가봐요",
          "original_form": "네 가을인가봐요",
          "speaker_id": "1",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100041192.1.4",
          "form": "네 여름이 가고 가을 온게 느껴지네요",
          "original_form": "네 여름이 가고 가을 온게 느껴지네요",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041192.1.5",
          "form": "네 언제 이렇게 가을이 왔는지",
          "original_form": "네 언제 이렇게 가을이 왔는지",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041192.1.6",
          "form": "며칠 전까진 에어컨 켰는데 말예요",
          "original_form": "며칠 전까진 에어컨 켰는데 말예요",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041192.1.7",
          "form": "맞아요 며칠전에 엄청 습하고 더워서 에어컨 켰었는데 ㅎㅎ",
          "original_form": "맞아요 며칠전에 엄청 습하고 더워서 에어컨 켰었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041192.1.8",
          "form": "가을옷 꺼내셨나요?",
          "original_form": "가을옷 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041192.1.9",
          "form": "본격적으로 꺼내진 않고",
          "original_form": "본격적으로 꺼내진 않고",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041192.1.10",
          "form": "얇은 가디건이랑 긴팔 블라우스 정도 꺼냈어요",
          "original_form": "얇은 가디건이랑 긴팔 블라우스 정도 꺼냈어요",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041192.1.11",
          "form": "이제 덥지 않으니 여행가고 싶어지네요 ㅋㅋ",
          "original_form": "이제 덥지 않으니 여행가고 싶어지네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100041192.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100041192.1.13",
          "form": "딱 나들이하고 싶은 날씨",
          "original_form": "딱 나들이하고 싶은 날씨",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100041192.1.14",
          "form": "좋아하는 계절 있으신가요",
          "original_form": "좋아하는 계절 있으신가요",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100041192.1.15",
          "form": "저는 봄 좋아했는데",
          "original_form": "저는 봄 좋아했는데",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041192.1.16",
          "form": "요즘은 가을도 좋아요",
          "original_form": "요즘은 가을도 좋아요",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041192.1.17",
          "form": "저랑 비슷하시네요 저도 요즘은 가을이 좋더라구요",
          "original_form": "저랑 비슷하시네요 저도 요즘은 가을이 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100041192.1.18",
          "form": "날씨가 딱 적당한 계절같아요",
          "original_form": "날씨가 딱 적당한 계절같아요",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100041192.1.19",
          "form": "네 미세먼지 황사도 적고",
          "original_form": "네 미세먼지 황사도 적고",
          "speaker_id": "1",
          "time": "20210905 20:05"
        }
      ]
