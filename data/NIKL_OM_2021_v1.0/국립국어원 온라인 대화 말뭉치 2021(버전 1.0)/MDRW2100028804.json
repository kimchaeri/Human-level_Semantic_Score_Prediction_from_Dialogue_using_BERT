      "utterance": [
        {
          "id": "MDRW2100028804.1.1",
          "form": "칼 같으세요!",
          "original_form": "칼 같으세요!",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028804.1.2",
          "form": "그거 젤 좋아했는데ㅎ",
          "original_form": "그거 젤 좋아했는데ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028804.1.3",
          "form": "장미칼인줄",
          "original_form": "장미칼인줄",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028804.1.4",
          "form": "마이구미 짱!",
          "original_form": "마이구미 짱!",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.5",
          "form": "ㅋㅋ 집에 장미칼 있으셨나요ㅋ",
          "original_form": "ㅋㅋ 집에 장미칼 있으셨나요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.6",
          "form": "그거 반틈 잘라서 입술에 붙이고 그랫는데",
          "original_form": "그거 반틈 잘라서 입술에 붙이고 그랫는데",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.7",
          "form": "아니요 ㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.8",
          "form": "아닠ㅋ 붙이면 붙나요",
          "original_form": "아닠ㅋ 붙이면 붙나요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.9",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.10",
          "form": "유행을 따라가지 않으셨군요",
          "original_form": "유행을 따라가지 않으셨군요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.11",
          "form": "침 안묻히면",
          "original_form": "침 안묻히면",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.12",
          "form": "붙어요 ㅋㅋ",
          "original_form": "붙어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.13",
          "form": "장미칼있서오?",
          "original_form": "장미칼있서오?",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.14",
          "form": "붙이고 뭐해요 젤리를ㅋㅋ",
          "original_form": "붙이고 뭐해요 젤리를ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028804.1.15",
          "form": "한때 부엌에 있었죠ㅋ",
          "original_form": "한때 부엌에 있었죠ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.16",
          "form": "엄청 화려함ㅎㅎ",
          "original_form": "엄청 화려함ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.17",
          "form": "서로 나눠먹어요",
          "original_form": "서로 나눠먹어요",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.18",
          "form": "후마",
          "original_form": "후마",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.19",
          "form": "후아",
          "original_form": "후아",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.20",
          "form": "모던한 부엌엔  장미칼 출입금지ㅎ",
          "original_form": "모던한 부엌엔  장미칼 출입금지ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.21",
          "form": "잘있냐 젤리!",
          "original_form": "잘있냐 젤리!",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.22",
          "form": "앗콩한조각도 아니구ㅋㅋ",
          "original_form": "앗콩한조각도 아니구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028804.1.23",
          "form": "잘 있겠냐구요 ㅋㅋㅋ",
          "original_form": "잘 있겠냐구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100028804.1.24",
          "form": "ㅋㅋㅋ저 멀리 떠낫습니다",
          "original_form": "ㅋㅋㅋ저 멀리 떠낫습니다",
