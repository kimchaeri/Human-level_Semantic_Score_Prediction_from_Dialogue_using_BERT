      "utterance": [
        {
          "id": "MDRW2100005147.1.1",
          "form": "먹는것만 좋아합니다 ㅋㅋㅋ",
          "original_form": "먹는것만 좋아합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005147.1.2",
          "form": "ㅎㅎ 해주는게 젤  맛나죠",
          "original_form": "ㅎㅎ 해주는게 젤  맛나죠",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005147.1.3",
          "form": "커피도 남이타준커피가 젤맛있구요ㅋㅋ",
          "original_form": "커피도 남이타준커피가 젤맛있구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005147.1.4",
          "form": "ㅎㅎ 전  그래도 고기좋아해서 삼겹살도 구워먹고",
          "original_form": "ㅎㅎ 전  그래도 고기좋아해서 삼겹살도 구워먹고",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005147.1.5",
          "form": "혼자서",
          "original_form": "혼자서",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005147.1.6",
          "form": "찌개도 해먹고",
          "original_form": "찌개도 해먹고",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005147.1.7",
          "form": "혼자서도 잘해드시는데요?ㅎㅎ 요리잘하는분들 부럽더라구요ㅠ",
          "original_form": "혼자서도 잘해드시는데요?ㅎㅎ 요리잘하는분들 부럽더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005147.1.8",
          "form": "저도 요리 잘하는사람 부러워요",
          "original_form": "저도 요리 잘하는사람 부러워요",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005147.1.9",
          "form": "전 조미료의 힘을 빌려서ㅋ",
          "original_form": "전 조미료의 힘을 빌려서ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005147.1.10",
          "form": "ㅋㅋ그래도 대단하세요! 제일잘하시는 음식이 뭔가요?",
          "original_form": "ㅋㅋ그래도 대단하세요! 제일잘하시는 음식이 뭔가요?",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005147.1.11",
          "form": "분식종류는 그나마 괜찮은거같아요ㅎ 쉬우니",
          "original_form": "분식종류는 그나마 괜찮은거같아요ㅎ 쉬우니",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005147.1.12",
          "form": "떡볶이 오뎅탕ㅎ",
          "original_form": "떡볶이 오뎅탕ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005147.1.13",
          "form": "얼마전에 전복사서 조림해봤어요",
          "original_form": "얼마전에 전복사서 조림해봤어요",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005147.1.14",
          "form": "전복손질도 직접?ㅎㅎ 멋진데요?",
          "original_form": "전복손질도 직접?ㅎㅎ 멋진데요?",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005147.1.15",
          "form": "네 생각보다 쉽더라구요",
          "original_form": "네 생각보다 쉽더라구요",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005147.1.16",
          "form": "그냥 솔로 박박 문지르고 내장떼면끝",
          "original_form": "그냥 솔로 박박 문지르고 내장떼면끝",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005147.1.17",
          "form": "이빨 제거하고",
          "original_form": "이빨 제거하고",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005147.1.18",
          "form": "인터넷검색해서 레시피 보고 하시나요?ㅎㅎ",
          "original_form": "인터넷검색해서 레시피 보고 하시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005147.1.19",
          "form": "네 블로그 보고 그냥 대충 제입맛에 맞게 만들어요",
          "original_form": "네 블로그 보고 그냥 대충 제입맛에 맞게 만들어요",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005147.1.20",
          "form": "남들이 먹음 맛없을지도ㅎ",
          "original_form": "남들이 먹음 맛없을지도ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005147.1.21",
