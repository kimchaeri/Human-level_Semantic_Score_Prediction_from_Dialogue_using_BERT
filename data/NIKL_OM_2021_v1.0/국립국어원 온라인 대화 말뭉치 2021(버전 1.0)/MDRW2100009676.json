      "utterance": [
        {
          "id": "MDRW2100009676.1.1",
          "form": "스포츠 경기 직접 보러 가신 적 있으신가요?",
          "original_form": "스포츠 경기 직접 보러 가신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100009676.1.2",
          "form": "저 한 번도 없어요",
          "original_form": "저 한 번도 없어요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100009676.1.3",
          "form": "근데 이번에 올림픽 보고",
          "original_form": "근데 이번에 올림픽 보고",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100009676.1.4",
          "form": "관심이 좀 많이 생겼어요",
          "original_form": "관심이 좀 많이 생겼어요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100009676.1.5",
          "form": "어떤 종목 재밌게 보셨어요?",
          "original_form": "어떤 종목 재밌게 보셨어요?",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100009676.1.6",
          "form": "여자배구도 재밌었고",
          "original_form": "여자배구도 재밌었고",
          "speaker_id": "2",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100009676.1.7",
          "form": "핸드볼이랑 양궁... 이것저것 거의 다 봤던 거 같아요",
          "original_form": "핸드볼이랑 양궁... 이것저것 거의 다 봤던 거 같아요",
          "speaker_id": "2",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100009676.1.8",
          "form": "저도 이것저것 다양하게 봤어요. 평창 동계올림픽 할 때 직접 못 가 본 게 아쉽네요",
          "original_form": "저도 이것저것 다양하게 봤어요. 평창 동계올림픽 할 때 직접 못 가 본 게 아쉽네요",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100009676.1.9",
          "form": "저도 그 생각 자주 합니다",
          "original_form": "저도 그 생각 자주 합니다",
          "speaker_id": "2",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100009676.1.10",
          "form": "한번 더 했으면 좋겠어요",
          "original_form": "한번 더 했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100009676.1.11",
          "form": "부산이라든가...",
          "original_form": "부산이라든가...",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100009676.1.12",
          "form": "죽기 전에 한 번은 더 할 것도 같고요 ㅋㅋ",
          "original_form": "죽기 전에 한 번은 더 할 것도 같고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100009676.1.13",
          "form": "전 인천아시안게임 때 목동야구장에서 했던 야구 경기 보러간 적은 있어요",
          "original_form": "전 인천아시안게임 때 목동야구장에서 했던 야구 경기 보러간 적은 있어요",
          "speaker_id": "1",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100009676.1.14",
          "form": "오와",
          "original_form": "오와",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100009676.1.15",
          "form": "야구 좋아하세요?",
          "original_form": "야구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100009676.1.16",
          "form": "네 야구 팬이기도 하고 비교적 가까이서 해서 갔었어요",
          "original_form": "네 야구 팬이기도 하고 비교적 가까이서 해서 갔었어요",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100009676.1.17",
          "form": "야구 어느팀 좋아하세요?",
          "original_form": "야구 어느팀 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100009676.1.18",
          "form": "저는 두산베어스 팬이에요",
          "original_form": "저는 두산베어스 팬이에요",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100009676.1.19",
          "form": "오와 지역 구단 팬인건가요?",
          "original_form": "오와 지역 구단 팬인건가요?",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100009676.1.20",
          "form": "네 그렇지만 야구 그 자체를 좋아하기도 해서 다른 팀 경기 볼 때도 있어요",
          "original_form": "네 그렇지만 야구 그 자체를 좋아하기도 해서 다른 팀 경기 볼 때도 있어요",
          "speaker_id": "1",
          "time": "20210823 22:30"
        }
      ]
