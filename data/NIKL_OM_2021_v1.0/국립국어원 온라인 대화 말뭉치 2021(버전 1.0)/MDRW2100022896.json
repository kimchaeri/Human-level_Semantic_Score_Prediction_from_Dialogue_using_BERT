      "utterance": [
        {
          "id": "MDRW2100022896.1.1",
          "form": "내일 71기 들어온데!",
          "original_form": "내일 71기 들어온데!",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.2",
          "form": "데 아니고 대야",
          "original_form": "데 아니고 대야",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.3",
          "form": "뭐라하지마",
          "original_form": "뭐라하지마",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.4",
          "form": "너는 직급이 뭐야?",
          "original_form": "너는 직급이 뭐야?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.5",
          "form": "너신고할거야",
          "original_form": "너신고할거야",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.6",
          "form": "국민신문고에다",
          "original_form": "국민신문고에다",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.7",
          "form": "계급이거든 바보야",
          "original_form": "계급이거든 바보야",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.8",
          "form": "상병이야상병",
          "original_form": "상병이야상병",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.9",
          "form": "뭐라하지마",
          "original_form": "뭐라하지마",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022896.1.10",
          "form": "상병이면 높은거야?",
          "original_form": "상병이면 높은거야?",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.11",
          "form": "미안",
          "original_form": "미안",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.12",
          "form": "6개월남았어",
          "original_form": "6개월남았어",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.13",
          "form": "일수로는 203일!^^",
          "original_form": "일수로는 203일!^^",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.14",
          "form": "계급 순서가 어떻게 돼?",
          "original_form": "계급 순서가 어떻게 돼?",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.15",
          "form": "이병",
          "original_form": "이병",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.16",
          "form": "일병",
          "original_form": "일병",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.17",
          "form": "상병",
          "original_form": "상병",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.18",
          "form": "병장",
          "original_form": "병장",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.19",
          "form": "그중에 난 상병",
          "original_form": "그중에 난 상병",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022896.1.20",
          "form": "그렇구나 후임은 언제언제 들어와?",
          "original_form": "그렇구나 후임은 언제언제 들어와?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022896.1.21",
          "form": "매달?",
          "original_form": "매달?",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022896.1.22",
          "form": "매달들어오나",
          "original_form": "매달들어오나",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022896.1.23",
          "form": "그럼 후임 얼마나 있어?",
          "original_form": "그럼 후임 얼마나 있어?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022896.1.24",
          "form": "지금 나 소대엔 7명",
          "original_form": "지금 나 소대엔 7명",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022896.1.25",
          "form": "후임이랑 잘 지내?",
          "original_form": "후임이랑 잘 지내?",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022896.1.26",
          "form": "후임들이 나 좋아해 안혼내거든",
          "original_form": "후임들이 나 좋아해 안혼내거든",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022896.1.27",
          "form": "꼰대 아니야?",
          "original_form": "꼰대 아니야?",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022896.1.28",
          "form": "얼마나 착한데 ㅡㅡ",
          "original_form": "얼마나 착한데 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022896.1.29",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022896.1.30",
          "form": "그래그래그래그래",
          "original_form": "그래그래그래그래",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022896.1.31",
          "form": "후임들 착해?",
          "original_form": "후임들 착해?",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022896.1.32",
