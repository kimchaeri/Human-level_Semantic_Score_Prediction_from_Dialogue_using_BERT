      "utterance": [
        {
          "id": "MDRW2100038765.1.1",
          "form": "씨제이는 쿠폰기간이 짧네요 ㅜㅠ",
          "original_form": "씨제이는 쿠폰기간이 짧네요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100038765.1.2",
          "form": "어제, 구매하려고 들어가니 기간 만료 ㅜㅠ",
          "original_form": "어제, 구매하려고 들어가니 기간 만료 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100038765.1.3",
          "form": "전 냉장고 터지고 올께 많아서 씨제이는 패스했거든요",
          "original_form": "전 냉장고 터지고 올께 많아서 씨제이는 패스했거든요",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038765.1.4",
          "form": "오늘 온스타일 적립금 3천 줘서 김밥김샀어요",
          "original_form": "오늘 온스타일 적립금 3천 줘서 김밥김샀어요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100038765.1.5",
          "form": "다시쿠폰발급하니 44퍼 ㅜㅠ",
          "original_form": "다시쿠폰발급하니 44퍼 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100038765.1.6",
          "form": "온스타일 3천 좋네요",
          "original_form": "온스타일 3천 좋네요",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100038765.1.7",
          "form": "저도 44퍼였어요",
          "original_form": "저도 44퍼였어요",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100038765.1.8",
          "form": "저는 전에 45퍼였거든요",
          "original_form": "저는 전에 45퍼였거든요",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100038765.1.9",
          "form": "천원정도 차이나더라구요 ㅎ",
          "original_form": "천원정도 차이나더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100038765.1.10",
          "form": "ㅎㅎ 별로 자이안나니 필요하신거있음 사세요",
          "original_form": "ㅎㅎ 별로 자이안나니 필요하신거있음 사세요",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100038765.1.11",
          "form": "차이^^",
          "original_form": "차이^^",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100038765.1.12",
          "form": "그래서 고민중요 ㅎ",
          "original_form": "그래서 고민중요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100038765.1.13",
          "form": "그냥 살까 다시 쿠폰받을수 있을때까지 기다릴까 ㅎ",
          "original_form": "그냥 살까 다시 쿠폰받을수 있을때까지 기다릴까 ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100038765.1.14",
          "form": "온 스타인ㅅ은 적립금 준다고 톡오죠?",
          "original_form": "온 스타인ㅅ은 적립금 준다고 톡오죠?",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100038765.1.15",
          "form": "네. 톡왔어요",
          "original_form": "네. 톡왔어요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100038765.1.16",
          "form": "저는 안왔으니 이번엔 없나보네요",
          "original_form": "저는 안왔으니 이번엔 없나보네요",
          "speaker_id": "1",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100038765.1.17",
          "form": "급하게 사실꺼없음 기다려보세요",
          "original_form": "급하게 사실꺼없음 기다려보세요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100038765.1.18",
          "form": "신랑도 없더라구요",
          "original_form": "신랑도 없더라구요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100038765.1.19",
          "form": "지난번에 물샀었는데",
          "original_form": "지난번에 물샀었는데",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100038765.1.20",
          "form": "또 주면 좋겠어요 ㅎ",
          "original_form": "또 주면 좋겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100038765.1.21",
          "form": "글게요. 온스타일은 무배가 많아서 좋더라구요",
          "original_form": "글게요. 온스타일은 무배가 많아서 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100038765.1.22",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100038765.1.23",
          "form": "어제 스토아 적립금으로도 물사서 친정보냈네요 ㅎ",
          "original_form": "어제 스토아 적립금으로도 물사서 친정보냈네요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100038765.1.24",
          "form": "잘하셨어요~~좋아하시겠어요",
          "original_form": "잘하셨어요~~좋아하시겠어요",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100038765.1.25",
          "form": "전 현미쌀~~",
          "original_form": "전 현미쌀~~",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100038765.1.26",
          "form": "무겁게 안가지고 올라가도된다고 좋아하시더라구요",
          "original_form": "무겁게 안가지고 올라가도된다고 좋아하시더라구요",
