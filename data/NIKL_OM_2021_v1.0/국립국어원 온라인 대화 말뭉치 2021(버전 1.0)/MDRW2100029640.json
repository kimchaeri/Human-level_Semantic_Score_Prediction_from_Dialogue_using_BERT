      "utterance": [
        {
          "id": "MDRW2100029640.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029640.1.2",
          "form": "아니요 안키워요",
          "original_form": "아니요 안키워요",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029640.1.3",
          "form": "반려동물 키우시나요??",
          "original_form": "반려동물 키우시나요??",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029640.1.4",
          "form": "구피요",
          "original_form": "구피요",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029640.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029640.1.6",
          "form": "구피면 그 지느러미 멋진 애들인가요ㅡㅡ",
          "original_form": "구피면 그 지느러미 멋진 애들인가요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029640.1.7",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029640.1.8",
          "form": "물 위로 올라와서 쉬기도 하는 애들이라고 들은 것 같아요",
          "original_form": "물 위로 올라와서 쉬기도 하는 애들이라고 들은 것 같아요",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029640.1.9",
          "form": "사육 난이도는 어때요??",
          "original_form": "사육 난이도는 어때요??",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029640.1.10",
          "form": "물관리가 힘들죠",
          "original_form": "물관리가 힘들죠",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029640.1.11",
          "form": "아 부지런해야 할 것 같아요 물관리는...",
          "original_form": "아 부지런해야 할 것 같아요 물관리는...",
          "speaker_id": "2",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029640.1.12",
          "form": "그렇됴",
          "original_form": "그렇됴",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029640.1.13",
          "form": "구피는 주인 알아보나요?",
          "original_form": "구피는 주인 알아보나요?",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029640.1.14",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029640.1.15",
          "form": "앗 그렇군요 ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "앗 그렇군요 ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029640.1.16",
          "form": "먹이는 뭘 먹이나요?",
          "original_form": "먹이는 뭘 먹이나요?",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029640.1.17",
          "form": "다이소에서 파는 사료 먹어요",
          "original_form": "다이소에서 파는 사료 먹어요",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029640.1.18",
          "form": "역시 다이소 없는 게 없네요",
          "original_form": "역시 다이소 없는 게 없네요",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029640.1.19",
          "form": "사료 가리는 건 없어요?",
          "original_form": "사료 가리는 건 없어요?",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029640.1.20",
          "form": "네 다 잘 먹더라구요",
          "original_form": "네 다 잘 먹더라구요",
