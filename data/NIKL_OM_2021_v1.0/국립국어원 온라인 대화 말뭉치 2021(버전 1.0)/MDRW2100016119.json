      "utterance": [
        {
          "id": "MDRW2100016119.1.1",
          "form": "다마고치 아세요?",
          "original_form": "다마고치 아세요?",
          "speaker_id": "1",
          "time": "20210826 09:54"
        },
        {
          "id": "MDRW2100016119.1.2",
          "form": "딸이 다마고치 갖고 싶대요 ㅡㅡ",
          "original_form": "딸이 다마고치 갖고 싶대요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 09:54"
        },
        {
          "id": "MDRW2100016119.1.3",
          "form": "다마고치 귀엽네요^^ 저희애들 어릴때 사줬었는데",
          "original_form": "다마고치 귀엽네요^^ 저희애들 어릴때 사줬었는데",
          "speaker_id": "2",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016119.1.4",
          "form": "ㅋㅋ그렇죠",
          "original_form": "ㅋㅋ그렇죠",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016119.1.5",
          "form": "갑자기 사고싶다고",
          "original_form": "갑자기 사고싶다고",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016119.1.6",
          "form": "근데 의외로 비싸요",
          "original_form": "근데 의외로 비싸요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016119.1.7",
          "form": "그래서 니돈으로 사라했어요",
          "original_form": "그래서 니돈으로 사라했어요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016119.1.8",
          "form": "맘에 걸려요 ㅡㅡ",
          "original_form": "맘에 걸려요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016119.1.9",
          "form": "그래도 아이들사고싶은거 다사주는것보단",
          "original_form": "그래도 아이들사고싶은거 다사주는것보단",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016119.1.10",
          "form": "경제관념가르치면서 직접사는것도 좋죠",
          "original_form": "경제관념가르치면서 직접사는것도 좋죠",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016119.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016119.1.12",
          "form": "그래서 요녀석 용돈 번다고 난리예요",
          "original_form": "그래서 요녀석 용돈 번다고 난리예요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016119.1.13",
          "form": "커피 타주고 천원씩받아요",
          "original_form": "커피 타주고 천원씩받아요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016119.1.14",
          "form": "갑부되서",
          "original_form": "갑부되서",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016119.1.15",
          "form": "금방사겠어요",
          "original_form": "금방사겠어요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016119.1.16",
          "form": "다마고치가 서로 결혼하고 애도낳고 하나봐요",
          "original_form": "다마고치가 서로 결혼하고 애도낳고 하나봐요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016119.1.17",
          "form": "아이들 뭐 사달라해요?",
          "original_form": "아이들 뭐 사달라해요?",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016119.1.18",
          "form": "귀엽다^^ 저도 어릴때부터 용돈주면서 용돈기입장인가도 하나사줬던기억이~ 딸은 잘하는데 아들은 잘 안했던거같아요",
          "original_form": "귀엽다^^ 저도 어릴때부터 용돈주면서 용돈기입장인가도 하나사줬던기억이~ 딸은 잘하는데 아들은 잘 안했던거같아요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016119.1.19",
          "form": "저희애들은 다커서 이제 그냥 알아서들 사요^^",
          "original_form": "저희애들은 다커서 이제 그냥 알아서들 사요^^",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016119.1.20",
          "form": "오히려 요즘은 인터넷쇼핑이 좋아서 아이들이 더 잘 아는거 같아요^^",
          "original_form": "오히려 요즘은 인터넷쇼핑이 좋아서 아이들이 더 잘 아는거 같아요^^",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016119.1.21",
          "form": "저희애는 고3 초6이라",
          "original_form": "저희애는 고3 초6이라",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016119.1.22",
          "form": "아직은 제가 사줘요",
          "original_form": "아직은 제가 사줘요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016119.1.23",
          "form": "맞아요 고등 아들만해도",
          "original_form": "맞아요 고등 아들만해도",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016119.1.24",
          "form": "자기돈으로 반은 알아서 쇼핑해요",
          "original_form": "자기돈으로 반은 알아서 쇼핑해요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016119.1.25",
          "form": "곧 이게 바뀔거예요 아이들이 최저가검색하고 그러더라구요",
          "original_form": "곧 이게 바뀔거예요 아이들이 최저가검색하고 그러더라구요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016119.1.26",
          "form": "제가 사주려 하면 요새 검열을 하네요 ㅡㅡ",
          "original_form": "제가 사주려 하면 요새 검열을 하네요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016119.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016119.1.28",
          "form": "어제 저한테 최저가 검색 깔아줬어요 ㅋㅋ",
          "original_form": "어제 저한테 최저가 검색 깔아줬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016119.1.29",
          "form": "점점 재손을 떠나요 살때도 물어봐서 사야하고",
          "original_form": "점점 재손을 떠나요 살때도 물어봐서 사야하고",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016119.1.30",
          "form": "주말엔",
          "original_form": "주말엔",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016119.1.31",
          "form": "용산가요",
          "original_form": "용산가요",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016119.1.32",
          "form": "정말 몇십년 만인듯하네요 ㅋㅋ",
          "original_form": "정말 몇십년 만인듯하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016119.1.33",
          "form": "예전엔 컴퓨터며 노트북 다 용산가서 샀는데",
          "original_form": "예전엔 컴퓨터며 노트북 다 용산가서 샀는데",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016119.1.34",
          "form": "요샌 온라인으로 사잖아요",
          "original_form": "요샌 온라인으로 사잖아요",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016119.1.35",
          "form": "간만에 작은애 태블릿 사러가요 ㅎㅎ",
          "original_form": "간만에 작은애 태블릿 사러가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016119.1.36",
          "form": "시대가 너무 많이바뀌었어요",
          "original_form": "시대가 너무 많이바뀌었어요",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016119.1.37",
          "form": "지금 태블릿 사러가시는거예요? 요즘 태블릿도 필수더라구요^^",
          "original_form": "지금 태블릿 사러가시는거예요? 요즘 태블릿도 필수더라구요^^",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016119.1.38",
          "form": "에 ㅎㅎ",
          "original_form": "에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016119.1.39",
          "form": "아이가 웹툰작가 한다고 ㅋㅋ",
          "original_form": "아이가 웹툰작가 한다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016119.1.40",
          "form": "그림그리기용으로 사려고요",
          "original_form": "그림그리기용으로 사려고요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.41",
          "form": "와~!! 너무멋진대요?",
          "original_form": "와~!! 너무멋진대요?",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.42",
          "form": "고건 아이가 보고 사야할것 같아서요 ㅎㅎ",
          "original_form": "고건 아이가 보고 사야할것 같아서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.43",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.44",
          "form": "그런거라면 적극지원해줘야죠^^",
          "original_form": "그런거라면 적극지원해줘야죠^^",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.45",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.46",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016119.1.47",
          "form": "공부도 하며 하면 좋겠어요 ㅎㅎ",
          "original_form": "공부도 하며 하면 좋겠어요 ㅎㅎ",
