      "utterance": [
        {
          "id": "MDRW2100036333.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036333.1.2",
          "form": "추어탕도",
          "original_form": "추어탕도",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036333.1.3",
          "form": "어릴땐 못먹엇는데 이제 잘먹어요",
          "original_form": "어릴땐 못먹엇는데 이제 잘먹어요",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036333.1.4",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036333.1.5",
          "form": "어릴땐 뭔가 그 가시가 목에 걸릴까봐",
          "original_form": "어릴땐 뭔가 그 가시가 목에 걸릴까봐",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036333.1.6",
          "form": "추어탕에 조금씩 씹히는 가시들이 무섭더라구요",
          "original_form": "추어탕에 조금씩 씹히는 가시들이 무섭더라구요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036333.1.7",
          "form": "어릴땐 이걸 무슨맛으로 먹나 햇는데",
          "original_form": "어릴땐 이걸 무슨맛으로 먹나 햇는데",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036333.1.8",
          "form": "이젠 너무 맛잇어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이젠 너무 맛잇어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036333.1.9",
          "form": "마자요 매콤한맛 더 추가해서 ㅋㅋㅋ 먹죠",
          "original_form": "마자요 매콤한맛 더 추가해서 ㅋㅋㅋ 먹죠",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036333.1.10",
          "form": "저는 어릴때 먹었던 장어에 가시가 많아서 너무 맛이 없는거예요!",
          "original_form": "저는 어릴때 먹었던 장어에 가시가 많아서 너무 맛이 없는거예요!",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100036333.1.11",
          "form": "그래서 먹을 기회가 있어도 안먹었거든요",
          "original_form": "그래서 먹을 기회가 있어도 안먹었거든요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036333.1.12",
          "form": "아... 내가 왜그랬을까",
          "original_form": "아... 내가 왜그랬을까",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036333.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ지난날 거부햇던 장어들이 너무 아깝죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ지난날 거부햇던 장어들이 너무 아깝죠",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036333.1.14",
          "form": "진짜 말그대로 지금은 없어서 못먹어요 ㅋㅋㅋㅋ",
          "original_form": "진짜 말그대로 지금은 없어서 못먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036333.1.15",
          "form": "전 어릴때 회를 별로 안좋아햇는데",
          "original_form": "전 어릴때 회를 별로 안좋아햇는데",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036333.1.16",
          "form": "나중에 땅을 치며 후회햇잖아요",
          "original_form": "나중에 땅을 치며 후회햇잖아요",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036333.1.17",
          "form": "이 맛잇는걸 왜 안먹엇니 왜",
          "original_form": "이 맛잇는걸 왜 안먹엇니 왜",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100036333.1.18",
          "form": "아아ㅠㅠ 회....",
          "original_form": "아아ㅠㅠ 회....",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036333.1.19",
          "form": "왜그러셨어요ㅠㅠㅠ 회를...",
          "original_form": "왜그러셨어요ㅠㅠㅠ 회를...",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036333.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036333.1.21",
          "form": "회하니까 생각나는데 저는 육회요!!!!",
          "original_form": "회하니까 생각나는데 저는 육회요!!!!",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100036333.1.22",
          "form": "결혼식가면 육회있잖아여",
          "original_form": "결혼식가면 육회있잖아여",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100036333.1.23",
          "form": "육회를 싫어하셧어요??",
          "original_form": "육회를 싫어하셧어요??",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100036333.1.24",
          "form": "저는 본능적으로 그 메뉴가 너무 궁금해서 이게 무슨음식이냐고 물어봤었거든요",
          "original_form": "저는 본능적으로 그 메뉴가 너무 궁금해서 이게 무슨음식이냐고 물어봤었거든요",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100036333.1.25",
          "form": "근데 저희엄마가!! 이건 너무 어린애들이 먹으면 안된다구",
          "original_form": "근데 저희엄마가!! 이건 너무 어린애들이 먹으면 안된다구",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100036333.1.26",
          "form": "본능적으로 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "본능적으로 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100036333.1.27",
          "form": "아앗...ㅠㅠ",
          "original_form": "아앗...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100036333.1.28",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100036333.1.29",
          "form": "그 안된다는 말은 어디서 나온건지 근거는 알수없었고...",
          "original_form": "그 안된다는 말은 어디서 나온건지 근거는 알수없었고...",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036333.1.30",
          "form": "저는 결국 뒤늦게 육회의 맛을 알았습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 결국 뒤늦게 육회의 맛을 알았습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036333.1.31",
          "form": "ㅜㅡㅜ",
          "original_form": "ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036333.1.32",
          "form": "저는 어렷을때부터 회는 별로 안좋아햇을지언정",
          "original_form": "저는 어렷을때부터 회는 별로 안좋아햇을지언정",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036333.1.33",
          "form": "육회랑 낙지탕탕이는 환장을 햇어요",
          "original_form": "육회랑 낙지탕탕이는 환장을 햇어요",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036333.1.34",
          "form": "없어서 못먹음 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "없어서 못먹음 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100036333.1.35",
          "form": "아아!!! 맛잘알",
          "original_form": "아아!!! 맛잘알{emoji:♡}",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036333.1.36",
          "form": "낙지탕탕이 완전이여",
          "original_form": "낙지탕탕이 완전이여",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036333.1.37",
          "form": "아 참기름솔솔...",
          "original_form": "아 참기름솔솔...",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100036333.1.38",
          "form": "너무맛있죠ㅠㅠ",
          "original_form": "너무맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100036333.1.39",
          "form": "그렇죠 깨도 솔솔 뿌려줘야죠",
          "original_form": "그렇죠 깨도 솔솔 뿌려줘야죠",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100036333.1.40",
          "form": "저는 낙지탕탕이는 어릴땐 질겁하다가",
          "original_form": "저는 낙지탕탕이는 어릴땐 질겁하다가",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036333.1.41",
          "form": "그때도 어렸는데 한순간에 태도를 바꿔 ㅋㅋㅋㅋ 엄청먹었던 기억이 나요",
          "original_form": "그때도 어렸는데 한순간에 태도를 바꿔 ㅋㅋㅋㅋ 엄청먹었던 기억이 나요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036333.1.42",
          "form": "진짜 환장하면서 먹었다..",
          "original_form": "진짜 환장하면서 먹었다..",
