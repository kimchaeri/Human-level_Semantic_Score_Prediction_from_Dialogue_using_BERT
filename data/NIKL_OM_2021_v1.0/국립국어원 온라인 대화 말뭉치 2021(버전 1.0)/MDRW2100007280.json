      "utterance": [
        {
          "id": "MDRW2100007280.1.1",
          "form": "누나 하고싶은 머리스타일 있어?",
          "original_form": "누나 하고싶은 머리스타일 있어?",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007280.1.2",
          "form": "나 사실 짧게 자르고싶어",
          "original_form": "나 사실 짧게 자르고싶어",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007280.1.3",
          "form": "단발?",
          "original_form": "단발?",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007280.1.4",
          "form": "응 아니면 아예 투블럭도 해보고싶어",
          "original_form": "응 아니면 아예 투블럭도 해보고싶어",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007280.1.5",
          "form": "숏컷이 나쁜 말이야?",
          "original_form": "숏컷이 나쁜 말이야?",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007280.1.6",
          "form": "아 아니네 투블럭 말고 숏컷",
          "original_form": "아 아니네 투블럭 말고 숏컷",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007280.1.7",
          "form": "숏컷이 길이가 얼마나 짧은거야?",
          "original_form": "숏컷이 길이가 얼마나 짧은거야?",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007280.1.8",
          "form": "나 어릴 때 숏컷한 적 있는데",
          "original_form": "나 어릴 때 숏컷한 적 있는데",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007280.1.9",
          "form": "일반 남자들 머리 정도?",
          "original_form": "일반 남자들 머리 정도?",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007280.1.10",
          "form": "단발은 별로야?",
          "original_form": "단발은 별로야?",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007280.1.11",
          "form": "단발도 좋은데 더 짧으면 더 편해",
          "original_form": "단발도 좋은데 더 짧으면 더 편해",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007280.1.12",
          "form": "난 너무 짧으면 어색할 것 같아",
          "original_form": "난 너무 짧으면 어색할 것 같아",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007280.1.13",
          "form": "응 실제로 자를 생각은 없어ㅋㅋㅋ",
          "original_form": "응 실제로 자를 생각은 없어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007280.1.14",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007280.1.15",
          "form": "넌 하고싶은 헤어스타일 없어?",
          "original_form": "넌 하고싶은 헤어스타일 없어?",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007280.1.16",
          "form": "난 파마하고 싶어",
          "original_form": "난 파마하고 싶어",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007280.1.17",
          "form": "베이비펌?",
          "original_form": "베이비펌?",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007280.1.18",
          "form": "그냥 아무펌이나 상관없어",
          "original_form": "그냥 아무펌이나 상관없어",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007280.1.19",
          "form": "요즘 피부가 너무 안좋아",
          "original_form": "요즘 피부가 너무 안좋아",
          "speaker_id": "2",
          "time": "20210823 14:39"
        }
      ]
