      "utterance": [
        {
          "id": "MDRW2100005979.1.1",
          "form": "악 왕날편 재밌죵",
          "original_form": "악 왕날편 재밌죵",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.2",
          "form": "왕십리에서 이사를 가셔가지구 근데...ㅠ 안하신다구 하더라구욤",
          "original_form": "왕십리에서 이사를 가셔가지구 근데...ㅠ 안하신다구 하더라구욤",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.3",
          "form": "아 증망",
          "original_form": "아 증망",
          "speaker_id": "2",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.4",
          "form": "티알피지 하는것도 보셔요!",
          "original_form": "티알피지 하는것도 보셔요!",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.5",
          "form": "내맴!",
          "original_form": "내맴!",
          "speaker_id": "2",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.6",
          "form": "게임은 몰라서요",
          "original_form": "게임은 몰라서요",
          "speaker_id": "2",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.7",
          "form": "고것도 시간 뗴우기 잼썼어요",
          "original_form": "고것도 시간 뗴우기 잼썼어요",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.8",
          "form": "약간 역할극 하는건데",
          "original_form": "약간 역할극 하는건데",
          "speaker_id": "1",
          "time": "20210907 18:15"
        },
        {
          "id": "MDRW2100005979.1.9",
          "form": "침착맨 삼국지 완전판도 재밌었구...",
          "original_form": "침착맨 삼국지 완전판도 재밌었구...",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005979.1.10",
          "form": "헉 근데 저 팬카페는 가입안했었는데",
          "original_form": "헉 근데 저 팬카페는 가입안했었는데",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005979.1.11",
          "form": "팬카페는 폐쇄됐다구 하더라구요",
          "original_form": "팬카페는 폐쇄됐다구 하더라구요",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005979.1.12",
          "form": "아ㅜ왜여",
          "original_form": "아ㅜ왜여",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005979.1.13",
          "form": "뇌절치는 친구들이 많아서 그랬다구 하더라구욤 ㅠ",
          "original_form": "뇌절치는 친구들이 많아서 그랬다구 하더라구욤 ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005979.1.14",
          "form": "침착맨 영상 나오는 다른 분들도 좋아하시는 편이셔요?",
          "original_form": "침착맨 영상 나오는 다른 분들도 좋아하시는 편이셔요?",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005979.1.15",
          "form": "아 침착맨님 은근히 유교적이심",
          "original_form": "아 침착맨님 은근히 유교적이심",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005979.1.16",
          "form": "침펄풍이 완전체다 이말이다~",
          "original_form": "침펄풍이 완전체다 이말이다~",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100005979.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:18"
        },
        {
          "id": "MDRW2100005979.1.18",
          "form": "그쳐그쳐~~!!",
          "original_form": "그쳐그쳐~~!!",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005979.1.19",
          "form": "ㅋㅋㅋㅋㅋ주변 친구분들은 안보시나봐요!",
          "original_form": "ㅋㅋㅋㅋㅋ주변 친구분들은 안보시나봐요!",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005979.1.20",
          "form": "저도 근데 주변에 보는 사람 없어요 ㅠ",
          "original_form": "저도 근데 주변에 보는 사람 없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005979.1.21",
          "form": "넹?",
          "original_form": "넹?",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005979.1.22",
          "form": "왜여!!!",
          "original_form": "왜여!!!",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005979.1.23",
          "form": "그니까요!! ㅠㅠㅠ",
          "original_form": "그니까요!! ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005979.1.24",
          "form": "뭔가 코드가 맞아야 하나봐욤",
          "original_form": "뭔가 코드가 맞아야 하나봐욤",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005979.1.25",
          "form": "그래도 상대적으로 클린한편이라 좋은디",
          "original_form": "그래도 상대적으로 클린한편이라 좋은디",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005979.1.26",
          "form": "근데 백예린님 노래 그건 좀 잘못했어요",
          "original_form": "근데 백예린님 노래 그건 좀 잘못했어요",
          "speaker_id": "2",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005979.1.27",
          "form": "ㅋ ㅋㅋㅋ ㅋㅋ  ㅋㅋ",
          "original_form": "ㅋ ㅋㅋㅋ ㅋㅋ  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005979.1.28",
          "form": "그건 아마 우리의 잘머ㅗㅅ은 아닐거야",
          "original_form": "그건 아마 우리의 잘머ㅗㅅ은 아닐거야",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005979.1.29",
          "form": "그거요?!",
          "original_form": "그거요?!",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005979.1.30",
          "form": "네 그거 알려줬다가 짜증나게 중독성까지 있다고",
          "original_form": "네 그거 알려줬다가 짜증나게 중독성까지 있다고",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005979.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005979.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005979.1.33",
          "form": "최근에 영상 가루삼겹살",
          "original_form": "최근에 영상 가루삼겹살",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005979.1.34",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100005979.1.35",
