      "utterance": [
        {
          "id": "MDRW2100034349.1.1",
          "form": "사진 잘찍고",
          "original_form": "사진 잘찍고",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100034349.1.2",
          "form": "아뇨 못했어요",
          "original_form": "아뇨 못했어요",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100034349.1.3",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034349.1.4",
          "form": "못파셨어요?",
          "original_form": "못파셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034349.1.5",
          "form": "잠이 너무와서 어제 폰들고잠들었어요",
          "original_form": "잠이 너무와서 어제 폰들고잠들었어요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100034349.1.6",
          "form": "사진을 찍어야 당근에 팔텐데 ㅡㅡ잠들고 못하셨군요",
          "original_form": "사진을 찍어야 당근에 팔텐데 ㅡㅡ잠들고 못하셨군요",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034349.1.7",
          "form": "머리속에는 이것두팔고 저것두팔고",
          "original_form": "머리속에는 이것두팔고 저것두팔고",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034349.1.8",
          "form": "그럴수 있어요",
          "original_form": "그럴수 있어요",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100034349.1.9",
          "form": "몸은 쇼파 껌딱지요 ㅋ",
          "original_form": "몸은 쇼파 껌딱지요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.10",
          "form": "맞아요 맘은 그런데 몸이 안따라오ㅓ요",
          "original_form": "맞아요 맘은 그런데 몸이 안따라오ㅓ요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.12",
          "form": "파는것도 체력이 되야해요",
          "original_form": "파는것도 체력이 되야해요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.13",
          "form": "맞아요 ㅎ",
          "original_form": "맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.14",
          "form": "얼마전에 고구마샀는데요",
          "original_form": "얼마전에 고구마샀는데요",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.15",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.16",
          "form": "맛이 없어서 큰일이예요 ㅜㅠ",
          "original_form": "맛이 없어서 큰일이예요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100034349.1.17",
          "form": "고구마철이 다가와서 샀는데",
          "original_form": "고구마철이 다가와서 샀는데",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.18",
          "form": "아 물고구마  사셨나보다 ㅜㅜ",
          "original_form": "아 물고구마  사셨나보다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.19",
          "form": "이리 맛없을줄이야...",
          "original_form": "이리 맛없을줄이야...",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.20",
          "form": "튀겨야 할까요 ㅜㅜ",
          "original_form": "튀겨야 할까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.21",
          "form": "언제다먹죠?ㅎㅈㅅ",
          "original_form": "언제다먹죠?ㅎㅈㅅ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.22",
          "form": "그수틱을 하면 어떨꺼요 ㅜㅜ",
          "original_form": "그수틱을 하면 어떨꺼요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.23",
          "form": "스틱",
          "original_form": "스틱",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.24",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.25",
          "form": "고구마스틱?",
          "original_form": "고구마스틱?",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.26",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.27",
          "form": "감자튀김처럼요?",
          "original_form": "감자튀김처럼요?",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100034349.1.28",
          "form": "마지막에 소금 살짝",
          "original_form": "마지막에 소금 살짝",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034349.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034349.1.30",
          "form": "휴게소 처럼 해서 먹음 좀나아요",
          "original_form": "휴게소 처럼 해서 먹음 좀나아요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034349.1.31",
          "form": "금방 먹어요",
          "original_form": "금방 먹어요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100034349.1.32",
          "form": "그나마 튀김하면 산거 안버릴거예요 ㅎㅎ",
          "original_form": "그나마 튀김하면 산거 안버릴거예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.33",
          "form": "휴게소에서 안먹어봐서",
          "original_form": "휴게소에서 안먹어봐서",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.34",
          "form": "고구마가 한박스 사서 먹기 힘들어요 ㅜㅜ",
          "original_form": "고구마가 한박스 사서 먹기 힘들어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.35",
          "form": "튀김해야겠어요 ㅎ",
          "original_form": "튀김해야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.36",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.37",
          "form": "자주 먹었어요",
          "original_form": "자주 먹었어요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.38",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100034349.1.39",
          "form": "얇게 채쳐서 ㅎㅎ",
          "original_form": "얇게 채쳐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034349.1.40",
          "form": "오~ 팁감사해요 소금간",
          "original_form": "오~ 팁감사해요 소금간",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034349.1.41",
          "form": "네 ㅋㅋ소금이 ㅋㅋ",
          "original_form": "네 ㅋㅋ소금이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034349.1.42",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034349.1.43",
          "form": "그럼잘먹을듯요 갑자기 희망이 보이네요 ㅎ",
          "original_form": "그럼잘먹을듯요 갑자기 희망이 보이네요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100034349.1.44",
          "form": "네 ㅎㅎ꼭 맛나면 좋겠네요",
          "original_form": "네 ㅎㅎ꼭 맛나면 좋겠네요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034349.1.45",
          "form": "사진도 후딱 찍어서",
          "original_form": "사진도 후딱 찍어서",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034349.1.46",
          "form": "당근에 비싸게 파세요",
          "original_form": "당근에 비싸게 파세요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034349.1.47",
          "form": "네 감사해요",
          "original_form": "네 감사해요",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034349.1.48",
          "form": "오늘도 수고하셨어요",
          "original_form": "오늘도 수고하셨어요",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100034349.1.49",
          "form": "굿밤되세용~^^",
          "original_form": "굿밤되세용~^^",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100034349.1.50",
