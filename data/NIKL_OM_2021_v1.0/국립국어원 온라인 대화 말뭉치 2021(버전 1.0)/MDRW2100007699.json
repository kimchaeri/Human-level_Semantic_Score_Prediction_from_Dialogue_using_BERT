      "utterance": [
        {
          "id": "MDRW2100007699.1.1",
          "form": "안년하세요 최근에 쇼핑한거 있으세요?",
          "original_form": "안년하세요 최근에 쇼핑한거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007699.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007699.1.3",
          "form": "네 최근에는 쌀샀어요.ㅋ",
          "original_form": "네 최근에는 쌀샀어요.ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007699.1.4",
          "form": "냉장고에 음식들 채워넣는다고 장보고왔어요.ㅋㅋ",
          "original_form": "냉장고에 음식들 채워넣는다고 장보고왔어요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007699.1.5",
          "form": "최근에 뭐사신거있으세요?",
          "original_form": "최근에 뭐사신거있으세요?",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007699.1.6",
          "form": "아 장보기필요하죠 ㅋㅋ 저는 가을옷 샀어요ㅎㅎ",
          "original_form": "아 장보기필요하죠 ㅋㅋ 저는 가을옷 샀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007699.1.7",
          "form": "아 가을옷ㅋㅋ맞아요.",
          "original_form": "아 가을옷ㅋㅋ맞아요.",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007699.1.8",
          "form": "이제사야되네요ㅋㅋ",
          "original_form": "이제사야되네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007699.1.9",
          "form": "몇 벌사셨어요?ㅋ",
          "original_form": "몇 벌사셨어요?ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007699.1.10",
          "form": "그냥 가디건이랑 코트 샀는데ㅋㅋ가을이 짧아서 입을 날이 얼마나 될지ㅎㅎ",
          "original_form": "그냥 가디건이랑 코트 샀는데ㅋㅋ가을이 짧아서 입을 날이 얼마나 될지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007699.1.11",
          "form": "ㅋㅋㅋ저도 사야겠네요.",
          "original_form": "ㅋㅋㅋ저도 사야겠네요.",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007699.1.12",
          "form": "옷은 왜 매번 없는것같은지 ㅋㅋ",
          "original_form": "옷은 왜 매번 없는것같은지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007699.1.13",
          "form": "가디건을 분명 산거같은데ㅋㅋ없더라구요",
          "original_form": "가디건을 분명 산거같은데ㅋㅋ없더라구요",
          "speaker_id": "1",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007699.1.14",
          "form": "ㅋㅋㅋ어디갔을까요?ㅋㅋ",
          "original_form": "ㅋㅋㅋ어디갔을까요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007699.1.15",
          "form": "분명 버리진않았는데ㅎㅎㅎ 다 새로 사야하네요 ㅋㅋ",
          "original_form": "분명 버리진않았는데ㅎㅎㅎ 다 새로 사야하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007699.1.16",
          "form": "그러니깐요ㅋㅋㅋ",
          "original_form": "그러니깐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007699.1.17",
          "form": "아이들도 커서 또 사야되는 계절이왔네요ㅋㅋ",
          "original_form": "아이들도 커서 또 사야되는 계절이왔네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007699.1.18",
          "form": "옷값 장난아니에요.ㅋㅋ",
          "original_form": "옷값 장난아니에요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007699.1.19",
          "form": "진짜...항상 새로 사야하니ㅠㅠ",
          "original_form": "진짜...항상 새로 사야하니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007699.1.20",
          "form": "그러니깐요 ㅋㅋㅋ 옷도 사야되고 ㅋㅋ 생필품도 사야되고",
          "original_form": "그러니깐요 ㅋㅋㅋ 옷도 사야되고 ㅋㅋ 생필품도 사야되고",
          "speaker_id": "2",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007699.1.21",
          "form": "살건많아요ㅋㅋ",
          "original_form": "살건많아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007699.1.22",
          "form": "돈은 한정되어 있는데ㅠㅠㅋㅋㅋ",
          "original_form": "돈은 한정되어 있는데ㅠㅠㅋㅋㅋ",
