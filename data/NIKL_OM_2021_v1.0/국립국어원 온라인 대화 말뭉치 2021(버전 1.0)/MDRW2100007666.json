        },
        {
          "id": "MDRW2100007666.1.2",
          "form": "네 일해요",
          "original_form": "네 일해요",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007666.1.3",
          "form": "저도 직장인",
          "original_form": "저도 직장인",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007666.1.4",
          "form": "일은 적성에 맞으세요?",
          "original_form": "일은 적성에 맞으세요?",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007666.1.5",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007666.1.6",
          "form": "저도 적성보다 그냥 흘러흘러 하게되는거같아요",
          "original_form": "저도 적성보다 그냥 흘러흘러 하게되는거같아요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007666.1.7",
          "form": "그래도 재미가 있었으멈",
          "original_form": "그래도 재미가 있었으멈",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007666.1.8",
          "form": "좋겠죠 요즘은 재미가 없네요 ㅎ",
          "original_form": "좋겠죠 요즘은 재미가 없네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007666.1.9",
          "form": "낙이 없어요",
          "original_form": "낙이 없어요",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007666.1.10",
          "form": "열정도 안생기구 ㅎㅎ",
          "original_form": "열정도 안생기구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007666.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007666.1.12",
          "form": "열정이라도 있으면 잘할수있는데",
          "original_form": "열정이라도 있으면 잘할수있는데",
