      "utterance": [
        {
          "id": "MDRW2100000764.1.1",
          "form": "날이 비올거같다",
          "original_form": "날이 비올거같다",
          "speaker_id": "1",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000764.1.2",
          "form": "마스크쓰니까 덥다",
          "original_form": "마스크쓰니까 덥다",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000764.1.3",
          "form": "ㅇㅇ 답답해",
          "original_form": "ㅇㅇ 답답해",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000764.1.4",
          "form": "여름이니 더 그런듯",
          "original_form": "여름이니 더 그런듯",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000764.1.5",
          "form": "덴탈쓰다가 80쓰니까 더 답답",
          "original_form": "덴탈쓰다가 80쓰니까 더 답답",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000764.1.6",
          "form": "난 94야",
          "original_form": "난 94야",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000764.1.7",
          "form": "와우 그래도 바람 좀 불긴하네",
          "original_form": "와우 그래도 바람 좀 불긴하네",
          "speaker_id": "1",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000764.1.8",
          "form": "빨리 여름가면 좋겠음",
          "original_form": "빨리 여름가면 좋겠음",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000764.1.9",
          "form": "그래도 매미가 좀 조용해짐",
          "original_form": "그래도 매미가 좀 조용해짐",
          "speaker_id": "1",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000764.1.10",
          "form": "아직 그래도 아파트시끄러움 특히 아침에",
          "original_form": "아직 그래도 아파트시끄러움 특히 아침에",
          "speaker_id": "2",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000764.1.11",
          "form": "한여름보다는 나아졌어 ᆢ",
          "original_form": "한여름보다는 나아졌어 ᆢ",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000764.1.12",
          "form": "가을이 오는게좋다",
          "original_form": "가을이 오는게좋다",
          "speaker_id": "2",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000764.1.13",
          "form": "겨울 금방이겠지",
          "original_form": "겨울 금방이겠지",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000764.1.14",
          "form": "구름 짠뜩 꼈다",
          "original_form": "구름 짠뜩 꼈다",
