      "utterance": [
        {
          "id": "MDRW2100041311.1.1",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100041311.1.2",
          "form": "엄마가 외가에 다녀오셔서",
          "original_form": "엄마가 외가에 다녀오셔서",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100041311.1.3",
          "form": "오랜만에 외할머니 반찬 먹었네용",
          "original_form": "오랜만에 외할머니 반찬 먹었네용",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041311.1.4",
          "form": "와 외할머니 반찬이라니 맛있었겠네요-",
          "original_form": "와 외할머니 반찬이라니 맛있었겠네요{emoji:♡}-{emoji:♡}",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041311.1.5",
          "form": "네! 너무...ㅠ 저희 외할머니 요리 엄청 잘 하셔서요",
          "original_form": "네! 너무...ㅠ 저희 외할머니 요리 엄청 잘 하셔서요",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100041311.1.6",
          "form": "넘 좋았어요 ㅋㅋㅋ",
          "original_form": "넘 좋았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100041311.1.7",
          "form": "ㅜㅜ 집밥짱입니다  할머님손맛이라면 더더욱 좋으셨을거같아요ㅋㅋㅋ 요리엄청잘하신다니ㅜㅜ와",
          "original_form": "ㅜㅜ 집밥짱입니다  할머님손맛이라면 더더욱 좋으셨을거같아요ㅋㅋㅋ 요리엄청잘하신다니ㅜㅜ와",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100041311.1.8",
          "form": "염치없이 잔뜻 받아왔어요 ㅋㅋㅋ",
          "original_form": "염치없이 잔뜻 받아왔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100041311.1.9",
          "form": "할머니의 몇십년 내공 담긴 손맛은 못 따라오는 것 같아요",
          "original_form": "할머니의 몇십년 내공 담긴 손맛은 못 따라오는 것 같아요",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100041311.1.10",
          "form": "내공은 무시할수없지요..ㅋㅋㅋ!!  정말 좋았을거같아여ㅋㅋㅋ밥두공기 때리셨어야죠!",
          "original_form": "내공은 무시할수없지요..ㅋㅋㅋ!!  정말 좋았을거같아여ㅋㅋㅋ밥두공기 때리셨어야죠!",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041311.1.11",
          "form": "ㅋㅋㅋㅋㅋ그러고 싶었는데 점심도 많이 먹어서...",
          "original_form": "ㅋㅋㅋㅋㅋ그러고 싶었는데 점심도 많이 먹어서...",
          "speaker_id": "2",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100041311.1.12",
          "form": "아직까지 배가 부른 것 같아요",
          "original_form": "아직까지 배가 부른 것 같아요",
          "speaker_id": "2",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100041311.1.13",
          "form": "작년 김장김치가 똑 떨어져서 배추김치 새로 받아왓거든요",
          "original_form": "작년 김장김치가 똑 떨어져서 배추김치 새로 받아왓거든요",
          "speaker_id": "2",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100041311.1.14",
          "form": "진짜 오랜만에 돼지고기 김치찌개 먹었더니",
          "original_form": "진짜 오랜만에 돼지고기 김치찌개 먹었더니",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100041311.1.15",
          "form": "넘 맛있더라구요...",
          "original_form": "넘 맛있더라구요...",
          "speaker_id": "2",
          "time": "20210906 00:03"
        },
        {
          "id": "MDRW2100041311.1.16",
          "form": "으아 ㅜㅜ최고의메뉴네요ㅋㅋㄱ으아 김치  돼지고기김치찌개ㅜㅜ",
          "original_form": "으아 ㅜㅜ최고의메뉴네요ㅋㅋㄱ으아 김치{emoji:♡}{emoji:♡}{emoji:♡}{emoji:♡}  돼지고기김치찌개ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 01:27"
        },
        {
          "id": "MDRW2100041311.1.17",
          "form": "돼지고기 김치찌개 완전 한국인의 소울푸드 아닐까요?",
          "original_form": "돼지고기 김치찌개 완전 한국인의 소울푸드 아닐까요?",
          "speaker_id": "2",
          "time": "20210906 07:19"
        },
        {
          "id": "MDRW2100041311.1.18",
          "form": "혹시 김치찌개에 당면 넣어 먹는 거 아세요?",
          "original_form": "혹시 김치찌개에 당면 넣어 먹는 거 아세요?",
          "speaker_id": "2",
          "time": "20210906 07:19"
        },
        {
          "id": "MDRW2100041311.1.19",
          "form": "네네알아용!",
          "original_form": "네네알아용!",
          "speaker_id": "1",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041311.1.20",
          "form": "그렇게드시나용?",
          "original_form": "그렇게드시나용?",
          "speaker_id": "1",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041311.1.21",
          "form": "헉 네 제가 엄청엄천 좋아하는데요",
          "original_form": "헉 네 제가 엄청엄천 좋아하는데요",
          "speaker_id": "2",
          "time": "20210906 13:12"
        },
        {
          "id": "MDRW2100041311.1.22",
          "form": "제 다른 친구들은 다 모르더라구요 ㅠ",
          "original_form": "제 다른 친구들은 다 모르더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 13:12"
        },
        {
          "id": "MDRW2100041311.1.23",
          "form": "유명한 줄 알았는데... 아시는 분 만나니 반가워요",
          "original_form": "유명한 줄 알았는데... 아시는 분 만나니 반가워요",
          "speaker_id": "2",
          "time": "20210906 13:12"
        },
        {
          "id": "MDRW2100041311.1.24",
          "form": "악ㅋㄲㄱ\n사실 그렇게많이 안먹긴한데 알긴알죵ㅋㅋ그렇게하는식당가본거같아용ㅋㅋ",
          "original_form": "악ㅋㄲㄱ\n사실 그렇게많이 안먹긴한데 알긴알죵ㅋㅋ그렇게하는식당가본거같아용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:29"
        },
        {
          "id": "MDRW2100041311.1.25",
          "form": "하 얘기하다 보니까 땡기네용... ㅠㅠ",
          "original_form": "하 얘기하다 보니까 땡기네용... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100041311.1.26",
          "form": "전 그런 의미로 부대찌개도 좋아해요",
          "original_form": "전 그런 의미로 부대찌개도 좋아해요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100041311.1.27",
          "form": "부대찌개도맛있죠ㅜㅜ..하..",
          "original_form": "부대찌개도맛있죠ㅜㅜ..하..",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100041311.1.28",
          "form": "밥땡깁니다..",
          "original_form": "밥땡깁니다..",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100041311.1.29",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100041311.1.30",
          "form": "하 야심한 밤에 부찌 이야기 넘 좋네요 ㅋㅋㅋㅋㅋㅋ",
