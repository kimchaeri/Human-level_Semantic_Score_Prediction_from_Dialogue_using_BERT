      "utterance": [
        {
          "id": "MDRW2100028158.1.1",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028158.1.2",
          "form": "전 부슬비정도는 그냥 맞아요 ㅎㅎ",
          "original_form": "전 부슬비정도는 그냥 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028158.1.3",
          "form": "힉 저두요",
          "original_form": "힉 저두요",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028158.1.4",
          "form": "근데 집가야하는데 소나기내릴땐 당황스러워욬ㅋㅋㅋ",
          "original_form": "근데 집가야하는데 소나기내릴땐 당황스러워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028158.1.5",
          "form": "소나기면 좀 기다리면 그칠수도 있는데",
          "original_form": "소나기면 좀 기다리면 그칠수도 있는데",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028158.1.6",
          "form": "저번에 좀 잦아들겠지 하고 기다리다가 더 와서 ㅠㅠ",
          "original_form": "저번에 좀 잦아들겠지 하고 기다리다가 더 와서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028158.1.7",
          "form": "혹시 엊그제요?ㅋ쿠ㅜㅡㅜ",
          "original_form": "혹시 엊그제요?ㅋ쿠ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100028158.1.8",
          "form": "엊그제는 아니고",
          "original_form": "엊그제는 아니고",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100028158.1.9",
          "form": "거의 한달전이네요",
          "original_form": "거의 한달전이네요",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028158.1.10",
          "form": "좀 억울했던 기억이 남어서...ㅎㅎ",
          "original_form": "좀 억울했던 기억이 남어서...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100028158.1.11",
          "form": "아ㅜㅜㅠ",
          "original_form": "아ㅜㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028158.1.12",
          "form": "그래도 오늘은 날씨 좋은편이어서",
          "original_form": "그래도 오늘은 날씨 좋은편이어서",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028158.1.13",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028158.1.14",
          "form": "네. 오늘 날씨가 좋아서 다행입니다.",
          "original_form": "네. 오늘 날씨가 좋아서 다행입니다.",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028158.1.15",
          "form": "그래도 이번 주말까진 비 안올거라 하네요",
          "original_form": "그래도 이번 주말까진 비 안올거라 하네요",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028158.1.16",
          "form": "헉 완전 좋아요",
          "original_form": "헉 완전 좋아요",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028158.1.17",
          "form": "날도 전보다 많이 시원해지고",
          "original_form": "날도 전보다 많이 시원해지고",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028158.1.18",
          "form": "비도안오네요ㅋㅋ",
          "original_form": "비도안오네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028158.1.19",
          "form": "근데 담주엔 온대요 ㅠㅠ",
          "original_form": "근데 담주엔 온대요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028158.1.20",
          "form": "앗 안와도되는데....",
          "original_form": "앗 안와도되는데....",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028158.1.21",
          "form": "그래도 어쩔수 없죠.",
          "original_form": "그래도 어쩔수 없죠.",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028158.1.22",
          "form": "출퇴근길만 피하길 비는 수밖에 ㅠㅠ",
          "original_form": "출퇴근길만 피하길 비는 수밖에 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028158.1.23",
          "form": "오늘 출근길 날씨 좋은것같아요!!",
          "original_form": "오늘 출근길 날씨 좋은것같아요!!",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100028158.1.24",
          "form": "오늘 밤에 창문 열구 잤는데 딱 좋더라구요",
          "original_form": "오늘 밤에 창문 열구 잤는데 딱 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 09:14"
        },
        {
          "id": "MDRW2100028158.1.25",
          "form": "네 맞아요.",
          "original_form": "네 맞아요.",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100028158.1.26",
          "form": "적당히 얇은 긴팔이 잘 어울리는 화사한 날씨!",
          "original_form": "적당히 얇은 긴팔이 잘 어울리는 화사한 날씨!",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100028158.1.27",
          "form": "이제 가을이랑 가까워지네요",
          "original_form": "이제 가을이랑 가까워지네요",
