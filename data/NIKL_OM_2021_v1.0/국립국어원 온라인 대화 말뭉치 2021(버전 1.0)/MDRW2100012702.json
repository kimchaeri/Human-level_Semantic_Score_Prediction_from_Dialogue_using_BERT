      "utterance": [
        {
          "id": "MDRW2100012702.1.1",
          "form": "최근에 아픈 적 있어요?",
          "original_form": "최근에 아픈 적 있어요?",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012702.1.2",
          "form": "아니요 강아지키우시나요?",
          "original_form": "아니요 강아지키우시나요?",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012702.1.3",
          "form": "저희 부모님께서 키우셔요",
          "original_form": "저희 부모님께서 키우셔요",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012702.1.4",
          "form": "저도 키우고싶은데 혼자 자취를 하다보니 원룸에서 키우기가..ㅜㅜ",
          "original_form": "저도 키우고싶은데 혼자 자취를 하다보니 원룸에서 키우기가..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 00:13"
        },
        {
          "id": "MDRW2100012702.1.5",
          "form": "저도 너무",
          "original_form": "저도 너무",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012702.1.6",
          "form": "키우고싶어요ㅠㅠ",
          "original_form": "키우고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100012702.1.7",
          "form": "그쳐 ㅜㅜ 어떤 거 키우고 싶어요?",
          "original_form": "그쳐 ㅜㅜ 어떤 거 키우고 싶어요?",
          "speaker_id": "1",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100012702.1.8",
          "form": "저 강아지요",
          "original_form": "저 강아지요",
          "speaker_id": "2",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012702.1.9",
          "form": "포메요ㅠㅠ",
          "original_form": "포메요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012702.1.10",
          "form": "아 포메라이언 너무 귀여워요 ㅠㅜ",
          "original_form": "아 포메라이언 너무 귀여워요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012702.1.11",
          "form": "근데 털빠짐이 심해서 키우기 어렵다던데요..흑",
          "original_form": "근데 털빠짐이 심해서 키우기 어렵다던데요..흑",
          "speaker_id": "1",
          "time": "20210825 00:34"
        },
        {
          "id": "MDRW2100012702.1.12",
          "form": "ㅠㅠ저는",
          "original_form": "ㅠㅠ저는",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012702.1.13",
          "form": "그것도 감수할거예요",
          "original_form": "그것도 감수할거예요",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012702.1.14",
          "form": "알러지는 없으시죠? 털알러지 있으면 눈물..ㅜㅠ",
          "original_form": "알러지는 없으시죠? 털알러지 있으면 눈물..ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100012702.1.15",
          "form": "네네 알러지는없는데",
          "original_form": "네네 알러지는없는데",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012702.1.16",
          "form": "사실 있는지없는지모르겠네요",
          "original_form": "사실 있는지없는지모르겠네요",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012702.1.17",
          "form": "아~아직 멍뭉이 키워본 적 없으신가봐요?",
          "original_form": "아~아직 멍뭉이 키워본 적 없으신가봐요?",
          "speaker_id": "1",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012702.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012702.1.19",
          "form": "좋아하긴엄청좋아하는데",
          "original_form": "좋아하긴엄청좋아하는데",
          "speaker_id": "2",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012702.1.20",
          "form": "아직은요",
          "original_form": "아직은요",
          "speaker_id": "2",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012702.1.21",
          "form": "강아지 막상 키우면 힘들다고 하더라구요 ㅜㅜ",
          "original_form": "강아지 막상 키우면 힘들다고 하더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100012702.1.22",
          "form": "저는 집돌이라 매일 산책 시킬 자신 없어서 포기..",
          "original_form": "저는 집돌이라 매일 산책 시킬 자신 없어서 포기..",
          "speaker_id": "1",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100012702.1.23",
          "form": "저는 산책",
          "original_form": "저는 산책",
          "speaker_id": "2",
          "time": "20210825 01:49"
        },
        {
          "id": "MDRW2100012702.1.24",
          "form": "3번시켜줄 자신있어요!",
          "original_form": "3번시켜줄 자신있어요!",
          "speaker_id": "2",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100012702.1.25",
          "form": "헉 하루에 산책 3번이요?",
          "original_form": "헉 하루에 산책 3번이요?",
          "speaker_id": "1",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100012702.1.26",
          "form": "강아지가 진짜 좋아하겠다 ㅜ",
          "original_form": "강아지가 진짜 좋아하겠다 ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100012702.1.27",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012702.1.28",
          "form": "일단근데",
          "original_form": "일단근데",
          "speaker_id": "2",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012702.1.29",
          "form": "강아지가 없으니요ㅠ",
          "original_form": "강아지가 없으니요ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:55"
        }
      ]
    }
