      "utterance": [
        {
          "id": "MDRW2100031136.1.1",
          "form": "강아지 키우고 오셨어요",
          "original_form": "강아지 키우고 오셨어요",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100031136.1.2",
          "form": "강아지 키워 봤어요",
          "original_form": "강아지 키워 봤어요",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100031136.1.3",
          "form": "어떤 종류의 강아지를 키우고 셨나요",
          "original_form": "어떤 종류의 강아지를 키우고 셨나요",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.4",
          "form": "요크셔테리어 한 마리 키워 봤어요",
          "original_form": "요크셔테리어 한 마리 키워 봤어요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.5",
          "form": "오 귀여웠겠네요",
          "original_form": "오 귀여웠겠네요",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.6",
          "form": "당연히 사랑스럽지요",
          "original_form": "당연히 사랑스럽지요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.7",
          "form": "지금은 강아지가 없나요",
          "original_form": "지금은 강아지가 없나요",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.8",
          "form": "대학교 때 죽었어요",
          "original_form": "대학교 때 죽었어요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.9",
          "form": "13살 이었어요",
          "original_form": "13살 이었어요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031136.1.10",
          "form": "보고 싶겠어요",
          "original_form": "보고 싶겠어요",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031136.1.11",
          "form": "네 요즘도 생각해요",
          "original_form": "네 요즘도 생각해요",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031136.1.12",
          "form": "강아지를 키우는 건 행복한 일이에요",
          "original_form": "강아지를 키우는 건 행복한 일이에요",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031136.1.13",
          "form": "저도 반려동물이 있으면 좋겠어요",
          "original_form": "저도 반려동물이 있으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031136.1.14",
          "form": "고양이는 어떠세요",
          "original_form": "고양이는 어떠세요",
          "speaker_id": "1",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031136.1.15",
          "form": "길냥이는 좋아해요",
          "original_form": "길냥이는 좋아해요",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031136.1.16",
          "form": "가끔 밥도 줘요",
          "original_form": "가끔 밥도 줘요",
          "speaker_id": "2",
          "time": "20210904 17:21"
        }
