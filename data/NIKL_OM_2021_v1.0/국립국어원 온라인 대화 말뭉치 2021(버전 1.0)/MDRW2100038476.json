      "utterance": [
        {
          "id": "MDRW2100038476.1.1",
          "form": "당황해서 마늘수급하러갔지",
          "original_form": "당황해서 마늘수급하러갔지",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100038476.1.2",
          "form": "ㅇㅇ 이거 90연 꽉 채우진 않더라고",
          "original_form": "ㅇㅇ 이거 90연 꽉 채우진 않더라고",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100038476.1.3",
          "form": "그렇다고 듣긴했는데",
          "original_form": "그렇다고 듣긴했는데",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100038476.1.4",
          "form": "웬만하면 70후반~80초반 안에 나온대",
          "original_form": "웬만하면 70후반~80초반 안에 나온대",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100038476.1.5",
          "form": "난 천장 찍어야 줄줄",
          "original_form": "난 천장 찍어야 줄줄",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100038476.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100038476.1.7",
          "form": "하도 안주길래...",
          "original_form": "하도 안주길래...",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100038476.1.8",
          "form": "불신...",
          "original_form": "불신...",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100038476.1.9",
          "form": "ㅇㅏ 나 신야근에",
          "original_form": "ㅇㅏ 나 신야근에",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038476.1.10",
          "form": "모든 의욕이 떨어졌어",
          "original_form": "모든 의욕이 떨어졌어",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038476.1.11",
          "form": "어떡할까",
          "original_form": "어떡할까",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038476.1.12",
          "form": "얘네가 다음 시즌 업뎃은 할까?",
          "original_form": "얘네가 다음 시즌 업뎃은 할까?",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038476.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038476.1.14",
          "form": "나도 22",
          "original_form": "나도 22",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038476.1.15",
          "form": "이거 열리긴 해?",
          "original_form": "이거 열리긴 해?",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038476.1.16",
          "form": "8월말에 보상준다더니",
          "original_form": "8월말에 보상준다더니",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038476.1.17",
          "form": "9월 1일까지 감감무소식이고",
          "original_form": "9월 1일까지 감감무소식이고",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038476.1.18",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100038476.1.19",
          "form": "점검한다더니 뜬금없이 로그인 오류나고",
          "original_form": "점검한다더니 뜬금없이 로그인 오류나고",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.20",
          "form": "ㄹㅇ이것도 걍",
          "original_form": "ㄹㅇ이것도 걍",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.21",
          "form": "뭐하는건지;;;",
          "original_form": "뭐하는건지;;;",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.22",
          "form": "한탕하고 접는",
          "original_form": "한탕하고 접는",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.23",
          "form": "그런건가?",
          "original_form": "그런건가?",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.24",
          "form": "인디겜 수준이여도",
          "original_form": "인디겜 수준이여도",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.25",
          "form": "열정만 보이면",
          "original_form": "열정만 보이면",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.26",
          "form": "어느정도 잡아줄텐데",
          "original_form": "어느정도 잡아줄텐데",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.27",
          "form": "왜 오토메겜들은",
          "original_form": "왜 오토메겜들은",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.28",
          "form": "다 이지경이지",
          "original_form": "다 이지경이지",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038476.1.29",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100038476.1.30",
          "form": "뚜껑 까보니까",
          "original_form": "뚜껑 까보니까",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100038476.1.31",
          "form": "펀딩 없는 인디겜보다 못한 수준이라",
          "original_form": "펀딩 없는 인디겜보다 못한 수준이라",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100038476.1.32",
          "form": "어이가 없을정도야 ㅋㅋㅋ",
          "original_form": "어이가 없을정도야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100038476.1.33",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ진짜",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ진짜",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038476.1.34",
          "form": "여성향 유저들을",
          "original_form": "여성향 유저들을",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038476.1.35",
          "form": "뭐라고 생각하는건지",
          "original_form": "뭐라고 생각하는건지",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038476.1.36",
          "form": "아니 내가",
          "original_form": "아니 내가",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038476.1.37",
          "form": "펀딩만 안받았어도",
          "original_form": "펀딩만 안받았어도",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038476.1.38",
          "form": "이런 말은 안함.",
          "original_form": "이런 말은 안함.",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100038476.1.39",
          "form": "펀딩받고 본게임 오픈까지 1년이나 시간이 있었는데",
          "original_form": "펀딩받고 본게임 오픈까지 1년이나 시간이 있었는데",
          "speaker_id": "1",
          "time": "20210902 14:32"
        }
      ]
    }
