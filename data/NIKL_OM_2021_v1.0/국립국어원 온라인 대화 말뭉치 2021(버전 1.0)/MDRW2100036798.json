      "utterance": [
        {
          "id": "MDRW2100036798.1.1",
          "form": "점심은 뭐 드세요?",
          "original_form": "점심은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210831 12:19"
        },
        {
          "id": "MDRW2100036798.1.2",
          "form": "아직 안먹어서요 name1님은요?",
          "original_form": "아직 안먹어서요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036798.1.3",
          "form": "전 소불고기 해먹을까 생각중이에요",
          "original_form": "전 소불고기 해먹을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036798.1.4",
          "form": "배달 자주 시켜 드시나요?",
          "original_form": "배달 자주 시켜 드시나요?",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036798.1.5",
          "form": "아뇨 배달 잘안시켜요 배민어플도지웠고...",
          "original_form": "아뇨 배달 잘안시켜요 배민어플도지웠고...",
          "speaker_id": "2",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036798.1.6",
          "form": "오~ 저 배달앱 깐적 없어서요.. 배민 별로인가요?",
          "original_form": "오~ 저 배달앱 깐적 없어서요.. 배민 별로인가요?",
          "speaker_id": "1",
          "time": "20210831 12:21"
        },
        {
          "id": "MDRW2100036798.1.7",
          "form": "그런건 아니고 폰용량이 적어서 지웠어요",
          "original_form": "그런건 아니고 폰용량이 적어서 지웠어요",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100036798.1.8",
          "form": "그렇구나..",
          "original_form": "그렇구나..",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036798.1.9",
          "form": "저도 배달음식 주문 잘 안해요",
          "original_form": "저도 배달음식 주문 잘 안해요",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036798.1.10",
          "form": "보통 그냥 해먹죠",
          "original_form": "보통 그냥 해먹죠",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100036798.1.11",
          "form": "좋기야 해먹는게 젤좋은거같아요",
          "original_form": "좋기야 해먹는게 젤좋은거같아요",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036798.1.12",
          "form": "파는건 위생문제가...",
          "original_form": "파는건 위생문제가...",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036798.1.13",
          "form": "맞아요.. 알면 못먹죠",
          "original_form": "맞아요.. 알면 못먹죠",
          "speaker_id": "1",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100036798.1.14",
          "form": "근데 가끔은 요리하기 귀찮을때 먹고 싶긴 해요..ㅎㅎ",
          "original_form": "근데 가끔은 요리하기 귀찮을때 먹고 싶긴 해요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036798.1.15",
          "form": "며칠전에 해물칼국수 먹다가 휴지가 나왔어요 ㅠㅜㅜ",
          "original_form": "며칠전에 해물칼국수 먹다가 휴지가 나왔어요 ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036798.1.16",
          "form": "헐... 배달이요? 식당에서요?",
          "original_form": "헐... 배달이요? 식당에서요?",
          "speaker_id": "1",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036798.1.17",
          "form": "해운대31해물칼국수라는 체인점이요",
          "original_form": "해운대31해물칼국수라는 체인점이요",
          "speaker_id": "2",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100036798.1.18",
          "form": "아 식당이요~ ",
          "original_form": "아 식당이요~ ",
