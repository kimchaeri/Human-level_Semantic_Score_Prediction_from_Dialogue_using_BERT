      "utterance": [
        {
          "id": "MDRW2100046319.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 22:01"
        },
        {
          "id": "MDRW2100046319.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211020 09:09"
        },
        {
          "id": "MDRW2100046319.1.3",
          "form": "아침 드셨나요!",
          "original_form": "아침 드셨나요!",
          "speaker_id": "2",
          "time": "20211020 09:10"
        },
        {
          "id": "MDRW2100046319.1.4",
          "form": "네 아침먹었습니다.",
          "original_form": "네 아침먹었습니다.",
          "speaker_id": "1",
          "time": "20211020 10:06"
        },
        {
          "id": "MDRW2100046319.1.5",
          "form": "오늘 빵 두조각이랑 계란 사과 우유 이렇게 먹었어요?",
          "original_form": "오늘 빵 두조각이랑 계란 사과 우유 이렇게 먹었어요?",
          "speaker_id": "1",
          "time": "20211020 10:07"
        },
        {
          "id": "MDRW2100046319.1.6",
          "form": "님은 식사하셨나요?",
          "original_form": "님은 식사하셨나요?",
          "speaker_id": "1",
          "time": "20211020 10:07"
        },
        {
          "id": "MDRW2100046319.1.7",
          "form": "오 되게 건강한 식단이네요~",
          "original_form": "오 되게 건강한 식단이네요~",
          "speaker_id": "2",
          "time": "20211020 10:07"
        },
        {
          "id": "MDRW2100046319.1.8",
          "form": "저는 아침을 잘안먹어요 ㅜㅜ",
          "original_form": "저는 아침을 잘안먹어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20211020 10:07"
        },
        {
          "id": "MDRW2100046319.1.9",
          "form": "그 시간에 잠을 더 자는 스타일입니다..",
          "original_form": "그 시간에 잠을 더 자는 스타일입니다..",
          "speaker_id": "2",
          "time": "20211020 10:07"
        },
        {
          "id": "MDRW2100046319.1.10",
          "form": "건강한지는 모르겠고 간단하게 먹을수있어서요",
          "original_form": "건강한지는 모르겠고 간단하게 먹을수있어서요",
          "speaker_id": "1",
          "time": "20211020 10:07"
        },
        {
          "id": "MDRW2100046319.1.11",
          "form": "잘은 모르지만 탄단지와 식이섬유까지 챙긴 건강한 식단같아용..!",
          "original_form": "잘은 모르지만 탄단지와 식이섬유까지 챙긴 건강한 식단같아용..!",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.12",
          "form": "아침을 안먹으면 일할때 너무 힘이들어서",
          "original_form": "아침을 안먹으면 일할때 너무 힘이들어서",
          "speaker_id": "1",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.13",
          "form": "아하 그건 그래요...하하",
          "original_form": "아하 그건 그래요...하하",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.14",
          "form": "사실 지금 제정신이 아닙니다",
          "original_form": "사실 지금 제정신이 아닙니다",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.16",
          "form": "잠도 자고 싶고하죠 ㅋㅋ많이 힘드실것 같아요",
          "original_form": "잠도 자고 싶고하죠 ㅋㅋ많이 힘드실것 같아요",
          "speaker_id": "1",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.17",
          "form": "그래도 평생 이렇게 살아온지라",
          "original_form": "그래도 평생 이렇게 살아온지라",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.18",
          "form": "괜찮아요 ㅎㅎㅎ",
          "original_form": "괜찮아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.19",
          "form": "사실 탄산수 홀짝이면서 배를 달래는중입니다",
          "original_form": "사실 탄산수 홀짝이면서 배를 달래는중입니다",
          "speaker_id": "2",
          "time": "20211020 10:08"
        },
        {
          "id": "MDRW2100046319.1.20",
          "form": "오늘 점심은 맛있는 걸 먹고싶네요...하핫",
          "original_form": "오늘 점심은 맛있는 걸 먹고싶네요...하핫",
          "speaker_id": "2",
          "time": "20211020 10:09"
        },
        {
          "id": "MDRW2100046319.1.21",
          "form": "저는 탄산수가 안맞아서 잘못마시겠더라구요",
          "original_form": "저는 탄산수가 안맞아서 잘못마시겠더라구요",
          "speaker_id": "1",
          "time": "20211020 10:09"
        },
        {
          "id": "MDRW2100046319.1.22",
          "form": "ㅋㅋ꼭 푸짐하게 맛있는걸루 많이 드세요",
          "original_form": "ㅋㅋ꼭 푸짐하게 맛있는걸루 많이 드세요",
          "speaker_id": "1",
          "time": "20211020 10:09"
        },
        {
          "id": "MDRW2100046319.1.23",
          "form": "아 ㅠㅠ 맞아요 탄산수가 맛 자체도 호불호가 갈리고 위 안좋으신 분들은 드시면 안좋다하더라구요~",
          "original_form": "아 ㅠㅠ 맞아요 탄산수가 맛 자체도 호불호가 갈리고 위 안좋으신 분들은 드시면 안좋다하더라구요~",
          "speaker_id": "2",
          "time": "20211020 10:09"
        },
        {
          "id": "MDRW2100046319.1.24",
          "form": "ㅎㅎ 네에",
          "original_form": "ㅎㅎ 네에",
          "speaker_id": "2",
          "time": "20211020 10:09"
        },
        {
          "id": "MDRW2100046319.1.25",
          "form": "점심 뭐 드실 예정이세요?",
          "original_form": "점심 뭐 드실 예정이세요?",
          "speaker_id": "2",
          "time": "20211020 10:09"
        },
        {
          "id": "MDRW2100046319.1.26",
          "form": "저는 회사 식당에서 밥을먹어요",
          "original_form": "저는 회사 식당에서 밥을먹어요",
          "speaker_id": "1",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.27",
          "form": "아 구내식당이 있으시구나.. 넘 부럽네요",
          "original_form": "아 구내식당이 있으시구나.. 넘 부럽네요",
          "speaker_id": "2",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.28",
          "form": "메뉴 고민안해도되는!",
          "original_form": "메뉴 고민안해도되는!",
          "speaker_id": "2",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.29",
          "form": "ㅋㅋ그건 좋긴한데 가끔 맛없으면 나가서 먹고싶어요",
          "original_form": "ㅋㅋ그건 좋긴한데 가끔 맛없으면 나가서 먹고싶어요",
          "speaker_id": "1",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.30",
          "form": "코로나때문에 나가서 먹지도 못하게해요",
          "original_form": "코로나때문에 나가서 먹지도 못하게해요",
          "speaker_id": "1",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.31",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "2",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.32",
          "form": "못나가게하는구나..;;",
          "original_form": "못나가게하는구나..;;",
          "speaker_id": "2",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.33",
          "form": "식품회사가 되어서 민감하거든요",
          "original_form": "식품회사가 되어서 민감하거든요",
          "speaker_id": "1",
          "time": "20211020 10:10"
        },
        {
          "id": "MDRW2100046319.1.34",
          "form": "아아....... 고생이 많으세요 ㅠㅠ",
          "original_form": "아아....... 고생이 많으세요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 10:11"
        },
        {
          "id": "MDRW2100046319.1.35",
          "form": "양치도 못하게하는걸요 ㅠㅠ",
          "original_form": "양치도 못하게하는걸요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20211020 10:11"
        },
        {
          "id": "MDRW2100046319.1.36",
          "form": "..........?!?! 정말 고충이 많으시겠군요....;;;;",
          "original_form": "..........?!?! 정말 고충이 많으시겠군요....;;;;",
          "speaker_id": "2",
          "time": "20211020 10:11"
        },
        {
          "id": "MDRW2100046319.1.37",
          "form": "양치로 전파자가 생겼다고해서",
          "original_form": "양치로 전파자가 생겼다고해서",
          "speaker_id": "1",
          "time": "20211020 10:11"
        },
        {
          "id": "MDRW2100046319.1.38",
          "form": "아 헉.......... 빨리 코로나가 끝나야겠네요.....",
          "original_form": "아 헉.......... 빨리 코로나가 끝나야겠네요.....",
          "speaker_id": "2",
          "time": "20211020 10:11"
        },
        {
          "id": "MDRW2100046319.1.39",
          "form": "오늘 하루도 화이팅하세요!",
          "original_form": "오늘 하루도 화이팅하세요!",
          "speaker_id": "2",
          "time": "20211020 10:11"
        },
        {
          "id": "MDRW2100046319.1.40",
          "form": "네 감사합니다. 오늘 하루 힘내세요",
          "original_form": "네 감사합니다. 오늘 하루 힘내세요",
