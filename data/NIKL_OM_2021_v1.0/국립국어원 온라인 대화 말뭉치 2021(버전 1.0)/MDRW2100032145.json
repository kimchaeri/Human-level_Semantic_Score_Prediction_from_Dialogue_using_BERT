      "utterance": [
        {
          "id": "MDRW2100032145.1.1",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100032145.1.2",
          "form": "저도 커피가 싫어졌음해요",
          "original_form": "저도 커피가 싫어졌음해요",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032145.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 커피가 잘 안맞더라고요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 커피가 잘 안맞더라고요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032145.1.4",
          "form": "커피먹으면 가끔 심장이 벌렁벌렁할때도 있고요",
          "original_form": "커피먹으면 가끔 심장이 벌렁벌렁할때도 있고요",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032145.1.5",
          "form": "커피 안먹으니깐 커피우유 먹게 되네요",
          "original_form": "커피 안먹으니깐 커피우유 먹게 되네요",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032145.1.6",
          "form": "저는 아무리 먹어도 별 이상이 없어요",
          "original_form": "저는 아무리 먹어도 별 이상이 없어요",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032145.1.7",
          "form": "잠도 잘자고",
          "original_form": "잠도 잘자고",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032145.1.8",
          "form": "우와 신기하네요~~~! 저도 잠은 잘 자는데! 가끔 저러더라고요?",
          "original_form": "우와 신기하네요~~~! 저도 잠은 잘 자는데! 가끔 저러더라고요?",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032145.1.9",
          "form": "커피우유 맛있죠~~~!",
          "original_form": "커피우유 맛있죠~~~!",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032145.1.10",
          "form": "최애커피우유있나요?",
          "original_form": "최애커피우유있나요?",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100032145.1.11",
          "form": "서울우유에서 나오는거요 팩에 있는거",
          "original_form": "서울우유에서 나오는거요 팩에 있는거",
          "speaker_id": "2",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032145.1.12",
          "form": "그 세모난 커피우유 얘기하시는거죠?",
          "original_form": "그 세모난 커피우유 얘기하시는거죠?",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032145.1.13",
          "form": "커피우유는 브랜드별로 다 먹어봤는데 서울우유에서 나온게 제 입맛에는 맞네요",
          "original_form": "커피우유는 브랜드별로 다 먹어봤는데 서울우유에서 나온게 제 입맛에는 맞네요",
          "speaker_id": "2",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032145.1.14",
          "form": "ㅋㅋㅋㅋㅋ저는 그 빙그레였나 덴마크? 초코초코우유가 좋더라고요!",
          "original_form": "ㅋㅋㅋㅋㅋ저는 그 빙그레였나 덴마크? 초코초코우유가 좋더라고요!",
          "speaker_id": "1",
          "time": "20210905 13:20"
        },
        {
          "id": "MDRW2100032145.1.15",
          "form": "세모난거 말고 우유곽에 있는거요",
          "original_form": "세모난거 말고 우유곽에 있는거요",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032145.1.16",
          "form": "아...? 아하!",
          "original_form": "아...? 아하!",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032145.1.17",
          "form": "거기것도 좋은데 커피맛보다 초코맛이 조금더 강해요",
          "original_form": "거기것도 좋은데 커피맛보다 초코맛이 조금더 강해요",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100032145.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그래요?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그래요?!",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032145.1.19",
          "form": "거기 우유 맛이 기억이 안나네요ㅜㅜ",
          "original_form": "거기 우유 맛이 기억이 안나네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032145.1.20",
          "form": "커피두유도 나왔으면 하는데...안 나올거 같네요",
          "original_form": "커피두유도 나왔으면 하는데...안 나올거 같네요",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100032145.1.21",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:23"
        },
        {
          "id": "MDRW2100032145.1.22",
          "form": "커피두유가 나온다면 진짜 맛있을 거 같아요 매일초코두유도 엄청 맛있거든요!",
          "original_form": "커피두유가 나온다면 진짜 맛있을 거 같아요 매일초코두유도 엄청 맛있거든요!",
          "speaker_id": "1",
          "time": "20210905 13:23"
        }
      ]
    }
