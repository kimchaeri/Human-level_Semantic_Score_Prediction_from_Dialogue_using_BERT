      "utterance": [
        {
          "id": "MDRW2100028345.1.1",
          "form": "제 학과 얘기네요 ㅎㅎ",
          "original_form": "제 학과 얘기네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028345.1.2",
          "form": "컴퓨터 공학과 아니니?",
          "original_form": "컴퓨터 공학과 아니니?",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028345.1.3",
          "form": "맞아요 근데",
          "original_form": "맞아요 근데",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028345.1.4",
          "form": "AI학부도 있어서",
          "original_form": "AI학부도 있어서",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028345.1.5",
          "form": "그럼 잘 알겠구나",
          "original_form": "그럼 잘 알겠구나",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028345.1.6",
          "form": "요즘 완전 트렌드라 왔죠...",
          "original_form": "요즘 완전 트렌드라 왔죠...",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028345.1.7",
          "form": "취직 잘 될거라고 하더라 다들",
          "original_form": "취직 잘 될거라고 하더라 다들",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028345.1.8",
          "form": "취직 보고 온건 맞지만 ㅠㅠ 너무 기대는 마세요",
          "original_form": "취직 보고 온건 맞지만 ㅠㅠ 너무 기대는 마세요",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028345.1.9",
          "form": "알아서 잘 하겠지",
          "original_form": "알아서 잘 하겠지",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028345.1.10",
          "form": "네 ㅎㅎ... 사람이 너무 많아서 취직 걱정돼요 사실 ㅠ",
          "original_form": "네 ㅎㅎ... 사람이 너무 많아서 취직 걱정돼요 사실 ㅠ",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028345.1.11",
          "form": "열심히 하면 될거야",
          "original_form": "열심히 하면 될거야",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028345.1.12",
          "form": "인공지능 대세라잖니",
          "original_form": "인공지능 대세라잖니",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028345.1.13",
          "form": "그래도 저는 인공지능만 배우는건 아니긴 해요",
          "original_form": "그래도 저는 인공지능만 배우는건 아니긴 해요",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028345.1.14",
          "form": "어떤 거 배우니?",
          "original_form": "어떤 거 배우니?",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028345.1.15",
          "form": "컴퓨터 구조도 배우고 그냥 프로그래밍도 배우고",
          "original_form": "컴퓨터 구조도 배우고 그냥 프로그래밍도 배우고",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028345.1.16",
          "form": "AI는 아무래도 어려우니까요",
          "original_form": "AI는 아무래도 어려우니까요",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028345.1.17",
          "form": "집중적으로 배우면 좋을텐데",
          "original_form": "집중적으로 배우면 좋을텐데",
          "speaker_id": "1",
          "time": "20210902 19:22"
        }
