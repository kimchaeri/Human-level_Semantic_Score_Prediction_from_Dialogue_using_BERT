      "utterance": [
        {
          "id": "MDRW2100025400.1.1",
          "form": "지금 가고싶은 여행지는 어딘가요",
          "original_form": "지금 가고싶은 여행지는 어딘가요",
          "speaker_id": "1",
          "time": "20210901 10:54"
        },
        {
          "id": "MDRW2100025400.1.2",
          "form": "저는 미국쪽 가고싶아요 ㅠ",
          "original_form": "저는 미국쪽 가고싶아요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100025400.1.3",
          "form": "오 저도 뉴욕 가보고싶은데 영어가 딸려서 무서워요",
          "original_form": "오 저도 뉴욕 가보고싶은데 영어가 딸려서 무서워요",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025400.1.4",
          "form": "저도영어 못해서 못가요..",
          "original_form": "저도영어 못해서 못가요..",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025400.1.5",
          "form": "그래도요즘 휴대폰이 발달되서..ㅎ",
          "original_form": "그래도요즘 휴대폰이 발달되서..ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025400.1.6",
          "form": "디즈니 랜드도 가보고싶은데",
          "original_form": "디즈니 랜드도 가보고싶은데",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025400.1.7",
          "form": "해리포터 테마파크나",
          "original_form": "해리포터 테마파크나",
          "speaker_id": "1",
          "time": "20210901 10:58"
        },
        {
          "id": "MDRW2100025400.1.8",
          "form": "저도 디즈니 랜드 가보고 싶어요 ㅠㅠ",
          "original_form": "저도 디즈니 랜드 가보고 싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025400.1.9",
          "form": "거기 파는 인형 기념품도 가지고 싶던데",
          "original_form": "거기 파는 인형 기념품도 가지고 싶던데",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025400.1.10",
          "form": "디즈니면다 기엽죠 ㅠㅠ",
          "original_form": "디즈니면다 기엽죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025400.1.11",
          "form": "일본에  유니버셜 가보고싶네요",
          "original_form": "일본에  유니버셜 가보고싶네요",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025400.1.12",
          "form": "맞아요 가까워서 좋은데 코로나 방사능이 조금 걸리지만",
          "original_form": "맞아요 가까워서 좋은데 코로나 방사능이 조금 걸리지만",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025400.1.13",
          "form": "일본 스시 먹고싶네요",
          "original_form": "일본 스시 먹고싶네요",
          "speaker_id": "1",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025400.1.14",
          "form": "맛있겠네요!",
          "original_form": "맛있겠네요!",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025400.1.15",
          "form": "한국스시와",
          "original_form": "한국스시와",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025400.1.16",
          "form": "차이가 있을꺼요??ㅋㅋㅋㅋ",
          "original_form": "차이가 있을꺼요??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025400.1.17",
          "form": "ㅋㅋㅋㅋㅋ 그렇긴 하네요 우리도 바다 근처서 먹으면 신선",
          "original_form": "ㅋㅋㅋㅋㅋ 그렇긴 하네요 우리도 바다 근처서 먹으면 신선",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025400.1.18",
          "form": "제주도도 가고싶네요",
          "original_form": "제주도도 가고싶네요",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025400.1.19",
          "form": "제주도도 좋어요수학여향으로 가봣는데 재미있더라구요",
          "original_form": "제주도도 좋어요수학여향으로 가봣는데 재미있더라구요",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025400.1.20",
          "form": "와 좋은학교네요 저는 그런데 안보내주던데",
          "original_form": "와 좋은학교네요 저는 그런데 안보내주던데",
