      "utterance": [
        {
          "id": "MDRW2100032416.1.1",
          "form": "좋인하는 프로그램 있으세요?",
          "original_form": "좋인하는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032416.1.2",
          "form": "저 무도 좋아했었어요!",
          "original_form": "저 무도 좋아했었어요!",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032416.1.3",
          "form": "최근엔 놀면 뭐하니 가끔 챙겨봐용",
          "original_form": "최근엔 놀면 뭐하니 가끔 챙겨봐용",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032416.1.4",
          "form": "무도는 다 좋죠",
          "original_form": "무도는 다 좋죠",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032416.1.5",
          "form": "가끔 유투브서 봐요",
          "original_form": "가끔 유투브서 봐요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032416.1.6",
          "form": "그거 아세요? 엠비씨m 채널 가면 무도 맨날 틀어줘요!!",
          "original_form": "그거 아세요? 엠비씨m 채널 가면 무도 맨날 틀어줘요!!",
          "speaker_id": "2",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032416.1.7",
          "form": "엇 몰랐어요 ㅋㅋㅋ",
          "original_form": "엇 몰랐어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100032416.1.8",
          "form": "근데 유투브가 편한거 같아오ㅡ",
          "original_form": "근데 유투브가 편한거 같아오ㅡ",
          "speaker_id": "1",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100032416.1.9",
          "form": "오오 저 유튜브는 그 뭐지 케이팝 유튜브만 보는거 같아요!",
          "original_form": "오오 저 유튜브는 그 뭐지 케이팝 유튜브만 보는거 같아요!",
          "speaker_id": "2",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100032416.1.10",
          "form": "유튜브로 뭐 보세용?",
          "original_form": "유튜브로 뭐 보세용?",
          "speaker_id": "2",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100032416.1.11",
          "form": "엥 왜 뭐가 왔다고 하고 안뜨지ㅠㅠ",
          "original_form": "엥 왜 뭐가 왔다고 하고 안뜨지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032416.1.12",
          "form": "다시 보여주실래요?!",
          "original_form": "다시 보여주실래요?!",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032416.1.13",
          "form": "저 요즘 먹방봐요",
          "original_form": "저 요즘 먹방봐요",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032416.1.14",
          "form": "히밥님요",
          "original_form": "히밥님요",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032416.1.15",
          "form": "헉 히밥님 알아요",
          "original_form": "헉 히밥님 알아요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032416.1.16",
          "form": "그 귀여우신분ㅋㅋㅋ",
          "original_form": "그 귀여우신분ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032416.1.17",
          "form": "네 엄청 잘먹더라구요",
          "original_form": "네 엄청 잘먹더라구요",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032416.1.18",
          "form": "속도도 빠르고",
          "original_form": "속도도 빠르고",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032416.1.19",
          "form": "저 먹방 유튜버는 또 그 햇님님이랑 쯔양님 알아요",
          "original_form": "저 먹방 유튜버는 또 그 햇님님이랑 쯔양님 알아요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032416.1.20",
          "form": "그분들도 잘먹죠",
          "original_form": "그분들도 잘먹죠",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032416.1.21",
          "form": "우왕 히밥님 팬이신가봐요",
          "original_form": "우왕 히밥님 팬이신가봐요",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032416.1.22",
          "form": "근데 저는 히밥님 속도가 빨라서 좋아요",
          "original_form": "근데 저는 히밥님 속도가 빨라서 좋아요",
          "speaker_id": "1",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032416.1.23",
          "form": "앗 저 밥먹구 올게요옹",
          "original_form": "앗 저 밥먹구 올게요옹",
          "speaker_id": "2",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100032416.1.24",
          "form": "네네 맛있게 드세요",
          "original_form": "네네 맛있게 드세요",
          "speaker_id": "1",
          "time": "20210905 17:23"
        }
      ]
    }
