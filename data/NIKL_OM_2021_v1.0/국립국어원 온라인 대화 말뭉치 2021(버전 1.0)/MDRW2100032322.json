      "utterance": [
        {
          "id": "MDRW2100032322.1.1",
          "form": "없어서 못먹지",
          "original_form": "없어서 못먹지",
          "speaker_id": "2",
          "time": "20210905 15:33"
        },
        {
          "id": "MDRW2100032322.1.2",
          "form": "과일 뭐 좋아하더라?",
          "original_form": "과일 뭐 좋아하더라?",
          "speaker_id": "1",
          "time": "20210905 15:34"
        },
        {
          "id": "MDRW2100032322.1.3",
          "form": "복숭아도 좋도 청포도도 좋고 메론 체리 사과도 좋고 귤도 좋음",
          "original_form": "복숭아도 좋도 청포도도 좋고 메론 체리 사과도 좋고 귤도 좋음",
          "speaker_id": "2",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100032322.1.4",
          "form": "이번여름 복숭아 진짜 달던데 많이 먹었어?",
          "original_form": "이번여름 복숭아 진짜 달던데 많이 먹었어?",
          "speaker_id": "1",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100032322.1.5",
          "form": "많이는 아니고 몇번 상자로 사와서 먹었지",
          "original_form": "많이는 아니고 몇번 상자로 사와서 먹었지",
          "speaker_id": "2",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100032322.1.6",
          "form": "너나 나나 경제적인 상황이 아쉽다...",
          "original_form": "너나 나나 경제적인 상황이 아쉽다...",
          "speaker_id": "1",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032322.1.7",
          "form": "별수있나 ㅋㅋㅋㅋㅋ 내년에많이먹자",
          "original_form": "별수있나 ㅋㅋㅋㅋㅋ 내년에많이먹자",
          "speaker_id": "2",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032322.1.8",
          "form": "너는 과일좋아하던가?",
          "original_form": "너는 과일좋아하던가?",
          "speaker_id": "2",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032322.1.9",
          "form": "나는 과일 그다지 안좋아하는데",
          "original_form": "나는 과일 그다지 안좋아하는데",
          "speaker_id": "1",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032322.1.10",
          "form": "자취하니까 좋아하게됨",
          "original_form": "자취하니까 좋아하게됨",
          "speaker_id": "1",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032322.1.11",
          "form": "나도 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032322.1.12",
          "form": "과일 더 좋아졌어 ㅋㅋㅋㅋ",
          "original_form": "과일 더 좋아졌어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032322.1.13",
          "form": "너무 귀함.....",
          "original_form": "너무 귀함.....",
          "speaker_id": "2",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100032322.1.14",
          "form": "진짜 이렇게 비쌀줄 몰랐어ㅋㅋㅋㅋ",
          "original_form": "진짜 이렇게 비쌀줄 몰랐어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032322.1.15",
          "form": "상하기는 또 빨리 상하니까 ㅋㅋㅋㅋ",
          "original_form": "상하기는 또 빨리 상하니까 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032322.1.16",
          "form": "진짜 관리도 일이다",
          "original_form": "진짜 관리도 일이다",
          "speaker_id": "1",
          "time": "20210905 15:39"
        },
        {
          "id": "MDRW2100032322.1.17",
          "form": "겨울에 귤 쌓아놓고 먹고싶네",
          "original_form": "겨울에 귤 쌓아놓고 먹고싶네",
          "speaker_id": "1",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100032322.1.18",
          "form": "상황보고 니생일맞춰서 보내줄께",
          "original_form": "상황보고 니생일맞춰서 보내줄께",
          "speaker_id": "2",
          "time": "20210905 15:40"
