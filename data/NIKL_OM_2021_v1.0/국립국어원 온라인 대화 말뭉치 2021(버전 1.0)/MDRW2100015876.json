      "utterance": [
        {
          "id": "MDRW2100015876.1.1",
          "form": "제가 괜찮은 상품 하나를 알고있는데",
          "original_form": "제가 괜찮은 상품 하나를 알고있는데",
          "speaker_id": "1",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015876.1.2",
          "form": "영업하자면",
          "original_form": "영업하자면",
          "speaker_id": "1",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015876.1.3",
          "form": "혹시 마라곤약 아시나요",
          "original_form": "혹시 마라곤약 아시나요",
          "speaker_id": "1",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015876.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건 뭔가요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건 뭔가요??",
          "speaker_id": "2",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015876.1.5",
          "form": "이걸 상품으로 엮어버리시다니",
          "original_form": "이걸 상품으로 엮어버리시다니",
          "speaker_id": "2",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015876.1.6",
          "form": "곤약을 마라소스에 달달 볶은거거든요?",
          "original_form": "곤약을 마라소스에 달달 볶은거거든요?",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015876.1.7",
          "form": "진짜 그냥 곤약들어간 마라샹궈",
          "original_form": "진짜 그냥 곤약들어간 마라샹궈",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015876.1.8",
          "form": "곤약만 건져먹는맛너요",
          "original_form": "곤약만 건져먹는맛너요",
          "speaker_id": "1",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015876.1.9",
          "form": "약간이케",
          "original_form": "약간이케",
          "speaker_id": "2",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015876.1.10",
          "form": "과자먹듯이 먹는 그런건가요?",
          "original_form": "과자먹듯이 먹는 그런건가요?",
          "speaker_id": "2",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015876.1.11",
          "form": "중국간식이에요!",
          "original_form": "중국간식이에요!",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015876.1.12",
          "form": "상당하네요",
          "original_form": "상당하네요",
          "speaker_id": "2",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015876.1.13",
          "form": "가격도 엄청싸요 작은팩에 두젓가락쯤인데",
          "original_form": "가격도 엄청싸요 작은팩에 두젓가락쯤인데",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015876.1.14",
          "form": "20팩 5500원이래요",
          "original_form": "20팩 5500원이래요",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015876.1.15",
          "form": "약간 얼얼한 맛찔이(?) 인데 먹을 수 있을까요?",
          "original_form": "약간 얼얼한 맛찔이(?) 인데 먹을 수 있을까요?",
          "speaker_id": "2",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015876.1.16",
          "form": "앗 그러면 쪼금 힘들수도 있어요!",
          "original_form": "앗 그러면 쪼금 힘들수도 있어요!",
          "speaker_id": "1",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015876.1.17",
          "form": "저는 처음 먹었을때",
          "original_form": "저는 처음 먹었을때",
          "speaker_id": "1",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015876.1.18",
          "form": "헉 ,,, ㅠㅠ",
          "original_form": "헉 ,,, ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015876.1.19",
          "form": "그거 먹고 나서 좀 마라맛을 밍밍하게 하는 가게 간 적 있는데",
          "original_form": "그거 먹고 나서 좀 마라맛을 밍밍하게 하는 가게 간 적 있는데",
          "speaker_id": "1",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015876.1.20",
          "form": "마라탕이 맹물같았어요ㄷㄷ 맛이 강하니까 애매한 마라탕은 덮어버리는",
          "original_form": "마라탕이 맹물같았어요ㄷㄷ 맛이 강하니까 애매한 마라탕은 덮어버리는",
          "speaker_id": "1",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015876.1.21",
          "form": "엄청난데요",
          "original_form": "엄청난데요",
          "speaker_id": "2",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015876.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015876.1.23",
          "form": "제가 아마 먹는게 애매한 마라탕일것같은 느낌이 ,,",
          "original_form": "제가 아마 먹는게 애매한 마라탕일것같은 느낌이 ,,",
          "speaker_id": "2",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015876.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅋ 근데 가격싸고 칼로리도 괜찮아서 자주 먹네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅋ 근데 가격싸고 칼로리도 괜찮아서 자주 먹네요",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015876.1.25",
          "form": "곤약이라 1팩 16칼로리에요 마라탕 칼로리 생각하면 마라탕대신 구매하게되는..",
          "original_form": "곤약이라 1팩 16칼로리에요 마라탕 칼로리 생각하면 마라탕대신 구매하게되는..",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015876.1.26",
          "form": "곤약젤리 느낌으루다가 먹게더ㅣ는 느낌이군요!!",
          "original_form": "곤약젤리 느낌으루다가 먹게더ㅣ는 느낌이군요!!",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015876.1.27",
          "form": "전 근데 이미 틀린것같아요",
          "original_form": "전 근데 이미 틀린것같아요",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015876.1.28",
          "form": "곤약젤리랑 식감은 달라요! 오징어맛",
          "original_form": "곤약젤리랑 식감은 달라요! 오징어맛",
          "speaker_id": "1",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015876.1.29",
          "form": "칼로리적은게 마치 곤약젤리같앗어용ㅋㅋㅋㅋ",
          "original_form": "칼로리적은게 마치 곤약젤리같앗어용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015876.1.30",
          "form": "그건 그렇죠ㅋㅋ",
          "original_form": "그건 그렇죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015876.1.31",
          "form": "여하튼 최근에 다 먹어서 다시 사려고 존버중이에요",
          "original_form": "여하튼 최근에 다 먹어서 다시 사려고 존버중이에요",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015876.1.32",
          "form": "네이버 멤버쉽 데이에 사려구요",
          "original_form": "네이버 멤버쉽 데이에 사려구요",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015876.1.33",
          "form": "꿀매 기원하겠습니다",
          "original_form": "꿀매 기원하겠습니다",
          "speaker_id": "2",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015876.1.34",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
