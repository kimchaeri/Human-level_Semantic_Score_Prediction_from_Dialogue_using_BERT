      "utterance": [
        {
          "id": "MDRW2100021345.1.1",
          "form": "노래 듣는거 좋아하세요?",
          "original_form": "노래 듣는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021345.1.2",
          "form": "이것저것 많이 듣죠..  뭐 주로 들으시나요?",
          "original_form": "이것저것 많이 듣죠..  뭐 주로 들으시나요?",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021345.1.3",
          "form": "전 여름엔 댄스 지금은 발라드요 ㅎㅎ",
          "original_form": "전 여름엔 댄스 지금은 발라드요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021345.1.4",
          "form": "가요 위주인가요?",
          "original_form": "가요 위주인가요?",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021345.1.5",
          "form": "네네 ! 한국 노래 좋아해요 ㅎㅎ",
          "original_form": "네네 ! 한국 노래 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021345.1.6",
          "form": "아이돌 음악도 들어요?",
          "original_form": "아이돌 음악도 들어요?",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021345.1.7",
          "form": "네 자주 들어요!",
          "original_form": "네 자주 들어요!",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021345.1.8",
          "form": "누구 좋아하세요?  방탄? 엔씨티?",
          "original_form": "누구 좋아하세요?  방탄? 엔씨티?",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021345.1.9",
          "form": "저 방탄 좋아했는데 요즘은 엔시티 자주 들어요 ㅋㅋ",
          "original_form": "저 방탄 좋아했는데 요즘은 엔시티 자주 들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021345.1.10",
          "form": "아미?",
          "original_form": "아미?",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021345.1.11",
          "form": "네네 ! 아미입니다 ㅋㅋ",
          "original_form": "네네 ! 아미입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021345.1.12",
          "form": "ㅋㅋㅋ 그 로고박힌 티셔트는 한정기간에망 파는거에요?",
          "original_form": "ㅋㅋㅋ 그 로고박힌 티셔트는 한정기간에망 파는거에요?",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021345.1.13",
          "form": "어서 구하는건지..",
          "original_form": "어서 구하는건지..",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021345.1.14",
          "form": "방탄 로고요 ???",
          "original_form": "방탄 로고요 ???",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021345.1.15",
          "form": "네.. 그 기둥.. 보라색티 엄청 이쁘던데..",
          "original_form": "네.. 그 기둥.. 보라색티 엄청 이쁘던데..",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100021345.1.16",
          "form": "그거 아마 공식샵에 있을텐데",
          "original_form": "그거 아마 공식샵에 있을텐데",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021345.1.17",
          "form": "가입을 하셔야될꺼에요 !",
          "original_form": "가입을 하셔야될꺼에요 !",
          "speaker_id": "1",
          "time": "20210828 21:28"
        }
      ]
