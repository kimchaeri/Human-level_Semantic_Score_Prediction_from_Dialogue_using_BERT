      "utterance": [
        {
          "id": "MDRW2100040879.1.1",
          "form": "아직은 안추워서 좋다",
          "original_form": "아직은 안추워서 좋다",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100040879.1.2",
          "form": "맞아 그리구",
          "original_form": "맞아 그리구",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100040879.1.3",
          "form": "그렇게 덥지도 않아 !!!",
          "original_form": "그렇게 덥지도 않아 !!!",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100040879.1.4",
          "form": "난 오늘 청자켓 입고 나왔어",
          "original_form": "난 오늘 청자켓 입고 나왔어",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100040879.1.5",
          "form": "ㅎㅎ넘 좋은날씨",
          "original_form": "ㅎㅎ넘 좋은날씨",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100040879.1.6",
          "form": "밤되면 추우려나",
          "original_form": "밤되면 추우려나",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100040879.1.7",
          "form": "급걱정이",
          "original_form": "급걱정이",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100040879.1.8",
          "form": "앗 그럴거같긴 한데!",
          "original_form": "앗 그럴거같긴 한데!",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100040879.1.9",
          "form": "아우터는 가져가야할거같다능~!",
          "original_form": "아우터는 가져가야할거같다능~!",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100040879.1.10",
          "form": "이제 20도 이렇게 내려가더라구",
          "original_form": "이제 20도 이렇게 내려가더라구",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100040879.1.11",
          "form": "이미 나와가지고",
          "original_form": "이미 나와가지고",
          "speaker_id": "2",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100040879.1.12",
          "form": "센스있는곳이라면 뭔가가 있겠지",
          "original_form": "센스있는곳이라면 뭔가가 있겠지",
          "speaker_id": "2",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100040879.1.13",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040879.1.14",
          "form": "벌써 밖이야?!",
          "original_form": "벌써 밖이야?!",
          "speaker_id": "1",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040879.1.15",
          "form": "그래서날씨를 알았구나!!",
          "original_form": "그래서날씨를 알았구나!!",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040879.1.16",
          "form": "응ㅎㅎ",
          "original_form": "응ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100040879.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100040879.1.18",
          "form": "어디 가는거야!!",
          "original_form": "어디 가는거야!!",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100040879.1.19",
          "form": "혹시;;;;",
          "original_form": "혹시;;;;",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100040879.1.20",
          "form": "미리 가 있는건 아니겠지?!?!",
          "original_form": "미리 가 있는건 아니겠지?!?!",
          "speaker_id": "1",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100040879.1.21",
          "form": "히번뜩",
          "original_form": "히번뜩",
          "speaker_id": "1",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100040879.1.22",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㄱㅋㅋㄱㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㄱㅋㅋㄱㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040879.1.23",
          "form": "아냐아냐",
          "original_form": "아냐아냐",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040879.1.24",
          "form": "낼부터 수행평가라 준비할게 많아서 출근했어",
          "original_form": "낼부터 수행평가라 준비할게 많아서 출근했어",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040879.1.25",
          "form": "집이 시끄럽기도하고",
          "original_form": "집이 시끄럽기도하고",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100040879.1.26",
          "form": "앗 그렇구나 세상에",
          "original_form": "앗 그렇구나 세상에",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040879.1.27",
          "form": "날씨 좋으면 출근할 때 그나마",
          "original_form": "날씨 좋으면 출근할 때 그나마",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040879.1.28",
          "form": "괜찮겠다!!!!",
          "original_form": "괜찮겠다!!!!",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100040879.1.29",
          "form": "ㅋㅋㅋㅋ억울해서 빨리 마무리하고 가려고",
          "original_form": "ㅋㅋㅋㅋ억울해서 빨리 마무리하고 가려고",
          "speaker_id": "2",
          "time": "20210905 12:53"
        },
        {
          "id": "MDRW2100040879.1.30",
          "form": "날씨가 너무조은데 막힌 학교에서,,,",
          "original_form": "날씨가 너무조은데 막힌 학교에서,,,",
          "speaker_id": "2",
          "time": "20210905 12:53"
