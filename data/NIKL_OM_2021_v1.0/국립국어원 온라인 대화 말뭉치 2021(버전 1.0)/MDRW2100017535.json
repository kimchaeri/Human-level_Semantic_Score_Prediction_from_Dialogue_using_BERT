      "utterance": [
        {
          "id": "MDRW2100017535.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017535.1.2",
          "form": "안녕하세요 좋아하는 음식있으신가요?",
          "original_form": "안녕하세요 좋아하는 음식있으신가요?",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017535.1.3",
          "form": "네! 저 브리또좋아해요 ㅎㅎ",
          "original_form": "네! 저 브리또좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017535.1.4",
          "form": "오오 브리또맛있조",
          "original_form": "오오 브리또맛있조",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017535.1.5",
          "form": "맞아요~ 혹시 배라 3픽이 어떻게되시나요?",
          "original_form": "맞아요~ 혹시 배라 3픽이 어떻게되시나요?",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017535.1.6",
          "form": "바람과 함께 사라지다 쿠키앤크림 아몬드봉봉이요ㅎ",
          "original_form": "바람과 함께 사라지다 쿠키앤크림 아몬드봉봉이요ㅎ",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017535.1.7",
          "form": "어떻게되세요?",
          "original_form": "어떻게되세요?",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017535.1.8",
          "form": "오 두개겹치네요 저는 피스타치오아몬드쿠앤크 그리고 아몬드봉봉이요 ㅎㅎ",
          "original_form": "오 두개겹치네요 저는 피스타치오아몬드쿠앤크 그리고 아몬드봉봉이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017535.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ혹시 민트초코도 좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ혹시 민트초코도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100017535.1.10",
          "form": "네~ 민초단이에욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네~ 민초단이에욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100017535.1.11",
          "form": "오옷 저도 좋아해욥!치약맛ㅋㅋ",
          "original_form": "오옷 저도 좋아해욥!치약맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100017535.1.12",
          "form": "앜ㅋㅋㅋ 민초가 치약맛이아니라 치약이 민초맛인것",
          "original_form": "앜ㅋㅋㅋ 민초가 치약맛이아니라 치약이 민초맛인것",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100017535.1.13",
          "form": "ㅋㅋㅋ아웃겨ㅋㅋㅋ그 말이 맞네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ아웃겨ㅋㅋㅋ그 말이 맞네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100017535.1.14",
          "form": "ㅋㅋㅋㅌ 민초는 배라가 독보적인거같아요",
          "original_form": "ㅋㅋㅋㅌ 민초는 배라가 독보적인거같아요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100017535.1.15",
          "form": "앗 민초 배라밖에 안먹어봤어요ㅋㅋ",
          "original_form": "앗 민초 배라밖에 안먹어봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100017535.1.16",
          "form": "그러시구나! 다른곳도 맛은있지만 특유의 배라맛은 흉내못내는거같아요",
          "original_form": "그러시구나! 다른곳도 맛은있지만 특유의 배라맛은 흉내못내는거같아요",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100017535.1.17",
          "form": "오옷 담에 다른데도 먹어봐야겠어요ㅎㅎㅎ",
          "original_form": "오옷 담에 다른데도 먹어봐야겠어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100017535.1.18",
          "form": "녹차맛은 좋아하시나요?",
          "original_form": "녹차맛은 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100017535.1.19",
          "form": "아니요 제가 녹차를 싫어해요 ㅜㅜ",
          "original_form": "아니요 제가 녹차를 싫어해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 06:39"
        },
        {
          "id": "MDRW2100017535.1.20",
          "form": "아 녹차도 맛있어서 혹시나한ㅋㅋ",
          "original_form": "아 녹차도 맛있어서 혹시나한ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100017535.1.21",
          "form": "전 녹차덕후라ㅋㅋ",
          "original_form": "전 녹차덕후라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100017535.1.22",
          "form": "전 녹차 싫어하지만 기억하기론 녹차는 나뚜루가 맛있다고..!",
          "original_form": "전 녹차 싫어하지만 기억하기론 녹차는 나뚜루가 맛있다고..!",
          "speaker_id": "1",
          "time": "20210830 10:35"
        }
      ]
    }
