      "utterance": [
        {
          "id": "MDRW2100042507.1.1",
          "form": "여름이 더 좋으세요 겨율이 더 좋으세요?",
          "original_form": "여름이 더 좋으세요 겨율이 더 좋으세요?",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042507.1.2",
          "form": "그래도 겨울이여!!",
          "original_form": "그래도 겨울이여!!",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042507.1.3",
          "form": "여름도 타거든요ㅋㅋ",
          "original_form": "여름도 타거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042507.1.4",
          "form": "정말 효율이 없는 몸입니다 ㅠㅠ",
          "original_form": "정말 효율이 없는 몸입니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042507.1.5",
          "form": "오노!!!",
          "original_form": "오노!!!",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042507.1.6",
          "form": "저도 겨울이요!!",
          "original_form": "저도 겨울이요!!",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042507.1.7",
          "form": "ㅋㅋㅋㅋ 넹",
          "original_form": "ㅋㅋㅋㅋ 넹",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042507.1.8",
          "form": "전 더위를 엄청 타요!",
          "original_form": "전 더위를 엄청 타요!",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042507.1.9",
          "form": "아 역시 겨울이 좋져",
          "original_form": "아 역시 겨울이 좋져",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.10",
          "form": "허벅지에 땀이",
          "original_form": "허벅지에 땀이",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.11",
          "form": "앗.. 저두..",
          "original_form": "앗.. 저두..",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.12",
          "form": "윽 끔찍합니다",
          "original_form": "윽 끔찍합니다",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.13",
          "form": "허벅지 땀.. 못참아..",
          "original_form": "허벅지 땀.. 못참아..",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.14",
          "form": "글구 전 긴바지입고싶은데",
          "original_form": "글구 전 긴바지입고싶은데",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.15",
          "form": "너무 더워서 반바지 입어야하는 상황.. 그런거 넘 싫어여",
          "original_form": "너무 더워서 반바지 입어야하는 상황.. 그런거 넘 싫어여",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.16",
          "form": "저 정말 더위를 넘 많이타서..",
          "original_form": "저 정말 더위를 넘 많이타서..",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.17",
          "form": "사실 오늘도 에어컨 키고있었어요",
          "original_form": "사실 오늘도 에어컨 키고있었어요",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042507.1.18",
          "form": "와ㅋㅋㅋㅋㅋ 오늘 에어컨 트셨다니!!",
          "original_form": "와ㅋㅋㅋㅋㅋ 오늘 에어컨 트셨다니!!",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042507.1.19",
          "form": "근데 생각해보니 저도 선풍기 잠깐 틀었었어요",
          "original_form": "근데 생각해보니 저도 선풍기 잠깐 틀었었어요",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042507.1.20",
          "form": "씻고나오니까 습해서",
          "original_form": "씻고나오니까 습해서",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042507.1.21",
          "form": "좀 더위를 많이 타는 편인거같아오 흑흑",
          "original_form": "좀 더위를 많이 타는 편인거같아오 흑흑",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042507.1.22",
          "form": "그래서 여름이 너무나 고통스럽습니다..",
          "original_form": "그래서 여름이 너무나 고통스럽습니다..",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042507.1.23",
          "form": "이제 좋은 계절이 왔어요!!!",
          "original_form": "이제 좋은 계절이 왔어요!!!",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042507.1.24",
          "form": "가을 짱!!!",
          "original_form": "가을 짱!!!",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042507.1.25",
          "form": "맞아요~~~",
          "original_form": "맞아요~~~",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042507.1.26",
          "form": "가을 쵝오#!",
          "original_form": "가을 쵝오#!",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042507.1.27",
          "form": "가을아 가지마~!",
          "original_form": "가을아 가지마~!",
