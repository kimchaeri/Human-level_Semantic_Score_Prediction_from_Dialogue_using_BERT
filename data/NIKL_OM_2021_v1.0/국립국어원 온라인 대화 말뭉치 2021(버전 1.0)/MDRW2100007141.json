      "utterance": [
        {
          "id": "MDRW2100007141.1.1",
          "form": "온라인 커뮤니티 하시나요?",
          "original_form": "온라인 커뮤니티 하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007141.1.2",
          "form": "눈팅만 하고 있어요",
          "original_form": "눈팅만 하고 있어요",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007141.1.3",
          "form": "정모는 가본적이 없어요",
          "original_form": "정모는 가본적이 없어요",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007141.1.4",
          "form": "저는 뽐뿌해요",
          "original_form": "저는 뽐뿌해요",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007141.1.5",
          "form": "뽐거지입니다ㅜ",
          "original_form": "뽐거지입니다ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007141.1.6",
          "form": "저는 이곳저곳 커뮤니티 돌아요",
          "original_form": "저는 이곳저곳 커뮤니티 돌아요",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007141.1.7",
          "form": "아하 하긴하시는군요",
          "original_form": "아하 하긴하시는군요",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007141.1.8",
          "form": "뽐뿌도 좋아요",
          "original_form": "뽐뿌도 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007141.1.9",
          "form": "네 전 제가 관심 있는 분야 커뮤니티만 가요",
          "original_form": "네 전 제가 관심 있는 분야 커뮤니티만 가요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007141.1.10",
          "form": "무슨 분야에 관심 있으세요?",
          "original_form": "무슨 분야에 관심 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007141.1.11",
          "form": "비평이요",
          "original_form": "비평이요",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007141.1.12",
          "form": "주로 작품비평을 해요",
          "original_form": "주로 작품비평을 해요",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007141.1.13",
          "form": "오오... 커뮤니티 활동 고급지시네여",
          "original_form": "오오... 커뮤니티 활동 고급지시네여",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007141.1.14",
          "form": "뽐뿌면 재테크에 관심 있으시겠어요",
          "original_form": "뽐뿌면 재테크에 관심 있으시겠어요",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007141.1.15",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007141.1.16",
          "form": "정모는 나가보셨나요?",
          "original_form": "정모는 나가보셨나요?",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100007141.1.17",
          "form": "모임은 있어요",
          "original_form": "모임은 있어요",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100007141.1.18",
          "form": "요즘 저는 뽐뿌해요",
          "original_form": "요즘 저는 뽐뿌해요",
