      "utterance": [
        {
          "id": "MDRW2100045816.1.1",
          "form": "간단히 계란밥이요",
          "original_form": "간단히 계란밥이요",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045816.1.2",
          "form": "뭐없을땐 그거 최고ㅋㅋ",
          "original_form": "뭐없을땐 그거 최고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045816.1.3",
          "form": "전 호박죽 먹었어요",
          "original_form": "전 호박죽 먹었어요",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045816.1.4",
          "form": "호박죽맛잇죠 근데 만들기 번거로워서",
          "original_form": "호박죽맛잇죠 근데 만들기 번거로워서",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045816.1.5",
          "form": "아는 동생이 만들어 준거예요",
          "original_form": "아는 동생이 만들어 준거예요",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045816.1.6",
          "form": "요리잘하는거 넘부러워요",
          "original_form": "요리잘하는거 넘부러워요",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045816.1.7",
          "form": "저도 요리를 잘하지는 못해요",
          "original_form": "저도 요리를 잘하지는 못해요",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045816.1.8",
          "form": "그냥 해서 먹을정도로만 해요",
          "original_form": "그냥 해서 먹을정도로만 해요",
          "speaker_id": "1",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045816.1.9",
          "form": "맨날 간편식이나 밀키트 사게되는거 같아여",
          "original_form": "맨날 간편식이나 밀키트 사게되는거 같아여",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045816.1.10",
          "form": "밀키트 편리해서 좋긴 하죠",
          "original_form": "밀키트 편리해서 좋긴 하죠",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045816.1.11",
          "form": "전 그냥 못해도 자꾸 해먹으려고 노력해요",
          "original_form": "전 그냥 못해도 자꾸 해먹으려고 노력해요",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045816.1.12",
          "form": "칼질도 너무 어려워요",
          "original_form": "칼질도 너무 어려워요",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045816.1.13",
          "form": "손다치지 않게 그냥 천천히 해요",
          "original_form": "손다치지 않게 그냥 천천히 해요",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045816.1.14",
          "form": "저도 아직도 못해서 천천히 썰어요",
          "original_form": "저도 아직도 못해서 천천히 썰어요",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045816.1.15",
          "form": "꼭손톱을 썰어서 간편히 해먹는거 찾게 되더라고요ㅋ",
          "original_form": "꼭손톱을 썰어서 간편히 해먹는거 찾게 되더라고요ㅋ",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045816.1.16",
          "form": "아이구 !\r\n간편식도 요즘 엄청 잘나오니깐 그것도 괜찮죠",
          "original_form": "아이구 !\r\n간편식도 요즘 엄청 잘나오니깐 그것도 괜찮죠",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045816.1.17",
          "form": "칼질도 그렇지만 간을따로 할필요없으니까 좋더라고요",
          "original_form": "칼질도 그렇지만 간을따로 할필요없으니까 좋더라고요",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045816.1.18",
          "form": "그렇군요 밀키트 구매를 안해봐서 모르겠네요",
          "original_form": "그렇군요 밀키트 구매를 안해봐서 모르겠네요",
