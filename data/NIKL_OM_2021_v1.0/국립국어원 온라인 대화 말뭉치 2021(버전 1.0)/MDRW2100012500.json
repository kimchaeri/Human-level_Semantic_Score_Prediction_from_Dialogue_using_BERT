      "utterance": [
        {
          "id": "MDRW2100012500.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012500.1.2",
          "form": "좋아하는 편은 아니예요",
          "original_form": "좋아하는 편은 아니예요",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012500.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012500.1.4",
          "form": "전 자주 못다니지만 좋아해요",
          "original_form": "전 자주 못다니지만 좋아해요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012500.1.5",
          "form": "자연경관을 보는걸 좋아해서",
          "original_form": "자연경관을 보는걸 좋아해서",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012500.1.6",
          "form": "어디 다녀오셨어요?",
          "original_form": "어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012500.1.7",
          "form": "제주도 다녀왔어요",
          "original_form": "제주도 다녀왔어요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012500.1.8",
          "form": "물론 코로나 전이지만",
          "original_form": "물론 코로나 전이지만",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012500.1.9",
          "form": "강릉도 가봤고",
          "original_form": "강릉도 가봤고",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012500.1.10",
          "form": "제주도 좋죠",
          "original_form": "제주도 좋죠",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012500.1.11",
          "form": "송도 가보셨나요?",
          "original_form": "송도 가보셨나요?",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012500.1.12",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012500.1.13",
          "form": "송도 어때요?",
          "original_form": "송도 어때요?",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012500.1.14",
          "form": "바다 야경이 예뻐요",
          "original_form": "바다 야경이 예뻐요",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012500.1.15",
          "form": "저 바다 좋아하는데",
          "original_form": "저 바다 좋아하는데",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012500.1.16",
          "form": "바다 야경이 예쁘다니 한번 가보고싶네요",
          "original_form": "바다 야경이 예쁘다니 한번 가보고싶네요",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012500.1.17",
          "form": "도심하늘에",
          "original_form": "도심하늘에",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012500.1.18",
          "form": "갈매기가 날아다니는게",
          "original_form": "갈매기가 날아다니는게",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012500.1.19",
          "form": "웃기더라고요",
          "original_form": "웃기더라고요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012500.1.20",
          "form": "오 이색적이에욬ㅋㅋㅋ",
          "original_form": "오 이색적이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012500.1.21",
          "form": "비둘기 대신 갈매기라니",
          "original_form": "비둘기 대신 갈매기라니",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012500.1.22",
          "form": "해양도시답네요",
          "original_form": "해양도시답네요",
          "speaker_id": "1",
          "time": "20210824 22:56"
        }
      ]
