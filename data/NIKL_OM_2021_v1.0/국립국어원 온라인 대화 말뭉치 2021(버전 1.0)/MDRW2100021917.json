      "utterance": [
        {
          "id": "MDRW2100021917.1.1",
          "form": "요새 이쪽 주식 많이 올랏던데",
          "original_form": "요새 이쪽 주식 많이 올랏던데",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021917.1.2",
          "form": "우리회사도 재택할때 사용하는중",
          "original_form": "우리회사도 재택할때 사용하는중",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021917.1.3",
          "form": "오 어떻게?",
          "original_form": "오 어떻게?",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100021917.1.4",
          "form": "게임처럼되있는데 가까이가면 캠이 나오는방식임",
          "original_form": "게임처럼되있는데 가까이가면 캠이 나오는방식임",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021917.1.5",
          "form": "세상좋아졌네",
          "original_form": "세상좋아졌네",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021917.1.6",
          "form": "근데 몇일써보니까",
          "original_form": "근데 몇일써보니까",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021917.1.7",
          "form": "실제만나는거보단 아무래도 불편하더라",
          "original_form": "실제만나는거보단 아무래도 불편하더라",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021917.1.8",
          "form": "가상세계 신세계다",
          "original_form": "가상세계 신세계다",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021917.1.9",
          "form": "우리 포립하던때도 메타버스랑 비슷하지않나?",
          "original_form": "우리 포립하던때도 메타버스랑 비슷하지않나?",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021917.1.10",
          "form": "그래도 면대면은 아니니깐?",
          "original_form": "그래도 면대면은 아니니깐?",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021917.1.11",
          "form": "그때는 온게임도 열광할때였지만,,",
          "original_form": "그때는 온게임도 열광할때였지만,,",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021917.1.12",
          "form": "음...그건다르긴하지",
          "original_form": "음...그건다르긴하지",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021917.1.13",
          "form": "이쪽이 유망하긴 한가봐",
          "original_form": "이쪽이 유망하긴 한가봐",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021917.1.14",
          "form": "좀더 발전하긴 해야할듯",
          "original_form": "좀더 발전하긴 해야할듯",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021917.1.15",
          "form": "아무래도 인터넷에서 뭔갈 많이하니깐",
          "original_form": "아무래도 인터넷에서 뭔갈 많이하니깐",
