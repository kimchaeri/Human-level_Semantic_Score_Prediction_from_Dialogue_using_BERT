      "utterance": [
        {
          "id": "MDRW2100039642.1.1",
          "form": "판타지도 좋아하시나요",
          "original_form": "판타지도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039642.1.2",
          "form": "네네 해리포터도 판타지죠?",
          "original_form": "네네 해리포터도 판타지죠?",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039642.1.3",
          "form": "해리포터 덕후에요..",
          "original_form": "해리포터 덕후에요..",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039642.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039642.1.5",
          "form": "저도 해리포터 좋아해요",
          "original_form": "저도 해리포터 좋아해요",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039642.1.6",
          "form": "ㅜㅜ재밌죠 완전 명작이에용..",
          "original_form": "ㅜㅜ재밌죠 완전 명작이에용..",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039642.1.7",
          "form": "정말요..",
          "original_form": "정말요..",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039642.1.8",
          "form": "몇푠 가장 좋아하세여?",
          "original_form": "몇푠 가장 좋아하세여?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039642.1.9",
          "form": "제일 분위기가 맘에드는건 마법사의돌이요ㅎㅎ",
          "original_form": "제일 분위기가 맘에드는건 마법사의돌이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039642.1.10",
          "form": "두근두근한 느낌이 있어요",
          "original_form": "두근두근한 느낌이 있어요",
          "speaker_id": "1",
          "time": "20210903 23:00"
        },
        {
          "id": "MDRW2100039642.1.11",
          "form": "헐 저도요ㅠㅠㅠㅠ",
          "original_form": "헐 저도요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039642.1.12",
          "form": "첫 시작이 너무 좋아요",
          "original_form": "첫 시작이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039642.1.13",
          "form": "해그리드랑 학용품 사러갈때도 넘 잼짜나요 흑흑",
          "original_form": "해그리드랑 학용품 사러갈때도 넘 잼짜나요 흑흑",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100039642.1.14",
          "form": "ㅠㅠ 맞아요......",
          "original_form": "ㅠㅠ 맞아요......",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100039642.1.15",
          "form": "처음 봤을 때 잊지 못해요",
          "original_form": "처음 봤을 때 잊지 못해요",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100039642.1.16",
          "form": "책으로두 보셨나요!?",
          "original_form": "책으로두 보셨나요!?",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100039642.1.17",
          "form": "책으러 1권보다가 멈췄어요",
          "original_form": "책으러 1권보다가 멈췄어요",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100039642.1.18",
          "form": "그렇ㄱ0 재밌다고 하던데요?",
          "original_form": "그렇ㄱ0 재밌다고 하던데요?",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100039642.1.19",
          "form": "네네 책으로 보는게 최고에요 사실!!",
          "original_form": "네네 책으로 보는게 최고에요 사실!!",
          "speaker_id": "1",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100039642.1.20",
