      "utterance": [
        {
          "id": "MDRW2100045564.1.1",
          "form": "라만차의 기사~\r\n이럴떄 목소리 완전 달라",
          "original_form": "라만차의 기사~\r\n이럴떄 목소리 완전 달라",
          "speaker_id": "2",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045564.1.2",
          "form": "할아버지 목소리 완전 확바뀜",
          "original_form": "할아버지 목소리 완전 확바뀜",
          "speaker_id": "2",
          "time": "20210908 03:34"
        },
        {
          "id": "MDRW2100045564.1.3",
          "form": "이영상은 좀 덜하지만",
          "original_form": "이영상은 좀 덜하지만",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.4",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.5",
          "form": "급 중후해짐",
          "original_form": "급 중후해짐",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.7",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.8",
          "form": "산초가",
          "original_form": "산초가",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.9",
          "form": "좀 킹받는데",
          "original_form": "좀 킹받는데",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.10",
          "form": "저만 그런가요",
          "original_form": "저만 그런가요",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.11",
          "form": "산초... 이훈진이 하면",
          "original_form": "산초... 이훈진이 하면",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.12",
          "form": "나는 싼초 나는 싼초 하는거",
          "original_form": "나는 싼초 나는 싼초 하는거",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.13",
          "form": "진짜 러블리한데...",
          "original_form": "진짜 러블리한데...",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.14",
          "form": "킹받네...",
          "original_form": "킹받네...",
          "speaker_id": "1",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그부분 훈진이가하면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그부분 훈진이가하면",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.16",
          "form": "귀여워....",
          "original_form": "귀여워....",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.17",
          "form": "저분이 킹받게하네....",
          "original_form": "저분이 킹받게하네....",
          "speaker_id": "2",
          "time": "20210908 03:35"
        },
        {
          "id": "MDRW2100045564.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ김재만 배우가 저분인가보네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ김재만 배우가 저분인가보네",
          "speaker_id": "1",
          "time": "20210908 03:36"
        },
        {
          "id": "MDRW2100045564.1.19",
          "form": "훈진..",
          "original_form": "훈진..",
          "speaker_id": "2",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100045564.1.20",
          "form": "지금 보내드렸는데",
          "original_form": "지금 보내드렸는데",
          "speaker_id": "2",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100045564.1.21",
          "form": "여배우는 아까 루시역할 김선영..",
          "original_form": "여배우는 아까 루시역할 김선영..",
          "speaker_id": "2",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100045564.1.22",
          "form": "훈진쓰... 러블리의 결정체...",
          "original_form": "훈진쓰... 러블리의 결정체...",
          "speaker_id": "2",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100045564.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100045564.1.24",
          "form": "심지어 이노래도 러블리ㅐㅎ...",
          "original_form": "심지어 이노래도 러블리ㅐㅎ...",
          "speaker_id": "2",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100045564.1.25",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:38"
        },
        {
          "id": "MDRW2100045564.1.26",
          "form": "어떤 느낌인지 알겠다",
          "original_form": "어떤 느낌인지 알겠다",
          "speaker_id": "1",
          "time": "20210908 03:38"
        },
