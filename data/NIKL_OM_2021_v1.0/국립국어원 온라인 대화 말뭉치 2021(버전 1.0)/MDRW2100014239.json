      "utterance": [
        {
          "id": "MDRW2100014239.1.1",
          "form": "치즈돈까스를 좋아하시나봐요",
          "original_form": "치즈돈까스를 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014239.1.2",
          "form": "일주일에 몇 번 정도 드세요?",
          "original_form": "일주일에 몇 번 정도 드세요?",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014239.1.3",
          "form": "두번정도 먹습니다",
          "original_form": "두번정도 먹습니다",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014239.1.4",
          "form": "없어서 못먹죠",
          "original_form": "없어서 못먹죠",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014239.1.5",
          "form": "오 자주 드시네요.",
          "original_form": "오 자주 드시네요.",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014239.1.6",
          "form": "치즈 돈까스 외에 최고 애정하는 음식이 또 있나요?",
          "original_form": "치즈 돈까스 외에 최고 애정하는 음식이 또 있나요?",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014239.1.7",
          "form": "치즈돈가스가 최고존엄이ㅛ",
          "original_form": "치즈돈가스가 최고존엄이ㅛ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014239.1.8",
          "form": "이죠",
          "original_form": "이죠",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014239.1.9",
          "form": "어떤음식 좋아하세요?",
          "original_form": "어떤음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014239.1.10",
          "form": "최고 존엄 중요하죠.",
          "original_form": "최고 존엄 중요하죠.",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014239.1.11",
          "form": "저는 중식러버라서 중화요리를 모두 좋아합니다.",
          "original_form": "저는 중식러버라서 중화요리를 모두 좋아합니다.",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014239.1.12",
          "form": "하나 고르기 어렵네요.",
          "original_form": "하나 고르기 어렵네요.",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014239.1.13",
          "form": "중화비빔밥 좋아하세요?",
          "original_form": "중화비빔밥 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014239.1.14",
          "form": "아뇨.",
          "original_form": "아뇨.",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014239.1.15",
          "form": "밥보다는 면을 선호해서",
          "original_form": "밥보다는 면을 선호해서",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014239.1.16",
          "form": "면보다는 또 고기를 선호해서",
          "original_form": "면보다는 또 고기를 선호해서",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014239.1.17",
          "form": "중화 비빔밥은 어떤 맛인가요?",
          "original_form": "중화 비빔밥은 어떤 맛인가요?",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014239.1.18",
          "form": "짬뽕밥이랑 비슷한것같아요",
          "original_form": "짬뽕밥이랑 비슷한것같아요",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014239.1.19",
          "form": "탕슉 좋아하시겠네요 그럼",
          "original_form": "탕슉 좋아하시겠네요 그럼",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014239.1.20",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014239.1.21",
          "form": "중화요리의 꽃이죠",
          "original_form": "중화요리의 꽃이죠",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014239.1.22",
          "form": "찍먹이세요 부먹이세요?",
          "original_form": "찍먹이세요 부먹이세요?",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014239.1.23",
          "form": "근본은 찍먹이죠.",
          "original_form": "근본은 찍먹이죠.",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014239.1.24",
          "form": "설마",
          "original_form": "설마",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014239.1.25",
          "form": "아니죠?",
          "original_form": "아니죠?",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014239.1.26",
          "form": "부먹은 머리에 소스를 부어버려야죠",
          "original_form": "부먹은 머리에 소스를 부어버려야죠",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014239.1.27",
          "form": "현명하시네요.",
          "original_form": "현명하시네요.",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014239.1.28",
          "form": "디저트는 어떤 것을 좋아하시나요?",
          "original_form": "디저트는 어떤 것을 좋아하시나요?",
