      "utterance": [
        {
          "id": "MDRW2100034718.1.1",
          "form": "요새 방통대 관심갖는 사람들 많더라구요",
          "original_form": "요새 방통대 관심갖는 사람들 많더라구요",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034718.1.2",
          "form": "오 저희 어머니도 불교방송대학다니세요",
          "original_form": "오 저희 어머니도 불교방송대학다니세요",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034718.1.3",
          "form": "혹시 name1님도 생각이 있으신가유",
          "original_form": "혹시 &name1&님도 생각이 있으신가유",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034718.1.4",
          "form": "입학은 쉬운데",
          "original_form": "입학은 쉬운데",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034718.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034718.1.6",
          "form": "졸업이 어렵다면서요",
          "original_form": "졸업이 어렵다면서요",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034718.1.7",
          "form": "네 졸업이 어려운 게 아무래도 오프라인에서 듣는 거같은 흐름이 없어서 흐지부지되기 쉽다는 얘기 같더라구요",
          "original_form": "네 졸업이 어려운 게 아무래도 오프라인에서 듣는 거같은 흐름이 없어서 흐지부지되기 쉽다는 얘기 같더라구요",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034718.1.8",
          "form": "불교방송대학 처음 들었어요!",
          "original_form": "불교방송대학 처음 들었어요!",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034718.1.9",
          "form": "목요일 저녁마다 줌에 모이셔서 절하시더라고요",
          "original_form": "목요일 저녁마다 줌에 모이셔서 절하시더라고요",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034718.1.10",
          "form": "응원하고있습니다",
          "original_form": "응원하고있습니다",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034718.1.11",
          "form": "방통대 가고싶은 학과 있으세요?",
          "original_form": "방통대 가고싶은 학과 있으세요?",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034718.1.12",
          "form": "아니 줌에서 절하시나요",
          "original_form": "아니 줌에서 절하시나요",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034718.1.13",
          "form": "불교학과인걸까요!",
          "original_form": "불교학과인걸까요!",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034718.1.14",
          "form": "네! 스님이 강의해주세요!",
          "original_form": "네! 스님이 강의해주세요!",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034718.1.15",
          "form": "나중에 방통대 법학과 가볼까 궁금하긴 해요 ㅋㅋ",
          "original_form": "나중에 방통대 법학과 가볼까 궁금하긴 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034718.1.16",
          "form": "아주아주 나중에!",
          "original_form": "아주아주 나중에!",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034718.1.17",
          "form": "오 법학~~ 멋져요",
          "original_form": "오 법학~~ 멋져요",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034718.1.18",
          "form": "아니 ㅋㅋ 줌으로 함께 절하시는 수업은 생각도 못했어요 당연할 수 있는데 엄청 신선해요",
          "original_form": "아니 ㅋㅋ 줌으로 함께 절하시는 수업은 생각도 못했어요 당연할 수 있는데 엄청 신선해요",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034718.1.19",
          "form": "이과생 name2님 진로와 미래가 문과로드이신",
          "original_form": "이과생 &name2&님 진로와 미래가 문과로드이신",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034718.1.20",
          "form": "그리고 서로 마음수련을 위해 대화도 하십니다",
          "original_form": "그리고 서로 마음수련을 위해 대화도 하십니다",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034718.1.21",
          "form": "법학에 어떤부분에 관심있으세요?",
          "original_form": "법학에 어떤부분에 관심있으세요?",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034718.1.22",
          "form": "저는 법학을 배우고 싶을 때는 누군갈 고소하고싶을때 뿐인데요.. ㅜ",
          "original_form": "저는 법학을 배우고 싶을 때는 누군갈 고소하고싶을때 뿐인데요.. ㅜ",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034718.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이제 문과길로 들어섰쥬",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이제 문과길로 들어섰쥬",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034718.1.24",
          "form": "부동산법이나 회계관련 법 알아보구 싶어요",
          "original_form": "부동산법이나 회계관련 법 알아보구 싶어요",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034718.1.25",
          "form": "눈탱이 덜맞을거 같아서요(?)",
          "original_form": "눈탱이 덜맞을거 같아서요(?)",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034718.1.26",
          "form": "합리적이십니다",
          "original_form": "합리적이십니다",
          "speaker_id": "1",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034718.1.27",
          "form": "마음수련 대화도 넘 멋지네요 뭔가 수업이 힐링일거 같은 느낌이에요",
          "original_form": "마음수련 대화도 넘 멋지네요 뭔가 수업이 힐링일거 같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034718.1.28",
          "form": "name1님 만약 방통대가신다고 하면 어떤 과 관심있으세요?",
          "original_form": "&name1&님 만약 방통대가신다고 하면 어떤 과 관심있으세요?",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034718.1.29",
          "form": "어머니가 마음의 평화를 많이 찾으신거같아요",
          "original_form": "어머니가 마음의 평화를 많이 찾으신거같아요",
          "speaker_id": "1",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034718.1.30",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034718.1.31",
          "form": "저는 고민되네요..!",
          "original_form": "저는 고민되네요..!",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034718.1.32",
          "form": "식물을 배우는 학과요?",
          "original_form": "식물을 배우는 학과요?",
          "speaker_id": "1",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034718.1.33",
          "form": "무인도 떨어져도 살아남을것 같아요",
          "original_form": "무인도 떨어져도 살아남을것 같아요",
          "speaker_id": "1",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034718.1.34",
          "form": "진심으로 언젠가 제가 살아있을때 지구가 물에 잠길거 같아서요",
          "original_form": "진심으로 언젠가 제가 살아있을때 지구가 물에 잠길거 같아서요",
          "speaker_id": "1",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034718.1.35",
          "form": "아 가능하죠",
          "original_form": "아 가능하죠",
          "speaker_id": "2",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100034718.1.36",
          "form": "벌써 지대 낮은 곳은 가라앉기 시작했으니까요..",
          "original_form": "벌써 지대 낮은 곳은 가라앉기 시작했으니까요..",
