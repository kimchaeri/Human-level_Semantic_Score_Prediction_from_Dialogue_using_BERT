      "utterance": [
        {
          "id": "MDRW2100045411.1.1",
          "form": "시옷미음 재료가 뭐가 있을까요",
          "original_form": "시옷미음 재료가 뭐가 있을까요",
          "speaker_id": "1",
          "time": "20210908 02:24"
        },
        {
          "id": "MDRW2100045411.1.2",
          "form": "되게 유명한 프랜차이즈에용",
          "original_form": "되게 유명한 프랜차이즈에용",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045411.1.3",
          "form": "시옷미움 재료 맞춰볼게요 그래야 국수를 맞출 것 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "시옷미움 재료 맞춰볼게요 그래야 국수를 맞출 것 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045411.1.4",
          "form": "프랜차이즈 이름도 네글자",
          "original_form": "프랜차이즈 이름도 네글자",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045411.1.5",
          "form": "생선인가요???",
          "original_form": "생선인가요???",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045411.1.6",
          "form": "엥 아니욬ㅋㅋㅋ",
          "original_form": "엥 아니욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045411.1.7",
          "form": "시옷미음이란 게",
          "original_form": "시옷미음이란 게",
          "speaker_id": "1",
          "time": "20210908 02:25"
        },
        {
          "id": "MDRW2100045411.1.8",
          "form": "야채종류인가요?",
          "original_form": "야채종류인가요?",
          "speaker_id": "1",
          "time": "20210908 02:26"
        },
        {
          "id": "MDRW2100045411.1.9",
          "form": "아니영 !",
          "original_form": "아니영 !",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045411.1.10",
          "form": "이 뭐라하지 흠ㅠ 완전 국수 주재료에요",
          "original_form": "이 뭐라하지 흠ㅠ 완전 국수 주재료에요",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045411.1.11",
          "form": "소면???",
          "original_form": "소면???",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045411.1.12",
          "form": "소면국수..???",
          "original_form": "소면국수..???",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045411.1.13",
          "form": "오 면까지맞아영",
          "original_form": "오 면까지맞아영",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045411.1.14",
          "form": "생각보다 어렵네옄ㅋㅋㅋㅠ",
          "original_form": "생각보다 어렵네옄ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045411.1.15",
          "form": "ㅅ은 다른거예여ㅎㅎ",
          "original_form": "ㅅ은 다른거예여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045411.1.16",
          "form": "그렇담 생면국수??????????",
          "original_form": "그렇담 생면국수??????????",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045411.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045411.1.18",
          "form": "맞습니당",
          "original_form": "맞습니당",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045411.1.19",
          "form": "와 왕 와와 드디어 맞췄네요",
          "original_form": "와 왕 와와 드디어 맞췄네요",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045411.1.20",
          "form": "인간승리다",
          "original_form": "인간승리다",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045411.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ 이거 유명한 메뉴지 않아여ㅋㅋㅋㅋㅋㅋ? 나름",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 이거 유명한 메뉴지 않아여ㅋㅋㅋㅋㅋㅋ? 나름",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045411.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
