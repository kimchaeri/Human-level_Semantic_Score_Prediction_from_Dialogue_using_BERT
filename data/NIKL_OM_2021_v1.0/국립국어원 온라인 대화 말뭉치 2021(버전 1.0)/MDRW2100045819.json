        },
        {
          "id": "MDRW2100045819.1.3",
          "form": "오 뭐보는데",
          "original_form": "오 뭐보는데",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.4",
          "form": "나 요즘 브나나 본다",
          "original_form": "나 요즘 브나나 본다",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.5",
          "form": "??그게 뭐야 브나나?",
          "original_form": "??그게 뭐야 브나나?",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.6",
          "form": "ㅋㅋ 브루클린 나인 나인",
          "original_form": "ㅋㅋ 브루클린 나인 나인",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.7",
          "form": "오 드라마야?",
          "original_form": "오 드라마야?",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.8",
          "form": "응 시트콤이야",
          "original_form": "응 시트콤이야",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.9",
          "form": "시트콤이면ㅁ재밌겠다",
          "original_form": "시트콤이면ㅁ재밌겠다",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.10",
          "form": "응 시간도 짧아서 좋아",
          "original_form": "응 시간도 짧아서 좋아",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100045819.1.11",
          "form": "밥먹을때 보기 딱이겠다",
          "original_form": "밥먹을때 보기 딱이겠다",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045819.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞어 한끼당 한편씩 보면 딱이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞어 한끼당 한편씩 보면 딱이야",
          "speaker_id": "1",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100045819.1.13",
          "form": "나도 짧은 시트콤 같은거 좋아해서 땡큐",
