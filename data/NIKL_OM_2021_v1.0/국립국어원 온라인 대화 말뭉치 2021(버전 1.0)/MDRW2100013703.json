      "utterance": [
        {
          "id": "MDRW2100013703.1.1",
          "form": "명절음식 뭐 좋아해요",
          "original_form": "명절음식 뭐 좋아해요",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013703.1.2",
          "form": "저 산적",
          "original_form": "저 산적",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.4",
          "form": "산적 배추전",
          "original_form": "산적 배추전",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.5",
          "form": "거봐 푸드메이트라니까",
          "original_form": "거봐 푸드메이트라니까",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.6",
          "form": "투톱입니다",
          "original_form": "투톱입니다",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.7",
          "form": "쓰읍 애호박..?",
          "original_form": "쓰읍 애호박..?",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.8",
          "form": "애호박전도 좋아하긴 하는데 너무 빨리 쉬지 안ㄹ나요",
          "original_form": "애호박전도 좋아하긴 하는데 너무 빨리 쉬지 안ㄹ나요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.9",
          "form": "아 is시오",
          "original_form": "아 is시오",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013703.1.10",
          "form": "나 오늘부터 배추전 좋아할랑께",
          "original_form": "나 오늘부터 배추전 좋아할랑께",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.11",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.12",
          "form": "쉬는게 어딨어요 다 내입으로 들어갈건디",
          "original_form": "쉬는게 어딨어요 다 내입으로 들어갈건디",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.13",
          "form": "제가 애호박전 조아해볼게요",
          "original_form": "제가 애호박전 조아해볼게요",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.14",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.16",
          "form": "이분 찐인",
          "original_form": "이분 찐인",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.17",
          "form": "산적재료 뭐 좋아해쇼",
          "original_form": "산적재료 뭐 좋아해쇼",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.18",
          "form": "고기 햄 맛살 당근 파",
          "original_form": "고기 햄 맛살 당근 파",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013703.1.19",
          "form": "재료가 뭐 있어요? 고기 양념말고?",
          "original_form": "재료가 뭐 있어요? 고기 양념말고?",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.20",
          "form": "아나 꼬치해서 드세요??!",
          "original_form": "아나 꼬치해서 드세요??!",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.21",
          "form": "몰랏다",
          "original_form": "몰랏다",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.22",
          "form": "대박맛잇겠다",
          "original_form": "대박맛잇겠다",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.23",
          "form": "꼬치안해먹어요?",
          "original_form": "꼬치안해먹어요?",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.24",
          "form": "아  몰랐네?",
          "original_form": "아  몰랐네?",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.25",
          "form": "저희는 그 산적 구워서 1인 1판이에요",
          "original_form": "저희는 그 산적 구워서 1인 1판이에요",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013703.1.26",
          "form": "단무지안넣는다길래 꼬치산적 말하는 줄",
          "original_form": "단무지안넣는다길래 꼬치산적 말하는 줄",
          "speaker_id": "1",
          "time": "20210825 15:43"
        }
