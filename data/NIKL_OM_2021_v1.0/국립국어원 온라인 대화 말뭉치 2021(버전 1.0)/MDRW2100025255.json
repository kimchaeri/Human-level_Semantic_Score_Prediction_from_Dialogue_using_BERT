      "utterance": [
        {
          "id": "MDRW2100025255.1.1",
          "form": "무사~~~ 무휼~~~",
          "original_form": "무사~~~ 무휼~~~",
          "speaker_id": "2",
          "time": "20210901 03:14"
        },
        {
          "id": "MDRW2100025255.1.2",
          "form": "갈수록 강해져서ㅋㅋ",
          "original_form": "갈수록 강해져서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025255.1.3",
          "form": "근데 방원이 아역이",
          "original_form": "근데 방원이 아역이",
          "speaker_id": "2",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025255.1.4",
          "form": "세종으로 또 나와서",
          "original_form": "세종으로 또 나와서",
          "speaker_id": "2",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025255.1.5",
          "form": "좀 잉했는데ㅋㅋ",
          "original_form": "좀 잉했는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:15"
        },
        {
          "id": "MDRW2100025255.1.6",
          "form": "아역이 중복으로 나왔나요?",
          "original_form": "아역이 중복으로 나왔나요?",
          "speaker_id": "1",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025255.1.7",
          "form": "네 방원이 아역이 나중에 세종 아역으로",
          "original_form": "네 방원이 아역이 나중에 세종 아역으로",
          "speaker_id": "2",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025255.1.8",
          "form": "겹쳐서 ㅋㅋ",
          "original_form": "겹쳐서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:18"
        },
        {
          "id": "MDRW2100025255.1.9",
          "form": "그랬었구나~ ㅋㅋㅋㅋㅋ 아니 하나의 오점이네요",
          "original_form": "그랬었구나~ ㅋㅋㅋㅋㅋ 아니 하나의 오점이네요",
          "speaker_id": "1",
          "time": "20210901 03:19"
        },
        {
          "id": "MDRW2100025255.1.10",
          "form": "네 뭐 그래도 드라마 괜찮았어요",
          "original_form": "네 뭐 그래도 드라마 괜찮았어요",
          "speaker_id": "2",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025255.1.11",
          "form": "그쵸 ㅋㅋㅋ 저는 그걸 몰랐어요",
          "original_form": "그쵸 ㅋㅋㅋ 저는 그걸 몰랐어요",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025255.1.12",
          "form": "무휼 캐릭터가 너무 멋있고 한글창제과정이 멋져서 좋았어요",
          "original_form": "무휼 캐릭터가 너무 멋있고 한글창제과정이 멋져서 좋았어요",
          "speaker_id": "1",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025255.1.13",
          "form": "이걸 한글날에 몰아서 봣었거든욬ㅋ",
          "original_form": "이걸 한글날에 몰아서 봣었거든욬ㅋ",
          "speaker_id": "1",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025255.1.14",
          "form": "아 잠깐",
          "original_form": "아 잠깐",
          "speaker_id": "2",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025255.1.15",
          "form": "뿌나 아닌데",
          "original_form": "뿌나 아닌데",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025255.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025255.1.17",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025255.1.18",
          "form": "육룡이나르샤",
          "original_form": "육룡이나르샤",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025255.1.19",
          "form": "뿌나 앞이야기였어요ㅠ",
          "original_form": "뿌나 앞이야기였어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 03:24"
        },
        {
          "id": "MDRW2100025255.1.20",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025255.1.21",
          "form": "무휼 거기서도 나와서ㅠ",
          "original_form": "무휼 거기서도 나와서ㅠ",
          "speaker_id": "2",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025255.1.22",
          "form": "그렇군여....ㅋㅋㅋㅋ",
          "original_form": "그렇군여....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025255.1.23",
          "form": "헷갈렸네요 ㅠㅠ",
          "original_form": "헷갈렸네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025255.1.24",
          "form": "조진웅 얘기하면서 무휼이야기 한것 같은데 ㅋㅋㅋ",
          "original_form": "조진웅 얘기하면서 무휼이야기 한것 같은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:26"
        },
        {
          "id": "MDRW2100025255.1.25",
          "form": "저도 그 드라마는 알아여!",
          "original_form": "저도 그 드라마는 알아여!",
          "speaker_id": "1",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025255.1.26",
          "form": "무휼이 그... 누구더라",
          "original_form": "무휼이 그... 누구더라",
          "speaker_id": "1",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025255.1.27",
          "form": "미생나온사람",
          "original_form": "미생나온사람",
          "speaker_id": "1",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025255.1.28",
          "form": "그게 넘어오면서 짤려서 ㅋㅋ",
          "original_form": "그게 넘어오면서 짤려서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025255.1.29",
          "form": "뿌나도 봐야되고 볼게 많군요",
          "original_form": "뿌나도 봐야되고 볼게 많군요",
          "speaker_id": "2",
          "time": "20210901 03:27"
        },
        {
          "id": "MDRW2100025255.1.30",
          "form": "얼굴밖에 생각이 안나네요..ㅋㅋㅋ",
          "original_form": "얼굴밖에 생각이 안나네요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:27"
        }
      ]
    }
  ]
