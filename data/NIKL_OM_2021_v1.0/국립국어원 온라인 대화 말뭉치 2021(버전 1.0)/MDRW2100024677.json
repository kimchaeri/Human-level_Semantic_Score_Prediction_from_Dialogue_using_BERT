      "utterance": [
        {
          "id": "MDRW2100024677.1.1",
          "form": "맞아ㅋㅋㅋ 안 그래도 나 일본여행 갔을 때 비가 좀 왔었다?",
          "original_form": "맞아ㅋㅋㅋ 안 그래도 나 일본여행 갔을 때 비가 좀 왔었다?",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024677.1.2",
          "form": "다행히 온천 가는 길에 와서 대충 다 맞고 씻었던 기억이 나",
          "original_form": "다행히 온천 가는 길에 와서 대충 다 맞고 씻었던 기억이 나",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024677.1.3",
          "form": "넌 여행할 때 날씨 중요하게 봐?",
          "original_form": "넌 여행할 때 날씨 중요하게 봐?",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024677.1.4",
          "form": "응 날씨가 좀중요해",
          "original_form": "응 날씨가 좀중요해",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024677.1.5",
          "form": "흐리면 그나마 참는데",
          "original_form": "흐리면 그나마 참는데",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024677.1.6",
          "form": "비오면 숙소에서 안나올듯",
          "original_form": "비오면 숙소에서 안나올듯",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024677.1.7",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋ의도치않은 호캉스",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋ의도치않은 호캉스",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024677.1.8",
          "form": "일본여행  도쿄도 재밌겠다",
          "original_form": "일본여행  도쿄도 재밌겠다",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100024677.1.9",
          "form": "그러게 도쿄도 가보고 싶어 일본의 수도는 어떤 모습이려나",
          "original_form": "그러게 도쿄도 가보고 싶어 일본의 수도는 어떤 모습이려나",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100024677.1.10",
          "form": "넌 사람 많은 곳으로 여행가는 걸 좋아해?",
          "original_form": "넌 사람 많은 곳으로 여행가는 걸 좋아해?",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100024677.1.11",
          "form": "나는 많으면 기빨리더라",
          "original_form": "나는 많으면 기빨리더라",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024677.1.12",
          "form": "막 오래구경하고싶은데",
          "original_form": "막 오래구경하고싶은데",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024677.1.13",
          "form": "치여서  이동해",
          "original_form": "치여서  이동해",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024677.1.14",
          "form": "맞아ㅋㅋㅋㅋ 여행객들 너무 많아서 휩쓸려다니잖아",
          "original_form": "맞아ㅋㅋㅋㅋ 여행객들 너무 많아서 휩쓸려다니잖아",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024677.1.15",
          "form": "그렇다고 너무없으면 가게들어가기 뻘쭘해",
          "original_form": "그렇다고 너무없으면 가게들어가기 뻘쭘해",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024677.1.16",
          "form": "맞아 진짜 동감",
          "original_form": "맞아 진짜 동감",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024677.1.17",
          "form": "안 사고 나가려고 할 때 좀 뻘쭘해짐",
          "original_form": "안 사고 나가려고 할 때 좀 뻘쭘해짐",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024677.1.18",
          "form": "구경하려고 가는게 반이니까",
          "original_form": "구경하려고 가는게 반이니까",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024677.1.19",
          "form": "맞아 기념품가게에 기상천외한 것들이 많잖아",
          "original_form": "맞아 기념품가게에 기상천외한 것들이 많잖아",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024677.1.20",
          "form": "또 막상 사려면 나한테 필요없는 물건이라",
          "original_form": "또 막상 사려면 나한테 필요없는 물건이라",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024677.1.21",
          "form": "정말 딱 그 여행지만 기념할 용도",
          "original_form": "정말 딱 그 여행지만 기념할 용도",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024677.1.22",
          "form": "먹는과자가 ㅈㅔ일 좋더라 그래서",
          "original_form": "먹는과자가 ㅈㅔ일 좋더라 그래서",
          "speaker_id": "1",
          "time": "20210831 17:09"
        }
      ]
    }
