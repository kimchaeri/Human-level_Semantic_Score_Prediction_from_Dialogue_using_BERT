        },
        {
          "id": "MDRW2100010369.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010369.1.3",
          "form": "영양제 드시나요?",
          "original_form": "영양제 드시나요?",
          "speaker_id": "1",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010369.1.4",
          "form": "넹 저는 유산균이랑 비오틴 먹어요!",
          "original_form": "넹 저는 유산균이랑 비오틴 먹어요!",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010369.1.5",
          "form": "영양제 드세요?",
          "original_form": "영양제 드세요?",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010369.1.6",
          "form": "오 비오틴드시는구나",
          "original_form": "오 비오틴드시는구나",
          "speaker_id": "1",
          "time": "20210823 17:40"
        },
        {
          "id": "MDRW2100010369.1.7",
          "form": "저는 종비랑 오메가 먹는답니다ㅋㅋㅋ",
          "original_form": "저는 종비랑 오메가 먹는답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:40"
        },
        {
          "id": "MDRW2100010369.1.8",
          "form": "머리카락이 너무얇아진거같아가지구,,",
          "original_form": "머리카락이 너무얇아진거같아가지구,,",
          "speaker_id": "2",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100010369.1.9",
          "form": "종비는 고함량으로 먹어야대요 ㅋㅋㅋㅋㅋ",
          "original_form": "종비는 고함량으로 먹어야대요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:01"
        },
        {
          "id": "MDRW2100010369.1.10",
          "form": "비오틴",
          "original_form": "비오틴",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100010369.1.11",
          "form": "효과좀 있나요?",
          "original_form": "효과좀 있나요?",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100010369.1.12",
          "form": "먹고있긴한데 확실히 네일받으러가니까 손톱엄청건강하다고하더라구요",
          "original_form": "먹고있긴한데 확실히 네일받으러가니까 손톱엄청건강하다고하더라구요",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100010369.1.13",
          "form": "효과는 있나봐요",
          "original_form": "효과는 있나봐요",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100010369.1.14",
          "form": "와우 좋네요ㅋㅋㅋ",
          "original_form": "와우 좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100010369.1.15",
          "form": "저는 영양제 더살까고민중이예요",
          "original_form": "저는 영양제 더살까고민중이예요",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100010369.1.16",
          "form": "어떤영양제요?",
          "original_form": "어떤영양제요?",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100010369.1.17",
          "form": "프로폴리스정도?",
          "original_form": "프로폴리스정도?",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100010369.1.18",
          "form": "영양제 잘 챙겨먹어야 하더라구요 나이먹다보니 느끼는게 많아서 ㅠㅋㅋ",
          "original_form": "영양제 잘 챙겨먹어야 하더라구요 나이먹다보니 느끼는게 많아서 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:20"
        },
        {
          "id": "MDRW2100010369.1.19",
          "form": "루테인도 챙겨먹으라고는 하던데",
          "original_form": "루테인도 챙겨먹으라고는 하던데",
          "speaker_id": "2",
          "time": "20210825 09:20"
        },
        {
          "id": "MDRW2100010369.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100010369.1.21",
          "form": "루테인 드세요ㅠ",
          "original_form": "루테인 드세요ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100010369.1.22",
          "form": "저도 먹으려구요",
          "original_form": "저도 먹으려구요",
          "speaker_id": "1",
          "time": "20210825 11:31"
        }
      ]
