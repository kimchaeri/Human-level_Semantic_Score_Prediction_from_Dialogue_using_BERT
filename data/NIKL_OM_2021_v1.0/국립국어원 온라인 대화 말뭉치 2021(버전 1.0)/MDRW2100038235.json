      "utterance": [
        {
          "id": "MDRW2100038235.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100038235.1.2",
          "form": "좋아하지는 않는데",
          "original_form": "좋아하지는 않는데",
          "speaker_id": "2",
          "time": "20210902 01:24"
        },
        {
          "id": "MDRW2100038235.1.3",
          "form": "주2회 하는건 있어요",
          "original_form": "주2회 하는건 있어요",
          "speaker_id": "2",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038235.1.4",
          "form": "필라테스요 ㅎ",
          "original_form": "필라테스요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038235.1.5",
          "form": "오 할때는 재미있으세요?",
          "original_form": "오 할때는 재미있으세요?",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038235.1.6",
          "form": "저도 필라테스 하려고 찾아보는데 마땅한 곳이 없어요",
          "original_form": "저도 필라테스 하려고 찾아보는데 마땅한 곳이 없어요",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038235.1.7",
          "form": "하고나면 좋아요",
          "original_form": "하고나면 좋아요",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038235.1.8",
          "form": "몸 움직이기 귀찮네요 회사 안나가니까 더욱 ㅋㅋㅋㅋ",
          "original_form": "몸 움직이기 귀찮네요 회사 안나가니까 더욱 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038235.1.9",
          "form": "역시 운동은 하고나면 좋긴 하죠",
          "original_form": "역시 운동은 하고나면 좋긴 하죠",
          "speaker_id": "1",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038235.1.10",
          "form": "원래는 허리아파서 시작했는데",
          "original_form": "원래는 허리아파서 시작했는데",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038235.1.11",
          "form": "하고나서 허리가 안아파서요",
          "original_form": "하고나서 허리가 안아파서요",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038235.1.12",
          "form": "계속하고 있어요 ㅎㅎ",
          "original_form": "계속하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038235.1.13",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038235.1.14",
          "form": "안하면 다시 아파질까봐 ㅎ",
          "original_form": "안하면 다시 아파질까봐 ㅎ",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038235.1.15",
          "form": "제 친구는 허리아파서 도수치료 꼬박꼬박 받더니 그래도 안되겠다고 하더라고요",
          "original_form": "제 친구는 허리아파서 도수치료 꼬박꼬박 받더니 그래도 안되겠다고 하더라고요",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038235.1.16",
          "form": "저도 도수치료요",
          "original_form": "저도 도수치료요",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038235.1.17",
          "form": "아파서 도수치료받고 좋아지다가 \r\n다시 삐끗....",
          "original_form": "아파서 도수치료받고 좋아지다가 \r\n다시 삐끗....",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038235.1.18",
          "form": "아이쿠야...",
          "original_form": "아이쿠야...",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038235.1.19",
          "form": "또 도수치료받으라고 이야기하시는데...\r\n이병원이 환자가 많아서 예약하기도",
          "original_form": "또 도수치료받으라고 이야기하시는데...\r\n이병원이 환자가 많아서 예약하기도",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038235.1.20",
          "form": "어렵고 금액도 비싸고 실비가 되지만",
          "original_form": "어렵고 금액도 비싸고 실비가 되지만",
          "speaker_id": "2",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038235.1.21",
          "form": "한번가면 2시간정도 걸려서...",
          "original_form": "한번가면 2시간정도 걸려서...",
          "speaker_id": "2",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038235.1.22",
          "form": "치료받기도 어렵네요",
          "original_form": "치료받기도 어렵네요",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038235.1.23",
          "form": "이돈으로 필라를 하자해서 시작했어요 ㅎㅎ",
          "original_form": "이돈으로 필라를 하자해서 시작했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038235.1.24",
          "form": "회사다니면서 운동하는거 진짜 대단한거라 생각해요",
          "original_form": "회사다니면서 운동하는거 진짜 대단한거라 생각해요",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038235.1.25",
          "form": "필라가 원래 재활운동이 시초이니...",
          "original_form": "필라가 원래 재활운동이 시초이니...",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038235.1.26",
          "form": "제 친구도 필라테스 하다가 바빠져서 못했더니 뻐근해서 죽을거 같다고 했었어요",
          "original_form": "제 친구도 필라테스 하다가 바빠져서 못했더니 뻐근해서 죽을거 같다고 했었어요",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038235.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038235.1.28",
          "form": "걔보면 허리 아픈게 진짜 고질병인거 같아요",
          "original_form": "걔보면 허리 아픈게 진짜 고질병인거 같아요",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038235.1.29",
          "form": "하다가 안하면 몸에서 티가 나더라구요",
          "original_form": "하다가 안하면 몸에서 티가 나더라구요",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038235.1.30",
          "form": "진짜 걔랑 똑같은 말 하시네여 ㅋㅋㅋ",
          "original_form": "진짜 걔랑 똑같은 말 하시네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038235.1.31",
          "form": "나아지셨으면 좋겠다",
          "original_form": "나아지셨으면 좋겠다",
          "speaker_id": "1",
          "time": "20210902 01:32"
        }
      ]
    }
