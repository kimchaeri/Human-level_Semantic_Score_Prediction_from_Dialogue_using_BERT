      "utterance": [
        {
          "id": "MDRW2100005335.1.1",
          "form": "밤공기는 제법 선선하네요~",
          "original_form": "밤공기는 제법 선선하네요~",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005335.1.2",
          "form": "그렇죠 지금도 시원하니 좋아요",
          "original_form": "그렇죠 지금도 시원하니 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005335.1.3",
          "form": "저 친한 언니는 비 좋아하는데",
          "original_form": "저 친한 언니는 비 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005335.1.4",
          "form": "전 비 싫어요",
          "original_form": "전 비 싫어요",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005335.1.5",
          "form": "그 우중충한 분위기 좋아 하더라구요 ㅎㅎ",
          "original_form": "그 우중충한 분위기 좋아 하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005335.1.6",
          "form": "어떨땐 알것도 같고요",
          "original_form": "어떨땐 알것도 같고요",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005335.1.7",
          "form": "비 좋아 하세요?",
          "original_form": "비 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005335.1.8",
          "form": "전 비에 신발 젖는게 싫어요",
          "original_form": "전 비에 신발 젖는게 싫어요",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005335.1.9",
          "form": "젖으묜 하루 종일 찝찝해서요",
          "original_form": "젖으묜 하루 종일 찝찝해서요",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005335.1.10",
          "form": "여름에 더울 때 한번씩 소나기 내리는 건 좋은데",
          "original_form": "여름에 더울 때 한번씩 소나기 내리는 건 좋은데",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005335.1.11",
          "form": "저도 축축한 걸 싫어해서",
          "original_form": "저도 축축한 걸 싫어해서",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005335.1.12",
          "form": "맞아요 그땐 시원한데",
          "original_form": "맞아요 그땐 시원한데",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005335.1.13",
          "form": "하루종일은 너무 사람이 묶이게 되서 싫어요",
          "original_form": "하루종일은 너무 사람이 묶이게 되서 싫어요",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005335.1.14",
          "form": "어제 베란다 열어놔서 비도샜어요 ㅡㅡ",
          "original_form": "어제 베란다 열어놔서 비도샜어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005335.1.15",
          "form": "네 근데 요즘은 너무 갑자기 비가 와서",
          "original_form": "네 근데 요즘은 너무 갑자기 비가 와서",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005335.1.16",
          "form": "또 그러다 갑자기 개고",
          "original_form": "또 그러다 갑자기 개고",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005335.1.17",
          "form": "그렇죠 대비가 안돼요",
          "original_form": "그렇죠 대비가 안돼요",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005335.1.18",
          "form": "나갔다 갑자기 비와서요",
          "original_form": "나갔다 갑자기 비와서요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005335.1.19",
          "form": "네 우산을 항상 챙겨야하나",
          "original_form": "네 우산을 항상 챙겨야하나",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005335.1.20",
          "form": "우산 작아도 짐이라 성가셔요",
          "original_form": "우산 작아도 짐이라 성가셔요",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005335.1.21",
          "form": "근데 챙겨야죠",
          "original_form": "근데 챙겨야죠",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005335.1.22",
          "form": "맞을순 없고",
          "original_form": "맞을순 없고",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005335.1.23",
          "form": "근데 갑자기 폭우 내리면",
          "original_form": "근데 갑자기 폭우 내리면",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005335.1.24",
          "form": "비가 넘쏟아져서 우산고 작으면",
          "original_form": "비가 넘쏟아져서 우산고 작으면",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005335.1.25",
          "form": "사실 우산은 쓰나마나예요",
          "original_form": "사실 우산은 쓰나마나예요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.26",
          "form": "다맞아요",
          "original_form": "다맞아요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.28",
          "form": "다 젖어요",
          "original_form": "다 젖어요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.29",
          "form": "정말 날씨가 요상해요 요즘",
          "original_form": "정말 날씨가 요상해요 요즘",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.30",
          "form": "대형 골프우산도 젖어요",
          "original_form": "대형 골프우산도 젖어요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.31",
          "form": "그러게요 다변했어요",
          "original_form": "그러게요 다변했어요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005335.1.33",
          "form": "오늘은 일단 아침에 날이 맑네요~",
          "original_form": "오늘은 일단 아침에 날이 맑네요~",
          "speaker_id": "1",
          "time": "20210822 10:15"
        }
      ]
    }
