      "utterance": [
        {
          "id": "MDRW2100045749.1.1",
          "form": "ㅋㅋㅋㅋ푹 주무셨나요! 저는 밤을 불태웠습니다 허허허",
          "original_form": "ㅋㅋㅋㅋ푹 주무셨나요! 저는 밤을 불태웠습니다 허허허",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045749.1.2",
          "form": "어제 저녁식사 하셨나요?",
          "original_form": "어제 저녁식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045749.1.3",
          "form": "네네 간단하게 먹었어요",
          "original_form": "네네 간단하게 먹었어요",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045749.1.4",
          "form": "헉 밤새셨어요?",
          "original_form": "헉 밤새셨어요?",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045749.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ오늘 마지막으로 달리기 위해 새브렀지 뭐예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ오늘 마지막으로 달리기 위해 새브렀지 뭐예요",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045749.1.6",
          "form": "그럼 제가 저녁 뭐 드셨었는지 맞춰볼게요!",
          "original_form": "그럼 제가 저녁 뭐 드셨었는지 맞춰볼게요!",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045749.1.7",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "2",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045749.1.8",
          "form": "헉 네 한식이예요",
          "original_form": "헉 네 한식이예요",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045749.1.9",
          "form": "매운 음식인가요?",
          "original_form": "매운 음식인가요?",
          "speaker_id": "2",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100045749.1.10",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045749.1.11",
          "form": "살짝 그 쪽인 것 같긴 하지만 맵지않았어요",
          "original_form": "살짝 그 쪽인 것 같긴 하지만 맵지않았어요",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045749.1.12",
          "form": "힌트입니당 ㅎㅎ",
          "original_form": "힌트입니당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045749.1.13",
          "form": "으으으음 빨간색 요리인가요?",
          "original_form": "으으으음 빨간색 요리인가요?",
          "speaker_id": "2",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100045749.1.14",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100045749.1.15",
          "form": "면 요리인가요?",
          "original_form": "면 요리인가요?",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045749.1.16",
          "form": "아니요 밥이에요",
          "original_form": "아니요 밥이에요",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045749.1.17",
          "form": "국물요리인가요!",
          "original_form": "국물요리인가요!",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045749.1.18",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045749.1.19",
          "form": "앗 고기가 들어가나요?",
          "original_form": "앗 고기가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045749.1.20",
          "form": "네 거의 근접해진 것 같아요 ㅎㅎ",
          "original_form": "네 거의 근접해진 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045749.1.21",
          "form": "김치가 들어가나요?",
          "original_form": "김치가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 07:46"
        },
        {
          "id": "MDRW2100045749.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:48"
        },
        {
