      "utterance": [
        {
          "id": "MDRW2100040688.1.1",
          "form": "저는 그게 귀여워서 좋아요",
          "original_form": "저는 그게 귀여워서 좋아요",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040688.1.2",
          "form": "세련된건 뭔가 그 느낌이 안나서!!",
          "original_form": "세련된건 뭔가 그 느낌이 안나서!!",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040688.1.3",
          "form": "투는 못해봤지만 시도도하지는 않을 것 같아요",
          "original_form": "투는 못해봤지만 시도도하지는 않을 것 같아요",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040688.1.4",
          "form": "어렸을때부터 투를 했거든욬ㅋㅋ",
          "original_form": "어렸을때부터 투를 했거든욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040688.1.5",
          "form": "아 처음 알았어요",
          "original_form": "아 처음 알았어요",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040688.1.6",
          "form": "그런데 뭔가 옹기종기한 영상이 잇죻ㅎ",
          "original_form": "그런데 뭔가 옹기종기한 영상이 잇죻ㅎ",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040688.1.7",
          "form": "ㅋㅋㅋ 귀여워요 그래픽이 좋지 않아서",
          "original_form": "ㅋㅋㅋ 귀여워요 그래픽이 좋지 않아서",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040688.1.8",
          "form": "입장객들이 레고같은 느낌?",
          "original_form": "입장객들이 레고같은 느낌?",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040688.1.9",
          "form": "미션 성공하고 다같이 풍선 날려줄때 좋아요",
          "original_form": "미션 성공하고 다같이 풍선 날려줄때 좋아요",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040688.1.10",
          "form": "아 표현 딱이에요 레고 같은 느낌",
          "original_form": "아 표현 딱이에요 레고 같은 느낌",
          "speaker_id": "2",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040688.1.11",
          "form": "ㅋㅋㅋㅋ 네",
          "original_form": "ㅋㅋㅋㅋ 네",
          "speaker_id": "1",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040688.1.12",
          "form": "귀여운거 좋아하거든요",
          "original_form": "귀여운거 좋아하거든요",
          "speaker_id": "1",
          "time": "20210905 00:10"
        },
        {
          "id": "MDRW2100040688.1.13",
          "form": "요즘 좋아하는 드라마나 영화 있으세요?",
          "original_form": "요즘 좋아하는 드라마나 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040688.1.14",
          "form": "저는 티비도 없고 티빙도 해지한후로는",
          "original_form": "저는 티비도 없고 티빙도 해지한후로는",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040688.1.15",
          "form": "보는게없어요",
          "original_form": "보는게없어요",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040688.1.16",
          "form": "웹툰보고",
          "original_form": "웹툰보고",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040688.1.17",
          "form": "예전엔 아즈망가대왕이라고",
          "original_form": "예전엔 아즈망가대왕이라고",
          "speaker_id": "2",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040688.1.18",
          "form": "그러시구나!",
          "original_form": "그러시구나!",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.19",
          "form": "일본 애니 엄청좋아했어욬ㅋㅋㅋ",
          "original_form": "일본 애니 엄청좋아했어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.20",
          "form": "아 저 그거 본적 있어요!!",
          "original_form": "아 저 그거 본적 있어요!!",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.21",
          "form": "코난 요런거",
          "original_form": "코난 요런거",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.22",
          "form": "종종 보고요",
          "original_form": "종종 보고요",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.23",
          "form": "아즈망가대왕이엄청 귀여워요",
          "original_form": "아즈망가대왕이엄청 귀여워요",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.24",
          "form": "맞아요 그래서 봤어요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 그래서 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040688.1.25",
          "form": "지금은 주로 뽀로로 폴리 를 접하고있어요",
          "original_form": "지금은 주로 뽀로로 폴리 를 접하고있어요",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040688.1.26",
          "form": "저 일본 애니메이션은 영상으로 잘 안보게 되던데 지금 말씀하신 코난이랑 아즈망가대왕은 봤어욬ㅋㅋㅋ",
          "original_form": "저 일본 애니메이션은 영상으로 잘 안보게 되던데 지금 말씀하신 코난이랑 아즈망가대왕은 봤어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040688.1.27",
          "form": "ㅋㅋㅋㅋ 저 브레드이발소 완전 좋아해요",
          "original_form": "ㅋㅋㅋㅋ 저 브레드이발소 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040688.1.28",
          "form": "혹시 아시는지 모르겠는데 브레드 이발소 아시나요??",
          "original_form": "혹시 아시는지 모르겠는데 브레드 이발소 아시나요??",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.29",
          "form": "지금 찾아봤는데",
          "original_form": "지금 찾아봤는데",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.30",
          "form": "키즈용인데 제 취향이에욬ㅋㅋㅋ",
          "original_form": "키즈용인데 제 취향이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.31",
          "form": "캐릭터만 봐도 느껴지는 귀여움 머죠",
          "original_form": "캐릭터만 봐도 느껴지는 귀여움 머죠",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.32",
          "form": "ㅋㅋㅋㅋㅋ 보면 더 좋아하실거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 보면 더 좋아하실거 같아요",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.33",
          "form": "내용은 모르지만 소시지 팬이될 것 같아요",
          "original_form": "내용은 모르지만 소시지 팬이될 것 같아요",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.34",
          "form": "애기랑 같이 볼 내용일까 싶긴 한데",
          "original_form": "애기랑 같이 볼 내용일까 싶긴 한데",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040688.1.36",
          "form": "전 윌크가 멍충한게 귀엽더라고요",
          "original_form": "전 윌크가 멍충한게 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040688.1.37",
          "form": "멍.청아니라 멍충이에요 귀여우니까",
          "original_form": "멍.청아니라 멍충이에요 귀여우니까",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040688.1.38",
          "form": "눈이땡그래서 똘똘할것 같은데 아니군요",
          "original_form": "눈이땡그래서 똘똘할것 같은데 아니군요",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040688.1.39",
          "form": "ㅋㅋㅋㅋㅋ 전혀 아니에요",
          "original_form": "ㅋㅋㅋㅋㅋ 전혀 아니에요",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040688.1.40",
          "form": "의외성에 놀라실거에요",
          "original_form": "의외성에 놀라실거에요",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040688.1.41",
          "form": "ㅋㅋㅋ아이보다 제가 먼저봐야곘어요",
          "original_form": "ㅋㅋㅋ아이보다 제가 먼저봐야곘어요",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040688.1.42",
          "form": "저 집에 라바 인형도 있어요",
          "original_form": "저 집에 라바 인형도 있어요",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040688.1.43",
          "form": "연애인보다 만화 캐릭터에관심이더 많은 아주미..",
          "original_form": "연애인보다 만화 캐릭터에관심이더 많은 아주미..",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040688.1.44",
          "form": "연예인..;;",
          "original_form": "연예인..;;",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040688.1.45",
          "form": "ㅋㅋㅋㅋ 라바는 안봤는데 귀엽긴 하더라고욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 라바는 안봤는데 귀엽긴 하더라고욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040688.1.46",
          "form": "저도 캐릭터가 더 좋아요 ㅋㅋㅋㅋ",
          "original_form": "저도 캐릭터가 더 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040688.1.47",
          "form": "제 최애 캐릭터는 미피",
          "original_form": "제 최애 캐릭터는 미피",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040688.1.48",
          "form": "귀여운거 좋아요ㅠㅠ",
          "original_form": "귀여운거 좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040688.1.49",
          "form": "저는 고전격인 피글렛이요",
          "original_form": "저는 고전격인 피글렛이요",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040688.1.50",
          "form": "피글렛 몸통같은 티도 사서 혼자 커플이라며 좋아하고",
