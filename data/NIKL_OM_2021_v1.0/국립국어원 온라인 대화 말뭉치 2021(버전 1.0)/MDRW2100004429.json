      "utterance": [
        {
          "id": "MDRW2100004429.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004429.1.2",
          "form": "저는 다른사람 여행다녀온얘기듣는건좋아해요",
          "original_form": "저는 다른사람 여행다녀온얘기듣는건좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004429.1.3",
          "form": "제가 여행을 가는건 그닥 안좋아하네여",
          "original_form": "제가 여행을 가는건 그닥 안좋아하네여",
          "speaker_id": "2",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004429.1.4",
          "form": "여기저기 다니는 거 별로 인가요?",
          "original_form": "여기저기 다니는 거 별로 인가요?",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004429.1.5",
          "form": "제가 경치구경애도 취미가 없어서",
          "original_form": "제가 경치구경애도 취미가 없어서",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004429.1.6",
          "form": "사람만나러 다니는건 좋아해요",
          "original_form": "사람만나러 다니는건 좋아해요",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004429.1.7",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004429.1.8",
          "form": "저는 돌아다니는거 좋아요",
          "original_form": "저는 돌아다니는거 좋아요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004429.1.9",
          "form": "어디가봤어요?",
          "original_form": "어디가봤어요?",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004429.1.10",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004429.1.11",
          "form": "헐 쩐다 한국사시는 분이죠..?",
          "original_form": "헐 쩐다 한국사시는 분이죠..?",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004429.1.12",
          "form": "워홀다녀왔어요?",
          "original_form": "워홀다녀왔어요?",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004429.1.13",
          "form": "아니요 그냥 가고 싶어셔요",
          "original_form": "아니요 그냥 가고 싶어셔요",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004429.1.14",
          "form": "우와 재밌었어요? 거기가서 뭐가 좋았어요?",
          "original_form": "우와 재밌었어요? 거기가서 뭐가 좋았어요?",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004429.1.15",
          "form": "오페라하우스요",
          "original_form": "오페라하우스요",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004429.1.16",
          "form": "시드니에 있믐 오페라하우스",
          "original_form": "시드니에 있믐 오페라하우스",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004429.1.17",
          "form": "직접 공연도 봤어요???",
          "original_form": "직접 공연도 봤어요???",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004429.1.18",
          "form": "그날 공연은 없더라구요",
          "original_form": "그날 공연은 없더라구요",
