      "utterance": [
        {
          "id": "MDRW2100010982.1.1",
          "form": "응 ㅠ 그래서 너무 놀랐지뭐야",
          "original_form": "응 ㅠ 그래서 너무 놀랐지뭐야",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010982.1.2",
          "form": "그래서 대비책으로 뭘 샀니?",
          "original_form": "그래서 대비책으로 뭘 샀니?",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.3",
          "form": "마침 지난번에 산 모기장텐트가 있어서 다행이었어",
          "original_form": "마침 지난번에 산 모기장텐트가 있어서 다행이었어",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.4",
          "form": "너 모기장 텐트 사봤니?",
          "original_form": "너 모기장 텐트 사봤니?",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.5",
          "form": "아니! 우와 그건 실내용이랴?",
          "original_form": "아니! 우와 그건 실내용이랴?",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.6",
          "form": "나는 사실 그걸 살면서 처음 사봤는데 정말 좋아",
          "original_form": "나는 사실 그걸 살면서 처음 사봤는데 정말 좋아",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.7",
          "form": "그런 거 같아.",
          "original_form": "그런 거 같아.",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.8",
          "form": "나도 친구들리 쓰는거 구경만 해봤어",
          "original_form": "나도 친구들리 쓰는거 구경만 해봤어",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.9",
          "form": "실외용으로 쓰기엔 안이 다 비치는걸..",
          "original_form": "실외용으로 쓰기엔 안이 다 비치는걸..",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.10",
          "form": "ㅠㅠ그렇구나 몰랐어",
          "original_form": "ㅠㅠ그렇구나 몰랐어",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.11",
          "form": "자취방에 모기나 벌레가 있다면 꼬옥...안아주면되",
          "original_form": "자취방에 모기나 벌레가 있다면 꼬옥...안아주면되",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.12",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.13",
          "form": "꼬옥 사",
          "original_form": "꼬옥 사",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.14",
          "form": "공주처럼 천장에 다는걸 샀니?",
          "original_form": "공주처럼 천장에 다는걸 샀니?",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.15",
          "form": "텐트는 그런 게 아냐",
          "original_form": "텐트는 그런 게 아냐",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.16",
          "form": "그건 캐노피겠지.",
          "original_form": "그건 캐노피겠지.",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010982.1.17",
          "form": "용도가 전혀 달라.",
          "original_form": "용도가 전혀 달라.",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010982.1.18",
          "form": "아 걔는 이름이 다르구나",
          "original_form": "아 걔는 이름이 다르구나",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010982.1.19",
          "form": "그냥 같은 모기장인쥴 알았어",
          "original_form": "그냥 같은 모기장인쥴 알았어",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010982.1.20",
          "form": "내가 쓰는건 정말 텐트란다.",
          "original_form": "내가 쓰는건 정말 텐트란다.",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010982.1.21",
          "form": "캐노피는 장식용이지않아?",
          "original_form": "캐노피는 장식용이지않아?",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010982.1.22",
          "form": "나중에 구경하러 놀러갈께",
          "original_form": "나중에 구경하러 놀러갈께",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010982.1.23",
          "form": "닭갈비 키트가 갖고싶어",
          "original_form": "닭갈비 키트가 갖고싶어",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010982.1.24",
          "form": "담다디 닭갈비 맛있잖아",
          "original_form": "담다디 닭갈비 맛있잖아",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010982.1.25",
          "form": "아니 닭볶음탕.",
          "original_form": "아니 닭볶음탕.",
          "speaker_id": "2",
          "time": "20210823 23:41"
        }
      ]
    }
  ]
