      "utterance": [
        {
          "id": "MDRW2100044398.1.1",
          "form": "얼굴 어디에",
          "original_form": "얼굴 어디에",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044398.1.2",
          "form": "예전에 잘 나지도 않던 이 마",
          "original_form": "예전에 잘 나지도 않던 이 마",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044398.1.3",
          "form": "이마에 하나밖에 없던데",
          "original_form": "이마에 하나밖에 없던데",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044398.1.4",
          "form": "없어질 거야",
          "original_form": "없어질 거야",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.5",
          "form": "지금 삼주 째 안 없어지고 있어요",
          "original_form": "지금 삼주 째 안 없어지고 있어요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.6",
          "form": "짜야 되는 건 아니고",
          "original_form": "짜야 되는 건 아니고",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.7",
          "form": "짜면은 더 덧날까봐 안 차고 있어요",
          "original_form": "짜면은 더 덧날까봐 안 차고 있어요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.8",
          "form": "곰지도 않고 있는 거야",
          "original_form": "곰지도 않고 있는 거야",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.9",
          "form": "응 그냥 뽈록튀어나와 있어",
          "original_form": "응 그냥 뽈록튀어나와 있어",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.10",
          "form": "이상하네 시간 지나면   빨갛게 될 텐데",
          "original_form": "이상하네 시간 지나면   빨갛게 될 텐데",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.11",
          "form": "그러니까 그렇게 되면 은 짝이라도 할 텐데",
          "original_form": "그러니까 그렇게 되면 은 짝이라도 할 텐데",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044398.1.12",
          "form": "뭐 바르는 거 없어",
          "original_form": "뭐 바르는 거 없어",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.13",
          "form": "계속 바르고 있는데 별 이상이 없네",
          "original_form": "계속 바르고 있는데 별 이상이 없네",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.14",
          "form": "왜 나는 건데",
          "original_form": "왜 나는 건데",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.15",
          "form": "호르몬 때문인 줄 알았는데 생리 끊어도 계속 나요",
          "original_form": "호르몬 때문인 줄 알았는데 생리 끊어도 계속 나요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.16",
          "form": "좀 더 시간이 지나야 되지 않을까",
          "original_form": "좀 더 시간이 지나야 되지 않을까",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.17",
          "form": "진짜 돈 써서 피부과 가고 싶다",
          "original_form": "진짜 돈 써서 피부과 가고 싶다",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.18",
          "form": "이번에 화장품 바꾼 게 안 맞는 거 아니야",
          "original_form": "이번에 화장품 바꾼 게 안 맞는 거 아니야",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.19",
          "form": "그런 걸 수도 있어서 일단 안 써보려고요",
          "original_form": "그런 걸 수도 있어서 일단 안 써보려고요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.20",
          "form": "많이 났어",
          "original_form": "많이 났어",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044398.1.21",
          "form": "이마 턱 말고도   볼에도 났어요",
          "original_form": "이마 턱 말고도   볼에도 났어요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044398.1.22",
          "form": "많이 났네",
          "original_form": "많이 났네",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044398.1.23",
          "form": "속상하겠네",
          "original_form": "속상하겠네",
          "speaker_id": "2",
