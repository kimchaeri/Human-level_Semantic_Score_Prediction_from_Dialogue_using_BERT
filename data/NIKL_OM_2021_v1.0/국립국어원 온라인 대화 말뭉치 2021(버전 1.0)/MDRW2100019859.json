      "utterance": [
        {
          "id": "MDRW2100019859.1.1",
          "form": "저 광주출장왔는데 가을같아요~",
          "original_form": "저 광주출장왔는데 가을같아요~",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100019859.1.2",
          "form": "가을 진짜 좋은데 너무 짧아ㅠ",
          "original_form": "가을 진짜 좋은데 너무 짧아ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100019859.1.3",
          "form": "비오니깐 너무 추워졌다",
          "original_form": "비오니깐 너무 추워졌다",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100019859.1.4",
          "form": "이제 반팔만은 못입겠어요 추벙",
          "original_form": "이제 반팔만은 못입겠어요 추벙",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100019859.1.5",
          "form": "자는데도 너무 춥더라 두꺼운 이불 꺼내야겠어",
          "original_form": "자는데도 너무 춥더라 두꺼운 이불 꺼내야겠어",
          "speaker_id": "1",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100019859.1.6",
          "form": "언니 이럴때 감기조심해야합니다 진짜 감기 훅 들어요",
          "original_form": "언니 이럴때 감기조심해야합니다 진짜 감기 훅 들어요",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100019859.1.7",
          "form": "응 누니도 감기조심해 옷 따뜻하게 입고다니고",
          "original_form": "응 누니도 감기조심해 옷 따뜻하게 입고다니고",
          "speaker_id": "1",
          "time": "20210901 18:11"
        },
        {
          "id": "MDRW2100019859.1.8",
          "form": "ㅎㅎ날이 추워지니 이제 옷이 걱정입니다\r\n얇으면춥고 두꺼우면 덥고",
          "original_form": "ㅎㅎ날이 추워지니 이제 옷이 걱정입니다\r\n얇으면춥고 두꺼우면 덥고",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100019859.1.9",
          "form": "옷장에 옷은 많은데 막상 입은건 없고ㅋㅋ",
          "original_form": "옷장에 옷은 많은데 막상 입은건 없고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100019859.1.10",
          "form": "맞아요!!매년 ㅎ 작년엔 벚고다녓나!",
          "original_form": "맞아요!!매년 ㅎ 작년엔 벚고다녓나!",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100019859.1.11",
          "form": "또 계절이 바꼈다는 핑계로 옷을 사야지ㅋㅋ",
          "original_form": "또 계절이 바꼈다는 핑계로 옷을 사야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100019859.1.12",
          "form": "ㅋㅋ 쇼핑 고고!!",
          "original_form": "ㅋㅋ 쇼핑 고고!!",
          "speaker_id": "2",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100019859.1.13",
          "form": "옷사면 신발도 사야되고 신발사면 가방도 사고ㅋㅋ",
          "original_form": "옷사면 신발도 사야되고 신발사면 가방도 사고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100019859.1.14",
          "form": "가을에 맞는 옷이랑 신발 사고싶슴당 ㅎㅎ 브라운과 레드!",
          "original_form": "가을에 맞는 옷이랑 신발 사고싶슴당 ㅎㅎ 브라운과 레드!",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100019859.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
