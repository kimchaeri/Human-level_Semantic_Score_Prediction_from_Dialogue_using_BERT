      "utterance": [
        {
          "id": "MDRW2100043690.1.1",
          "form": "재밌게 보시는 드라마있으신가요?",
          "original_form": "재밌게 보시는 드라마있으신가요?",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043690.1.2",
          "form": "음 티비는 잘 안보구 넷플리스로 굿플레이스라는 시리즈 보고있어요",
          "original_form": "음 티비는 잘 안보구 넷플리스로 굿플레이스라는 시리즈 보고있어요",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043690.1.3",
          "form": "아 굿플레이는 외국 드라마인가요?",
          "original_form": "아 굿플레이는 외국 드라마인가요?",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043690.1.4",
          "form": "네 맞아요! 넷플릭스 이용하시나요?",
          "original_form": "네 맞아요! 넷플릭스 이용하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043690.1.5",
          "form": "저는 상견니때문에 한동안 보다가",
          "original_form": "저는 상견니때문에 한동안 보다가",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043690.1.6",
          "form": "요즘은 잘 안보네요ㅜ",
          "original_form": "요즘은 잘 안보네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043690.1.7",
          "form": "무브투헤븐 보셨어요?",
          "original_form": "무브투헤븐 보셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043690.1.8",
          "form": "저 그거보고 엄청 울었어요 ㅠ",
          "original_form": "저 그거보고 엄청 울었어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043690.1.9",
          "form": "눈물나는거 싫어하시나요?",
          "original_form": "눈물나는거 싫어하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043690.1.10",
          "form": "싫어하진않는데 가끔 그 감정이 잠을 방해하는경우가 있어서요ㅜ",
          "original_form": "싫어하진않는데 가끔 그 감정이 잠을 방해하는경우가 있어서요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043690.1.11",
          "form": "보보경심려보고ㅜ잠을 못 잤어요ㅜ심장 아파서요ㅜ",
          "original_form": "보보경심려보고ㅜ잠을 못 잤어요ㅜ심장 아파서요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043690.1.12",
          "form": "슬퍼서요??",
          "original_form": "슬퍼서요??",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043690.1.13",
          "form": "네 이준기 눈빛이 너무 슬퍼서요ㅜ",
          "original_form": "네 이준기 눈빛이 너무 슬퍼서요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043690.1.14",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043690.1.15",
          "form": "ㅠㅠㅠ한편인가 두편 밖에 못봤어요 ㅠ 그냥 틀어져있는거 잠깐씩 봣거든요",
          "original_form": "ㅠㅠㅠ한편인가 두편 밖에 못봤어요 ㅠ 그냥 틀어져있는거 잠깐씩 봣거든요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043690.1.16",
          "form": "아이고 그러시면 무브투헤븐 보지마세요",
          "original_form": "아이고 그러시면 무브투헤븐 보지마세요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043690.1.17",
          "form": "몸이 약하셔가지구 안되겠어요 ㅠ",
          "original_form": "몸이 약하셔가지구 안되겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043690.1.18",
          "form": "몸이 약하진않은데ㅜ감성적이예요ㅜㅎㅎ",
          "original_form": "몸이 약하진않은데ㅜ감성적이예요ㅜㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043690.1.19",
          "form": "심장아파서 잠 못잘정도면 약하신거에요!ㅋㅋㅋ큐ㅠㅠㅠㅠㅠ잉",
          "original_form": "심장아파서 잠 못잘정도면 약하신거에요!ㅋㅋㅋ큐ㅠㅠㅠㅠㅠ잉",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043690.1.20",
          "form": "감성이 풍부하신분이시군요",
          "original_form": "감성이 풍부하신분이시군요",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043690.1.21",
          "form": "아 슬퍼서 심장이 찌릿해져서ㅜ",
          "original_form": "아 슬퍼서 심장이 찌릿해져서ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043690.1.22",
          "form": "최근에는 놀면뭐하니 msg워너비 재밌어서 한번에 쫙 봤어요~",
          "original_form": "최근에는 놀면뭐하니 msg워너비 재밌어서 한번에 쫙 봤어요~",
          "speaker_id": "1",
          "time": "20210907 20:12"
        }
      ]
