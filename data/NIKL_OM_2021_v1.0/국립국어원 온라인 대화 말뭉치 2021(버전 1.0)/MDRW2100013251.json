      "utterance": [
        {
          "id": "MDRW2100013251.1.1",
          "form": "오늘 엄마가 닭갈비해주심",
          "original_form": "오늘 엄마가 닭갈비해주심",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013251.1.2",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013251.1.3",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013251.1.4",
          "form": "응응 완전!",
          "original_form": "응응 완전!",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.5",
          "form": "하나도 안매웟어",
          "original_form": "하나도 안매웟어",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.6",
          "form": "그래???",
          "original_form": "그래???",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.7",
          "form": "전에 쭈꾸미는",
          "original_form": "전에 쭈꾸미는",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.8",
          "form": "많이 매웠자너",
          "original_form": "많이 매웠자너",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.9",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.10",
          "form": "그래서 진짜 배아프고",
          "original_form": "그래서 진짜 배아프고",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.11",
          "form": "힘들었지...",
          "original_form": "힘들었지...",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.12",
          "form": "이젠 갠찬아!",
          "original_form": "이젠 갠찬아!",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.13",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.14",
          "form": "나도 닭갈비묵고싶다...",
          "original_form": "나도 닭갈비묵고싶다...",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.15",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.16",
          "form": "너도먹어!",
          "original_form": "너도먹어!",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.17",
          "form": "컄컄",
          "original_form": "컄컄",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.18",
          "form": "집에가야지 막지",
          "original_form": "집에가야지 막지",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.19",
          "form": "먹지",
          "original_form": "먹지",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.20",
          "form": "얼른오라고오",
          "original_form": "얼른오라고오",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.21",
          "form": "떡도 잇다",
          "original_form": "떡도 잇다",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013251.1.22",
          "form": "떡 진짜맛나겠다",
          "original_form": "떡 진짜맛나겠다",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.23",
          "form": "나 왜아직 학겨..",
          "original_form": "나 왜아직 학겨..",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.24",
          "form": "킼킼",
          "original_form": "킼킼",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.25",
          "form": "근데 고구마는없어ㅠ",
          "original_form": "근데 고구마는없어ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.26",
          "form": "좀아쉽..ㅡ.",
          "original_form": "좀아쉽..ㅡ.",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.27",
          "form": "나두..",
          "original_form": "나두..",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.28",
          "form": "글도 맛있다",
          "original_form": "글도 맛있다",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.29",
          "form": "아근데 깻잎도 있어ㅠ",
          "original_form": "아근데 깻잎도 있어ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.30",
          "form": "이건좀",
          "original_form": "이건좀",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.31",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.32",
          "form": "깻잎향때메",
          "original_form": "깻잎향때메",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.33",
          "form": "좀 시른데",
          "original_form": "좀 시른데",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.34",
          "form": "나도그랫는데",
          "original_form": "나도그랫는데",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013251.1.35",
          "form": "그래도 맛잇드라",
          "original_form": "그래도 맛잇드라",
          "speaker_id": "2",
          "time": "20210825 13:42"
        }
      ]
    }
  ]
