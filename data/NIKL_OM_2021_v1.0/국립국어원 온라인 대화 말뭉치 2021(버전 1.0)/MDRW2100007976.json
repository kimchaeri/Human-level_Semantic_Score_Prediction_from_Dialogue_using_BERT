      "utterance": [
        {
          "id": "MDRW2100007976.1.1",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007976.1.2",
          "form": "네! 있죠!",
          "original_form": "네! 있죠!",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007976.1.3",
          "form": "귀엽고 간섭이 거의 없는 사람을 좋아해요!",
          "original_form": "귀엽고 간섭이 거의 없는 사람을 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007976.1.4",
          "form": "사적인 영역을 따로 두시는군요",
          "original_form": "사적인 영역을 따로 두시는군요",
          "speaker_id": "1",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007976.1.5",
          "form": "네네 뭐라고 해야할까",
          "original_form": "네네 뭐라고 해야할까",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007976.1.6",
          "form": "특별히 어디 안가고 집밖에만 나갔는데 메세지만 몇십통 보냈던",
          "original_form": "특별히 어디 안가고 집밖에만 나갔는데 메세지만 몇십통 보냈던",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007976.1.7",
          "form": "사람이랑 연애도 해본적이 있는지라..",
          "original_form": "사람이랑 연애도 해본적이 있는지라..",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007976.1.8",
          "form": "지나친 간섭은 너무 싫어해요",
          "original_form": "지나친 간섭은 너무 싫어해요",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007976.1.9",
          "form": "몇십통이면 집착 수준이네요",
          "original_form": "몇십통이면 집착 수준이네요",
          "speaker_id": "1",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007976.1.10",
          "form": "저도 그런 간섭은 싫어요",
          "original_form": "저도 그런 간섭은 싫어요",
          "speaker_id": "1",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007976.1.11",
          "form": "네 정확힌",
          "original_form": "네 정확힌",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007976.1.12",
          "form": "집착없는 사람을 좋아하죠",
          "original_form": "집착없는 사람을 좋아하죠",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007976.1.13",
          "form": "ㅠ.ㅠ 너무 힘들었어요",
          "original_form": "ㅠ.ㅠ 너무 힘들었어요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007976.1.14",
          "form": "고생 많으셨어요 좋은 사람 만나실 거예요",
          "original_form": "고생 많으셨어요 좋은 사람 만나실 거예요",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007976.1.15",
          "form": "감사합니다! ㅠㅠ",
          "original_form": "감사합니다! ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007976.1.16",
          "form": "요즘 마음에 두고 있는 사람이 있나요?",
          "original_form": "요즘 마음에 두고 있는 사람이 있나요?",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007976.1.17",
          "form": "아직은 없네요",
          "original_form": "아직은 없네요",
          "speaker_id": "2",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100007976.1.18",
          "form": "코로나라서 사람 만나기 힘들긴 하죠",
          "original_form": "코로나라서 사람 만나기 힘들긴 하죠",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100007976.1.19",
          "form": "맞아요..ㅠㅠ",
          "original_form": "맞아요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100007976.1.20",
          "form": "코로나 끝나고 나서는 소개팅 나가보실 생각 있으세요?",
          "original_form": "코로나 끝나고 나서는 소개팅 나가보실 생각 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100007976.1.21",
          "form": "소개팅을 그닥 안좋아해서",
          "original_form": "소개팅을 그닥 안좋아해서",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100007976.1.22",
          "form": "자연스러움을 추구하거든요",
          "original_form": "자연스러움을 추구하거든요",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100007976.1.23",
          "form": "뭔가 인공적으로 자리잡아서 만난다란 개념을 싫어하기도 하고",
          "original_form": "뭔가 인공적으로 자리잡아서 만난다란 개념을 싫어하기도 하고",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100007976.1.24",
          "form": "소개팅해서 잘된 케이스도 못본지라",
          "original_form": "소개팅해서 잘된 케이스도 못본지라",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100007976.1.25",
          "form": "자만추이시군요",
          "original_form": "자만추이시군요",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100007976.1.26",
          "form": "네 맞아용",
          "original_form": "네 맞아용",
          "speaker_id": "2",
          "time": "20210822 15:17"
        }
      ]
    }
  ]
