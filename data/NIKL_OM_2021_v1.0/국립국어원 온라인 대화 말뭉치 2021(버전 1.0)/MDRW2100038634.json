      "utterance": [
        {
          "id": "MDRW2100038634.1.1",
          "form": "훈련시킨거 있으신가요 ㅎㅎ",
          "original_form": "훈련시킨거 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100038634.1.2",
          "form": "그거 뭐죠? 말 녹음해서 벨 누르는 거",
          "original_form": "그거 뭐죠? 말 녹음해서 벨 누르는 거",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100038634.1.3",
          "form": "아 쫀덕이가 하는건가요?",
          "original_form": "아 쫀덕이가 하는건가요?",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100038634.1.4",
          "form": "녹음벨?!",
          "original_form": "녹음벨?!",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100038634.1.5",
          "form": "네 그거 맞아요 ㅎㅎ",
          "original_form": "네 그거 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100038634.1.6",
          "form": "간식은 시도때도없이 달라고 할 것 같아서",
          "original_form": "간식은 시도때도없이 달라고 할 것 같아서",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100038634.1.7",
          "form": "밥 주세요랑 놀아주세요만 알려줬어요 ㅋㅋㅋㅋ",
          "original_form": "밥 주세요랑 놀아주세요만 알려줬어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100038634.1.8",
          "form": "헉 ㅋㅋㅋㅋㅋ 그건 수시로 안누르나요?",
          "original_form": "헉 ㅋㅋㅋㅋㅋ 그건 수시로 안누르나요?",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100038634.1.9",
          "form": "놀아주세요를 좀 자주 눌러요 ㅎㅎ",
          "original_form": "놀아주세요를 좀 자주 눌러요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038634.1.10",
          "form": "밥은 자율급식하다가 갑자기 너무 먹는 것 같아서 제한급식으로 바꿨는데 그 때 가르쳐줬어요 ㅎㅎ",
          "original_form": "밥은 자율급식하다가 갑자기 너무 먹는 것 같아서 제한급식으로 바꿨는데 그 때 가르쳐줬어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038634.1.11",
          "form": "넘 기여운거아닌가요",
          "original_form": "넘 기여운거아닌가요",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038634.1.12",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038634.1.13",
          "form": "울애는",
          "original_form": "울애는",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100038634.1.14",
          "form": "새벽 4시에 밥달라고 깨워요",
          "original_form": "새벽 4시에 밥달라고 깨워요",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038634.1.15",
          "form": "새벽 4시는 너무 가혹하네요 ㅋㅋㅋㅋ\r\n애기야 조금만 늦춰주지않겠니ㅠㅠ ㅋㅋㅋㅋ",
          "original_form": "새벽 4시는 너무 가혹하네요 ㅋㅋㅋㅋ\r\n애기야 조금만 늦춰주지않겠니ㅠㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038634.1.16",
          "form": "저희는 6-7시쯤 깨우는 것 같아요\r\n그래서 서로 누가 먼저 일어나나 하고있어요 ㅎㅎ",
          "original_form": "저희는 6-7시쯤 깨우는 것 같아요\r\n그래서 서로 누가 먼저 일어나나 하고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038634.1.17",
          "form": "그래서 저는 자동 밥 기계 샀어요 ㅎㅎ",
          "original_form": "그래서 저는 자동 밥 기계 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100038634.1.18",
          "form": "맞아요 그시간에 깨워요 ㅋㅋㅋㅋ 이제",
          "original_form": "맞아요 그시간에 깨워요 ㅋㅋㅋㅋ 이제",
          "speaker_id": "1",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100038634.1.19",
          "form": "자동 밥 기계는 청소하기 어렵지않나요??",
          "original_form": "자동 밥 기계는 청소하기 어렵지않나요??",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100038634.1.20",
          "form": "괜찮으면 하나 사야겠어요 ㅎㅎ",
          "original_form": "괜찮으면 하나 사야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100038634.1.21",
          "form": "쫀덕이가 때리는 거처럼 생겼어요 ㅎㅎ",
          "original_form": "쫀덕이가 때리는 거처럼 생겼어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100038634.1.22",
          "form": "카메라 달린",
          "original_form": "카메라 달린",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100038634.1.23",
          "form": "밥그릇부분이 스댕이 아닌걸로 사서 쫌 별루긴해요",
          "original_form": "밥그릇부분이 스댕이 아닌걸로 사서 쫌 별루긴해요",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100038634.1.24",
          "form": "아 쫀덕이 채널은 그 녹음벨 영상만 봐서 몰랐네요 ㅎㅎ",
          "original_form": "아 쫀덕이 채널은 그 녹음벨 영상만 봐서 몰랐네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100038634.1.25",
          "form": "한번 알아봐야겠어요",
          "original_form": "한번 알아봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100038634.1.26",
          "form": "뚜드려 패는거 웃기다는 ㅎㅎ",
          "original_form": "뚜드려 패는거 웃기다는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100038634.1.27",
          "form": "때리면 사료가 나오더라구요",
          "original_form": "때리면 사료가 나오더라구요",
          "speaker_id": "1",
          "time": "20210902 19:33"
        }
      ]
