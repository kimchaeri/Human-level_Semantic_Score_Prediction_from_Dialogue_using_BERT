      "utterance": [
        {
          "id": "MDRW2100027645.1.1",
          "form": "사서 일이 많아지면 직원 늘리겠지",
          "original_form": "사서 일이 많아지면 직원 늘리겠지",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027645.1.2",
          "form": "분명 도서관사서도 바쁠거고 분명할일이 있을건데",
          "original_form": "분명 도서관사서도 바쁠거고 분명할일이 있을건데",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027645.1.3",
          "form": "모르는사람들은 할일없다",
          "original_form": "모르는사람들은 할일없다",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027645.1.4",
          "form": "쉬워보인다 식으로 말하는것도 그렇고",
          "original_form": "쉬워보인다 식으로 말하는것도 그렇고",
          "speaker_id": "1",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027645.1.5",
          "form": "ㅡ.ㅡ",
          "original_form": "ㅡ.ㅡ",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027645.1.6",
          "form": "지가 해보등지",
          "original_form": "지가 해보등지",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027645.1.7",
          "form": "남의일 쉬운거아닌데 넘한다",
          "original_form": "남의일 쉬운거아닌데 넘한다",
          "speaker_id": "2",
          "time": "20210902 14:01"
        },
        {
          "id": "MDRW2100027645.1.8",
          "form": "난 선생님도 맨날 철밥통이다 하는사람들 싫었음",
          "original_form": "난 선생님도 맨날 철밥통이다 하는사람들 싫었음",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027645.1.9",
          "form": "비록 나는 임용 안됐지만 ㅋㅋ",
          "original_form": "비록 나는 임용 안됐지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027645.1.10",
          "form": "아 기타기타 시리즈 추천",
          "original_form": "아 기타기타 시리즈 추천",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027645.1.11",
          "form": "종이책 오랜만에 읽으니좋음",
          "original_form": "종이책 오랜만에 읽으니좋음",
          "speaker_id": "1",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027645.1.12",
          "form": "선생님도 힘들다궁ㅜ",
          "original_form": "선생님도 힘들다궁ㅜ",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027645.1.13",
          "form": "종이책이 좋아 이북은 뭔가 불편해",
          "original_form": "종이책이 좋아 이북은 뭔가 불편해",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027645.1.14",
          "form": "그리고 이 책도 좋음",
          "original_form": "그리고 이 책도 좋음",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027645.1.15",
          "form": "뭔가 별 트릭없는데 ㅋㅋ사실",
          "original_form": "뭔가 별 트릭없는데 ㅋㅋ사실",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027645.1.16",
          "form": "침대 누워서 종이책으로 이리저리 넘기다 다시 읽는게 매력이야",
          "original_form": "침대 누워서 종이책으로 이리저리 넘기다 다시 읽는게 매력이야",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027645.1.17",
          "form": "기타기타 기회되믄봐야징",
          "original_form": "기타기타 기회되믄봐야징",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100027645.1.18",
          "form": "약간 그런 느낌도 듦",
          "original_form": "약간 그런 느낌도 듦",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027645.1.19",
          "form": "특이한 이야기? 같은",
          "original_form": "특이한 이야기? 같은",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027645.1.20",
          "form": "가담항설인가 알아요?",
          "original_form": "가담항설인가 알아요?",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027645.1.21",
          "form": "기묘한이야기?",
          "original_form": "기묘한이야기?",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027645.1.22",
          "form": "제목만봄",
          "original_form": "제목만봄",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027645.1.23",
          "form": "진짜 그냥 딱 추리소설이라기보다 기묘한 이야기같음 ㅋㅋ",
          "original_form": "진짜 그냥 딱 추리소설이라기보다 기묘한 이야기같음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027645.1.24",
          "form": "그 드라마 말고 그냥 문자그대로 기묘한 이야기들",
          "original_form": "그 드라마 말고 그냥 문자그대로 기묘한 이야기들",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027645.1.25",
          "form": "음 재밌겠어",
          "original_form": "음 재밌겠어",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027645.1.26",
          "form": "나는 10년전에 모방범 재밌게읽음",
          "original_form": "나는 10년전에 모방범 재밌게읽음",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027645.1.27",
          "form": "보통 그책으로 다들 미미월드 입문하는듯 ㅋㅋ",
          "original_form": "보통 그책으로 다들 미미월드 입문하는듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027645.1.28",
          "form": "미미월드 가있음 이미 name1님",
          "original_form": "미미월드 가있음 이미 &name1&님",
