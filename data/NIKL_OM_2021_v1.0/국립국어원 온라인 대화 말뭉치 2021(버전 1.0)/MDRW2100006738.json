      "utterance": [
        {
          "id": "MDRW2100006738.1.1",
          "form": "아래집 왜?",
          "original_form": "아래집 왜?",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006738.1.2",
          "form": "아니 앞베란다에서 요리함...",
          "original_form": "아니 앞베란다에서 요리함...",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006738.1.3",
          "form": "그럼 너희집으로 냄새 다 들어오는거 아니야?",
          "original_form": "그럼 너희집으로 냄새 다 들어오는거 아니야?",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006738.1.4",
          "form": "응 그게 문젠데 더 큰 문제는",
          "original_form": "응 그게 문젠데 더 큰 문제는",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006738.1.5",
          "form": "삼겹살 아침점심저녁으로 먹어댐...",
          "original_form": "삼겹살 아침점심저녁으로 먹어댐...",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006738.1.6",
          "form": "헐 그건 좀 심했다",
          "original_form": "헐 그건 좀 심했다",
          "speaker_id": "2",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006738.1.7",
          "form": "경비아저씨한테 얘기 해달라고 해봤어,",
          "original_form": "경비아저씨한테 얘기 해달라고 해봤어,",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006738.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006738.1.9",
          "form": "이미 2년인가 3년전부터 그래서 관리실도가보고 경비아저씨가 전화도 해봤지만 소용이없어",
          "original_form": "이미 2년인가 3년전부터 그래서 관리실도가보고 경비아저씨가 전화도 해봤지만 소용이없어",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006738.1.10",
          "form": "오히려 더 화낸대ㅋㅋㅋ",
          "original_form": "오히려 더 화낸대ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100006738.1.11",
          "form": "그렇게나 오래됐어? 몰상식하네 ...",
          "original_form": "그렇게나 오래됐어? 몰상식하네 ...",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100006738.1.12",
          "form": "빨래해서 널었다하면 여지없이 삼겹살 구움",
          "original_form": "빨래해서 널었다하면 여지없이 삼겹살 구움",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100006738.1.13",
          "form": "스트레스 받을만하네",
          "original_form": "스트레스 받을만하네",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006738.1.14",
          "form": "밤에는 모기향을 엄청 피워서 그것도 스트레스",
          "original_form": "밤에는 모기향을 엄청 피워서 그것도 스트레스",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006738.1.15",
          "form": "가지가지 골고루 하는구나",
          "original_form": "가지가지 골고루 하는구나",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006738.1.16",
          "form": "빨래 걷으면 섬유유연제 냄새는 나지도않고 이상하냄새나",
          "original_form": "빨래 걷으면 섬유유연제 냄새는 나지도않고 이상하냄새나",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100006738.1.17",
          "form": "어떻하냐... 스트레스받아서",
          "original_form": "어떻하냐... 스트레스받아서",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100006738.1.18",
          "form": "내말이ㅠㅠ 아후 진짜 노답이야 지금도 삼겹살냄새 올라옴...",
          "original_form": "내말이ㅠㅠ 아후 진짜 노답이야 지금도 삼겹살냄새 올라옴...",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100006738.1.19",
          "form": "이웃 잘 만나야하는데... 안타깝다",
          "original_form": "이웃 잘 만나야하는데... 안타깝다",
          "speaker_id": "2",
          "time": "20210908 08:19"
        }
      ]
