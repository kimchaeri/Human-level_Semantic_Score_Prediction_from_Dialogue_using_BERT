      "utterance": [
        {
          "id": "MDRW2100000403.1.1",
          "form": "안녕하세요 키우는 동물 있으세요??",
          "original_form": "안녕하세요 키우는 동물 있으세요??",
          "speaker_id": "1",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000403.1.2",
          "form": "구피 키워요",
          "original_form": "구피 키워요",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000403.1.3",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000403.1.4",
          "form": "무슨 동물 키우세요?",
          "original_form": "무슨 동물 키우세요?",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000403.1.5",
          "form": "얼마전에 구피인지 아닌지 잘 모르겠는 아이들을 선물받았는데요",
          "original_form": "얼마전에 구피인지 아닌지 잘 모르겠는 아이들을 선물받았는데요",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000403.1.6",
          "form": "밥을 엄청 조금 주라던데",
          "original_form": "밥을 엄청 조금 주라던데",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000403.1.7",
          "form": "대체 얼마나 조금 줘야하는 거죠",
          "original_form": "대체 얼마나 조금 줘야하는 거죠",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000403.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000403.1.9",
          "form": "저희도 엄청 적게 줘요.",
          "original_form": "저희도 엄청 적게 줘요.",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000403.1.10",
          "form": "진짜군요",
          "original_form": "진짜군요",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000403.1.11",
          "form": "오늘 구피 먹이 다떨어져가서 주문했어요",
          "original_form": "오늘 구피 먹이 다떨어져가서 주문했어요",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000403.1.12",
          "form": "매일 줘야되나요?",
          "original_form": "매일 줘야되나요?",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000403.1.13",
          "form": "저희는 매일 한번만 줘요",
          "original_form": "저희는 매일 한번만 줘요",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000403.1.14",
          "form": "아침저녁으로 주기도 하던데 그럼 똥도 많이 싸서 물도 자주 갈아줘야하고 해서요.",
          "original_form": "아침저녁으로 주기도 하던데 그럼 똥도 많이 싸서 물도 자주 갈아줘야하고 해서요.",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000403.1.15",
          "form": "물 갈 때는",
          "original_form": "물 갈 때는",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000403.1.16",
          "form": "일단 저보단 남편이 매일 관리하는 중이에요.^^",
          "original_form": "일단 저보단 남편이 매일 관리하는 중이에요.^^",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000403.1.17",
          "form": "아이들 어떻게 빼내요?",
          "original_form": "아이들 어떻게 빼내요?",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000403.1.18",
          "form": "물 걸 때요?",
          "original_form": "물 걸 때요?",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000403.1.19",
          "form": "갈 때?",
          "original_form": "갈 때?",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000403.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000403.1.21",
          "form": "바가지 같은걸로요",
          "original_form": "바가지 같은걸로요",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000403.1.22",
          "form": "한번 국자로 건지긴 했는데",
          "original_form": "한번 국자로 건지긴 했는데",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000403.1.23",
          "form": "잘 도망가서 잡기 힘들어요.",
          "original_form": "잘 도망가서 잡기 힘들어요.",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000403.1.24",
          "form": "다들 도망다녀서 너무 힘들어여",
          "original_form": "다들 도망다녀서 너무 힘들어여",
