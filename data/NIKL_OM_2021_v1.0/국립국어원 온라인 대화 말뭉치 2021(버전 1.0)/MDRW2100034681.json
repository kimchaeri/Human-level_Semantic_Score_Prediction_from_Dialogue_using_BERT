      "utterance": [
        {
          "id": "MDRW2100034681.1.1",
          "form": "저녁 메뉴는 정하셨나요",
          "original_form": "저녁 메뉴는 정하셨나요",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034681.1.2",
          "form": "닭볶음탕 먹기로했어요!",
          "original_form": "닭볶음탕 먹기로했어요!",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034681.1.3",
          "form": "와!! 맛나개 드셨나요~!",
          "original_form": "와!! 맛나개 드셨나요~!",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100034681.1.4",
          "form": "네네 맛있게 먹었네요ㅎㅎ 역시 맛있어요!",
          "original_form": "네네 맛있게 먹었네요ㅎㅎ 역시 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034681.1.5",
          "form": "치즈도 올랴드셨어요?!",
          "original_form": "치즈도 올랴드셨어요?!",
          "speaker_id": "1",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100034681.1.6",
          "form": "치즈말고 당면 넣어서 먹었어용 ㅎㅎ",
          "original_form": "치즈말고 당면 넣어서 먹었어용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100034681.1.7",
          "form": "아 당면도 넘 좋죠 ㅠㅠ",
          "original_form": "아 당면도 넘 좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100034681.1.8",
          "form": "소스랑 너무 잘어울려요",
          "original_form": "소스랑 너무 잘어울려요",
          "speaker_id": "1",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100034681.1.9",
          "form": "맞아요 ㅠㅠ 소스가 당면에 쫙배서 맛있더라구요",
          "original_form": "맞아요 ㅠㅠ 소스가 당면에 쫙배서 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100034681.1.10",
          "form": "당면은 어떤걸 선호하세요? 가장 일반?",
          "original_form": "당면은 어떤걸 선호하세요? 가장 일반?",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100034681.1.11",
          "form": "닭볶음탕에는 얇은거 좋아하구 떡볶이는 굵은거 좋아하네요 ㅎㅎ",
          "original_form": "닭볶음탕에는 얇은거 좋아하구 떡볶이는 굵은거 좋아하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100034681.1.12",
          "form": "엽떡에 중당 추가하면 너무최고죠",
          "original_form": "엽떡에 중당 추가하면 너무최고죠",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100034681.1.13",
          "form": "맛잘알맛잘알",
          "original_form": "맛잘알맛잘알",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100034681.1.14",
          "form": "그쵸 선택이 아닌 필수죠... 중당 너무 좋아요 ㅠㅠ",
          "original_form": "그쵸 선택이 아닌 필수죠... 중당 너무 좋아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100034681.1.15",
          "form": "넘 쫄깃하죠 ㅠㅠ",
          "original_form": "넘 쫄깃하죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100034681.1.16",
          "form": "인절미 과자 드셔보셨나요",
          "original_form": "인절미 과자 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100034681.1.17",
          "form": "네네 좋아해요",
