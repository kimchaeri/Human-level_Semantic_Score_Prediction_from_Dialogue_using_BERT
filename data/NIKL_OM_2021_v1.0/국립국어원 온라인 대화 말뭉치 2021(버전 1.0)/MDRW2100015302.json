      "utterance": [
        {
          "id": "MDRW2100015302.1.1",
          "form": "누가 자몽 허니 블랙티 꿀팁 줬는데 까먹었어요",
          "original_form": "누가 자몽 허니 블랙티 꿀팁 줬는데 까먹었어요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015302.1.2",
          "form": "그것도 스벅 직원분이 알려주신건데...",
          "original_form": "그것도 스벅 직원분이 알려주신건데...",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015302.1.3",
          "form": "아 정말요? ㅋㅋㅋ아쉽네요 ㅋㅋ",
          "original_form": "아 정말요? ㅋㅋㅋ아쉽네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015302.1.4",
          "form": "스벅 케이크는 어떻게 생각하세요?",
          "original_form": "스벅 케이크는 어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015302.1.5",
          "form": "스벅에서 푸드류도 드세요?",
          "original_form": "스벅에서 푸드류도 드세요?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015302.1.6",
          "form": "저 스벅 80%는 푸드 먹으러 가요 ㅋㅋㅋ",
          "original_form": "저 스벅 80%는 푸드 먹으러 가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015302.1.7",
          "form": "아 통했네요 ㅋㅋ아 진짜요? 추천좀 해주세요",
          "original_form": "아 통했네요 ㅋㅋ아 진짜요? 추천좀 해주세요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015302.1.8",
          "form": "블루베리쿠키치즈케이크 추천 합니다..",
          "original_form": "블루베리쿠키치즈케이크 추천 합니다..",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.9",
          "form": "와 꼭 먹어볼게요",
          "original_form": "와 꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.10",
          "form": "그리고 티라미슈도 은근 맛있어요!",
          "original_form": "그리고 티라미슈도 은근 맛있어요!",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.11",
          "form": "저는 푸드류는 실패만해서 ㅠ",
          "original_form": "저는 푸드류는 실패만해서 ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.12",
          "form": "티라미슈 기억할게요",
          "original_form": "티라미슈 기억할게요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.13",
          "form": "앗 진짜요? 무엇 먹어보셨는데요?",
          "original_form": "앗 진짜요? 무엇 먹어보셨는데요?",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.14",
          "form": "가나슈 케이크 그거랑 무슨 샐러드?ㅋㅋㅋ호두케이크도 취향이 아니더라구요",
          "original_form": "가나슈 케이크 그거랑 무슨 샐러드?ㅋㅋㅋ호두케이크도 취향이 아니더라구요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015302.1.15",
          "form": "호두케이크가 혹시 당근케이크..?",
          "original_form": "호두케이크가 혹시 당근케이크..?",
