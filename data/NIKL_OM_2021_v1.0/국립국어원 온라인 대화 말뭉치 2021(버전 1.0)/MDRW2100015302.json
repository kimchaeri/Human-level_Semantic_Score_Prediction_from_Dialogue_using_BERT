        {
          "id": "MDRW2100015302.1.16",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015302.1.17",
          "form": "그냥 그랬어요 ㅋㅋㅋ",
          "original_form": "그냥 그랬어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015302.1.18",
          "form": "가나슈는 그냥 기본이에요..",
          "original_form": "가나슈는 그냥 기본이에요..",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015302.1.19",
          "form": "티라미슈는 꼭 포크나 숟가락으로 퍼먹으세요..",
          "original_form": "티라미슈는 꼭 포크나 숟가락으로 퍼먹으세요..",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015302.1.20",
          "form": "오 ....근데 포크숟가락아니면 어떻게 먹어요?",
          "original_form": "오 ....근데 포크숟가락아니면 어떻게 먹어요?",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015302.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ손??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ손??",
