      "utterance": [
        {
          "id": "MDRW2100013435.1.1",
          "form": "초코 처돌이 ㅎㅎ",
          "original_form": "초코 처돌이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013435.1.2",
          "form": "근데 어느순간",
          "original_form": "근데 어느순간",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013435.1.3",
          "form": "한번 바닐라라떼 먹고나니깐",
          "original_form": "한번 바닐라라떼 먹고나니깐",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013435.1.4",
          "form": "맛있드라",
          "original_form": "맛있드라",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013435.1.5",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.6",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.7",
          "form": "진짜 커피중에 젤최고",
          "original_form": "진짜 커피중에 젤최고",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.8",
          "form": "그정도라니...",
          "original_form": "그정도라니...",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.9",
          "form": "진짜야",
          "original_form": "진짜야",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.10",
          "form": "카페가면 바닐라라떼 먼저 찾게돼",
          "original_form": "카페가면 바닐라라떼 먼저 찾게돼",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.11",
          "form": "자동적으루?*",
          "original_form": "자동적으루?*",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.12",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.13",
          "form": "아니면 오레오나 초코!",
          "original_form": "아니면 오레오나 초코!",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.14",
          "form": "맛있는 초코초코!",
          "original_form": "맛있는 초코초코!",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.15",
          "form": "초코가 좋긴해~",
          "original_form": "초코가 좋긴해~",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.16",
          "form": "그건 마쟈 ㅋㅋ",
          "original_form": "그건 마쟈 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013435.1.17",
          "form": "근데 나도 성인이니깐",
          "original_form": "근데 나도 성인이니깐",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.18",
          "form": "커피를 좀 먹어보고싶더라",
          "original_form": "커피를 좀 먹어보고싶더라",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.20",
          "form": "그거랑 먼상관?",
          "original_form": "그거랑 먼상관?",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.21",
          "form": "성관잇더근요",
          "original_form": "성관잇더근요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.22",
          "form": "상관잇거든요",
          "original_form": "상관잇거든요",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.23",
          "form": "커피를 먹어야 찐어른같자나",
          "original_form": "커피를 먹어야 찐어른같자나",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.24",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.25",
          "form": "초코는 애고??",
          "original_form": "초코는 애고??",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.26",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013435.1.27",
          "form": "콜라는 먼 콜라가 조아?",
          "original_form": "콜라는 먼 콜라가 조아?",
          "speaker_id": "1",
