      "utterance": [
        {
          "id": "MDRW2100027714.1.1",
          "form": "오늘 점심은 뭐 드셨어요? 저는 된장찌개!",
          "original_form": "오늘 점심은 뭐 드셨어요? 저는 된장찌개!",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027714.1.2",
          "form": "불닭 먹었어요",
          "original_form": "불닭 먹었어요",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027714.1.3",
          "form": "마침 내일 강의가 없더라구요",
          "original_form": "마침 내일 강의가 없더라구요",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027714.1.4",
          "form": "불태웠죠",
          "original_form": "불태웠죠",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027714.1.5",
          "form": "부럽다!!! 정말 매운건 다음날 스케줄 확인 필수에요ㅋㅋ",
          "original_form": "부럽다!!! 정말 매운건 다음날 스케줄 확인 필수에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027714.1.6",
          "form": "저도 주말에 뭔가.. 불닭 얘기를 들었는데 왜 갑자기 곱창이 땡길까요",
          "original_form": "저도 주말에 뭔가.. 불닭 얘기를 들었는데 왜 갑자기 곱창이 땡길까요",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027714.1.7",
          "form": "ㅋㅋ 음식이 땡기는데 이유는 없죠",
          "original_form": "ㅋㅋ 음식이 땡기는데 이유는 없죠",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027714.1.8",
          "form": "복숭아도 얼른 더 먹어놔야하는데..",
          "original_form": "복숭아도 얼른 더 먹어놔야하는데..",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027714.1.9",
          "form": "감의 계절이 오고 있어요",
          "original_form": "감의 계절이 오고 있어요",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027714.1.10",
          "form": "저도 복숭아 보내기 전에 많이 먹으려구요",
          "original_form": "저도 복숭아 보내기 전에 많이 먹으려구요",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027714.1.11",
          "form": "곧 배도 나오겠네요",
          "original_form": "곧 배도 나오겠네요",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027714.1.12",
          "form": "아삭아삭 배~ 가을 과일은 전 홍시가 제일 좋아요",
          "original_form": "아삭아삭 배~ 가을 과일은 전 홍시가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027714.1.13",
          "form": "먹는건 좀.. 불편하지만ㅎㅎ",
          "original_form": "먹는건 좀.. 불편하지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027714.1.14",
          "form": "저도 홍시 제일 좋아해요",
          "original_form": "저도 홍시 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027714.1.15",
          "form": "특히 얼려먹는걸 좋아해요",
          "original_form": "특히 얼려먹는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027714.1.16",
          "form": "아 드실줄 아신다",
          "original_form": "아 드실줄 아신다",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027714.1.17",
          "form": "겨울과일은 역시.. 귤이죠?ㅋㅋㅋㅋㅋ",
          "original_form": "겨울과일은 역시.. 귤이죠?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027714.1.18",
          "form": "맞아요! 보일러 키고 이불속에서 먹는 귤",
          "original_form": "맞아요! 보일러 키고 이불속에서 먹는 귤",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027714.1.19",
          "form": "세상 최고죠",
          "original_form": "세상 최고죠",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027714.1.20",
          "form": "어릴 때 빌려온 만화책이랑 슈퍼에서 귤 삼천원어치면 세상 행복했는데",
          "original_form": "어릴 때 빌려온 만화책이랑 슈퍼에서 귤 삼천원어치면 세상 행복했는데",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100027714.1.21",
          "form": "지금은 만화책 대신 폰을.. 머쓱",
          "original_form": "지금은 만화책 대신 폰을.. 머쓱",
          "speaker_id": "1",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027714.1.22",
          "form": "폰만 있으면 다 되죠 요즘은ㅎㅎ",
          "original_form": "폰만 있으면 다 되죠 요즘은ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100027714.1.23",
          "form": "맞아요ㅋㅋㅋ 이북 서비스도 많이 좋아지고",
          "original_form": "맞아요ㅋㅋㅋ 이북 서비스도 많이 좋아지고",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027714.1.24",
          "form": "아녀도 즐길게 많아서 너무 바쁜것",
          "original_form": "아녀도 즐길게 많아서 너무 바쁜것",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027714.1.25",
          "form": "사실 웹툰 하나만 잡아도 시간 잘가요",
          "original_form": "사실 웹툰 하나만 잡아도 시간 잘가요",
          "speaker_id": "2",
          "time": "20210902 14:52"
        }
      ]
    }
