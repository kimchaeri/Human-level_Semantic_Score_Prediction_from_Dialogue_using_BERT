      "utterance": [
        {
          "id": "MDRW2100036948.1.1",
          "form": "매운거 못 먹어요...",
          "original_form": "매운거 못 먹어요...",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036948.1.2",
          "form": "저도 못먹는데 맛있어서 떨면서 먹어여ㅋㅋ",
          "original_form": "저도 못먹는데 맛있어서 떨면서 먹어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100036948.1.3",
          "form": "그런 매력이 있죠 매운맛은",
          "original_form": "그런 매력이 있죠 매운맛은",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036948.1.4",
          "form": "네 진짜 매운건 왜그렇게 맛있죠\n전 불닭도 좋아해여",
          "original_form": "네 진짜 매운건 왜그렇게 맛있죠\n전 불닭도 좋아해여",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036948.1.5",
          "form": "저두요 그저께 하나 끓여먹었어요ㅋㅋ",
          "original_form": "저두요 그저께 하나 끓여먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036948.1.6",
          "form": "오리지널 좋아하세요?",
          "original_form": "오리지널 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100036948.1.7",
          "form": "네 딸국질하면서 먹어욬ㅋ",
          "original_form": "네 딸국질하면서 먹어욬ㅋ",
          "speaker_id": "2",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100036948.1.8",
          "form": "저는 울면서 먹어요ㅋㅋ",
          "original_form": "저는 울면서 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:40"
        },
        {
          "id": "MDRW2100036948.1.9",
          "form": "ㅋㅋㅋ 평소에는 짜파게티에 섞어 먹는데\n가끔 그냥먹고싶을때가 있더라고요",
          "original_form": "ㅋㅋㅋ 평소에는 짜파게티에 섞어 먹는데\n가끔 그냥먹고싶을때가 있더라고요",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036948.1.10",
          "form": "짜파게티에 섞어 먹으면 맛있나요?",
          "original_form": "짜파게티에 섞어 먹으면 맛있나요?",
          "speaker_id": "1",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036948.1.11",
          "form": "네 저희집은 보통 세개에 하나정도 섞는데 그래도 매워요 ㅋ",
          "original_form": "네 저희집은 보통 세개에 하나정도 섞는데 그래도 매워요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 15:44"
        },
        {
          "id": "MDRW2100036948.1.12",
          "form": "역시 불닭이 강하군요 한번 해먹어보고 싶네요",
          "original_form": "역시 불닭이 강하군요 한번 해먹어보고 싶네요",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036948.1.13",
          "form": "매콤해서 진짜 맛나요",
          "original_form": "매콤해서 진짜 맛나요",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036948.1.14",
          "form": "저녁까지 라면 땡길까요...",
          "original_form": "저녁까지 라면 땡길까요...",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100036948.1.15",
          "form": "전 저녁에 라면 먹으려고요ㅋㅋㅋ",
          "original_form": "전 저녁에 라면 먹으려고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:49"
        }
      ]
