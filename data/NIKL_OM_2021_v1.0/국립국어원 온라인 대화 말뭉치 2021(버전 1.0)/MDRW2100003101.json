      "utterance": [
        {
          "id": "MDRW2100003101.1.1",
          "form": "집에서 키우는 반려동물 있나요?",
          "original_form": "집에서 키우는 반려동물 있나요?",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003101.1.2",
          "form": "구피 키워요",
          "original_form": "구피 키워요",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003101.1.3",
          "form": "아 구피를 키우시는군용ㅋㅋ",
          "original_form": "아 구피를 키우시는군용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003101.1.4",
          "form": "구피도 번식력 좋죠?",
          "original_form": "구피도 번식력 좋죠?",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003101.1.5",
          "form": "번식력 좋죠",
          "original_form": "번식력 좋죠",
          "speaker_id": "2",
          "time": "20210820 23:48"
        },
        {
          "id": "MDRW2100003101.1.6",
          "form": "번식하는대로 다 키우시는거에요?",
          "original_form": "번식하는대로 다 키우시는거에요?",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003101.1.7",
          "form": "아님 입양 보내세용?",
          "original_form": "아님 입양 보내세용?",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003101.1.8",
          "form": "그대로 키워요",
          "original_form": "그대로 키워요",
          "speaker_id": "2",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003101.1.9",
          "form": "오래 키우니 번식력도 떨어지네요",
          "original_form": "오래 키우니 번식력도 떨어지네요",
          "speaker_id": "2",
          "time": "20210821 00:07"
        },
        {
          "id": "MDRW2100003101.1.10",
          "form": "아... 구피들도 나이가 들어가나봅니당",
          "original_form": "아... 구피들도 나이가 들어가나봅니당",
          "speaker_id": "1",
          "time": "20210821 00:07"
        },
        {
          "id": "MDRW2100003101.1.11",
          "form": "1년 정도 산다고 하더라구요",
          "original_form": "1년 정도 산다고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003101.1.12",
          "form": "생각보다 수명이 짧네요ㅠㅠ",
          "original_form": "생각보다 수명이 짧네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:18"
        },
        {
          "id": "MDRW2100003101.1.13",
          "form": "그쵸. 오래 살면 2년 정도 사는거 같아요.",
          "original_form": "그쵸. 오래 살면 2년 정도 사는거 같아요.",
          "speaker_id": "2",
          "time": "20210821 00:28"
        },
        {
          "id": "MDRW2100003101.1.14",
          "form": "구피 키우는건 어렵지 않나요?",
          "original_form": "구피 키우는건 어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210821 00:29"
        },
        {
          "id": "MDRW2100003101.1.15",
          "form": "네. 먹이만 주고 물만 갈아주면 됩니다\r\nㅋㅋㅋ",
          "original_form": "네. 먹이만 주고 물만 갈아주면 됩니다\r\nㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 00:55"
        },
        {
          "id": "MDRW2100003101.1.16",
          "form": "저도 아이들 좀 키우면\r\n구피도 키워봐야겠어요^^",
