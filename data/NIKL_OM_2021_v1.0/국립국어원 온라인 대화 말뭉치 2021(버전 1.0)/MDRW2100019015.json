        },
        {
          "id": "MDRW2100019015.1.4",
          "form": "참치김밥이요",
          "original_form": "참치김밥이요",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019015.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019015.1.6",
          "form": "저는 육개장국밥먹었어요~",
          "original_form": "저는 육개장국밥먹었어요~",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019015.1.7",
          "form": "맛있는거 드셨네요///!!",
          "original_form": "맛있는거 드셨네요///!!",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019015.1.8",
          "form": "맞아요~ 장조림하고 먹었답니당",
          "original_form": "맞아요~ 장조림하고 먹었답니당",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019015.1.9",
          "form": "아아 최고죠!",
          "original_form": "아아 최고죠!",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019015.1.10",
          "form": "저는 저녁에 국밥먹을예정이라,,,!",
          "original_form": "저는 저녁에 국밥먹을예정이라,,,!",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019015.1.11",
          "form": "오! 어떤국밥 드실건가요?",
          "original_form": "오! 어떤국밥 드실건가요?",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019015.1.12",
          "form": "아 돼지국밥이죠!",
          "original_form": "아 돼지국밥이죠!",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019015.1.13",
          "form": "저두 돼지국밥 너무좋아해요 혹시 어느지역사시나요?",
          "original_form": "저두 돼지국밥 너무좋아해요 혹시 어느지역사시나요?",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019015.1.14",
          "form": "저는 대구요!",
          "original_form": "저는 대구요!",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019015.1.15",
          "form": "아하~ 대구도 국밥맛집이 많은가요?",
          "original_form": "아하~ 대구도 국밥맛집이 많은가요?",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019015.1.16",
          "form": "네네네!",
          "original_form": "네네네!",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019015.1.17",
          "form": "name1님은 어디사세요~?",
          "original_form": "&name1&님은 어디사세요~?",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019015.1.18",
          "form": "저는 부산살아요 ㅎㅎ",
          "original_form": "저는 부산살아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019015.1.19",
          "form": "아하 가깝네요!",
          "original_form": "아하 가깝네요!",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019015.1.20",
          "form": "ㅋㅋㅋ요새 요리에 빠져서 요리해먹어요!",
          "original_form": "ㅋㅋㅋ요새 요리에 빠져서 요리해먹어요!",
