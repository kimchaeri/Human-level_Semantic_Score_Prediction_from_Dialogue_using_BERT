      "utterance": [
        {
          "id": "MDRW2100038683.1.1",
          "form": "제일 싫어하는 집안일이 뭐예요?",
          "original_form": "제일 싫어하는 집안일이 뭐예요?",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038683.1.2",
          "form": "흠...설거지요",
          "original_form": "흠...설거지요",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038683.1.3",
          "form": "넘 귀찮아요ㅋㅋ",
          "original_form": "넘 귀찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038683.1.4",
          "form": "끝이없는게 너무 싫죠ㅠㅠ",
          "original_form": "끝이없는게 너무 싫죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100038683.1.5",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100038683.1.6",
          "form": "식기세척기 사고싶으시겠어요",
          "original_form": "식기세척기 사고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100038683.1.7",
          "form": "ㅋㅋㅋ완전요 식기세척기 써보셨어요?",
          "original_form": "ㅋㅋㅋ완전요 식기세척기 써보셨어요?",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100038683.1.8",
          "form": "본가에서 써봤어요",
          "original_form": "본가에서 써봤어요",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038683.1.9",
          "form": "익숙해지면 편하더라구요",
          "original_form": "익숙해지면 편하더라구요",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100038683.1.10",
          "form": "오 써보고싶어요",
          "original_form": "오 써보고싶어요",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100038683.1.11",
          "form": "저는 빨래건조가 비올때마다 힘들어서",
          "original_form": "저는 빨래건조가 비올때마다 힘들어서",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100038683.1.12",
          "form": "건조기 사고싶어요",
          "original_form": "건조기 사고싶어요",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100038683.1.13",
          "form": "완전요",
          "original_form": "완전요",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100038683.1.14",
          "form": "에어컨만 엄청 틀고있어요ㅠㅠ",
          "original_form": "에어컨만 엄청 틀고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100038683.1.15",
          "form": "에어컨에 선풍기까지 다동원해요...ㅠ",
          "original_form": "에어컨에 선풍기까지 다동원해요...ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100038683.1.16",
          "form": "이번장마시즌 빨래힘들었어요",
          "original_form": "이번장마시즌 빨래힘들었어요",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038683.1.17",
          "form": "맞아요 이번에 비 너무 많이와가지고ㅠㅠ",
          "original_form": "맞아요 이번에 비 너무 많이와가지고ㅠㅠ",
