      "utterance": [
        {
          "id": "MDRW2100045576.1.1",
          "form": "기사인 내가 지켜야할 레이디둘시네아라고",
          "original_form": "기사인 내가 지켜야할 레이디둘시네아라고",
          "speaker_id": "2",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.2",
          "form": "갑자기그러면서따라다님",
          "original_form": "갑자기그러면서따라다님",
          "speaker_id": "2",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.3",
          "form": "여자가",
          "original_form": "여자가",
          "speaker_id": "1",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.4",
          "form": "근데 알돈자는",
          "original_form": "근데 알돈자는",
          "speaker_id": "2",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.5",
          "form": "산초보고",
          "original_form": "산초보고",
          "speaker_id": "1",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.6",
          "form": "어이털려한 이유를",
          "original_form": "어이털려한 이유를",
          "speaker_id": "1",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.7",
          "form": "알겟다",
          "original_form": "알겟다",
          "speaker_id": "1",
          "time": "20210908 03:43"
        },
        {
          "id": "MDRW2100045576.1.8",
          "form": "몸 파는 여종인데",
          "original_form": "몸 파는 여종인데",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045576.1.9",
          "form": "돈키가",
          "original_form": "돈키가",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045576.1.10",
          "form": "자기의레이디라고하면서 예를갖춰서",
          "original_form": "자기의레이디라고하면서 예를갖춰서",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045576.1.11",
          "form": "대하거든",
          "original_form": "대하거든",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045576.1.12",
          "form": "물론귀찮게한",
          "original_form": "물론귀찮게한",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045576.1.13",
          "form": "ㅇㅇ....",
          "original_form": "ㅇㅇ....",
          "speaker_id": "2",
          "time": "20210908 03:44"
        },
        {
          "id": "MDRW2100045576.1.14",
          "form": "막전투에 나갈때 승리를기원하는 이정표를달라고 떼쓰거나",
          "original_form": "막전투에 나갈때 승리를기원하는 이정표를달라고 떼쓰거나",
          "speaker_id": "2",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045576.1.15",
          "form": "자꾸 둘시네아라그래서 알돈자를 곤란하게만드는...?",
          "original_form": "자꾸 둘시네아라그래서 알돈자를 곤란하게만드는...?",
          "speaker_id": "2",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045576.1.16",
          "form": "근데 다들 몸만 보는 사람들과달리 자꾸 자기를 존중해주는 돈키호테를보고 마음을 열어여",
          "original_form": "근데 다들 몸만 보는 사람들과달리 자꾸 자기를 존중해주는 돈키호테를보고 마음을 열어여",
          "speaker_id": "2",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045576.1.17",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㄱ저건 그 귀찮게하는 거 ㅇㅇㅇ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㄱ저건 그 귀찮게하는 거 ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045576.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 03:45"
        },
        {
          "id": "MDRW2100045576.1.19",
          "form": "정병인데 알맹이는 썩지않은 정병....",
          "original_form": "정병인데 알맹이는 썩지않은 정병....",
          "speaker_id": "1",
          "time": "20210908 03:46"
        },
        {
          "id": "MDRW2100045576.1.20",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210908 03:46"
        },
        {
          "id": "MDRW2100045576.1.21",
          "form": "정병은 또 되네",
          "original_form": "정병은 또 되네",
          "speaker_id": "1",
          "time": "20210908 03:46"
        },
        {
          "id": "MDRW2100045576.1.22",
          "form": "알수없는 필터링기준",
          "original_form": "알수없는 필터링기준",
          "speaker_id": "1",
          "time": "20210908 03:46"
        },
        {
          "id": "MDRW2100045576.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 03:47"
        },
        {
          "id": "MDRW2100045576.1.24",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 03:47"
        },
        {
          "id": "MDRW2100045576.1.25",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210908 03:47"
        },
        {
          "id": "MDRW2100045576.1.26",
          "form": "홍광호는",
          "original_form": "홍광호는",
          "speaker_id": "1",
          "time": "20210908 03:47"
        },
        {
          "id": "MDRW2100045576.1.27",
