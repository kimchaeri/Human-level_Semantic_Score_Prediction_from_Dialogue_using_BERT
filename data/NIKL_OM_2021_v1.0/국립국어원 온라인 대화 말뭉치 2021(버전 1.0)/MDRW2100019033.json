      "utterance": [
        {
          "id": "MDRW2100019033.1.1",
          "form": "전 국물라면이요",
          "original_form": "전 국물라면이요",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019033.1.2",
          "form": "신라면?",
          "original_form": "신라면?",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019033.1.3",
          "form": "진라면 순한맛 좋아해요",
          "original_form": "진라면 순한맛 좋아해요",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019033.1.4",
          "form": "오 특이하시다",
          "original_form": "오 특이하시다",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019033.1.5",
          "form": "진라면순한맛에 날계란 휘뚜루마뚜루 먹으면 최고...",
          "original_form": "진라면순한맛에 날계란 휘뚜루마뚜루 먹으면 최고...",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019033.1.6",
          "form": "ㅋㄲㅋ",
          "original_form": "ㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019033.1.7",
          "form": "저는 불닭볶음면 좋아해요",
          "original_form": "저는 불닭볶음면 좋아해요",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019033.1.8",
          "form": "매운 거 좋아하세요?",
          "original_form": "매운 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019033.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019033.1.10",
          "form": "전 잘못먹어가지고",
          "original_form": "전 잘못먹어가지고",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019033.1.11",
          "form": "저도 잘 못 먹는데 좋아해서 찾아먹어요ㅋㅋ",
          "original_form": "저도 잘 못 먹는데 좋아해서 찾아먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019033.1.12",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019033.1.13",
          "form": "맛있는 거 시켜먹고싶네요",
          "original_form": "맛있는 거 시켜먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019033.1.14",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        }
