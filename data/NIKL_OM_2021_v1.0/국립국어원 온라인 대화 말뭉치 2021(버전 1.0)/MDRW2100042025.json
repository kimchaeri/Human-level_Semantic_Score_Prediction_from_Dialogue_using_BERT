      "utterance": [
        {
          "id": "MDRW2100042025.1.1",
          "form": "혹시 술도 드세요?",
          "original_form": "혹시 술도 드세요?",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100042025.1.2",
          "form": "칵테일좋아해요",
          "original_form": "칵테일좋아해요",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100042025.1.3",
          "form": "소주마시면 쓰러져요",
          "original_form": "소주마시면 쓰러져요",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100042025.1.4",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100042025.1.5",
          "form": "칵테일은 뭐 즐겨드세요?",
          "original_form": "칵테일은 뭐 즐겨드세요?",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100042025.1.6",
          "form": "저는 깔루아밀크",
          "original_form": "저는 깔루아밀크",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100042025.1.7",
          "form": "좋아해요 ㅋㅋ",
          "original_form": "좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100042025.1.8",
          "form": "저는 도수있는거 마셔서 종류는잘모르겠어요",
          "original_form": "저는 도수있는거 마셔서 종류는잘모르겠어요",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042025.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100042025.1.10",
          "form": "그렇군영ㅋㅋ",
          "original_form": "그렇군영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:57"
        },
        {
          "id": "MDRW2100042025.1.11",
          "form": "머릿속에 온통 치킨생각밖에없어요",
          "original_form": "머릿속에 온통 치킨생각밖에없어요",
          "speaker_id": "2",
          "time": "20210906 18:59"
        },
        {
          "id": "MDRW2100042025.1.12",
          "form": "ㅋㅋㅋㅋ치킨먹을때\n맥주 안드세여?",
          "original_form": "ㅋㅋㅋㅋ치킨먹을때\n맥주 안드세여?",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100042025.1.13",
          "form": "지금 치킨뜯고있어요",
          "original_form": "지금 치킨뜯고있어요",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042025.1.14",
          "form": "물이랑.....ㅋㅋㅋ",
          "original_form": "물이랑.....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042025.1.15",
          "form": "아니... 음료 안드세염?",
          "original_form": "아니... 음료 안드세염?",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042025.1.16",
          "form": "탄산이라도...",
          "original_form": "탄산이라도...",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042025.1.17",
          "form": "탄산이없어요ㅠㅠ",
          "original_form": "탄산이없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042025.1.18",
          "form": "토레타는있어요",
          "original_form": "토레타는있어요",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042025.1.19",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042025.1.20",
          "form": "탄산수... 맛없....ㅜㅜ",
          "original_form": "탄산수... 맛없....ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042025.1.21",
          "form": "물이랑 치킨 조합",
          "original_form": "물이랑 치킨 조합",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042025.1.22",
          "form": "물이랑은 안먹어본거 같아영",
          "original_form": "물이랑은 안먹어본거 같아영",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042025.1.23",
          "form": "먹을만해요",
          "original_form": "먹을만해요",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042025.1.24",
