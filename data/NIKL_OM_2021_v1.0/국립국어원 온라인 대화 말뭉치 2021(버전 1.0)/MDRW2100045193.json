      "utterance": [
        {
          "id": "MDRW2100045193.1.1",
          "form": "여행은 자주 다니시나요?",
          "original_form": "여행은 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045193.1.2",
          "form": "원래 좋아하는데 요즘은 못다니고 있어요",
          "original_form": "원래 좋아하는데 요즘은 못다니고 있어요",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045193.1.3",
          "form": "그쵸ㅠㅠ 코로나 때문에..",
          "original_form": "그쵸ㅠㅠ 코로나 때문에..",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045193.1.4",
          "form": "저도 요새 너무 여행가고싶어요",
          "original_form": "저도 요새 너무 여행가고싶어요",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045193.1.5",
          "form": "어디 가고싶으세요?",
          "original_form": "어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045193.1.6",
          "form": "저는 친구들이랑 계획세웠다가 무산된게 있어서",
          "original_form": "저는 친구들이랑 계획세웠다가 무산된게 있어서",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045193.1.7",
          "form": "미국 여행이 너무 가고싶어요ㅠㅠ",
          "original_form": "미국 여행이 너무 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045193.1.8",
          "form": "미국 어디요??",
          "original_form": "미국 어디요??",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045193.1.9",
          "form": "워싱턴 쪽이요! 이정도만 정해두고 다같이 돈 모으고 있었는데 물거품 됐어요 ㅠㅠㅋㅋ",
          "original_form": "워싱턴 쪽이요! 이정도만 정해두고 다같이 돈 모으고 있었는데 물거품 됐어요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045193.1.10",
          "form": "아쉽네요 ㅠㅜ",
          "original_form": "아쉽네요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045193.1.11",
          "form": "저도 미국 가보고싶어요",
          "original_form": "저도 미국 가보고싶어요",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045193.1.12",
          "form": "예전엔 보통 어디여행 다니셨어요? 국내여행??",
          "original_form": "예전엔 보통 어디여행 다니셨어요? 국내여행??",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045193.1.13",
          "form": "전 보통 반년동안 원기옥 모아서",
          "original_form": "전 보통 반년동안 원기옥 모아서",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045193.1.14",
          "form": "해외여행 갔었어요!",
          "original_form": "해외여행 갔었어요!",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045193.1.15",
          "form": "우와!!ㅠㅠ 진짜 대단해요",
          "original_form": "우와!!ㅠㅠ 진짜 대단해요",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045193.1.16",
          "form": "전 돈 모으는걸 잘못해서 조금만 모이면 써버리고 그래서 ㅋㅋㅋ ㅜㅜ",
          "original_form": "전 돈 모으는걸 잘못해서 조금만 모이면 써버리고 그래서 ㅋㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045193.1.17",
          "form": "저도 결국엔 쓰는거라 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 결국엔 쓰는거라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045193.1.18",
          "form": "한방에 쓸 뿐이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "한방에 쓸 뿐이에요 ㅋㅋㅋㅋㅋ",
