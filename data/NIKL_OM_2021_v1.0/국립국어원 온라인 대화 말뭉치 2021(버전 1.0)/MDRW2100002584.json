      "utterance": [
        {
          "id": "MDRW2100002584.1.1",
          "form": "예전에",
          "original_form": "예전에",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002584.1.2",
          "form": "학교다닐때는 독후감때문에",
          "original_form": "학교다닐때는 독후감때문에",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002584.1.3",
          "form": "베스트셀러 책 막 읽고그랬는데...",
          "original_form": "베스트셀러 책 막 읽고그랬는데...",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002584.1.4",
          "form": "나이드니ㄲ ㅏ 읽을 일이 없네여",
          "original_form": "나이드니ㄲ ㅏ 읽을 일이 없네여",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002584.1.5",
          "form": "전 그래서 요즘 웹툰만봐요",
          "original_form": "전 그래서 요즘 웹툰만봐요",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002584.1.6",
          "form": "그냥 가볍게 읽을게 필요해서..",
          "original_form": "그냥 가볍게 읽을게 필요해서..",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002584.1.7",
          "form": "웹툰 뭐보세요?",
          "original_form": "웹툰 뭐보세요?",
          "speaker_id": "1",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002584.1.8",
          "form": "네이버쪽 웹툰 주로봐요",
          "original_form": "네이버쪽 웹툰 주로봐요",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002584.1.9",
          "form": "숲속의 담 \r\n집이없어\r\n호랑이형님\r\n화이트블러드",
          "original_form": "숲속의 담 \r\n집이없어\r\n호랑이형님\r\n화이트블러드",
          "speaker_id": "2",
          "time": "20210820 13:48"
        },
        {
          "id": "MDRW2100002584.1.10",
          "form": "헐 숲속의담 살짝",
          "original_form": "헐 숲속의담 살짝",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.11",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.12",
          "form": "세계멸망(?)그런분위기인대",
          "original_form": "세계멸망(?)그런분위기인대",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.13",
          "form": "그림체가 따뜻해서",
          "original_form": "그림체가 따뜻해서",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.14",
          "form": "기억해요",
          "original_form": "기억해요",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.15",
          "form": "약간 멸망보다는",
          "original_form": "약간 멸망보다는",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.16",
          "form": "멸망 후? 라고 생각하면 될거같아요",
          "original_form": "멸망 후? 라고 생각하면 될거같아요",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.17",
          "form": "그 안에서 살아남은 후대이야기라",
          "original_form": "그 안에서 살아남은 후대이야기라",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002584.1.18",
          "form": "보다가 말았는데...!!!! 다시봐야겠어요!!",
          "original_form": "보다가 말았는데...!!!! 다시봐야겠어요!!",
          "speaker_id": "1",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002584.1.19",
          "form": "name1님은 보고계신 웹툰있으세요?",
          "original_form": "&name1&님은 보고계신 웹툰있으세요?",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002584.1.20",
          "form": "저도 추천하나 해주세용",
          "original_form": "저도 추천하나 해주세용",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002584.1.21",
          "form": "저는 여주실격이요!!",
          "original_form": "저는 여주실격이요!!",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002584.1.22",
          "form": "그거 완전재밌어",
          "original_form": "그거 완전재밌어",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002584.1.23",
          "form": "기맹기 작님꺼잖아요",
          "original_form": "기맹기 작님꺼잖아요",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002584.1.24",
