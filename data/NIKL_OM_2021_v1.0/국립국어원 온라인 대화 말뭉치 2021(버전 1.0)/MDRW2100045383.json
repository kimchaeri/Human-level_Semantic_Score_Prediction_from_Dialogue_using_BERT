        },
        {
          "id": "MDRW2100045383.1.2",
          "form": "님은 뭐 좋아해요",
          "original_form": "님은 뭐 좋아해요",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045383.1.3",
          "form": "저는 가을이요",
          "original_form": "저는 가을이요",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045383.1.4",
          "form": "지금이네요",
          "original_form": "지금이네요",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045383.1.5",
          "form": "선선해서요",
          "original_form": "선선해서요",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045383.1.6",
          "form": "가을엔 맛난거도 많죠",
          "original_form": "가을엔 맛난거도 많죠",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045383.1.7",
          "form": "저는 전어 좋아하는 ㅎㅎ",
          "original_form": "저는 전어 좋아하는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045383.1.8",
          "form": "전어 짱이지요",
          "original_form": "전어 짱이지요",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045383.1.9",
          "form": "못먹는사람도 많던데",
          "original_form": "못먹는사람도 많던데",
          "speaker_id": "2",
          "time": "20210908 02:16"
