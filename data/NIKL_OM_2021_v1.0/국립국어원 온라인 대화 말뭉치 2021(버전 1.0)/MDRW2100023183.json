      "utterance": [
        {
          "id": "MDRW2100023183.1.1",
          "form": "우와 스포츠!",
          "original_form": "우와 스포츠!",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023183.1.2",
          "form": "저 원래 올림픽에 관심 없었는데",
          "original_form": "저 원래 올림픽에 관심 없었는데",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023183.1.3",
          "form": "양궁은 정말 숨죽이고 봤어요!",
          "original_form": "양궁은 정말 숨죽이고 봤어요!",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023183.1.4",
          "form": "양궁은 다른 스포츠처럼 평소에 자주 접할 수 없는 스포츠다보니까 올림픽 때 더 집중 받는 것 같아요 ㅋㅋㅋ",
          "original_form": "양궁은 다른 스포츠처럼 평소에 자주 접할 수 없는 스포츠다보니까 올림픽 때 더 집중 받는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023183.1.5",
          "form": "저도 양궁은 시간 찾아가면서 챙겨 봤어요 ㅎㅎ",
          "original_form": "저도 양궁은 시간 찾아가면서 챙겨 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023183.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023183.1.7",
          "form": "선수는 정적인데",
          "original_form": "선수는 정적인데",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023183.1.8",
          "form": "화살은 정말 빠르다는 점에서",
          "original_form": "화살은 정말 빠르다는 점에서",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023183.1.9",
          "form": "신선하기도 하고...!!!",
          "original_form": "신선하기도 하고...!!!",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023183.1.10",
          "form": "우리나라 양궁 잘하는 거 너무 신기해요 ㅎㅎㅎ",
          "original_form": "우리나라 양궁 잘하는 거 너무 신기해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023183.1.11",
          "form": "우스갯소리로 쏴야 할 사람이 많아서 양궁 잘한다고 그러기도 하더라고요 ㅎㅎ",
          "original_form": "우스갯소리로 쏴야 할 사람이 많아서 양궁 잘한다고 그러기도 하더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:10"
        },
        {
          "id": "MDRW2100023183.1.12",
          "form": "저도 과녁에 4명 정도 사진 걸 수 있을 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "저도 과녁에 4명 정도 사진 걸 수 있을 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:11"
        },
        {
          "id": "MDRW2100023183.1.13",
          "form": "아나 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아나 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023183.1.14",
          "form": "그런 말이 있었어요?! 몰랐네요!!!!",
          "original_form": "그런 말이 있었어요?! 몰랐네요!!!!",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023183.1.15",
          "form": "우리나라는 하긴... 옛날부터",
          "original_form": "우리나라는 하긴... 옛날부터",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023183.1.16",
          "form": "고구려 시대에도 활을 잘 쐈더랬죠!!",
          "original_form": "고구려 시대에도 활을 잘 쐈더랬죠!!",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023183.1.17",
          "form": "그리고 부정부패가 없는 것도 한몫하는 거 같아요 ㅎㅎ",
          "original_form": "그리고 부정부패가 없는 것도 한몫하는 거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023183.1.18",
          "form": "협회가 운영을 잘해서 참 마음에 들어요 ㅎㅎ",
          "original_form": "협회가 운영을 잘해서 참 마음에 들어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100023183.1.19",
          "form": "저는 요즘 배구도 챙겨 보는데....",
          "original_form": "저는 요즘 배구도 챙겨 보는데....",
          "speaker_id": "2",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100023183.1.20",
          "form": "과녁에 배구협회 사진도 걸어야 할 것 같아요 ^.^",
          "original_form": "과녁에 배구협회 사진도 걸어야 할 것 같아요 ^.^",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023183.1.21",
          "form": "헉 맞아요",
          "original_form": "헉 맞아요",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023183.1.22",
          "form": "아니 그리고",
          "original_form": "아니 그리고",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023183.1.23",
          "form": "여남에 따라서",
          "original_form": "여남에 따라서",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023183.1.24",
          "form": "연봉 다른거 진짜",
          "original_form": "연봉 다른거 진짜",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023183.1.25",
          "form": "개에바 아니냐구요",
          "original_form": "개에바 아니냐구요",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023183.1.26",
          "form": "맞아요. 실력대로 연봉 책정되는 건 당연한건데",
          "original_form": "맞아요. 실력대로 연봉 책정되는 건 당연한건데",
          "speaker_id": "2",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100023183.1.27",
          "form": "배구 같은 경우는 연봉이 서로 바껴야 정상이죠",
          "original_form": "배구 같은 경우는 연봉이 서로 바껴야 정상이죠",
          "speaker_id": "2",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100023183.1.28",
          "form": "올림픽 누가 출전했는지... ㅎㅎ",
          "original_form": "올림픽 누가 출전했는지... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100023183.1.29",
          "form": "그러게 말이에요 ㅎ",
          "original_form": "그러게 말이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 19:28"
        },
        {
          "id": "MDRW2100023183.1.30",
          "form": "심지어 인지도도 더 높은데 말이에요",
          "original_form": "심지어 인지도도 더 높은데 말이에요",
          "speaker_id": "1",
          "time": "20210830 19:28"
        },
        {
          "id": "MDRW2100023183.1.31",
          "form": "남자 야구는",
          "original_form": "남자 야구는",
          "speaker_id": "1",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100023183.1.32",
          "form": "툭하면 성매매해서 들키기나 하는데 말이죠",
          "original_form": "툭하면 성매매해서 들키기나 하는데 말이죠",
          "speaker_id": "1",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100023183.1.33",
          "form": "저도 김연경 선수 경기하는거 실제로 보고싶네요ㅠ",
          "original_form": "저도 김연경 선수 경기하는거 실제로 보고싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 19:30"
        },
        {
          "id": "MDRW2100023183.1.34",
          "form": "중국 가지않는 이상 이제 기회가 없겠죠ㅠㅠ",
          "original_form": "중국 가지않는 이상 이제 기회가 없겠죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023183.1.35",
          "form": "야국는 진짜.. 이번에 껌 논란도 그렇고 헤엄 쳐서 왔어야 해요 ㅋㅋㅋ",
          "original_form": "야국는 진짜.. 이번에 껌 논란도 그렇고 헤엄 쳐서 왔어야 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023183.1.36",
          "form": "맞아요 진짜 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 진짜 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023183.1.37",
          "form": "비행기 타면 안되고 헤엄쳐서 와야죠 ㅋㅋㅋ 아 너무 웃겨요",
          "original_form": "비행기 타면 안되고 헤엄쳐서 와야죠 ㅋㅋㅋ 아 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023183.1.38",
          "form": "제 친구들 중에 야구 오랫동안 보는 애들 많았는데",
          "original_form": "제 친구들 중에 야구 오랫동안 보는 애들 많았는데",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023183.1.39",
          "form": "* 봐왔던 !",
          "original_form": "* 봐왔던 !",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023183.1.40",
          "form": "최근에 다들 탈덕했다고 하더라구요",
          "original_form": "최근에 다들 탈덕했다고 하더라구요",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023183.1.41",
          "form": "다들 참다참다 터졌나봐요 ㅋㅋㅋㅋ",
          "original_form": "다들 참다참다 터졌나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023183.1.42",
          "form": "그 연봉을 차라리 여배한테 줘야하는데...",
          "original_form": "그 연봉을 차라리 여배한테 줘야하는데...",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023183.1.43",
          "form": "그 연봉 받으면서 뭐하는건지 모르겠어요",
          "original_form": "그 연봉 받으면서 뭐하는건지 모르겠어요",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023183.1.44",
          "form": "그러니까 말이에요! 세상이 바뀌어야해요!!!",
          "original_form": "그러니까 말이에요! 세상이 바뀌어야해요!!!",
