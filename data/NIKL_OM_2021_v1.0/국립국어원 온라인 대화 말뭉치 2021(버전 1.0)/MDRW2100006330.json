      "utterance": [
        {
          "id": "MDRW2100006330.1.1",
          "form": "씨제이 더마켓 쿠폰 매일주나봐요",
          "original_form": "씨제이 더마켓 쿠폰 매일주나봐요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006330.1.2",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006330.1.3",
          "form": "저번주 사셨어요?",
          "original_form": "저번주 사셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006330.1.4",
          "form": "저번에 43프로받았는데",
          "original_form": "저번에 43프로받았는데",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006330.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006330.1.6",
          "form": "오늘 보니 매일되는거더라구요",
          "original_form": "오늘 보니 매일되는거더라구요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100006330.1.7",
          "form": "한번사서 안주는줄알았는데",
          "original_form": "한번사서 안주는줄알았는데",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.8",
          "form": "구매하셨어요?",
          "original_form": "구매하셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.9",
          "form": "43프로나와서 죽 왕창샀어요",
          "original_form": "43프로나와서 죽 왕창샀어요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.10",
          "form": "와~~ 저도 쿠폰만 받아봐야겠어요",
          "original_form": "와~~ 저도 쿠폰만 받아봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.11",
          "form": "중복5프로쓰고 페이고 12프로 ㅋ",
          "original_form": "중복5프로쓰고 페이고 12프로 ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.12",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.13",
          "form": "종류다양하더라구요",
          "original_form": "종류다양하더라구요",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.14",
          "form": "오~~~ 중복에 페이코할인 좋네요",
          "original_form": "오~~~ 중복에 페이코할인 좋네요",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100006330.1.15",
          "form": "10가지도 넘는듯",
          "original_form": "10가지도 넘는듯",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006330.1.16",
          "form": "저번주엔 페이코없었죠?",
          "original_form": "저번주엔 페이코없었죠?",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006330.1.17",
          "form": "안살꺼라 안알아봤어요",
          "original_form": "안살꺼라 안알아봤어요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006330.1.18",
          "form": "근데 사야할꺼같은 느낌이...",
          "original_form": "근데 사야할꺼같은 느낌이...",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100006330.1.19",
          "form": "필요한거 없으심 안사는게 돈버는거같아요ㅎㅎ",
          "original_form": "필요한거 없으심 안사는게 돈버는거같아요ㅎㅎ",
