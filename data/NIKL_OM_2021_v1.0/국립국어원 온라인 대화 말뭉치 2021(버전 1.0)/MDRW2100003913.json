      "utterance": [
        {
          "id": "MDRW2100003913.1.1",
          "form": "어떤 커뮤니티 해요?",
          "original_form": "어떤 커뮤니티 해요?",
          "speaker_id": "1",
          "time": "20210821 13:45"
        },
        {
          "id": "MDRW2100003913.1.2",
          "form": "커뮤니티는 안하고 인스타는해요",
          "original_form": "커뮤니티는 안하고 인스타는해요",
          "speaker_id": "2",
          "time": "20210821 13:45"
        },
        {
          "id": "MDRW2100003913.1.3",
          "form": "name1님은 하고계신 커뮤니티있나요?",
          "original_form": "&name1&님은 하고계신 커뮤니티있나요?",
          "speaker_id": "2",
          "time": "20210821 13:45"
        },
        {
          "id": "MDRW2100003913.1.4",
          "form": "인스타요? 전 예전에 했지만 요즘은 잘 안해요 가끔 이벤트 참여땐 해요",
          "original_form": "인스타요? 전 예전에 했지만 요즘은 잘 안해요 가끔 이벤트 참여땐 해요",
          "speaker_id": "1",
          "time": "20210821 13:46"
        },
        {
          "id": "MDRW2100003913.1.5",
          "form": "다음카페나 네이버 블로그 해요",
          "original_form": "다음카페나 네이버 블로그 해요",
          "speaker_id": "1",
          "time": "20210821 13:46"
        },
        {
          "id": "MDRW2100003913.1.6",
          "form": "블로그요?",
          "original_form": "블로그요?",
          "speaker_id": "2",
          "time": "20210821 13:47"
        },
        {
          "id": "MDRW2100003913.1.7",
          "form": "블로그면 저번에 오늘일기인가 그것도 참여하셨나요?",
          "original_form": "블로그면 저번에 오늘일기인가 그것도 참여하셨나요?",
          "speaker_id": "2",
          "time": "20210821 13:47"
        },
        {
          "id": "MDRW2100003913.1.8",
          "form": "저 그 때 한 10년만에 블로그 다시 들어가본거같은데",
          "original_form": "저 그 때 한 10년만에 블로그 다시 들어가본거같은데",
          "speaker_id": "2",
          "time": "20210821 13:48"
        },
        {
          "id": "MDRW2100003913.1.9",
          "form": "블로그는 하지 않아요 정보 찾기용이에요",
          "original_form": "블로그는 하지 않아요 정보 찾기용이에요",
          "speaker_id": "1",
          "time": "20210821 13:49"
        },
        {
          "id": "MDRW2100003913.1.10",
          "form": "네 저도 오늘일기 했었어요",
          "original_form": "네 저도 오늘일기 했었어요",
          "speaker_id": "1",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003913.1.11",
          "form": "근데 일찍 선잠 들어서 하루 빼먹었어요",
          "original_form": "근데 일찍 선잠 들어서 하루 빼먹었어요",
          "speaker_id": "1",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003913.1.12",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "2",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003913.1.13",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003913.1.14",
          "form": "너무 아쉬웠겠어요ㅠㅠ",
          "original_form": "너무 아쉬웠겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003913.1.15",
          "form": "처음꺼는 다했어요",
          "original_form": "처음꺼는 다했어요",
          "speaker_id": "1",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.16",
          "form": "다시 시작하는 건 3일했었나 ㅋㅋㅋ",
          "original_form": "다시 시작하는 건 3일했었나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.17",
          "form": "그때 말 많았잖아요",
          "original_form": "그때 말 많았잖아요",
          "speaker_id": "1",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.18",
          "form": "ㅇㅏ 두번째에서 실패하셨구나",
          "original_form": "ㅇㅏ 두번째에서 실패하셨구나",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.20",
          "form": "그 때 작심 3일=네이버라고",
          "original_form": "그 때 작심 3일=네이버라고",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.21",
          "form": "ㅋㅋㅋ 너무웃겼는데",
          "original_form": "ㅋㅋㅋ 너무웃겼는데",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003913.1.22",
          "form": "그때 사람들 드립들이 너무 웃겼어요",
          "original_form": "그때 사람들 드립들이 너무 웃겼어요",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003913.1.23",
          "form": "기억나요",
          "original_form": "기억나요",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003913.1.24",
          "form": "그러니까요 괜히 책임지지도 못 할 이벤트로",
          "original_form": "그러니까요 괜히 책임지지도 못 할 이벤트로",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003913.1.25",
          "form": "조롱이나 당하고 처음부터 잘하지",
          "original_form": "조롱이나 당하고 처음부터 잘하지",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003913.1.26",
          "form": "맞아요 너무 안일하게 봤어요",
          "original_form": "맞아요 너무 안일하게 봤어요",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003913.1.27",
          "form": "생각이 짧았던거같아요",
          "original_form": "생각이 짧았던거같아요",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003913.1.28",
          "form": "이름있는 포털이..",
          "original_form": "이름있는 포털이..",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003913.1.29",
          "form": "다음카페는 어떤가요?",
          "original_form": "다음카페는 어떤가요?",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003913.1.30",
          "form": "그냥 짤짤한 정보 얻기 좋아요",
          "original_form": "그냥 짤짤한 정보 얻기 좋아요",
