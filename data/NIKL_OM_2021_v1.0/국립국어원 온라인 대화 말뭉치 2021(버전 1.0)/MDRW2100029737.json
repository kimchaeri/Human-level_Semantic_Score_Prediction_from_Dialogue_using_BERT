      "utterance": [
        {
          "id": "MDRW2100029737.1.1",
          "form": "안녕하세요~\n유튜브 많이하세요?ㅎ",
          "original_form": "안녕하세요~\n유튜브 많이하세요?ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:40"
        },
        {
          "id": "MDRW2100029737.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029737.1.3",
          "form": "예전에는 즐겨봤는데",
          "original_form": "예전에는 즐겨봤는데",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029737.1.4",
          "form": "넷플 입문하고는\n잘 안보게 되네요",
          "original_form": "넷플 입문하고는\n잘 안보게 되네요",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029737.1.5",
          "form": "아하 그거더 나은가요?ㅎ",
          "original_form": "아하 그거더 나은가요?ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029737.1.6",
          "form": "음...  드라마들\n몰아보는 재미가 있어서요ㅋㅋ",
          "original_form": "음...  드라마들\n몰아보는 재미가 있어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029737.1.7",
          "form": "님은 주로 유툽 보세요?",
          "original_form": "님은 주로 유툽 보세요?",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029737.1.8",
          "form": "영상자체는 잘 안보구요 노래나 듣죠ㅋ",
          "original_form": "영상자체는 잘 안보구요 노래나 듣죠ㅋ",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100029737.1.9",
          "form": "오 좋아하는 가수 있으세여?",
          "original_form": "오 좋아하는 가수 있으세여?",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029737.1.10",
          "form": "방탄인가염? ㅋㅋ",
          "original_form": "방탄인가염? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029737.1.11",
          "form": "죄송요ㅋㅋㅋㄱ방탄 잘모르구요",
          "original_form": "죄송요ㅋㅋㅋㄱ방탄 잘모르구요",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029737.1.12",
          "form": "옛날노래요ㅜ기",
          "original_form": "옛날노래요ㅜ기",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029737.1.13",
          "form": "오 옛날 노래라하면",
          "original_form": "오 옛날 노래라하면",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029737.1.14",
          "form": "90년대 노래요?",
          "original_form": "90년대 노래요?",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029737.1.15",
          "form": "ㅋㅋㅋ1020이좋더라구요ㅋ",
          "original_form": "ㅋㅋㅋ1020이좋더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100029737.1.16",
          "form": "아항ㅋㅋ 1020",
          "original_form": "아항ㅋㅋ 1020",
          "speaker_id": "2",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029737.1.17",
          "form": "젤좋아하는곡은요?",
          "original_form": "젤좋아하는곡은요?",
          "speaker_id": "2",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100029737.1.18",
          "form": "ㅋㅋ그냥 발라드장르좋아해요ㅋ",
          "original_form": "ㅋㅋ그냥 발라드장르좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029737.1.19",
          "form": "꼭 좋아하느 가수고 곡은 딱히",
          "original_form": "꼭 좋아하느 가수고 곡은 딱히",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029737.1.20",
          "form": "ㅋㅋ그렇군요 ㅋㅋ",
          "original_form": "ㅋㅋ그렇군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029737.1.21",
          "form": "저는 ㅎㅏㄴ 곡에 꽂히면\n그 노래만 계속 들어서요 ㅋ",
          "original_form": "저는 ㅎㅏㄴ 곡에 꽂히면\n그 노래만 계속 들어서요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029737.1.22",
          "form": "ㅋㅋㅋ저도 그러긴해요ㅋ",
          "original_form": "ㅋㅋㅋ저도 그러긴해요ㅋ",
          "speaker_id": "1",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029737.1.23",
          "form": "요즘은 최신곡듣기가 힘드네요ㅜ",
          "original_form": "요즘은 최신곡듣기가 힘드네요ㅜ",
          "speaker_id": "1",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029737.1.24",
          "form": "나이가 들어가나봐요 ㅋ",
          "original_form": "나이가 들어가나봐요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029737.1.25",
          "form": "옛노래가 좋아지는거 보면",
          "original_form": "옛노래가 좋아지는거 보면",
          "speaker_id": "2",
          "time": "20210903 16:24"
        }
      ]
    }
  ]
