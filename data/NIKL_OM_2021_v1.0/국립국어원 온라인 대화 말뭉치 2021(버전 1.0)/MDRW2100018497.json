      "utterance": [
        {
          "id": "MDRW2100018497.1.1",
          "form": "런닝맨 저도 좋아했던 프로라서",
          "original_form": "런닝맨 저도 좋아했던 프로라서",
          "speaker_id": "2",
          "time": "20210827 06:34"
        },
        {
          "id": "MDRW2100018497.1.2",
          "form": "광수하차에 아쉬웠어요ㅠㅠ",
          "original_form": "광수하차에 아쉬웠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.3",
          "form": "네네ㅠㅠ 배신기린 최고였는데ㅋㅋㅋ",
          "original_form": "네네ㅠㅠ 배신기린 최고였는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.4",
          "form": "최근에 런닝맨 보셨나용??",
          "original_form": "최근에 런닝맨 보셨나용??",
          "speaker_id": "2",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.5",
          "form": "광수하차하고 나서는 안보고",
          "original_form": "광수하차하고 나서는 안보고",
          "speaker_id": "1",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.6",
          "form": "종종 클립만 봤어용",
          "original_form": "종종 클립만 봤어용",
          "speaker_id": "1",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.7",
          "form": "오히려 하차하고 다들 이악물고 하는지",
          "original_form": "오히려 하차하고 다들 이악물고 하는지",
          "speaker_id": "2",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.8",
          "form": "아하 그 영어로만 말하는 게임 봤는데",
          "original_form": "아하 그 영어로만 말하는 게임 봤는데",
          "speaker_id": "1",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.9",
          "form": "다른 케미가 생기더라구요 !",
          "original_form": "다른 케미가 생기더라구요 !",
          "speaker_id": "2",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.10",
          "form": "진짜 웃겨요ㅋㅋㅋㅋ",
          "original_form": "진짜 웃겨요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:35"
        },
        {
          "id": "MDRW2100018497.1.11",
          "form": "그리고 웃음참기편도 보셨나용?",
          "original_form": "그리고 웃음참기편도 보셨나용?",
          "speaker_id": "1",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.12",
          "form": "ㅋㅋㅋ최근에 저는 이영지 나와서 봤는데",
          "original_form": "ㅋㅋㅋ최근에 저는 이영지 나와서 봤는데",
          "speaker_id": "2",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.13",
          "form": "가볍게 보기 좋아요",
          "original_form": "가볍게 보기 좋아요",
          "speaker_id": "2",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.14",
          "form": "앜ㅋㅋㄱ봤어요 보셨죠?",
          "original_form": "앜ㅋㅋㄱ봤어요 보셨죠?",
          "speaker_id": "2",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.15",
          "form": "네네ㅋㅋㅋㅋ 울다가",
          "original_form": "네네ㅋㅋㅋㅋ 울다가",
          "speaker_id": "1",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.16",
          "form": "울뻔했어요ㅋㅋㅋ",
          "original_form": "울뻔했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.17",
          "form": "저도모르게 보면서 같이 웃음 참게되더라구옄ㅋㅋㅋ",
          "original_form": "저도모르게 보면서 같이 웃음 참게되더라구옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.18",
          "form": "한기범 선수 나올때 쓰러졌어욬ㅋㅋㅋ",
          "original_form": "한기범 선수 나올때 쓰러졌어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018497.1.19",
          "form": "마지막으로 보신 영화있으세요?",
          "original_form": "마지막으로 보신 영화있으세요?",
          "speaker_id": "1",
          "time": "20210827 06:37"
        },
        {
          "id": "MDRW2100018497.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 넘웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 넘웃겨요",
          "speaker_id": "2",
          "time": "20210827 06:37"
        }
