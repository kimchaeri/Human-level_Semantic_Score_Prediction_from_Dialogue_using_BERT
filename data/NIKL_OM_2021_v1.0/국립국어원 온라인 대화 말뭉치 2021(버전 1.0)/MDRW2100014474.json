      "utterance": [
        {
          "id": "MDRW2100014474.1.1",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014474.1.2",
          "form": "아니요! 미혼이에요",
          "original_form": "아니요! 미혼이에요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014474.1.3",
          "form": "결혼계획은 있나요,?",
          "original_form": "결혼계획은 있나요,?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014474.1.4",
          "form": "전  비혼주위라",
          "original_form": "전  비혼주위라",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014474.1.5",
          "form": "아니요 없어요 저도",
          "original_form": "아니요 없어요 저도",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014474.1.6",
          "form": "비혼은 아니고",
          "original_form": "비혼은 아니고",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014474.1.7",
          "form": "굳이 할 생각은 없는?",
          "original_form": "굳이 할 생각은 없는?",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014474.1.8",
          "form": "슬프네요 ㅋ굳이라는 말이 ㅋ",
          "original_form": "슬프네요 ㅋ굳이라는 말이 ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014474.1.9",
          "form": "필수라고 생각 안 해서요!",
          "original_form": "필수라고 생각 안 해서요!",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014474.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014474.1.11",
          "form": "연애일때가 가장좋다곤해요",
          "original_form": "연애일때가 가장좋다곤해요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014474.1.12",
          "form": "맞아요 맞아!",
          "original_form": "맞아요 맞아!",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014474.1.13",
          "form": "그냥 연애만 하다가 죽을래요!",
          "original_form": "그냥 연애만 하다가 죽을래요!",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014474.1.14",
          "form": "오늘 심심이 깔았는데 재밋네요 ㅋ",
          "original_form": "오늘 심심이 깔았는데 재밋네요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014474.1.15",
          "form": "그냥 수다떨기에 딱이에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 수다떨기에 딱이에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014474.1.16",
          "form": "모르는사람이라서 더 편하게 말할수있다니 ㅋ",
          "original_form": "모르는사람이라서 더 편하게 말할수있다니 ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014474.1.17",
          "form": "세상 혼자 살기 좋네요 ㅋ",
          "original_form": "세상 혼자 살기 좋네요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014474.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014474.1.19",
          "form": "혼자 뭐 하면 누구 만날 시간도 없어요",
          "original_form": "혼자 뭐 하면 누구 만날 시간도 없어요",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014474.1.20",
          "form": "혼자가 점점 편해지는 느낌!",
          "original_form": "혼자가 점점 편해지는 느낌!",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014474.1.21",
          "form": "코시국이라서 점점 혼자 더 많은걸 하는것 같아요",
          "original_form": "코시국이라서 점점 혼자 더 많은걸 하는것 같아요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014474.1.22",
          "form": "미팅 같은거 해봤어요?",
          "original_form": "미팅 같은거 해봤어요?",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014474.1.23",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014474.1.24",
          "form": "미팅은 대학생때 이후로는 안 해 봤어요!",
          "original_form": "미팅은 대학생때 이후로는 안 해 봤어요!",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014474.1.25",
          "form": "요즘은 자만추죠",
          "original_form": "요즘은 자만추죠",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014474.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014474.1.27",
          "form": "불가능인 것 같아요. 자만추",
          "original_form": "불가능인 것 같아요. 자만추",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014474.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ어디에서 할 수 있어요 그거?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어디에서 할 수 있어요 그거?",
          "speaker_id": "1",
          "time": "20210825 20:29"
        }
      ]
    }
  ]
