          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030291.1.3",
          "form": "얼른 종교 얘기 해봐",
          "original_form": "얼른 종교 얘기 해봐",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.4",
          "form": "ㅎㅎ 일단 난 무교임",
          "original_form": "ㅎㅎ 일단 난 무교임",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.5",
          "form": "나도 무교",
          "original_form": "나도 무교",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.6",
          "form": "근데 개인적으로 종교에 대해서는 찬성함",
          "original_form": "근데 개인적으로 종교에 대해서는 찬성함",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.7",
          "form": "얼마전에 자매님이 전화오셨는데 너한텐 전화 안 옴?",
          "original_form": "얼마전에 자매님이 전화오셨는데 너한텐 전화 안 옴?",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.8",
          "form": "옴",
          "original_form": "옴",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.9",
          "form": "ㅋㅋㅋㅋ뭐야 우리 토론 하는거야?",
          "original_form": "ㅋㅋㅋㅋ뭐야 우리 토론 하는거야?",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.10",
          "form": "나도 찬성은 하지 근데 강요는 좀..,",
          "original_form": "나도 찬성은 하지 근데 강요는 좀..,",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.11",
          "form": "그런 거 아닌가?ㅋㅋㅋㅋ",
          "original_form": "그런 거 아닌가?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.12",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.13",
          "form": "난 그냥 흥미로워서",
          "original_form": "난 그냥 흥미로워서",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030291.1.14",
          "form": "ㅋㅋㅋ나 얼마전에 이상한 종교인들 길거리에서 만났다",
          "original_form": "ㅋㅋㅋ나 얼마전에 이상한 종교인들 길거리에서 만났다",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030291.1.15",
          "form": "오 난 아직 한 번도 안 만나 봤는데",
          "original_form": "오 난 아직 한 번도 안 만나 봤는데",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030291.1.16",
          "form": "뭐라고 하더라 ㅠㅠ 나보고 기가 아름답다부터 시작해서 전생에 공덕을 많이 쌓았다고 하던데",
          "original_form": "뭐라고 하더라 ㅠㅠ 나보고 기가 아름답다부터 시작해서 전생에 공덕을 많이 쌓았다고 하던데",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030291.1.17",
          "form": "두 번 만났으",
          "original_form": "두 번 만났으",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030291.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ웃기네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ웃기네",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030291.1.19",
          "form": "ㅋㅋ시내 가면 이상한 사이비 종교인들 널렸어",
          "original_form": "ㅋㅋ시내 가면 이상한 사이비 종교인들 널렸어",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030291.1.20",
          "form": "가장 많은건 역시 신천지지,,",
          "original_form": "가장 많은건 역시 신천지지,,",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030291.1.21",
          "form": "맨날 팻말들고 있고",
          "original_form": "맨날 팻말들고 있고",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030291.1.22",
          "form": "그게 신천지인가?",
          "original_form": "그게 신천지인가?",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030291.1.23",
          "form": "그런거 아니야?",
          "original_form": "그런거 아니야?",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030291.1.24",
          "form": "하나님 어머니회?그런것도 있던데",
          "original_form": "하나님 어머니회?그런것도 있던데",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030291.1.25",
          "form": "어쩐지 좀 민폐드라",
          "original_form": "어쩐지 좀 민폐드라",
          "speaker_id": "2",
          "time": "20210903 23:37"
        }
      ]
