      "utterance": [
        {
          "id": "MDRW2100015865.1.1",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210826 02:26"
        },
        {
          "id": "MDRW2100015865.1.2",
          "form": "아니요 아직 미혼입니다",
          "original_form": "아니요 아직 미혼입니다",
          "speaker_id": "2",
          "time": "20210826 02:29"
        },
        {
          "id": "MDRW2100015865.1.3",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "1",
          "time": "20210826 02:29"
        },
        {
          "id": "MDRW2100015865.1.4",
          "form": "자취하시는건가요?",
          "original_form": "자취하시는건가요?",
          "speaker_id": "1",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015865.1.5",
          "form": "아니요 name1님은 자취하세요?",
          "original_form": "아니요 &name1&님은 자취하세요?",
          "speaker_id": "2",
          "time": "20210826 02:30"
        },
        {
          "id": "MDRW2100015865.1.6",
          "form": "아 아뇨 저도 가족하고 살아요",
          "original_form": "아 아뇨 저도 가족하고 살아요",
          "speaker_id": "1",
          "time": "20210826 02:31"
        },
        {
          "id": "MDRW2100015865.1.7",
          "form": "그럼 집안일 많이 하세요?",
          "original_form": "그럼 집안일 많이 하세요?",
          "speaker_id": "1",
          "time": "20210826 02:31"
        },
        {
          "id": "MDRW2100015865.1.8",
          "form": "저는 방청소랑 설거지 정도만 해요.. ㅋㅋ",
          "original_form": "저는 방청소랑 설거지 정도만 해요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:32"
        },
        {
          "id": "MDRW2100015865.1.9",
          "form": "저도 요리/설거지/그리고 방청소랑 분리수거 정도만 해요ㅎㅎ",
          "original_form": "저도 요리/설거지/그리고 방청소랑 분리수거 정도만 해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 02:35"
        },
        {
          "id": "MDRW2100015865.1.10",
          "form": "청소기 돌릴 때는 거실도 함께 하구요",
          "original_form": "청소기 돌릴 때는 거실도 함께 하구요",
          "speaker_id": "2",
          "time": "20210826 02:35"
        },
        {
          "id": "MDRW2100015865.1.11",
          "form": "저랑 비슷하시네요",
          "original_form": "저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210826 02:36"
        },
        {
          "id": "MDRW2100015865.1.12",
          "form": "청소하기 전에는 몰랐는데 세상엔 먼지가 참 많은거 같아요..ㅋㅋㅋ",
          "original_form": "청소하기 전에는 몰랐는데 세상엔 먼지가 참 많은거 같아요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:38"
        },
        {
          "id": "MDRW2100015865.1.13",
          "form": "3일만 지나도 수북하더라구요",
          "original_form": "3일만 지나도 수북하더라구요",
          "speaker_id": "1",
          "time": "20210826 02:38"
        },
        {
          "id": "MDRW2100015865.1.14",
          "form": "그쵸! 그리고 머리카락도 얼마나 많이 떨어지는지...",
          "original_form": "그쵸! 그리고 머리카락도 얼마나 많이 떨어지는지...",
          "speaker_id": "2",
          "time": "20210826 02:39"
        },
        {
          "id": "MDRW2100015865.1.15",
          "form": "맞아요!! 테이프클리너 없이는 못살아요ㅠ",
          "original_form": "맞아요!! 테이프클리너 없이는 못살아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 02:39"
        },
        {
          "id": "MDRW2100015865.1.16",
          "form": "테이프클리너 정말 좋은 아이템이죠!",
          "original_form": "테이프클리너 정말 좋은 아이템이죠!",
          "speaker_id": "2",
          "time": "20210826 02:40"
        },
        {
          "id": "MDRW2100015865.1.17",
          "form": "혹시 반려동물 키우세요?",
          "original_form": "혹시 반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210826 02:41"
        },
        {
          "id": "MDRW2100015865.1.18",
          "form": "아니요 name1님은요?",
          "original_form": "아니요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 02:45"
        },
