      "utterance": [
        {
          "id": "MDRW2100027529.1.1",
          "form": "오늘 아침 뭐먹엇어?",
          "original_form": "오늘 아침 뭐먹엇어?",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027529.1.2",
          "form": "오늘 아침 뭐먹엇어?",
          "original_form": "오늘 아침 뭐먹엇어?",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027529.1.3",
          "form": "오늘은 고기볶음밥을 먹었어",
          "original_form": "오늘은 고기볶음밥을 먹었어",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027529.1.4",
          "form": "내가 조리법 알려줄까?",
          "original_form": "내가 조리법 알려줄까?",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027529.1.5",
          "form": "맛잇엇겟다... 조리법은 됏구 나두 한입만",
          "original_form": "맛잇엇겟다... 조리법은 됏구 나두 한입만",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027529.1.6",
          "form": "조리법 알려줄께",
          "original_form": "조리법 알려줄께",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.7",
          "form": "이잉",
          "original_form": "이잉",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.8",
          "form": "프라이팬을 우선",
          "original_form": "프라이팬을 우선",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.9",
          "form": "예열해.",
          "original_form": "예열해.",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.10",
          "form": "그래 어디 알려줘봐",
          "original_form": "그래 어디 알려줘봐",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.11",
          "form": "알려줘보라구",
          "original_form": "알려줘보라구",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.12",
          "form": "그리고 고기를 볶고 참기름을 한숟가락 넣구",
          "original_form": "그리고 고기를 볶고 참기름을 한숟가락 넣구",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.13",
          "form": "볶다가",
          "original_form": "볶다가",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.14",
          "form": "밥도 넣어서 볶아.",
          "original_form": "밥도 넣어서 볶아.",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.15",
          "form": "소금 후추를 적당히 뿌려서 마무리하면",
          "original_form": "소금 후추를 적당히 뿌려서 마무리하면",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.16",
          "form": "돼. 간단하지?",
          "original_form": "돼. 간단하지?",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.18",
          "form": "어려웡",
          "original_form": "어려웡",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027529.1.19",
          "form": "그럼 관도",
          "original_form": "그럼 관도",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.20",
          "form": "그냥 난 뺏어먹을래.",
          "original_form": "그냥 난 뺏어먹을래.",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.21",
          "form": "저녁밥 뭐먹을래?",
          "original_form": "저녁밥 뭐먹을래?",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.22",
          "form": "엄마가 저녁 뭐먹을거냬",
          "original_form": "엄마가 저녁 뭐먹을거냬",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.23",
          "form": "오늘은 맛잇는거 시켜먹자",
          "original_form": "오늘은 맛잇는거 시켜먹자",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.24",
          "form": "뭐 시켜먹자",
          "original_form": "뭐 시켜먹자",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.25",
          "form": "햄버거 먹자",
          "original_form": "햄버거 먹자",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.26",
          "form": "햄버거는 롯데리아지",
          "original_form": "햄버거는 롯데리아지",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.27",
          "form": "맥도날드는 배달이 안되고",
          "original_form": "맥도날드는 배달이 안되고",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.28",
          "form": "그건 맞지",
          "original_form": "그건 맞지",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.29",
          "form": "버거킹은 너무 비싸고",
          "original_form": "버거킹은 너무 비싸고",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.30",
          "form": "근데 오늘은 다른거 먹어!!!!",
          "original_form": "근데 오늘은 다른거 먹어!!!!",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.31",
          "form": "맘스터치는 너무 많이먹어서 질렸어.",
          "original_form": "맘스터치는 너무 많이먹어서 질렸어.",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.32",
          "form": "다른거 뭐먹어",
          "original_form": "다른거 뭐먹어",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027529.1.33",
          "form": "룰렛 돌리기 해서 골라",
          "original_form": "룰렛 돌리기 해서 골라",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.34",
          "form": "메뉴를 말을 해봐",
          "original_form": "메뉴를 말을 해봐",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.35",
          "form": "그것도",
          "original_form": "그것도",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.36",
          "form": "먹을게있어야하지",
          "original_form": "먹을게있어야하지",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.37",
          "form": "돈까스 파스타",
          "original_form": "돈까스 파스타",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.38",
          "form": "생선구이 햄버거 비빔밥 해물스파게티 냉면 보쌈",
          "original_form": "생선구이 햄버거 비빔밥 해물스파게티 냉면 보쌈",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.39",
          "form": "이렇게 하면 언니 납득?",
          "original_form": "이렇게 하면 언니 납득?",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.40",
          "form": "안할거잖아",
          "original_form": "안할거잖아",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.41",
          "form": "으!",
          "original_form": "으!",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.42",
          "form": "그럼 그냥 햄버거 먹자 ㅎㅎ",
          "original_form": "그럼 그냥 햄버거 먹자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.43",
          "form": "메뉴 추천이나 해봐",
          "original_form": "메뉴 추천이나 해봐",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027529.1.44",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027529.1.45",
          "form": "햄버거가 짱이지 ㅎㅎ",
          "original_form": "햄버거가 짱이지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:31"
        }
      ]
    }
