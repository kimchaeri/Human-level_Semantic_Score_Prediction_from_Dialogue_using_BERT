      "utterance": [
        {
          "id": "MDRW2100022554.1.1",
          "form": "네네 배라랑 버거킹이렁거 많이 팔더라구요 ㅋㅋㅋㅋ",
          "original_form": "네네 배라랑 버거킹이렁거 많이 팔더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022554.1.2",
          "form": "거의 매달 배라는 사놓는거같아요!",
          "original_form": "거의 매달 배라는 사놓는거같아요!",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022554.1.3",
          "form": "베라랑 버거킹은 여기저기서 할인/판촉 많이 하는 것 같아요",
          "original_form": "베라랑 버거킹은 여기저기서 할인/판촉 많이 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022554.1.4",
          "form": "그래서 저번에 네고왕 베라 때 메리트 없었는데",
          "original_form": "그래서 저번에 네고왕 베라 때 메리트 없었는데",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.5",
          "form": "알바생만 죽어나갔따면서요 ㅠㅠㅠ",
          "original_form": "알바생만 죽어나갔따면서요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.6",
          "form": "아 맞아요 멍든사진보닠가 맘아프던데",
          "original_form": "아 맞아요 멍든사진보닠가 맘아프던데",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.7",
          "form": "근데 저는 베라 할인 그렇게 자주하는 줄 몰랐어요...",
          "original_form": "근데 저는 베라 할인 그렇게 자주하는 줄 몰랐어요...",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.8",
          "form": "근데 그게 원래 상시할인하는 가격이래요",
          "original_form": "근데 그게 원래 상시할인하는 가격이래요",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.9",
          "form": "그래서 다들 네고왕 때 우르르 사러 갔나봐요",
          "original_form": "그래서 다들 네고왕 때 우르르 사러 갔나봐요",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.10",
          "form": "ㅋㅋㅋ정말...",
          "original_form": "ㅋㅋㅋ정말...",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.11",
          "form": "네고왕이 글케 싸진않앗대요",
          "original_form": "네고왕이 글케 싸진않앗대요",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.12",
          "form": "알바생만 ㅠㅠ",
          "original_form": "알바생만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.13",
          "form": "저 그때 사려고 했는데 안사길 잘했어요",
          "original_form": "저 그때 사려고 했는데 안사길 잘했어요",
          "speaker_id": "1",
          "time": "20210829 15:42"
        },
        {
          "id": "MDRW2100022554.1.14",
          "form": "처음으로 작은 냉동고에게 감사하네요",
          "original_form": "처음으로 작은 냉동고에게 감사하네요",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.15",
          "form": "아 ㅋㅋㅋㅋㅋ그렇군요",
          "original_form": "아 ㅋㅋㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.16",
          "form": "혹시 베라 추천 맛 있어요?",
          "original_form": "혹시 베라 추천 맛 있어요?",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.17",
          "form": "배라 무슨맛 구입하세요?",
          "original_form": "배라 무슨맛 구입하세요?",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.18",
          "form": "무슨 맛 제일 좋아해요?",
          "original_form": "무슨 맛 제일 좋아해요?",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.19",
          "form": "전 엄마는 외계인이랑 무슨 녹차들어간거 좋아해요",
          "original_form": "전 엄마는 외계인이랑 무슨 녹차들어간거 좋아해요",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.20",
          "form": "피스타치오에 과자 들어간거? 그거 좋아해요!!",
          "original_form": "피스타치오에 과자 들어간거? 그거 좋아해요!!",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022554.1.21",
          "form": "ㅋㅋㅋㅋㅋ저는 민초 좋아해여 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저는 민초 좋아해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
