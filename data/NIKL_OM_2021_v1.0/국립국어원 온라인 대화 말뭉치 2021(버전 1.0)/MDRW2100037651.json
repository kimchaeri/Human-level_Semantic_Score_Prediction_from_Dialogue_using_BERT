      "utterance": [
        {
          "id": "MDRW2100037651.1.1",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037651.1.2",
          "form": "네 좋아해요 님은 좋아하세요?",
          "original_form": "네 좋아해요 님은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100037651.1.3",
          "form": "오~ 저도 엄청 좋아했죠... (과거형)",
          "original_form": "오~ 저도 엄청 좋아했죠... (과거형)",
          "speaker_id": "1",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100037651.1.4",
          "form": "지금은 커피 안 좋아하나요?",
          "original_form": "지금은 커피 안 좋아하나요?",
          "speaker_id": "2",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100037651.1.5",
          "form": "아뇨.. 좋아하는데.. 속아파서 이젠 못먹어요",
          "original_form": "아뇨.. 좋아하는데.. 속아파서 이젠 못먹어요",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100037651.1.6",
          "form": "자주 마시면 위에 안 좋다긴하다네요",
          "original_form": "자주 마시면 위에 안 좋다긴하다네요",
          "speaker_id": "2",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100037651.1.7",
          "form": "전 아까도 카페라떼 만들어 먹었어요",
          "original_form": "전 아까도 카페라떼 만들어 먹었어요",
          "speaker_id": "2",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100037651.1.8",
          "form": "오~ 저는 주로 원두내려 마셨어요",
          "original_form": "오~ 저는 주로 원두내려 마셨어요",
          "speaker_id": "1",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037651.1.9",
          "form": "근데 하루에 1리터씩 마시다가.. 위가 나빠져서 끊었죠",
          "original_form": "근데 하루에 1리터씩 마시다가.. 위가 나빠져서 끊었죠",
          "speaker_id": "1",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037651.1.10",
          "form": "우와 향기 좋겠다",
          "original_form": "우와 향기 좋겠다",
          "speaker_id": "2",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037651.1.11",
          "form": "헉 1리터씩이나요?",
          "original_form": "헉 1리터씩이나요?",
          "speaker_id": "2",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037651.1.12",
          "form": "그래서 병났죠..ㅋㅋㅋ",
          "original_form": "그래서 병났죠..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100037651.1.13",
          "form": "위 한번 망치면 계속 아프다던데",
          "original_form": "위 한번 망치면 계속 아프다던데",
          "speaker_id": "2",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100037651.1.14",
          "form": "약먹고 지금은 괜찮은데..",
          "original_form": "약먹고 지금은 괜찮은데..",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100037651.1.15",
          "form": "전 카누랑 믹스커피 섞고 얼음 넣고 우유 거품 나게 하고 넣어서 라떼 만들어요",
          "original_form": "전 카누랑 믹스커피 섞고 얼음 넣고 우유 거품 나게 하고 넣어서 라떼 만들어요",
          "speaker_id": "2",
          "time": "20210901 15:15"
        },
        {
          "id": "MDRW2100037651.1.16",
          "form": "또 아플까봐 자제줒이에요",
          "original_form": "또 아플까봐 자제줒이에요",
          "speaker_id": "1",
          "time": "20210901 15:15"
        },
