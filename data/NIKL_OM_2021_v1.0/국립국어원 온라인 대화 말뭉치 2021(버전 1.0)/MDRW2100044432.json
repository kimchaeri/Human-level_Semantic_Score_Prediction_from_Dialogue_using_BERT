      "utterance": [
        {
          "id": "MDRW2100044432.1.1",
          "form": "어떤 과목을 제일 좋아하세요?",
          "original_form": "어떤 과목을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044432.1.2",
          "form": "앗 학교 졸업한지 좀 돼서",
          "original_form": "앗 학교 졸업한지 좀 돼서",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044432.1.3",
          "form": "전 국어가 그나마 좋았어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 국어가 그나마 좋았어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044432.1.4",
          "form": "괜찮아요 ㅋㅋ 저도 졸업한지 조금 됐지만",
          "original_form": "괜찮아요 ㅋㅋ 저도 졸업한지 조금 됐지만",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044432.1.5",
          "form": "앗 그런가요?",
          "original_form": "앗 그런가요?",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044432.1.6",
          "form": "국어! 저도 국어 좋아했어요",
          "original_form": "국어! 저도 국어 좋아했어요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044432.1.7",
          "form": "국어가 그나마 암기 안해도 되고",
          "original_form": "국어가 그나마 암기 안해도 되고",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044432.1.8",
          "form": "네!! 국어는 암기 같이 느껴지는 부분이 적어서 좋아했어요",
          "original_form": "네!! 국어는 암기 같이 느껴지는 부분이 적어서 좋아했어요",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044432.1.9",
          "form": "그쵸 그리고 공부대상이 문학작품이나 좀 재밌는 소재 비문학이라 좋았던거같아요",
          "original_form": "그쵸 그리고 공부대상이 문학작품이나 좀 재밌는 소재 비문학이라 좋았던거같아요",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044432.1.10",
          "form": "문학이 제일 좋았어요",
          "original_form": "문학이 제일 좋았어요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044432.1.11",
          "form": "비문학은.. 요새 다시 보니까",
          "original_form": "비문학은.. 요새 다시 보니까",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044432.1.12",
          "form": "전문적인 정보들이 엄청 많은 것 같더라구요",
          "original_form": "전문적인 정보들이 엄청 많은 것 같더라구요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044432.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044432.1.14",
          "form": "그당시에도 사실 좀 당황스러운 소재들 많았어요",
          "original_form": "그당시에도 사실 좀 당황스러운 소재들 많았어요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044432.1.15",
          "form": "특히 과학지문들 나오면 한참 헤맸던거같아요",
          "original_form": "특히 과학지문들 나오면 한참 헤맸던거같아요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044432.1.16",
          "form": "와.. 진짜루요..",
          "original_form": "와.. 진짜루요..",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044432.1.17",
          "form": "전 경제 부분 나오면",
          "original_form": "전 경제 부분 나오면",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044432.1.18",
          "form": "머리가 핑 도는 느낌이었어요",
          "original_form": "머리가 핑 도는 느낌이었어요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044432.1.19",
          "form": "앗 ㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "앗 ㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044432.1.20",
          "form": "경제도 어렵죠",
          "original_form": "경제도 어렵죠",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044432.1.21",
          "form": "어쩜 비문학은 논문에서 그렇게 따서 쓰는건지..",
          "original_form": "어쩜 비문학은 논문에서 그렇게 따서 쓰는건지..",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044432.1.22",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044432.1.23",
          "form": "그쵸 ㅋㅋㅋ 학생땐 몰랐는데",
          "original_form": "그쵸 ㅋㅋㅋ 학생땐 몰랐는데",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044432.1.24",
          "form": "커서보니까 그냥 봐도 어려운걸",
          "original_form": "커서보니까 그냥 봐도 어려운걸",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044432.1.25",
          "form": "문학은 책보는것 같아서 좋았는데 ㅠ",
          "original_form": "문학은 책보는것 같아서 좋았는데 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044432.1.26",
          "form": "시간제한 두고 읽으라 해놨더라구요?",
          "original_form": "시간제한 두고 읽으라 해놨더라구요?",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044432.1.27",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044432.1.28",
          "form": "지금 봐도 어려워요 ㅋㅋㅋ",
          "original_form": "지금 봐도 어려워요 ㅋㅋㅋ",
