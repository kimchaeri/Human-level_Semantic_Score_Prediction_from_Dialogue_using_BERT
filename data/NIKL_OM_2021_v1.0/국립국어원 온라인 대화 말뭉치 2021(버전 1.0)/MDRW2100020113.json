        {
          "id": "MDRW2100020113.1.2",
          "form": "부럽다요",
          "original_form": "부럽다요",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020113.1.3",
          "form": "오늘도 그래도 나름 잘벌려서 좋았네요 ㅎㅎ",
          "original_form": "오늘도 그래도 나름 잘벌려서 좋았네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020113.1.4",
          "form": "얼마나?",
          "original_form": "얼마나?",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020113.1.5",
          "form": "정확한 금액은 말할순없지만",
          "original_form": "정확한 금액은 말할순없지만",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020113.1.6",
          "form": "대충 10정도 벌었네용",
          "original_form": "대충 10정도 벌었네용",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020113.1.7",
          "form": "그정도면",
          "original_form": "그정도면",
          "speaker_id": "1",
          "time": "20210828 07:38"
        },
        {
          "id": "MDRW2100020113.1.8",
          "form": "나름 많은편이죠 부업치고 ㅎㅎ",
          "original_form": "나름 많은편이죠 부업치고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100020113.1.9",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100020113.1.10",
          "form": "조금씩 조금씩 짭짤한게",
