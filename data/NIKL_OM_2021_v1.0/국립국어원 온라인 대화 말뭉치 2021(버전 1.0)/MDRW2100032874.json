      "utterance": [
        {
          "id": "MDRW2100032874.1.1",
          "form": "날씨랑 계절 흠",
          "original_form": "날씨랑 계절 흠",
          "speaker_id": "2",
          "time": "20210906 03:41"
        },
        {
          "id": "MDRW2100032874.1.2",
          "form": "일단 난 추워서 선풍기를 넣었네요",
          "original_form": "일단 난 추워서 선풍기를 넣었네요",
          "speaker_id": "2",
          "time": "20210906 03:41"
        },
        {
          "id": "MDRW2100032874.1.3",
          "form": "선풍기는 집에 없고 에어컨을 껐다켰다 하는중...",
          "original_form": "선풍기는 집에 없고 에어컨을 껐다켰다 하는중...",
          "speaker_id": "1",
          "time": "20210906 03:42"
        },
        {
          "id": "MDRW2100032874.1.4",
          "form": "비오면 집이 너무 습해서 끄고 싶어도 끌 수가 없네ㅠㅠ",
          "original_form": "비오면 집이 너무 습해서 끄고 싶어도 끌 수가 없네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 03:42"
        },
        {
          "id": "MDRW2100032874.1.5",
          "form": "에어컨을.............?",
          "original_form": "에어컨을.............?",
          "speaker_id": "2",
          "time": "20210906 03:43"
        },
        {
          "id": "MDRW2100032874.1.6",
          "form": "대단한걸",
          "original_form": "대단한걸",
          "speaker_id": "2",
          "time": "20210906 03:43"
        },
        {
          "id": "MDRW2100032874.1.7",
          "form": "ㅠㅠㅠ아 습하면 그럴 수 있지",
          "original_form": "ㅠㅠㅠ아 습하면 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210906 03:43"
        },
        {
          "id": "MDRW2100032874.1.8",
          "form": "일주일전엔 비 많이 왔어",
          "original_form": "일주일전엔 비 많이 왔어",
          "speaker_id": "2",
          "time": "20210906 03:44"
        },
        {
          "id": "MDRW2100032874.1.9",
          "form": "비 많이 온날 추운데 끄지도 못했지....ㅠㅠ",
          "original_form": "비 많이 온날 추운데 끄지도 못했지....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 03:44"
        },
        {
          "id": "MDRW2100032874.1.10",
          "form": "집이 습하면 돈벌레나 노랭이 같은 벌레들이 들어와서 최대한 뽀송해야해...",
          "original_form": "집이 습하면 돈벌레나 노랭이 같은 벌레들이 들어와서 최대한 뽀송해야해...",
          "speaker_id": "1",
          "time": "20210906 03:45"
        },
        {
          "id": "MDRW2100032874.1.11",
          "form": "제습기늘 없으...?",
          "original_form": "제습기늘 없으...?",
          "speaker_id": "2",
          "time": "20210906 03:49"
        },
        {
          "id": "MDRW2100032874.1.12",
          "form": "아으으으으으 진짜싫어",
          "original_form": "아으으으으으 진짜싫어",
          "speaker_id": "2",
          "time": "20210906 03:49"
        },
        {
          "id": "MDRW2100032874.1.13",
          "form": "벌레ㅠㅠㅠㅠ아ㅠㅠㅠ욱 토나와",
          "original_form": "벌레ㅠㅠㅠㅠ아ㅠㅠㅠ욱 토나와",
          "speaker_id": "2",
          "time": "20210906 03:50"
        },
        {
          "id": "MDRW2100032874.1.14",
          "form": "그래서여름싫어하는데",
          "original_form": "그래서여름싫어하는데",
          "speaker_id": "2",
          "time": "20210906 03:50"
        },
        {
          "id": "MDRW2100032874.1.15",
          "form": "이 집에서 여름을 처음 맞아서 이럴 줄 몰랐어...",
          "original_form": "이 집에서 여름을 처음 맞아서 이럴 줄 몰랐어...",
          "speaker_id": "1",
          "time": "20210906 03:51"
        },
        {
          "id": "MDRW2100032874.1.16",
          "form": "그래도 지금은 많이 추워져서 톡토기라고 하는 벌래는 안나온다...",
          "original_form": "그래도 지금은 많이 추워져서 톡토기라고 하는 벌래는 안나온다...",
          "speaker_id": "1",
          "time": "20210906 03:51"
        },
        {
          "id": "MDRW2100032874.1.17",
          "form": "여름에 그것때문에 스트레스 많이 받았는데 그건 좋아",
          "original_form": "여름에 그것때문에 스트레스 많이 받았는데 그건 좋아",
          "speaker_id": "1",
          "time": "20210906 03:51"
        },
        {
          "id": "MDRW2100032874.1.18",
          "form": "톡토기 이름만 귀엽네",
          "original_form": "톡토기 이름만 귀엽네",
          "speaker_id": "2",
          "time": "20210906 03:52"
        },
        {
          "id": "MDRW2100032874.1.19",
          "form": "ㅋㅋㅋㅋ제습기 장만해야겠구나",
          "original_form": "ㅋㅋㅋㅋ제습기 장만해야겠구나",
          "speaker_id": "2",
          "time": "20210906 03:52"
        },
        {
          "id": "MDRW2100032874.1.20",
          "form": "사이도 엄청 작아 근데 잡으려고하면 톡톡 튀어서 잡기힘들어...",
          "original_form": "사이도 엄청 작아 근데 잡으려고하면 톡톡 튀어서 잡기힘들어...",
          "speaker_id": "1",
          "time": "20210906 03:53"
        },
        {
          "id": "MDRW2100032874.1.21",
          "form": "태풍오기전에 제습기 빨리 사려구ㅠㅠ",
          "original_form": "태풍오기전에 제습기 빨리 사려구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 03:54"
        },
        {
          "id": "MDRW2100032874.1.22",
          "form": "아으아아아아악",
          "original_form": "아으아아아아악",
          "speaker_id": "2",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032874.1.23",
          "form": "아오;;아요 벌써 스트레스받음",
          "original_form": "아오;;아요 벌써 스트레스받음",
          "speaker_id": "2",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032874.1.24",
          "form": "징그럿ㅠㅡㅠ",
          "original_form": "징그럿ㅠㅡㅠ",
          "speaker_id": "2",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032874.1.25",
          "form": "레알 스트레스야...",
          "original_form": "레알 스트레스야...",
          "speaker_id": "1",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032874.1.26",
          "form": "후...가을이니 태풍 오겠구나",
          "original_form": "후...가을이니 태풍 오겠구나",
          "speaker_id": "2",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032874.1.27",
          "form": "언넝 사 빨리 살 수록 삶의 질 높아질듯",
          "original_form": "언넝 사 빨리 살 수록 삶의 질 높아질듯",
          "speaker_id": "2",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032874.1.28",
          "form": "그래도 톡토기는 징그럽진 않아. 다만 까매가지고 벽지에 튀었을때 잡으면 까맣게 얼룩 남음.... 이게 더 거지 같앸ㅋㅋㅋㅋ",
          "original_form": "그래도 톡토기는 징그럽진 않아. 다만 까매가지고 벽지에 튀었을때 잡으면 까맣게 얼룩 남음.... 이게 더 거지 같앸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:56"
        },
        {
          "id": "MDRW2100032874.1.29",
          "form": "찾아봤는데 세스코에서도 이건 못잡는다고 하더라고",
          "original_form": "찾아봤는데 세스코에서도 이건 못잡는다고 하더라고",
          "speaker_id": "1",
          "time": "20210906 03:56"
        },
        {
          "id": "MDRW2100032874.1.30",
          "form": "날이 더우면 나오는거라고...",
          "original_form": "날이 더우면 나오는거라고...",
          "speaker_id": "1",
          "time": "20210906 03:56"
        },
        {
          "id": "MDRW2100032874.1.31",
          "form": "1층이라 앞에 화단있고 날 더우니까 이게 나온거였어...",
          "original_form": "1층이라 앞에 화단있고 날 더우니까 이게 나온거였어...",
          "speaker_id": "1",
          "time": "20210906 03:57"
        },
        {
          "id": "MDRW2100032874.1.32",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "2",
          "time": "20210906 03:57"
        },
        {
          "id": "MDRW2100032874.1.33",
          "form": "1층....",
          "original_form": "1층....",
          "speaker_id": "2",
          "time": "20210906 03:57"
        },
        {
          "id": "MDRW2100032874.1.34",
          "form": "벌레 견뎌야하지...ㅋㅋㅋ",
          "original_form": "벌레 견뎌야하지...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:57"
        },
        {
