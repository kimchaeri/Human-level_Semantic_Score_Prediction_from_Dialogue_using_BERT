          "speaker_id": "2",
          "time": "20210827 14:31"
        },
        {
          "id": "MDRW2100018995.1.4",
          "form": "배달로 맡겨서",
          "original_form": "배달로 맡겨서",
          "speaker_id": "2",
          "time": "20210827 14:31"
        },
        {
          "id": "MDRW2100018995.1.5",
          "form": "3시 뒤에 온대요",
          "original_form": "3시 뒤에 온대요",
          "speaker_id": "2",
          "time": "20210827 14:31"
