      "utterance": [
        {
          "id": "MDRW2100001717.1.1",
          "form": "이번에는 날씨다",
          "original_form": "이번에는 날씨다",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001717.1.2",
          "form": "요즘 밖에 날씨 어때??",
          "original_form": "요즘 밖에 날씨 어때??",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001717.1.3",
          "form": "요즘 갑자기 비가 너무 많이와",
          "original_form": "요즘 갑자기 비가 너무 많이와",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.4",
          "form": "아그래??",
          "original_form": "아그래??",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.5",
          "form": "나는 요즘 집콕해서",
          "original_form": "나는 요즘 집콕해서",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.6",
          "form": "몰랐네잉",
          "original_form": "몰랐네잉",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.7",
          "form": "그래서 우산 필수임",
          "original_form": "그래서 우산 필수임",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.8",
          "form": "맨날 가지고다녀",
          "original_form": "맨날 가지고다녀",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.9",
          "form": "가방에다가 미리미리 가지고다녀야지",
          "original_form": "가방에다가 미리미리 가지고다녀야지",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.10",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.11",
          "form": "그럼 더워??",
          "original_form": "그럼 더워??",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.12",
          "form": "왜이러는걸까",
          "original_form": "왜이러는걸까",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.13",
          "form": "엉 더우면서",
          "original_form": "엉 더우면서",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.14",
          "form": "갑자기 비와",
          "original_form": "갑자기 비와",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001717.1.15",
          "form": "으 완전 열대날씨네...",
          "original_form": "으 완전 열대날씨네...",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.16",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.17",
          "form": "내가 젤 싫어하는 날씨야",
          "original_form": "내가 젤 싫어하는 날씨야",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.18",
          "form": "무슨 스콜처럼",
          "original_form": "무슨 스콜처럼",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.19",
          "form": "스콜 진짜싫다 ㅡㅡ",
          "original_form": "스콜 진짜싫다 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.20",
          "form": "막 우수수수",
          "original_form": "막 우수수수",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.21",
          "form": "비오고나서는 그래도 하늘이 맑드라!!",
          "original_form": "비오고나서는 그래도 하늘이 맑드라!!",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.22",
          "form": "맞아 사실 더운데",
          "original_form": "맞아 사실 더운데",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.23",
          "form": "비와서 조금 시원해지긴함",
          "original_form": "비와서 조금 시원해지긴함",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.24",
          "form": "맞어 이제 조금 추움",
          "original_form": "맞어 이제 조금 추움",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001717.1.25",
          "form": "오늘도 비온다",
          "original_form": "오늘도 비온다",
