      "utterance": [
        {
          "id": "MDRW2100026407.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026407.1.2",
          "form": "날씨.. 요즘 저희지역은 비가 잦아서 힘들어요..",
          "original_form": "날씨.. 요즘 저희지역은 비가 잦아서 힘들어요..",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026407.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026407.1.4",
          "form": "요즘 비가 잔득오죠 ㅠ",
          "original_form": "요즘 비가 잔득오죠 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026407.1.5",
          "form": "비가 또 오려고 하면 한 번에 쭉 오고 말지",
          "original_form": "비가 또 오려고 하면 한 번에 쭉 오고 말지",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026407.1.6",
          "form": "월요일 왔다 화요일 안왔다가 수요일 오전 찔끔 왔다 안왔다 하니까",
          "original_form": "월요일 왔다 화요일 안왔다가 수요일 오전 찔끔 왔다 안왔다 하니까",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026407.1.7",
          "form": "종잡을 수가 없네요",
          "original_form": "종잡을 수가 없네요",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026407.1.8",
          "form": "아..정말요",
          "original_form": "아..정말요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026407.1.9",
          "form": "오늘도 비안올거같아서 우산안들고나갓늨데",
          "original_form": "오늘도 비안올거같아서 우산안들고나갓늨데",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026407.1.10",
          "form": "생쥐되어서 집에..ㅠ",
          "original_form": "생쥐되어서 집에..ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026407.1.11",
          "form": "헉 그럴 때 진짜 짜증나죠ㅠㅠ",
          "original_form": "헉 그럴 때 진짜 짜증나죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026407.1.12",
          "form": "저도 우산 안가져간날에 진짜 심장 쪼이면서 다녔어요",
          "original_form": "저도 우산 안가져간날에 진짜 심장 쪼이면서 다녔어요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026407.1.13",
          "form": "비올까봐ㅠㅠ",
          "original_form": "비올까봐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026407.1.14",
          "form": "근데 또 우산은 무거워서 맨날 들고다니기고 싫고 ㅠ",
          "original_form": "근데 또 우산은 무거워서 맨날 들고다니기고 싫고 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026407.1.15",
          "form": "지금도 비오는데 ㅠㅠㅠㅠ",
          "original_form": "지금도 비오는데 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026407.1.16",
          "form": "어유 ㅠㅠㅠㅠ",
          "original_form": "어유 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026407.1.17",
          "form": "ㅠㅜㅠ그러니까요",
          "original_form": "ㅠㅜㅠ그러니까요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026407.1.18",
          "form": "덥고 비오고 습하고 ㅠㅠㅠㅠ",
          "original_form": "덥고 비오고 습하고 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026407.1.19",
          "form": "우산도 짐이라고 그거 하나 더 느는데 귀칞아요",
          "original_form": "우산도 짐이라고 그거 하나 더 느는데 귀칞아요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026407.1.20",
          "form": "전 집에서 빗소리 듣는 정도만 딱 좋아요",
          "original_form": "전 집에서 빗소리 듣는 정도만 딱 좋아요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026407.1.21",
          "form": "근데 그건 유튜브로도 들을 수 있으니까...^^",
          "original_form": "근데 그건 유튜브로도 들을 수 있으니까...^^",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026407.1.22",
          "form": "맞아요 요즘",
          "original_form": "맞아요 요즘",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026407.1.23",
          "form": "비오는거 듣기좋은 유튭많더라구요",
          "original_form": "비오는거 듣기좋은 유튭많더라구요",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026407.1.24",
          "form": "막 여러개 들어봤어요",
          "original_form": "막 여러개 들어봤어요",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026407.1.25",
          "form": "오 전 자기 전에 빗소리 들으면서 많이 자거든요",
          "original_form": "오 전 자기 전에 빗소리 들으면서 많이 자거든요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026407.1.26",
          "form": "효과가 있나요?",
          "original_form": "효과가 있나요?",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026407.1.27",
          "form": "약간 이제는 파블로프의 개마냥",
          "original_form": "약간 이제는 파블로프의 개마냥",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026407.1.28",
          "form": "잠이 안오면 일단 냅다 틀어봐요",
          "original_form": "잠이 안오면 일단 냅다 틀어봐요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026407.1.29",
          "form": "내 뇌가 인지 부조화를 이뤄서 방전 돼버리게...",
          "original_form": "내 뇌가 인지 부조화를 이뤄서 방전 돼버리게...",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026407.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그게 효과가 있다니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그게 효과가 있다니",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026407.1.31",
          "form": "저도 해봐여겠네요",
          "original_form": "저도 해봐여겠네요",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026407.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ대박",
          "original_form": "ㅋㅋㅋㅋㅋㅋ대박",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026407.1.33",
          "form": "ㅋㅋㅋㅋㅋ한 번 해보세요",
          "original_form": "ㅋㅋㅋㅋㅋ한 번 해보세요",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026407.1.34",
          "form": "자기전에 그렇게 여러번 해보시다보면",
          "original_form": "자기전에 그렇게 여러번 해보시다보면",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026407.1.35",
          "form": "이제 틀어야 자는게 아니라 그냥 틀면 자게 되는 기적을 볼 수 있어요",
          "original_form": "이제 틀어야 자는게 아니라 그냥 틀면 자게 되는 기적을 볼 수 있어요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026407.1.36",
          "form": "워어어...",
          "original_form": "워어어...",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026407.1.37",
          "form": "한달 찐으로 해봅니다",
          "original_form": "한달 찐으로 해봅니다",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026407.1.38",
          "form": "주로 뭐 들으세요??",
          "original_form": "주로 뭐 들으세요??",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.39",
          "form": "비오는거??",
          "original_form": "비오는거??",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.40",
          "form": "어떤 유튭..?",
          "original_form": "어떤 유튭..?",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.41",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.42",
          "form": "전 크게 안가리거든요",
          "original_form": "전 크게 안가리거든요",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.43",
          "form": "길게 틀진 않고",
          "original_form": "길게 틀진 않고",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.44",
          "form": "짧으면 10분 짜리나 길면 30분 안으로 끝나는 영상들로 주로 봐요",
          "original_form": "짧으면 10분 짜리나 길면 30분 안으로 끝나는 영상들로 주로 봐요",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026407.1.45",
          "form": "그 사이에",
          "original_form": "그 사이에",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026407.1.46",
          "form": "주무실수있어요??",
          "original_form": "주무실수있어요??",
          "speaker_id": "2",
