        },
        {
          "id": "MDRW2100014031.1.17",
          "form": "오옹 알아갑니당",
          "original_form": "오옹 알아갑니당",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014031.1.18",
          "form": "저는 뭔가 개인 카페해보고싶어용",
          "original_form": "저는 뭔가 개인 카페해보고싶어용",
