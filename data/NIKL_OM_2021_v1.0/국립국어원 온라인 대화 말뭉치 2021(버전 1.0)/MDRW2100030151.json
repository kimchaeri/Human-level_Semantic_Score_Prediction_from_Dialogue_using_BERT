      "utterance": [
        {
          "id": "MDRW2100030151.1.1",
          "form": "근데 칼국수는 배 빨리 꺼지지 않나요",
          "original_form": "근데 칼국수는 배 빨리 꺼지지 않나요",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100030151.1.2",
          "form": "삶은계란만 드셨다니ㄷㄷ",
          "original_form": "삶은계란만 드셨다니ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100030151.1.3",
          "form": "칼국수 먹고 볶음밥까지 ㅎㅎ",
          "original_form": "칼국수 먹고 볶음밥까지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100030151.1.4",
          "form": "군대...는 잘 모르지만",
          "original_form": "군대...는 잘 모르지만",
          "speaker_id": "2",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100030151.1.5",
          "form": "아하ㅎ 그럼 얘기가 다르죠ㅎ",
          "original_form": "아하ㅎ 그럼 얘기가 다르죠ㅎ",
          "speaker_id": "1",
          "time": "20210904 20:22"
        },
        {
          "id": "MDRW2100030151.1.6",
          "form": "요새는 군대로도 엄마들이 전화를 많이 하신대요",
          "original_form": "요새는 군대로도 엄마들이 전화를 많이 하신대요",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100030151.1.7",
          "form": "아이고 군대로 전화라니",
          "original_form": "아이고 군대로 전화라니",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100030151.1.8",
          "form": "사단에 하는 걸까요ㄷㄷ",
          "original_form": "사단에 하는 걸까요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100030151.1.9",
          "form": "우리아들 양말 등등 잃어버렸다는 식으로",
          "original_form": "우리아들 양말 등등 잃어버렸다는 식으로",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100030151.1.10",
          "form": "아ㅎㅎㅎ무슨 유치원인가요",
          "original_form": "아ㅎㅎㅎ무슨 유치원인가요",
          "speaker_id": "1",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100030151.1.11",
          "form": "맙소사",
          "original_form": "맙소사",
          "speaker_id": "1",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100030151.1.12",
          "form": "대박이죠 저도 듣고 엄청 웃었어요",
          "original_form": "대박이죠 저도 듣고 엄청 웃었어요",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100030151.1.13",
          "form": "그런데 드문일은 아닌가보더라고요",
          "original_form": "그런데 드문일은 아닌가보더라고요",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100030151.1.14",
          "form": "옛날이랑 군대가 많이 달라져서 그런가ㄷㄷ",
          "original_form": "옛날이랑 군대가 많이 달라져서 그런가ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100030151.1.15",
          "form": "저 친구들 군대갔을땐 편지도 주고받았는데",
          "original_form": "저 친구들 군대갔을땐 편지도 주고받았는데",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100030151.1.16",
          "form": "그래도 그런 전화는 군대간 아들도 쪽팔릴 것 같은데ㅜ",
          "original_form": "그래도 그런 전화는 군대간 아들도 쪽팔릴 것 같은데ㅜ",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100030151.1.17",
          "form": "이제 홈피에 입력하면 출력해서 군인들에게 준다면서요",
          "original_form": "이제 홈피에 입력하면 출력해서 군인들에게 준다면서요",
          "speaker_id": "2",
          "time": "20210904 20:26"
        },
