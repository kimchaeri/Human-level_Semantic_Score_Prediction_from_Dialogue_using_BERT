      "utterance": [
        {
          "id": "MDRW2100023026.1.1",
          "form": "너무 뻔히 보이는 거짓말은 가끔 부작용도 일어나요ㅎㅎ",
          "original_form": "너무 뻔히 보이는 거짓말은 가끔 부작용도 일어나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023026.1.2",
          "form": "그래서 그 상무가 정직원을 뽑음요ㅎㅎㅎㅎㅎㅎ",
          "original_form": "그래서 그 상무가 정직원을 뽑음요ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023026.1.3",
          "form": "그쵸 스스로 자신을 깍아먹눈거죠",
          "original_form": "그쵸 스스로 자신을 깍아먹눈거죠",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023026.1.4",
          "form": "아무래도 바보같애요",
          "original_form": "아무래도 바보같애요",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100023026.1.5",
          "form": "저희회사가 여직원들한테는 엄청 좋은회사거든요",
          "original_form": "저희회사가 여직원들한테는 엄청 좋은회사거든요",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100023026.1.6",
          "form": "지금이야 다르지만",
          "original_form": "지금이야 다르지만",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100023026.1.7",
          "form": "정년보장될꺼에요",
          "original_form": "정년보장될꺼에요",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100023026.1.8",
          "form": "그런 회사가 최고 좋지요~~",
          "original_form": "그런 회사가 최고 좋지요~~",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023026.1.9",
          "form": "이제 오티 접속하고 올께요ㅎㅎ",
          "original_form": "이제 오티 접속하고 올께요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023026.1.10",
          "form": "떨리네....",
          "original_form": "떨리네....",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100023026.1.11",
          "form": "네넹 ㅋ 저는 배터리 나간지도 모르고ㅠㅋ",
          "original_form": "네넹 ㅋ 저는 배터리 나간지도 모르고ㅠㅋ",
          "speaker_id": "1",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100023026.1.12",
          "form": "오티 궁금하긴 하네요 ㅋ",
          "original_form": "오티 궁금하긴 하네요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100023026.1.13",
          "form": "오ㅎㅎ 멀티가되요ㅎㅎ",
          "original_form": "오ㅎㅎ 멀티가되요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100023026.1.14",
          "form": "보면서 심심이 할수있을것같애요ㅎ",
          "original_form": "보면서 심심이 할수있을것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100023026.1.15",
