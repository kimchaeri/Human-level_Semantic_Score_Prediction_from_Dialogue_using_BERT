      "utterance": [
        {
          "id": "MDRW2100008425.1.1",
          "form": "h쇼핑몰 사셨나요?",
          "original_form": "h쇼핑몰 사셨나요?",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008425.1.2",
          "form": "네 저는 4개아뒤중에 3개샀어요",
          "original_form": "네 저는 4개아뒤중에 3개샀어요",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008425.1.3",
          "form": "사셨어요?",
          "original_form": "사셨어요?",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008425.1.4",
          "form": "전 어제 타미 티셔츠샀어요",
          "original_form": "전 어제 타미 티셔츠샀어요",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008425.1.5",
          "form": "와~~~아뒤많으시네요",
          "original_form": "와~~~아뒤많으시네요",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008425.1.6",
          "form": "ㅎㅎㅎ 출첵하기힘들었어요 ㅋㅋ",
          "original_form": "ㅎㅎㅎ 출첵하기힘들었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008425.1.7",
          "form": "저도 타미 티셔츠하나사고\r\nDKNY 티셔츠하나 타미모자 하나샀네요",
          "original_form": "저도 타미 티셔츠하나사고\r\nDKNY 티셔츠하나 타미모자 하나샀네요",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008425.1.8",
          "form": "모자도 사고싶었는데 신랑껀 출석을 안해서 아쉽네요.",
          "original_form": "모자도 사고싶었는데 신랑껀 출석을 안해서 아쉽네요.",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008425.1.9",
          "form": "아까워요",
          "original_form": "아까워요",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008425.1.10",
          "form": "담엔 출석해야겠어요",
          "original_form": "담엔 출석해야겠어요",
          "speaker_id": "2",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008425.1.11",
          "form": "이럴때 이용해야하는곳이라 ㅎㅎ",
          "original_form": "이럴때 이용해야하는곳이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008425.1.12",
          "form": "아니면 너무 비싸잖아요",
          "original_form": "아니면 너무 비싸잖아요",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008425.1.13",
          "form": "타미를 저렴하게 살수있는 기회죠",
          "original_form": "타미를 저렴하게 살수있는 기회죠",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100008425.1.14",
          "form": "마자요. 별생각없었는데 어제 사고나니깐 이좋은걸 왜안했나싶어요",
          "original_form": "마자요. 별생각없었는데 어제 사고나니깐 이좋은걸 왜안했나싶어요",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008425.1.15",
          "form": "한개아뒤는 만근해서 사야겠어요",
          "original_form": "한개아뒤는 만근해서 사야겠어요",
          "speaker_id": "1",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008425.1.16",
          "form": "만근 화이팅!!",
          "original_form": "만근 화이팅!!",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008425.1.17",
          "form": "실결제있더라도 이렇게 많이 할인받고 사긴어렵죠",
          "original_form": "실결제있더라도 이렇게 많이 할인받고 사긴어렵죠",
