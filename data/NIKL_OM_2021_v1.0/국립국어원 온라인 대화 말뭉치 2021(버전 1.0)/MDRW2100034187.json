      "utterance": [
        {
          "id": "MDRW2100034187.1.1",
          "form": "풋사과 좋아 하세요?",
          "original_form": "풋사과 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100034187.1.2",
          "form": "전 좋아 하는데",
          "original_form": "전 좋아 하는데",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100034187.1.3",
          "form": "올해 잘못사서 푸석 하네요",
          "original_form": "올해 잘못사서 푸석 하네요",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100034187.1.4",
          "form": "전 좀 퍼석한 느낌이라 안 좋아해요 ㅠ 하지만 주면 먹어요",
          "original_form": "전 좀 퍼석한 느낌이라 안 좋아해요 ㅠ 하지만 주면 먹어요",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100034187.1.5",
          "form": "아 홍로는 새콤하고",
          "original_form": "아 홍로는 새콤하고",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100034187.1.6",
          "form": "아오리는 좀 푸석한 느낌이지 않나요? 그래서 전 빨간사과를 좀더 좋아해요",
          "original_form": "아오리는 좀 푸석한 느낌이지 않나요? 그래서 전 빨간사과를 좀더 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100034187.1.7",
          "form": "전 다 좋아해요",
          "original_form": "전 다 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100034187.1.8",
          "form": "그래도 아오리 좋아하는데 요번엔 완전 푸석",
          "original_form": "그래도 아오리 좋아하는데 요번엔 완전 푸석",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100034187.1.9",
          "form": "실패요",
          "original_form": "실패요",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100034187.1.10",
          "form": "배는 싫어해요 ㅡㅡ",
          "original_form": "배는 싫어해요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100034187.1.11",
          "form": "아 배는 좀 꺼끌하죠 그 감촉이 싫으신가요?",
          "original_form": "아 배는 좀 꺼끌하죠 그 감촉이 싫으신가요?",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100034187.1.12",
          "form": "딱히 맛을 모르겠어요",
          "original_form": "딱히 맛을 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.13",
          "form": "배는",
          "original_form": "배는",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.14",
          "form": "귤은 좋아해요",
          "original_form": "귤은 좋아해요",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.15",
          "form": "귤도 좋아하세요",
          "original_form": "귤도 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.16",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.17",
          "form": "배는 시원하고 서걱 거리는 맛이에요",
          "original_form": "배는 시원하고 서걱 거리는 맛이에요",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.18",
          "form": "귤도좋아해요 까기 편해요",
          "original_form": "귤도좋아해요 까기 편해요",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034187.1.20",
          "form": "맞아요 금방 까지고 전 조그마한귤이 맛나더라구요",
          "original_form": "맞아요 금방 까지고 전 조그마한귤이 맛나더라구요",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034187.1.21",
          "form": "하우스 귤 얘기신가요? 저도 좋아해요",
          "original_form": "하우스 귤 얘기신가요? 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034187.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034187.1.23",
          "form": "저아는애는 귤을 너무 많이 먹어서",
          "original_form": "저아는애는 귤을 너무 많이 먹어서",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034187.1.24",
          "form": "노래져서 병원간적 있대요 ㅋㅋ",
          "original_form": "노래져서 병원간적 있대요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034187.1.25",
          "form": "쌤이 절대 그렇게 한번에 먹지마라고 뭐라하더래요 ㅋㅋ",
          "original_form": "쌤이 절대 그렇게 한번에 먹지마라고 뭐라하더래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034187.1.26",
          "form": "과욕은 금물이죠",
          "original_form": "과욕은 금물이죠",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034187.1.27",
          "form": "근데 한박스는 금방 꼴딱 꼴닥 넘어가요",
          "original_form": "근데 한박스는 금방 꼴딱 꼴닥 넘어가요",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100034187.1.28",
          "form": "그렇죠 ㅋㅌ",
          "original_form": "그렇죠 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100034187.1.29",
          "form": "안씻어도 되고 안까도 되니 술술술",
          "original_form": "안씻어도 되고 안까도 되니 술술술",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100034187.1.30",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
