      "utterance": [
        {
          "id": "MDRW2100024448.1.1",
          "form": "쉴때 주로 뭐하세요?",
          "original_form": "쉴때 주로 뭐하세요?",
          "speaker_id": "2",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100024448.1.2",
          "form": "회사에서요? 아님 집에서요?",
          "original_form": "회사에서요? 아님 집에서요?",
          "speaker_id": "1",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100024448.1.3",
          "form": "폰겜할듯요 ㅋㅋㅋ둘다폰겜함",
          "original_form": "폰겜할듯요 ㅋㅋㅋ둘다폰겜함",
          "speaker_id": "1",
          "time": "20210831 10:09"
        },
        {
          "id": "MDRW2100024448.1.4",
          "form": "머하세요?",
          "original_form": "머하세요?",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100024448.1.5",
          "form": "요새 할 폰겜 없던데",
          "original_form": "요새 할 폰겜 없던데",
          "speaker_id": "2",
          "time": "20210831 10:12"
        },
        {
          "id": "MDRW2100024448.1.6",
          "form": "브롤스타즈라고,,,",
          "original_form": "브롤스타즈라고,,,",
          "speaker_id": "1",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100024448.1.7",
          "form": "초딩들좋아하는거있음ㅋㅋㅋㅋㅋ",
          "original_form": "초딩들좋아하는거있음ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.9",
          "form": "초딩들의 인싸게임",
          "original_form": "초딩들의 인싸게임",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.10",
          "form": "오 아네요?",
          "original_form": "오 아네요?",
          "speaker_id": "1",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.11",
          "form": "폰의 롤",
          "original_form": "폰의 롤",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.12",
          "form": "알죠",
          "original_form": "알죠",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.13",
          "form": "저 왕잘함",
          "original_form": "저 왕잘함",
          "speaker_id": "1",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.14",
          "form": "와 늙은이 취급하시네",
          "original_form": "와 늙은이 취급하시네",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.16",
          "form": "캐릭터다있음",
          "original_form": "캐릭터다있음",
          "speaker_id": "1",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.17",
          "form": "거기 총쏘는애 좀 봤어요 물풍선던졌던개",
          "original_form": "거기 총쏘는애 좀 봤어요 물풍선던졌던개",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.18",
          "form": "뚱땡이랑",
          "original_form": "뚱땡이랑",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.19",
          "form": "갑자기 날아서 짜부하던데",
          "original_form": "갑자기 날아서 짜부하던데",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024448.1.20",
          "form": "오 그래도 꽤 최근까지 아시네",
          "original_form": "오 그래도 꽤 최근까지 아시네",
          "speaker_id": "1",
          "time": "20210831 10:15"
        },
        {
          "id": "MDRW2100024448.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:15"
        },
        {
          "id": "MDRW2100024448.1.22",
          "form": "이거완전스트레스 ㅠ",
          "original_form": "이거완전스트레스 ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024448.1.23",
          "form": "이제 급,식안해서 점심 뭐먹을지 메뉴 맨날 생각해야댐",
          "original_form": "이제 급,식안해서 점심 뭐먹을지 메뉴 맨날 생각해야댐",
          "speaker_id": "1",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024448.1.24",
          "form": "오히려 좋은거 아닙니까",
          "original_form": "오히려 좋은거 아닙니까",
          "speaker_id": "2",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024448.1.25",
          "form": "다같이면 좀 스트레슨데",
          "original_form": "다같이면 좀 스트레슨데",
          "speaker_id": "2",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024448.1.26",
          "form": "혼자면 뭐",
          "original_form": "혼자면 뭐",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024448.1.27",
          "form": "왜좋아요 돈안들고좋앗는데 ㅋㅋㅋ갑자기 점심에 돈나감 ㅠ",
          "original_form": "왜좋아요 돈안들고좋앗는데 ㅋㅋㅋ갑자기 점심에 돈나감 ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024448.1.28",
          "form": "그래서 오늘도 편의점에서 샌드위치샀는데",
          "original_form": "그래서 오늘도 편의점에서 샌드위치샀는데",
          "speaker_id": "1",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024448.1.29",
          "form": "아침에가니까 삼각김밥도없드라구요 ㅡㅡ",
          "original_form": "아침에가니까 삼각김밥도없드라구요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024448.1.30",
          "form": "엥 돈 안주고",
          "original_form": "엥 돈 안주고",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024448.1.31",
          "form": "사먹능거네요?",
          "original_form": "사먹능거네요?",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024448.1.32",
          "form": "아 급,식이요?",
          "original_form": "아 급,식이요?",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024448.1.33",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024448.1.34",
          "form": "그냥먹엇죠 ㅋㅋㅋ애들밥인데 걍 낑겨서먹어요",
          "original_form": "그냥먹엇죠 ㅋㅋㅋ애들밥인데 걍 낑겨서먹어요",
          "speaker_id": "1",
          "time": "20210831 10:18"
        },
        {
          "id": "MDRW2100024448.1.35",
          "form": "아니아니 점심값 안줘요? 헐",
          "original_form": "아니아니 점심값 안줘요? 헐",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024448.1.36",
          "form": "보통 대주지않나 회사에서",
          "original_form": "보통 대주지않나 회사에서",
          "speaker_id": "2",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024448.1.37",
          "form": "네 쨋든 급식비 들어오는데 그걸로턱없이부족ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 쨋든 급식비 들어오는데 그걸로턱없이부족ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:22"
        },
        {
          "id": "MDRW2100024448.1.38",
          "form": "음 정부에서 지원해주는거군요",
          "original_form": "음 정부에서 지원해주는거군요",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024448.1.39",
          "form": "공무원들하고 수당똑같이들어옴 ㅋㅋㅋㅋ",
          "original_form": "공무원들하고 수당똑같이들어옴 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024448.1.40",
          "form": "가족수당",
          "original_form": "가족수당",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024448.1.41",
          "form": "밥값은 따로 없구요?",
          "original_form": "밥값은 따로 없구요?",
          "speaker_id": "2",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024448.1.42",
          "form": "제대로받으려면 빨리결혼해서 애많이낳아야댐",
          "original_form": "제대로받으려면 빨리결혼해서 애많이낳아야댐",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024448.1.43",
          "form": "넹없음 ㅠ",
          "original_form": "넹없음 ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:23"
        },
        {
          "id": "MDRW2100024448.1.44",
          "form": "수당이 사실 17만원들어와서 그안에서 해결하라 이소린데",
          "original_form": "수당이 사실 17만원들어와서 그안에서 해결하라 이소린데",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024448.1.45",
          "form": "솔직히 아깝자나요 ㅋㅋㅋㅋ",
          "original_form": "솔직히 아깝자나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024448.1.46",
          "form": "아깝죠",
          "original_form": "아깝죠",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024448.1.47",
          "form": "하 이게 참 난제임",
          "original_form": "하 이게 참 난제임",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024448.1.48",
          "form": "먹고살려고 돈버는데",
          "original_form": "먹고살려고 돈버는데",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024448.1.49",
          "form": "막상 돈벌면 식비 아끼는ㅋㅋㅋ",
          "original_form": "막상 돈벌면 식비 아끼는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024448.1.50",
          "form": "회사에서 쓰는돈은 너무아까움ㅋㅋㅋㅋㅋㅋ",
          "original_form": "회사에서 쓰는돈은 너무아까움ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
