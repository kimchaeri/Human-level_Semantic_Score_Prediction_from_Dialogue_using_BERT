      "utterance": [
        {
          "id": "MDRW2100023433.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023433.1.2",
          "form": "안녕하세요. 가족중에 종교에 심취한 분이 계시나여?",
          "original_form": "안녕하세요. 가족중에 종교에 심취한 분이 계시나여?",
          "speaker_id": "1",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023433.1.3",
          "form": "네 저희 언니요",
          "original_form": "네 저희 언니요",
          "speaker_id": "2",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023433.1.4",
          "form": "무슨종굔데여?",
          "original_form": "무슨종굔데여?",
          "speaker_id": "1",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023433.1.5",
          "form": "말하기 난감하네여",
          "original_form": "말하기 난감하네여",
          "speaker_id": "2",
          "time": "20210830 02:21"
        },
        {
          "id": "MDRW2100023433.1.6",
          "form": "그러시군요. 가족의 개인사니까 어쩔수없죠",
          "original_form": "그러시군요. 가족의 개인사니까 어쩔수없죠",
          "speaker_id": "1",
          "time": "20210830 02:21"
        },
        {
          "id": "MDRW2100023433.1.7",
          "form": "님네 가족엔있나요?",
          "original_form": "님네 가족엔있나요?",
          "speaker_id": "2",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023433.1.8",
          "form": "네.외할머니께서 기독교에 심취하셧다고하시더라고요.",
          "original_form": "네.외할머니께서 기독교에 심취하셧다고하시더라고요.",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023433.1.9",
          "form": "나이많으신 분은 신앙 있으시면 좋죠",
          "original_form": "나이많으신 분은 신앙 있으시면 좋죠",
          "speaker_id": "2",
          "time": "20210830 02:23"
        },
        {
          "id": "MDRW2100023433.1.10",
          "form": "네 맞아요.!!저희부모님은 무교신데 종교행사있어도 안가도되서 좋아요.",
          "original_form": "네 맞아요.!!저희부모님은 무교신데 종교행사있어도 안가도되서 좋아요.",
          "speaker_id": "1",
          "time": "20210830 02:24"
        },
        {
          "id": "MDRW2100023433.1.11",
          "form": "가면 사교성 길러지는건 좋지 않나요?",
          "original_form": "가면 사교성 길러지는건 좋지 않나요?",
          "speaker_id": "2",
          "time": "20210830 02:25"
        },
        {
          "id": "MDRW2100023433.1.12",
          "form": "그러면 name2도 한번 믿어보세여",
          "original_form": "그러면 &name2&도 한번 믿어보세여",
          "speaker_id": "1",
          "time": "20210830 02:26"
        },
        {
          "id": "MDRW2100023433.1.13",
          "form": "지금가기 부담스러워요",
          "original_form": "지금가기 부담스러워요",
