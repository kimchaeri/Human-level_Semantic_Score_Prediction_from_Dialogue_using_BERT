      "utterance": [
        {
          "id": "MDRW2100025667.1.1",
          "form": "나 어제 오징어샀어",
          "original_form": "나 어제 오징어샀어",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025667.1.2",
          "form": "오 웬 오징어?",
          "original_form": "오 웬 오징어?",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025667.1.3",
          "form": "반건조 오징어 있잖아 좋아해?",
          "original_form": "반건조 오징어 있잖아 좋아해?",
          "speaker_id": "2",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025667.1.4",
          "form": "엄청 좋아하지",
          "original_form": "엄청 좋아하지",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025667.1.5",
          "form": "나도 ㅠㅠ 근데 요즘 너무 비싸서",
          "original_form": "나도 ㅠㅠ 근데 요즘 너무 비싸서",
          "speaker_id": "2",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025667.1.6",
          "form": "요즘 마리당 얼마나 해?",
          "original_form": "요즘 마리당 얼마나 해?",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100025667.1.7",
          "form": "요즘 거의 한마리에 6~7천원",
          "original_form": "요즘 거의 한마리에 6~7천원",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025667.1.8",
          "form": "헐 엄청 비싸네ㅠㅠㅠ",
          "original_form": "헐 엄청 비싸네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025667.1.9",
          "form": "그치...저거도 좀 할인가야",
          "original_form": "그치...저거도 좀 할인가야",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025667.1.10",
          "form": "나는 오징어 회도 엄청 좋아해 언니는?",
          "original_form": "나는 오징어 회도 엄청 좋아해 언니는?",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025667.1.11",
          "form": "나도 좋아해 초장맛으로 먹는거긴 하지만ㅋㅋ",
          "original_form": "나도 좋아해 초장맛으로 먹는거긴 하지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025667.1.12",
          "form": "맞아ㅋㅋㅋ 거의 초장맛이지ㅋㅋ",
          "original_form": "맞아ㅋㅋㅋ 거의 초장맛이지ㅋㅋ",
