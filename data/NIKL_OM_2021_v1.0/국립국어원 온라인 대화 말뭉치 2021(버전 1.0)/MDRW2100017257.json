      "utterance": [
        {
          "id": "MDRW2100017257.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017257.1.2",
          "form": "안녕하세요~ 게임 좋아하시나요",
          "original_form": "안녕하세요~ 게임 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017257.1.3",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017257.1.4",
          "form": "어떤 게임 주로 하세요?",
          "original_form": "어떤 게임 주로 하세요?",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017257.1.5",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017257.1.6",
          "form": "오 저도 한때 했었는데 어렵던데 ㅋㅋ",
          "original_form": "오 저도 한때 했었는데 어렵던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017257.1.7",
          "form": "잘하시나요?",
          "original_form": "잘하시나요?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017257.1.8",
          "form": "음 즐겨합니닿ㅎ",
          "original_form": "음 즐겨합니닿ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017257.1.9",
          "form": "ㅋㅋ 즐겨하면 보통 잘하시더라구요",
          "original_form": "ㅋㅋ 즐겨하면 보통 잘하시더라구요",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017257.1.10",
          "form": "롤 같은 게임도 하시나요",
          "original_form": "롤 같은 게임도 하시나요",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017257.1.11",
          "form": "롤은 넘 어려워요ㅜ",
          "original_form": "롤은 넘 어려워요ㅜ",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017257.1.12",
          "form": "저도 ㅎㅎ 어려워서 항상 친구가 캐리해요 ㅋㅋ",
          "original_form": "저도 ㅎㅎ 어려워서 항상 친구가 캐리해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017257.1.13",
          "form": "얼마나 하셨나요",
          "original_form": "얼마나 하셨나요",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100017257.1.14",
          "form": "저 한 3년전에 처음한거같아요 !",
          "original_form": "저 한 3년전에 처음한거같아요 !",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100017257.1.15",
          "form": "지금도 못하지만 ㅋㅋ 그땐 진짜 욕먹었어요",
          "original_form": "지금도 못하지만 ㅋㅋ 그땐 진짜 욕먹었어요",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017257.1.16",
          "form": "와 오래되셨네요",
          "original_form": "와 오래되셨네요",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100017257.1.17",
          "form": "맞죠 ㅎㅎ 최근에도 하고 있어요",
          "original_form": "맞죠 ㅎㅎ 최근에도 하고 있어요",
          "speaker_id": "2",
          "time": "20210826 23:23"
        }
      ]
