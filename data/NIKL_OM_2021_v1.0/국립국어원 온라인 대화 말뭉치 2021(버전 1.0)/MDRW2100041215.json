      "utterance": [
        {
          "id": "MDRW2100041215.1.1",
          "form": "네ㅋㅋ 저녁 뭐 드셨나요?",
          "original_form": "네ㅋㅋ 저녁 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100041215.1.2",
          "form": "저 아직 안먹었어요!",
          "original_form": "저 아직 안먹었어요!",
          "speaker_id": "2",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100041215.1.3",
          "form": "아까 과자 먹었더니ㅋㅋㅋㅋ",
          "original_form": "아까 과자 먹었더니ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100041215.1.4",
          "form": "name1님은 뭐 드셨어요?",
          "original_form": "&name1&님은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100041215.1.5",
          "form": "ㅋㅋㅋ전 오늘 치킨이요!",
          "original_form": "ㅋㅋㅋ전 오늘 치킨이요!",
          "speaker_id": "1",
          "time": "20210905 20:27"
        },
        {
          "id": "MDRW2100041215.1.6",
          "form": "오! 치킨 좋아하세요?",
          "original_form": "오! 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100041215.1.7",
          "form": "네 좋아해요ㅋㅋ",
          "original_form": "네 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:32"
        },
        {
          "id": "MDRW2100041215.1.8",
          "form": "브랜드나 그냥 동네것도 잘 시켜먹어요",
          "original_form": "브랜드나 그냥 동네것도 잘 시켜먹어요",
          "speaker_id": "1",
          "time": "20210905 20:33"
        },
        {
          "id": "MDRW2100041215.1.9",
          "form": "헿  어디치킨 제일 선호하세요?",
          "original_form": "헿  어디치킨 제일 선호하세요?",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041215.1.10",
          "form": "전 요새 노랑통닭이요",
          "original_form": "전 요새 노랑통닭이요",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041215.1.11",
          "form": "알마치가 노랑통닭인가요?",
          "original_form": "알마치가 노랑통닭인가요?",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041215.1.12",
          "form": "네 마늘소스가 따로 와요",
          "original_form": "네 마늘소스가 따로 와요",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041215.1.13",
          "form": "전 튀김옷이 그냥 맛있어서 후라이드가 좋더라구요",
          "original_form": "전 튀김옷이 그냥 맛있어서 후라이드가 좋더라구요",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041215.1.14",
          "form": "노랑통닭에 그냥 후라이드요?",
          "original_form": "노랑통닭에 그냥 후라이드요?",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041215.1.15",
          "form": "저 지금 내일 시켜먹으려고 물어보는거엥욬ㅋㅋㅋ",
          "original_form": "저 지금 내일 시켜먹으려고 물어보는거엥욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041215.1.16",
          "form": "드셨다니까 갑자기 저두 먹고 싶어요",
          "original_form": "드셨다니까 갑자기 저두 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041215.1.17",
          "form": "ㅋㅋㅋㅋ그냥 마늘치킨 시키면 될거에요",
          "original_form": "ㅋㅋㅋㅋ그냥 마늘치킨 시키면 될거에요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041215.1.18",
          "form": "후라이드에 마늘소스가 따로 오는거더라구요",
          "original_form": "후라이드에 마늘소스가 따로 오는거더라구요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041215.1.19",
          "form": "아아!! ㅎㅎ 내일 시켜먹어야겠어요 감사합니당",
          "original_form": "아아!! ㅎㅎ 내일 시켜먹어야겠어요 감사합니당",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041215.1.20",
          "form": "네넹ㅋㅋ 여기 깐풍치킨도 인기있더라구요. 맛있기도 하구요ㅋㅋ",
          "original_form": "네넹ㅋㅋ 여기 깐풍치킨도 인기있더라구요. 맛있기도 하구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041215.1.21",
          "form": "일단 마늘치킨 정복 후에 깐풍치킨도 가볼게요!",
          "original_form": "일단 마늘치킨 정복 후에 깐풍치킨도 가볼게요!",
          "speaker_id": "2",
          "time": "20210905 20:44"
        }
      ]
