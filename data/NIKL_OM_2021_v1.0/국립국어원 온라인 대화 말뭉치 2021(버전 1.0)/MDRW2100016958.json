      "utterance": [
        {
          "id": "MDRW2100016958.1.1",
          "form": "야 name3 연락 온거 있어?",
          "original_form": "야 &name3& 연락 온거 있어?",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016958.1.2",
          "form": "어 아니 아직 나 지금 name4랑 name5이 만나러 체대가는 길인데 너랑 같이 미대에 있는거 아니야?",
          "original_form": "어 아니 아직 나 지금 &name4&랑 &name5&이 만나러 체대가는 길인데 너랑 같이 미대에 있는거 아니야?",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016958.1.3",
          "form": "너네 무슨 공모전 준비한다며",
          "original_form": "너네 무슨 공모전 준비한다며",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016958.1.4",
          "form": "아니 얘 아픈가??",
          "original_form": "아니 얘 아픈가??",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016958.1.5",
          "form": "연락이 없어",
          "original_form": "연락이 없어",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016958.1.6",
          "form": "전화해봐",
          "original_form": "전화해봐",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016958.1.7",
          "form": "점심때 목소리 안좋아서 좀 쉬고 저녁에 보기로 했는데",
          "original_form": "점심때 목소리 안좋아서 좀 쉬고 저녁에 보기로 했는데",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016958.1.8",
          "form": "내가 애들만나면 애들한테도 물어볼게",
          "original_form": "내가 애들만나면 애들한테도 물어볼게",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016958.1.9",
          "form": "자나보네",
          "original_form": "자나보네",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016958.1.10",
          "form": "ㄱㅊㄱㅊ 걍 찾아가볼게",
          "original_form": "ㄱㅊㄱㅊ 걍 찾아가볼게",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016958.1.11",
          "form": "알써 자취방가도 없음 연락 다 돌려봐야지...",
          "original_form": "알써 자취방가도 없음 연락 다 돌려봐야지...",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016958.1.12",
          "form": "어제 영통으로 회의하다 덥다더니 창문열고 잤나봐",
          "original_form": "어제 영통으로 회의하다 덥다더니 창문열고 잤나봐",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016958.1.13",
          "form": "아니 걔는 몸도 그닥 튼튼한거 아니면서",
          "original_form": "아니 걔는 몸도 그닥 튼튼한거 아니면서",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016958.1.14",
          "form": "왜그려",
          "original_form": "왜그려",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016958.1.15",
          "form": "아 이재 일어났다는데",
          "original_form": "아 이재 일어났다는데",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016958.1.16",
          "form": "방금 작작 전화하라고 하고 끊음",
          "original_form": "방금 작작 전화하라고 하고 끊음",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016958.1.17",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.18",
          "form": "나랑 애들도 전화걸었거든...",
          "original_form": "나랑 애들도 전화걸었거든...",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.19",
          "form": "하... 걍 걔네집에서 놀다 가야겠다",
          "original_form": "하... 걍 걔네집에서 놀다 가야겠다",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.20",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.21",
          "form": "너가 운이 나빴네",
          "original_form": "너가 운이 나빴네",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.23",
          "form": "엉엉 우리 먼저 고기 굽고 있을게",
          "original_form": "엉엉 우리 먼저 고기 굽고 있을게",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016958.1.24",
          "form": "name3 상태 보고 연락 할게",
          "original_form": "&name3& 상태 보고 연락 할게",
