      "utterance": [
        {
          "id": "MDRW2100046590.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046590.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046590.1.3",
          "form": "군대 다녀오셨나요?",
          "original_form": "군대 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046590.1.4",
          "form": "네 다녀왔습니다",
          "original_form": "네 다녀왔습니다",
          "speaker_id": "1",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046590.1.5",
          "form": "군대 계셨을 때",
          "original_form": "군대 계셨을 때",
          "speaker_id": "2",
          "time": "20211102 00:07"
        },
        {
          "id": "MDRW2100046590.1.6",
          "form": "밥은 잘나왔나용",
          "original_form": "밥은 잘나왔나용",
          "speaker_id": "2",
          "time": "20211102 00:07"
        },
        {
          "id": "MDRW2100046590.1.7",
          "form": "요즘 부실급식이니 뭐니 말이 많던데 ㅠㅠ",
          "original_form": "요즘 부실급식이니 뭐니 말이 많던데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211102 00:07"
        },
        {
          "id": "MDRW2100046590.1.8",
          "form": "기억으론 나쁘진 않았던것 같아요",
          "original_form": "기억으론 나쁘진 않았던것 같아요",
          "speaker_id": "1",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046590.1.9",
          "form": "워낙 식성이 좋기도 했고",
          "original_form": "워낙 식성이 좋기도 했고",
          "speaker_id": "1",
          "time": "20211102 00:08"
        },
        {
          "id": "MDRW2100046590.1.10",
          "form": "오오 그래도 다행이네요",
          "original_form": "오오 그래도 다행이네요",
          "speaker_id": "2",
          "time": "20211102 00:09"
        },
        {
          "id": "MDRW2100046590.1.11",
          "form": "제 친구들이 말하는거 들어보면",
          "original_form": "제 친구들이 말하는거 들어보면",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046590.1.12",
          "form": "부대 바이 부대라고",
          "original_form": "부대 바이 부대라고",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046590.1.13",
          "form": "해야되나 ㅋㅋㅋㅋ",
          "original_form": "해야되나 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046590.1.14",
          "form": "극과극이더라구요.....",
          "original_form": "극과극이더라구요.....",
          "speaker_id": "2",
          "time": "20211102 00:10"
        },
        {
          "id": "MDRW2100046590.1.15",
          "form": "네 맞습니다",
          "original_form": "네 맞습니다",
          "speaker_id": "1",
          "time": "20211102 00:32"
        },
        {
          "id": "MDRW2100046590.1.16",
          "form": "탈도 많고 말도 많은 곳이 군대니까요",
          "original_form": "탈도 많고 말도 많은 곳이 군대니까요",
          "speaker_id": "1",
          "time": "20211102 00:32"
        },
        {
          "id": "MDRW2100046590.1.17",
          "form": "그러게요 ㅠ",
          "original_form": "그러게요 ㅠ",
          "speaker_id": "2",
          "time": "20211101 13:21"
        },
        {
          "id": "MDRW2100046590.1.18",
          "form": "너무 폐쇄적이에요...군대는",
          "original_form": "너무 폐쇄적이에요...군대는",
          "speaker_id": "2",
          "time": "20211101 13:21"
        },
        {
          "id": "MDRW2100046590.1.19",
          "form": "군 특성상 어쩔수 없는 부분이긴 한것 같습니다",
          "original_form": "군 특성상 어쩔수 없는 부분이긴 한것 같습니다",
          "speaker_id": "1",
          "time": "20211101 13:36"
        },
        {
          "id": "MDRW2100046590.1.20",
          "form": "ㅎㅎㅠㅠ 그건 그렇죠",
          "original_form": "ㅎㅎㅠㅠ 그건 그렇죠",
          "speaker_id": "2",
          "time": "20211101 13:37"
        },
        {
          "id": "MDRW2100046590.1.21",
          "form": "군인들 대우가 좀 더 나아져야할텐데 말이에요...",
          "original_form": "군인들 대우가 좀 더 나아져야할텐데 말이에요...",
          "speaker_id": "2",
          "time": "20211101 13:37"
        },
        {
          "id": "MDRW2100046590.1.22",
          "form": "그래도 요즘 좀 나아져보이긴했는데 여전히 안좋은 사건사고가 많아서..",
          "original_form": "그래도 요즘 좀 나아져보이긴했는데 여전히 안좋은 사건사고가 많아서..",
          "speaker_id": "2",
          "time": "20211101 13:37"
        },
        {
          "id": "MDRW2100046590.1.23",
          "form": "군이 개혁과 혁신을 한다고는 하지만 징병제도 안에서 한계는 분명히 있을겁니다",
          "original_form": "군이 개혁과 혁신을 한다고는 하지만 징병제도 안에서 한계는 분명히 있을겁니다",
          "speaker_id": "1",
          "time": "20211101 13:43"
        },
        {
          "id": "MDRW2100046590.1.24",
          "form": "그럴 거 같네요 아무래도..ㅠㅠ 씁쓸하군요\r\n",
          "original_form": "그럴 거 같네요 아무래도..ㅠㅠ 씁쓸하군요\r\n",
          "speaker_id": "2",
          "time": "20211101 13:43"
        }
      ]
    }
  ]
