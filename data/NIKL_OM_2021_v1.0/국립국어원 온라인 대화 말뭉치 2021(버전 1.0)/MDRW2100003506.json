      "utterance": [
        {
          "id": "MDRW2100003506.1.1",
          "form": "건강은 어떠세요! ㅎㅎ",
          "original_form": "건강은 어떠세요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100003506.1.2",
          "form": "전 출산이후 몸이 급격하게 안좋아지는게 느껴져요",
          "original_form": "전 출산이후 몸이 급격하게 안좋아지는게 느껴져요",
          "speaker_id": "1",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003506.1.3",
          "form": "저도 첫째낳고 좀힘들다가",
          "original_form": "저도 첫째낳고 좀힘들다가",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003506.1.4",
          "form": "출산후에 엉망이 된거같아요ㅎ",
          "original_form": "출산후에 엉망이 된거같아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003506.1.5",
          "form": "그쵸ㅠㅠ 육아까지하니 몸이 돌아오질않네요",
          "original_form": "그쵸ㅠㅠ 육아까지하니 몸이 돌아오질않네요",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003506.1.6",
          "form": "ㅎㅎ그러게요ㅎㅎ둘째낳고나면 더 엉망될꺼같아요",
          "original_form": "ㅎㅎ그러게요ㅎㅎ둘째낳고나면 더 엉망될꺼같아요",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003506.1.7",
          "form": "나름 꾸미던 미혼시절이 그립기도해요",
          "original_form": "나름 꾸미던 미혼시절이 그립기도해요",
          "speaker_id": "1",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003506.1.8",
          "form": "그러게요~ 그때는 꾸미고 다니고 운동도 많이 했는데",
          "original_form": "그러게요~ 그때는 꾸미고 다니고 운동도 많이 했는데",
          "speaker_id": "2",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003506.1.9",
          "form": "애들신경쓰고 애들챙기느라 쉽지가않네요~~",
          "original_form": "애들신경쓰고 애들챙기느라 쉽지가않네요~~",
          "speaker_id": "1",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003506.1.10",
          "form": "지금은 애보는게 운동이 됬네요ㅎㅎ",
          "original_form": "지금은 애보는게 운동이 됬네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003506.1.11",
          "form": "그쵸 애보면서 운동하는거보면 너무 신기해요",
          "original_form": "그쵸 애보면서 운동하는거보면 너무 신기해요",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003506.1.12",
          "form": "유튜버들이나 연옌들이용ㅋ",
          "original_form": "유튜버들이나 연옌들이용ㅋ",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003506.1.13",
          "form": "체력이없으니 운동이 필수인데..ㅠ",
          "original_form": "체력이없으니 운동이 필수인데..ㅠ",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003506.1.14",
          "form": "요즘 정말 저질체력임을  실감하고있네요;",
          "original_form": "요즘 정말 저질체력임을  실감하고있네요;",
          "speaker_id": "1",
          "time": "20210821 00:17"
        },
        {
          "id": "MDRW2100003506.1.15",
          "form": "처녀시절엔 등산도 많이 가고는 했는데 나중에도 할수있을지 모르겠어요ㅎ",
          "original_form": "처녀시절엔 등산도 많이 가고는 했는데 나중에도 할수있을지 모르겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:19"
        },
        {
          "id": "MDRW2100003506.1.16",
          "form": "name2님은 당분간 어려우시겠지만",
          "original_form": "&name2&님은 당분간 어려우시겠지만",
          "speaker_id": "1",
          "time": "20210821 00:21"
        },
        {
          "id": "MDRW2100003506.1.17",
          "form": "그쳐. 당분간은 꼼짝마라에요",
          "original_form": "그쳐. 당분간은 꼼짝마라에요",
          "speaker_id": "2",
          "time": "20210821 00:21"
        },
        {
          "id": "MDRW2100003506.1.18",
          "form": "조금 키워놓고 꼭 추억삼아 다시 도전해보세용",
          "original_form": "조금 키워놓고 꼭 추억삼아 다시 도전해보세용",
          "speaker_id": "1",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.19",
          "form": "코로나때문에 등산 많이다니시더라구요",
          "original_form": "코로나때문에 등산 많이다니시더라구요",
          "speaker_id": "1",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.20",
          "form": "네ㅎ 조금만 키우고 운동 다시해야조ㅡ",
          "original_form": "네ㅎ 조금만 키우고 운동 다시해야조ㅡ",
          "speaker_id": "2",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.21",
          "form": "전 홈트한다고 유튜브 많이봤는데",
          "original_form": "전 홈트한다고 유튜브 많이봤는데",
          "speaker_id": "1",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.22",
          "form": "여자의 평생숙제 다요트",
          "original_form": "여자의 평생숙제 다요트",
          "speaker_id": "2",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.23",
          "form": "엄~~청 힘들더라구요^^;;ㅠㅠ",
          "original_form": "엄~~청 힘들더라구요^^;;ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.24",
          "form": "피부도 엉망이라 레이져도 하고싶구요ㅎㅎ",
          "original_form": "피부도 엉망이라 레이져도 하고싶구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003506.1.25",
          "form": "저도 피부과 너무 가고싶네요 ㅠㅠㅠ",
          "original_form": "저도 피부과 너무 가고싶네요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100003506.1.26",
          "form": "홈트는 진짜 맘먹고하기 어려운거같아요ㅎㅎ",
          "original_form": "홈트는 진짜 맘먹고하기 어려운거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100003506.1.27",
          "form": "기미가 막ㅜㅜ",
          "original_form": "기미가 막ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003506.1.28",
          "form": "맞아요 홈트는 포기가 쉬워요 ㅋㅋㄱㅋ ㅠㅠ",
          "original_form": "맞아요 홈트는 포기가 쉬워요 ㅋㅋㄱㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003506.1.29",
          "form": "기미 저도..기미 주근깨 때문에 ㅠㅠ",
          "original_form": "기미 저도..기미 주근깨 때문에 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003506.1.30",
          "form": "포기하면 편하죠ㅎ",
          "original_form": "포기하면 편하죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003506.1.31",
          "form": "애가지니 심해졌어요 더ㅎ",
          "original_form": "애가지니 심해졌어요 더ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003506.1.32",
          "form": "저는 애낳고나서 애본다고 저한테 너무 신경을 못썼더니 많이 심해졌어요ㅠㅠ",
          "original_form": "저는 애낳고나서 애본다고 저한테 너무 신경을 못썼더니 많이 심해졌어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003506.1.33",
          "form": "피부를 싹 뒤집고싶네요!!",
          "original_form": "피부를 싹 뒤집고싶네요!!",
          "speaker_id": "2",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003506.1.34",
          "form": "그럴수만있다면ㅋㅋㅋ저도 함께해요~ㅋㅋ",
          "original_form": "그럴수만있다면ㅋㅋㅋ저도 함께해요~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003506.1.35",
          "form": "저도 저희어머니가 너 얼굴도 좀 관리하라고ㅋㅋ",
          "original_form": "저도 저희어머니가 너 얼굴도 좀 관리하라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003506.1.36",
          "form": "어머어머 완전 똑같네요ㅜ",
          "original_form": "어머어머 완전 똑같네요ㅜ",
          "speaker_id": "1",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003506.1.37",
          "form": "돈좀 쓰라고 하십니다ㅋㅋ",
          "original_form": "돈좀 쓰라고 하십니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003506.1.38",
          "form": "엄마는 딸이 그러니 속상하신가봐요",
          "original_form": "엄마는 딸이 그러니 속상하신가봐요",
          "speaker_id": "1",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003506.1.39",
