      "utterance": [
        {
          "id": "MDRW2100013345.1.1",
          "form": "오늘 지에스 짱이지?",
          "original_form": "오늘 지에스 짱이지?",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100013345.1.2",
          "form": "대박이야~~~",
          "original_form": "대박이야~~~",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100013345.1.3",
          "form": "그래.  물건공짜로사니 좋네",
          "original_form": "그래.  물건공짜로사니 좋네",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100013345.1.4",
          "form": "ㅎㅎ 아까 픽업가줘서 고마워~",
          "original_form": "ㅎㅎ 아까 픽업가줘서 고마워~",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100013345.1.5",
          "form": "그정도는 해줘야지",
          "original_form": "그정도는 해줘야지",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100013345.1.6",
          "form": "말일까진데 쿠폰 사라졌다는 사람이있더라구",
          "original_form": "말일까진데 쿠폰 사라졌다는 사람이있더라구",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013345.1.7",
          "form": "난아직 있어",
          "original_form": "난아직 있어",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013345.1.8",
          "form": "그래? 사라졌어?",
          "original_form": "그래? 사라졌어?",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013345.1.9",
          "form": "응. 나도 담에 한번 더 사용하면 없어질꺼같아",
          "original_form": "응. 나도 담에 한번 더 사용하면 없어질꺼같아",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013345.1.10",
          "form": "한번만이라 아쉽네",
          "original_form": "한번만이라 아쉽네",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013345.1.11",
          "form": "그러게.  과자랑 아직 더 사야하는데...",
          "original_form": "그러게.  과자랑 아직 더 사야하는데...",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100013345.1.12",
          "form": "아쉬워하지마.  괜히 싸다고 사서 살만찐다",
          "original_form": "아쉬워하지마.  괜히 싸다고 사서 살만찐다",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100013345.1.13",
          "form": "그건 맞아.  먹을께 넘많다. 냉장고도 터지고ㅠ",
          "original_form": "그건 맞아.  먹을께 넘많다. 냉장고도 터지고ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100013345.1.14",
          "form": "그래 냉장고좀 비우고 나중에사.",
          "original_form": "그래 냉장고좀 비우고 나중에사.",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100013345.1.15",
          "form": "채소 다 썩고있다",
          "original_form": "채소 다 썩고있다",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100013345.1.16",
          "form": "ㅋㅋ 그러게 요즘 넘 음식을 안했네 시켜먹고하느라 ㅡ",
          "original_form": "ㅋㅋ 그러게 요즘 넘 음식을 안했네 시켜먹고하느라 ㅡ",
          "speaker_id": "1",
          "time": "20210827 22:15"
        }
      ]
