        {
          "id": "MDRW2100034039.1.1",
          "form": "저 지금 바라만 본다 ㅋㅋ들었네요",
          "original_form": "저 지금 바라만 본다 ㅋㅋ들었네요",
          "speaker_id": "2",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100034039.1.2",
          "form": "전 아직도",
          "original_form": "전 아직도",
          "speaker_id": "2",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100034039.1.3",
          "form": "놀면 뭐하니에서 못 벗어 났어요 ㅋㅋ",
          "original_form": "놀면 뭐하니에서 못 벗어 났어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100034039.1.4",
          "form": "ㅋㅋㅋ그게 은근 오래가요",
          "original_form": "ㅋㅋㅋ그게 은근 오래가요",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100034039.1.5",
          "form": "귓가에도 맴돌고",
          "original_form": "귓가에도 맴돌고",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100034039.1.6",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100034039.1.7",
          "form": "너무 잘불러주셔서",
          "original_form": "너무 잘불러주셔서",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100034039.1.8",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100034039.1.9",
          "form": "어쩜그리 찰떡인지",
          "original_form": "어쩜그리 찰떡인지",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100034039.1.10",
          "form": "그렇죠 유재석 진짜 잘뽑아요",
          "original_form": "그렇죠 유재석 진짜 잘뽑아요",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100034039.1.11",
          "form": "노래도 매번 최고입니다",
          "original_form": "노래도 매번 최고입니다",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100034039.1.12",
          "form": "싹쓰리 지금도 들어요",
          "original_form": "싹쓰리 지금도 들어요",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100034039.1.13",
          "form": "저도요ㅋㅋ 여름 지나도 비오니 생각나요",
          "original_form": "저도요ㅋㅋ 여름 지나도 비오니 생각나요",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034039.1.14",
          "form": "그렇죠 꼭 여름 아니어도 좋아요",
          "original_form": "그렇죠 꼭 여름 아니어도 좋아요",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100034039.1.15",
          "form": "담에 겨울쯤 msg 다시 활동하면 좋겠어요",
          "original_form": "담에 겨울쯤 msg 다시 활동하면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100034039.1.16",
          "form": "이대로 끝나지 않음 좋겠어요 ㅎㅎ",
          "original_form": "이대로 끝나지 않음 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100034039.1.17",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100034039.1.18",
          "form": "한번만으론 아까워요",
          "original_form": "한번만으론 아까워요",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100034039.1.19",
          "form": "그렇죠 다시 결합 꼭해주지",
          "original_form": "그렇죠 다시 결합 꼭해주지",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100034039.1.20",
          "form": "여름마다 하는건 아쉬워요 ㅎㅎ",
          "original_form": "여름마다 하는건 아쉬워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100034039.1.21",
          "form": "넷플에서 잼난거 있나요?",
          "original_form": "넷플에서 잼난거 있나요?",
          "speaker_id": "2",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100034039.1.22",
          "form": "전 왓챠는 애니 보려고 ㅋㅋ",
          "original_form": "전 왓챠는 애니 보려고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100034039.1.23",
          "form": "팝콘 샀어요",
          "original_form": "팝콘 샀어요",
