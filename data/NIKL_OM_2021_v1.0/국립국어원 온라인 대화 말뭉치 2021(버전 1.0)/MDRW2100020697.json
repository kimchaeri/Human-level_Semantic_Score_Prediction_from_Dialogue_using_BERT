      "utterance": [
        {
          "id": "MDRW2100020697.1.1",
          "form": "오 닙은 몇호 끼세요 ?",
          "original_form": "오 닙은 몇호 끼세요 ?",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020697.1.2",
          "form": "재보셨어요 ?",
          "original_form": "재보셨어요 ?",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020697.1.3",
          "form": "검지는 12호일걸요?",
          "original_form": "검지는 12호일걸요?",
          "speaker_id": "1",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020697.1.4",
          "form": "오 중지는요 ?",
          "original_form": "오 중지는요 ?",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020697.1.5",
          "form": "근데 검지에 끼면 안불편하세요 ?",
          "original_form": "근데 검지에 끼면 안불편하세요 ?",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020697.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020697.1.7",
          "form": "연필 잡으셔야하잖아요",
          "original_form": "연필 잡으셔야하잖아요",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020697.1.8",
          "form": "중지도 비슷할거예요",
          "original_form": "중지도 비슷할거예요",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020697.1.9",
          "form": "전 검지는 불편해서 못끼겠더라구요",
          "original_form": "전 검지는 불편해서 못끼겠더라구요",
          "speaker_id": "2",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020697.1.10",
          "form": "연필 잡을 일이 거의 없어요",
          "original_form": "연필 잡을 일이 거의 없어요",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020697.1.11",
          "form": "학생아니세요 ?",
          "original_form": "학생아니세요 ?",
          "speaker_id": "2",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020697.1.12",
          "form": "근데 약지나 소지에는 맞는 게 없어요 안 팔아요",
          "original_form": "근데 약지나 소지에는 맞는 게 없어요 안 팔아요",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020697.1.13",
          "form": "맞는데요",
          "original_form": "맞는데요",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020697.1.14",
          "form": "약지 소지는 호수가 작으신가봐요",
          "original_form": "약지 소지는 호수가 작으신가봐요",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020697.1.15",
          "form": "아 그럼 연필을 많이 안쓰시는군요",
          "original_form": "아 그럼 연필을 많이 안쓰시는군요",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020697.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020697.1.17",
          "form": "님은 많이 쓰시나요",
          "original_form": "님은 많이 쓰시나요",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020697.1.18",
          "form": "네 저는 완전 이론 쪽 전공이라서요",
          "original_form": "네 저는 완전 이론 쪽 전공이라서요",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020697.1.19",
          "form": "저 최근에 잠바 샀는데 환불 받아야해요",
          "original_form": "저 최근에 잠바 샀는데 환불 받아야해요",
