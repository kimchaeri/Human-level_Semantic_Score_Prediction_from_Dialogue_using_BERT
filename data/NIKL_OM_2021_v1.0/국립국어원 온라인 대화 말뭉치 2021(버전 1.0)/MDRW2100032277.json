      "utterance": [
        {
          "id": "MDRW2100032277.1.1",
          "form": "점심먹었어?",
          "original_form": "점심먹었어?",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032277.1.2",
          "form": "먹었어 너는?",
          "original_form": "먹었어 너는?",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032277.1.3",
          "form": "먹었지 뭐먹었는데?",
          "original_form": "먹었지 뭐먹었는데?",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032277.1.4",
          "form": "나는 컵라면 먹었음",
          "original_form": "나는 컵라면 먹었음",
          "speaker_id": "2",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032277.1.5",
          "form": "나는 계란 토스트 해먹었다",
          "original_form": "나는 계란 토스트 해먹었다",
          "speaker_id": "1",
          "time": "20210905 14:51"
        },
        {
          "id": "MDRW2100032277.1.6",
          "form": "역시 주말점심은 간단하게!",
          "original_form": "역시 주말점심은 간단하게!",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100032277.1.7",
          "form": "대충먹는거지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대충먹는거지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032277.1.8",
          "form": "컵라면 뭐먹었는데?",
          "original_form": "컵라면 뭐먹었는데?",
          "speaker_id": "1",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032277.1.9",
          "form": "육개장 소컵",
          "original_form": "육개장 소컵",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032277.1.10",
          "form": "맛잘알이네",
          "original_form": "맛잘알이네",
          "speaker_id": "1",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032277.1.11",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100032277.1.12",
          "form": "너 미원컵라면 먹어봄?",
          "original_form": "너 미원컵라면 먹어봄?",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032277.1.13",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032277.1.14",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "1",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032277.1.15",
          "form": "이게 컵라면이지 싶어",
          "original_form": "이게 컵라면이지 싶어",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032277.1.16",
          "form": "살짝맵고 조미료맛 강하고",
          "original_form": "살짝맵고 조미료맛 강하고",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032277.1.17",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "2",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032277.1.18",
          "form": "먹어봐야겠다",
          "original_form": "먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032277.1.19",
          "form": "조미료맛 좋지",
          "original_form": "조미료맛 좋지",
          "speaker_id": "1",
