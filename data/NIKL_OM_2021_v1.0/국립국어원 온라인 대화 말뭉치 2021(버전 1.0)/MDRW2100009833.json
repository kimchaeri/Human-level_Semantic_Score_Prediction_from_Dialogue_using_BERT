      "utterance": [
        {
          "id": "MDRW2100009833.1.1",
          "form": "국내에 좋은데 어디있나요",
          "original_form": "국내에 좋은데 어디있나요",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009833.1.2",
          "form": "저는 속초도 좋았어요ㅎㅎ",
          "original_form": "저는 속초도 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009833.1.3",
          "form": "속초 바다 좋죠",
          "original_form": "속초 바다 좋죠",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009833.1.4",
          "form": "아부지고향이에요",
          "original_form": "아부지고향이에요",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009833.1.5",
          "form": "건어물비싸더라구요",
          "original_form": "건어물비싸더라구요",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009833.1.6",
          "form": "우와 나중에 속초사는게 꿈이에요ㅎㅎ한적하고 바다가 깨끗하더라구요",
          "original_form": "우와 나중에 속초사는게 꿈이에요ㅎㅎ한적하고 바다가 깨끗하더라구요",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009833.1.7",
          "form": "오징어ㅎㅎ 좋아하는데 진짜 비싸요",
          "original_form": "오징어ㅎㅎ 좋아하는데 진짜 비싸요",
          "speaker_id": "2",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009833.1.8",
          "form": "오징어 좋아해요",
          "original_form": "오징어 좋아해요",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009833.1.9",
          "form": "비싸요ㅜ 말린거 잘먹는데",
          "original_form": "비싸요ㅜ 말린거 잘먹는데",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009833.1.10",
          "form": "맞아요그리고 산지에서 파는게 좋다구 해서 사는데 깜짝 놀랐네요ㅠㅠㅋㅋ",
          "original_form": "맞아요그리고 산지에서 파는게 좋다구 해서 사는데 깜짝 놀랐네요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100009833.1.11",
          "form": "임은하푸드인가 거기서 해산물 싸게팔아요",
          "original_form": "임은하푸드인가 거기서 해산물 싸게팔아요",
          "speaker_id": "1",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100009833.1.12",
          "form": "군산에 가보고싶어요",
          "original_form": "군산에 가보고싶어요",
          "speaker_id": "1",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100009833.1.13",
          "form": "저도 여수가보고싶어요ㅎㅎ",
          "original_form": "저도 여수가보고싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009833.1.14",
          "form": "거긴 해산물이 싸겠지요",
          "original_form": "거긴 해산물이 싸겠지요",
          "speaker_id": "1",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009833.1.15",
          "form": "여수~~",
          "original_form": "여수~~",
          "speaker_id": "1",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009833.1.16",
          "form": "해수욕장 좋아요",
          "original_form": "해수욕장 좋아요",
          "speaker_id": "1",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009833.1.17",
          "form": "ㅎㅎ근데 다녀봐도 저렴한곳은 없는거같아요 다 싯가ㅋㅋ",
          "original_form": "ㅎㅎ근데 다녀봐도 저렴한곳은 없는거같아요 다 싯가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009833.1.18",
          "form": "여수 가보셨어요?",
          "original_form": "여수 가보셨어요?",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009833.1.19",
          "form": "ㅜㅜ 이제 없나봐요 시가가올라서",
          "original_form": "ㅜㅜ 이제 없나봐요 시가가올라서",
          "speaker_id": "1",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009833.1.20",
          "form": "여수 가물가물하네요",
          "original_form": "여수 가물가물하네요",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009833.1.21",
          "form": "여름에 갔어야하는데..ㅠㅠ",
          "original_form": "여름에 갔어야하는데..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 11:56"
        }
      ]
