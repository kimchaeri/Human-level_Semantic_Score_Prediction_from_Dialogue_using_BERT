      "utterance": [
        {
          "id": "MDRW2100024584.1.1",
          "form": "갯마을 차차차 넷플로봐야겠어ㅋㅋ",
          "original_form": "갯마을 차차차 넷플로봐야겠어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024584.1.2",
          "form": "나그거 다봤어",
          "original_form": "나그거 다봤어",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024584.1.3",
          "form": "완전 유치하고 뻔하고 옛날느낌인데",
          "original_form": "완전 유치하고 뻔하고 옛날느낌인데",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024584.1.4",
          "form": "신민아 김선호 비주얼이 살림",
          "original_form": "신민아 김선호 비주얼이 살림",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024584.1.5",
          "form": "아그래???",
          "original_form": "아그래???",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024584.1.6",
          "form": "누구는 동백꽃생각난다더라고ㅋㅋㅋㅋ",
          "original_form": "누구는 동백꽃생각난다더라고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024584.1.7",
          "form": "아 느낌있긴함 근데 동백이는 사회성은 있지않았나",
          "original_form": "아 느낌있긴함 근데 동백이는 사회성은 있지않았나",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024584.1.8",
          "form": "신민아 캐릭터는 사회성이 너무낮아",
          "original_form": "신민아 캐릭터는 사회성이 너무낮아",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024584.1.9",
          "form": "아그래??",
          "original_form": "아그래??",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024584.1.10",
          "form": "근데 신민아랑 김선호 케미는 좋은듯",
          "original_form": "근데 신민아랑 김선호 케미는 좋은듯",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024584.1.11",
          "form": "이게 원작이 옛날거라 더 옛스러움",
          "original_form": "이게 원작이 옛날거라 더 옛스러움",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100024584.1.12",
          "form": "아쉬운게 남여가 바꼈으면 재밌었을것같고 아니면 남주를 연하로 설정했으면 존잼일듯",
          "original_form": "아쉬운게 남여가 바꼈으면 재밌었을것같고 아니면 남주를 연하로 설정했으면 존잼일듯",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100024584.1.13",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.14",
          "form": "나는 홍천기 이것도 기대중인데",
          "original_form": "나는 홍천기 이것도 기대중인데",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.15",
          "form": "아 남주오바",
          "original_form": "아 남주오바",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.16",
          "form": "이번주 첫방해서 반응보고 보려고ㅋㅋㅋ",
          "original_form": "이번주 첫방해서 반응보고 보려고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.17",
          "form": "유정이는 배우복이 왜이러나",
          "original_form": "유정이는 배우복이 왜이러나",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.18",
          "form": "안효섭이지?ㅋㅋㅋㅋ",
          "original_form": "안효섭이지?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.19",
          "form": "어... 못.생.",
          "original_form": "어... 못.생.",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024584.1.20",
          "form": "그니까 근데 홍천기 유정이 타이틀롤인데",
          "original_form": "그니까 근데 홍천기 유정이 타이틀롤인데",
          "speaker_id": "2",
          "time": "20210831 22:52"
        }
