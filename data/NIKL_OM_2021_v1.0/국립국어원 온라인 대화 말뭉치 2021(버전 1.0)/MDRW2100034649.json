      "utterance": [
        {
          "id": "MDRW2100034649.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100034649.1.2",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034649.1.3",
          "form": "로제떡볶이 좋아하세요?",
          "original_form": "로제떡볶이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034649.1.4",
          "form": "너무 좋아하죠",
          "original_form": "너무 좋아하죠",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034649.1.5",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034649.1.6",
          "form": "넘 좋져 ㅠㅠ",
          "original_form": "넘 좋져 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034649.1.7",
          "form": "칼로리가 사악하지만..",
          "original_form": "칼로리가 사악하지만..",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034649.1.8",
          "form": "어디깨좋으새요?",
          "original_form": "어디깨좋으새요?",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034649.1.9",
          "form": "저는 신전이요!",
          "original_form": "저는 신전이요!",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034649.1.10",
          "form": "양이 적당하고 꾸덕해서 조항요",
          "original_form": "양이 적당하고 꾸덕해서 조항요",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034649.1.11",
          "form": "좋아요 ㅋㅋㅋ",
          "original_form": "좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100034649.1.12",
          "form": "태리 로제떡볶이 드셔보셨어요?",
          "original_form": "태리 로제떡볶이 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034649.1.13",
          "form": "아니요 ㅜㅜ",
          "original_form": "아니요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034649.1.14",
          "form": "그거 완전 맛있대요 ㅠㅠ",
          "original_form": "그거 완전 맛있대요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100034649.1.15",
          "form": "와 소문이 좋네요 거기는",
          "original_form": "와 소문이 좋네요 거기는",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100034649.1.16",
          "form": "네 완전 꾸덕꾸덕 맛난대요",
          "original_form": "네 완전 꾸덕꾸덕 맛난대요",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034649.1.17",
          "form": "근데 신전도 대박니죠",
          "original_form": "근데 신전도 대박니죠",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034649.1.18",
          "form": "소스가 진짜..",
          "original_form": "소스가 진짜..",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034649.1.19",
          "form": "맞아요로제는",
          "original_form": "맞아요로제는",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034649.1.20",
          "form": "꾸덕이 생명이죠..!!",
          "original_form": "꾸덕이 생명이죠..!!",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034649.1.21",
          "form": "엽떡은 별로 안좋아하세여?",
          "original_form": "엽떡은 별로 안좋아하세여?",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034649.1.22",
          "form": "오리지널만 먹어보고 로제는 아직요!",
          "original_form": "오리지널만 먹어보고 로제는 아직요!",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034649.1.23",
          "form": "맛나나요?",
          "original_form": "맛나나요?",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034649.1.24",
          "form": "전 너무 좋았어요 ㅜㅜ",
          "original_form": "전 너무 좋았어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034649.1.25",
          "form": "치즈 안섞어먹어야 제맛..",
          "original_form": "치즈 안섞어먹어야 제맛..",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034649.1.26",
          "form": "아 격공합니다",
          "original_form": "아 격공합니다",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034649.1.27",
          "form": "전 거긴 안먹어봤지만 치즈는 안섞는게 좋아요",
          "original_form": "전 거긴 안먹어봤지만 치즈는 안섞는게 좋아요",
          "speaker_id": "1",
          "time": "20210828 17:21"
        }
      ]
    }
