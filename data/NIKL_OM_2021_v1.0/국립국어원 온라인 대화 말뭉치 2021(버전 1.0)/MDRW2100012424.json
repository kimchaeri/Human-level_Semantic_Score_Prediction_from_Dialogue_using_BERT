      "utterance": [
        {
          "id": "MDRW2100012424.1.1",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012424.1.2",
          "form": "하는건 안좋아하고 먹는거 좋아해요",
          "original_form": "하는건 안좋아하고 먹는거 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012424.1.3",
          "form": "현실은 제가 다하죠ㅠ",
          "original_form": "현실은 제가 다하죠ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012424.1.4",
          "form": "어떤 요리가 자신있으세요?",
          "original_form": "어떤 요리가 자신있으세요?",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012424.1.5",
          "form": "부추전?같은?",
          "original_form": "부추전?같은?",
          "speaker_id": "2",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012424.1.6",
          "form": "얇게 잘구워요ㅋㅋ",
          "original_form": "얇게 잘구워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012424.1.7",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012424.1.8",
          "form": "부추전 맛있죠",
          "original_form": "부추전 맛있죠",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012424.1.9",
          "form": "노하우좀 알려주세요!",
          "original_form": "노하우좀 알려주세요!",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012424.1.10",
          "form": "팬에 반죽을 조금만 넣고 얇게 잘 펴야죠~",
          "original_form": "팬에 반죽을 조금만 넣고 얇게 잘 펴야죠~",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012424.1.11",
          "form": "얇게가 어렵더라고요",
          "original_form": "얇게가 어렵더라고요",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012424.1.12",
          "form": "긁듯이 옆으로 옆으로",
          "original_form": "긁듯이 옆으로 옆으로",
          "speaker_id": "2",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012424.1.13",
          "form": "저는 냄비밥 잘해요 ㅋㅋ",
          "original_form": "저는 냄비밥 잘해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012424.1.14",
          "form": "전 그게 더어려운것 같은데ㅋ한번도 안해봤어요",
          "original_form": "전 그게 더어려운것 같은데ㅋ한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012424.1.15",
          "form": "익숙해지면 쉬워요",
          "original_form": "익숙해지면 쉬워요",
          "speaker_id": "1",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012424.1.16",
          "form": "신랑도 잘하더라구요ㅋㅋ",
          "original_form": "신랑도 잘하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012424.1.17",
          "form": "맛집이나 카페 다니시나요?",
          "original_form": "맛집이나 카페 다니시나요?",
          "speaker_id": "1",
          "time": "20210824 22:16"
        }
      ]
