      "utterance": [
        {
          "id": "MDRW2100039251.1.1",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100039251.1.2",
          "form": "양파 튀김 먹고 싶은데",
          "original_form": "양파 튀김 먹고 싶은데",
          "speaker_id": "2",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039251.1.3",
          "form": "먹고 싶은 거도 많다",
          "original_form": "먹고 싶은 거도 많다",
          "speaker_id": "1",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039251.1.4",
          "form": "양파 튀김 해 먹을까?",
          "original_form": "양파 튀김 해 먹을까?",
          "speaker_id": "2",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039251.1.5",
          "form": "두세개만 함 될거 가튼데",
          "original_form": "두세개만 함 될거 가튼데",
          "speaker_id": "2",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039251.1.6",
          "form": "야 집 난리나서 안돼",
          "original_form": "야 집 난리나서 안돼",
          "speaker_id": "1",
          "time": "20210903 15:08"
        },
        {
          "id": "MDRW2100039251.1.7",
          "form": "아 왜 에프에 돌림되지",
          "original_form": "아 왜 에프에 돌림되지",
          "speaker_id": "2",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100039251.1.8",
          "form": "기름에 해야 되잖아",
          "original_form": "기름에 해야 되잖아",
          "speaker_id": "1",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100039251.1.9",
          "form": "기름 살짝 발라서 에프에 돌림 안되나?",
          "original_form": "기름 살짝 발라서 에프에 돌림 안되나?",
          "speaker_id": "2",
          "time": "20210903 15:09"
        },
        {
          "id": "MDRW2100039251.1.10",
          "form": "안 될거 가튼데",
          "original_form": "안 될거 가튼데",
          "speaker_id": "1",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039251.1.11",
          "form": "할라면 니가 해라",
          "original_form": "할라면 니가 해라",
          "speaker_id": "1",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039251.1.12",
          "form": "아 내가 할 거거든?",
          "original_form": "아 내가 할 거거든?",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039251.1.13",
          "form": "잔소리나",
          "original_form": "잔소리나",
          "speaker_id": "2",
          "time": "20210903 15:10"
        },
        {
          "id": "MDRW2100039251.1.14",
          "form": "하지마",
          "original_form": "하지마",
          "speaker_id": "2",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039251.1.15",
          "form": "잔소리 안 하게 잘해",
          "original_form": "잔소리 안 하게 잘해",
          "speaker_id": "1",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039251.1.16",
          "form": "어지르지 말고",
          "original_form": "어지르지 말고",
          "speaker_id": "1",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039251.1.17",
          "form": "헐 나중에 맛있다고 다 뺏어먹지나 마셈",
          "original_form": "헐 나중에 맛있다고 다 뺏어먹지나 마셈",
          "speaker_id": "2",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100039251.1.18",
          "form": "아주 먹고 싶은 거 많아서 큰일났어",
          "original_form": "아주 먹고 싶은 거 많아서 큰일났어",
          "speaker_id": "1",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100039251.1.19",
          "form": "먹고 싶은 건 많지만 많이 못 먹으니까 괜춘",
          "original_form": "먹고 싶은 건 많지만 많이 못 먹으니까 괜춘",
          "speaker_id": "2",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100039251.1.20",
          "form": "그런애들이 식비가 많이 드는거다",
          "original_form": "그런애들이 식비가 많이 드는거다",
          "speaker_id": "1",
          "time": "20210903 15:12"
        }
      ]
