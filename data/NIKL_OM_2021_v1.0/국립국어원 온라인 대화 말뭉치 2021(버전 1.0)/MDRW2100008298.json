      "utterance": [
        {
          "id": "MDRW2100008298.1.1",
          "form": "밖에 덥나요?",
          "original_form": "밖에 덥나요?",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008298.1.2",
          "form": "집이 왜케 더운걸까요?",
          "original_form": "집이 왜케 더운걸까요?",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008298.1.3",
          "form": "비오려고 그런건지 끈끈하고 그러네요",
          "original_form": "비오려고 그런건지 끈끈하고 그러네요",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008298.1.4",
          "form": "차라리 비 얼렁 왔음 좋겠어요",
          "original_form": "차라리 비 얼렁 왔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008298.1.5",
          "form": "오늘은 비안오는거같아요",
          "original_form": "오늘은 비안오는거같아요",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008298.1.6",
          "form": "해나는 거 같기도 하고",
          "original_form": "해나는 거 같기도 하고",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008298.1.7",
          "form": "일기예보는 비온다는데 여기도 해나왔어요.",
          "original_form": "일기예보는 비온다는데 여기도 해나왔어요.",
          "speaker_id": "2",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008298.1.8",
          "form": "낼부터 다시올거같아요",
          "original_form": "낼부터 다시올거같아요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008298.1.9",
          "form": "바람은 선선하죠?",
          "original_form": "바람은 선선하죠?",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008298.1.10",
          "form": "태풍도 올라온다는거같아요",
          "original_form": "태풍도 올라온다는거같아요",
          "speaker_id": "2",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008298.1.11",
          "form": "네. 선풍기 바람이 시원하네요",
          "original_form": "네. 선풍기 바람이 시원하네요",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008298.1.12",
          "form": "어제 뉴스보니 이미 부산은 물난리던데요",
          "original_form": "어제 뉴스보니 이미 부산은 물난리던데요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008298.1.13",
          "form": "아직 태풍영향권은 아닌거같던데",
          "original_form": "아직 태풍영향권은 아닌거같던데",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008298.1.14",
          "form": "그러게요. 비피해없었음 좋겠어요",
          "original_form": "그러게요. 비피해없었음 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008298.1.15",
          "form": "그러게요 피해있는지역만 매번 그러는듯요",
          "original_form": "그러게요 피해있는지역만 매번 그러는듯요",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008298.1.16",
          "form": "슬슬 가을옷도 꺼내야겠어요",
          "original_form": "슬슬 가을옷도 꺼내야겠어요",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008298.1.17",
          "form": "이번 비지남 바로 가을 스쳐지나갈듯해요",
          "original_form": "이번 비지남 바로 가을 스쳐지나갈듯해요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008298.1.18",
          "form": "이럴때가 가장 옷입기 애매해요",
          "original_form": "이럴때가 가장 옷입기 애매해요",
          "speaker_id": "2",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008298.1.19",
          "form": "간절기 옷 사야지 하다보면 겨울이예요",
          "original_form": "간절기 옷 사야지 하다보면 겨울이예요",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008298.1.20",
          "form": "안사자니 입을 옷이 없고 ㅎ",
          "original_form": "안사자니 입을 옷이 없고 ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008298.1.21",
          "form": "그러게요. 몰 느낄새도없이 바로 겨울직행이네요",
          "original_form": "그러게요. 몰 느낄새도없이 바로 겨울직행이네요",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008298.1.22",
          "form": "전 거의 집에만있어서 대강 있는거 주워입어요^^",
          "original_form": "전 거의 집에만있어서 대강 있는거 주워입어요^^",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008298.1.23",
          "form": "어떤계절 좋아하세요?ㅎ",
          "original_form": "어떤계절 좋아하세요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008298.1.24",
          "form": "전 봄이요~~ 근데 미세먼지때문에 싫어지네요",
          "original_form": "전 봄이요~~ 근데 미세먼지때문에 싫어지네요",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008298.1.25",
          "form": "저도 봄가을이좋은데 특히 가을좋아해요",
          "original_form": "저도 봄가을이좋은데 특히 가을좋아해요",
