        },
        {
          "id": "MDRW2100043269.1.2",
          "form": "name1님의 선택은?",
          "original_form": "&name1&님의 선택은?",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043269.1.3",
          "form": "볼케이노욬ㅋㅋㅋ",
          "original_form": "볼케이노욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043269.1.4",
          "form": "매운거 못잃어..",
          "original_form": "매운거 못잃어..",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043269.1.5",
          "form": "맞죸ㅋㅋㅋㅋㅋ",
          "original_form": "맞죸ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043269.1.6",
          "form": "저두요방금 매운소스들어간 샌드위치먹구왔어요 ㅋㅋ",
          "original_form": "저두요방금 매운소스들어간 샌드위치먹구왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043269.1.7",
          "form": "ㅋㅋㅋㅋ 맛있겠네요",
          "original_form": "ㅋㅋㅋㅋ 맛있겠네요",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043269.1.8",
          "form": "저는 샤브샤브!",
          "original_form": "저는 샤브샤브!",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043269.1.9",
          "form": "오오 샤브샤브도 맛나죠 ㅎㅎㅎ",
          "original_form": "오오 샤브샤브도 맛나죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043269.1.10",
          "form": "퇴근하신거에요?",
          "original_form": "퇴근하신거에요?",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043269.1.11",
          "form": "네네 ㅋㅋㅋ 집이에여",
          "original_form": "네네 ㅋㅋㅋ 집이에여",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043269.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043269.1.13",
          "form": "집안일 주로 담당하시는게 있나요?",
          "original_form": "집안일 주로 담당하시는게 있나요?",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043269.1.14",
          "form": "저는 설거지랑 빨래랑..",
          "original_form": "저는 설거지랑 빨래랑..",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043269.1.15",
          "form": "저녁차리기..?",
          "original_form": "저녁차리기..?",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043269.1.16",
          "form": "오오 청소제외 대부분아닌가요? ㅎㅎ",
          "original_form": "오오 청소제외 대부분아닌가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043269.1.17",
          "form": "ㅋㅋㅋㅋ네그러네여..주말에는 거의하고 평일ㄴ 거의 엄마가하세요ㅋㅋ큐ㅠ",
          "original_form": "ㅋㅋㅋㅋ네그러네여..주말에는 거의하고 평일ㄴ 거의 엄마가하세요ㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043269.1.18",
          "form": "name2님은여??",
          "original_form": "&name2&님은여??",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043269.1.19",
          "form": "오오 저는 자취중이라몇안듀ㅣ는 집안일 제가 다 합니다 ㅎㅎ..",
          "original_form": "오오 저는 자취중이라몇안듀ㅣ는 집안일 제가 다 합니다 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043269.1.20",
          "form": "그래서 요리도 잘 안해묵개듀ㅣ더라구요 하하",
          "original_form": "그래서 요리도 잘 안해묵개듀ㅣ더라구요 하하",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043269.1.21",
          "form": "ㅋㅋㅋ오오 그러시구나 힘드시겠어요 혼자 다하시려면 ㅠㅠ",
