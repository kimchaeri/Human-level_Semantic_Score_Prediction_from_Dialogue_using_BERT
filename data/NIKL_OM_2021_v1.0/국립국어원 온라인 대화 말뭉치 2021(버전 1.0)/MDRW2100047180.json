          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047180.1.5",
          "form": "민초진짜",
          "original_form": "민초진짜",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047180.1.6",
          "form": "뭔맛인지모르겠어요",
          "original_form": "뭔맛인지모르겠어요",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047180.1.7",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047180.1.8",
          "form": "음 민초가 그정도군요",
          "original_form": "음 민초가 그정도군요",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047180.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047180.1.10",
          "form": "잘드세요?",
          "original_form": "잘드세요?",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047180.1.11",
          "form": "저는 배고프면 다먹습니다",
          "original_form": "저는 배고프면 다먹습니다",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047180.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100047180.1.13",
          "form": "배민 등급이 무엇인가요?",
          "original_form": "배민 등급이 무엇인가요?",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100047180.1.14",
          "form": "저는 배달음식은 잘 안먹어요",
          "original_form": "저는 배달음식은 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100047180.1.15",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100047180.1.16",
          "form": "그럼 집에서요리해먹나요?",
          "original_form": "그럼 집에서요리해먹나요?",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100047180.1.17",
          "form": "그냥 대충때워요 오뎅이나 떡..ㅎㅎ 도시락싸요",
          "original_form": "그냥 대충때워요 오뎅이나 떡..ㅎㅎ 도시락싸요",
          "speaker_id": "1",
          "time": "20210826 13:30"
        },
        {
          "id": "MDRW2100047180.1.18",
          "form": "매일요?!!",
          "original_form": "매일요?!!",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100047180.1.19",
          "form": "음 암튼 어케든 먹는데.. 배달은 거의 안시켜요",
          "original_form": "음 암튼 어케든 먹는데.. 배달은 거의 안시켜요",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100047180.1.20",
          "form": "신기하네요ㄷㄷㄷㄷ",
          "original_form": "신기하네요ㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100047180.1.21",
