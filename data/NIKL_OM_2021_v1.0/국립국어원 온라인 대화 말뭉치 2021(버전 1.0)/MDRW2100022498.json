      "utterance": [
        {
          "id": "MDRW2100022498.1.1",
          "form": "최근에 여행 다녀오셨어요?",
          "original_form": "최근에 여행 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022498.1.2",
          "form": "코시국이긴 하지만",
          "original_form": "코시국이긴 하지만",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022498.1.3",
          "form": "여행을 자주가는 편은 아니라서",
          "original_form": "여행을 자주가는 편은 아니라서",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022498.1.4",
          "form": "근데 최근에 가고싶네요",
          "original_form": "근데 최근에 가고싶네요",
          "speaker_id": "2",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100022498.1.5",
          "form": "어디 가고 싶으신데요?",
          "original_form": "어디 가고 싶으신데요?",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100022498.1.6",
          "form": "저는 유럽이요ㅜ",
          "original_form": "저는 유럽이요ㅜ",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100022498.1.7",
          "form": "해외는 안다녀봐서",
          "original_form": "해외는 안다녀봐서",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022498.1.8",
          "form": "저는 부산 가고싶네요",
          "original_form": "저는 부산 가고싶네요",
          "speaker_id": "2",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022498.1.9",
          "form": "아~부산 좋죠",
          "original_form": "아~부산 좋죠",
          "speaker_id": "1",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022498.1.10",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "1",
          "time": "20210829 15:09"
        },
        {
          "id": "MDRW2100022498.1.11",
          "form": "안가봤는데 로망은 있어요",
          "original_form": "안가봤는데 로망은 있어요",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022498.1.12",
          "form": "바다도 보고 맛있는것도 먹고",
          "original_form": "바다도 보고 맛있는것도 먹고",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022498.1.13",
          "form": "크.... 바다 좋죠",
          "original_form": "크.... 바다 좋죠",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022498.1.14",
          "form": "근데 저는 부산보다는 강릉 속초 이쪽이 더 좋은거같아요",
          "original_form": "근데 저는 부산보다는 강릉 속초 이쪽이 더 좋은거같아요",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022498.1.15",
          "form": "심심하긴 해도 부산은 사람이 너무너무너무많아서ㅜ",
          "original_form": "심심하긴 해도 부산은 사람이 너무너무너무많아서ㅜ",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022498.1.16",
          "form": "강릉도 좋죠",
          "original_form": "강릉도 좋죠",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022498.1.17",
          "form": "자주가서 할거없는데",
          "original_form": "자주가서 할거없는데",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022498.1.18",
          "form": "산보단 바다가 편하죠",
          "original_form": "산보단 바다가 편하죠",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022498.1.19",
          "form": "요즘은 카페나 맛집 많이생겨서",
          "original_form": "요즘은 카페나 맛집 많이생겨서",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022498.1.20",
          "form": "여행다닐맛나욯ㅎ",
          "original_form": "여행다닐맛나욯ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022498.1.21",
          "form": "그렇죠 사진도 찍으면서",
          "original_form": "그렇죠 사진도 찍으면서",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022498.1.22",
          "form": "돌아다니면 여행다닐만 하죠",
          "original_form": "돌아다니면 여행다닐만 하죠",
          "speaker_id": "2",
          "time": "20210829 15:14"
        },
        {
          "id": "MDRW2100022498.1.23",
          "form": "그렇져ㅎㅎㅎㅎ",
          "original_form": "그렇져ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100022498.1.24",
          "form": "남는건 사진뿐",
          "original_form": "남는건 사진뿐",
          "speaker_id": "1",
          "time": "20210829 15:15"
        }
      ]
    }
