        },
        {
          "id": "MDRW2100019990.1.2",
          "form": "토끼요?",
          "original_form": "토끼요?",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019990.1.3",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019990.1.4",
          "form": "신기하네요 보통 강아지나 고양이 얘기하시던데",
          "original_form": "신기하네요 보통 강아지나 고양이 얘기하시던데",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019990.1.5",
          "form": "전 토끼가좋더라구요",
          "original_form": "전 토끼가좋더라구요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019990.1.6",
          "form": "귀여워서요?",
          "original_form": "귀여워서요?",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019990.1.7",
          "form": "네 귀엽고 조용조용하고",
          "original_form": "네 귀엽고 조용조용하고",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019990.1.8",
          "form": "스트레스에 엄청 취약하대요",
          "original_form": "스트레스에 엄청 취약하대요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019990.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019990.1.10",
          "form": "전 고양이파에요",
          "original_form": "전 고양이파에요",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019990.1.11",
          "form": "고양이 귀엽죠",
          "original_form": "고양이 귀엽죠",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019990.1.12",
          "form": "몽글몽글한게귀여워요",
          "original_form": "몽글몽글한게귀여워요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019990.1.13",
          "form": "몽글몽글이라니",
          "original_form": "몽글몽글이라니",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019990.1.14",
          "form": "넘 귀엽네요",
