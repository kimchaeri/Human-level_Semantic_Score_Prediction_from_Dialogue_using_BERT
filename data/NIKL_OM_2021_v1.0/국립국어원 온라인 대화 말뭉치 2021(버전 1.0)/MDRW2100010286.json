      "utterance": [
        {
          "id": "MDRW2100010286.1.1",
          "form": "우리 사야될 거 뭐있지?",
          "original_form": "우리 사야될 거 뭐있지?",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010286.1.2",
          "form": "물티슈 각티슈 그런거야",
          "original_form": "물티슈 각티슈 그런거야",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010286.1.3",
          "form": "물티슈 많지 않아?",
          "original_form": "물티슈 많지 않아?",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010286.1.4",
          "form": "향 난거 밖에 없어",
          "original_form": "향 난거 밖에 없어",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010286.1.5",
          "form": "그래도 아까우니까 다 써야지",
          "original_form": "그래도 아까우니까 다 써야지",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010286.1.6",
          "form": "물티슈 말고 더 필요한 것은?",
          "original_form": "물티슈 말고 더 필요한 것은?",
          "speaker_id": "1",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010286.1.7",
          "form": "수세미 영양제",
          "original_form": "수세미 영양제",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010286.1.8",
          "form": "가루 비타민c 먹어보니까 어때?",
          "original_form": "가루 비타민c 먹어보니까 어때?",
          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010286.1.9",
          "form": "괜찮지?",
          "original_form": "괜찮지?",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010286.1.10",
          "form": "편해 괜찮아?",
          "original_form": "편해 괜찮아?",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010286.1.11",
          "form": "난 좋아",
          "original_form": "난 좋아",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010286.1.12",
          "form": "저번에 내가 샀으니까 이번에 너가 사",
          "original_form": "저번에 내가 샀으니까 이번에 너가 사",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010286.1.13",
          "form": "돈 나온다",
          "original_form": "돈 나온다",
          "speaker_id": "2",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010286.1.14",
          "form": "나도 돈없다",
          "original_form": "나도 돈없다",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010286.1.15",
          "form": "수세미는 주방 서랍에 있어",
          "original_form": "수세미는 주방 서랍에 있어",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010286.1.16",
          "form": "포인트로 살수 있을지 모르겠다",
          "original_form": "포인트로 살수 있을지 모르겠다",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010286.1.17",
          "form": "또 다른 필요한 것 더 있나?",
          "original_form": "또 다른 필요한 것 더 있나?",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010286.1.18",
          "form": "내가 사온 세탁세제 써봤어?",
          "original_form": "내가 사온 세탁세제 써봤어?",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010286.1.19",
          "form": "아니 한번도 안 써봤어",
          "original_form": "아니 한번도 안 써봤어",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010286.1.20",
          "form": "다른거 없어",
          "original_form": "다른거 없어",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010286.1.21",
          "form": "그렇군. 아까 낮에 온 택배 정리해놔",
          "original_form": "그렇군. 아까 낮에 온 택배 정리해놔",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010286.1.22",
          "form": "만들기 전에 정리하면 되겠다",
          "original_form": "만들기 전에 정리하면 되겠다",
          "speaker_id": "2",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100010286.1.23",
          "form": "응 사이즈 어떤걸로 샀어?",
          "original_form": "응 사이즈 어떤걸로 샀어?",
          "speaker_id": "1",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100010286.1.24",
          "form": "수퍼롱 샀어",
          "original_form": "수퍼롱 샀어",
          "speaker_id": "2",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100010286.1.25",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210823 16:38"
        }
      ]
    }
