      "utterance": [
        {
          "id": "MDRW2100039442.1.1",
          "form": "어떤 음료수를 좋아하세요?",
          "original_form": "어떤 음료수를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039442.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039442.1.3",
          "form": "요즘은 두유에 빠져서 열심히 마시고 있어요ㅎㅎ",
          "original_form": "요즘은 두유에 빠져서 열심히 마시고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039442.1.4",
          "form": "name1님은 어떤 거 좋아하세요?",
          "original_form": "&name1&님은 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039442.1.5",
          "form": "저는 밀키스 좋아해요~",
          "original_form": "저는 밀키스 좋아해요~",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.6",
          "form": "밀키스 맛있죠!",
          "original_form": "밀키스 맛있죠!",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.7",
          "form": "최애음료에요",
          "original_form": "최애음료에요",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.8",
          "form": "근데 최근에 나온 식빵 맛은 별로더라구요 ㅠ",
          "original_form": "근데 최근에 나온 식빵 맛은 별로더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.9",
          "form": "사이다랑 콜라랑은 또 다르게 청량해서 좋아요",
          "original_form": "사이다랑 콜라랑은 또 다르게 청량해서 좋아요",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.11",
          "form": "그런 게 나왔어요?",
          "original_form": "그런 게 나왔어요?",
          "speaker_id": "2",
          "time": "20210903 18:00"
        },
        {
          "id": "MDRW2100039442.1.12",
          "form": "상상이 안 가요 식빵맛 음료수..?",
          "original_form": "상상이 안 가요 식빵맛 음료수..?",
          "speaker_id": "2",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039442.1.13",
          "form": "네 우유식빵맛?이라고 나왔는데 정말 식빵 향이나요",
          "original_form": "네 우유식빵맛?이라고 나왔는데 정말 식빵 향이나요",
          "speaker_id": "1",
          "time": "20210903 18:01"
        },
        {
          "id": "MDRW2100039442.1.14",
          "form": "어떻게 빵을 마실 걸로 치환할 생각을 했을까요",
          "original_form": "어떻게 빵을 마실 걸로 치환할 생각을 했을까요",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039442.1.15",
          "form": "ㅋㅋㅋㅌㅋ진짜 독특하네요",
          "original_form": "ㅋㅋㅋㅌㅋ진짜 독특하네요",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039442.1.16",
          "form": "저는 밀키스 기본맛 하얀 거밖에 안 먹어봤는데 맛이 다양한가요?",
          "original_form": "저는 밀키스 기본맛 하얀 거밖에 안 먹어봤는데 맛이 다양한가요?",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039442.1.17",
          "form": "그건 잘 모르겠어요. 기본맛 이외에 먹은건 식빵맛이 처음이었어요",
          "original_form": "그건 잘 모르겠어요. 기본맛 이외에 먹은건 식빵맛이 처음이었어요",
          "speaker_id": "1",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039442.1.18",
          "form": "그렇구나..빵 맛 음료라니 컬쳐쇼크예요",
          "original_form": "그렇구나..빵 맛 음료라니 컬쳐쇼크예요",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039442.1.19",
          "form": "궁금해서 다음에 발견하면 먹어봐야겠어요ㅋㅋㅋ",
          "original_form": "궁금해서 다음에 발견하면 먹어봐야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039442.1.20",
          "form": "좋지 않은 생각일까요?",
          "original_form": "좋지 않은 생각일까요?",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039442.1.21",
          "form": "한번쯤은 호기심에 먹어볼만해요 ㅋㅋㅋㅋㅋ",
          "original_form": "한번쯤은 호기심에 먹어볼만해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039442.1.22",
          "form": "아주 못먹을 정돈 아녜요~",
          "original_form": "아주 못먹을 정돈 아녜요~",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039442.1.23",
          "form": "ㅋㅋㅋㅋㅋ좋아유 체험해보겠습니다",
          "original_form": "ㅋㅋㅋㅋㅋ좋아유 체험해보겠습니다",
          "speaker_id": "2",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039442.1.24",
          "form": "전 두유에 빠진 지 얼마 안 됐는데",
          "original_form": "전 두유에 빠진 지 얼마 안 됐는데",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039442.1.25",
          "form": "사실 끼니 챙기기 귀찮아서 먹게 되더라구요",
          "original_form": "사실 끼니 챙기기 귀찮아서 먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039442.1.26",
          "form": "어렸을 땐 몰랐는데 크고 보니 밥 챙겨먹기가 은근 번거로워요",
          "original_form": "어렸을 땐 몰랐는데 크고 보니 밥 챙겨먹기가 은근 번거로워요",
          "speaker_id": "2",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039442.1.27",
          "form": "먹는 것도 일이니깐요 ㅠ",
          "original_form": "먹는 것도 일이니깐요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039442.1.28",
          "form": "두유가 든든하면서도 간단하죠",
          "original_form": "두유가 든든하면서도 간단하죠",
          "speaker_id": "1",
