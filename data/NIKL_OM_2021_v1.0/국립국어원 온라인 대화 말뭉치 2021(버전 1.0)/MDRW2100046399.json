      "utterance": [
        {
          "id": "MDRW2100046399.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211021 14:58"
        },
        {
          "id": "MDRW2100046399.1.2",
          "form": "안녕하세요~ 날씨가 많이 선선해졌어요!",
          "original_form": "안녕하세요~ 날씨가 많이 선선해졌어요!",
          "speaker_id": "2",
          "time": "20211021 14:59"
        },
        {
          "id": "MDRW2100046399.1.3",
          "form": "네 추워요 ㅎ",
          "original_form": "네 추워요 ㅎ",
          "speaker_id": "1",
          "time": "20211021 14:59"
        },
        {
          "id": "MDRW2100046399.1.4",
          "form": "감기 조심하세요",
          "original_form": "감기 조심하세요",
          "speaker_id": "1",
          "time": "20211021 14:59"
        },
        {
          "id": "MDRW2100046399.1.5",
          "form": "아침엔 정말 겨울같아서 꽁꽁 싸매야하더라구요",
          "original_form": "아침엔 정말 겨울같아서 꽁꽁 싸매야하더라구요",
          "speaker_id": "2",
          "time": "20211021 15:00"
        },
        {
          "id": "MDRW2100046399.1.6",
          "form": "네 아침에 ㅎㄷㄷ;",
          "original_form": "네 아침에 ㅎㄷㄷ;",
          "speaker_id": "1",
          "time": "20211021 15:00"
        },
        {
          "id": "MDRW2100046399.1.7",
          "form": "덕분에 겨울옷을 벌써 꺼냈어요 ㅠ",
          "original_form": "덕분에 겨울옷을 벌써 꺼냈어요 ㅠ",
          "speaker_id": "2",
          "time": "20211021 15:00"
        },
        {
          "id": "MDRW2100046399.1.8",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20211021 15:00"
        },
        {
          "id": "MDRW2100046399.1.9",
          "form": "밤엔 온열장판 틀고자요",
          "original_form": "밤엔 온열장판 틀고자요",
          "speaker_id": "1",
          "time": "20211021 15:01"
        },
        {
          "id": "MDRW2100046399.1.10",
          "form": "전 아직 꺼내진 않았는데",
          "original_form": "전 아직 꺼내진 않았는데",
          "speaker_id": "2",
          "time": "20211021 15:01"
        },
        {
          "id": "MDRW2100046399.1.11",
          "form": "못버티고 곧 꺼낼 것 같아요 ㅋㅋ",
          "original_form": "못버티고 곧 꺼낼 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211021 15:01"
        },
        {
          "id": "MDRW2100046399.1.12",
          "form": "제가 추위를 많이타서 일찍 꺼냈어요",
          "original_form": "제가 추위를 많이타서 일찍 꺼냈어요",
          "speaker_id": "1",
          "time": "20211021 15:02"
        },
        {
          "id": "MDRW2100046399.1.13",
          "form": "엇 그럼 겨울보단 여름을 좋아하시려나요?",
          "original_form": "엇 그럼 겨울보단 여름을 좋아하시려나요?",
          "speaker_id": "2",
          "time": "20211021 15:02"
        },
        {
          "id": "MDRW2100046399.1.14",
          "form": "네 겨울보단 여름을 더 좋아해요",
          "original_form": "네 겨울보단 여름을 더 좋아해요",
          "speaker_id": "1",
          "time": "20211021 15:03"
        },
        {
          "id": "MDRW2100046399.1.15",
          "form": "여름도 그렇게 좋아하는건아니지만",
          "original_form": "여름도 그렇게 좋아하는건아니지만",
          "speaker_id": "1",
          "time": "20211021 15:03"
        },
        {
          "id": "MDRW2100046399.1.16",
          "form": "ㅋㅋ저는 항상 여름엔 겨울이 빨리 왔으면하고 겨울엔 여름이 그리운 것 같아요",
          "original_form": "ㅋㅋ저는 항상 여름엔 겨울이 빨리 왔으면하고 겨울엔 여름이 그리운 것 같아요",
          "speaker_id": "2",
          "time": "20211021 15:04"
        },
        {
          "id": "MDRW2100046399.1.17",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "1",
          "time": "20211021 15:04"
        },
        {
          "id": "MDRW2100046399.1.18",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20211021 15:05"
        },
        {
          "id": "MDRW2100046399.1.19",
          "form": "겨울은 장판틀고 귤까먹는 계절이니",
          "original_form": "겨울은 장판틀고 귤까먹는 계절이니",
          "speaker_id": "2",
          "time": "20211021 15:05"
        },
        {
          "id": "MDRW2100046399.1.20",
          "form": "넵ㅎㅎ 맛있게 먹었습니다!",
          "original_form": "넵ㅎㅎ 맛있게 먹었습니다!",
          "speaker_id": "2",
          "time": "20211021 15:05"
        },
        {
          "id": "MDRW2100046399.1.21",
          "form": "장판틀고 귤까먹으면 맛있죠",
          "original_form": "장판틀고 귤까먹으면 맛있죠",
          "speaker_id": "1",
          "time": "20211021 15:05"
        }
      ]
