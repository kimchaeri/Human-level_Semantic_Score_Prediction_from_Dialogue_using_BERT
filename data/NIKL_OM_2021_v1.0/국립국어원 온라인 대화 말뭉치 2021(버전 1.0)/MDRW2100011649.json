        {
          "id": "MDRW2100011649.1.1",
          "form": "싸이월드 부활한거 아나?",
          "original_form": "싸이월드 부활한거 아나?",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011649.1.2",
          "form": "부활했다고하드라",
          "original_form": "부활했다고하드라",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011649.1.3",
          "form": "응 사진 많나?",
          "original_form": "응 사진 많나?",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011649.1.4",
          "form": "거기 내사진이랑 아직잘있을란가",
          "original_form": "거기 내사진이랑 아직잘있을란가",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011649.1.5",
          "form": "비번아나?",
          "original_form": "비번아나?",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011649.1.6",
          "form": "젊었을때 사진",
          "original_form": "젊었을때 사진",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011649.1.7",
          "form": "비번모름",
          "original_form": "비번모름",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011649.1.8",
          "form": "찾아야겠네",
          "original_form": "찾아야겠네",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011649.1.9",
          "form": "언니야는 도토리 남은거 없나",
          "original_form": "언니야는 도토리 남은거 없나",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011649.1.10",
          "form": "없지",
          "original_form": "없지",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011649.1.11",
          "form": "니는 있나?",
          "original_form": "니는 있나?",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011649.1.12",
          "form": "난 있지싶다",
          "original_form": "난 있지싶다",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011649.1.13",
          "form": "도토리도 샀나?ㅋ",
          "original_form": "도토리도 샀나?ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011649.1.14",
          "form": "천원 정도",
          "original_form": "천원 정도",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
