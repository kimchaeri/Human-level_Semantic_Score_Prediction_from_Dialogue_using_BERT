        },
        {
          "id": "MDRW2100002324.1.2",
          "form": "name2야 얼른 글을 읽어주렴",
          "original_form": "&name2&야 얼른 글을 읽어주렴",
          "speaker_id": "1",
          "time": "20210820 10:30"
        },
        {
          "id": "MDRW2100002324.1.3",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100002324.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100002324.1.5",
          "form": "언제 결혼하셨나요?",
          "original_form": "언제 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100002324.1.6",
          "form": "2016년도에 했어요",
          "original_form": "2016년도에 했어요",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100002324.1.7",
          "form": "당신은 하셨나요?",
          "original_form": "당신은 하셨나요?",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100002324.1.8",
          "form": "저는 아직 안했어요",
          "original_form": "저는 아직 안했어요",
          "speaker_id": "2",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100002324.1.9",
          "form": "결혼에 긍정이신가요? 부정이신가요?",
          "original_form": "결혼에 긍정이신가요? 부정이신가요?",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100002324.1.10",
          "form": "결혼관도 궁금하네요",
          "original_form": "결혼관도 궁금하네요",
          "speaker_id": "1",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100002324.1.11",
          "form": "저는 아직 고민 중이에요",
          "original_form": "저는 아직 고민 중이에요",
          "speaker_id": "2",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100002324.1.12",
          "form": "결혼에 긍정적이신가요?",
          "original_form": "결혼에 긍정적이신가요?",
          "speaker_id": "2",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100002324.1.13",
          "form": "긍정적이예요",
          "original_form": "긍정적이예요",
          "speaker_id": "1",
          "time": "20210820 12:01"
        },
        {
          "id": "MDRW2100002324.1.14",
          "form": "어떤점이 긍정적이신가요?",
          "original_form": "어떤점이 긍정적이신가요?",
          "speaker_id": "2",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100002324.1.15",
          "form": "든든한 편이 생겨서 좋네요",
          "original_form": "든든한 편이 생겨서 좋네요",
          "speaker_id": "1",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100002324.1.16",
          "form": "맞아요 그게 가장 큰 장점인것 같아요",
          "original_form": "맞아요 그게 가장 큰 장점인것 같아요",
          "speaker_id": "2",
          "time": "20210820 12:06"
        },
        {
          "id": "MDRW2100002324.1.17",
          "form": "결혼에 대해서 뭐가 제일 무서우시나요?",
          "original_form": "결혼에 대해서 뭐가 제일 무서우시나요?",
          "speaker_id": "1",
          "time": "20210820 12:10"
        },
        {
          "id": "MDRW2100002324.1.18",
          "form": "시댁과의 관계가 무섭네요",
          "original_form": "시댁과의 관계가 무섭네요",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002324.1.19",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002324.1.20",
          "form": "남편한테 잘하라고 해야겠네요",
          "original_form": "남편한테 잘하라고 해야겠네요",
