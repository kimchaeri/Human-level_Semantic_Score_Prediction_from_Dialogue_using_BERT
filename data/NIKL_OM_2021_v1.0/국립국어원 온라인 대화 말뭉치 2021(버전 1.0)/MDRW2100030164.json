      "utterance": [
        {
          "id": "MDRW2100030164.1.1",
          "form": "안녕하세요! name2님은 드라마 보는 거 좋아하세요?!",
          "original_form": "안녕하세요! &name2&님은 드라마 보는 거 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030164.1.2",
          "form": "드라마 싫어해요!",
          "original_form": "드라마 싫어해요!",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030164.1.3",
          "form": "앗 노래는 자주 들으시나요?",
          "original_form": "앗 노래는 자주 들으시나요?",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030164.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030164.1.5",
          "form": "좋아하는 가수 있으세요?",
          "original_form": "좋아하는 가수 있으세요?",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030164.1.6",
          "form": "켈리클락슨 좋아해요",
          "original_form": "켈리클락슨 좋아해요",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030164.1.7",
          "form": "들어봐야겠네요 ~",
          "original_form": "들어봐야겠네요 ~",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030164.1.8",
          "form": "노래 좋은거 추천 해주실수있나요?",
          "original_form": "노래 좋은거 추천 해주실수있나요?",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030164.1.9",
          "form": "혹시 올리비아 로드리고 아시나요?",
          "original_form": "혹시 올리비아 로드리고 아시나요?",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030164.1.10",
          "form": "그 가수 데자부란 노래가 좋더라고요",
          "original_form": "그 가수 데자부란 노래가 좋더라고요",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030164.1.11",
          "form": "몰라요 한번 찾아들어봐야겠네요",
          "original_form": "몰라요 한번 찾아들어봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030164.1.12",
          "form": "잔잔한건가요?",
          "original_form": "잔잔한건가요?",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030164.1.13",
          "form": "아니면 지르는?",
          "original_form": "아니면 지르는?",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030164.1.14",
          "form": "약간 잔잔한데 하이라이트가 좀 쎈 느낌이에요",
          "original_form": "약간 잔잔한데 하이라이트가 좀 쎈 느낌이에요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030164.1.15",
          "form": "굿 포 유는 좀 지르는? 느낌에 가깝고요",
          "original_form": "굿 포 유는 좀 지르는? 느낌에 가깝고요",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030164.1.16",
          "form": "어 굿포유 아는거같은데",
          "original_form": "어 굿포유 아는거같은데",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030164.1.17",
          "form": "이번에 나왔던 거라 아실 것 같아요",
          "original_form": "이번에 나왔던 거라 아실 것 같아요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        }
