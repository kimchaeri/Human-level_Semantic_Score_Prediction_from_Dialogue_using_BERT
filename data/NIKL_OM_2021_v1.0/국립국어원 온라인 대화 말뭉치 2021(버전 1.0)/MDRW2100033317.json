      "utterance": [
        {
          "id": "MDRW2100033317.1.1",
          "form": "오 무슨겜하세요?",
          "original_form": "오 무슨겜하세요?",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100033317.1.2",
          "form": "모바일게임이지요ㅋ",
          "original_form": "모바일게임이지요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033317.1.3",
          "form": "뭐 딱히 자주 드려다 볼 시간이없어서",
          "original_form": "뭐 딱히 자주 드려다 볼 시간이없어서",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033317.1.4",
          "form": "단순한 게임하곤 합니다.",
          "original_form": "단순한 게임하곤 합니다.",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033317.1.5",
          "form": "쿠키런 킹덤?\r\n했네요ㅋ",
          "original_form": "쿠키런 킹덤?\r\n했네요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033317.1.6",
          "form": "ㅋ저도 에져ㆍ 롤 뿐ㅋ",
          "original_form": "ㅋ저도 에져ㆍ 롤 뿐ㅋ",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033317.1.7",
          "form": "달리기요?",
          "original_form": "달리기요?",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033317.1.8",
          "form": "달리기는 달리기인데 무슨 이상한 쿠키도 뽑고 신기한 게임이요",
          "original_form": "달리기는 달리기인데 무슨 이상한 쿠키도 뽑고 신기한 게임이요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033317.1.9",
          "form": "조카가 하자해서 했다가\r\n요즘은 접었어요ㅋ",
          "original_form": "조카가 하자해서 했다가\r\n요즘은 접었어요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033317.1.10",
          "form": "폰으로 넷플릭스 미드 or 영드봅니다ㅋ",
          "original_form": "폰으로 넷플릭스 미드 or 영드봅니다ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033317.1.11",
          "form": "오~~ 그 퍼즐마추기(?)",
          "original_form": "오~~ 그 퍼즐마추기(?)",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033317.1.12",
          "form": "애니팡(?)",
          "original_form": "애니팡(?)",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033317.1.13",
          "form": "아ㅋㅋ그거 재밋지요",
          "original_form": "아ㅋㅋ그거 재밋지요",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100033317.1.14",
          "form": "요즘도 있나 모르겠네요",
          "original_form": "요즘도 있나 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100033317.1.15",
          "form": "제 주 게임은 온라인 테트리스였는데",
          "original_form": "제 주 게임은 온라인 테트리스였는데",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100033317.1.16",
          "form": "요즘은 없던데요...ㅋ",
          "original_form": "요즘은 없던데요...ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100033317.1.17",
          "form": "아 오락실 테트리스ㅋ",
          "original_form": "아 오락실 테트리스ㅋ",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100033317.1.18",
          "form": "한게임인가 거기서도 좀했어요",
          "original_form": "한게임인가 거기서도 좀했어요",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100033317.1.19",
          "form": "저도.. ㅋ한게임인지 넷마블인지..",
          "original_form": "저도.. ㅋ한게임인지 넷마블인지..",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033317.1.20",
          "form": "오락실은 펌프지요ㅋ",
          "original_form": "오락실은 펌프지요ㅋ",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033317.1.21",
          "form": "아 철권도..",
          "original_form": "아 철권도..",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033317.1.22",
          "form": "아 디디알ㅋ",
          "original_form": "아 디디알ㅋ",
          "speaker_id": "2",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033317.1.23",
          "form": "디디알...\r\n버터플라이ㅋ",
          "original_form": "디디알...\r\n버터플라이ㅋ",
