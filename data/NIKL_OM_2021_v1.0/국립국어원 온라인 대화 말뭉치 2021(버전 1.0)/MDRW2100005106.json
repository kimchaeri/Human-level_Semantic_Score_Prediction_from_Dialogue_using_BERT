      "utterance": [
        {
          "id": "MDRW2100005106.1.1",
          "form": "요즘 일교차가 심한데 아픈 곳 없으신가요ㅠ",
          "original_form": "요즘 일교차가 심한데 아픈 곳 없으신가요ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005106.1.2",
          "form": "아픈 곳은 없어요ㅠㅜ",
          "original_form": "아픈 곳은 없어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005106.1.3",
          "form": "name1님은 괜찮으신가요?",
          "original_form": "&name1&님은 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005106.1.4",
          "form": "저는 요즘 결막염으로 고생하고 있습니다,,\r\nname2님은 절대 걸리지 마세요 고통,,,",
          "original_form": "저는 요즘 결막염으로 고생하고 있습니다,,\r\n&name2&님은 절대 걸리지 마세요 고통,,,",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005106.1.5",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005106.1.6",
          "form": "결막염은 눈병이랑 비슷한가요?",
          "original_form": "결막염은 눈병이랑 비슷한가요?",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005106.1.7",
          "form": "옮는 결막염이 있고 아닌 게 있는데 대부분 안 옮는 알레르기성 결막염이더라고요",
          "original_form": "옮는 결막염이 있고 아닌 게 있는데 대부분 안 옮는 알레르기성 결막염이더라고요",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005106.1.8",
          "form": "눈이 엄청 가려워요\r\n안 비비고는 못베길정도로",
          "original_form": "눈이 엄청 가려워요\r\n안 비비고는 못베길정도로",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005106.1.9",
          "form": "그리고 눈꼽이 엄청 많이 생겨서 다음날 일어날때 눈꼽이 찐득찐득 달라붙어서 눈이 안 떠져요 ㅎ..",
          "original_form": "그리고 눈꼽이 엄청 많이 생겨서 다음날 일어날때 눈꼽이 찐득찐득 달라붙어서 눈이 안 떠져요 ㅎ..",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005106.1.10",
          "form": "어머...",
          "original_form": "어머...",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005106.1.11",
          "form": "안약이나 약 잘 챙겨 드세요!!ㅠㅜ",
          "original_form": "안약이나 약 잘 챙겨 드세요!!ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005106.1.12",
          "form": "아직도 걸린 원인을 모르겠어서\r\n그 어느때보다 비누칠 열심히 하고 있슴다 ㅎ",
          "original_form": "아직도 걸린 원인을 모르겠어서\r\n그 어느때보다 비누칠 열심히 하고 있슴다 ㅎ",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005106.1.13",
          "form": "어두운 곳에서 전자기기 보는 것도 안좋대요\r\n안압이 엄청 높아진다고,,",
          "original_form": "어두운 곳에서 전자기기 보는 것도 안좋대요\r\n안압이 엄청 높아진다고,,",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005106.1.14",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005106.1.15",
          "form": "제가 그래서 안압이 높아요...",
          "original_form": "제가 그래서 안압이 높아요...",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005106.1.16",
          "form": "으악 지금도 어두운 곳에 계신 거 아니죠,,",
          "original_form": "으악 지금도 어두운 곳에 계신 거 아니죠,,",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005106.1.17",
          "form": "그래서 시력 검사겸 병원 갈 때마다 안압 체크해요..",
          "original_form": "그래서 시력 검사겸 병원 갈 때마다 안압 체크해요..",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005106.1.18",
          "form": "지금은 아니에요!!",
          "original_form": "지금은 아니에요!!",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005106.1.19",
          "form": "저도 간만에 안압 체크해봐야겠군요,,\r\n\r\n돌아올 수 없는 눈 건강과 치아 건강 넘나 중요..",
          "original_form": "저도 간만에 안압 체크해봐야겠군요,,\r\n\r\n돌아올 수 없는 눈 건강과 치아 건강 넘나 중요..",
          "speaker_id": "1",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005106.1.20",
          "form": "다행히 치아는 튼튼해요!",
          "original_form": "다행히 치아는 튼튼해요!",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005106.1.21",
          "form": "좀.. 이상한 자랑이지만..",
          "original_form": "좀.. 이상한 자랑이지만..",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005106.1.22",
          "form": "사랑니도 다 바르게 자라서",
          "original_form": "사랑니도 다 바르게 자라서",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005106.1.23",
          "form": "아유 완전 자랑이죠ㅋㅋ",
          "original_form": "아유 완전 자랑이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005106.1.24",
          "form": "안 빼도 된다고 하시더라구요!!",
          "original_form": "안 빼도 된다고 하시더라구요!!",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005106.1.25",
          "form": "와 세상부럽네요",
          "original_form": "와 세상부럽네요",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005106.1.26",
          "form": "저는 사랑니 4개 다 있습니다 ....",
          "original_form": "저는 사랑니 4개 다 있습니다 ....",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005106.1.27",
          "form": "4개다 바르데요!",
          "original_form": "4개다 바르데요!",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005106.1.28",
          "form": "그것도 다 뽑아야 하는...",
          "original_form": "그것도 다 뽑아야 하는...",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005106.1.29",
          "form": "어.... 화이팅 하세요...",
          "original_form": "어.... 화이팅 하세요...",
          "speaker_id": "2",
          "time": "20210821 20:42"
        }
      ]
    }
