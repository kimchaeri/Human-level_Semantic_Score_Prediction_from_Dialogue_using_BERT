      "utterance": [
        {
          "id": "MDRW2100037999.1.1",
          "form": "name2님 가실거에요?ㅋㅋㅋ",
          "original_form": "&name2&님 가실거에요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037999.1.2",
          "form": "쿠헹ㅜㅜㅋㅋㅋ name1님이랑 대화가 너무 잘통해서 일단 미뤘어요ㅋㅋㅋ",
          "original_form": "쿠헹ㅜㅜㅋㅋㅋ &name1&님이랑 대화가 너무 잘통해서 일단 미뤘어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037999.1.3",
          "form": "대화 잘 통하시는분 몇 없었는데..",
          "original_form": "대화 잘 통하시는분 몇 없었는데..",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037999.1.4",
          "form": "그러게요ㅠ",
          "original_form": "그러게요ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037999.1.5",
          "form": "이것까지만 하고 가요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이것까지만 하고 가요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037999.1.6",
          "form": "뭔가 섭섭하네요 ㅠㅠ",
          "original_form": "뭔가 섭섭하네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037999.1.7",
          "form": "오늘 머리 잘랐어요!",
          "original_form": "오늘 머리 잘랐어요!",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037999.1.8",
          "form": "그니까용ㅠ 흑",
          "original_form": "그니까용ㅠ 흑",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037999.1.9",
          "form": "오오오! 어떻게 자르셨어요?!",
          "original_form": "오오오! 어떻게 자르셨어요?!",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037999.1.10",
          "form": "거지존 단발이었는데",
          "original_form": "거지존 단발이었는데",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037999.1.11",
          "form": "더 짧게잘랗어용 ㅋㅋ",
          "original_form": "더 짧게잘랗어용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037999.1.12",
          "form": "오옹ㅋㅋ 단발! 전 단발 중학생 이후로 해본적이 없어 부럽네용ㅋㅋ",
          "original_form": "오옹ㅋㅋ 단발! 전 단발 중학생 이후로 해본적이 없어 부럽네용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037999.1.13",
          "form": "저도 그이후로없다가 첨했어요 ㅋ",
          "original_form": "저도 그이후로없다가 첨했어요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100037999.1.14",
          "form": "대박대박ㅋㅋ 그러셨구나",
          "original_form": "대박대박ㅋㅋ 그러셨구나",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037999.1.15",
          "form": "이맘때되면 단발하고싶어지는거 진짜 병이에오",
          "original_form": "이맘때되면 단발하고싶어지는거 진짜 병이에오",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037999.1.16",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037999.1.17",
          "form": "고민을 없애고 후회를 남겨요....ㅋㅋㅋ",
          "original_form": "고민을 없애고 후회를 남겨요....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100037999.1.18",
          "form": "엇ㅋㅋ 매번 여름은 어찌 그렇게 되는걸까요",
          "original_form": "엇ㅋㅋ 매번 여름은 어찌 그렇게 되는걸까요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100037999.1.19",
          "form": "여기부산인데 어제보다 많이 쌀쌀해졌어요",
          "original_form": "여기부산인데 어제보다 많이 쌀쌀해졌어요",
          "speaker_id": "1",
          "time": "20210902 06:33"
        },
        {
          "id": "MDRW2100037999.1.20",
          "form": "헉ㄷㄷ 비는 안와요??",
          "original_form": "헉ㄷㄷ 비는 안와요??",
          "speaker_id": "2",
          "time": "20210902 06:39"
        },
        {
          "id": "MDRW2100037999.1.21",
          "form": "비오고나서 선선해졌어요",
          "original_form": "비오고나서 선선해졌어요",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100037999.1.22",
          "form": "다행이에요!",
          "original_form": "다행이에요!",
          "speaker_id": "2",
          "time": "20210902 12:03"
        }
      ]
    }
