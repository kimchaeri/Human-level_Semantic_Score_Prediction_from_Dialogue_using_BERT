      "utterance": [
        {
          "id": "MDRW2100038116.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038116.1.2",
          "form": "네 좋아해요! 코로나 이후로는 못갔지만요...",
          "original_form": "네 좋아해요! 코로나 이후로는 못갔지만요...",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038116.1.3",
          "form": "최근에 간 나라가 어디에요?",
          "original_form": "최근에 간 나라가 어디에요?",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038116.1.4",
          "form": "전 몇년전 일본이 마지막이에요ㅋㅋㅋㅋㅋ",
          "original_form": "전 몇년전 일본이 마지막이에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100038116.1.5",
          "form": "어디 가셨나요",
          "original_form": "어디 가셨나요",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100038116.1.6",
          "form": "저도 몇년전 마카오가 마지막이네요ㅜㅠ",
          "original_form": "저도 몇년전 마카오가 마지막이네요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038116.1.7",
          "form": "일본도 또 가고싶은데..",
          "original_form": "일본도 또 가고싶은데..",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100038116.1.8",
          "form": "일본 요새는 많이 안가죠..!",
          "original_form": "일본 요새는 많이 안가죠..!",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038116.1.9",
          "form": "예전에는 유행 했었는데!!",
          "original_form": "예전에는 유행 했었는데!!",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038116.1.10",
          "form": "그쵸ㅜ근데 뭔가 가끔 그리워요ㅋㅋㅋ 일본 어디 가셨었어요?",
          "original_form": "그쵸ㅜ근데 뭔가 가끔 그리워요ㅋㅋㅋ 일본 어디 가셨었어요?",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038116.1.11",
          "form": "저 오사카요ㅋㅋㅋㅋ!",
          "original_form": "저 오사카요ㅋㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038116.1.12",
          "form": "유니버셜도 갔는데 좋았어요",
          "original_form": "유니버셜도 갔는데 좋았어요",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038116.1.13",
          "form": "오 저두 오사카 갔었는데 혼자갔거든요ㅋㅋㅋ",
          "original_form": "오 저두 오사카 갔었는데 혼자갔거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038116.1.14",
          "form": "유니버셜도 혼자가서 놀았어욬ㅋㅋ",
          "original_form": "유니버셜도 혼자가서 놀았어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:49"
        },
        {
          "id": "MDRW2100038116.1.15",
          "form": "우와 혼자요?",
          "original_form": "우와 혼자요?",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038116.1.16",
          "form": "대받 멋있어요ㅋㅋㅋㅋㅋ",
          "original_form": "대받 멋있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038116.1.17",
          "form": "네..혼자 칠면조다리 뜯고..",
          "original_form": "네..혼자 칠면조다리 뜯고..",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038116.1.18",
          "form": "일본 남학생들이 이상하게 쳐다보더라구요",
          "original_form": "일본 남학생들이 이상하게 쳐다보더라구요",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038116.1.19",
          "form": "혼자 여행간게.정말 멋있네요",
          "original_form": "혼자 여행간게.정말 멋있네요",
          "speaker_id": "2",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100038116.1.20",
          "form": "혼자 여행 해보고 싶어가지구ㅎㅎ오사카가 젤 만만하더라구요",
          "original_form": "혼자 여행 해보고 싶어가지구ㅎㅎ오사카가 젤 만만하더라구요",
          "speaker_id": "1",
          "time": "20210902 08:35"
        },
        {
          "id": "MDRW2100038116.1.21",
          "form": "전 그래도 해외는",
          "original_form": "전 그래도 해외는",
          "speaker_id": "2",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100038116.1.22",
          "form": "혼자 가기 무섭더라구요ㅋㅋㅋㅋ",
          "original_form": "혼자 가기 무섭더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:49"
        }
      ]
