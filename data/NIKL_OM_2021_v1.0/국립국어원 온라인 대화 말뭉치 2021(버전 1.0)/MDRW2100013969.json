        },
        {
          "id": "MDRW2100013969.1.2",
          "form": "ㅇㄴ 취업하셨어요?",
          "original_form": "ㅇㄴ 취업하셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013969.1.3",
          "form": "혹시 고졸이신가요 대졸이신가요",
          "original_form": "혹시 고졸이신가요 대졸이신가요",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013969.1.4",
          "form": "그것부터...알아가봐야..(",
          "original_form": "그것부터...알아가봐야..({emoji:☺}",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013969.1.5",
          "form": "고졸이요 대학안갔어요",
          "original_form": "고졸이요 대학안갔어요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013969.1.6",
          "form": "대학가셨나요",
          "original_form": "대학가셨나요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013969.1.7",
          "form": "일단 취업은했고",
          "original_form": "일단 취업은했고",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013969.1.8",
          "form": "일도하고있고",
          "original_form": "일도하고있고",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013969.1.9",
          "form": "그냥 그래요..ㅎ",
          "original_form": "그냥 그래요..ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013969.1.10",
          "form": "헉 그러면 바로 취업하셨군요??",
          "original_form": "헉 그러면 바로 취업하셨군요??",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013969.1.11",
          "form": "대박 멋있다",
          "original_form": "대박 멋있다",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013969.1.12",
          "form": "저는 취준하는데",
          "original_form": "저는 취준하는데",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.13",
          "form": "걍...걍....걍..쓰읍...그냥..살아요",
          "original_form": "걍...걍....걍..쓰읍...그냥..살아요",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.15",
          "form": "인생의 쓴 맛을 보고있어요",
          "original_form": "인생의 쓴 맛을 보고있어요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.16",
          "form": "어깨펴요",
          "original_form": "어깨펴요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.17",
          "form": "누구보다 멋진 인생을 살아갈 name1",
          "original_form": "누구보다 멋진 인생을 살아갈 &name1&",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.18",
          "form": "님",
          "original_form": "님",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013969.1.19",
          "form": "아나 이걸로 갑자기",
          "original_form": "아나 이걸로 갑자기",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.20",
          "form": "아 욕한거아님",
          "original_form": "아 욕한거아님",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.21",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.22",
          "form": "감동적인 분위기라니",
          "original_form": "감동적인 분위기라니",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.23",
          "form": "오타난거 알죠",
          "original_form": "오타난거 알죠",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.24",
          "form": "쓰읍...우선 알겠어여(?)",
          "original_form": "쓰읍...우선 알겠어여(?)",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.25",
          "form": "와 나 지금 너무 식겁함",
          "original_form": "와 나 지금 너무 식겁함",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.27",
          "form": "근데 저게 어케 보내진거지?!",
          "original_form": "근데 저게 어케 보내진거지?!",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.28",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.29",
          "form": "근데 먼저 취업하거",
          "original_form": "근데 먼저 취업하거",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.30",
          "form": "아쉬웠던 점은 없나요",
          "original_form": "아쉬웠던 점은 없나요",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013969.1.31",
          "form": "통장보면 아쉬움이 풀리시겠지만 그래도",
          "original_form": "통장보면 아쉬움이 풀리시겠지만 그래도",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013969.1.32",
          "form": "어.. 아무래도 남들 놀 때 일하는거",
          "original_form": "어.. 아무래도 남들 놀 때 일하는거",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013969.1.33",
          "form": "그게 좀 아쉽긴 하더라구요",
          "original_form": "그게 좀 아쉽긴 하더라구요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013969.1.34",
          "form": "대학은 별로 안부러운데",
          "original_form": "대학은 별로 안부러운데",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013969.1.35",
          "form": "그냥 뭐랄까",
          "original_form": "그냥 뭐랄까",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013969.1.36",
          "form": "인생이 돈이 다가 아닌느낌",
          "original_form": "인생이 돈이 다가 아닌느낌",
          "speaker_id": "2",
          "time": "20210825 16:44"
        }
      ]
