      "utterance": [
        {
          "id": "MDRW2100013969.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "speaker_id": "1",
