        },
        {
          "id": "MDRW2100046322.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.3",
          "form": "올드보이처럼 평생 한 메뉴만 먹을 수 있다면",
          "original_form": "올드보이처럼 평생 한 메뉴만 먹을 수 있다면",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.4",
          "form": "뭘 드실건가요",
          "original_form": "뭘 드실건가요",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.5",
          "form": "음.. 회요",
          "original_form": "음.. 회요",
          "speaker_id": "1",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.6",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.7",
          "form": "무슨 회를 제일 좋아하세요?",
          "original_form": "무슨 회를 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.8",
          "form": "돌돔요",
          "original_form": "돌돔요",
          "speaker_id": "1",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.9",
          "form": "고급 입맛이시군요..!",
          "original_form": "고급 입맛이시군요..!",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.11",
          "form": "저는 회를 잘 못먹지만",
          "original_form": "저는 회를 잘 못먹지만",
          "speaker_id": "2",
          "time": "20211020 13:57"
        },
        {
          "id": "MDRW2100046322.1.12",
          "form": "광어 지느러미는",
          "original_form": "광어 지느러미는",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.13",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.14",
          "form": "오독오독",
          "original_form": "오독오독",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.15",
          "form": "고소하죠 ㅎㅎ",
          "original_form": "고소하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.16",
          "form": "네네 ㅎㅎ 근데",
          "original_form": "네네 ㅎㅎ 근데",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.17",
          "form": "참치회는 진짜",
          "original_form": "참치회는 진짜",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.18",
          "form": "무슨맛으로 먹는지..",
          "original_form": "무슨맛으로 먹는지..",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.19",
          "form": "개인적으로는 잘 모르겠더라구요",
          "original_form": "개인적으로는 잘 모르겠더라구요",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.20",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.21",
          "form": "참치회도 잘드세요?",
          "original_form": "참치회도 잘드세요?",
          "speaker_id": "2",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.22",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "1",
          "time": "20211020 13:58"
        },
        {
          "id": "MDRW2100046322.1.23",
          "form": "님이 생각하시는 참치회의 매력은",
          "original_form": "님이 생각하시는 참치회의 매력은",
          "speaker_id": "2",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046322.1.24",
          "form": "무엇이죱?!",
          "original_form": "무엇이죱?!",
          "speaker_id": "2",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046322.1.25",
          "form": "고소하니 맛나던데요",
          "original_form": "고소하니 맛나던데요",
          "speaker_id": "1",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046322.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ",
          "speaker_id": "2",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046322.1.27",
          "form": "고소함..",
          "original_form": "고소함..",
          "speaker_id": "2",
          "time": "20211020 13:59"
        },
        {
          "id": "MDRW2100046322.1.28",
          "form": "다음엔 한 번 고소함을 잘 느껴보도록..",
          "original_form": "다음엔 한 번 고소함을 잘 느껴보도록..",
          "speaker_id": "2",
          "time": "20211020 13:59"
