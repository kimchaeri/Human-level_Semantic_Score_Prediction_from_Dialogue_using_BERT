      "utterance": [
        {
          "id": "MDRW2100045799.1.1",
          "form": "베리만 묵는데",
          "original_form": "베리만 묵는데",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045799.1.2",
          "form": "살이찐다니",
          "original_form": "살이찐다니",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045799.1.3",
          "form": "살이 쪄요 ㄷㄷ",
          "original_form": "살이 쪄요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045799.1.4",
          "form": "그래서 운동을 넣어놨나...",
          "original_form": "그래서 운동을 넣어놨나...",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045799.1.5",
          "form": "게임에서도 운동을 해야한다니요",
          "original_form": "게임에서도 운동을 해야한다니요",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045799.1.6",
          "form": "화가난다...",
          "original_form": "화가난다...",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045799.1.7",
          "form": "그래도 계절은 구현안한느낌",
          "original_form": "그래도 계절은 구현안한느낌",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045799.1.8",
          "form": "그래도 더위는느끼던데요",
          "original_form": "그래도 더위는느끼던데요",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045799.1.9",
          "form": "그럼 계절이 있을지도 ㄷㄷㄷ",
          "original_form": "그럼 계절이 있을지도 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045799.1.10",
          "form": "그러게요 ㅠㅜ",
          "original_form": "그러게요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045799.1.11",
          "form": "책읽기 바쁘시넹",
          "original_form": "책읽기 바쁘시넹",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045799.1.12",
          "form": "렙도올려야하고",
          "original_form": "렙도올려야하고",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045799.1.13",
          "form": "티비는 꺼진지 오래인가보ㅓ요",
          "original_form": "티비는 꺼진지 오래인가보ㅓ요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045799.1.14",
          "form": "천둥도치네요 ㄷㄷ",
          "original_form": "천둥도치네요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045799.1.15",
          "form": "감전도 될런가",
          "original_form": "감전도 될런가",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100045799.1.16",
          "form": "심상치않은 브금",
          "original_form": "심상치않은 브금",
