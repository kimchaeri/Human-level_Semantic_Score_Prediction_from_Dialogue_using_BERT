      "utterance": [
        {
          "id": "MDRW2100009115.1.1",
          "form": "그럼 새로운 카페 가보는 거 좋아하시나요?",
          "original_form": "그럼 새로운 카페 가보는 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009115.1.2",
          "form": "네 디저트 좋아해서요",
          "original_form": "네 디저트 좋아해서요",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009115.1.3",
          "form": "단 음식 좋아하나 보네요!",
          "original_form": "단 음식 좋아하나 보네요!",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009115.1.4",
          "form": "어떤 디저트를 제일 좋아하세요?",
          "original_form": "어떤 디저트를 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009115.1.5",
          "form": "네ㅋㅋ 빵 종류 완전 좋아해요",
          "original_form": "네ㅋㅋ 빵 종류 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009115.1.6",
          "form": "케이크요!",
          "original_form": "케이크요!",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009115.1.7",
          "form": "name1님은 디저트 좋아하세요?",
          "original_form": "&name1&님은 디저트 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009115.1.8",
          "form": "저는 파이처럼 겹겹이 쌓인 디저트 좋아해요",
          "original_form": "저는 파이처럼 겹겹이 쌓인 디저트 좋아해요",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009115.1.9",
          "form": "겉바속촉이 취향이거든요!!",
          "original_form": "겉바속촉이 취향이거든요!!",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009115.1.10",
          "form": "케이크라니 전 치즈케이크 제일 좋아해요 ㅋㅋㅋ",
          "original_form": "케이크라니 전 치즈케이크 제일 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009115.1.11",
          "form": "오 먹고 싶어졌어요",
          "original_form": "오 먹고 싶어졌어요",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009115.1.12",
          "form": "바스크 치즈케이크 저도 좋아해요",
          "original_form": "바스크 치즈케이크 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009115.1.13",
          "form": "아아에 치즈케잌 한조각이면 아주 극락이죠",
          "original_form": "아아에 치즈케잌 한조각이면 아주 극락이죠",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009115.1.14",
          "form": "투썸에 네모난 치즈 케이크 드셔보셨어요?",
          "original_form": "투썸에 네모난 치즈 케이크 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009115.1.15",
          "form": "안에 있는 딸기퓨레가 상큼해서 아아랑 잘 어울려요",
          "original_form": "안에 있는 딸기퓨레가 상큼해서 아아랑 잘 어울려요",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009115.1.16",
          "form": "전에 기프티콘 받아서 먹어본 적 있어요!",
          "original_form": "전에 기프티콘 받아서 먹어본 적 있어요!",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009115.1.17",
          "form": "맞아요 투썸이 디저트 참 잘해서 먹기 좋아요",
          "original_form": "맞아요 투썸이 디저트 참 잘해서 먹기 좋아요",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009115.1.18",
          "form": "그쵸 아이스박스도 좋아해요",
          "original_form": "그쵸 아이스박스도 좋아해요",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009115.1.19",
          "form": "예전에 흑임자케이크 먹어봤었는데 이거 진짜 맛있어요",
          "original_form": "예전에 흑임자케이크 먹어봤었는데 이거 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009115.1.20",
          "form": "조각으로는 없고 홀케이크로 파는데 많이 안 달아서 흡입할 수 있어요",
          "original_form": "조각으로는 없고 홀케이크로 파는데 많이 안 달아서 흡입할 수 있어요",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009115.1.21",
          "form": "홀케잌 이라니 생일 때 한번 도전해봐야 겠어요",
          "original_form": "홀케잌 이라니 생일 때 한번 도전해봐야 겠어요",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009115.1.22",
          "form": "저도 생일 때 먹었었어요ㅋㅋ",
          "original_form": "저도 생일 때 먹었었어요ㅋㅋ",
