        {
          "id": "MDRW2100041318.1.1",
          "form": "팩올렸네요~~",
          "original_form": "팩올렸네요~~",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041318.1.2",
          "form": "우와 시원하시죠",
          "original_form": "우와 시원하시죠",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041318.1.3",
          "form": "네..좋네요",
          "original_form": "네..좋네요",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041318.1.4",
          "form": "이게 귀찮아서 그렇지 하면 또 좋아요",
          "original_form": "이게 귀찮아서 그렇지 하면 또 좋아요",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041318.1.5",
          "form": "운동같아요 ㅋㅋ",
          "original_form": "운동같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041318.1.6",
          "form": "팩사다놓구귀찮다는핑계로",
          "original_form": "팩사다놓구귀찮다는핑계로",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041318.1.7",
          "form": "열심히해야되는디",
          "original_form": "열심히해야되는디",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041318.1.8",
          "form": "그쵸 살땐 마니 사는데",
          "original_form": "그쵸 살땐 마니 사는데",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041318.1.9",
          "form": "친오빠가 팩만드는회사다니는디",
          "original_form": "친오빠가 팩만드는회사다니는디",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041318.1.10",
          "form": "우오ㅓ 정말요",
          "original_form": "우오ㅓ 정말요",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041318.1.11",
          "form": "그럼 얻을수도있어요?",
          "original_form": "그럼 얻을수도있어요?",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041318.1.12",
          "form": "네..줘요",
          "original_form": "네..줘요",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041318.1.13",
          "form": "우와 좋으시겠다",
          "original_form": "우와 좋으시겠다",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041318.1.14",
          "form": "서울에있어서 지금은거의못봐요",
          "original_form": "서울에있어서 지금은거의못봐요",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041318.1.15",
          "form": "코로나로 만나기가",
          "original_form": "코로나로 만나기가",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041318.1.16",
          "form": "그렇군여ㅜㅠ 택배로 받아요 ㅋㅋㅋ",
          "original_form": "그렇군여ㅜㅠ 택배로 받아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100041318.1.17",
          "form": "언니면 택배로 보내줬을텐데",
