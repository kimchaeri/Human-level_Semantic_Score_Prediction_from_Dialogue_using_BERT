      "utterance": [
        {
          "id": "MDRW2100023636.1.1",
          "form": "한라산 안 힘드셧어요?",
          "original_form": "한라산 안 힘드셧어요?",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023636.1.2",
          "form": "12시간 걸렸어요 ㄷㄷ",
          "original_form": "12시간 걸렸어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100023636.1.3",
          "form": "아침 7시에 출발햇는데 해가 져서",
          "original_form": "아침 7시에 출발햇는데 해가 져서",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023636.1.4",
          "form": "위험하고 그랫어요",
          "original_form": "위험하고 그랫어요",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100023636.1.5",
          "form": "엄청 오래 걸리네요... \n힘든거도 엄청 힘들죠?",
          "original_form": "엄청 오래 걸리네요... \n힘든거도 엄청 힘들죠?",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023636.1.6",
          "form": "네 ㅋㅋㅋ 특히 전 비오는 날에 갔어서",
          "original_form": "네 ㅋㅋㅋ 특히 전 비오는 날에 갔어서",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023636.1.7",
          "form": "미끄러워서 힘들엇어요",
          "original_form": "미끄러워서 힘들엇어요",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100023636.1.8",
          "form": "아.. 비오는데요?",
          "original_form": "아.. 비오는데요?",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100023636.1.9",
          "form": "우비 같은거 쓰고요?",
          "original_form": "우비 같은거 쓰고요?",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100023636.1.10",
          "form": "네 우비쓰고 스틱 들고 다녀왔어요",
          "original_form": "네 우비쓰고 스틱 들고 다녀왔어요",
          "speaker_id": "2",
          "time": "20210830 13:36"
        },
        {
          "id": "MDRW2100023636.1.11",
          "form": "아.. 비온날 다음날 가도 땅 질척거려서 힘든데..\n우중등산은...",
          "original_form": "아.. 비온날 다음날 가도 땅 질척거려서 힘든데..\n우중등산은...",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023636.1.12",
          "form": "다음날 멀쩡히 살아계셧나요?",
          "original_form": "다음날 멀쩡히 살아계셧나요?",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100023636.1.13",
          "form": "앓아누웟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "앓아누웟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:05"
        },
        {
          "id": "MDRW2100023636.1.14",
          "form": "돌길이라 미끄럽더라구요",
          "original_form": "돌길이라 미끄럽더라구요",
          "speaker_id": "2",
          "time": "20210830 14:05"
        },
        {
          "id": "MDRW2100023636.1.15",
          "form": "원래 평소 운동 좋아하셔서 등산도 하시는거에요?",
          "original_form": "원래 평소 운동 좋아하셔서 등산도 하시는거에요?",
          "speaker_id": "1",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023636.1.16",
          "form": "운동을 워낙 안해서",
          "original_form": "운동을 워낙 안해서",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023636.1.17",
          "form": "의무감에..ㅋㅋㅋㅋ",
          "original_form": "의무감에..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023636.1.18",
          "form": "음.. 심오하네요.. ㅋㅋㅋㅋ",
          "original_form": "음.. 심오하네요.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023636.1.19",
          "form": "등산이 잘 맞앗던거긴하쥬? ㅋㅋㅋ",
          "original_form": "등산이 잘 맞앗던거긴하쥬? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023636.1.20",
          "form": "ㅋㅋㅋㅋ 등산 성취감이 있어요!",
          "original_form": "ㅋㅋㅋㅋ 등산 성취감이 있어요!",
          "speaker_id": "2",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023636.1.21",
          "form": "꼭대기 가면 성취감이 커서 중독적이에요",
          "original_form": "꼭대기 가면 성취감이 커서 중독적이에요",
          "speaker_id": "2",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023636.1.22",
          "form": "역시 정상에서 인증샷 찍는 맛이죠.. ㅋㅋㅋ",
          "original_form": "역시 정상에서 인증샷 찍는 맛이죠.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:03"
        }
      ]
    }
