      "utterance": [
        {
          "id": "MDRW2100038499.1.1",
          "form": "나는 랍스터",
          "original_form": "나는 랍스터",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100038499.1.2",
          "form": "오 ㅋㅋ 좋은 선택이네",
          "original_form": "오 ㅋㅋ 좋은 선택이네",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100038499.1.3",
          "form": "맛있을 듯",
          "original_form": "맛있을 듯",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038499.1.4",
          "form": "웅 근데 뭐든 배터지게 먹으면 맛을 잘 못느낄것같은데 ㅋㅋ",
          "original_form": "웅 근데 뭐든 배터지게 먹으면 맛을 잘 못느낄것같은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038499.1.5",
          "form": "음미하면서 먹어야지",
          "original_form": "음미하면서 먹어야지",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100038499.1.6",
          "form": "음 맛있게 먹기로 셀링 포인트를 ㅋ",
          "original_form": "음 맛있게 먹기로 셀링 포인트를 ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.7",
          "form": "그래 맛있게 먹어야지",
          "original_form": "그래 맛있게 먹어야지",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.8",
          "form": "웅웅 한번 도전해볼까 진짜",
          "original_form": "웅웅 한번 도전해볼까 진짜",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.9",
          "form": "됐다",
          "original_form": "됐다",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.10",
          "form": "ㅠㅠㅋㅋ 랍스터 많이 살 돈이 없구만",
          "original_form": "ㅠㅠㅋㅋ 랍스터 많이 살 돈이 없구만",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.11",
          "form": "가끔가다 먹어야 맛있지",
          "original_form": "가끔가다 먹어야 맛있지",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.12",
          "form": "구래 ㅠㅋ 한번에 많이 먹긴 힘들다",
          "original_form": "구래 ㅠㅋ 한번에 많이 먹긴 힘들다",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100038499.1.13",
          "form": "뷔페나 가자",
          "original_form": "뷔페나 가자",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100038499.1.14",
          "form": "웅 ㅜ먹방은 담에..",
          "original_form": "웅 ㅜ먹방은 담에..",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100038499.1.15",
          "form": "엄마 유튜브에 홍진경 나오는거 있는데 재밌다 ㅋㅋ",
