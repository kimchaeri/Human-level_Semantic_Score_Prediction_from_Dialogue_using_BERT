        },
        {
          "id": "MDRW2100037234.1.3",
          "form": "9내식당 아니야",
          "original_form": "9내식당 아니야",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037234.1.4",
          "form": "우리는 킥식이라 불러",
          "original_form": "우리는 킥식이라 불러",
          "speaker_id": "1",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037234.1.5",
          "form": "우와 신기하네 그게 뭐야",
          "original_form": "우와 신기하네 그게 뭐야",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037234.1.6",
          "form": "키스트 급식을 줄여서",
          "original_form": "키스트 급식을 줄여서",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037234.1.7",
          "form": "밥 맛있어?",
          "original_form": "밥 맛있어?",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037234.1.8",
          "form": "싸고 맛있어",
          "original_form": "싸고 맛있어",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037234.1.9",
          "form": "내일 사진 찍어서 보여줘",
          "original_form": "내일 사진 찍어서 보여줘",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037234.1.10",
          "form": "내일 불고기나왔으면 좋겠다",
          "original_form": "내일 불고기나왔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037234.1.11",
          "form": "식단을 미리 알 수가 있나",
          "original_form": "식단을 미리 알 수가 있나",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037234.1.12",
          "form": "모르는재미도있지",
          "original_form": "모르는재미도있지",
          "speaker_id": "1",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100037234.1.13",
          "form": "맛있는 거를 골라 먹어야",
          "original_form": "맛있는 거를 골라 먹어야",
          "speaker_id": "2",
          "time": "20210831 21:46"
