      "utterance": [
        {
          "id": "MDRW2100031336.1.1",
          "form": "안녕하세요? 여행 좋아하세요",
          "original_form": "안녕하세요? 여행 좋아하세요",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100031336.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031336.1.3",
          "form": "여행을 좋아해요! 자주는 못가는게 아쉽지만요",
          "original_form": "여행을 좋아해요! 자주는 못가는게 아쉽지만요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031336.1.4",
          "form": "name1님도 여행을 좋아하시나요?",
          "original_form": "&name1&님도 여행을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031336.1.5",
          "form": "그쵸 지금같은땐 더욱 그렇죠",
          "original_form": "그쵸 지금같은땐 더욱 그렇죠",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031336.1.6",
          "form": "네 저도 여행은 좋아해요 국내에선 어디가 좋으셨어요?",
          "original_form": "네 저도 여행은 좋아해요 국내에선 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031336.1.7",
          "form": "국내에서는 부산 여행이 제일 기억에 남아요",
          "original_form": "국내에서는 부산 여행이 제일 기억에 남아요",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100031336.1.8",
          "form": "와 부산 좋죠",
          "original_form": "와 부산 좋죠",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.9",
          "form": "저도 두번 가봤는데 두번 다 좋았어요",
          "original_form": "저도 두번 가봤는데 두번 다 좋았어요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.10",
          "form": "12월쯤에 가서 불빛 가득한 축제를 본적이 있었는데",
          "original_form": "12월쯤에 가서 불빛 가득한 축제를 본적이 있었는데",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.11",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.12",
          "form": "오 축제때 맞춰 가셨네요",
          "original_form": "오 축제때 맞춰 가셨네요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.13",
          "form": "name1님은 어디 여행이 가장 좋으셨나요?",
          "original_form": "&name1&님은 어디 여행이 가장 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.14",
          "form": "떨결에 맞춰갔어요 ㅋㅋ",
          "original_form": "떨결에 맞춰갔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.15",
          "form": "저는 통영도 좋았고요",
          "original_form": "저는 통영도 좋았고요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.16",
          "form": "오대산도 좋았어요",
          "original_form": "오대산도 좋았어요",
          "speaker_id": "1",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100031336.1.17",
          "form": "최근엔 원주 뮤지엄산 다녀왔는데 진짜 건물이 예쁘더라고요",
          "original_form": "최근엔 원주 뮤지엄산 다녀왔는데 진짜 건물이 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100031336.1.18",
          "form": "여행을 많이 다니시는군요!",
          "original_form": "여행을 많이 다니시는군요!",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031336.1.19",
          "form": "뮤지엄산?은 처음들어봐요!",
          "original_form": "뮤지엄산?은 처음들어봐요!",
          "speaker_id": "2",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031336.1.20",
          "form": "예쁜 건물이에요",
          "original_form": "예쁜 건물이에요",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031336.1.21",
          "form": "종이 박물관 등이 안에 있고 얼마전 마인 이라는 드라마에도 나왔었다고하더라고요",
          "original_form": "종이 박물관 등이 안에 있고 얼마전 마인 이라는 드라마에도 나왔었다고하더라고요",
          "speaker_id": "1",
          "time": "20210904 20:44"
        },
        {
          "id": "MDRW2100031336.1.22",
          "form": "아 그 드라마에 나왔었군요",
          "original_form": "아 그 드라마에 나왔었군요",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031336.1.23",
          "form": "미술관이 참 예쁘다고 생각했는데 같은 곳이었구나",
          "original_form": "미술관이 참 예쁘다고 생각했는데 같은 곳이었구나",
          "speaker_id": "2",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031336.1.24",
          "form": "네 누구나 찍는 그 입구에는 줄이 서있어요 사진찍으려고요 ㅎㅎ",
          "original_form": "네 누구나 찍는 그 입구에는 줄이 서있어요 사진찍으려고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:47"
        },
        {
          "id": "MDRW2100031336.1.25",
          "form": "ㅋㅋ 어떻게 알고 찾아가셨나요?",
          "original_form": "ㅋㅋ 어떻게 알고 찾아가셨나요?",
          "speaker_id": "2",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031336.1.26",
          "form": "지인 추천이요 골프장이 근처라 라운딩 갔다가 추천해주셨어요",
          "original_form": "지인 추천이요 골프장이 근처라 라운딩 갔다가 추천해주셨어요",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031336.1.27",
          "form": "와 골프치시는구나",
          "original_form": "와 골프치시는구나",
          "speaker_id": "2",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031336.1.28",
          "form": "저는 아니고 지인만요 ㅋㅋ",
          "original_form": "저는 아니고 지인만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:59"
        }
      ]
