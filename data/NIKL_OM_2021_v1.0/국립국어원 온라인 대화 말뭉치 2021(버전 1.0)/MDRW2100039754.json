      "utterance": [
        {
          "id": "MDRW2100039754.1.1",
          "form": "학교가기싫어요",
          "original_form": "학교가기싫어요",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039754.1.2",
          "form": "학교생활 어땠어?",
          "original_form": "학교생활 어땠어?",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039754.1.3",
          "form": "학교는 늘 가기싫어",
          "original_form": "학교는 늘 가기싫어",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039754.1.4",
          "form": "학교 아침에 일어나기 힘들었어",
          "original_form": "학교 아침에 일어나기 힘들었어",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039754.1.5",
          "form": "아침형인간이 아니라서",
          "original_form": "아침형인간이 아니라서",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039754.1.6",
          "form": "나는 기숙사에 살아서 조금 더 늦게 챙겨도 됐었어. 참 다행이었지..",
          "original_form": "나는 기숙사에 살아서 조금 더 늦게 챙겨도 됐었어. 참 다행이었지..",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039754.1.7",
          "form": "부럽다 하지만 기숙사에 살았어도 난 게을렀을거야",
          "original_form": "부럽다 하지만 기숙사에 살았어도 난 게을렀을거야",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039754.1.8",
          "form": "나도 게을렀어! 잠은 더 자고싶고 지각은 안되고 그래서 아침밥을 포기했지",
          "original_form": "나도 게을렀어! 잠은 더 자고싶고 지각은 안되고 그래서 아침밥을 포기했지",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039754.1.9",
          "form": "멋진걸:) 학생때였으면 더 배고팠을텐데!",
          "original_form": "멋진걸:) 학생때였으면 더 배고팠을텐데!",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039754.1.10",
          "form": "대신 간식이 있었으니까!",
          "original_form": "대신 간식이 있었으니까!",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039754.1.11",
          "form": "무슨 간식을 자주 먹었어?",
          "original_form": "무슨 간식을 자주 먹었어?",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039754.1.12",
          "form": "주로 매점빵이나 두유?",
          "original_form": "주로 매점빵이나 두유?",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039754.1.13",
          "form": "과자일줄알았는데ㅎㅎ 의외네",
          "original_form": "과자일줄알았는데ㅎㅎ 의외네",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039754.1.14",
          "form": "두유가 배부르니까..",
          "original_form": "두유가 배부르니까..",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039754.1.15",
          "form": "가성비 최고네",
          "original_form": "가성비 최고네",
          "speaker_id": "2",
          "time": "20210903 21:53"
        }
      ]
