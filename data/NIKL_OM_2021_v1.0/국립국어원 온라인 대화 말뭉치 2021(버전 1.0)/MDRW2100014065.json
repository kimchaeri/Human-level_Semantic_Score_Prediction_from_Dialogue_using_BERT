      "utterance": [
        {
          "id": "MDRW2100014065.1.1",
          "form": "그럼 약간",
          "original_form": "그럼 약간",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014065.1.2",
          "form": "힙합?",
          "original_form": "힙합?",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014065.1.3",
          "form": "바비같은?",
          "original_form": "바비같은?",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014065.1.4",
          "form": "으 그건 더 아닌데영",
          "original_form": "으 그건 더 아닌데영",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014065.1.5",
          "form": "그럼 누님 세대면",
          "original_form": "그럼 누님 세대면",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014065.1.6",
          "form": "버즈?",
          "original_form": "버즈?",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014065.1.7",
          "form": "바비는 제스타일 전혀 아니에여...",
          "original_form": "바비는 제스타일 전혀 아니에여...",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014065.1.8",
          "form": "오...버즈는 좋아했었는데 지 ㅣ금은 놉",
          "original_form": "오...버즈는 좋아했었는데 지 ㅣ금은 놉",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014065.1.9",
          "form": "예전에는 좋아하는 스타일이 비슷했네요",
          "original_form": "예전에는 좋아하는 스타일이 비슷했네요",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014065.1.10",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014065.1.11",
          "form": "강동원이나 민경훈 뭔가 비슷한 느낌",
          "original_form": "강동원이나 민경훈 뭔가 비슷한 느낌",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014065.1.12",
          "form": "강동원을 좋아한다구요?",
          "original_form": "강동원을 좋아한다구요?",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014065.1.13",
          "form": "예전에여",
          "original_form": "예전에여",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014065.1.14",
          "form": "약간 골든리트리버상 좋아하시나",
          "original_form": "약간 골든리트리버상 좋아하시나",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014065.1.15",
          "form": "첫 남친이 좀 저런스타일들이었고",
          "original_form": "첫 남친이 좀 저런스타일들이었고",
          "speaker_id": "2",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014065.1.16",
          "form": "아 박보검도 강동원 민경훈라인이려나",
          "original_form": "아 박보검도 강동원 민경훈라인이려나",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014065.1.17",
          "form": "박보검ㅋㅋㅋㅋㅋㅋ",
          "original_form": "박보검ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014065.1.18",
          "form": "차은우 좋아하시죠",
          "original_form": "차은우 좋아하시죠",
          "speaker_id": "1",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014065.1.19",
          "form": "지성도 참 좋아했는데",
          "original_form": "지성도 참 좋아했는데",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014065.1.20",
          "form": "차은우는 그냥 아 잘생겼구나 정도지 좋아하진않아요",
          "original_form": "차은우는 그냥 아 잘생겼구나 정도지 좋아하진않아요",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100014065.1.21",
          "form": "아 맞아 지성도 약간",
          "original_form": "아 맞아 지성도 약간",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014065.1.22",
          "form": "공유는 안좋아하십니까",
          "original_form": "공유는 안좋아하십니까",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014065.1.23",
          "form": "공유도 좋아하긴하지만 그런쪽으로 좋아하는건 아닌...?",
          "original_form": "공유도 좋아하긴하지만 그런쪽으로 좋아하는건 아닌...?",
          "speaker_id": "2",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100014065.1.24",
          "form": "아 찾았다",
          "original_form": "아 찾았다",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014065.1.25",
          "form": "송재림좋아한다했자나요",
          "original_form": "송재림좋아한다했자나요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014065.1.26",
          "form": "송재림 박시후",
          "original_form": "송재림 박시후",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014065.1.27",
          "form": "송재림 누군지 몰라서 까먹었어요ㅋㅋㅋㅋ",
          "original_form": "송재림 누군지 몰라서 까먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014065.1.28",
          "form": "박시후는 알아요",
          "original_form": "박시후는 알아요",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014065.1.29",
          "form": "실땅님 스타일 좋아하시네",
          "original_form": "실땅님 스타일 좋아하시네",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014065.1.30",
          "form": "아 또 있는데 비슷한 얼굴상",
          "original_form": "아 또 있는데 비슷한 얼굴상",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014065.1.31",
          "form": "환상속 실땅님",
          "original_form": "환상속 실땅님",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014065.1.32",
          "form": "실땅님",
          "original_form": "실땅님",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014065.1.33",
          "form": "아 주지훈도ㅋㅋ",
          "original_form": "아 주지훈도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:14"
        }
      ]
    }
