      "utterance": [
        {
          "id": "MDRW2100028090.1.1",
          "form": "저녁 뭐먹어",
          "original_form": "저녁 뭐먹어",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028090.1.2",
          "form": "난 알리오올리오 파스타먹음",
          "original_form": "난 알리오올리오 파스타먹음",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028090.1.3",
          "form": "나... 아마 떡볶이?",
          "original_form": "나... 아마 떡볶이?",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028090.1.4",
          "form": "뭐야 님만 먹나요",
          "original_form": "뭐야 님만 먹나요",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028090.1.5",
          "form": "어디꺼",
          "original_form": "어디꺼",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028090.1.6",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028090.1.7",
          "form": "몰라 누가 사왔음",
          "original_form": "몰라 누가 사왔음",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028090.1.8",
          "form": "님은 무슨 떡볶이 좋아하시나요",
          "original_form": "님은 무슨 떡볶이 좋아하시나요",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028090.1.9",
          "form": "무슨 떡볶이를 안 좋아하냐고 묻는 게 빠를 텐데",
          "original_form": "무슨 떡볶이를 안 좋아하냐고 묻는 게 빠를 텐데",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.10",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.11",
          "form": "그건 나도",
          "original_form": "그건 나도",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.12",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.13",
          "form": "근데 안 좋아하는 떡볶이 있나요",
          "original_form": "근데 안 좋아하는 떡볶이 있나요",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.14",
          "form": "아무래도 없는 편이죠",
          "original_form": "아무래도 없는 편이죠",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.15",
          "form": "아무래도 그렇지...",
          "original_form": "아무래도 그렇지...",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.16",
          "form": "우리 집 앞에 새로 떡볶이집 생겼는데",
          "original_form": "우리 집 앞에 새로 떡볶이집 생겼는데",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.17",
          "form": "난 로제는 내 돈 주고는 굳이 안 사먹는 듯",
          "original_form": "난 로제는 내 돈 주고는 굳이 안 사먹는 듯",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.18",
          "form": "거기 소스가 취향이 아니긴 해",
          "original_form": "거기 소스가 취향이 아니긴 해",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.19",
          "form": "아그래??",
          "original_form": "아그래??",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100028090.1.20",
          "form": "떡볶이의 바이브가 아니야",
          "original_form": "떡볶이의 바이브가 아니야",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028090.1.21",
          "form": "적폐다",
          "original_form": "적폐다",
          "speaker_id": "1",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028090.1.22",
          "form": "그건 파스타 아니냐고",
          "original_form": "그건 파스타 아니냐고",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100028090.1.23",
          "form": "#저기요",
          "original_form": "#저기요",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.24",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.25",
          "form": "너 크림떡볶이 먹어 안 먹어",
          "original_form": "너 크림떡볶이 먹어 안 먹어",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.26",
          "form": "내 돈 내고 안 먹지",
          "original_form": "내 돈 내고 안 먹지",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.27",
          "form": "아니이런적폐떡볶이러버를봣나.",
          "original_form": "아니이런적폐떡볶이러버를봣나.",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.28",
          "form": "사실 크림 파스타도 내 돈 주고 안 먹음",
          "original_form": "사실 크림 파스타도 내 돈 주고 안 먹음",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.29",
          "form": "네가그러고도떡볶이사랑맨이야?(그런말한적없음)",
          "original_form": "네가그러고도떡볶이사랑맨이야?(그런말한적없음)",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.30",
          "form": "진짜충격적이다",
          "original_form": "진짜충격적이다",
          "speaker_id": "1",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100028090.1.31",
          "form": "어이업내",
          "original_form": "어이업내",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.32",
          "form": "근데 나는 가리는 거 없으니까 파이팅",
          "original_form": "근데 나는 가리는 거 없으니까 파이팅",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.33",
          "form": "생각해보면 로제도 딱히 안 먹어서",
          "original_form": "생각해보면 로제도 딱히 안 먹어서",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.34",
          "form": "떡볶이다좋아",
          "original_form": "떡볶이다좋아",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.35",
          "form": "걍 그 장르를 싫어하는 걸지도",
          "original_form": "걍 그 장르를 싫어하는 걸지도",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.36",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.37",
          "form": "그수잇그수잇",
          "original_form": "그수잇그수잇",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100028090.1.38",
          "form": "로제파스타도... 별로야",
          "original_form": "로제파스타도... 별로야",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028090.1.39",
          "form": "오일이 좋다",
          "original_form": "오일이 좋다",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100028090.1.40",
          "form": "너의취향을존중한다",
          "original_form": "너의취향을존중한다",
          "speaker_id": "1",
          "time": "20210902 17:36"
        }
      ]
    }
