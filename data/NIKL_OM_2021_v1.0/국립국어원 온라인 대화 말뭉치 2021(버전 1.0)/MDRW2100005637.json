      "utterance": [
        {
          "id": "MDRW2100005637.1.1",
          "form": "공연 전시회..",
          "original_form": "공연 전시회..",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005637.1.2",
          "form": "난 별로 가본 적이 업다",
          "original_form": "난 별로 가본 적이 업다",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005637.1.3",
          "form": "당신은 있는가",
          "original_form": "당신은 있는가",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005637.1.4",
          "form": "전시회? 오타쿠 시절 홈마 전시회가 끝인뎁쇼",
          "original_form": "전시회? 오타쿠 시절 홈마 전시회가 끝인뎁쇼",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005637.1.5",
          "form": "연극 본 건 있는가",
          "original_form": "연극 본 건 있는가",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005637.1.6",
          "form": "연극?",
          "original_form": "연극?",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005637.1.7",
          "form": "중딩 때 학교 연극동아리에서 하는거 보러간 적은 잇다",
          "original_form": "중딩 때 학교 연극동아리에서 하는거 보러간 적은 잇다",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005637.1.8",
          "form": "생각해보니 전시회는 몇번 가본 듯",
          "original_form": "생각해보니 전시회는 몇번 가본 듯",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005637.1.9",
          "form": "오 나도 중학생 때 학교 축제에서 연극부 애들이 연극하는 거 본 적 있음",
          "original_form": "오 나도 중학생 때 학교 축제에서 연극부 애들이 연극하는 거 본 적 있음",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005637.1.10",
          "form": "체육관 강당에 전교생 모여가지고 옹기종기 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "체육관 강당에 전교생 모여가지고 옹기종기 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005637.1.11",
          "form": "재밌었다",
          "original_form": "재밌었다",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005637.1.12",
          "form": "난.. 그때 김도앞에 소극장까지 갔었다",
          "original_form": "난.. 그때 김도앞에 소극장까지 갔었다",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005637.1.13",
          "form": "학교가 수업빼면서까지 보러 가라고 해서 갔던 기억이..",
          "original_form": "학교가 수업빼면서까지 보러 가라고 해서 갔던 기억이..",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005637.1.14",
          "form": "너네 학교에서 김도까지면 꽤 먼데",
          "original_form": "너네 학교에서 김도까지면 꽤 먼데",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005637.1.15",
          "form": "나는 연지공원 시민체육관...",
          "original_form": "나는 연지공원 시민체육관...",
          "speaker_id": "1",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005637.1.16",
          "form": "나 affiliation이어서 그리 멀진 않앗다",
          "original_form": "나 &affiliation&이어서 그리 멀진 않앗다",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005637.1.17",
          "form": "연지공원 시민체육관...",
          "original_form": "연지공원 시민체육관...",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005637.1.18",
          "form": "거기 무대가 있었나",
          "original_form": "거기 무대가 있었나",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005637.1.19",
          "form": "그냥 낮은 단 있고",
          "original_form": "그냥 낮은 단 있고",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005637.1.20",
          "form": "강당보다는 단이 더 낮은데",
          "original_form": "강당보다는 단이 더 낮은데",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005637.1.21",
          "form": "그거 무대로 쓰고 학생들은 바닥에 앉ㄱ",
          "original_form": "그거 무대로 쓰고 학생들은 바닥에 앉ㄱ",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005637.1.22",
          "form": "앉아서 구경하고",
          "original_form": "앉아서 구경하고",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005637.1.23",
          "form": "무슨 내용이었는지는 기억 안나는데",
          "original_form": "무슨 내용이었는지는 기억 안나는데",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005637.1.24",
          "form": "연극부는 열심히 공연했던 거 같다..",
          "original_form": "연극부는 열심히 공연했던 거 같다..",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005637.1.25",
          "form": "나도 연극 내용은 기억 안 나는데",
          "original_form": "나도 연극 내용은 기억 안 나는데",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005637.1.26",
          "form": "걔네는 열심히 공연하고 나는 재밌게 보고...",
          "original_form": "걔네는 열심히 공연하고 나는 재밌게 보고...",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005637.1.27",
          "form": "친구가 그때 연극부였어서 더 재밌게 봤던 듯",
          "original_form": "친구가 그때 연극부였어서 더 재밌게 봤던 듯",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005637.1.28",
          "form": "친구가 연극부라니..",
          "original_form": "친구가 연극부라니..",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005637.1.29",
          "form": "둘다 중딩 때 본게 마지막 연극인 게",
          "original_form": "둘다 중딩 때 본게 마지막 연극인 게",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005637.1.30",
          "form": "놀랍군",
          "original_form": "놀랍군",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005637.1.31",
          "form": "어 고딩 때 다른 데서 연극 공연 하지 않았나",
          "original_form": "어 고딩 때 다른 데서 연극 공연 하지 않았나",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005637.1.32",
          "form": "뮤지컬같은 것도 했던 것 같기도",
          "original_form": "뮤지컬같은 것도 했던 것 같기도",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005637.1.33",
          "form": "아님 말고..",
          "original_form": "아님 말고..",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005637.1.34",
          "form": "고딩 때 했었지",
          "original_form": "고딩 때 했었지",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005637.1.35",
          "form": "1학년 때 부산으로 소풍갔는데",
          "original_form": "1학년 때 부산으로 소풍갔는데",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005637.1.36",
          "form": "그때 연극 봤었지",
          "original_form": "그때 연극 봤었지",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005637.1.37",
          "form": "넌... 안 갔던 걸로 기억하지만...",
          "original_form": "넌... 안 갔던 걸로 기억하지만...",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005637.1.38",
          "form": "1학년은... 갔다",
          "original_form": "1학년은... 갔다",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005637.1.39",
          "form": "2학년을...",
          "original_form": "2학년을...",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005637.1.40",
          "form": "지금 생각해도 화나네",
          "original_form": "지금 생각해도 화나네",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005637.1.41",
          "form": "어잠만 나 고딩때 연극 본 적 있었네",
          "original_form": "어잠만 나 고딩때 연극 본 적 있었네",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005637.1.42",
          "form": "같은 연극을 두번 봤다..",
          "original_form": "같은 연극을 두번 봤다..",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005637.1.43",
          "form": "뭐야... 바본가?",
          "original_form": "뭐야... 바본가?",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005637.1.44",
          "form": "근데 기억 안 날 만 하지",
          "original_form": "근데 기억 안 날 만 하지",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005637.1.45",
          "form": "그때가 몇년전인데...",
          "original_form": "그때가 몇년전인데...",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005637.1.46",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005637.1.47",
          "form": "대학탐방가서 봣엇네",
          "original_form": "대학탐방가서 봣엇네",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005637.1.48",
          "form": "까먹을만함",
          "original_form": "까먹을만함",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005637.1.49",
          "form": "아 서울 가서?",
          "original_form": "아 서울 가서?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005637.1.50",
          "form": "오래 됐네...",
          "original_form": "오래 됐네...",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005637.1.51",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005637.1.52",
          "form": "근데 중딩 때 본 건 기억한다는게",
          "original_form": "근데 중딩 때 본 건 기억한다는게",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005637.1.53",
          "form": "아이러니하지만",
          "original_form": "아이러니하지만",
          "speaker_id": "2",
          "time": "20210821 23:57"
        }
      ]
