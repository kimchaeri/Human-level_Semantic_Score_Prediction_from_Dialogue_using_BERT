        },
        {
          "id": "MDRW2100026541.1.2",
          "form": "반려동물키우세요?",
          "original_form": "반려동물키우세요?",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026541.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026541.1.4",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026541.1.5",
          "form": "그럼 키우고 싶은 반려동물은 있으네요?",
          "original_form": "그럼 키우고 싶은 반려동물은 있으네요?",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026541.1.6",
          "form": "고양이 키우고싶어요 ㅠ",
          "original_form": "고양이 키우고싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026541.1.7",
          "form": "하지만 집에선 어머니가 알레르기셔서 ㅠㅠ",
          "original_form": "하지만 집에선 어머니가 알레르기셔서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026541.1.8",
          "form": "동물은 불가하겠죠 ㅜㅜ",
          "original_form": "동물은 불가하겠죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026541.1.9",
          "form": "아이고ㅠ 정말 알레르기가 문제에요",
          "original_form": "아이고ㅠ 정말 알레르기가 문제에요",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026541.1.10",
          "form": "고양이들 키우면 종종 애교부리는게 너무 귀엽더라고요",
          "original_form": "고양이들 키우면 종종 애교부리는게 너무 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026541.1.11",
          "form": "ㅠㅠ그쵸",
          "original_form": "ㅠㅠ그쵸",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026541.1.12",
          "form": "키우다가 생기는 경우도 있대요",
          "original_form": "키우다가 생기는 경우도 있대요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026541.1.13",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026541.1.14",
          "form": "대신 랜선집사합니다",
          "original_form": "대신 랜선집사합니다",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026541.1.15",
          "form": "아니 키우다가 없어지는것도 아니고",
          "original_form": "아니 키우다가 없어지는것도 아니고",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026541.1.16",
          "form": "생긴다고요?!?!",
          "original_form": "생긴다고요?!?!",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026541.1.17",
          "form": "네 키우다가 알레르기가 생긴대요",
          "original_form": "네 키우다가 알레르기가 생긴대요",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026541.1.18",
          "form": "정말 알레르기는 나쁘네요...",
          "original_form": "정말 알레르기는 나쁘네요...",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026541.1.19",
          "form": "저도 유튜브로나 인스타로 랜선집사해요",
          "original_form": "저도 유튜브로나 인스타로 랜선집사해요",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026541.1.20",
          "form": "좋아요 ㅎㅎ 저고 인스타도 봐요!",
          "original_form": "좋아요 ㅎㅎ 저고 인스타도 봐요!",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026541.1.21",
          "form": "랜덤으로 띄워주는거 보는데 볼때마다 너무 귀여워요",
          "original_form": "랜덤으로 띄워주는거 보는데 볼때마다 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026541.1.22",
          "form": "맞아요 ㅠㅠㅠ그래서 더 키우고싶어요",
          "original_form": "맞아요 ㅠㅠㅠ그래서 더 키우고싶어요",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100026541.1.23",
          "form": "진짜 보면 키우고 싶은마음이 계속 드는데 현실은 랜선집사..ㅠㅠ",
          "original_form": "진짜 보면 키우고 싶은마음이 계속 드는데 현실은 랜선집사..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026541.1.24",
          "form": "그초.. 저 하나도 책임지기 힘들어요 ㅠ",
          "original_form": "그초.. 저 하나도 책임지기 힘들어요 ㅠ",
