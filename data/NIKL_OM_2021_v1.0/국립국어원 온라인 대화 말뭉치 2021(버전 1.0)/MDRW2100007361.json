      "utterance": [
        {
          "id": "MDRW2100007361.1.1",
          "form": "네네 맞아요 ㅎㅎ",
          "original_form": "네네 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007361.1.2",
          "form": "오 이것도 우지님 곡",
          "original_form": "오 이것도 우지님 곡",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007361.1.3",
          "form": "세븐틴 노래는",
          "original_form": "세븐틴 노래는",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007361.1.4",
          "form": "거의 90퍼가",
          "original_form": "거의 90퍼가",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007361.1.5",
          "form": "우지 작곡이예요",
          "original_form": "우지 작곡이예요",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007361.1.6",
          "form": "우리 천재 작곡가..",
          "original_form": "우리 천재 작곡가..",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007361.1.7",
          "form": "진짜 천재신 것 같아요",
          "original_form": "진짜 천재신 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007361.1.8",
          "form": "소나기도 너무 좋아해요!!",
          "original_form": "소나기도 너무 좋아해요!!",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007361.1.9",
          "form": "헉 소나기 작곡한것도 아시는구나",
          "original_form": "헉 소나기 작곡한것도 아시는구나",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007361.1.10",
          "form": "너무 명곡이예요...",
          "original_form": "너무 명곡이예요...",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007361.1.11",
          "form": "저 입덕 초기에",
          "original_form": "저 입덕 초기에",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007361.1.12",
          "form": "소나기 우지 단독작사인 거 알았을때",
          "original_form": "소나기 우지 단독작사인 거 알았을때",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007361.1.13",
          "form": "진짜 뒤집어졌잔ㄹ아요..",
          "original_form": "진짜 뒤집어졌잔ㄹ아요..",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007361.1.14",
          "form": "어떻게 이런 가사를 쓰냐며",
          "original_form": "어떻게 이런 가사를 쓰냐며{emoji:…}",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007361.1.15",
          "form": "저는 처음에 우지님이 쓰셨다길래 노래 나오기 전부터 기대하고 있었는데",
          "original_form": "저는 처음에 우지님이 쓰셨다길래 노래 나오기 전부터 기대하고 있었는데",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007361.1.16",
          "form": "나오고 듣자마자 와 진짜... 좋을 거라 생각은 했지만 진짜 너무 좋아서 한동안 계속 들었어요",
          "original_form": "나오고 듣자마자 와 진짜... 좋을 거라 생각은 했지만 진짜 너무 좋아서 한동안 계속 들었어요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007361.1.17",
          "form": "진짜 너무 좋죠 ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "진짜 너무 좋죠 ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007361.1.18",
          "form": "진짜 눈물 나는 노래...",
          "original_form": "진짜 눈물 나는 노래...",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007361.1.19",
          "form": "지금같은 새벽에도 듣기 좋구요 ㅠㅠ",
          "original_form": "지금같은 새벽에도 듣기 좋구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007361.1.20",
          "form": "맞아요! 생각난 김에 듣다가 자야겠어요",
          "original_form": "맞아요! 생각난 김에 듣다가 자야겠어요",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007361.1.21",
          "form": "좋아요ㅠㅠ 저도 들어야겠어요ㅎㅎ",
          "original_form": "좋아요ㅠㅠ 저도 들어야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007361.1.22",
          "form": "오늘 마침 비도 와서 딱 좋네요",
          "original_form": "오늘 마침 비도 와서 딱 좋네요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007361.1.23",
          "form": "지금은 그쳤지만",
          "original_form": "지금은 그쳤지만",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007361.1.24",
          "form": "name1님 사시는 지역도 오늘 비왔나요?",
          "original_form": "&name1&님 사시는 지역도 오늘 비왔나요?",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007361.1.25",
          "form": "네! 오늘 비 되게 많이 오더라구요",
          "original_form": "네! 오늘 비 되게 많이 오더라구요",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007361.1.26",
          "form": "저희도 오전에 엄청 쏟아지더라구요 ㅋㅋㅋ",
          "original_form": "저희도 오전에 엄청 쏟아지더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007361.1.27",
          "form": "그쵸 이런 날은 집에만 있어야 하는데!",
          "original_form": "그쵸 이런 날은 집에만 있어야 하는데!",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100007361.1.28",
          "form": "맞아요 ㅋㅋㅋ 전 그래서 하루종일 집에 있었어요",
          "original_form": "맞아요 ㅋㅋㅋ 전 그래서 하루종일 집에 있었어요",
          "speaker_id": "2",
