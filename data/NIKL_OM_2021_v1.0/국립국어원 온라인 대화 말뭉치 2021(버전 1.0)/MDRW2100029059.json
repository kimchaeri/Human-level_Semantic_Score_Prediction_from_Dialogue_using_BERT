      "utterance": [
        {
          "id": "MDRW2100029059.1.1",
          "form": "반려동물 있으신가요?",
          "original_form": "반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029059.1.2",
          "form": "저는 있었슴니댜",
          "original_form": "저는 있었슴니댜",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029059.1.3",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029059.1.4",
          "form": "아 저도 있긴 했는데...",
          "original_form": "아 저도 있긴 했는데...",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029059.1.5",
          "form": "나중에 또 키울 생각 있으신가요...?",
          "original_form": "나중에 또 키울 생각 있으신가요...?",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029059.1.6",
          "form": "앗 혹시 그 목이 길고...",
          "original_form": "앗 혹시 그 목이 길고...",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029059.1.7",
          "form": "아직은 없슴니댜",
          "original_form": "아직은 없슴니댜",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029059.1.8",
          "form": "앗 아닙니다 저는 햄스터를 키웠어요 예전에",
          "original_form": "앗 아닙니다 저는 햄스터를 키웠어요 예전에",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.9",
          "form": "앗 햄스터",
          "original_form": "앗 햄스터",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.10",
          "form": "오오 이름이 뭐였나요",
          "original_form": "오오 이름이 뭐였나요",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.11",
          "form": "수명이 짧아서 안타깝죠",
          "original_form": "수명이 짧아서 안타깝죠",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.12",
          "form": "name3랑  name4입니다",
          "original_form": "&name3&랑  &name4&입니다",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.13",
          "form": "햄찌 이름이 다 그렇죠",
          "original_form": "햄찌 이름이 다 그렇죠",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.14",
          "form": "와 클래식한 이름인데오",
          "original_form": "와 클래식한 이름인데오",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.15",
          "form": "햄찌들 넘 귀여운데 무서워요",
          "original_form": "햄찌들 넘 귀여운데 무서워요",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029059.1.16",
          "form": "키우고 싶은 동물 있나요?",
          "original_form": "키우고 싶은 동물 있나요?",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.17",
          "form": "그쵸 막 무니까요",
          "original_form": "그쵸 막 무니까요",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.18",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.19",
          "form": "요새는 페럿이요",
          "original_form": "요새는 페럿이요",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.20",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.21",
          "form": "저는 읍어요",
          "original_form": "저는 읍어요",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.22",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029059.1.23",
          "form": "카피바라 키워보고 싶어요",
          "original_form": "카피바라 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029059.1.24",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029059.1.25",
          "form": "카피바라 순둥하대요",
          "original_form": "카피바라 순둥하대요",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029059.1.26",
          "form": "아이스크림 나눠먹을 수 있다던데",
          "original_form": "아이스크림 나눠먹을 수 있다던데",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029059.1.27",
          "form": "저는 그 당근 먹는 모습에 반했어오",
          "original_form": "저는 그 당근 먹는 모습에 반했어오",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
