      "utterance": [
        {
          "id": "MDRW2100024169.1.1",
          "form": "커피 자주 드세요??",
          "original_form": "커피 자주 드세요??",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024169.1.2",
          "form": "가끔 카페가면 먹어요",
          "original_form": "가끔 카페가면 먹어요",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024169.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024169.1.4",
          "form": "저는 일하는 날엔 거의 매일 먹고",
          "original_form": "저는 일하는 날엔 거의 매일 먹고",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024169.1.5",
          "form": "아닌날엔 일부러 덜 먹으려고 해요ㅠ",
          "original_form": "아닌날엔 일부러 덜 먹으려고 해요ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024169.1.6",
          "form": "맞아요 뭔가 일하면",
          "original_form": "맞아요 뭔가 일하면",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024169.1.7",
          "form": "카페인이 땡기는것 같아요",
          "original_form": "카페인이 땡기는것 같아요",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100024169.1.8",
          "form": "어떤 커피 좋아하세요??",
          "original_form": "어떤 커피 좋아하세요??",
          "speaker_id": "1",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100024169.1.9",
          "form": "저는 크림라떼요 ㅎㅎ",
          "original_form": "저는 크림라떼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100024169.1.10",
          "form": "아 크림라떼도 맛있죠!!",
          "original_form": "아 크림라떼도 맛있죠!!",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100024169.1.11",
          "form": "커피 배달로도 드시나요?",
          "original_form": "커피 배달로도 드시나요?",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024169.1.12",
          "form": "아니요 ㅋㅋㅋ왠지",
          "original_form": "아니요 ㅋㅋㅋ왠지",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024169.1.13",
          "form": "녹을까봐요",
          "original_form": "녹을까봐요",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024169.1.14",
          "form": "금방 안녹으려나요??",
          "original_form": "금방 안녹으려나요??",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024169.1.15",
          "form": "아 녹기도 하겠어요!",
          "original_form": "아 녹기도 하겠어요!",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024169.1.16",
          "form": "저희동네는 커피 배달료가 5천원이라 시켜먹은적 없어요ㅠ",
          "original_form": "저희동네는 커피 배달료가 5천원이라 시켜먹은적 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100024169.1.17",
          "form": "맞아요 커피만 몇만원치 시키지않고서야..",
          "original_form": "맞아요 커피만 몇만원치 시키지않고서야..",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100024169.1.18",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100024169.1.19",
          "form": "내일 아침은 어떻게 하실 생각인가요??",
          "original_form": "내일 아침은 어떻게 하실 생각인가요??",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100024169.1.20",
          "form": "ㅋㅋㅋ낼은 김치찌개 먹으려구요",
          "original_form": "ㅋㅋㅋ낼은 김치찌개 먹으려구요",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100024169.1.21",
          "form": "저는 아마 순대국 먹을 것 같아요~!",
          "original_form": "저는 아마 순대국 먹을 것 같아요~!",
