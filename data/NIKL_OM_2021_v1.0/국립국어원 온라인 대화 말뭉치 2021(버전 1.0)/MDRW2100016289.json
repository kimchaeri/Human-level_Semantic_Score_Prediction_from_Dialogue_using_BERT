      "utterance": [
        {
          "id": "MDRW2100016289.1.1",
          "form": "유튭 보시나요",
          "original_form": "유튭 보시나요",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016289.1.2",
          "form": "네 유튜브 보죠 ㅋㅋ 어떤거 보세요?",
          "original_form": "네 유튜브 보죠 ㅋㅋ 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016289.1.3",
          "form": "여행유튭이요",
          "original_form": "여행유튭이요",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016289.1.4",
          "form": "여행유튜브좋죠",
          "original_form": "여행유튜브좋죠",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100016289.1.5",
          "form": "저는 재테크,줌바 유튜브요ㅋ",
          "original_form": "저는 재테크,줌바 유튜브요ㅋ",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016289.1.6",
          "form": "오 추천하실만한거있나요",
          "original_form": "오 추천하실만한거있나요",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016289.1.7",
          "form": "추천까지는 아니고ㅋㅋ 이거 저거 보고있어요ㅋ",
          "original_form": "추천까지는 아니고ㅋㅋ 이거 저거 보고있어요ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016289.1.8",
          "form": "재테크면 신사임당 채널 이런건가요",
          "original_form": "재테크면 신사임당 채널 이런건가요",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016289.1.9",
          "form": "네 그런거요ㅋㅋ",
          "original_form": "네 그런거요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100016289.1.10",
          "form": "도움이 좀 되시나요",
          "original_form": "도움이 좀 되시나요",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100016289.1.11",
          "form": "네 저는 주식,공모주, 제테크 이런거 보고있어요. 도움 많이되서요.ㅋ",
          "original_form": "네 저는 주식,공모주, 제테크 이런거 보고있어요. 도움 많이되서요.ㅋ",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100016289.1.12",
          "form": "저도카뱅 첨으로 공모주해봤네요",
          "original_form": "저도카뱅 첨으로 공모주해봤네요",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100016289.1.13",
          "form": "네 저도 한지 얼마안됐어요.",
          "original_form": "네 저도 한지 얼마안됐어요.",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100016289.1.14",
          "form": "유튜브로 많이 배우고있어요ㅋ",
          "original_form": "유튜브로 많이 배우고있어요ㅋ",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100016289.1.15",
          "form": "찾아봐야겠어요!",
          "original_form": "찾아봐야겠어요!",
          "speaker_id": "1",
          "time": "20210828 17:05"
        }
      ]
    }
