      "utterance": [
        {
          "id": "MDRW2100040473.1.1",
          "form": "네 폭립이랑 피자 먹었어요",
          "original_form": "네 폭립이랑 피자 먹었어요",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040473.1.2",
          "form": "오 맛있었겠네요",
          "original_form": "오 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100040473.1.3",
          "form": "저는 집에 늙은호박이 생겨서",
          "original_form": "저는 집에 늙은호박이 생겨서",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040473.1.4",
          "form": "그걸로 전 부쳐먹었어요",
          "original_form": "그걸로 전 부쳐먹었어요",
          "speaker_id": "1",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040473.1.5",
          "form": "호박전 맛있죠! 요즘 늙은호박이 싸더라구요 저희집에도 두개 사놨어요",
          "original_form": "호박전 맛있죠! 요즘 늙은호박이 싸더라구요 저희집에도 두개 사놨어요",
          "speaker_id": "2",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100040473.1.6",
          "form": "호박죽 호박전 부쳐먹으려고요",
          "original_form": "호박죽 호박전 부쳐먹으려고요",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040473.1.7",
          "form": "오 그러셨구나",
          "original_form": "오 그러셨구나",
          "speaker_id": "1",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040473.1.8",
          "form": "전 선물받은 늙은호박이 꽤 커서",
          "original_form": "전 선물받은 늙은호박이 꽤 커서",
          "speaker_id": "1",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040473.1.9",
          "form": "며칠동안 먹어야할 것 같아요",
          "original_form": "며칠동안 먹어야할 것 같아요",
          "speaker_id": "1",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040473.1.10",
          "form": "늙은호박전 경상도에서만 부쳐먹는대요 알고 계셨나요?",
          "original_form": "늙은호박전 경상도에서만 부쳐먹는대요 알고 계셨나요?",
          "speaker_id": "2",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100040473.1.11",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100040473.1.12",
          "form": "전 호박 어떻게 해먹나 찾다가 발견한 요리인데",
          "original_form": "전 호박 어떻게 해먹나 찾다가 발견한 요리인데",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100040473.1.13",
          "form": "네 인터넷에서 그런글을 봤거든요 신기했어요 늙은호박전 진짜 맛있는데 다른지역에선 첨 본다고 그러더라구요",
          "original_form": "네 인터넷에서 그런글을 봤거든요 신기했어요 늙은호박전 진짜 맛있는데 다른지역에선 첨 본다고 그러더라구요",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100040473.1.14",
          "form": "아 저도 이번에 처음 해먹었어요",
          "original_form": "아 저도 이번에 처음 해먹었어요",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100040473.1.15",
          "form": "맛있던데요ㅎㅎ",
          "original_form": "맛있던데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100040473.1.16",
          "form": "그죠 ㅋㅋㅋ 전 호박죽보다 호박전이 더 맛있더라구요",
          "original_form": "그죠 ㅋㅋㅋ 전 호박죽보다 호박전이 더 맛있더라구요",
          "speaker_id": "2",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100040473.1.17",
          "form": "네 달달하고 쫀득하고",
          "original_form": "네 달달하고 쫀득하고",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100040473.1.18",
          "form": "네네 너무 맛있죠!!",
          "original_form": "네네 너무 맛있죠!!",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100040473.1.19",
          "form": "갑자기 호박전 먹고싶어지네요",
          "original_form": "갑자기 호박전 먹고싶어지네요",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100040473.1.20",
          "form": "ㅎㅎ저희집엔 지금 넘쳐나는데",
          "original_form": "ㅎㅎ저희집엔 지금 넘쳐나는데",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100040473.1.21",
          "form": "가져다 드리고 싶네요",
          "original_form": "가져다 드리고 싶네요",
          "speaker_id": "1",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100040473.1.22",
          "form": "감사합니다 ㅋㅋㅋ",
          "original_form": "감사합니다 ㅋㅋㅋ",
          "speaker_id": "2",
