      "utterance": [
        {
          "id": "MDRW2100017128.1.1",
          "form": "사고 싶은 물건 있으신가요? 저는 식기세척기요!",
          "original_form": "사고 싶은 물건 있으신가요? 저는 식기세척기요!",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017128.1.2",
          "form": "와 식기세척기 완전 사고싶어요",
          "original_form": "와 식기세척기 완전 사고싶어요",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017128.1.3",
          "form": "삶의 질이 달라진다던데",
          "original_form": "삶의 질이 달라진다던데",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017128.1.4",
          "form": "저는 LP판 테이블 사고싶어요!",
          "original_form": "저는 LP판 테이블 사고싶어요!",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017128.1.5",
          "form": "와 LP판 멋지네요",
          "original_form": "와 LP판 멋지네요",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100017128.1.6",
          "form": "분위기 있을 것 같아요 ㅎㅎ",
          "original_form": "분위기 있을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100017128.1.7",
          "form": "맞아요 분위기 내려고 하는거죠 하하",
          "original_form": "맞아요 분위기 내려고 하는거죠 하하",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100017128.1.8",
          "form": "문제는 LP판을 많이 사면 어디에 둬야 할지..",
          "original_form": "문제는 LP판을 많이 사면 어디에 둬야 할지..",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100017128.1.9",
          "form": "그게 문제네요 하하",
          "original_form": "그게 문제네요 하하",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100017128.1.10",
          "form": "보관할 곳을 따로 마련해야겠군요ㅋㅋ",
          "original_form": "보관할 곳을 따로 마련해야겠군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100017128.1.11",
          "form": "저는 결국 식기세척기 샀어요ㅋㅋㅋㅋ",
          "original_form": "저는 결국 식기세척기 샀어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100017128.1.12",
          "form": "우와??? 뭐에요 사고 싶다고 말 하시더니",
          "original_form": "우와??? 뭐에요 사고 싶다고 말 하시더니",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100017128.1.13",
          "form": "바로 사셨네요 ㅋㅋㅋㅋㅋ",
          "original_form": "바로 사셨네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100017128.1.14",
          "form": "이제 설거지 시대는 끝났군요",
          "original_form": "이제 설거지 시대는 끝났군요",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100017128.1.15",
          "form": "ㅋㅋㅋㅋㅋ 너무 좋아요! 다 넣고 버튼 하나만 누르면 끝이니 너무 행복하네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 너무 좋아요! 다 넣고 버튼 하나만 누르면 끝이니 너무 행복하네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100017128.1.16",
          "form": "와..저 자취하면 그거 꼭 장만합니다 진짜",
          "original_form": "와..저 자취하면 그거 꼭 장만합니다 진짜",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100017128.1.17",
          "form": "저는 블루트스 마이크 사고 싶더라고요",
          "original_form": "저는 블루트스 마이크 사고 싶더라고요",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100017128.1.18",
          "form": "좋은거 아는거 있으신가요?",
          "original_form": "좋은거 아는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100017128.1.19",
          "form": "저는 마이크 사고 싶단 생각을 한 번도 안해봐서 아쉽게도 잘 모르겠네유ㅋㅋㅋㅋ",
          "original_form": "저는 마이크 사고 싶단 생각을 한 번도 안해봐서 아쉽게도 잘 모르겠네유ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100017128.1.20",
          "form": "마이크 사서 신나는 시간 보내시겠군요ㅋㅋ",
          "original_form": "마이크 사서 신나는 시간 보내시겠군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100017128.1.21",
          "form": "헐 그렇군요",
          "original_form": "헐 그렇군요",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100017128.1.22",
          "form": "네 있으면 좋죠 ㅎㅎ",
          "original_form": "네 있으면 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100017128.1.23",
          "form": "당근마켓 이용하시나요?",
          "original_form": "당근마켓 이용하시나요?",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100017128.1.24",
          "form": "저는 안하는데 주변에서 꽤 하더군요",
          "original_form": "저는 안하는데 주변에서 꽤 하더군요",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100017128.1.25",
          "form": "저 당근으로 많이 팔아봤어요ㅋㅋㅋ",
          "original_form": "저 당근으로 많이 팔아봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100017128.1.26",
          "form": "아이디어가 참 좋은 것 같아요 당신 근처의 마켓이라고 이름도 당근마켓으로 짓고 아이디어가 대박이에요",
          "original_form": "아이디어가 참 좋은 것 같아요 당신 근처의 마켓이라고 이름도 당근마켓으로 짓고 아이디어가 대박이에요",
          "speaker_id": "1",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100017128.1.27",
          "form": "그러니까요!! 완전 천재",
          "original_form": "그러니까요!! 완전 천재",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100017128.1.28",
          "form": "우와 많이 파셨구나....",
          "original_form": "우와 많이 파셨구나....",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100017128.1.29",
          "form": "집에 안쓰는데 괜찮은 물건 많아서 팔까 고민했거든요",
          "original_form": "집에 안쓰는데 괜찮은 물건 많아서 팔까 고민했거든요",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100017128.1.30",
          "form": "이게 팔릴까 싶은데 일단 등록해두면 팔려요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이게 팔릴까 싶은데 일단 등록해두면 팔려요ㅋㅋㅋㅋㅋㅋ",
