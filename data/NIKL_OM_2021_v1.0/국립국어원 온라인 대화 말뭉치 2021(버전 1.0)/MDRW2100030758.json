      "utterance": [
        {
          "id": "MDRW2100030758.1.1",
          "form": "인정입니다",
          "original_form": "인정입니다",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030758.1.2",
          "form": "안 그래도 제 휴대폰이 요즘 배터리가 빨리 닳아서 바꾸고 싶은데",
          "original_form": "안 그래도 제 휴대폰이 요즘 배터리가 빨리 닳아서 바꾸고 싶은데",
          "speaker_id": "2",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030758.1.3",
          "form": "사용하신지 얼마나 됐나요",
          "original_form": "사용하신지 얼마나 됐나요",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.4",
          "form": "요즘 휴대폰 가격이 워낙 비싸다 보니 망설여지네요",
          "original_form": "요즘 휴대폰 가격이 워낙 비싸다 보니 망설여지네요",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.5",
          "form": "갤럭시 노트가 좋긴하던데",
          "original_form": "갤럭시 노트가 좋긴하던데",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.6",
          "form": "2년은 넘었고 2년 반 정도 된 것 같네요",
          "original_form": "2년은 넘었고 2년 반 정도 된 것 같네요",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.7",
          "form": "펜이 있으니까 편해요",
          "original_form": "펜이 있으니까 편해요",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.8",
          "form": "저도 조금 더 기다렸다가 노트 시리즈 나오면 그때 바꿀까 생각 중이에요",
          "original_form": "저도 조금 더 기다렸다가 노트 시리즈 나오면 그때 바꿀까 생각 중이에요",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.9",
          "form": "S 펜이 크게 나와서 훨씬 편하더라고요",
          "original_form": "S 펜이 크게 나와서 훨씬 편하더라고요",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.10",
          "form": "노트 10 저렴해요",
          "original_form": "노트 10 저렴해요",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.11",
          "form": "둘레가 엣지 있는게 좀 불편해요",
          "original_form": "둘레가 엣지 있는게 좀 불편해요",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.12",
          "form": "태블릿 S 펜도 휴대폰에 인식이 되다 보니 따로 S 펜 긴 글 쓰는게 좋아요",
          "original_form": "태블릿 S 펜도 휴대폰에 인식이 되다 보니 따로 S 펜 긴 글 쓰는게 좋아요",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030758.1.13",
          "form": "저는 엣지 기능 잘 써서 괜찮아요",
          "original_form": "저는 엣지 기능 잘 써서 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.14",
          "form": "게임 할 때 클릭이 잘",
          "original_form": "게임 할 때 클릭이 잘",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.15",
          "form": "그거 고정으로 해 놓고 색깔 연하게 바꿔 놓으세요",
          "original_form": "그거 고정으로 해 놓고 색깔 연하게 바꿔 놓으세요",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.16",
          "form": "저는 a 시리즈에요",
          "original_form": "저는 a 시리즈에요",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.17",
          "form": "안 움직이게 고정 해놓으면 좋습니다",
          "original_form": "안 움직이게 고정 해놓으면 좋습니다",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.18",
          "form": "S 시리즈가 무선 충전이 되어서 편해요",
          "original_form": "S 시리즈가 무선 충전이 되어서 편해요",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.19",
          "form": "남편 거니까 알아서 하겠죠",
          "original_form": "남편 거니까 알아서 하겠죠",
          "speaker_id": "1",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100030758.1.20",
          "form": "A 시리즈도 가격대비 괜찮아요",
