      "utterance": [
        {
          "id": "MDRW2100008692.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008692.1.2",
          "form": "아니요 학생이세요?",
          "original_form": "아니요 학생이세요?",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008692.1.3",
          "form": "학교 다녀요",
          "original_form": "학교 다녀요",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008692.1.4",
          "form": "ㅡㅜ 내일개학",
          "original_form": "ㅡㅜ 내일개학",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008692.1.5",
          "form": "요즘 온라인 수업 아닌가봐요ㅜ",
          "original_form": "요즘 온라인 수업 아닌가봐요ㅜ",
          "speaker_id": "2",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008692.1.6",
          "form": "아항.... 안해봤어요ㅋㅋ",
          "original_form": "아항.... 안해봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008692.1.7",
          "form": "졸업 언제하셨어요?",
          "original_form": "졸업 언제하셨어요?",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008692.1.8",
          "form": "저는 30대라 한참 전에 졸업을 ㅋㅋ",
          "original_form": "저는 30대라 한참 전에 졸업을 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008692.1.9",
          "form": "다시 학생이고파요ㅋㅋㅋ",
          "original_form": "다시 학생이고파요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008692.1.10",
          "form": "헉! 30대 여 이신가용",
          "original_form": "헉! 30대 여 이신가용",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008692.1.11",
          "form": "어 뭐 그렇게 되네요? ㅋㅋ",
          "original_form": "어 뭐 그렇게 되네요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008692.1.12",
          "form": "ㅋㅋ별로 어른 된것같지는 않지만요",
          "original_form": "ㅋㅋ별로 어른 된것같지는 않지만요",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008692.1.13",
          "form": "헉...저도 삼십대인데 학교다녀요ㅜ",
          "original_form": "헉...저도 삼십대인데 학교다녀요ㅜ",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008692.1.14",
          "form": "오 어느 학과세요?",
          "original_form": "오 어느 학과세요?",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008692.1.15",
          "form": "그건은 창피합니다ㅜㅜ",
          "original_form": "그건은 창피합니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008692.1.16",
          "form": "학교 어느과졸업하셨어요?",
          "original_form": "학교 어느과졸업하셨어요?",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008692.1.17",
          "form": "저는 간호대학 편입 생각하고 있어요",
          "original_form": "저는 간호대학 편입 생각하고 있어요",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008692.1.18",
          "form": "삼십대에 대학원도 아니고 학부 생활이라니",
          "original_form": "삼십대에 대학원도 아니고 학부 생활이라니",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008692.1.19",
          "form": "겁도 나네요",
          "original_form": "겁도 나네요",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008692.1.20",
          "form": "엇 그래도 새출발 결심만으로",
          "original_form": "엇 그래도 새출발 결심만으로",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008692.1.21",
          "form": "대단하신듯요",
          "original_form": "대단하신듯요",
          "speaker_id": "1",
          "time": "20210822 18:29"
        }
      ]
