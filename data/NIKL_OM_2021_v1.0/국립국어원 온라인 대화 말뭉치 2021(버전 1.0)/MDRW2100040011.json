      "utterance": [
        {
          "id": "MDRW2100040011.1.1",
          "form": "주말 날씨 넘 조을거같아요",
          "original_form": "주말 날씨 넘 조을거같아요",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040011.1.2",
          "form": "맞아요 어디 드라이브라도 나가고 싶어요",
          "original_form": "맞아요 어디 드라이브라도 나가고 싶어요",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040011.1.3",
          "form": "정말 가을이네요",
          "original_form": "정말 가을이네요",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100040011.1.4",
          "form": "어디 안가시나용",
          "original_form": "어디 안가시나용",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100040011.1.5",
          "form": "아직 계획은 없어요",
          "original_form": "아직 계획은 없어요",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100040011.1.6",
          "form": "밤산책 하기도 넘 조은 날씨에용",
          "original_form": "밤산책 하기도 넘 조은 날씨에용",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100040011.1.7",
          "form": "이젠 밤산책 할거면",
          "original_form": "이젠 밤산책 할거면",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100040011.1.8",
          "form": "긴팔 긴바지 입어야겠어요",
          "original_form": "긴팔 긴바지 입어야겠어요",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100040011.1.9",
          "form": "ㅋㅋㅋ 넹 머라도 걸치고 . .",
          "original_form": "ㅋㅋㅋ 넹 머라도 걸치고 . .",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100040011.1.10",
          "form": "비도 거의 그쳤나봐요",
          "original_form": "비도 거의 그쳤나봐요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100040011.1.11",
          "form": "예보갠찬네요",
          "original_form": "예보갠찬네요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100040011.1.12",
          "form": "네 장마도 갔죠ㅎㅎ",
          "original_form": "네 장마도 갔죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100040011.1.13",
          "form": "가을 조아하세요?",
          "original_form": "가을 조아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100040011.1.14",
          "form": "조아요ㅎㅎ 더운거보단 추운걸 좋아해서요",
          "original_form": "조아요ㅎㅎ 더운거보단 추운걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100040011.1.15",
          "form": "좋네요 드뎌 안덥고",
          "original_form": "좋네요 드뎌 안덥고",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100040011.1.16",
          "form": "맞아요 지금이 천국입니다",
          "original_form": "맞아요 지금이 천국입니다",
          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100040011.1.17",
          "form": "낼은 외출해야겟어요",
          "original_form": "낼은 외출해야겟어요",
          "speaker_id": "1",
