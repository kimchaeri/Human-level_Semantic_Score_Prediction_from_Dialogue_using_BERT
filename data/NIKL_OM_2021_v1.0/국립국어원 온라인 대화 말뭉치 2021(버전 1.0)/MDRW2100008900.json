      "utterance": [
        {
          "id": "MDRW2100008900.1.1",
          "form": "각본도 딱 꼬투리 안잡힐 느낌 ㅎㅎ",
          "original_form": "각본도 딱 꼬투리 안잡힐 느낌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008900.1.2",
          "form": "요샌 좀 지나간영화 폰으로보는것도 나쁘지않더라고요",
          "original_form": "요샌 좀 지나간영화 폰으로보는것도 나쁘지않더라고요",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008900.1.3",
          "form": "대기업맛 ㅋㅋ",
          "original_form": "대기업맛 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008900.1.4",
          "form": "크루엘라 좀 실망이네요ㅋㅋ",
          "original_form": "크루엘라 좀 실망이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008900.1.5",
          "form": "딱 평타만치자 한건가",
          "original_form": "딱 평타만치자 한건가",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008900.1.6",
          "form": "그러기엔 기대치가 좀 높았는데 ㅋㅋ",
          "original_form": "그러기엔 기대치가 좀 높았는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008900.1.7",
          "form": "앜ㅋㅋㅋ 근데 그게 되게 괜찮았어요 모두를 만족시킬만한..",
          "original_form": "앜ㅋㅋㅋ 근데 그게 되게 괜찮았어요 모두를 만족시킬만한..",
          "speaker_id": "1",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008900.1.8",
          "form": "그리고 주인공이 되게 개성있고 매력적이에요",
          "original_form": "그리고 주인공이 되게 개성있고 매력적이에요",
          "speaker_id": "1",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008900.1.9",
          "form": "왓챠 같은데 찾아봐야겠어요",
          "original_form": "왓챠 같은데 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100008900.1.10",
          "form": "근데 그영화는 비추였어요",
          "original_form": "근데 그영화는 비추였어요",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008900.1.11",
          "form": "아 그런가요 전 재밌게ㅐ보긴 했어요 ㅎㅎ 그거 보고 들떠서",
          "original_form": "아 그런가요 전 재밌게ㅐ보긴 했어요 ㅎㅎ 그거 보고 들떠서",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008900.1.12",
          "form": "여성 빌런 영화 보고싶어서 버즈오브프레이도 집에 와서 봤었어요 ㅎㅎ",
          "original_form": "여성 빌런 영화 보고싶어서 버즈오브프레이도 집에 와서 봤었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008900.1.13",
          "form": "제가 비추한다고했던건 아깐 제목 기억이 안나서",
          "original_form": "제가 비추한다고했던건 아깐 제목 기억이 안나서",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100008900.1.14",
          "form": "급 검색했어요",
          "original_form": "급 검색했어요",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100008900.1.15",
          "form": "인더하이츠요",
          "original_form": "인더하이츠요",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100008900.1.16",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008900.1.17",
          "form": "저도 지금 검색하고왔어요",
          "original_form": "저도 지금 검색하고왔어요",
          "speaker_id": "1",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008900.1.18",
          "form": "되게 흔한 뮤지컬영화일거같네요",
          "original_form": "되게 흔한 뮤지컬영화일거같네요",
          "speaker_id": "1",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100008900.1.19",
          "form": "되게 졸렸어요",
          "original_form": "되게 졸렸어요",
          "speaker_id": "2",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100008900.1.20",
          "form": "태어나서 처음으로 영화보다가 숙면하고왔어요 ㄷㄷ",
          "original_form": "태어나서 처음으로 영화보다가 숙면하고왔어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100008900.1.21",
          "form": "뮤지컬영화 보다가 자기도 쉽지않은뎈ㅋㅋㅋ",
          "original_form": "뮤지컬영화 보다가 자기도 쉽지않은뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100008900.1.22",
          "form": "어지간히 지루한가봐요",
          "original_form": "어지간히 지루한가봐요",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100008900.1.23",
          "form": "심지어 노래도 취향이 아니었어요",
          "original_form": "심지어 노래도 취향이 아니었어요",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100008900.1.24",
          "form": "심지어 지금 내용도 잘기억 안날정도 ㄷㄷㄷ",
          "original_form": "심지어 지금 내용도 잘기억 안날정도 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100008900.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 최악중의 최악인가봐욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 최악중의 최악인가봐욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100008900.1.26",
          "form": "그렇게 비추할만한 영화 저는 생각이 안나서 아쉬워욬ㅋㅋㅋ",
          "original_form": "그렇게 비추할만한 영화 저는 생각이 안나서 아쉬워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100008900.1.27",
          "form": "웃긴건 지금도 잊혀지지않는게",
          "original_form": "웃긴건 지금도 잊혀지지않는게",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008900.1.28",
          "form": "영화관에 5명있었는데",
          "original_form": "영화관에 5명있었는데",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008900.1.29",
          "form": "최소 3명은 잤어요 ㅋㅋ",
          "original_form": "최소 3명은 잤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008900.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뮤지컬영화가 사람을 그렇게 많이 재우는것도 대단하네욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뮤지컬영화가 사람을 그렇게 많이 재우는것도 대단하네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100008900.1.31",
          "form": "저도 예전에 누가 코골면서 잤던 영화 있는데 제목이 생각안나네욬ㅋㅋ ㅠㅠ",
          "original_form": "저도 예전에 누가 코골면서 잤던 영화 있는데 제목이 생각안나네욬ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100008900.1.32",
          "form": "두명은 제뒤에있어 안봤지만",
          "original_form": "두명은 제뒤에있어 안봤지만",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008900.1.33",
          "form": "제앞의 커플은 서로기대서 자고있더라고요",
          "original_form": "제앞의 커플은 서로기대서 자고있더라고요",
          "speaker_id": "2",
          "time": "20210822 22:00"
        },
        {
          "id": "MDRW2100008900.1.34",
          "form": "진짜 신선한 기억이네요 ㅋㅋ",
          "original_form": "진짜 신선한 기억이네요 ㅋㅋ",
