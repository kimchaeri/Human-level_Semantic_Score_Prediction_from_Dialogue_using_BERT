      "utterance": [
        {
          "id": "MDRW2100021278.1.1",
          "form": "저녁은 뭐드시나요?",
          "original_form": "저녁은 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021278.1.2",
          "form": "밥에 있는 반찬이랑 김이요 ㅋㅋㅋ",
          "original_form": "밥에 있는 반찬이랑 김이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021278.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021278.1.4",
          "form": "저도 아직 정하진 않앗어요",
          "original_form": "저도 아직 정하진 않앗어요",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021278.1.5",
          "form": "전 정했어요 ㅠ 밥이랑 김이요 ㅋㅋㅋ",
          "original_form": "전 정했어요 ㅠ 밥이랑 김이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021278.1.6",
          "form": "ㅋㅋ가볍게 먹고좋네요",
          "original_form": "ㅋㅋ가볍게 먹고좋네요",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021278.1.7",
          "form": "님도 오늘은 가볍게 ㄱㄱ해보세요",
          "original_form": "님도 오늘은 가볍게 ㄱㄱ해보세요",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021278.1.8",
          "form": "전 주말은 그래도 맛있는거요ㅎㅎ",
          "original_form": "전 주말은 그래도 맛있는거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021278.1.9",
          "form": "하긴 그것도 그렇네요 전 디저트로 치즈케이크를 먹을거라서",
          "original_form": "하긴 그것도 그렇네요 전 디저트로 치즈케이크를 먹을거라서",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021278.1.10",
          "form": "밥은 적게 먹을거에요",
          "original_form": "밥은 적게 먹을거에요",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021278.1.11",
          "form": "아ㅎㅎ 합리적이네요",
          "original_form": "아ㅎㅎ 합리적이네요",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021278.1.12",
          "form": "밥도 디저트도 다 챙겨먹자니 기력이 달려서,,",
          "original_form": "밥도 디저트도 다 챙겨먹자니 기력이 달려서,,",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021278.1.13",
          "form": "오늘 디저트는 뭘 드세요?",
          "original_form": "오늘 디저트는 뭘 드세요?",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021278.1.14",
          "form": "전 좀전에 수박을 먹엇더니 배불러서",
          "original_form": "전 좀전에 수박을 먹엇더니 배불러서",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021278.1.15",
          "form": "저녁을 멀 먹나 고민중이에요",
          "original_form": "저녁을 멀 먹나 고민중이에요",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021278.1.16",
          "form": "이미 디저트를 드셨군요! 그치만 수박은 대부분 물이니깐 늦더라도 잘 챙겨드세요 ㅋㅋ",
          "original_form": "이미 디저트를 드셨군요! 그치만 수박은 대부분 물이니깐 늦더라도 잘 챙겨드세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021278.1.17",
          "form": "ㅎㅎ네 소화되야 맛있는걸 먹을텐데요",
          "original_form": "ㅎㅎ네 소화되야 맛있는걸 먹을텐데요",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021278.1.18",
          "form": "그동안 맛있는 메뉴가 떠오르시면 저에게도 살짝 알려주세요 ㅋㅋ",
          "original_form": "그동안 맛있는 메뉴가 떠오르시면 저에게도 살짝 알려주세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:34"
        }
      ]
