      "utterance": [
        {
          "id": "MDRW2100000763.1.1",
          "form": "뮤지컬은 화려하고 재밌었던것같은데",
          "original_form": "뮤지컬은 화려하고 재밌었던것같은데",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000763.1.2",
          "form": "레미제라블 보셨습니까",
          "original_form": "레미제라블 보셨습니까",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000763.1.3",
          "form": "아 당연하죠",
          "original_form": "아 당연하죠",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000763.1.4",
          "form": "노래 띵곡입니다",
          "original_form": "노래 띵곡입니다",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000763.1.5",
          "form": "이거 저도 제출 된건가용",
          "original_form": "이거 저도 제출 된건가용",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100000763.1.6",
          "form": "네. 레미제라블은 진짜 노래들이",
          "original_form": "네. 레미제라블은 진짜 노래들이",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000763.1.7",
          "form": "다 명곡이죠",
          "original_form": "다 명곡이죠",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000763.1.8",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000763.1.9",
          "form": "저번 연휴때 여행같은거 다녀오셨나요",
          "original_form": "저번 연휴때 여행같은거 다녀오셨나요",
          "speaker_id": "2",
          "time": "20210819 19:12"
        },
        {
          "id": "MDRW2100000763.1.10",
          "form": "여행은 시기가 시기이니만큼 못갔습니다",
          "original_form": "여행은 시기가 시기이니만큼 못갔습니다",
          "speaker_id": "1",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000763.1.11",
          "form": "연말엔 꼭 가고싶네요",
          "original_form": "연말엔 꼭 가고싶네요",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000763.1.12",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000763.1.13",
          "form": "빨리 끝나야할텐데",
          "original_form": "빨리 끝나야할텐데",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000763.1.14",
          "form": "바닷가로 가고싶네요",
          "original_form": "바닷가로 가고싶네요",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000763.1.15",
          "form": "바닷가 하면 부산이죠",
          "original_form": "바닷가 하면 부산이죠",
          "speaker_id": "1",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000763.1.16",
          "form": "여름에 부산 여행을 가야되는데",
          "original_form": "여름에 부산 여행을 가야되는데",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000763.1.17",
          "form": "하 부산가서",
          "original_form": "하 부산가서",
          "speaker_id": "2",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000763.1.18",
          "form": "회랑",
