      "utterance": [
        {
          "id": "MDRW2100037252.1.1",
          "form": "자기보다 모자란 남자가 자기한테 평생",
          "original_form": "자기보다 모자란 남자가 자기한테 평생",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037252.1.2",
          "form": "네 글고 잘난 남자는 약간 멍한 여자 좋아하니까 못만나고",
          "original_form": "네 글고 잘난 남자는 약간 멍한 여자 좋아하니까 못만나고",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037252.1.3",
          "form": "잘해줄거라고 착각하고",
          "original_form": "잘해줄거라고 착각하고",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037252.1.4",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037252.1.5",
          "form": "잘난 남자는 멍한 여자를 가스라이팅하려고",
          "original_form": "잘난 남자는 멍한 여자를 가스라이팅하려고",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.6",
          "form": "그냥 혼자사는게 나아요 그럴바엔요",
          "original_form": "그냥 혼자사는게 나아요 그럴바엔요",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.7",
          "form": "추구하는거죠",
          "original_form": "추구하는거죠",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.8",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.9",
          "form": "남자들이 어린 여자 좋아하는 이유가",
          "original_form": "남자들이 어린 여자 좋아하는 이유가",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.10",
          "form": "외모뿐만이 아니예요",
          "original_form": "외모뿐만이 아니예요",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.11",
          "form": "자기들 입맛대로 조종할 수 있어서죠",
          "original_form": "자기들 입맛대로 조종할 수 있어서죠",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.12",
          "form": "맞아요 그거에요",
          "original_form": "맞아요 그거에요",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.13",
          "form": "딱 그겁니다!!!",
          "original_form": "딱 그겁니다!!!",
          "speaker_id": "1",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.14",
          "form": "근데 또 연상 좋아한다고 해도 좋게 보이지가 않아욬ㅋㅋㅋㅋㅋ",
          "original_form": "근데 또 연상 좋아한다고 해도 좋게 보이지가 않아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.15",
          "form": "하 이러나 저러나",
          "original_form": "하 이러나 저러나",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037252.1.16",
          "form": "의도가 순수해보이지 않아서....",
          "original_form": "의도가 순수해보이지 않아서....",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.17",
          "form": "네 그건 또 그거대로 문제있죠",
          "original_form": "네 그건 또 그거대로 문제있죠",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.18",
          "form": "근데 name1님 부모님은 결혼하라고 압박 안하시나요?",
          "original_form": "근데 &name1&님 부모님은 결혼하라고 압박 안하시나요?",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.19",
          "form": "알면 못만나요",
          "original_form": "알면 못만나요",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.20",
          "form": "요즘 엄마가 가끔그래요",
          "original_form": "요즘 엄마가 가끔그래요",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.21",
          "form": "저는 아빠는 결혼하지 말라는데 오히려 엄마가 더 난리예요",
          "original_form": "저는 아빠는 결혼하지 말라는데 오히려 엄마가 더 난리예요",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.22",
          "form": "제가 늦둥이인데",
          "original_form": "제가 늦둥이인데",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.23",
          "form": "엄마아빠 죽고나면 너 혼자 어떻게 살려고 그러냐고...",
          "original_form": "엄마아빠 죽고나면 너 혼자 어떻게 살려고 그러냐고...",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037252.1.24",
          "form": "근데 남편이 먼저죽으면요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 남편이 먼저죽으면요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037252.1.25",
          "form": "그때는어쩔건지",
          "original_form": "그때는어쩔건지",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037252.1.26",
          "form": "그니까요!!!",
          "original_form": "그니까요!!!",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037252.1.27",
          "form": "아니근데 남편도 못믿죠",
          "original_form": "아니근데 남편도 못믿죠",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037252.1.28",
          "form": "남편이 때리지나 않으면 다행",
          "original_form": "남편이 때리지나 않으면 다행",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037252.1.29",
          "form": "그냥 요즘세대 남자들이 유난히 이상해요",
          "original_form": "그냥 요즘세대 남자들이 유난히 이상해요",
          "speaker_id": "2",
          "time": "20210831 21:59"
        }
      ]
