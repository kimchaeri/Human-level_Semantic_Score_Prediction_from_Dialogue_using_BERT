      "utterance": [
        {
          "id": "MDRW2100034214.1.1",
          "form": "혹시 홈베이킹 좋아하세요?",
          "original_form": "혹시 홈베이킹 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100034214.1.2",
          "form": "음  예전엔 잘했어요",
          "original_form": "음  예전엔 잘했어요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034214.1.3",
          "form": "홈런볼도 만들고",
          "original_form": "홈런볼도 만들고",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034214.1.4",
          "form": "근데요샌 안하게 되네요",
          "original_form": "근데요샌 안하게 되네요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034214.1.5",
          "form": "잘하실것 같아요",
          "original_form": "잘하실것 같아요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100034214.1.6",
          "form": "좋아 하세요?",
          "original_form": "좋아 하세요?",
          "speaker_id": "2",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100034214.1.7",
          "form": "아주 좋아하진 않는데 재료가 있을떈 해요",
          "original_form": "아주 좋아하진 않는데 재료가 있을떈 해요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100034214.1.8",
          "form": "요즘은 생지를 구워머거요",
          "original_form": "요즘은 생지를 구워머거요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100034214.1.9",
          "form": "오 그럴것 같아요",
          "original_form": "오 그럴것 같아요",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.10",
          "form": "저도 생지살까 싶어요",
          "original_form": "저도 생지살까 싶어요",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.11",
          "form": "안사봤는데",
          "original_form": "안사봤는데",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.12",
          "form": "에어 프라이에 하나요?",
          "original_form": "에어 프라이에 하나요?",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.13",
          "form": "저는 에어으파리어에 구워요 네",
          "original_form": "저는 에어으파리어에 구워요 네",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.14",
          "form": "근데 이게 크기에 따라서 가운데가 좀 덜익을수도 있어서",
          "original_form": "근데 이게 크기에 따라서 가운데가 좀 덜익을수도 있어서",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.15",
          "form": "아 요새 에어프라이가 최고예요",
          "original_form": "아 요새 에어프라이가 최고예요",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.16",
          "form": "아 세상에",
          "original_form": "아 세상에",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100034214.1.17",
          "form": "출력이 좀 적으면 작은 생지를 구우세요",
          "original_form": "출력이 좀 적으면 작은 생지를 구우세요",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034214.1.18",
          "form": "네ㅠㅜ",
          "original_form": "네ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034214.1.19",
          "form": "제건 왠지 그럴것 같네요",
          "original_form": "제건 왠지 그럴것 같네요",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034214.1.20",
          "form": "뭐가 맛나요?",
          "original_form": "뭐가 맛나요?",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034214.1.21",
          "form": "홈플러스도 팔던데",
          "original_form": "홈플러스도 팔던데",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100034214.1.22",
          "form": "전 울프백 생지를 좋ㅎ아하는데 이건 버터가 들어서 비싸구요",
          "original_form": "전 울프백 생지를 좋ㅎ아하는데 이건 버터가 들어서 비싸구요",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034214.1.23",
          "form": "성분보고 버터가 조금이라도 들어간걸로 가격적당한걸로 머거요",
          "original_form": "성분보고 버터가 조금이라도 들어간걸로 가격적당한걸로 머거요",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034214.1.24",
          "form": "아 맞아요 버터 중요해요 ㅎㅎ",
          "original_form": "아 맞아요 버터 중요해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034214.1.25",
          "form": "안들어가면 음 맛없어요",
          "original_form": "안들어가면 음 맛없어요",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034214.1.26",
          "form": "울프백 아",
          "original_form": "울프백 아",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034214.1.27",
          "form": "개당가격이 빠바 크로아상보다 비싼느낌 ㅠㅋ",
          "original_form": "개당가격이 빠바 크로아상보다 비싼느낌 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034214.1.28",
          "form": "엥 ㅋㅋ",
          "original_form": "엥 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034214.1.29",
          "form": "그래도 먹고싶네요",
          "original_form": "그래도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034214.1.30",
          "form": "맛있어요 ㅋㅋ 오븐있음 사드셔보세요 에프는 살짝 화력모자란느김",
          "original_form": "맛있어요 ㅋㅋ 오븐있음 사드셔보세요 에프는 살짝 화력모자란느김",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034214.1.31",
          "form": "네 오븐 있어요",
          "original_form": "네 오븐 있어요",
          "speaker_id": "2",
          "time": "20210827 22:00"
        }
      ]
    }
  ]
