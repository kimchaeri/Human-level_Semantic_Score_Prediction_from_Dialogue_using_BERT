      "utterance": [
        {
          "id": "MDRW2100028008.1.1",
          "form": "요즘 날씨 많이 추워져서 쌀쌀해",
          "original_form": "요즘 날씨 많이 추워져서 쌀쌀해",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028008.1.2",
          "form": "맞아맞아. 요즘 부쩍 추워져서 겉옷 들고 다녀야해",
          "original_form": "맞아맞아. 요즘 부쩍 추워져서 겉옷 들고 다녀야해",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028008.1.3",
          "form": "비도 오고 그래서",
          "original_form": "비도 오고 그래서",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028008.1.4",
          "form": "일기예보도 꼭꼭 챙겨보게 된다니까",
          "original_form": "일기예보도 꼭꼭 챙겨보게 된다니까",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028008.1.5",
          "form": "난 여름 아니게 된지 오래되서",
          "original_form": "난 여름 아니게 된지 오래되서",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028008.1.6",
          "form": "맨날 가을옷 입어야함",
          "original_form": "맨날 가을옷 입어야함",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028008.1.7",
          "form": "난 그래도 한낮에는 더워서",
          "original_form": "난 그래도 한낮에는 더워서",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028008.1.8",
          "form": "아직 여름옷 입는 중이야",
          "original_form": "아직 여름옷 입는 중이야",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028008.1.9",
          "form": "거기에 겉옷을 걸친단다",
          "original_form": "거기에 겉옷을 걸친단다",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028008.1.10",
          "form": "지금 코트입기 딱좋은 날씨인데",
          "original_form": "지금 코트입기 딱좋은 날씨인데",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028008.1.11",
          "form": "나갈일이 내일뿐.",
          "original_form": "나갈일이 내일뿐.",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028008.1.12",
          "form": "코트입기 딱좋지~",
          "original_form": "코트입기 딱좋지~",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028008.1.13",
          "form": "가을은 코트의 계절~",
          "original_form": "가을은 코트의 계절~",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028008.1.14",
          "form": "그러고보면 요즘 미세먼지 없어서 하늘 맑아서 좋음",
          "original_form": "그러고보면 요즘 미세먼지 없어서 하늘 맑아서 좋음",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028008.1.15",
          "form": "그러니까~ 미세먼지 없는게 정말 좋은것같아",
          "original_form": "그러니까~ 미세먼지 없는게 정말 좋은것같아",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028008.1.16",
          "form": "맑은날씨 짱!",
          "original_form": "맑은날씨 짱!",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028008.1.17",
          "form": "사진찍어도 이쁘게 나오고 너무 조아",
          "original_form": "사진찍어도 이쁘게 나오고 너무 조아",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028008.1.18",
          "form": "특히 구름찍힌 9월의 사진이란~ 정말 좋아",
          "original_form": "특히 구름찍힌 9월의 사진이란~ 정말 좋아",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028008.1.19",
          "form": "다음달엔 더 추워지겠지?",
          "original_form": "다음달엔 더 추워지겠지?",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028008.1.20",
          "form": "금방 패딩의 계절이 오겠구만",
          "original_form": "금방 패딩의 계절이 오겠구만",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028008.1.21",
          "form": "진짜 싫다",
          "original_form": "진짜 싫다",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028008.1.22",
          "form": "지금이 딱 좋은데",
          "original_form": "지금이 딱 좋은데",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028008.1.23",
          "form": "맞아 지금날씨가 딱 좋아. 선선하고~",
          "original_form": "맞아 지금날씨가 딱 좋아. 선선하고~",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028008.1.24",
          "form": "겨울은 너무 추워",
          "original_form": "겨울은 너무 추워",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028008.1.25",
          "form": "덥고 추운게 중간이 없어ㅜ",
          "original_form": "덥고 추운게 중간이 없어ㅜ",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028008.1.26",
          "form": "확실히 요즘날씨는 중간이없어",
          "original_form": "확실히 요즘날씨는 중간이없어",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028008.1.27",
          "form": "겨울여름만 있는기분",
          "original_form": "겨울여름만 있는기분",
          "speaker_id": "2",
          "time": "20210902 19:47"
        }
      ]
    }
  ]
