      "utterance": [
        {
          "id": "MDRW2100013377.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013377.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013377.1.3",
          "form": "요즘즐겨보는 드라마 .tv프로그램있나요,",
          "original_form": "요즘즐겨보는 드라마 .tv프로그램있나요,",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013377.1.4",
          "form": "전 놀토 좋아해요",
          "original_form": "전 놀토 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013377.1.5",
          "form": "드라마는 슬의생이요",
          "original_form": "드라마는 슬의생이요",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013377.1.6",
          "form": "놀토가 놀라운토요일 인가요?",
          "original_form": "놀토가 놀라운토요일 인가요?",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013377.1.7",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013377.1.8",
          "form": "저도 슬의생 보고있어요.",
          "original_form": "저도 슬의생 보고있어요.",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013377.1.9",
          "form": "슬의생에서 좋아하는 캐릭터있나요?",
          "original_form": "슬의생에서 좋아하는 캐릭터있나요?",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013377.1.10",
          "form": "정경호요ㅋㅋ",
          "original_form": "정경호요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013377.1.11",
          "form": "근데 익준이가 재미있긴 하죠",
          "original_form": "근데 익준이가 재미있긴 하죠",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013377.1.12",
          "form": "전 조정석 이익준 캐릭터요",
          "original_form": "전 조정석 이익준 캐릭터요",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013377.1.13",
          "form": "거의 조정석이 주인공이라고 해도 될정도긴 해요",
          "original_form": "거의 조정석이 주인공이라고 해도 될정도긴 해요",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013377.1.14",
          "form": "맞아요.전미도 채송화캐릭터도 좋아요.",
          "original_form": "맞아요.전미도 채송화캐릭터도 좋아요.",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013377.1.15",
          "form": "지난주 탁구하는거 보셨어여?ㅋㄲ",
          "original_form": "지난주 탁구하는거 보셨어여?ㅋㄲ",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013377.1.16",
          "form": "네. 현정화 감독 나온거 봤어요",
          "original_form": "네. 현정화 감독 나온거 봤어요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013377.1.17",
          "form": "조정석 너무 웃겼어요ㅋㅋ",
          "original_form": "조정석 너무 웃겼어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100013377.1.18",
          "form": "매회 너무 우껴요.",
          "original_form": "매회 너무 우껴요.",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013377.1.19",
          "form": "그니까요. 생활연기 달인이에요 진짜",
          "original_form": "그니까요. 생활연기 달인이에요 진짜",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100013377.1.20",
          "form": "찰떡연기에 매회 기다려져요.  유연석 엄마로 나오는 김해숙씨랑 김갑수씨 도 참좋더라구요",
          "original_form": "찰떡연기에 매회 기다려져요.  유연석 엄마로 나오는 김해숙씨랑 김갑수씨 도 참좋더라구요",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013377.1.21",
          "form": "맞아여 그둘도 재미있어여",
          "original_form": "맞아여 그둘도 재미있어여",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013377.1.22",
          "form": "매주목요일이 기다려집니다",
          "original_form": "매주목요일이 기다려집니다",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013377.1.23",
          "form": "나이들어서도 남녀사이 좋은친구 보기 좋더라구요",
          "original_form": "나이들어서도 남녀사이 좋은친구 보기 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013377.1.24",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013377.1.25",
          "form": "이번주엔 어떤내용일지 기대되요",
          "original_form": "이번주엔 어떤내용일지 기대되요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        }
      ]
    }
