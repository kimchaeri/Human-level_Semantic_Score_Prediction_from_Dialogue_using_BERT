      "utterance": [
        {
          "id": "MDRW2100036844.1.1",
          "form": "ㅇㅅㅇ 하 이놈의 디톡스가 뭔지",
          "original_form": "ㅇㅅㅇ 하 이놈의 디톡스가 뭔지",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036844.1.2",
          "form": "60ml과채주스따위",
          "original_form": "60ml과채주스따위",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036844.1.3",
          "form": "60ml 하루 다섯개면 300ml 먹는건가요",
          "original_form": "60ml 하루 다섯개면 300ml 먹는건가요",
          "speaker_id": "1",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036844.1.4",
          "form": "밥도안먹고..",
          "original_form": "밥도안먹고..",
          "speaker_id": "1",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036844.1.5",
          "form": "아 저거원액이라 물120이랑 같이요",
          "original_form": "아 저거원액이라 물120이랑 같이요",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100036844.1.6",
          "form": "그냥 200치죠",
          "original_form": "그냥 200치죠",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100036844.1.7",
          "form": "하루 주스만 1리터먹는거군요",
          "original_form": "하루 주스만 1리터먹는거군요",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100036844.1.8",
          "form": "뱃속에 아무것도 안남을거같은데요",
          "original_form": "뱃속에 아무것도 안남을거같은데요",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036844.1.9",
          "form": "아무것도 안남는 느낌이긴해요",
          "original_form": "아무것도 안남는 느낌이긴해요",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100036844.1.10",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100036844.1.11",
          "form": "하 최대한공복을유지하라는데",
          "original_form": "하 최대한공복을유지하라는데",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100036844.1.12",
          "form": "그럼 물도 최대한 안먹어야하는건가.",
          "original_form": "그럼 물도 최대한 안먹어야하는건가.",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100036844.1.13",
          "form": "물은 드셔야하는거 아닌가요?! 건강상하면 우째요",
          "original_form": "물은 드셔야하는거 아닌가요?! 건강상하면 우째요",
          "speaker_id": "1",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100036844.1.14",
          "form": "해봐야 이틀이니까요 방금까지 후기 찾아보고 있었어요 ㅋㅋ",
          "original_form": "해봐야 이틀이니까요 방금까지 후기 찾아보고 있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100036844.1.15",
          "form": "아 이틀 단기로 빼는건가여?! 그럼 요요오진않나요",
          "original_form": "아 이틀 단기로 빼는건가여?! 그럼 요요오진않나요",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100036844.1.16",
          "form": "대단히 빠지는거같지도않아요 후기보니까",
          "original_form": "대단히 빠지는거같지도않아요 후기보니까",
          "speaker_id": "2",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100036844.1.17",
          "form": "저의 고통은 한5키로는 빠져야할거같은데 ㅋㅋ",
          "original_form": "저의 고통은 한5키로는 빠져야할거같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100036844.1.18",
          "form": "이틀이나 굶어야한다니 너무 가혹하군여",
          "original_form": "이틀이나 굶어야한다니 너무 가혹하군여",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100036844.1.19",
          "form": "후기보니까 진짜 대박빠진사람이 3키로?",
          "original_form": "후기보니까 진짜 대박빠진사람이 3키로?",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100036844.1.20",
          "form": "장난하나",
          "original_form": "장난하나",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100036844.1.21",
          "form": "와 배송비하면 거의 사만원인데",
          "original_form": "와 배송비하면 거의 사만원인데",
          "speaker_id": "1",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100036844.1.22",
          "form": "이틀을 굶으면 3kg 걍 빠지지않나요",
          "original_form": "이틀을 굶으면 3kg 걍 빠지지않나요",
          "speaker_id": "1",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100036844.1.23",
          "form": "그러게요ㅜ",
          "original_form": "그러게요ㅜ",
          "speaker_id": "2",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100036844.1.24",
          "form": "그리고 금방 다시찌는거잖아요 수분이라ㅜ",
