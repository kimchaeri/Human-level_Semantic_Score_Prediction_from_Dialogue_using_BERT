      "utterance": [
        {
          "id": "MDRW2100018999.1.1",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018999.1.2",
          "form": "막 살짝만 스쳐도 눌리는게 있고",
          "original_form": "막 살짝만 스쳐도 눌리는게 있고",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018999.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018999.1.4",
          "form": "힘을 줘야지만 눌리는 게 있대",
          "original_form": "힘을 줘야지만 눌리는 게 있대",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018999.1.5",
          "form": "어떤 느낌인지 알겠음",
          "original_form": "어떤 느낌인지 알겠음",
          "speaker_id": "1",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100018999.1.6",
          "form": "이게 키압 차이로 일어나는 일이지",
          "original_form": "이게 키압 차이로 일어나는 일이지",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100018999.1.7",
          "form": "유독 림줘야 눌리는 키보드가 있음",
          "original_form": "유독 림줘야 눌리는 키보드가 있음",
          "speaker_id": "1",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100018999.1.8",
          "form": "근데 너무 잘 눌려도 불편하지 않음?",
          "original_form": "근데 너무 잘 눌려도 불편하지 않음?",
          "speaker_id": "1",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100018999.1.9",
          "form": "ㅏㅇ 그치그치",
          "original_form": "ㅏㅇ 그치그치",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100018999.1.10",
          "form": "그런거임",
          "original_form": "그런거임",
          "speaker_id": "2",
          "time": "20210827 14:35"
        },
        {
          "id": "MDRW2100018999.1.11",
          "form": "근데 너는 의자도 신경 써서 사야 하지않아?",
          "original_form": "근데 너는 의자도 신경 써서 사야 하지않아?",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018999.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018999.1.13",
          "form": "맞는데",
          "original_form": "맞는데",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018999.1.14",
          "form": "맞지...",
          "original_form": "맞지...",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018999.1.15",
          "form": "의자아무거나쓰면",
          "original_form": "의자아무거나쓰면",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018999.1.16",
          "form": "허리나가잖아",
          "original_form": "허리나가잖아",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018999.1.17",
          "form": "그니까 그런데 의자가 꽤 비싸더라고",
          "original_form": "그니까 그런데 의자가 꽤 비싸더라고",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018999.1.18",
          "form": "포기하고 그냥 일반 의자 쓰는 중임",
          "original_form": "포기하고 그냥 일반 의자 쓰는 중임",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018999.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018999.1.20",
          "form": "의자비싸긴해",
          "original_form": "의자비싸긴해",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018999.1.21",
          "form": "왜이렇게비싼지잘모르겠지만",
          "original_form": "왜이렇게비싼지잘모르겠지만",
          "speaker_id": "1",
          "time": "20210827 14:39"
