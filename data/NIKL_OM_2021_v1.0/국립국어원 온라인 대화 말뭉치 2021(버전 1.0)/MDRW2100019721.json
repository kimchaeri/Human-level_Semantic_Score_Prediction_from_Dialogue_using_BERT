      "utterance": [
        {
          "id": "MDRW2100019721.1.1",
          "form": "음악 듣는 거 좋아해요?",
          "original_form": "음악 듣는 거 좋아해요?",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019721.1.2",
          "form": "음악 듣는거 좋아하죠",
          "original_form": "음악 듣는거 좋아하죠",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019721.1.3",
          "form": "어떤 장르 좋아해요?",
          "original_form": "어떤 장르 좋아해요?",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019721.1.4",
          "form": "저는 가요나 발라드도 좋아해요 사실 막 가리진않아요",
          "original_form": "저는 가요나 발라드도 좋아해요 사실 막 가리진않아요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019721.1.5",
          "form": "듣고 좋으면 계속 듣는편이예요",
          "original_form": "듣고 좋으면 계속 듣는편이예요",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019721.1.6",
          "form": "저도 가요랑 발라드 좋아하는데 ㅎㅎ",
          "original_form": "저도 가요랑 발라드 좋아하는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019721.1.7",
          "form": "최애곡 있어요 그럼?",
          "original_form": "최애곡 있어요 그럼?",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019721.1.8",
          "form": "요즘은 우효의 민들레 라는 노래 좋아하고있어요",
          "original_form": "요즘은 우효의 민들레 라는 노래 좋아하고있어요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019721.1.9",
          "form": "아이유노래는 다좋구요",
          "original_form": "아이유노래는 다좋구요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019721.1.10",
          "form": "오~ 저도 아이유 노래 좋아하는데 최근에는 정승환 노래에 꽂혔어요 ㅎㅎ",
          "original_form": "오~ 저도 아이유 노래 좋아하는데 최근에는 정승환 노래에 꽂혔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019721.1.11",
          "form": "정승환 저도 좋아했었는데 어떤노래에 꽂히셨나요??",
          "original_form": "정승환 저도 좋아했었는데 어떤노래에 꽂히셨나요??",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019721.1.12",
          "form": "스타트업 오스티로 나온 곡인데요",
          "original_form": "스타트업 오스티로 나온 곡인데요",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019721.1.13",
          "form": "혹시 아세요?",
          "original_form": "혹시 아세요?",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019721.1.14",
          "form": "아쉽게도 처음들어봤어요 ㅜㅋㅋ",
          "original_form": "아쉽게도 처음들어봤어요 ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019721.1.15",
          "form": "아 진짜요? 그러면 김경호 가수는요?",
          "original_form": "아 진짜요? 그러면 김경호 가수는요?",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019721.1.16",
          "form": "오스티 곡 제목이 day&night 인데 나중에 들어봐용",
          "original_form": "오스티 곡 제목이 day&night 인데 나중에 들어봐용",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019721.1.17",
          "form": "네 들어보고 좋으면 저도 계속들을께요ㅋㅋ",
          "original_form": "네 들어보고 좋으면 저도 계속들을께요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019721.1.18",
          "form": "김경호 알아요",
          "original_form": "김경호 알아요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019721.1.19",
          "form": "김경호 가수는 아쉽지만 제 취향은 아니더라구요..",
          "original_form": "김경호 가수는 아쉽지만 제 취향은 아니더라구요..",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019721.1.20",
          "form": "대신 폴킴이 부른 <너를 만나> 한 때 빠졌어요 ㅎㅎ",
          "original_form": "대신 폴킴이 부른 <너를 만나> 한 때 빠졌어요 ㅎㅎ",
