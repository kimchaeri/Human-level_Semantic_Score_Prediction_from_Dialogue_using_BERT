        },
        {
          "id": "MDRW2100013271.1.2",
          "form": "비 많이와서?",
          "original_form": "비 많이와서?",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013271.1.3",
          "form": "아니 이슬비처럼 내려",
          "original_form": "아니 이슬비처럼 내려",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.4",
          "form": "응 여기도 그러네",
          "original_form": "응 여기도 그러네",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.5",
          "form": "비오니 넘 꿉꿉해",
          "original_form": "비오니 넘 꿉꿉해",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.6",
          "form": "후덥지근한 불쾌지수높네",
          "original_form": "후덥지근한 불쾌지수높네",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.7",
          "form": "현장은 더덥지",
          "original_form": "현장은 더덥지",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.8",
          "form": "응 사무실은 괜찮아",
          "original_form": "응 사무실은 괜찮아",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.9",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013271.1.10",
          "form": "제습기능으로  하니 사무실은 쾌적",
          "original_form": "제습기능으로  하니 사무실은 쾌적",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013271.1.11",
          "form": "현장엔 에어컨 없지",
          "original_form": "현장엔 에어컨 없지",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013271.1.12",
          "form": "응 없지",
          "original_form": "응 없지",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013271.1.13",
          "form": "오늘은 현장 갈일 별로 없음 좋겠구만",
          "original_form": "오늘은 현장 갈일 별로 없음 좋겠구만",
          "speaker_id": "1",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013271.1.14",
          "form": "그러게  말이야",
          "original_form": "그러게  말이야",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013271.1.15",
          "form": "별루 없을겨 내가 그러라 할게 ㅋㅋ",
