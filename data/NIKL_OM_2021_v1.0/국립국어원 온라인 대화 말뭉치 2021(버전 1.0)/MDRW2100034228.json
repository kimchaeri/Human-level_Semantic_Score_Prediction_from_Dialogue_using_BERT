      "utterance": [
        {
          "id": "MDRW2100034228.1.1",
          "form": "다낭에서 먹은 코코넛이 생각나네",
          "original_form": "다낭에서 먹은 코코넛이 생각나네",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034228.1.2",
          "form": "나도! 넘 맛있었다..",
          "original_form": "나도! 넘 맛있었다..",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034228.1.3",
          "form": "웅 ㅋㅋ 바로 앞에서 깎아 주는 코코넛 최고",
          "original_form": "웅 ㅋㅋ 바로 앞에서 깎아 주는 코코넛 최고",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034228.1.4",
          "form": "그러니까~ 넘 맛있다 ㅠㅠ 또먹고싶네",
          "original_form": "그러니까~ 넘 맛있다 ㅠㅠ 또먹고싶네",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034228.1.5",
          "form": "담에 가면 또 먹자",
          "original_form": "담에 가면 또 먹자",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034228.1.6",
          "form": "웅 ㅋ 엄마 좋아하는 두리안도~",
          "original_form": "웅 ㅋ 엄마 좋아하는 두리안도~",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034228.1.7",
          "form": "그래 두리안도 꿀맛이었다 ㅋㅋ",
          "original_form": "그래 두리안도 꿀맛이었다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034228.1.8",
          "form": "웅ㅋㅋㅋ",
          "original_form": "웅ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034228.1.9",
          "form": "맛난게 많네",
          "original_form": "맛난게 많네",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034228.1.10",
          "form": "웅 ㅋㅋ 날씨가 더워서 열대과일이 잘 나나봐",
          "original_form": "웅 ㅋㅋ 날씨가 더워서 열대과일이 잘 나나봐",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034228.1.11",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
