      "utterance": [
        {
          "id": "MDRW2100016693.1.1",
          "form": "안녕하세요! 점심 뭐 드셨나요?",
          "original_form": "안녕하세요! 점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016693.1.2",
          "form": "닭갈비 먹었어용!",
          "original_form": "닭갈비 먹었어용!",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016693.1.3",
          "form": "헉 맛있겠어요",
          "original_form": "헉 맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016693.1.4",
          "form": "밥도 볶으셨나요",
          "original_form": "밥도 볶으셨나요",
          "speaker_id": "1",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100016693.1.5",
          "form": "아니용ㅋㅋㅋ 배달 시켜먹었어요!!",
          "original_form": "아니용ㅋㅋㅋ 배달 시켜먹었어요!!",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016693.1.6",
          "form": "글쿤요! 평소에 배달 어떤거 많이 시켜먹으세요?",
          "original_form": "글쿤요! 평소에 배달 어떤거 많이 시켜먹으세요?",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016693.1.7",
          "form": "여러가지 종류별로 다 시키는거 같아요ㅋㅋ",
          "original_form": "여러가지 종류별로 다 시키는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016693.1.8",
          "form": "근데 특히 초밥류를 좀 많이 시키는거 같네요",
          "original_form": "근데 특히 초밥류를 좀 많이 시키는거 같네요",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016693.1.9",
          "form": "초밥 맛있죠ㅠㅠㅜㅠ",
          "original_form": "초밥 맛있죠ㅠㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016693.1.10",
          "form": "연어초밥 먹고 싶네요!!",
          "original_form": "연어초밥 먹고 싶네요!!",
          "speaker_id": "1",
          "time": "20210826 14:54"
        },
        {
          "id": "MDRW2100016693.1.11",
          "form": "저도 연어초밥 제일 좋아해요!!",
          "original_form": "저도 연어초밥 제일 좋아해요!!",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016693.1.12",
          "form": "헉 글쿤요! 평소에는 모듬으로 시키세요??",
          "original_form": "헉 글쿤요! 평소에는 모듬으로 시키세요??",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100016693.1.13",
          "form": "아니요... 싫어하는게 많아서 그냥 연어로 많이 시키는거 같아요ㅋㅋㅋ",
          "original_form": "아니요... 싫어하는게 많아서 그냥 연어로 많이 시키는거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100016693.1.14",
          "form": "가리비나 문어 이런초밥을 안좋아하는데 꼭 한두개씩 있더라구요ㅠㅠ",
          "original_form": "가리비나 문어 이런초밥을 안좋아하는데 꼭 한두개씩 있더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100016693.1.15",
          "form": "헉 맞아요 모듬으로 시키면 꼭 안좋아하는거 몇개씩 껴있죠ㅠㅠ",
          "original_form": "헉 맞아요 모듬으로 시키면 꼭 안좋아하는거 몇개씩 껴있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100016693.1.16",
          "form": "저는 새우를 별로 안좋아해서 모듬시킬때마다 하나 손해봐요ㅠㅠ",
          "original_form": "저는 새우를 별로 안좋아해서 모듬시킬때마다 하나 손해봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100016693.1.17",
          "form": "아 새우 안좋아하시는구나ㅠㅠ 근데 저도 초밥에 있는 새우는 조금 안좋아해요ㅋㅋㅋ",
          "original_form": "아 새우 안좋아하시는구나ㅠㅠ 근데 저도 초밥에 있는 새우는 조금 안좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100016693.1.18",
          "form": "특히 간장새우나 생새우같은겈ㅋㅋㅋ",
          "original_form": "특히 간장새우나 생새우같은겈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100016693.1.19",
          "form": "맞아요 저도 새우 자체보다는 초밥에 넣는게 싫더라구요!!ㅋㅋㅋㅋ",
          "original_form": "맞아요 저도 새우 자체보다는 초밥에 넣는게 싫더라구요!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100016693.1.20",
          "form": "초밥말고 또 어떤거 많이 시켜드세요? 저는 떡볶이 제일 자주 시키는 것 같아요ㅋㅋ",
          "original_form": "초밥말고 또 어떤거 많이 시켜드세요? 저는 떡볶이 제일 자주 시키는 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:50"
        }
      ]
    }
