      "utterance": [
        {
          "id": "MDRW2100030700.1.1",
          "form": "이제 김밥 끝 부분 남았어요",
          "original_form": "이제 김밥 끝 부분 남았어요",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.2",
          "form": "베트남 땡초 조금 들고 왔어요",
          "original_form": "베트남 땡초 조금 들고 왔어요",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.3",
          "form": "먹을 때 말하니 인식이 잘 안 되네요",
          "original_form": "먹을 때 말하니 인식이 잘 안 되네요",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.4",
          "form": "땡초 가루",
          "original_form": "땡초 가루",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.5",
          "form": "음식에 넣으면 완전 매워 지지요",
          "original_form": "음식에 넣으면 완전 매워 지지요",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.6",
          "form": "겁나 겁나 매워요",
          "original_form": "겁나 겁나 매워요",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.7",
          "form": "낙지볶음에 한 스푼 넣었는데",
          "original_form": "낙지볶음에 한 스푼 넣었는데",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.8",
          "form": "울면서 먹었어요",
          "original_form": "울면서 먹었어요",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030700.1.9",
          "form": "저희 숙모가 좋아하시겠네요 성모님 매운음식을 엄청 드시거든요",
          "original_form": "저희 숙모가 좋아하시겠네요 성모님 매운음식을 엄청 드시거든요",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.10",
          "form": "관심 있으신가요",
          "original_form": "관심 있으신가요",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.11",
          "form": "나눔 가능합니다",
          "original_form": "나눔 가능합니다",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.12",
          "form": "괜찮습니다 넌 잘 안 해요",
          "original_form": "괜찮습니다 넌 잘 안 해요",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.13",
          "form": "정말 정말 맵거든요",
          "original_form": "정말 정말 맵거든요",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.14",
          "form": "저는 요리를 잘 안 해요",
          "original_form": "저는 요리를 잘 안 해요",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.15",
          "form": "그냥 음식에 한 스푼 넣어서 먹어요",
          "original_form": "그냥 음식에 한 스푼 넣어서 먹어요",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.16",
          "form": "짜장면 위에 뿌린다거나",
          "original_form": "짜장면 위에 뿌린다거나",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.17",
          "form": "저희 고모가 매운 건 못 드세요",
          "original_form": "저희 고모가 매운 건 못 드세요",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.18",
          "form": "찍어 먹거나",
          "original_form": "찍어 먹거나",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.19",
          "form": "음식 시킬 때 매운 걸 못 시켜요",
          "original_form": "음식 시킬 때 매운 걸 못 시켜요",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030700.1.20",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210904 11:34"
        }
      ]
