      "utterance": [
        {
          "id": "MDRW2100006265.1.1",
          "form": "복직 후 어떠세요",
          "original_form": "복직 후 어떠세요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006265.1.2",
          "form": "너무 힘들어요ㅋㅋ",
          "original_form": "너무 힘들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006265.1.3",
          "form": "월요일엔 지역에 확진자가 심지어 인근 중학교에서 나와서 학생들 오자마자 다 다시 돌려보내고 원격 수업 준비하고 난리도 아니었네요 ㅠ",
          "original_form": "월요일엔 지역에 확진자가 심지어 인근 중학교에서 나와서 학생들 오자마자 다 다시 돌려보내고 원격 수업 준비하고 난리도 아니었네요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006265.1.4",
          "form": "더군다나 금요일까지 학부모 공개수업안 제출..",
          "original_form": "더군다나 금요일까지 학부모 공개수업안 제출..",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006265.1.5",
          "form": "대면으로 한대요;; 이 와중에 ㅠ",
          "original_form": "대면으로 한대요;; 이 와중에 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006265.1.6",
          "form": "학부모 공개수업을요??????",
          "original_form": "학부모 공개수업을요??????",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006265.1.7",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006265.1.8",
          "form": "네..그렇다네요...아하하",
          "original_form": "네..그렇다네요...아하하",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006265.1.9",
          "form": "날짜는요??",
          "original_form": "날짜는요??",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006265.1.10",
          "form": "16일에요~",
          "original_form": "16일에요~",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006265.1.11",
          "form": "날짜도 엄청 빠르네요 일하시기 힘드시겠어요",
          "original_form": "날짜도 엄청 빠르네요 일하시기 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100006265.1.12",
          "form": "네..ㅠ 빡세네요ㅋㅋㅋ",
          "original_form": "네..ㅠ 빡세네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100006265.1.13",
          "form": "복직하고 좋은점도 있나요?",
          "original_form": "복직하고 좋은점도 있나요?",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006265.1.14",
          "form": "아직까진....월급이 들어와야 좋을 것 같아요ㅋㅋㅋ",
          "original_form": "아직까진....월급이 들어와야 좋을 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
