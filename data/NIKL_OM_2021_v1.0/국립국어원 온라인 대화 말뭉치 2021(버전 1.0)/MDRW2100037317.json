      "utterance": [
        {
          "id": "MDRW2100037317.1.1",
          "form": "뭔가 이중적이고 비겁한 사람들 같아요",
          "original_form": "뭔가 이중적이고 비겁한 사람들 같아요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037317.1.2",
          "form": "맞아요 ㅎㅎㅎ 학부생들한테는",
          "original_form": "맞아요 ㅎㅎㅎ 학부생들한테는",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037317.1.3",
          "form": "그러면서 체면치레 많구요",
          "original_form": "그러면서 체면치레 많구요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037317.1.4",
          "form": "대학원 들어오라고 꼬시면서 착한 척 하고",
          "original_form": "대학원 들어오라고 꼬시면서 착한 척 하고",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.5",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.6",
          "form": "사실 어느 직업군이든",
          "original_form": "사실 어느 직업군이든",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.7",
          "form": "문제는 많지만... ㅎㅎㅎ",
          "original_form": "문제는 많지만... ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.8",
          "form": "하여튼 그 아줌마는 전혀",
          "original_form": "하여튼 그 아줌마는 전혀",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.9",
          "form": "자랑할 일이 아닌데",
          "original_form": "자랑할 일이 아닌데",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.10",
          "form": "안타깝네요 ㅠ",
          "original_form": "안타깝네요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.11",
          "form": "ㅋㅋㅋ 되게 유명인인데",
          "original_form": "ㅋㅋㅋ 되게 유명인인데",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.12",
          "form": "그 아줌마가요?!",
          "original_form": "그 아줌마가요?!",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037317.1.13",
          "form": "여기서 말해도되겠죠 안잡으러오겠지 ㅋㅋ",
          "original_form": "여기서 말해도되겠죠 안잡으러오겠지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.14",
          "form": "name1님이랑은 실제로 아는 분이에요?",
          "original_form": "&name1&님이랑은 실제로 아는 분이에요?",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.15",
          "form": "네네 sns유명인이요",
          "original_form": "네네 sns유명인이요",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.16",
          "form": "당연하죠 저도 연예인 뭐 알려드릴게요",
          "original_form": "당연하죠 저도 연예인 뭐 알려드릴게요",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.17",
          "form": "이젠 친구아니에요 ㅋㅋㅋ",
          "original_form": "이젠 친구아니에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.18",
          "form": "이따가 주제를 연예인으로 바꿀게요",
          "original_form": "이따가 주제를 연예인으로 바꿀게요",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.20",
          "form": "그들에게 결혼은 무엇인가...",
          "original_form": "그들에게 결혼은 무엇인가...",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.21",
          "form": "진짜ㅠ 남자한테 자아의탁하고 결혼으로 신분상승하려는 사람들 보면 답답해요",
          "original_form": "진짜ㅠ 남자한테 자아의탁하고 결혼으로 신분상승하려는 사람들 보면 답답해요",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037317.1.22",
          "form": "그런 사람들은 자기 남편이 나쁜 짓해도 피해자 탓하더라구요",
          "original_form": "그런 사람들은 자기 남편이 나쁜 짓해도 피해자 탓하더라구요",
          "speaker_id": "2",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037317.1.23",
          "form": "이남자 저남자 추근대고 기혼녀들에게 의심당해야하나 싶고.",
          "original_form": "이남자 저남자 추근대고 기혼녀들에게 의심당해야하나 싶고.",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037317.1.24",
          "form": "그러니까 말이에요 흠",
          "original_form": "그러니까 말이에요 흠",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037317.1.25",
          "form": "무슨 의미가 있나...",
