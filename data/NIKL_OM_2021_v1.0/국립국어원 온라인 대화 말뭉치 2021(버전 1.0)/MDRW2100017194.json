        },
        {
          "id": "MDRW2100017194.1.6",
          "form": "일관적 게임 취향",
          "original_form": "일관적 게임 취향",
          "speaker_id": "2",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100017194.1.7",
          "form": "다 메이저한 게임이라 부럽다",
          "original_form": "다 메이저한 게임이라 부럽다",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017194.1.8",
          "form": "로오히 스토리 밀고 접으려고 했는데",
          "original_form": "로오히 스토리 밀고 접으려고 했는데",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017194.1.9",
          "form": "더 할까 싶음...",
          "original_form": "더 할까 싶음...",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017194.1.10",
          "form": "스토리 아직 남았으면서 고민만 함ㅋㅋ",
          "original_form": "스토리 아직 남았으면서 고민만 함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017194.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 계속 해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 계속 해",
