      "utterance": [
        {
          "id": "MDRW2100035379.1.1",
          "form": "이상형이랑 연애해본 적 있으세요?",
          "original_form": "이상형이랑 연애해본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100035379.1.2",
          "form": "이상형을 잘모르겟어요ㅠㅠ ㅋㅋ매번바뀌는거같기도하고 ㅎㅎ",
          "original_form": "이상형을 잘모르겟어요ㅠㅠ ㅋㅋ매번바뀌는거같기도하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100035379.1.3",
          "form": "그러시군요 ㅎㅎ",
          "original_form": "그러시군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100035379.1.4",
          "form": "이상형이 어떻게되세여?",
          "original_form": "이상형이 어떻게되세여?",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100035379.1.5",
          "form": "말통하고 저한테 잘하는 남자요",
          "original_form": "말통하고 저한테 잘하는 남자요",
          "speaker_id": "1",
          "time": "20210829 16:18"
        },
        {
          "id": "MDRW2100035379.1.6",
          "form": "그런분을 만나셨나요?",
          "original_form": "그런분을 만나셨나요?",
          "speaker_id": "2",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100035379.1.7",
          "form": "아뇨 ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100035379.1.8",
          "form": "순 상것들만 만난듯요",
          "original_form": "순 상것들만 만난듯요",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100035379.1.9",
          "form": "허걱 ㅠㅠㅠ ㅋㅋㅋㅋ",
          "original_form": "허걱 ㅠㅠㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035379.1.10",
          "form": "예전에 처음 사귄 남자믄",
          "original_form": "예전에 처음 사귄 남자믄",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035379.1.11",
          "form": "는",
          "original_form": "는",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035379.1.12",
          "form": "크리스마스때 선물이라고",
          "original_form": "크리스마스때 선물이라고",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035379.1.13",
          "form": "만화책 세권을 검은 비닐봉지에 싸서 줬어요",
          "original_form": "만화책 세권을 검은 비닐봉지에 싸서 줬어요",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035379.1.14",
          "form": "어머나 무슨생각이래여",
          "original_form": "어머나 무슨생각이래여",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100035379.1.15",
          "form": "그냥 미친거죠",
          "original_form": "그냥 미친거죠",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035379.1.16",
          "form": "미친사람의 생각을 더이상 알고싶지않아요",
          "original_form": "미친사람의 생각을 더이상 알고싶지않아요",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100035379.1.17",
          "form": "에고 이상한 분을 만났군요 헤어지셔서다행이에요!",
          "original_form": "에고 이상한 분을 만났군요 헤어지셔서다행이에요!",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100035379.1.18",
          "form": "네 너무 기분나빴어요",
          "original_form": "네 너무 기분나빴어요",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100035379.1.19",
          "form": "그거 말고도 마트에서 파는 세일상품",
          "original_form": "그거 말고도 마트에서 파는 세일상품",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100035379.1.20",
          "form": "흠 묻은거",
          "original_form": "흠 묻은거",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100035379.1.21",
          "form": "세탁해서 선물주고",
          "original_form": "세탁해서 선물주고",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035379.1.22",
          "form": "와;;;",
          "original_form": "와;;;",
          "speaker_id": "2",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035379.1.23",
          "form": "너무 일반인의 상식과 동떨어진 짓을 해서;;;",
          "original_form": "너무 일반인의 상식과 동떨어진 짓을 해서;;;",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035379.1.24",
          "form": "근데 그걸로 싸워도",
          "original_form": "근데 그걸로 싸워도",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035379.1.25",
          "form": "표정이 그냥 무표정?",
          "original_form": "표정이 그냥 무표정?",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035379.1.26",
          "form": "제가 왜 화났는지 이해못하는 표정 있죠",
          "original_form": "제가 왜 화났는지 이해못하는 표정 있죠",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100035379.1.27",
          "form": "에고 답답한 관계였네요",
          "original_form": "에고 답답한 관계였네요",
          "speaker_id": "2",
          "time": "20210829 16:42"
        }
      ]
    }
