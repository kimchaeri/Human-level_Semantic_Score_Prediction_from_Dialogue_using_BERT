      "utterance": [
        {
          "id": "MDRW2100014886.1.1",
          "form": "괜찮아요!!",
          "original_form": "괜찮아요!!",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.2",
          "form": "그렇지만 맛집에 대해서 더 이야기를 하고 싶습니더",
          "original_form": "그렇지만 맛집에 대해서 더 이야기를 하고 싶습니더",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.4",
          "form": "저도 맛집 엄청 좋아하거든요!!!!",
          "original_form": "저도 맛집 엄청 좋아하거든요!!!!",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.5",
          "form": "맛집은 찾아가는 스타일은 아니고",
          "original_form": "맛집은 찾아가는 스타일은 아니고",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.6",
          "form": "막입이라 어디서든 잘먹는 스타일이에요",
          "original_form": "막입이라 어디서든 잘먹는 스타일이에요",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.7",
          "form": "하지만 맛없는건 용서 못함",
          "original_form": "하지만 맛없는건 용서 못함",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014886.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맛없으면 유죄죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맛없으면 유죄죠",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014886.1.9",
          "form": "혹시 서울에 사시나요?",
          "original_form": "혹시 서울에 사시나요?",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014886.1.10",
          "form": "서울에서 5센치 아래인",
          "original_form": "서울에서 5센치 아래인",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014886.1.11",
          "form": "분당에서 살고있어요 ㅎㅎ",
          "original_form": "분당에서 살고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014886.1.12",
          "form": "악 글쿠나",
          "original_form": "악 글쿠나",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014886.1.13",
          "form": "제가 서울 맛집을 알고싶어서 여기저기 다녀봤거든요",
          "original_form": "제가 서울 맛집을 알고싶어서 여기저기 다녀봤거든요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014886.1.14",
          "form": "분당의 최고 맛집은 어디라고 생각하세요?",
          "original_form": "분당의 최고 맛집은 어디라고 생각하세요?",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014886.1.15",
          "form": "서울 주로 맛집이 몰려있는곳은 어딘가요??",
          "original_form": "서울 주로 맛집이 몰려있는곳은 어딘가요??",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014886.1.16",
          "form": "분당엔 맛집보다..프렌차이즈가 많아서요..",
          "original_form": "분당엔 맛집보다..프렌차이즈가 많아서요..",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014886.1.17",
          "form": "판교역쪽엔 스시집을 추천드리고싶네요!!",
          "original_form": "판교역쪽엔 스시집을 추천드리고싶네요!!",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014886.1.18",
          "form": "저 스시 엄청 좋아하는데 어딘지 알려주실 수 있나요?",
          "original_form": "저 스시 엄청 좋아하는데 어딘지 알려주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014886.1.19",
          "form": "원래는 홍대쪽에 맛집이 많다 생각했는데",
          "original_form": "원래는 홍대쪽에 맛집이 많다 생각했는데",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014886.1.20",
          "form": "혜화~ 성신여대 쪽을 가보고",
          "original_form": "혜화~ 성신여대 쪽을 가보고",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014886.1.21",
          "form": "진짜 맛집은 거기에 몰려있다는걸 깨달았어요",
          "original_form": "진짜 맛집은 거기에 몰려있다는걸 깨달았어요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014886.1.22",
          "form": "건대입구쪽에도 맛집이 많대요",
          "original_form": "건대입구쪽에도 맛집이 많대요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014886.1.23",
          "form": "판교역에 아바나 스시앤 누들이라고",
          "original_form": "판교역에 아바나 스시앤 누들이라고",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014886.1.24",
          "form": "롤초밥 파는곳인데 크게가 엄청커요!!!",
          "original_form": "롤초밥 파는곳인데 크게가 엄청커요!!!",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014886.1.25",
          "form": "근데 진짜 마싯어요!",
          "original_form": "근데 진짜 마싯어요!",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014886.1.26",
          "form": "성신여대에도",
          "original_form": "성신여대에도",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014886.1.27",
          "form": "마싯는 마라샹궈집있다고 들었는데 기억이 안나네용...ㅠ",
          "original_form": "마싯는 마라샹궈집있다고 들었는데 기억이 안나네용...ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014886.1.28",
          "form": "헉 어디일까요",
          "original_form": "헉 어디일까요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014886.1.29",
          "form": "만약 성신여대쪽을 가신다면",
          "original_form": "만약 성신여대쪽을 가신다면",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014886.1.30",
          "form": "문화식당을 추천드립니더",
          "original_form": "문화식당을 추천드립니더",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014886.1.31",
          "form": "파스타 집인데 마싯어요",
          "original_form": "파스타 집인데 마싯어요",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014886.1.32",
          "form": "문화식당...기억할게요",
          "original_form": "문화식당...기억할게요",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014886.1.33",
          "form": "파스타 좋아용...",
          "original_form": "파스타 좋아용...",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014886.1.34",
          "form": "디저트도 찾아오ㅓㅅ어요",
          "original_form": "디저트도 찾아오ㅓㅅ어요",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014886.1.35",
          "form": "떡케이크 좋아하시나요? 만약 그렇담 설기당 추천드립니다",
          "original_form": "떡케이크 좋아하시나요? 만약 그렇담 설기당 추천드립니다",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014886.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ쩝쩝 박사이시군요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ쩝쩝 박사이시군요!!",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
