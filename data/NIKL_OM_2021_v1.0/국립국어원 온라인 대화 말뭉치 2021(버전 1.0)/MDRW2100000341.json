        },
        {
          "id": "MDRW2100000341.1.3",
          "form": "오늘 반차쓰고 퇴근하고",
          "original_form": "오늘 반차쓰고 퇴근하고",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000341.1.4",
          "form": "name3이랑 데이트했어",
          "original_form": "&name3&이랑 데이트했어",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000341.1.5",
          "form": "와  잘했네",
          "original_form": "와  잘했네",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000341.1.6",
          "form": "역시 좋은 직장이로다",
          "original_form": "역시 좋은 직장이로다",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000341.1.7",
          "form": "너 연가 며칠이나 있어?",
          "original_form": "너 연가 며칠이나 있어?",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000341.1.8",
          "form": "일년에 15일밖에 없어",
          "original_form": "일년에 15일밖에 없어",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000341.1.9",
          "form": "매년 똑같애??",
          "original_form": "매년 똑같애??",
          "speaker_id": "1",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000341.1.10",
          "form": "아니 근속년수 늘면 같이 늘어나",
          "original_form": "아니 근속년수 늘면 같이 늘어나",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000341.1.11",
          "form": "좋네.. 나는 올해 12일뿐이 없어",
          "original_form": "좋네.. 나는 올해 12일뿐이 없어",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000341.1.12",
          "form": "첫해라 그렇겠지 곧 늘어날거야",
          "original_form": "첫해라 그렇겠지 곧 늘어날거야",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100000341.1.13",
          "form": "월차는 없어?",
          "original_form": "월차는 없어?",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100000341.1.14",
          "form": "없어 연차야",
          "original_form": "없어 연차야",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100000341.1.15",
          "form": "공무원음 노동자의 지위가 아니라 그런건없지\r\n.",
          "original_form": "공무원음 노동자의 지위가 아니라 그런건없지\r\n.",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100000341.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100000341.1.17",
          "form": "그건 좀 서글프네",
          "original_form": "그건 좀 서글프네",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100000341.1.18",
          "form": "출근했어??",
          "original_form": "출근했어??",
          "speaker_id": "1",
          "time": "20210820 08:31"
        },
        {
          "id": "MDRW2100000341.1.19",
          "form": "나오늘 두시간 조퇴쓸까 생각중이야 ㅋㅋㅋ",
          "original_form": "나오늘 두시간 조퇴쓸까 생각중이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:31"
        },
        {
          "id": "MDRW2100000341.1.20",
          "form": "응 왜 무슨일 있어?",
          "original_form": "응 왜 무슨일 있어?",
          "speaker_id": "2",
          "time": "20210820 08:44"
        },
        {
          "id": "MDRW2100000341.1.21",
          "form": "아니 그냥 일찍 집에 가고 싶어 ㅋㅋㅋㅋㅋ",
          "original_form": "아니 그냥 일찍 집에 가고 싶어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:45"
        },
        {
          "id": "MDRW2100000341.1.22",
          "form": "그럼 그런날이 있지",
          "original_form": "그럼 그런날이 있지",
          "speaker_id": "2",
          "time": "20210820 08:53"
        },
