      "utterance": [
        {
          "id": "MDRW2100020310.1.1",
          "form": "근데이건 님은 괜찮을듯",
          "original_form": "근데이건 님은 괜찮을듯",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.2",
          "form": "님이 내 침대에서자면",
          "original_form": "님이 내 침대에서자면",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.4",
          "form": "나는 님의",
          "original_form": "나는 님의",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.5",
          "form": "침대에서",
          "original_form": "침대에서",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.6",
          "form": "자면",
          "original_form": "자면",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.7",
          "form": "되",
          "original_form": "되",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.8",
          "form": "니까",
          "original_form": "니까",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.9",
          "form": "나도 ㄱㅊ다",
          "original_form": "나도 ㄱㅊ다",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.10",
          "form": "뭐갸욕이냐구",
          "original_form": "뭐갸욕이냐구",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.11",
          "form": "그만 짤라",
          "original_form": "그만 짤라",
          "speaker_id": "2",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100020310.1.13",
          "form": "글고 책상 닦느넉 괜챊읃데",
          "original_form": "글고 책상 닦느넉 괜챊읃데",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.14",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.15",
          "form": "안닦을듯",
          "original_form": "안닦을듯",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.16",
          "form": "버번에 엄마가 책상 맘대로 닥앗는데",
          "original_form": "버번에 엄마가 책상 맘대로 닥앗는데",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.17",
          "form": "걱정마세요",
          "original_form": "걱정마세요",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.18",
          "form": "멎지랑 머리카라깅 오히려",
          "original_form": "멎지랑 머리카라깅 오히려",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.19",
          "form": "더 묻어잇어가지고",
          "original_form": "더 묻어잇어가지고",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.20",
          "form": "짜증",
          "original_form": "짜증",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.21",
          "form": "닦을일이업으니",
          "original_form": "닦을일이업으니",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.22",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.23",
          "form": "내책상이나",
          "original_form": "내책상이나",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.24",
          "form": "잘다끙면",
          "original_form": "잘다끙면",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.26",
          "form": "좀 낫지얺을까싶다",
          "original_form": "좀 낫지얺을까싶다",
          "speaker_id": "1",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.27",
          "form": "구리고....",
          "original_form": "구리고....",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100020310.1.28",
          "form": "말안하고",
          "original_form": "말안하고",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.29",
          "form": "나 음싱은",
          "original_form": "나 음싱은",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.30",
          "form": "사람대려오는거",
          "original_form": "사람대려오는거",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.31",
          "form": "냉장고에",
          "original_form": "냉장고에",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.32",
          "form": "진짜 개 안되는거만 냉장고 앞에",
          "original_form": "진짜 개 안되는거만 냉장고 앞에",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.33",
          "form": "실어함",
          "original_form": "실어함",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.34",
          "form": "화이트보드나 뭐",
          "original_form": "화이트보드나 뭐",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.35",
          "form": "메모 달아두고",
          "original_form": "메모 달아두고",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.36",
          "form": "나머지는 맘대로 먹어도 괜란",
          "original_form": "나머지는 맘대로 먹어도 괜란",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.37",
          "form": "그건 나도",
          "original_form": "그건 나도",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.38",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.39",
          "form": "기본이지",
          "original_form": "기본이지",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.40",
          "form": "난 집애서",
          "original_form": "난 집애서",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.41",
          "form": "그게잘안된다 ㅎㄹ",
          "original_form": "그게잘안된다 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.42",
          "form": "난 근데ㅠ데려올 일 없을듯?",
          "original_form": "난 근데ㅠ데려올 일 없을듯?",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.43",
          "form": "ㅎㅎ임",
          "original_form": "ㅎㅎ임",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.44",
          "form": "그게뭔데요",
          "original_form": "그게뭔데요",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020310.1.45",
          "form": "왜요 잇읏ㄹ수도잇지",
          "original_form": "왜요 잇읏ㄹ수도잇지",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.46",
          "form": "말업이 누가 방문하는거요",
          "original_form": "말업이 누가 방문하는거요",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.47",
          "form": "님은 말업싱 잦주 데리고왓어요?",
          "original_form": "님은 말업싱 잦주 데리고왓어요?",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.48",
          "form": "아뇨 지금도 막",
          "original_form": "아뇨 지금도 막",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.49",
          "form": "고뫃나삼촌이",
          "original_form": "고뫃나삼촌이",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.50",
          "form": "갑자기 집애오고",
          "original_form": "갑자기 집애오고",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.51",
          "form": "이럴때 많잔아요",
          "original_form": "이럴때 많잔아요",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.52",
          "form": "아 남들이 그런다고 ㅋ",
          "original_form": "아 남들이 그런다고 ㅋ",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.53",
          "form": "그게 너무 스트레스이",
          "original_form": "그게 너무 스트레스이",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.54",
          "form": "위리집은 .. 너무 좁은나머지",
          "original_form": "위리집은 .. 너무 좁은나머지",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020310.1.55",
          "form": "엄마아빠가 누구 데려오는거 금지시팀",
          "original_form": "엄마아빠가 누구 데려오는거 금지시팀",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020310.1.56",
          "form": "그래서 누구 안데려오는게 너무 당연한 일상이 되엇네요",
          "original_form": "그래서 누구 안데려오는게 너무 당연한 일상이 되엇네요",
          "speaker_id": "2",
          "time": "20210828 01:19"
        }
      ]
