      "utterance": [
        {
          "id": "MDRW2100045845.1.1",
          "form": "어제는 비오고 우중충하더니",
          "original_form": "어제는 비오고 우중충하더니",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045845.1.2",
          "form": "시원해서 좋네요",
          "original_form": "시원해서 좋네요",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100045845.1.3",
          "form": "이런날은 걷기 운동이 짱인데",
          "original_form": "이런날은 걷기 운동이 짱인데",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045845.1.4",
          "form": "선선해서 운동 잘될듯요",
          "original_form": "선선해서 운동 잘될듯요",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045845.1.5",
          "form": "가을엔 꽃게가  제철이래요",
          "original_form": "가을엔 꽃게가  제철이래요",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045845.1.6",
          "form": "어 파는거 봤는대",
          "original_form": "어 파는거 봤는대",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100045845.1.7",
          "form": "요즘 살아있는거 팔더라구요",
          "original_form": "요즘 살아있는거 팔더라구요",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045845.1.8",
          "form": "무슨 요리해 드새요",
          "original_form": "무슨 요리해 드새요",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045845.1.9",
          "form": "할줄몰라 걍 찌는걸로 ㅋㅋ",
          "original_form": "할줄몰라 걍 찌는걸로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045845.1.10",
          "form": "제철이면 그래도 될듯요",
          "original_form": "제철이면 그래도 될듯요",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100045845.1.11",
          "form": "꽃게탕도 찾아봐야할듯요",
          "original_form": "꽃게탕도 찾아봐야할듯요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
