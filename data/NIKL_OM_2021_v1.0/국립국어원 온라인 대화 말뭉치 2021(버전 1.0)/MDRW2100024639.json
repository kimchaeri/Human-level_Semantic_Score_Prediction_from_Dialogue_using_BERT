      "utterance": [
        {
          "id": "MDRW2100024639.1.1",
          "form": "저녁은 뭘 드시나요?",
          "original_form": "저녁은 뭘 드시나요?",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100024639.1.2",
          "form": "아직 메뉴를 결정 안했네요",
          "original_form": "아직 메뉴를 결정 안했네요",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024639.1.3",
          "form": "비가오니 따뜻한거 먹을까해요",
          "original_form": "비가오니 따뜻한거 먹을까해요",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024639.1.4",
          "form": "칼국수나 수제비 먹는다는 분들도 계시더라구요",
          "original_form": "칼국수나 수제비 먹는다는 분들도 계시더라구요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024639.1.5",
          "form": "저는 부침개가 생각나구요",
          "original_form": "저는 부침개가 생각나구요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024639.1.6",
          "form": "부침개 너무 좋아해요",
          "original_form": "부침개 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100024639.1.7",
          "form": "어제 치킨배달 시켜먹었으니 오늘은 요리를 해야해요",
          "original_form": "어제 치킨배달 시켜먹었으니 오늘은 요리를 해야해요",
          "speaker_id": "2",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100024639.1.8",
          "form": "하하 그렇군요.. 매일 배달 시킬 순 업으니요",
          "original_form": "하하 그렇군요.. 매일 배달 시킬 순 업으니요",
          "speaker_id": "1",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024639.1.9",
          "form": "김치부침으로 할까해요",
          "original_form": "김치부침으로 할까해요",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024639.1.10",
          "form": "묵은지가 많아서 처리해야하거든요",
          "original_form": "묵은지가 많아서 처리해야하거든요",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024639.1.11",
          "form": "뭐드실건가요?",
          "original_form": "뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024639.1.12",
          "form": "저도 전쪽을 생각했는데.. 김치전을 할지 감자전을 할지 고민입니다",
          "original_form": "저도 전쪽을 생각했는데.. 김치전을 할지 감자전을 할지 고민입니다",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024639.1.13",
          "form": "김치도 많은데 감자도 만거든요",
          "original_form": "김치도 많은데 감자도 만거든요",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024639.1.14",
          "form": "감자전을 얼마전에 먹어서요",
          "original_form": "감자전을 얼마전에 먹어서요",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100024639.1.15",
          "form": "그렇군요.. 장마라 전을 자주 먹어요..ㅎㅎ",
          "original_form": "그렇군요.. 장마라 전을 자주 먹어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024639.1.16",
          "form": "얼마전에 야채전 먹었었죠",
          "original_form": "얼마전에 야채전 먹었었죠",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024639.1.17",
          "form": "ㅎㅎ 다 비슷하게 먹는군요",
          "original_form": "ㅎㅎ 다 비슷하게 먹는군요",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024639.1.18",
          "form": "막걸리와 같이 드세요",
          "original_form": "막걸리와 같이 드세요",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024639.1.19",
          "form": "ㅎㅎ 비오는 날 떠올리는 음식이 비슷한가봅니다",
          "original_form": "ㅎㅎ 비오는 날 떠올리는 음식이 비슷한가봅니다",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024639.1.20",
          "form": "막걸리랑 먹었죠..ㅎㅎ 오늘은 막걸리가 업네요 아십게도",
          "original_form": "막걸리랑 먹었죠..ㅎㅎ 오늘은 막걸리가 업네요 아십게도",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024639.1.21",
          "form": "저는 갑자기 메뉴 변경해서 알밥을 먹기로했어요",
          "original_form": "저는 갑자기 메뉴 변경해서 알밥을 먹기로했어요",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024639.1.22",
          "form": "날치알 꺼내 놓은게 있어서요",
          "original_form": "날치알 꺼내 놓은게 있어서요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024639.1.23",
          "form": "알밥 맛있겠네요",
          "original_form": "알밥 맛있겠네요",
          "speaker_id": "1",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024639.1.24",
          "form": "메뉴결정하셨나요?",
          "original_form": "메뉴결정하셨나요?",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024639.1.25",
          "form": "김치전은 남편이 좋아하고 감자전은 딸이 좋아해서 고민이네요..ㅎㅎ 그냥 둘다 해야할까 봅니다",
          "original_form": "김치전은 남편이 좋아하고 감자전은 딸이 좋아해서 고민이네요..ㅎㅎ 그냥 둘다 해야할까 봅니다",
          "speaker_id": "1",
          "time": "20210831 18:05"
        },
        {
          "id": "MDRW2100024639.1.26",
          "form": "수저얹고 싶네요 ㅋ",
          "original_form": "수저얹고 싶네요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024639.1.27",
          "form": "맛있는 알밥 드시면서..ㅎㅎ",
          "original_form": "맛있는 알밥 드시면서..ㅎㅎ",
          "speaker_id": "1",
