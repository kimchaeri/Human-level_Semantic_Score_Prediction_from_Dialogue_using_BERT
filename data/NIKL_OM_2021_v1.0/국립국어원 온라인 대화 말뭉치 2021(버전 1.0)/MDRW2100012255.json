      "utterance": [
        {
          "id": "MDRW2100012255.1.1",
          "form": "맞제 너무덥다",
          "original_form": "맞제 너무덥다",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012255.1.2",
          "form": "비는시원하게 내리던데",
          "original_form": "비는시원하게 내리던데",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012255.1.3",
          "form": "여름은 여름인듯 막바지네",
          "original_form": "여름은 여름인듯 막바지네",
          "speaker_id": "1",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012255.1.4",
          "form": "내일도 비와?",
          "original_form": "내일도 비와?",
          "speaker_id": "2",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012255.1.5",
          "form": "계속 비라고 하긴하던데",
          "original_form": "계속 비라고 하긴하던데",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012255.1.6",
          "form": "여기는 비 안와",
          "original_form": "여기는 비 안와",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012255.1.7",
          "form": "비오니 너무꿉꿉하네",
          "original_form": "비오니 너무꿉꿉하네",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012255.1.8",
          "form": "아까 대구는 엄청내렸어",
          "original_form": "아까 대구는 엄청내렸어",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012255.1.9",
          "form": "여기는 그치긴했는데",
          "original_form": "여기는 그치긴했는데",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012255.1.10",
          "form": "이러다 가을 오겠지",
          "original_form": "이러다 가을 오겠지",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012255.1.11",
          "form": "짧은 옷이 춥더라",
          "original_form": "짧은 옷이 춥더라",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012255.1.12",
          "form": "밖에? 대구는 아직 더운데",
          "original_form": "밖에? 대구는 아직 더운데",
          "speaker_id": "2",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012255.1.13",
          "form": "춥다니...이상하군.",
          "original_form": "춥다니...이상하군.",
          "speaker_id": "2",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012255.1.14",
          "form": "춥다 덥다하다",
          "original_form": "춥다 덥다하다",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012255.1.15",
          "form": "곧 가을이네",
          "original_form": "곧 가을이네",
          "speaker_id": "2",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012255.1.16",
          "form": "난 가을이 별로다",
          "original_form": "난 가을이 별로다",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012255.1.17",
          "form": "애매한계절..으스스 서글프고",
          "original_form": "애매한계절..으스스 서글프고",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012255.1.18",
          "form": "특히 초겨울ㅜ스산하고 춥고 옷도 안 따뜻하고ㅜ바람불고",
          "original_form": "특히 초겨울ㅜ스산하고 춥고 옷도 안 따뜻하고ㅜ바람불고",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012255.1.19",
          "form": "난 초여름이 좋다",
          "original_form": "난 초여름이 좋다",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100012255.1.20",
          "form": "나도 초여름",
          "original_form": "나도 초여름",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012255.1.21",
          "form": "맞제",
          "original_form": "맞제",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012255.1.22",
          "form": "늦가을은 초겨울은 진짜 싫다",
          "original_form": "늦가을은 초겨울은 진짜 싫다",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012255.1.23",
          "form": "초록초록하니",
          "original_form": "초록초록하니",
          "speaker_id": "1",
          "time": "20210824 21:23"
        }
      ]
