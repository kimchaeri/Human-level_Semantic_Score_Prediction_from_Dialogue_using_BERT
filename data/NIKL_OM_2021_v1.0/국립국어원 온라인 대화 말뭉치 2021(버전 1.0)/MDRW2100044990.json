      "utterance": [
        {
          "id": "MDRW2100044990.1.1",
          "form": "전 갤럭시 워치 4요!",
          "original_form": "전 갤럭시 워치 4요!",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044990.1.2",
          "form": "사고싶으신 위시리스트 있으세요?",
          "original_form": "사고싶으신 위시리스트 있으세요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044990.1.3",
          "form": "워치!! 좋조!!",
          "original_form": "워치!! 좋조!!",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044990.1.4",
          "form": "이번에 반응이 좋더라구요",
          "original_form": "이번에 반응이 좋더라구요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044990.1.5",
          "form": "BMI도 된다고 하더라구요!",
          "original_form": "BMI도 된다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044990.1.6",
          "form": "그쵸 ㅋㅋㅋㅋ 그 기능 좀 탐나요",
          "original_form": "그쵸 ㅋㅋㅋㅋ 그 기능 좀 탐나요",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044990.1.7",
          "form": "앜ㅋㅋㅋㅋ 그 기능이 탐이나셨군요",
          "original_form": "앜ㅋㅋㅋㅋ 그 기능이 탐이나셨군요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044990.1.8",
          "form": "저는 그냥전화 되는 기능이요",
          "original_form": "저는 그냥전화 되는 기능이요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044990.1.9",
          "form": "워치에서 전화 받는거요?",
          "original_form": "워치에서 전화 받는거요?",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044990.1.10",
          "form": "네네!! 제 워치는 조금 초반 세대라서 전화는 안되거든요 ㅋㅋㅋ",
          "original_form": "네네!! 제 워치는 조금 초반 세대라서 전화는 안되거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044990.1.11",
          "form": "아하 이것도 나중세대에서 추가된 기능이엇군요 ㅋㅋㅋㅋ",
          "original_form": "아하 이것도 나중세대에서 추가된 기능이엇군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044990.1.12",
          "form": "지금 워치 쓰고계세요?",
          "original_form": "지금 워치 쓰고계세요?",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044990.1.13",
          "form": "네네 쓰고 있어요",
          "original_form": "네네 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044990.1.14",
          "form": "거의 만보기에 가깝지만요 ㅋㅋ",
          "original_form": "거의 만보기에 가깝지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044990.1.15",
          "form": "어떤 기능을 젤 많이 쓰세요?",
          "original_form": "어떤 기능을 젤 많이 쓰세요?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044990.1.16",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044990.1.17",
          "form": "주로 워치한데 많이 혼나요",
          "original_form": "주로 워치한데 많이 혼나요",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044990.1.18",
          "form": "제발 좀 걸으라고 그만 앉아있으라며..",
          "original_form": "제발 좀 걸으라고 그만 앉아있으라며..",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044990.1.19",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044990.1.20",
          "form": "그런 알림도 잇나요??",
          "original_form": "그런 알림도 잇나요??",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044990.1.21",
          "form": "얘가 자꾸 잔소리해요 ㅠㅠ",
          "original_form": "얘가 자꾸 잔소리해요 ㅠㅠ",
