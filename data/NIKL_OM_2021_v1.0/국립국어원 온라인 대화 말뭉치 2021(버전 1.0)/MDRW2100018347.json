      "utterance": [
        {
          "id": "MDRW2100018347.1.1",
          "form": "수강신청 하셧나요?",
          "original_form": "수강신청 하셧나요?",
          "speaker_id": "1",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100018347.1.2",
          "form": "안녕하세요~ 학생이 아닌지라! 학생이세요.",
          "original_form": "안녕하세요~ 학생이 아닌지라! 학생이세요.",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100018347.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100018347.1.4",
          "form": "수강신청 잘 하셨나요~",
          "original_form": "수강신청 잘 하셨나요~",
          "speaker_id": "2",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018347.1.5",
          "form": "저도 어른이라.. ㅎㅎㅎ",
          "original_form": "저도 어른이라.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018347.1.6",
          "form": "학교때 수업은 잘들으셧나여?",
          "original_form": "학교때 수업은 잘들으셧나여?",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018347.1.7",
          "form": "네 나름대로 열심히 들었죠!ㅋㅋㅋ 근데 지금은 정말 기억이 안나네요......",
          "original_form": "네 나름대로 열심히 들었죠!ㅋㅋㅋ 근데 지금은 정말 기억이 안나네요......",
          "speaker_id": "2",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100018347.1.8",
          "form": "수업 때 어떠셨나요?",
          "original_form": "수업 때 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100018347.1.9",
          "form": "저는 모범생이라.. ㅋㅋㅋ",
          "original_form": "저는 모범생이라.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018347.1.10",
          "form": "곧은 자세로.. 집중해서.. ㅋㅋㅋㅋ",
          "original_form": "곧은 자세로.. 집중해서.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018347.1.11",
          "form": "에프 받아존적은 없으세요?",
          "original_form": "에프 받아존적은 없으세요?",
          "speaker_id": "1",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018347.1.12",
          "form": "공부도 엄청 잘 하셨을 것 같네요...!!!",
          "original_form": "공부도 엄청 잘 하셨을 것 같네요...!!!",
          "speaker_id": "2",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100018347.1.13",
          "form": "네 ㅋㅋㅋ 에프는 다행히도 없었네요! 에프 받으신 적 없으실 것 같아요!",
          "original_form": "네 ㅋㅋㅋ 에프는 다행히도 없었네요! 에프 받으신 적 없으실 것 같아요!",
          "speaker_id": "2",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100018347.1.14",
          "form": "저는 비뿔 이하 없었어요.. ㅋㅋㅋ",
          "original_form": "저는 비뿔 이하 없었어요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018347.1.15",
          "form": "어머......",
          "original_form": "어머......",
          "speaker_id": "2",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018347.1.16",
          "form": "꾸진 학교라.. 점수가..",
          "original_form": "꾸진 학교라.. 점수가..",
          "speaker_id": "1",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018347.1.17",
          "form": "그래놓고 엄청 좋은 학교셨던건 아니실지....ㅠㅠㅋㅋㅋ",
          "original_form": "그래놓고 엄청 좋은 학교셨던건 아니실지....ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018347.1.18",
          "form": "학교 생활은 재미있으셨나요~?",
          "original_form": "학교 생활은 재미있으셨나요~?",
          "speaker_id": "2",
          "time": "20210827 00:13"
        },
        {
          "id": "MDRW2100018347.1.19",
          "form": "ㅋㅋㅋ 학교 앞에서 자취해서..",
          "original_form": "ㅋㅋㅋ 학교 앞에서 자취해서..",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100018347.1.20",
          "form": "익사이팅했죠... 그냥 사랑방이라..",
          "original_form": "익사이팅했죠... 그냥 사랑방이라..",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100018347.1.21",
          "form": "앗ㅋㅋㅋ 자취하는 분들은 꼭 사랑방이 되곤 하더라구요",
          "original_form": "앗ㅋㅋㅋ 자취하는 분들은 꼭 사랑방이 되곤 하더라구요",
          "speaker_id": "2",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018347.1.22",
          "form": "친구분들 많이 오셨겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "친구분들 많이 오셨겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100018347.1.23",
          "form": "개판이죠..ㅋㅋㅋ",
          "original_form": "개판이죠..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100018347.1.24",
          "form": "진짜 다들 술먹고 개가되더라구요..",
          "original_form": "진짜 다들 술먹고 개가되더라구요..",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100018347.1.25",
          "form": "지금 생각해보니 학교생활 잼있었네요..",
          "original_form": "지금 생각해보니 학교생활 잼있었네요..",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100018347.1.26",
          "form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ 학교 다니실 때 술도 많이 드셨나요....ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋㅋㅋ 학교 다니실 때 술도 많이 드셨나요....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018347.1.27",
          "form": "근데 다들 대학생 되면 술 진짜 궤짝으로도 마시겠더라구요 ㅠㅠㅋㅋㅋ",
          "original_form": "근데 다들 대학생 되면 술 진짜 궤짝으로도 마시겠더라구요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018347.1.28",
          "form": "저도 그때가 제일 술 많이 마신 때 같네요~",
          "original_form": "저도 그때가 제일 술 많이 마신 때 같네요~",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018347.1.29",
          "form": "저는 술이 안취해서 많이 안마샤요.. ㅋㅋ",
          "original_form": "저는 술이 안취해서 많이 안마샤요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018347.1.30",
          "form": "진짜 학생들은 미친듯.. ㅋㅋㅋ 마시죠..",
          "original_form": "진짜 학생들은 미친듯.. ㅋㅋㅋ 마시죠..",
