        },
        {
          "id": "MDRW2100020380.1.2",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020380.1.3",
          "form": "평소에 집에서 요리 많이 하시나요?",
          "original_form": "평소에 집에서 요리 많이 하시나요?",
          "speaker_id": "2",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020380.1.4",
          "form": "네.",
          "original_form": "네.",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020380.1.5",
          "form": "name2님은여?",
          "original_form": "&name2&님은여?",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020380.1.6",
          "form": "저도 자주 하는 편이에요",
          "original_form": "저도 자주 하는 편이에요",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020380.1.7",
          "form": "내일 뭐드세요",
          "original_form": "내일 뭐드세요",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100020380.1.8",
          "form": "아직 생각해보지 않아서,\r\n아마 자고 일어나야 생각할거 같아요",
          "original_form": "아직 생각해보지 않아서,\r\n아마 자고 일어나야 생각할거 같아요",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020380.1.9",
          "form": "오 그렇군요.",
          "original_form": "오 그렇군요.",
          "speaker_id": "1",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020380.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020380.1.11",
          "form": "저는 고로케 해먹을까 고민중이에요",
          "original_form": "저는 고로케 해먹을까 고민중이에요",
          "speaker_id": "1",
          "time": "20210828 02:45"
        },
        {
          "id": "MDRW2100020380.1.12",
          "form": "오 고로케 맛있죠!",
          "original_form": "오 고로케 맛있죠!",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100020380.1.13",
          "form": "어떤 재료 넣어서 만드세요?",
          "original_form": "어떤 재료 넣어서 만드세요?",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100020380.1.14",
          "form": "햄이랑 맛살 양파 카레 감자 등등 넣으려고했었는데\r\n안먹엇어요",
          "original_form": "햄이랑 맛살 양파 카레 감자 등등 넣으려고했었는데\r\n안먹엇어요",
          "speaker_id": "1",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100020380.1.15",
          "form": "앗 그렇군요\r\n계획을 바꾸셨나봐요",
          "original_form": "앗 그렇군요\r\n계획을 바꾸셨나봐요",
          "speaker_id": "2",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100020380.1.16",
          "form": "그냥 인스턴트 쌀국수 먹엇어요",
          "original_form": "그냥 인스턴트 쌀국수 먹엇어요",
          "speaker_id": "1",
          "time": "20210828 15:18"
        }
