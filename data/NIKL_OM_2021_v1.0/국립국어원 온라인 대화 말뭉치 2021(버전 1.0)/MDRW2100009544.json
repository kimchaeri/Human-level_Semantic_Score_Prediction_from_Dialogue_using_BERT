      "utterance": [
        {
          "id": "MDRW2100009544.1.1",
          "form": "근데 담주예고보니",
          "original_form": "근데 담주예고보니",
          "speaker_id": "1",
          "time": "20210823 00:25"
        },
        {
          "id": "MDRW2100009544.1.2",
          "form": "주단태도 탈주했더라구요",
          "original_form": "주단태도 탈주했더라구요",
          "speaker_id": "1",
          "time": "20210823 00:25"
        },
        {
          "id": "MDRW2100009544.1.3",
          "form": "한국으로 돌아옴",
          "original_form": "한국으로 돌아옴",
          "speaker_id": "1",
          "time": "20210823 00:25"
        },
        {
          "id": "MDRW2100009544.1.4",
          "form": "비서는 다어디갔어요?",
          "original_form": "비서는 다어디갔어요?",
          "speaker_id": "2",
          "time": "20210823 00:25"
        },
        {
          "id": "MDRW2100009544.1.5",
          "form": "조비서",
          "original_form": "조비서",
          "speaker_id": "2",
          "time": "20210823 00:26"
        },
        {
          "id": "MDRW2100009544.1.6",
          "form": "제니아빠편이죠?",
          "original_form": "제니아빠편이죠?",
          "speaker_id": "2",
          "time": "20210823 00:26"
        },
        {
          "id": "MDRW2100009544.1.7",
          "form": "비서는 감옥",
          "original_form": "비서는 감옥",
          "speaker_id": "1",
          "time": "20210823 00:26"
        },
        {
          "id": "MDRW2100009544.1.8",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:26"
        },
        {
          "id": "MDRW2100009544.1.9",
          "form": "아니다 근데 드간것같진않아요",
          "original_form": "아니다 근데 드간것같진않아요",
          "speaker_id": "1",
          "time": "20210823 00:26"
        },
        {
          "id": "MDRW2100009544.1.10",
          "form": "하은별이 엄마 죽일려는건가요",
          "original_form": "하은별이 엄마 죽일려는건가요",
          "speaker_id": "2",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009544.1.11",
          "form": "주단태가 오윤희건으로 경찰한테  쫓길때. 탈주를도왔다고해서",
          "original_form": "주단태가 오윤희건으로 경찰한테  쫓길때. 탈주를도왔다고해서",
          "speaker_id": "1",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009544.1.12",
          "form": "잡혔는데조비서가",
          "original_form": "잡혔는데조비서가",
          "speaker_id": "1",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009544.1.13",
          "form": "근데 담주예고에 나오는거보면 감옥은안간것같구요. 제니아빠 밑에있던 조폭이었는데",
          "original_form": "근데 담주예고에 나오는거보면 감옥은안간것같구요. 제니아빠 밑에있던 조폭이었는데",
          "speaker_id": "1",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009544.1.14",
          "form": "주단태쪽으로?좀? 더기운것같은데",
          "original_form": "주단태쪽으로?좀? 더기운것같은데",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009544.1.15",
          "form": "역시 권력에 휘둘리는군요",
          "original_form": "역시 권력에 휘둘리는군요",
          "speaker_id": "2",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009544.1.16",
          "form": "오윤희때보면 막 제니아빠가 주단태한테 협박하고 맞을때",
          "original_form": "오윤희때보면 막 제니아빠가 주단태한테 협박하고 맞을때",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009544.1.17",
          "form": "오윤희 죽인거 증거 남기려는건지 동영상인가뭔가찍고",
          "original_form": "오윤희 죽인거 증거 남기려는건지 동영상인가뭔가찍고",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009544.1.18",
          "form": "주단태편이었거든요",
          "original_form": "주단태편이었거든요",
          "speaker_id": "1",
          "time": "20210823 00:28"
        },
        {
          "id": "MDRW2100009544.1.19",
          "form": "내막이 있을지도  모르죠",
          "original_form": "내막이 있을지도  모르죠",
          "speaker_id": "2",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009544.1.20",
          "form": "그니까요ㅋㅋ그래서",
          "original_form": "그니까요ㅋㅋ그래서",
          "speaker_id": "1",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009544.1.21",
          "form": "반전에 반전에 반전ㅎ",
          "original_form": "반전에 반전에 반전ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009544.1.22",
          "form": "제니아빠를 도우려면?오윤희가 살아있어야하니",
          "original_form": "제니아빠를 도우려면?오윤희가 살아있어야하니",
          "speaker_id": "1",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009544.1.23",
          "form": "그런가능성도 있다",
          "original_form": "그런가능성도 있다",
          "speaker_id": "1",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009544.1.24",
          "form": "이런ㅋㅋㅈㅋ",
          "original_form": "이런ㅋㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100009544.1.25",
          "form": "이야기죠~",
          "original_form": "이야기죠~",
          "speaker_id": "1",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100009544.1.26",
          "form": "은별이는 엄마의 나쁜기억을 없애주러고",
          "original_form": "은별이는 엄마의 나쁜기억을 없애주러고",
          "speaker_id": "1",
          "time": "20210823 00:30"
        }
      ]
    }
