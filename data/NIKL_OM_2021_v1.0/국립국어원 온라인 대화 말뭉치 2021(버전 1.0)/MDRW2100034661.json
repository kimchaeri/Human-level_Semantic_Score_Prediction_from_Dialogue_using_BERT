        },
        {
          "id": "MDRW2100034661.1.2",
          "form": "대단한기능",
          "original_form": "대단한기능",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034661.1.3",
          "form": "계산도잘함",
          "original_form": "계산도잘함",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100034661.1.4",
          "form": "독립하게되문 세탁기돌려",
          "original_form": "독립하게되문 세탁기돌려",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100034661.1.5",
          "form": "아 시험 진짜",
          "original_form": "아 시험 진짜",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100034661.1.6",
          "form": "하 망한듯",
          "original_form": "하 망한듯",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100034661.1.7",
          "form": "가답안 영상보니",
          "original_form": "가답안 영상보니",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100034661.1.8",
          "form": "채점중?",
          "original_form": "채점중?",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100034661.1.9",
          "form": "개망함",
          "original_form": "개망함",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100034661.1.10",
          "form": "채점은 또 정확히못한게",
          "original_form": "채점은 또 정확히못한게",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100034661.1.11",
          "form": "답 못적어옴",
          "original_form": "답 못적어옴",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100034661.1.12",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100034661.1.13",
          "form": "좀 섭섭하겠네",
          "original_form": "좀 섭섭하겠네",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100034661.1.14",
          "form": "아니 시험관이 답적어가면 무효처리하고1년간 못치게한대서",
          "original_form": "아니 시험관이 답적어가면 무효처리하고1년간 못치게한대서",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100034661.1.15",
          "form": "홍",
          "original_form": "홍",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100034661.1.16",
          "form": "어케그래",
          "original_form": "어케그래",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100034661.1.17",
          "form": "문제유출이래여",
          "original_form": "문제유출이래여",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100034661.1.18",
          "form": "글쿠낭",
          "original_form": "글쿠낭",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100034661.1.19",
          "form": "아니 또 그렇게 미리 고지를 하니까",
          "original_form": "아니 또 그렇게 미리 고지를 하니까",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100034661.1.20",
          "form": "못적어오겠는거임",
          "original_form": "못적어오겠는거임",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100034661.1.21",
          "form": "어쩔수없지",
          "original_form": "어쩔수없지",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100034661.1.22",
