      "utterance": [
        {
          "id": "MDRW2100043975.1.1",
          "form": "가을 옷 꺼냈어요?",
          "original_form": "가을 옷 꺼냈어요?",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043975.1.2",
          "form": "아직이야",
          "original_form": "아직이야",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043975.1.3",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043975.1.4",
          "form": "저는 여름옷 이제 조금씩 정리 하고 있네요",
          "original_form": "저는 여름옷 이제 조금씩 정리 하고 있네요",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043975.1.5",
          "form": "전 아직 생각만 하고 있어요",
          "original_form": "전 아직 생각만 하고 있어요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043975.1.6",
          "form": "여름이 길게 느껴졌는데",
          "original_form": "여름이 길게 느껴졌는데",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043975.1.7",
          "form": "막상 여름 옷 집어넣으려고 하다 보니 참",
          "original_form": "막상 여름 옷 집어넣으려고 하다 보니 참",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043975.1.8",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043975.1.9",
          "form": "금방 지나가는 거 같아요",
          "original_form": "금방 지나가는 거 같아요",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043975.1.10",
          "form": "사계절이 늘 그래요 지나면 아쉬워요",
          "original_form": "사계절이 늘 그래요 지나면 아쉬워요",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043975.1.11",
          "form": "이번 여름 덥다 그래서 바짝 긴장했는데",
          "original_form": "이번 여름 덥다 그래서 바짝 긴장했는데",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043975.1.12",
          "form": "지나고 나니 별로 그렇게 덥지 않았던 거 같고",
          "original_form": "지나고 나니 별로 그렇게 덥지 않았던 거 같고",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043975.1.13",
          "form": "저도 많이 더워 땀 느낌은 없는 듯해요",
          "original_form": "저도 많이 더워 땀 느낌은 없는 듯해요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043975.1.14",
          "form": "마스크를 쓰고 있으니 좀 답답하긴 했지만",
          "original_form": "마스크를 쓰고 있으니 좀 답답하긴 했지만",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043975.1.15",
          "form": "그럭저럭 견딜 만 했던 듯해요",
          "original_form": "그럭저럭 견딜 만 했던 듯해요",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043975.1.16",
          "form": "겨울 추위는 어쩔지 벌써 걱정이네요",
          "original_form": "겨울 추위는 어쩔지 벌써 걱정이네요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043975.1.17",
          "form": "겨울에 덜 춥고 여름에 덜 더우면",
          "original_form": "겨울에 덜 춥고 여름에 덜 더우면",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043975.1.18",
          "form": "좋을텐데요",
          "original_form": "좋을텐데요",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043975.1.19",
          "form": "그렇지만 자연을 생각하면 여름은 여름답고 겨울은 겨울다운게 맞는 거 같아요",
          "original_form": "그렇지만 자연을 생각하면 여름은 여름답고 겨울은 겨울다운게 맞는 거 같아요",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043975.1.20",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "1",
          "time": "20210907 21:45"
        }
      ]
