      "utterance": [
        {
          "id": "MDRW2100047672.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100047672.1.2",
          "form": "저 이제 먹으려거요~~~! name1님은요?",
          "original_form": "저 이제 먹으려거요~~~! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100047672.1.3",
          "form": "저는 볶음쌀국수 먹었어용",
          "original_form": "저는 볶음쌀국수 먹었어용",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100047672.1.4",
          "form": "뭐 드시러 가시나요 !",
          "original_form": "뭐 드시러 가시나요 !",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100047672.1.5",
          "form": "하 저는... 죽 먹어요....",
          "original_form": "하 저는... 죽 먹어요....",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100047672.1.6",
          "form": "식도염때문에요ㅜㅜㅋㅋㅋㅋ 볶음 쌀 국 수 맛 있 겠 다 !!!!!!!!!!!!!!",
          "original_form": "식도염때문에요ㅜㅜㅋㅋㅋㅋ 볶음 쌀 국 수 맛 있 겠 다 !!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100047672.1.7",
          "form": "죽도 .. 맛있죠... 담백하고.. 건강에ㅜ좋고 ..!",
          "original_form": "죽도 .. 맛있죠... 담백하고.. 건강에ㅜ좋고 ..!",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100047672.1.8",
          "form": "죽이랑 쌀국수는 소화가 빨리 되는 건 기분 탓일까요..?",
          "original_form": "죽이랑 쌀국수는 소화가 빨리 되는 건 기분 탓일까요..?",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100047672.1.9",
          "form": "하 맞아요!!!!!!",
          "original_form": "하 맞아요!!!!!!",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100047672.1.10",
          "form": "금방 다시 배고파지잖아요ㅠ",
          "original_form": "금방 다시 배고파지잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100047672.1.11",
          "form": "면종류가 특히 더 그러죠?!",
          "original_form": "면종류가 특히 더 그러죠?!",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100047672.1.12",
          "form": "맞아요.. 과장 보태서 돌아서면 배고픈 ..",
          "original_form": "맞아요.. 과장 보태서 돌아서면 배고픈 ..",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100047672.1.13",
          "form": "저녁도 죽드시나요 ㅠㅠ",
          "original_form": "저녁도 죽드시나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100047672.1.14",
          "form": "아뇨 속을 버리고 파스타 먹을 겁니다!!!!!!",
          "original_form": "아뇨 속을 버리고 파스타 먹을 겁니다!!!!!!",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100047672.1.15",
          "form": "백신핑계로 다 먹네요 저...",
          "original_form": "백신핑계로 다 먹네요 저...",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100047672.1.16",
          "form": "좋죠 파스타 저도 어제 백신핑계로 먹고싶었던 간식 다 먹었어요..^^",
          "original_form": "좋죠 파스타 저도 어제 백신핑계로 먹고싶었던 간식 다 먹었어요..^^",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100047672.1.17",
          "form": "먹어야 힘이나서 좋은작용을 하지않을까라는 생각해봅니다..",
          "original_form": "먹어야 힘이나서 좋은작용을 하지않을까라는 생각해봅니다..",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100047672.1.18",
          "form": "어떤 파스타 드실예정이신가요!",
          "original_form": "어떤 파스타 드실예정이신가요!",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
