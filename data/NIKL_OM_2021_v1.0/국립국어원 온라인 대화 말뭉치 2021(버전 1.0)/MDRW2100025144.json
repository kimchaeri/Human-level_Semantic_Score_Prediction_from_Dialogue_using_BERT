      "utterance": [
        {
          "id": "MDRW2100025144.1.1",
          "form": "요즘 무슨게임하니",
          "original_form": "요즘 무슨게임하니",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025144.1.2",
          "form": "요즘은 특별히 하는 건 없는데 기대되는 게임은 있어",
          "original_form": "요즘은 특별히 하는 건 없는데 기대되는 게임은 있어",
          "speaker_id": "1",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025144.1.3",
          "form": "'도깨비' 아니?",
          "original_form": "'도깨비' 아니?",
          "speaker_id": "1",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025144.1.4",
          "form": "아 알지 그거 유명하잖아",
          "original_form": "아 알지 그거 유명하잖아",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025144.1.5",
          "form": "한국배경 잘살려서",
          "original_form": "한국배경 잘살려서",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100025144.1.6",
          "form": "반응 좋길래 기대하는 중",
          "original_form": "반응 좋길래 기대하는 중",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025144.1.7",
          "form": "난 아직도 쿠키런해",
          "original_form": "난 아직도 쿠키런해",
          "speaker_id": "2",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025144.1.8",
          "form": "그거 오래하네",
          "original_form": "그거 오래하네",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025144.1.9",
          "form": "나는 샤벳맛 쿠키가 좋았어",
          "original_form": "나는 샤벳맛 쿠키가 좋았어",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025144.1.10",
          "form": "다시들어와",
          "original_form": "다시들어와",
          "speaker_id": "2",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025144.1.11",
          "form": "맥스업 이벤트해",
          "original_form": "맥스업 이벤트해",
          "speaker_id": "2",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100025144.1.12",
          "form": "쿠킹덤은 안 해?",
          "original_form": "쿠킹덤은 안 해?",
          "speaker_id": "1",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.13",
          "form": "그것도 했었잖아",
          "original_form": "그것도 했었잖아",
          "speaker_id": "1",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.14",
          "form": "재미없더라고",
          "original_form": "재미없더라고",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.15",
          "form": "난 자동사냥이 너무 재미없어",
          "original_form": "난 자동사냥이 너무 재미없어",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.16",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.17",
          "form": "그리고 공장 굴리기 힘들더라",
          "original_form": "그리고 공장 굴리기 힘들더라",
          "speaker_id": "1",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.18",
          "form": "맞아 너무 노가다게임이야",
          "original_form": "맞아 너무 노가다게임이야",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.19",
          "form": "난 방탈출게임이",
          "original_form": "난 방탈출게임이",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.20",
          "form": "하고싶어",
          "original_form": "하고싶어",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100025144.1.21",
          "form": "러스트 레이크였나?",
          "original_form": "러스트 레이크였나?",
          "speaker_id": "1",
          "time": "20210831 23:45"
        },
        {
          "id": "MDRW2100025144.1.22",
          "form": "맞아 나 그거좋아해",
          "original_form": "맞아 나 그거좋아해",
          "speaker_id": "2",
          "time": "20210831 23:45"
        },
        {
          "id": "MDRW2100025144.1.23",
          "form": "요즘 신작이없더라",
          "original_form": "요즘 신작이없더라",
          "speaker_id": "2",
          "time": "20210831 23:45"
        },
        {
          "id": "MDRW2100025144.1.24",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210831 23:45"
        }
      ]
    }
