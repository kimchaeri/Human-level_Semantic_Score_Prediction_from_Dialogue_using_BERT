        {
          "id": "MDRW2100044823.1.1",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044823.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044823.1.3",
          "form": "뭐 드셨는지 제가 맞춰볼게요ㅎㅎ",
          "original_form": "뭐 드셨는지 제가 맞춰볼게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044823.1.4",
          "form": "중식인가요?",
          "original_form": "중식인가요?",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044823.1.5",
          "form": "노노",
          "original_form": "노노",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044823.1.6",
          "form": "약간 더 구체적으로 대답해주시면 더 좋아요ㅋㅋㅋ",
          "original_form": "약간 더 구체적으로 대답해주시면 더 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044823.1.7",
          "form": "일식인가요?",
          "original_form": "일식인가요?",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044823.1.8",
          "form": "ㅋㅋㅋㅋ아니요 안주로 먹었어요",
          "original_form": "ㅋㅋㅋㅋ아니요 안주로 먹었어요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044823.1.9",
          "form": "안주라면 음",
          "original_form": "안주라면 음",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044823.1.10",
          "form": "마른 음식인가요?",
          "original_form": "마른 음식인가요?",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044823.1.11",
          "form": "아니용 !! 누구나 좋아하는음식",
          "original_form": "아니용 !! 누구나 좋아하는음식",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044823.1.12",
          "form": "앗 그럼 쌀이 들어가는 요리인가요?",
          "original_form": "앗 그럼 쌀이 들어가는 요리인가요?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044823.1.13",
          "form": "땡 ㅎㅎ 기름져요",
          "original_form": "땡 ㅎㅎ 기름져요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044823.1.14",
          "form": "앗 그럼 면이 들어가나요?",
          "original_form": "앗 그럼 면이 들어가나요?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044823.1.15",
          "form": "아니요 ㅠㅋㅋㅋㅋ 김치 버섯과 잘어울리는음식 !",
          "original_form": "아니요 ㅠㅋㅋㅋㅋ 김치 버섯과 잘어울리는음식 !",
          "speaker_id": "2",
