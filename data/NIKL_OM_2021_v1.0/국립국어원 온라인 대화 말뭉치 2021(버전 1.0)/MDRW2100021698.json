      "utterance": [
        {
          "id": "MDRW2100021698.1.1",
          "form": "헤헤 그쵸ㅎㅎ 이거 연예인들도 자주 쓰더라구용",
          "original_form": "헤헤 그쵸ㅎㅎ 이거 연예인들도 자주 쓰더라구용",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021698.1.2",
          "form": "양산 혹시 쓰고 다니세요?",
          "original_form": "양산 혹시 쓰고 다니세요?",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021698.1.3",
          "form": "아니요 전 고딩이라",
          "original_form": "아니요 전 고딩이라",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.4",
          "form": "잘 나가지도 않고ㅋㅋㅋㅋ",
          "original_form": "잘 나가지도 않고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.5",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.6",
          "form": "모자나 선크림만 발라요",
          "original_form": "모자나 선크림만 발라요",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.7",
          "form": "저는 양산 쓰는데",
          "original_form": "저는 양산 쓰는데",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.8",
          "form": "사람들이 쳐다보긴해도",
          "original_form": "사람들이 쳐다보긴해도",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.9",
          "form": "확실히 정수리도 덜 뜨겁고",
          "original_form": "확실히 정수리도 덜 뜨겁고",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.10",
          "form": "피부 자극도 덜 받더라구요",
          "original_form": "피부 자극도 덜 받더라구요",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.11",
          "form": "방학때 너무 햇빛이 강해서",
          "original_form": "방학때 너무 햇빛이 강해서",
          "speaker_id": "1",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100021698.1.12",
          "form": "몇번 쓴 적있는데",
          "original_form": "몇번 쓴 적있는데",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.13",
          "form": "확실히 낫더라구요",
          "original_form": "확실히 낫더라구요",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.14",
          "form": "맞아용ㅋㅋㅋ",
          "original_form": "맞아용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.15",
          "form": "양산 구매하실땐 겉이 밝고 안쪽이 어두운걸로 사세요",
          "original_form": "양산 구매하실땐 겉이 밝고 안쪽이 어두운걸로 사세요",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.16",
          "form": "이중암막으로 된거!",
          "original_form": "이중암막으로 된거!",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.17",
          "form": "왜요???",
          "original_form": "왜요???",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.18",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.19",
          "form": "이중암막",
          "original_form": "이중암막",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.20",
          "form": "네 참고할게요!!고마워용",
          "original_form": "네 참고할게요!!고마워용",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021698.1.21",
          "form": "ㅎㅎ 피부과 선생님이 양산이나 토시나 모자같은 물리적 차단이 최고라고 하시더라구요",
          "original_form": "ㅎㅎ 피부과 선생님이 양산이나 토시나 모자같은 물리적 차단이 최고라고 하시더라구요",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021698.1.22",
          "form": "하긴 썬크림도 다 녹고",
          "original_form": "하긴 썬크림도 다 녹고",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.23",
          "form": "마스크때문에 배출도 안되잖아요",
          "original_form": "마스크때문에 배출도 안되잖아요",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.24",
          "form": "매번 클렌징해주기도 귀찮고",
          "original_form": "매번 클렌징해주기도 귀찮고",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.25",
          "form": "요즘 기미가 부쩍 늘은 사람들이 많은데",
          "original_form": "요즘 기미가 부쩍 늘은 사람들이 많은데",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.26",
          "form": "하얀 마스크 써서 그렇대요ㅠ",
          "original_form": "하얀 마스크 써서 그렇대요ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.27",
          "form": "하얀색에 반사되어서",
          "original_form": "하얀색에 반사되어서",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.28",
          "form": "기미가 더 생긴다고 하더라구요",
          "original_form": "기미가 더 생긴다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100021698.1.29",
          "form": "혹시 블랙헤드는 모..없나요",
          "original_form": "혹시 블랙헤드는 모..없나요",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021698.1.30",
          "form": "마슼크쓰고 확실히 더 심해졌는데ㅠㅜ",
          "original_form": "마슼크쓰고 확실히 더 심해졌는데ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021698.1.31",
          "form": "코가 계속 닿아서 자극가고 그래서 그런거 아닐까요?",
          "original_form": "코가 계속 닿아서 자극가고 그래서 그런거 아닐까요?",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100021698.1.32",
          "form": "마스크때문에 피부 안좋아지신 분들이 많더라구요ㅠ",
          "original_form": "마스크때문에 피부 안좋아지신 분들이 많더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:45"
        }
      ]
    }
