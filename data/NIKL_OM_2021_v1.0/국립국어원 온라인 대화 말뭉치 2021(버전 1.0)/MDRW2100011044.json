      "utterance": [
        {
          "id": "MDRW2100011044.1.1",
          "form": "그거알아?",
          "original_form": "그거알아?",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011044.1.2",
          "form": "어떤??",
          "original_form": "어떤??",
          "speaker_id": "1",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011044.1.3",
          "form": "군대에선 눈을 하늘에서 내리는 쓰레기라고 한대!",
          "original_form": "군대에선 눈을 하늘에서 내리는 쓰레기라고 한대!",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011044.1.4",
          "form": "ㅠㅠ 그렇겠다",
          "original_form": "ㅠㅠ 그렇겠다",
          "speaker_id": "1",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011044.1.5",
          "form": "역시 일과 관련되면 감성을 조금씩 잃게 되나봐.",
          "original_form": "역시 일과 관련되면 감성을 조금씩 잃게 되나봐.",
          "speaker_id": "2",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011044.1.6",
          "form": "눈을 보면 치워야한다는 생각밖에 안드나봐",
          "original_form": "눈을 보면 치워야한다는 생각밖에 안드나봐",
          "speaker_id": "1",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011044.1.7",
          "form": "맞아ㅡ",
          "original_form": "맞아ㅡ",
          "speaker_id": "2",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011044.1.8",
          "form": "그래도 요즘 군대에선 폰도 만질 수 있으니",
          "original_form": "그래도 요즘 군대에선 폰도 만질 수 있으니",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.9",
          "form": "그래서 예전에 관련 ucc도 있었지",
          "original_form": "그래서 예전에 관련 ucc도 있었지",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.10",
          "form": "그 감성들이 조금 돌아오지 않았을까?",
          "original_form": "그 감성들이 조금 돌아오지 않았을까?",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.11",
          "form": "그들에게 한줌의 감성이 내려지기를 바라.",
          "original_form": "그들에게 한줌의 감성이 내려지기를 바라.",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.12",
          "form": "맞아 핸드폰 할수있는거 너무 꿀인거같아",
          "original_form": "맞아 핸드폰 할수있는거 너무 꿀인거같아",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.13",
          "form": "책을 통해 마음의 양식을 쌓았으면 좋겠어.",
          "original_form": "책을 통해 마음의 양식을 쌓았으면 좋겠어.",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.14",
          "form": "그러게 거기 있을때 책을 읽지",
          "original_form": "그러게 거기 있을때 책을 읽지",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.15",
          "form": "그들이 시간을 허비한다는 생각을 하자 않았으면 해.",
          "original_form": "그들이 시간을 허비한다는 생각을 하자 않았으면 해.",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.16",
          "form": "나오면 언제 읽겠어",
          "original_form": "나오면 언제 읽겠어",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011044.1.17",
          "form": "너 그들에게 상당한 안타까움을 품고있구나.",
          "original_form": "너 그들에게 상당한 안타까움을 품고있구나.",
          "speaker_id": "2",
          "time": "20210824 00:07"
        }
      ]
