      "utterance": [
        {
          "id": "MDRW2100039795.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039795.1.2",
          "form": "좋아해요 요즘은 제주도 가고싶네요",
          "original_form": "좋아해요 요즘은 제주도 가고싶네요",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039795.1.3",
          "form": "제주도 좋네요",
          "original_form": "제주도 좋네요",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039795.1.4",
          "form": "자주다니시나요?",
          "original_form": "자주다니시나요?",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039795.1.5",
          "form": "아니요 ㅜ 잘 가지는 못해요",
          "original_form": "아니요 ㅜ 잘 가지는 못해요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039795.1.6",
          "form": "님은어때요",
          "original_form": "님은어때요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039795.1.7",
          "form": "코로나전에는 많이 다녔는데.",
          "original_form": "코로나전에는 많이 다녔는데.",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039795.1.8",
          "form": "요새는 못가요ㅜㅜ",
          "original_form": "요새는 못가요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039795.1.9",
          "form": "ㅜㅜ 국내 어디가 좋나요",
          "original_form": "ㅜㅜ 국내 어디가 좋나요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039795.1.10",
          "form": "저는 화순이 좋았어요",
          "original_form": "저는 화순이 좋았어요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039795.1.11",
          "form": "볼거리가 많은가요?!",
          "original_form": "볼거리가 많은가요?!",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039795.1.12",
          "form": "농사를 짓다보니 자연보기에는 좋았어요ㅎㅎ",
          "original_form": "농사를 짓다보니 자연보기에는 좋았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039795.1.13",
          "form": "?! 농사르 지으시는건가요",
          "original_form": "?! 농사르 지으시는건가요",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039795.1.14",
          "form": "아뇨! 저말구요ㅋㅋ",
          "original_form": "아뇨! 저말구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039795.1.15",
          "form": "화순에 농사를 많이 지어서ㅋㅋ",
          "original_form": "화순에 농사를 많이 지어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039795.1.16",
          "form": "정말 초록초록하더라구요ㅎㅎ",
          "original_form": "정말 초록초록하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039795.1.17",
          "form": "아하 ㅎㅎㅎ 저도 시골풍경  정겹고좋아해요",
          "original_form": "아하 ㅎㅎㅎ 저도 시골풍경  정겹고좋아해요",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039795.1.18",
          "form": "남해도 좋더라구요",
          "original_form": "남해도 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039795.1.19",
          "form": "남해! 너무좋죠?",
          "original_form": "남해! 너무좋죠?",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039795.1.20",
          "form": "남해는 어디가 좋으셨어요?",
          "original_form": "남해는 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:49"
        }
      ]
