      "utterance": [
        {
          "id": "MDRW2100011666.1.1",
          "form": "저녁은 뭐 먹어?",
          "original_form": "저녁은 뭐 먹어?",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100011666.1.2",
          "form": "소고기 국 끓일거야",
          "original_form": "소고기 국 끓일거야",
          "speaker_id": "2",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100011666.1.3",
          "form": "장봐놨어 소고기 파 무",
          "original_form": "장봐놨어 소고기 파 무",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.4",
          "form": "소고기 국?",
          "original_form": "소고기 국?",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.5",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.6",
          "form": "외식하러 나가고싶다",
          "original_form": "외식하러 나가고싶다",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.7",
          "form": "누가 좋아해?",
          "original_form": "누가 좋아해?",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.8",
          "form": "아들이 엄청 좋아해",
          "original_form": "아들이 엄청 좋아해",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.9",
          "form": "나도 외식하고싶다",
          "original_form": "나도 외식하고싶다",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.10",
          "form": "맛있게 잘끓인다",
          "original_form": "맛있게 잘끓인다",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.11",
          "form": "매운것도 잘 먹네..",
          "original_form": "매운것도 잘 먹네..",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011666.1.12",
          "form": "외식할려해도.. 요즘 포장할만함..음식이 없음",
          "original_form": "외식할려해도.. 요즘 포장할만함..음식이 없음",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011666.1.13",
          "form": "나가서 먹는다면 뭐 먹고싶어?",
          "original_form": "나가서 먹는다면 뭐 먹고싶어?",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011666.1.14",
          "form": "간단하게 포장해올 만한 가게가 많이 생겼음 좋겠다",
          "original_form": "간단하게 포장해올 만한 가게가 많이 생겼음 좋겠다",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011666.1.15",
          "form": "나는 곱창 먹고싶다ㅜ",
          "original_form": "나는 곱창 먹고싶다ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011666.1.16",
          "form": "삼겹살 구우러",
          "original_form": "삼겹살 구우러",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011666.1.17",
          "form": "나도 곱창도 좋아한다",
          "original_form": "나도 곱창도 좋아한다",
          "speaker_id": "2",
          "time": "20210824 14:59"
        }
      ]
