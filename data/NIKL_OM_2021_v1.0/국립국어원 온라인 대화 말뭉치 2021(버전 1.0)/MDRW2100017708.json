      "utterance": [
        {
          "id": "MDRW2100017708.1.1",
          "form": "네 ㅋㅋㅋㅋ 근데 복숭아맛 재현해놔서 아몬드 질감 궁금하시면 사드셔도 돼요",
          "original_form": "네 ㅋㅋㅋㅋ 근데 복숭아맛 재현해놔서 아몬드 질감 궁금하시면 사드셔도 돼요",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017708.1.2",
          "form": "저 과자는 최근엔 그 감튀",
          "original_form": "저 과자는 최근엔 그 감튀",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017708.1.3",
          "form": "아니에요 굳이 먹지 않겟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니에요 굳이 먹지 않겟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017708.1.4",
          "form": "근데 탄수화물 줄여야겠다 싶어서 과자를 줄이는 중이에요",
          "original_form": "근데 탄수화물 줄여야겠다 싶어서 과자를 줄이는 중이에요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017708.1.5",
          "form": "저도 과자 줄이려고 노력중이에요 ㅠㅠ",
          "original_form": "저도 과자 줄이려고 노력중이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017708.1.6",
          "form": "노력만...ㅋㅋㅋㅋㅋ",
          "original_form": "노력만...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017708.1.7",
          "form": "ㅋㅋㅋㅋ무슨 과자 많이 드세요",
          "original_form": "ㅋㅋㅋㅋ무슨 과자 많이 드세요",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017708.1.8",
          "form": "저는 감자칩 다좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 감자칩 다좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017708.1.9",
          "form": "아 감자칩은 인정이죠",
          "original_form": "아 감자칩은 인정이죠",
          "speaker_id": "2",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017708.1.10",
          "form": "와그작와그작",
          "original_form": "와그작와그작",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017708.1.11",
          "form": "중독성이 너무 강해요 ㅋㅋㅋㅋㅋ",
          "original_form": "중독성이 너무 강해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017708.1.12",
          "form": "예전엔 감자칩의 매력을 잘 모르고",
          "original_form": "예전엔 감자칩의 매력을 잘 모르고",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017708.1.13",
          "form": "맨날 초코과자 같은 거 먹었는데",
          "original_form": "맨날 초코과자 같은 거 먹었는데",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017708.1.14",
          "form": "짠맛이 진리가 되엇어요",
          "original_form": "짠맛이 진리가 되엇어요",
          "speaker_id": "2",
          "time": "20210826 20:54"
        },
        {
          "id": "MDRW2100017708.1.15",
          "form": "전 어릴때부터 감자칩만 열심히 좋아햇어요 ㅋㅋㅋㅋ",
          "original_form": "전 어릴때부터 감자칩만 열심히 좋아햇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:58"
        },
        {
          "id": "MDRW2100017708.1.16",
          "form": "일찍부터 진리를 깨우치셨군요",
          "original_form": "일찍부터 진리를 깨우치셨군요",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017708.1.17",
          "form": "맛잘알",
          "original_form": "맛잘알",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017708.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ포카칩을 그렇게 좋아햇어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ포카칩을 그렇게 좋아햇어요",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017708.1.19",
          "form": "양파맛",
          "original_form": "양파맛",
          "speaker_id": "1",
          "time": "20210826 21:02"
        },
        {
          "id": "MDRW2100017708.1.20",
          "form": "아 갑자기 포카칩 땡기는데요",
          "original_form": "아 갑자기 포카칩 땡기는데요",
          "speaker_id": "2",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017708.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ맥주는 땡기지 않으시나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맥주는 땡기지 않으시나요",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017708.1.22",
          "form": "저 사실 맥주보다 소주파라서",
          "original_form": "저 사실 맥주보다 소주파라서",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017708.1.23",
          "form": "맥주는 진짜 기념일때만 마시고",
          "original_form": "맥주는 진짜 기념일때만 마시고",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017708.1.24",
          "form": "아 정말요? 전 소주도 좋아하지만 맥주를 더 좋아하는데",
          "original_form": "아 정말요? 전 소주도 좋아하지만 맥주를 더 좋아하는데",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017708.1.25",
          "form": "맥주는 좀 배부르달까..",
          "original_form": "맥주는 좀 배부르달까..",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017708.1.26",
          "form": "주량의 향기가 ㅋㅋㅋㅋㅋ",
          "original_form": "주량의 향기가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017708.1.27",
          "form": "주당이신가요 ㅋㅋㅋㅋ",
          "original_form": "주당이신가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017708.1.28",
          "form": "근데 1년에 한 1-2번정도만 술 마셔요",
          "original_form": "근데 1년에 한 1-2번정도만 술 마셔요",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017708.1.29",
          "form": "한번 마실때 주당처럼",
          "original_form": "한번 마실때 주당처럼",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017708.1.30",
          "form": "와 자제력 대단하시다",
          "original_form": "와 자제력 대단하시다",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017708.1.31",
          "form": "그냥 인싸가 아니라 술자리가 없어서요....ㅋㅋㅋㅋ",
          "original_form": "그냥 인싸가 아니라 술자리가 없어서요....ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017708.1.32",
          "form": "에이 대세는 혼술이죠",
          "original_form": "에이 대세는 혼술이죠",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017708.1.33",
          "form": "아 술자리 분위를 좋아해서 술 마시는거라",
          "original_form": "아 술자리 분위를 좋아해서 술 마시는거라",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017708.1.34",
          "form": "뭔가 혼자는 잘 안마시게 되어요ㅋㅋㅋ집에 가족들도 뭔가 눈치주고ㅋㅋㅋ",
          "original_form": "뭔가 혼자는 잘 안마시게 되어요ㅋㅋㅋ집에 가족들도 뭔가 눈치주고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017708.1.35",
          "form": "아 그러시구나 가끔 술자리에서 제대로 달리실때 엄청 기분좋으시겟어요 ㅋㅋㅋ",
          "original_form": "아 그러시구나 가끔 술자리에서 제대로 달리실때 엄청 기분좋으시겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017708.1.36",
          "form": "넹 얼른 모임을 자유롭게 하는 날이 오면 좋겠어요",
          "original_form": "넹 얼른 모임을 자유롭게 하는 날이 오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017708.1.37",
          "form": "식사하셧어요?",
          "original_form": "식사하셧어요?",
          "speaker_id": "1",
          "time": "20210907 18:36"
        }
      ]
