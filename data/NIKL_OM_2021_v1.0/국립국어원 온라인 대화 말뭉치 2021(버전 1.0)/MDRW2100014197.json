      "utterance": [
        {
          "id": "MDRW2100014197.1.1",
          "form": "넷플릭스 구독해?",
          "original_form": "넷플릭스 구독해?",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014197.1.2",
          "form": "응 넷플릭스 랑 왓챠 구독중이야",
          "original_form": "응 넷플릭스 랑 왓챠 구독중이야",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014197.1.3",
          "form": "넷플릭스는 오리지널 컨텐츠가좋다던데",
          "original_form": "넷플릭스는 오리지널 컨텐츠가좋다던데",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014197.1.4",
          "form": "왓챠 도 단독 콘텐츠 많아서 괜잖아",
          "original_form": "왓챠 도 단독 콘텐츠 많아서 괜잖아",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014197.1.5",
          "form": "추천해줄수있어?",
          "original_form": "추천해줄수있어?",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014197.1.6",
          "form": "요즘 나는 와이 우먼 킬 시즌2를 보고있어",
          "original_form": "요즘 나는 와이 우먼 킬 시즌2를 보고있어",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014197.1.7",
          "form": "아 그거 들어봤어  시즌1도본거야?",
          "original_form": "아 그거 들어봤어  시즌1도본거야?",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014197.1.8",
          "form": "응 시즌 1이 더 재미있던것 같긴해",
          "original_form": "응 시즌 1이 더 재미있던것 같긴해",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014197.1.9",
          "form": "그렇구나 기묘한이야기도 오리지널인가?",
          "original_form": "그렇구나 기묘한이야기도 오리지널인가?",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014197.1.10",
          "form": "응 맞아 제일 대표적인 오리지널 드라마지",
          "original_form": "응 맞아 제일 대표적인 오리지널 드라마지",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014197.1.11",
          "form": "나도 그거 재밌더라!",
          "original_form": "나도 그거 재밌더라!",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100014197.1.12",
          "form": "그치 곧 시즌 4나온대서 기대돼",
          "original_form": "그치 곧 시즌 4나온대서 기대돼",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014197.1.13",
          "form": "헉!얼른 시즌3봐야겠네",
          "original_form": "헉!얼른 시즌3봐야겠네",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014197.1.14",
          "form": "이번 9월에 어둠속으로 시즌 2도 나온대",
          "original_form": "이번 9월에 어둠속으로 시즌 2도 나온대",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014197.1.15",
          "form": "그게 뭐더라?",
          "original_form": "그게 뭐더라?",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014197.1.16",
          "form": "안녕",
          "original_form": "안녕",
