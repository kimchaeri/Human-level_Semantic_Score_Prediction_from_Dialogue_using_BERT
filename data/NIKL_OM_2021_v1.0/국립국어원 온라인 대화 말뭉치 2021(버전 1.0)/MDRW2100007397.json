      "utterance": [
        {
          "id": "MDRW2100007397.1.1",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007397.1.2",
          "form": "예약은 해놓은 상태예요",
          "original_form": "예약은 해놓은 상태예요",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007397.1.3",
          "form": "전 오늘 맞고왔어요",
          "original_form": "전 오늘 맞고왔어요",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007397.1.4",
          "form": "오오 괜찮으세요??",
          "original_form": "오오 괜찮으세요??",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.5",
          "form": "팔만 아픈것 같아요",
          "original_form": "팔만 아픈것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.6",
          "form": "다들 1차는 괜찮다고 하던데 그래도 팔이 뻐근하긴 하다길래...",
          "original_form": "다들 1차는 괜찮다고 하던데 그래도 팔이 뻐근하긴 하다길래...",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.7",
          "form": "몸살은 안오더라구요",
          "original_form": "몸살은 안오더라구요",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.8",
          "form": "오 네네 맞아요",
          "original_form": "오 네네 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.9",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.10",
          "form": "근데 부모님은 az라 그런지",
          "original_form": "근데 부모님은 az라 그런지",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.11",
          "form": "두분 다 몸살 오셨었어요",
          "original_form": "두분 다 몸살 오셨었어요",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.12",
          "form": "어이구ㅜㅜ",
          "original_form": "어이구ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007397.1.13",
          "form": "몸살이라니 고생이 많으셨네요",
          "original_form": "몸살이라니 고생이 많으셨네요",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007397.1.14",
          "form": "네 그리고 기저질환도 있으셔서 걱정되더라구요",
          "original_form": "네 그리고 기저질환도 있으셔서 걱정되더라구요",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007397.1.15",
          "form": "백신 언제 맞으세요?",
          "original_form": "백신 언제 맞으세요?",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007397.1.16",
          "form": "저는 추석연휴에 맞는 걸로 예약했어요",
          "original_form": "저는 추석연휴에 맞는 걸로 예약했어요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007397.1.17",
          "form": "백신 맞고 푹 쉬려구요",
          "original_form": "백신 맞고 푹 쉬려구요",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007397.1.18",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007397.1.19",
          "form": "혹시 모르니까..",
          "original_form": "혹시 모르니까..",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007397.1.20",
          "form": "백신 맞은 사람들이 느는데도 코로나 환자도 계속 늘어서 마음이 안 좋아요",
          "original_form": "백신 맞은 사람들이 느는데도 코로나 환자도 계속 늘어서 마음이 안 좋아요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007397.1.21",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007397.1.22",
          "form": "오늘 보니까 제 앞의 할아버지는 이미 코로나 걸리신적 있으신 분이더라구요",
          "original_form": "오늘 보니까 제 앞의 할아버지는 이미 코로나 걸리신적 있으신 분이더라구요",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007397.1.23",
          "form": "어머 너무 고생하셨겠다ㅜㅜ",
          "original_form": "어머 너무 고생하셨겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007397.1.24",
          "form": "코로나 걸렸던 사람이 또 맞으면 백신 효과가 좋다는 뉴스도 본 것 같아요",
          "original_form": "코로나 걸렸던 사람이 또 맞으면 백신 효과가 좋다는 뉴스도 본 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007397.1.25",
          "form": "부스터 샷 효과난대요",
          "original_form": "부스터 샷 효과난대요",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007397.1.26",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007397.1.27",
          "form": "그치만 백신 다 맞을때까지 코로나로부터 안전했으면 좋겠네요",
          "original_form": "그치만 백신 다 맞을때까지 코로나로부터 안전했으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210822 01:56"
        }
      ]
    }
