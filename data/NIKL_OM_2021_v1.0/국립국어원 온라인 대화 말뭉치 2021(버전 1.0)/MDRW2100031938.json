      "utterance": [
        {
          "id": "MDRW2100031938.1.1",
          "form": "넵 일요일도 일해요 ㅜㅜ",
          "original_form": "넵 일요일도 일해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100031938.1.2",
          "form": "헉 힘드시겠어요ㅠㅠㅠ",
          "original_form": "헉 힘드시겠어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100031938.1.3",
          "form": "감사해야겠죠 지금 상황에...가을님은요",
          "original_form": "감사해야겠죠 지금 상황에...가을님은요",
          "speaker_id": "2",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100031938.1.4",
          "form": "어떤일을 하세요?",
          "original_form": "어떤일을 하세요?",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100031938.1.5",
          "form": "저는....취준생입니다....ㅠ 빨리 취업해야할텐데 걱정이에요",
          "original_form": "저는....취준생입니다....ㅠ 빨리 취업해야할텐데 걱정이에요",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100031938.1.6",
          "form": "꽃을 만지고 있어요",
          "original_form": "꽃을 만지고 있어요",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031938.1.7",
          "form": "아하! 그럼....꽃집에서 일을 하시나봐요~",
          "original_form": "아하! 그럼....꽃집에서 일을 하시나봐요~",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031938.1.8",
          "form": "꽃집 일은 어떠세요?",
          "original_form": "꽃집 일은 어떠세요?",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031938.1.9",
          "form": "비슷해요...",
          "original_form": "비슷해요...",
          "speaker_id": "2",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031938.1.10",
          "form": "좋으면서도 힘들어요",
          "original_form": "좋으면서도 힘들어요",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031938.1.11",
          "form": "특히 힘든점이 있다면요?",
          "original_form": "특히 힘든점이 있다면요?",
          "speaker_id": "1",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031938.1.12",
          "form": "초록의 식물들이 예뻐요",
          "original_form": "초록의 식물들이 예뻐요",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031938.1.13",
          "form": "ㅋㅋㅋㅋ 그쵸~~! 우울할때 식물보고 치유하는 분들 많더라고요~~",
          "original_form": "ㅋㅋㅋㅋ 그쵸~~! 우울할때 식물보고 치유하는 분들 많더라고요~~",
          "speaker_id": "1",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031938.1.14",
          "form": "전 잘하고 싶은데 다른사람 눈엔 부족하게 보일때요",
          "original_form": "전 잘하고 싶은데 다른사람 눈엔 부족하게 보일때요",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031938.1.15",
          "form": "아하...맞아요 일 할때 그게 제일어렵죠...ㅠ",
          "original_form": "아하...맞아요 일 할때 그게 제일어렵죠...ㅠ",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031938.1.16",
          "form": "아마 name2님은 잘 하고 계실거예요! ",
          "original_form": "아마 &name2&님은 잘 하고 계실거예요! ",
          "speaker_id": "1",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031938.1.17",
          "form": "제가 선물하는 작은것들로 인해 기뻐하는이들을 볼때 좋아요",
          "original_form": "제가 선물하는 작은것들로 인해 기뻐하는이들을 볼때 좋아요",
          "speaker_id": "2",
          "time": "20210905 09:23"
        },
        {
          "id": "MDRW2100031938.1.18",
          "form": "name1님은 어떤꽃을 좋아하나요",
          "original_form": "&name1&님은 어떤꽃을 좋아하나요",
          "speaker_id": "2",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100031938.1.19",
          "form": "꽃을 선물하시나봐요? 저는....안개꽃이랑 작약을 좋아해요~~!",
          "original_form": "꽃을 선물하시나봐요? 저는....안개꽃이랑 작약을 좋아해요~~!",
