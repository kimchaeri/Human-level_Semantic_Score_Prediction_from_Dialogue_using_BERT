      "utterance": [
        {
          "id": "MDRW2100037435.1.1",
          "form": "오 소스는여!",
          "original_form": "오 소스는여!",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100037435.1.2",
          "form": "스위트 어니언  , 스위트 칠리 먹어요!",
          "original_form": "스위트 어니언  , 스위트 칠리 먹어요!",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100037435.1.3",
          "form": "맛없을 수없는 소스 ㅠㅠ",
          "original_form": "맛없을 수없는 소스 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037435.1.4",
          "form": "대박이죠...",
          "original_form": "대박이죠...",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037435.1.5",
          "form": "어우 허기지네요 저녁 드셨나요",
          "original_form": "어우 허기지네요 저녁 드셨나요",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037435.1.6",
          "form": "네! 고추바사삭 시켜먹었어요",
          "original_form": "네! 고추바사삭 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100037435.1.7",
          "form": "대박 그거 맛나나요?",
          "original_form": "대박 그거 맛나나요?",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037435.1.8",
          "form": "저도 이번에 기프트콘 받게되어서 먹어봤는데 그러저럭 맛있어요!",
          "original_form": "저도 이번에 기프트콘 받게되어서 먹어봤는데 그러저럭 맛있어요!",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037435.1.9",
          "form": "깔끔한맛...?",
          "original_form": "깔끔한맛...?",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037435.1.10",
          "form": "저는 오리지날이 더 좋긴한데 이것도 가끔 좋네요",
          "original_form": "저는 오리지날이 더 좋긴한데 이것도 가끔 좋네요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037435.1.11",
          "form": "앗 벌써 받으셨나요 ㄷㄷ",
          "original_form": "앗 벌써 받으셨나요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100037435.1.12",
          "form": "굽네 맛나서 ㅎㅎ 맛날거같긴햐요",
          "original_form": "굽네 맛나서 ㅎㅎ 맛날거같긴햐요",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100037435.1.13",
          "form": "매콤한가요?",
          "original_form": "매콤한가요?",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100037435.1.14",
          "form": "그죠 구운치킨 맛있어서 좋아요",
          "original_form": "그죠 구운치킨 맛있어서 좋아요",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100037435.1.15",
          "form": "약간 알싸한맛이",
          "original_form": "약간 알싸한맛이",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100037435.1.16",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100037435.1.17",
          "form": "오 빨리 먹어보고싶네요",
          "original_form": "오 빨리 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100037435.1.18",
          "form": "분발해야지 ㅎㅎ",
          "original_form": "분발해야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100037435.1.19",
          "form": "치킨 최애 어디예요?",
          "original_form": "치킨 최애 어디예요?",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100037435.1.20",
          "form": "저는 후라이드는 bbp 간장은 교촌이요",
          "original_form": "저는 후라이드는 bbp 간장은 교촌이요",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100037435.1.21",
          "form": "아 저두 bbq 후라이드 젤 좋아해요",
          "original_form": "아 저두 bbq 후라이드 젤 좋아해요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100037435.1.22",
          "form": "간장 교촌은 안먹어봤는데 먹고싶내요..",
          "original_form": "간장 교촌은 안먹어봤는데 먹고싶내요..",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100037435.1.23",
          "form": "교촌 오리지널이 양이 좀 적어서그렇지 맛있어요",
          "original_form": "교촌 오리지널이 양이 좀 적어서그렇지 맛있어요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100037435.1.24",
          "form": "오리지널콤보로 드셔보세요",
          "original_form": "오리지널콤보로 드셔보세요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100037435.1.25",
          "form": "콤보는 뭐가달라요?",
          "original_form": "콤보는 뭐가달라요?",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100037435.1.26",
          "form": "다리랑 날개만 줘요!",
          "original_form": "다리랑 날개만 줘요!",
          "speaker_id": "1",
          "time": "20210902 13:25"
        }
      ]
    }
  ]
