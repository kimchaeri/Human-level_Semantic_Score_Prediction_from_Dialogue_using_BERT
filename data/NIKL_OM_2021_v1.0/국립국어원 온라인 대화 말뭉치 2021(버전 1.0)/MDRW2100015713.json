        {
          "id": "MDRW2100015713.1.1",
          "form": "넷플릭스 좋아하시나요 ㅎㅎ",
          "original_form": "넷플릭스 좋아하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015713.1.2",
          "form": "넷플 와챠 가입되었있어요",
          "original_form": "넷플 와챠 가입되었있어요",
          "speaker_id": "1",
          "time": "20210826 07:05"
        },
        {
          "id": "MDRW2100015713.1.3",
          "form": "초에는 많이 받는데 ㅋㅋ요새 좀 덜봐요",
          "original_form": "초에는 많이 받는데 ㅋㅋ요새 좀 덜봐요",
          "speaker_id": "1",
          "time": "20210826 07:05"
        },
        {
          "id": "MDRW2100015713.1.4",
          "form": "킹덤 아신전까지 봤어요",
          "original_form": "킹덤 아신전까지 봤어요",
          "speaker_id": "1",
          "time": "20210826 07:05"
        },
        {
          "id": "MDRW2100015713.1.5",
          "form": "전 위쳐 2 기다려요",
          "original_form": "전 위쳐 2 기다려요",
          "speaker_id": "1",
          "time": "20210826 07:05"
        },
        {
          "id": "MDRW2100015713.1.6",
          "form": "넘재밌게 본 시리즈예요",
          "original_form": "넘재밌게 본 시리즈예요",
          "speaker_id": "1",
          "time": "20210826 07:05"
        },
        {
          "id": "MDRW2100015713.1.7",
          "form": "위쳐 넷플에서 볼 수 있나용ㅋㅋ",
          "original_form": "위쳐 넷플에서 볼 수 있나용ㅋㅋ",
