      "utterance": [
        {
          "id": "MDRW2100047585.1.1",
          "form": "아직 설거지 않했어요 ㅋㅋ",
          "original_form": "아직 설거지 않했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047585.1.2",
          "form": "자고 일어나니 설거지가",
          "original_form": "자고 일어나니 설거지가",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047585.1.3",
          "form": "치킨 먹었는데 설거지라니 ㅋㅋ",
          "original_form": "치킨 먹었는데 설거지라니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047585.1.4",
          "form": "뭐든 막음 집안일이 생겨요",
          "original_form": "뭐든 막음 집안일이 생겨요",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100047585.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ원래 설거지 안하려고 치킨먹는건데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ원래 설거지 안하려고 치킨먹는건데",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047585.1.6",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047585.1.7",
          "form": "근데 설거지가 딱",
          "original_form": "근데 설거지가 딱",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047585.1.8",
          "form": "자고 일어나니 생긴기분이네요 ㅋㅋ",
          "original_form": "자고 일어나니 생긴기분이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047585.1.9",
          "form": "더 억울한 기분 ㅋㅋㅋㅋㅋ",
          "original_form": "더 억울한 기분 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047585.1.10",
          "form": "치킨을 먹엇는데 왜 설거지를 해야하나...!!",
          "original_form": "치킨을 먹엇는데 왜 설거지를 해야하나...!!",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100047585.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047585.1.12",
          "form": "아 집안일 끝이 없어요 ㅋㅋ",
          "original_form": "아 집안일 끝이 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047585.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ집안일만큼 지겨운게없어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ집안일만큼 지겨운게없어요",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100047585.1.14",
          "form": "맞아요 똑같고 반복",
          "original_form": "맞아요 똑같고 반복",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047585.1.15",
          "form": "뭔가 티도 안나고",
          "original_form": "뭔가 티도 안나고",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047585.1.16",
          "form": "당연히 생각하죠",
          "original_form": "당연히 생각하죠",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047585.1.17",
          "form": "맞아요 티가 안나요ㅠㅠ",
          "original_form": "맞아요 티가 안나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047585.1.18",
          "form": "열심히 해봣자 순간일뿐",
          "original_form": "열심히 해봣자 순간일뿐",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047585.1.19",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100047585.1.20",
          "form": "돌아오면 이녀석들 뭐 먹음 포장지 꼭 그냥놓고",
          "original_form": "돌아오면 이녀석들 뭐 먹음 포장지 꼭 그냥놓고",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.21",
          "form": "안하면 그렇게 티가 나고 ㅋㅋㅋㅋ",
          "original_form": "안하면 그렇게 티가 나고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.22",
          "form": "휴지통에 안버리고",
          "original_form": "휴지통에 안버리고",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.23",
          "form": "참나 버리는 사람 따로있죠",
          "original_form": "참나 버리는 사람 따로있죠",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.25",
          "form": "아이고 ㅋㅋㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.26",
          "form": "버리는 사람 따로 치우는 사람 따로 ㅠㅠ",
          "original_form": "버리는 사람 따로 치우는 사람 따로 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047585.1.27",
          "form": "네 어휴 열받네요 ㅡㅡ",
          "original_form": "네 어휴 열받네요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100047585.1.28",
          "form": "당연해서 소중함을 모르는 존재가 저입니다",
          "original_form": "당연해서 소중함을 모르는 존재가 저입니다",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100047585.1.29",
          "form": "당연히 모두 해주니까요",
          "original_form": "당연히 모두 해주니까요",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100047585.1.30",
          "form": "ㅠㅠㅠㅠㅠㅠ슬퍼요",
          "original_form": "ㅠㅠㅠㅠㅠㅠ슬퍼요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100047585.1.31",
          "form": "이녀석들...!!",
          "original_form": "이녀석들...!!",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047585.1.32",
          "form": "네 힘내세요 우리모두",
          "original_form": "네 힘내세요 우리모두",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047585.1.33",
          "form": "어느날은 막 말안하고 어디 가볼까하는데",
          "original_form": "어느날은 막 말안하고 어디 가볼까하는데",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047585.1.34",
          "form": "애들이 커서 잘살듯해요 ㅋㅋ",
          "original_form": "애들이 커서 잘살듯해요 ㅋㅋ",
