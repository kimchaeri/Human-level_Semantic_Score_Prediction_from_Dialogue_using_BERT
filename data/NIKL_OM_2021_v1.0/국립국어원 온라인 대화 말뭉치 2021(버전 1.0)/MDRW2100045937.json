      "utterance": [
        {
          "id": "MDRW2100045937.1.1",
          "form": "에이드나 블렌디드류 많이 마시는 거 같아요",
          "original_form": "에이드나 블렌디드류 많이 마시는 거 같아요",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045937.1.2",
          "form": "시원한거 좋아하시는군요?",
          "original_form": "시원한거 좋아하시는군요?",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045937.1.3",
          "form": "여름이라서 더 그랬던 거 같아요!",
          "original_form": "여름이라서 더 그랬던 거 같아요!",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045937.1.4",
          "form": "전 밀크티를 자주 먹어요ㅎㅎㅎ",
          "original_form": "전 밀크티를 자주 먹어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045937.1.5",
          "form": "밀크티는 어디가 맛있나요?",
          "original_form": "밀크티는 어디가 맛있나요?",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045937.1.6",
          "form": "자주가는 프렌차이즈 카페가 있으신가요?",
          "original_form": "자주가는 프렌차이즈 카페가 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045937.1.7",
          "form": "아니요 딱히 없어요!",
          "original_form": "아니요 딱히 없어요!",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045937.1.8",
          "form": "그냥 그때그때 눈에 보이는데 들어가는 편이에요!",
          "original_form": "그냥 그때그때 눈에 보이는데 들어가는 편이에요!",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045937.1.9",
          "form": "밀크티는 커피빈이 괜찮더라구요",
          "original_form": "밀크티는 커피빈이 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045937.1.10",
          "form": "하필 집 주변에 없는 커피빈이네요ㅠㅠㅠ",
          "original_form": "하필 집 주변에 없는 커피빈이네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045937.1.11",
          "form": "나중에 약속있을때 주변에 커피빈이 있다면 꼭 가서 마셔 볼게요ㅠㅠ",
          "original_form": "나중에 약속있을때 주변에 커피빈이 있다면 꼭 가서 마셔 볼게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045937.1.12",
          "form": "커피빈매장이 적어요",
          "original_form": "커피빈매장이 적어요",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045937.1.13",
          "form": "그러니까요 너무 아쉬워요ㅠㅠ",
          "original_form": "그러니까요 너무 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045937.1.14",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045937.1.15",
          "form": "아직 안 먹었어요 뭐가 좋을지 모르겠네요..",
          "original_form": "아직 안 먹었어요 뭐가 좋을지 모르겠네요..",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045937.1.16",
          "form": "name1님은 드셨나요?",
          "original_form": "&name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045937.1.17",
          "form": "저는 주로 과일을 먹는데 배가 있어서 먹고",
          "original_form": "저는 주로 과일을 먹는데 배가 있어서 먹고",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100045937.1.18",
          "form": "사무실와서 맥심을 마시고 있어요ㅋㅋㅋㅋ",
          "original_form": "사무실와서 맥심을 마시고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100045937.1.19",
          "form": "아침에 과일 드시기 번거롭지는 않으신가요?",
          "original_form": "아침에 과일 드시기 번거롭지는 않으신가요?",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100045937.1.20",
          "form": "역시 커피믹스는 맥심인가보네욬ㅋㅋ",
          "original_form": "역시 커피믹스는 맥심인가보네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100045937.1.21",
          "form": "감자칼로 쓱쓱하기때문에ㅋㅋㅋㅋ",
          "original_form": "감자칼로 쓱쓱하기때문에ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100045937.1.22",
