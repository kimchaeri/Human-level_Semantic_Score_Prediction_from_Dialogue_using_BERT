      "utterance": [
        {
          "id": "MDRW2100020048.1.1",
          "form": "그래도 너무 기대돼요",
          "original_form": "그래도 너무 기대돼요",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020048.1.2",
          "form": "해운대 갈 예정인데",
          "original_form": "해운대 갈 예정인데",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020048.1.3",
          "form": "근방 맛집 아는거 있으세요?",
          "original_form": "근방 맛집 아는거 있으세요?",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020048.1.4",
          "form": "아하 해운대라.. 제가 그쪽 안살아서 잘은모르겠는데 지금 떠오르는건 해운대쪽 해목 버거샵 하얀오징어집 이렇게네요 ㅋㅋ",
          "original_form": "아하 해운대라.. 제가 그쪽 안살아서 잘은모르겠는데 지금 떠오르는건 해운대쪽 해목 버거샵 하얀오징어집 이렇게네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020048.1.5",
          "form": "수제버거집이랑 오징어요리집인가요?",
          "original_form": "수제버거집이랑 오징어요리집인가요?",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020048.1.6",
          "form": "네 버거샵은 수제버거빕이구 하얀오징어는 오징어회 유명한걸로알아요~",
          "original_form": "네 버거샵은 수제버거빕이구 하얀오징어는 오징어회 유명한걸로알아요~",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020048.1.7",
          "form": "그렇구나 좋은 정보 감사합니다!!",
          "original_form": "그렇구나 좋은 정보 감사합니다!!",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020048.1.8",
          "form": "저는 울산 살아서",
          "original_form": "저는 울산 살아서",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020048.1.9",
          "form": "부산 자주 가보긴했는더",
          "original_form": "부산 자주 가보긴했는더",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020048.1.10",
          "form": "그래도 휴가로 가니까 기분이 색달라요",
          "original_form": "그래도 휴가로 가니까 기분이 색달라요",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020048.1.11",
          "form": "아하! 울산사시는군요~",
          "original_form": "아하! 울산사시는군요~",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020048.1.12",
          "form": "울산은 괜찮은명소 잇나요??",
          "original_form": "울산은 괜찮은명소 잇나요??",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020048.1.13",
          "form": "태화강국가정원 울산대공원 대왕암공원 같은 것들이 있죠",
          "original_form": "태화강국가정원 울산대공원 대왕암공원 같은 것들이 있죠",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020048.1.14",
          "form": "울산 갈곳 많아요",
          "original_form": "울산 갈곳 많아요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020048.1.15",
          "form": "오! 담번에 울산여행 계획해봐야겠어요 ㅋㅋ",
          "original_form": "오! 담번에 울산여행 계획해봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020048.1.16",
          "form": "정원 기대되는데요?",
          "original_form": "정원 기대되는데요?",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020048.1.17",
          "form": "큰 공원 느낌이에요",
          "original_form": "큰 공원 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100020048.1.18",
          "form": "주말 오전에 가면 공원에서 요가 클래스도 있더라고요",
          "original_form": "주말 오전에 가면 공원에서 요가 클래스도 있더라고요",
          "speaker_id": "2",
          "time": "20210828 09:11"
        },
        {
          "id": "MDRW2100020048.1.19",
          "form": "오 되게 여유넘치는느낌일꺼같아요~",
          "original_form": "오 되게 여유넘치는느낌일꺼같아요~",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020048.1.20",
          "form": "네 사람들 나와서 요가하는데 뭔가 멋있더라고요",
          "original_form": "네 사람들 나와서 요가하는데 뭔가 멋있더라고요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020048.1.21",
          "form": "저도 해보고싶은데 내성적이라..ㅎㅎ",
          "original_form": "저도 해보고싶은데 내성적이라..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020048.1.22",
          "form": "부산에도 큰 공원 있나요?",
          "original_form": "부산에도 큰 공원 있나요?",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020048.1.23",
          "form": "음 집앞레 시민공원있어서 자주가욮ㅋㅋ",
          "original_form": "음 집앞레 시민공원있어서 자주가욮ㅋㅋ",
