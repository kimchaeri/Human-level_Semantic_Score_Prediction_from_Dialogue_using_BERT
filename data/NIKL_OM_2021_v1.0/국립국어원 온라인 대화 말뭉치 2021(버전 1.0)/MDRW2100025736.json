      "utterance": [
        {
          "id": "MDRW2100025736.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025736.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025736.1.3",
          "form": "국내 여행 많이 다녀 보셨나요?",
          "original_form": "국내 여행 많이 다녀 보셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025736.1.4",
          "form": "몇번 가봤어요",
          "original_form": "몇번 가봤어요",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025736.1.5",
          "form": "주로 어디 가보셨나요?",
          "original_form": "주로 어디 가보셨나요?",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025736.1.6",
          "form": "통영 가봤어요",
          "original_form": "통영 가봤어요",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100025736.1.7",
          "form": "통영 어떤가요? 저는 너무 어렸을 때 가봐서요",
          "original_form": "통영 어떤가요? 저는 너무 어렸을 때 가봐서요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100025736.1.8",
          "form": "대게가 맛있어요 ㅎㅎ...",
          "original_form": "대게가 맛있어요 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025736.1.9",
          "form": "바다 있는 곳은 다 좋은 걱 같아요",
          "original_form": "바다 있는 곳은 다 좋은 걱 같아요",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100025736.1.10",
          "form": "그렇군요..대게 맛있죠!",
          "original_form": "그렇군요..대게 맛있죠!",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100025736.1.11",
          "form": "저는 많이 가보진 않았지만 부산 요새 가고 싶더라구요",
          "original_form": "저는 많이 가보진 않았지만 부산 요새 가고 싶더라구요",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100025736.1.12",
          "form": "오 부산 저도 가고싶어요!",
          "original_form": "오 부산 저도 가고싶어요!",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100025736.1.13",
          "form": "그쵸ㅠ..저는 광안리 가고 싶어요..",
          "original_form": "그쵸ㅠ..저는 광안리 가고 싶어요..",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100025736.1.14",
          "form": "어디든 바다 보러 가고싶네요..",
          "original_form": "어디든 바다 보러 가고싶네요..",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100025736.1.15",
          "form": "그러게요..ㅎㅎ..",
          "original_form": "그러게요..ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100025736.1.16",
          "form": "저는 겨울 바다 보는 거 좋아하는데 점점 가까워져서 신나네요",
          "original_form": "저는 겨울 바다 보는 거 좋아하는데 점점 가까워져서 신나네요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100025736.1.17",
          "form": "겨울쯤 되면 이제 자유롭개게 갈 수 있겠죠? ㅎㅎ",
          "original_form": "겨울쯤 되면 이제 자유롭개게 갈 수 있겠죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100025736.1.18",
          "form": "그러길 바라고 있답니다..",
          "original_form": "그러길 바라고 있답니다..",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100025736.1.19",
          "form": "1년동안 코로나 때문에 여행 많이 못 가본 게 너무 슬프네요ㅠ",
          "original_form": "1년동안 코로나 때문에 여행 많이 못 가본 게 너무 슬프네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100025736.1.20",
          "form": "맞아요 올해는 여행 좀 다니고 싶었는데 그냥 다 날리다니.... ㅠㅠ",
          "original_form": "맞아요 올해는 여행 좀 다니고 싶었는데 그냥 다 날리다니.... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        }
      ]
