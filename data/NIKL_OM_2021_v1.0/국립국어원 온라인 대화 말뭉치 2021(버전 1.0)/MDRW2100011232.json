      "utterance": [
        {
          "id": "MDRW2100011232.1.1",
          "form": "아니요 일어나자마자 폭풍 수강신청하고",
          "original_form": "아니요 일어나자마자 폭풍 수강신청하고",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.2",
          "form": "넘 귀찮아서",
          "original_form": "넘 귀찮아서",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.3",
          "form": "원래 아침을 거릅니다...",
          "original_form": "원래 아침을 거릅니다...",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.4",
          "form": "아 수강신청~~",
          "original_form": "아 수강신청~~",
          "speaker_id": "1",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.5",
          "form": "오늘 요거트 만들려고햤는ㄷ0",
          "original_form": "오늘 요거트 만들려고햤는ㄷ0",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.6",
          "form": "난 아침에 오리훈제먹고나옴 ㅋㅋㅋㅋ",
          "original_form": "난 아침에 오리훈제먹고나옴 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.7",
          "form": "비가 넘 마니오네",
          "original_form": "비가 넘 마니오네",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.8",
          "form": "오리훈제ㅜㅜㅜ 개맛있곗다",
          "original_form": "오리훈제ㅜㅜㅜ 개맛있곗다",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.9",
          "form": "저도 주세요",
          "original_form": "저도 주세요",
          "speaker_id": "2",
          "time": "20210824 09:21"
        },
        {
          "id": "MDRW2100011232.1.10",
          "form": "엄마랑살면 밥은진짜 잘먹는듯",
          "original_form": "엄마랑살면 밥은진짜 잘먹는듯",
          "speaker_id": "1",
          "time": "20210824 09:22"
        },
        {
          "id": "MDRW2100011232.1.11",
          "form": "나 살빼려면 자취해야함 ㄹㅇ로 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나 살빼려면 자취해야함 ㄹㅇ로 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:22"
        },
        {
          "id": "MDRW2100011232.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ ㄹㅇ이야구",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ ㄹㅇ이야구",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011232.1.13",
          "form": "귀찮아서 안 먹게되는 경지에",
          "original_form": "귀찮아서 안 먹게되는 경지에",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011232.1.14",
          "form": "올라야되는데",
          "original_form": "올라야되는데",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011232.1.15",
          "form": "밥 끼니 맞춰서 나오면 나도 다먹음 ㅎㅎ 집밥 최고",
          "original_form": "밥 끼니 맞춰서 나오면 나도 다먹음 ㅎㅎ 집밥 최고",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011232.1.16",
          "form": "나 집에 혼자있으면 귀찮아서 안먹ㄱ거든?",
          "original_form": "나 집에 혼자있으면 귀찮아서 안먹ㄱ거든?",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011232.1.17",
          "form": "그럼 엄마가 존나화냄",
          "original_form": "그럼 엄마가 존나화냄",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011232.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100011232.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ개웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋ개웃겨",
          "speaker_id": "2",
          "time": "20210824 09:58"
        },
        {
          "id": "MDRW2100011232.1.20",
          "form": "그것이 가족의 사랑..",
          "original_form": "그것이 가족의 사랑..",
          "speaker_id": "2",
          "time": "20210824 09:58"
        },
        {
          "id": "MDRW2100011232.1.21",
          "form": "밥먹기 귀찮다 무ㅜ먹지",
          "original_form": "밥먹기 귀찮다 무ㅜ먹지",
          "speaker_id": "2",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011232.1.22",
          "form": "냉동실에 볶음밥 개많을듯",
          "original_form": "냉동실에 볶음밥 개많을듯",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011232.1.23",
          "form": "하나돌려먹어",
          "original_form": "하나돌려먹어",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011232.1.24",
          "form": "와 정확합니다",
          "original_form": "와 정확합니다",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011232.1.25",
          "form": "삼겹살김치볶늠밥 먹엇어",
          "original_form": "삼겹살김치볶늠밥 먹엇어",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011232.1.26",
          "form": "점심 뭐먹었어",
          "original_form": "점심 뭐먹었어",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011232.1.27",
          "form": "아 맛잇겟다 윤미표인가요?",
          "original_form": "아 맛잇겟다 윤미표인가요?",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011232.1.28",
          "form": "닭봉조림먹엇슴다",
          "original_form": "닭봉조림먹엇슴다",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011232.1.29",
          "form": "배달표 입니닥",
          "original_form": "배달표 입니닥",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
