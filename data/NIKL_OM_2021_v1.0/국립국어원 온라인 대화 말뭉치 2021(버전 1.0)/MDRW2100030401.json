      "utterance": [
        {
          "id": "MDRW2100030401.1.1",
          "form": "남친 화이자 1차???",
          "original_form": "남친 화이자 1차???",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030401.1.2",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030401.1.3",
          "form": "조금 아팠댜",
          "original_form": "조금 아팠댜",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030401.1.4",
          "form": "아 열나고???",
          "original_form": "아 열나고???",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030401.1.5",
          "form": "걔는 그 백신휴가 받아서 개부럽",
          "original_form": "걔는 그 백신휴가 받아서 개부럽",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030401.1.6",
          "form": "넌 화이자 2차까지 맞은거지?",
          "original_form": "넌 화이자 2차까지 맞은거지?",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030401.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.8",
          "form": "부럽",
          "original_form": "부럽",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.9",
          "form": "ㄴㄴ 열은 안나구 몸살기운 있데",
          "original_form": "ㄴㄴ 열은 안나구 몸살기운 있데",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.10",
          "form": "백신휴가 진심 부럽다",
          "original_form": "백신휴가 진심 부럽다",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.11",
          "form": "네넹 저는 끝났슴니댜",
          "original_form": "네넹 저는 끝났슴니댜",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.13",
          "form": "그나마 다행인가",
          "original_form": "그나마 다행인가",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.14",
          "form": "이제 우리 봐 도됭...",
          "original_form": "이제 우리 봐 도됭...",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.15",
          "form": "님 안아팠나요????",
          "original_form": "님 안아팠나요????",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.16",
          "form": "3중 2가 백신 다 맞음",
          "original_form": "3중 2가 백신 다 맞음",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.18",
          "form": "오 우리이제 깍두기 ㅋㅋㅋ",
          "original_form": "오 우리이제 깍두기 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.19",
          "form": "저는 1차는 잘 지나가고 2차는 개아픔...",
          "original_form": "저는 1차는 잘 지나가고 2차는 개아픔...",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.20",
          "form": "ㅁㅈㅁㅈ가 왜 나쁜말이지",
          "original_form": "ㅁㅈㅁㅈ가 왜 나쁜말이지",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.21",
          "form": "6일부터 저녁도 완화 되더라",
          "original_form": "6일부터 저녁도 완화 되더라",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.22",
          "form": "2차 때 열 오름??",
          "original_form": "2차 때 열 오름??",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.23",
          "form": "헐 기준이 뭐여",
          "original_form": "헐 기준이 뭐여",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.24",
          "form": "아 다행이다...",
          "original_form": "아 다행이다...",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030401.1.25",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030401.1.26",
          "form": "줏대가 없네",
          "original_form": "줏대가 없네",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030401.1.27",
          "form": "약간 오르고 말음ㅇㅇ 하지만 근육통이ㅋㅋ",
          "original_form": "약간 오르고 말음ㅇㅇ 하지만 근육통이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030401.1.28",
          "form": "약 머금??",
          "original_form": "약 머금??",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030401.1.29",
          "form": "고생했다 이겨내느라",
          "original_form": "고생했다 이겨내느라",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030401.1.30",
          "form": "ㅇㅇ 머금",
          "original_form": "ㅇㅇ 머금",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030401.1.31",
          "form": "감사합댜",
          "original_form": "감사합댜",
          "speaker_id": "2",
          "time": "20210904 00:35"
        }
      ]
