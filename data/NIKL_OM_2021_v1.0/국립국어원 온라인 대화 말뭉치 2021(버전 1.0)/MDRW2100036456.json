      "utterance": [
        {
          "id": "MDRW2100036456.1.1",
          "form": "아니 아닐껄??",
          "original_form": "아니 아닐껄??",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.2",
          "form": "여긴 안오는거닽은데",
          "original_form": "여긴 안오는거닽은데",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.3",
          "form": "오 같구나",
          "original_form": "오 같구나",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.4",
          "form": "사실 암맘커튼을 치고 살아서",
          "original_form": "사실 암맘커튼을 치고 살아서",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.5",
          "form": "요새 밤에 자꾸 비가 와",
          "original_form": "요새 밤에 자꾸 비가 와",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.6",
          "form": "날씨를 잘 모르겠어",
          "original_form": "날씨를 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.7",
          "form": "나도사실",
          "original_form": "나도사실",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.8",
          "form": "커튼도 치고사는데다 고층이라 잘 안보여",
          "original_form": "커튼도 치고사는데다 고층이라 잘 안보여",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.9",
          "form": "비는 낮에 오는 것 보다는 밤에 오는게 나은 것 같아",
          "original_form": "비는 낮에 오는 것 보다는 밤에 오는게 나은 것 같아",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.10",
          "form": "창문까지 닫으면 아무것도 안들려",
          "original_form": "창문까지 닫으면 아무것도 안들려",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.11",
          "form": "맞아 시원하고 좋아",
          "original_form": "맞아 시원하고 좋아",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.12",
          "form": "그치 고층이면 빗소리도 안들려",
          "original_form": "그치 고층이면 빗소리도 안들려",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.13",
          "form": "낮에는 우산쓰기 귀찮잖아",
          "original_form": "낮에는 우산쓰기 귀찮잖아",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036456.1.14",
          "form": "어우 발이 다 젖는것도 정말 너무싫어",
          "original_form": "어우 발이 다 젖는것도 정말 너무싫어",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036456.1.15",
          "form": "슬리퍼를 신어야지.",
          "original_form": "슬리퍼를 신어야지.",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036456.1.16",
          "form": "근데 슬리퍼 사이로 모래가들어사",
          "original_form": "근데 슬리퍼 사이로 모래가들어사",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036456.1.17",
          "form": "내일도 비온대 근데",
          "original_form": "내일도 비온대 근데",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036456.1.18",
          "form": "아 그거 알아",
          "original_form": "아 그거 알아",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036456.1.19",
          "form": "휴ㅠ 너무 싫다",
          "original_form": "휴ㅠ 너무 싫다",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036456.1.20",
          "form": "그러면 물로 또 헹궈",
          "original_form": "그러면 물로 또 헹궈",
          "speaker_id": "1",
          "time": "20210830 23:21"
        }
      ]
    }
