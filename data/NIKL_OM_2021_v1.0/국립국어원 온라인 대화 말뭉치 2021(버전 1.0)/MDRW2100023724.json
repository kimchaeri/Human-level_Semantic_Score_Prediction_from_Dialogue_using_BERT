      "utterance": [
        {
          "id": "MDRW2100023724.1.1",
          "form": "막 돌고래보고 키티박물관인가 가고 그랬거든요ㅠㅠ",
          "original_form": "막 돌고래보고 키티박물관인가 가고 그랬거든요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023724.1.2",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100023724.1.3",
          "form": "어린위주의 코스는 거의 없었거든요",
          "original_form": "어린위주의 코스는 거의 없었거든요",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100023724.1.4",
          "form": "아쿠아플라넷인가 거기 가셨나요",
          "original_form": "아쿠아플라넷인가 거기 가셨나요",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100023724.1.5",
          "form": "키티박물관 지나가기만 했는데",
          "original_form": "키티박물관 지나가기만 했는데",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100023724.1.6",
          "form": "네네 맞아요 \n돌고래도 사진 찍고",
          "original_form": "네네 맞아요 \n돌고래도 사진 찍고",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100023724.1.7",
          "form": "되게 규모가 있더라구요",
          "original_form": "되게 규모가 있더라구요",
          "speaker_id": "2",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100023724.1.8",
          "form": "돈 더 내면 손잡고 찍게 해주더라구요",
          "original_form": "돈 더 내면 손잡고 찍게 해주더라구요",
          "speaker_id": "1",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100023724.1.9",
          "form": "뽑아도주곸ㅋ",
          "original_form": "뽑아도주곸ㅋ",
          "speaker_id": "1",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100023724.1.10",
          "form": "담에 키티 좋아하는 친구 오면 같이 가봐야겠네요",
          "original_form": "담에 키티 좋아하는 친구 오면 같이 가봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100023724.1.11",
          "form": "제주에 국제학교 있어서 겸사겸사 내려와 사는 돈많은 부모들도 있는것 같어요",
          "original_form": "제주에 국제학교 있어서 겸사겸사 내려와 사는 돈많은 부모들도 있는것 같어요",
          "speaker_id": "2",
          "time": "20210830 15:53"
        },
        {
          "id": "MDRW2100023724.1.12",
          "form": "와 부럽네요 ㅎㅎ",
          "original_form": "와 부럽네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:53"
        },
        {
          "id": "MDRW2100023724.1.13",
          "form": "자연이.좋으니까 여행 겸 거기에 코로나까지",
          "original_form": "자연이.좋으니까 여행 겸 거기에 코로나까지",
          "speaker_id": "2",
          "time": "20210830 15:53"
        },
        {
          "id": "MDRW2100023724.1.14",
          "form": "제주는 아직 조금 덜나오죠??ㅠㅠ",
          "original_form": "제주는 아직 조금 덜나오죠??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100023724.1.15",
          "form": "여름 여행 성수기에 많아졌다 줄어드는 추세예요",
          "original_form": "여름 여행 성수기에 많아졌다 줄어드는 추세예요",
          "speaker_id": "2",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100023724.1.16",
          "form": "해외여행을 못가니 아무래도 제주 여행 주가가 높은 것 같네요",
          "original_form": "해외여행을 못가니 아무래도 제주 여행 주가가 높은 것 같네요",
          "speaker_id": "2",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100023724.1.17",
          "form": "맞아요 해외를 못가니 정말 답답하면 제주라도 가고싶을것 같아요",
          "original_form": "맞아요 해외를 못가니 정말 답답하면 제주라도 가고싶을것 같아요",
          "speaker_id": "1",
          "time": "20210830 15:55"
        },
        {
          "id": "MDRW2100023724.1.18",
          "form": "저도 그래서 제주로 온 케이스요",
          "original_form": "저도 그래서 제주로 온 케이스요",
          "speaker_id": "2",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100023724.1.19",
          "form": "제주살이 하면 보통 집은 얼마쯤 하나요??",
          "original_form": "제주살이 하면 보통 집은 얼마쯤 하나요??",
