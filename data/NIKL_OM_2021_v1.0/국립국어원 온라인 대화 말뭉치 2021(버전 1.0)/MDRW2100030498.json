      "utterance": [
        {
          "id": "MDRW2100030498.1.1",
          "form": "네... 9시에 또",
          "original_form": "네... 9시에 또",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030498.1.2",
          "form": "안타깝다",
          "original_form": "안타깝다",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030498.1.3",
          "form": "매일매일 갱신되나",
          "original_form": "매일매일 갱신되나",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030498.1.4",
          "form": "30분마다...",
          "original_form": "30분마다...",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030498.1.5",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030498.1.6",
          "form": "자리 생기면...",
          "original_form": "자리 생기면...",
          "speaker_id": "2",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030498.1.7",
          "form": "끔찍해...",
          "original_form": "끔찍해...",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030498.1.8",
          "form": "우리는 걍 즉각적으로 뜨는데",
          "original_form": "우리는 걍 즉각적으로 뜨는데",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030498.1.9",
          "form": "우리도 옛날엔 그랬었는데",
          "original_form": "우리도 옛날엔 그랬었는데",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030498.1.10",
          "form": "왜 그런 시스템을",
          "original_form": "왜 그런 시스템을",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030498.1.11",
          "form": "바뀌고 걍 30분마다 수강신청 됨",
          "original_form": "바뀌고 걍 30분마다 수강신청 됨",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030498.1.12",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030498.1.13",
          "form": "바꿀 때는... 하루종일 봐야하니까 좀 쉬다가 30분에 한번 확인하세요~ 하는 의도였는데",
          "original_form": "바꿀 때는... 하루종일 봐야하니까 좀 쉬다가 30분에 한번 확인하세요~ 하는 의도였는데",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030498.1.14",
          "form": "아냐.......... 제발 이러지마",
          "original_form": "아냐.......... 제발 이러지마",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030498.1.15",
          "form": "오히려 30분마다 안 보면 불안하잖아",
          "original_form": "오히려 30분마다 안 보면 불안하잖아",
          "speaker_id": "1",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030498.1.16",
          "form": "시간 맞춰서 봐야하고...",
          "original_form": "시간 맞춰서 봐야하고...",
          "speaker_id": "1",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030498.1.17",
          "form": "그러니까...",
          "original_form": "그러니까...",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030498.1.18",
          "form": "잡기도 어렵고...",
          "original_form": "잡기도 어렵고...",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030498.1.19",
          "form": "싫다",
          "original_form": "싫다",
          "speaker_id": "1",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030498.1.20",
          "form": "잡기 어려우니까 사람들 잘 안 버리고...",
          "original_form": "잡기 어려우니까 사람들 잘 안 버리고...",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030498.1.21",
          "form": "맨날 악순환임",
          "original_form": "맨날 악순환임",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030498.1.22",
          "form": "고통",
          "original_form": "고통",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030498.1.23",
          "form": "최악이에요",
          "original_form": "최악이에요",
          "speaker_id": "1",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030498.1.24",
          "form": "님네 학교가 수강신청 시스템 하나는 월등히 별로인 듯",
          "original_form": "님네 학교가 수강신청 시스템 하나는 월등히 별로인 듯",
          "speaker_id": "1",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030498.1.25",
          "form": "진심... 너무 싫어",
          "original_form": "진심... 너무 싫어",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030498.1.26",
          "form": "제발 다시 바꿔줬으면...",
          "original_form": "제발 다시 바꿔줬으면...",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030498.1.27",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030498.1.28",
          "form": "안타깝다",
          "original_form": "안타깝다",
          "speaker_id": "1",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030498.1.29",
          "form": "이거 결정한 사람 월급도 수강신청해서 줘야함",
          "original_form": "이거 결정한 사람 월급도 수강신청해서 줘야함",
          "speaker_id": "1",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030498.1.30",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210904 02:18"
        },
        {
          "id": "MDRW2100030498.1.31",
          "form": "자리 안 나서 30분마다 수강신청 해봐야",
          "original_form": "자리 안 나서 30분마다 수강신청 해봐야",
          "speaker_id": "2",
          "time": "20210904 02:19"
        },
        {
          "id": "MDRW2100030498.1.32",
          "form": "아 이건 진짜 아니구나 하지",
          "original_form": "아 이건 진짜 아니구나 하지",
          "speaker_id": "2",
          "time": "20210904 02:19"
        },
        {
          "id": "MDRW2100030498.1.33",
          "form": "직접 본인의 월급을 잡아보시지",
          "original_form": "직접 본인의 월급을 잡아보시지",
          "speaker_id": "1",
          "time": "20210904 02:19"
        },
        {
          "id": "MDRW2100030498.1.34",
          "form": "내 시간표 맨날 바뀜",
          "original_form": "내 시간표 맨날 바뀜",
          "speaker_id": "1",
          "time": "20210906 19:26"
        }
      ]
