      "utterance": [
        {
          "id": "MDRW2100042286.1.1",
          "form": "아ㅠㅠ 여행 갔는데 날씨 안좋으면 아쉽죠...",
          "original_form": "아ㅠㅠ 여행 갔는데 날씨 안좋으면 아쉽죠...",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042286.1.2",
          "form": "제주도에서 저는",
          "original_form": "제주도에서 저는",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042286.1.3",
          "form": "망고떡이 제일 좋았는데",
          "original_form": "망고떡이 제일 좋았는데",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042286.1.4",
          "form": "사람들이 은근 잘 모르더라구요...",
          "original_form": "사람들이 은근 잘 모르더라구요...",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042286.1.5",
          "form": "기념품으로 최고인데!",
          "original_form": "기념품으로 최고인데!",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042286.1.6",
          "form": "헐 저두 첨들어봐요ㅋㅋㅋ",
          "original_form": "헐 저두 첨들어봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042286.1.7",
          "form": "오메기떡만 알았네여ㅜㅜ",
          "original_form": "오메기떡만 알았네여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042286.1.8",
          "form": "오메기떡도 맛있는데 아마 파는 곳 옆에 보면 망고떡도 팔 거예요",
          "original_form": "오메기떡도 맛있는데 아마 파는 곳 옆에 보면 망고떡도 팔 거예요",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042286.1.9",
          "form": "진짜 너무너무 맛있엇어요",
          "original_form": "진짜 너무너무 맛있엇어요",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042286.1.10",
          "form": "제주도 또 가고 싶네요 정말",
          "original_form": "제주도 또 가고 싶네요 정말",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042286.1.11",
          "form": "해물라면도 넘 좋앗구...",
          "original_form": "해물라면도 넘 좋앗구...",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042286.1.12",
          "form": "해물이 엄청 실했겠어요",
          "original_form": "해물이 엄청 실했겠어요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042286.1.13",
          "form": "저는 고등어회가 젤 기억에남아여",
          "original_form": "저는 고등어회가 젤 기억에남아여",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042286.1.14",
          "form": "고등어회 진짜 특이하다고 해야 하나",
          "original_form": "고등어회 진짜 특이하다고 해야 하나",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042286.1.15",
          "form": "신기한 것 같아요",
          "original_form": "신기한 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042286.1.16",
          "form": "원래 회로 잘 안먹는 물고기다 보니까...",
          "original_form": "원래 회로 잘 안먹는 물고기다 보니까...",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042286.1.17",
          "form": "해외 여행은 어디 가보셨어요?",
          "original_form": "해외 여행은 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042286.1.18",
          "form": "그쵸 저두 첨먹어봤는데 반했네여",
          "original_form": "그쵸 저두 첨먹어봤는데 반했네여",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042286.1.19",
          "form": "저는 일본 대만 마카오 방콕 요정도만 가봤어용",
          "original_form": "저는 일본 대만 마카오 방콕 요정도만 가봤어용",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042286.1.20",
          "form": "헉 다 제가 좋아하는 여행지예요!ㅋㅋㅋ",
          "original_form": "헉 다 제가 좋아하는 여행지예요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042286.1.21",
          "form": "맛잇는 거 많은 곳으로 알차게 다니셨네요!!",
          "original_form": "맛잇는 거 많은 곳으로 알차게 다니셨네요!!",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042286.1.22",
          "form": "ㅎㅎ거의 먹방여행이죠",
          "original_form": "ㅎㅎ거의 먹방여행이죠",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042286.1.23",
          "form": "다시 가보고싶은 나라 있으세여?",
          "original_form": "다시 가보고싶은 나라 있으세여?",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042286.1.24",
          "form": "저는 일본이 제일 좋아요!",
          "original_form": "저는 일본이 제일 좋아요!",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100042286.1.25",
          "form": "분위기도 음식도 다 잘맞아서ㅎㅎ",
          "original_form": "분위기도 음식도 다 잘맞아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042286.1.26",
          "form": "도쿄, 나고야, 오사카 등등을 가봤는데 다음에는 교토랑 홋카이도로",
          "original_form": "도쿄, 나고야, 오사카 등등을 가봤는데 다음에는 교토랑 홋카이도로",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042286.1.27",
          "form": "가고 싶어요",
          "original_form": "가고 싶어요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042286.1.28",
          "form": "저도 일본 좋았어요ㅜㅜ도쿄랑 오사카는 가봤는데 나고야도 좋은가요??",
          "original_form": "저도 일본 좋았어요ㅜㅜ도쿄랑 오사카는 가봤는데 나고야도 좋은가요??",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100042286.1.29",
          "form": "나고야는 정말 도심지고",
          "original_form": "나고야는 정말 도심지고",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042286.1.30",
          "form": "여행지가 아니라서 로컬 느낌 나는 게 좋았어요!",
          "original_form": "여행지가 아니라서 로컬 느낌 나는 게 좋았어요!",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042286.1.31",
          "form": "나고야 음식은 간이 센 걸로도 유명한데",
          "original_form": "나고야 음식은 간이 센 걸로도 유명한데",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042286.1.32",
          "form": "히쯔마부시가 정말 맛있더라구요ㅎㅎ",
          "original_form": "히쯔마부시가 정말 맛있더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042286.1.33",
          "form": "헉 간이 더 세요?ㅋㅋ저 오사카에서도 음식이 너무 짜서 놀랐거든요",
          "original_form": "헉 간이 더 세요?ㅋㅋ저 오사카에서도 음식이 너무 짜서 놀랐거든요",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042286.1.34",
          "form": "ㅋㅋㅋㅋㅋ좀 단맛이 강해요!",
          "original_form": "ㅋㅋㅋㅋㅋ좀 단맛이 강해요!",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042286.1.35",
          "form": "일본 음식 안맞는 친구는 음식이 너무 달아서",
          "original_form": "일본 음식 안맞는 친구는 음식이 너무 달아서",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042286.1.36",
          "form": "매운 걸 자꾸 찾더라구요ㅋㅋ",
          "original_form": "매운 걸 자꾸 찾더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:40"
        }
      ]
    }
