      "utterance": [
        {
          "id": "MDRW2100015592.1.1",
          "form": "오늘 아점저 뭐드셨어요",
          "original_form": "오늘 아점저 뭐드셨어요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015592.1.2",
          "form": "아침음 계란 두알",
          "original_form": "아침음 계란 두알",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015592.1.3",
          "form": "점심은 옛날 도시락",
          "original_form": "점심은 옛날 도시락",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015592.1.4",
          "form": "저녁은 맥도날드 창녕갈릭버거",
          "original_form": "저녁은 맥도날드 창녕갈릭버거",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015592.1.5",
          "form": "사이사이에 도넛2개, 메이플피칸파이, 고로케 두개",
          "original_form": "사이사이에 도넛2개, 메이플피칸파이, 고로케 두개",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015592.1.6",
          "form": "진정한 폭식의 날",
          "original_form": "진정한 폭식의 날",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015592.1.7",
          "form": "오 ㄱㅖ란 루틴 지키고있네요",
          "original_form": "오 ㄱㅖ란 루틴 지키고있네요",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015592.1.8",
          "form": "헐 찬녕갈릭버거뭐야",
          "original_form": "헐 찬녕갈릭버거뭐야",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015592.1.9",
          "form": "근데 추천은 안해요",
          "original_form": "근데 추천은 안해요",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015592.1.10",
          "form": "소스가 너무 많이 들어있어서 깔끔하게 먹기 힘듭니다",
          "original_form": "소스가 너무 많이 들어있어서 깔끔하게 먹기 힘듭니다",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015592.1.11",
          "form": "아 그런거구나ㅠㅠ",
          "original_form": "아 그런거구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015592.1.12",
          "form": "너무 질질 흘리고 먹었어요",
          "original_form": "너무 질질 흘리고 먹었어요",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015592.1.13",
          "form": "소스 빼주세요 안되려나",
          "original_form": "소스 빼주세요 안되려나",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015592.1.14",
          "form": "갈릭버거에 갈릭소스가 생명같아요",
          "original_form": "갈릭버거에 갈릭소스가 생명같아요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015592.1.15",
          "form": "녹차라떼에 녹차빼달라는 느낌같네요",
          "original_form": "녹차라떼에 녹차빼달라는 느낌같네요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015592.1.16",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015592.1.17",
          "form": "서브웨이처럼",
          "original_form": "서브웨이처럼",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015592.1.18",
          "form": "name1님은 아점저 뭐드셨나요",
          "original_form": "&name1&님은 아점저 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015592.1.19",
          "form": "조금 적게 넣어주세요",
          "original_form": "조금 적게 넣어주세요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015592.1.20",
          "form": "아침 고구마 스무디 토마튜 주스",
          "original_form": "아침 고구마 스무디 토마튜 주스",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015592.1.21",
          "form": "점심 짬뽕 샤인머스캣 에읻.",
