      "utterance": [
        {
          "id": "MDRW2100020069.1.1",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020069.1.2",
          "form": "점심 메뉴 정하셨나요",
          "original_form": "점심 메뉴 정하셨나요",
          "speaker_id": "1",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020069.1.3",
          "form": "앗 저녁은 치킨먹었어요~",
          "original_form": "앗 저녁은 치킨먹었어요~",
          "speaker_id": "2",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100020069.1.4",
          "form": "점심은 방금 돼지국밥먹었어요 국물도 남김없이요ㅎㅎ",
          "original_form": "점심은 방금 돼지국밥먹었어요 국물도 남김없이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100020069.1.5",
          "form": "돼지국밥 맛있죠ㅜ 안먹은지 몇년됐는데 그립네요",
          "original_form": "돼지국밥 맛있죠ㅜ 안먹은지 몇년됐는데 그립네요",
          "speaker_id": "1",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020069.1.6",
          "form": "근처에 맛집이 있나봐요",
          "original_form": "근처에 맛집이 있나봐요",
          "speaker_id": "1",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020069.1.7",
          "form": "오 맞아요 ㅎㅎ 걸어서 갔다 왔죠~",
          "original_form": "오 맞아요 ㅎㅎ 걸어서 갔다 왔죠~",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020069.1.8",
          "form": "얼마전에 처음먹었는데 맛있어서 또 먹었어요",
          "original_form": "얼마전에 처음먹었는데 맛있어서 또 먹었어요",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020069.1.9",
          "form": "님은 점심은 뭐드시나요?",
          "original_form": "님은 점심은 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020069.1.10",
          "form": "짬뽕과 탕수육 군만두 먹었어요",
          "original_form": "짬뽕과 탕수육 군만두 먹었어요",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020069.1.11",
          "form": "군만두가 일품이네요ㅎㅎ",
          "original_form": "군만두가 일품이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020069.1.12",
          "form": "우와 맛있는거 드시네요",
          "original_form": "우와 맛있는거 드시네요",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020069.1.13",
          "form": "아니ㅋㅋㅋ 짬뽕은 어떠셨어요? 군만두 좋아하시나보네요",
          "original_form": "아니ㅋㅋㅋ 짬뽕은 어떠셨어요? 군만두 좋아하시나보네요",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020069.1.14",
          "form": "송탄 영빈루 좋아했는데 근처에 분점이 생겼더라고요",
          "original_form": "송탄 영빈루 좋아했는데 근처에 분점이 생겼더라고요",
          "speaker_id": "1",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020069.1.15",
          "form": "여기 원래 짬뽕맛집이에요",
          "original_form": "여기 원래 짬뽕맛집이에요",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020069.1.16",
          "form": "고기 토핑에 국물이 찐함",
          "original_form": "고기 토핑에 국물이 찐함",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020069.1.17",
          "form": "오 대박 먹을 줄 아시는 분이네요 ㅋㅋㅋ",
          "original_form": "오 대박 먹을 줄 아시는 분이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020069.1.18",
          "form": "국물 찐하다.. 침고이네요 밥 방금 먹었는데",
          "original_form": "국물 찐하다.. 침고이네요 밥 방금 먹었는데",
          "speaker_id": "2",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100020069.1.19",
          "form": "여기 군만두도 고기가 실하고 육즙이 풍부해요",
          "original_form": "여기 군만두도 고기가 실하고 육즙이 풍부해요",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020069.1.20",
          "form": "지나갈일 생기면 드셔보세요!",
          "original_form": "지나갈일 생기면 드셔보세요!",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020069.1.21",
          "form": "우와 군만두에 육즙나오는거 진짜 좋아하거든요ㅜㅜ",
          "original_form": "우와 군만두에 육즙나오는거 진짜 좋아하거든요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020069.1.22",
          "form": "네 꼭 먹어봐야겠어요!",
          "original_form": "네 꼭 먹어봐야겠어요!",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020069.1.23",
          "form": "지금은 카페에서 꽃에이드 먹고있는데 괜찮네요",
          "original_form": "지금은 카페에서 꽃에이드 먹고있는데 괜찮네요",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020069.1.24",
          "form": "네ㅎ 집근처 분점은 군만두 5피스에 7000원이길래 미묘하게 비싼가했는데 전혀 아니었어요",
          "original_form": "네ㅎ 집근처 분점은 군만두 5피스에 7000원이길래 미묘하게 비싼가했는데 전혀 아니었어요",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020069.1.25",
          "form": "크기도 큼!",
          "original_form": "크기도 큼!",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020069.1.26",
          "form": "꽃에이드요? 안마셔봐서 궁금하네요",
          "original_form": "꽃에이드요? 안마셔봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020069.1.27",
          "form": "체인카페가 아닌가요?",
          "original_form": "체인카페가 아닌가요?",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020069.1.28",
          "form": "헉 가격보고 놀랐는데 크기가 크면 아주 만족이네요",
          "original_form": "헉 가격보고 놀랐는데 크기가 크면 아주 만족이네요",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020069.1.29",
          "form": "네 개인카페에요. 그냥 상큼 달달한 에이드에 꽃향기가 나네요",
          "original_form": "네 개인카페에요. 그냥 상큼 달달한 에이드에 꽃향기가 나네요",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020069.1.30",
          "form": "가격이 어떻길래ㄷㄷㄷ",
          "original_form": "가격이 어떻길래ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 14:02"
        }
      ]
    }
