          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019352.1.29",
          "form": "비오는 날엔 장보면 안돼",
          "original_form": "비오는 날엔 장보면 안돼",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019352.1.30",
          "form": "ㅇㅇ아니 ㄹㅇ",
          "original_form": "ㅇㅇ아니 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019352.1.31",
          "form": "1분 창문열어놓으니까",
          "original_form": "1분 창문열어놓으니까",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019352.1.32",
          "form": "바닥에 물이 고이더라구",
          "original_form": "바닥에 물이 고이더라구",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019352.1.33",
          "form": "이런날에는 집에 콕 박혀있어야해서",
          "original_form": "이런날에는 집에 콕 박혀있어야해서",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019352.1.34",
          "form": "실천했지",
          "original_form": "실천했지",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019352.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019352.1.36",
          "form": "암튼 그래셔",
          "original_form": "암튼 그래셔",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019352.1.37",
          "form": "내일은 출근해서",
          "original_form": "내일은 출근해서",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019352.1.38",
          "form": "반숙란이랑 우유를 아침으로 먹고",
          "original_form": "반숙란이랑 우유를 아침으로 먹고",
