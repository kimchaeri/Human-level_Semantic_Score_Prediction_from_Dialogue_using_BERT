          "original_form": "???",
          "speaker_id": "2",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100019352.1.7",
          "form": "회사에서 한달에 한번?정도 사는데",
          "original_form": "회사에서 한달에 한번?정도 사는데",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.8",
          "form": "뭘 그렇게 많이 샀어?",
          "original_form": "뭘 그렇게 많이 샀어?",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.9",
          "form": "주로 과자 음료",
          "original_form": "주로 과자 음료",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.10",
          "form": "그러다가 요즘은 직원들의 수요를 반영해서",
          "original_form": "그러다가 요즘은 직원들의 수요를 반영해서",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.11",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.12",
          "form": "반숙란 우유 두유 샤인머스캣",
          "original_form": "반숙란 우유 두유 샤인머스캣",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.13",
          "form": "언니돈으로 사는건 아니구나",
          "original_form": "언니돈으로 사는건 아니구나",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.14",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019352.1.15",
          "form": "샤인머스캣ㅋㅋㅋㅋㅋㅋㅋㅋ",
