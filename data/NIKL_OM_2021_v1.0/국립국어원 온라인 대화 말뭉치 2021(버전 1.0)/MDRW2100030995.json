      "utterance": [
        {
          "id": "MDRW2100030995.1.1",
          "form": "지금 여행중이시면",
          "original_form": "지금 여행중이시면",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030995.1.2",
          "form": "다음엔 어디가세요?",
          "original_form": "다음엔 어디가세요?",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030995.1.3",
          "form": "오후엔 민속촌 가요ㅎㅎ",
          "original_form": "오후엔 민속촌 가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030995.1.4",
          "form": "와 민속촌간지 진짜 오래됐어요",
          "original_form": "와 민속촌간지 진짜 오래됐어요",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030995.1.5",
          "form": "지금 놀이공원 줄서 계신거예요?",
          "original_form": "지금 놀이공원 줄서 계신거예요?",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030995.1.6",
          "form": "네네, 기다리다가 이제 타러 가요",
          "original_form": "네네, 기다리다가 이제 타러 가요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030995.1.7",
          "form": "아마존 익스프레스",
          "original_form": "아마존 익스프레스",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030995.1.8",
          "form": "앗 잘타고 오세요",
          "original_form": "앗 잘타고 오세요",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030995.1.9",
          "form": "타고 왔습니당",
          "original_form": "타고 왔습니당",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030995.1.10",
          "form": "이제 민속촌 가야하는데",
          "original_form": "이제 민속촌 가야하는데",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030995.1.11",
          "form": "한시간 반 걸려요...",
          "original_form": "한시간 반 걸려요...",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030995.1.12",
          "form": "넘나 먼것",
          "original_form": "넘나 먼것",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030995.1.13",
          "form": "헉 ᆢ",
          "original_form": "헉 ᆢ",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030995.1.14",
          "form": "그렇게 먼줄은 몰랐네요",
          "original_form": "그렇게 먼줄은 몰랐네요",
          "speaker_id": "1",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030995.1.15",
          "form": "한복 빌려입으세요?",
          "original_form": "한복 빌려입으세요?",
          "speaker_id": "1",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030995.1.16",
          "form": "한복은 안 입을 것 같아요ㅎㅎ",
          "original_form": "한복은 안 입을 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030995.1.17",
          "form": "친구들이랑 갔으면 빌려입었을지도",
          "original_form": "친구들이랑 갔으면 빌려입었을지도",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030995.1.18",
          "form": "가족끼리라서",
          "original_form": "가족끼리라서",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030995.1.19",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030995.1.20",
          "form": "여행지에 사람 많아요?",
          "original_form": "여행지에 사람 많아요?",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100030995.1.21",
          "form": "에버랜드는 사람 많습니당",
          "original_form": "에버랜드는 사람 많습니당",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030995.1.22",
          "form": "바글바글",
          "original_form": "바글바글",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030995.1.23",
          "form": "열쇠고리도 샀어요",
          "original_form": "열쇠고리도 샀어요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030995.1.24",
          "form": "짱귀욤",
          "original_form": "짱귀욤",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100030995.1.25",
          "form": "어떤 모양사셨어요?ㅎ",
          "original_form": "어떤 모양사셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100030995.1.26",
          "form": "펭귄이용ㅎㅎ ",
          "original_form": "펭귄이용ㅎㅎ ",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100030995.1.27",
          "form": "귀엽겠네요",
          "original_form": "귀엽겠네요",
          "speaker_id": "1",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100030995.1.28",
          "form": "이 나이 먹어도 기념품은 그냥 지나갈수가 없네요",
          "original_form": "이 나이 먹어도 기념품은 그냥 지나갈수가 없네요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        }
      ]
    }
  ]
