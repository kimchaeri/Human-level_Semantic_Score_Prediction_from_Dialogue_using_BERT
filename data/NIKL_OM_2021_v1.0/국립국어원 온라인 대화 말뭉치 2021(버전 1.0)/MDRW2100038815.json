          "form": "헉스",
          "original_form": "헉스",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038815.1.28",
          "form": "학교 가는게 나아?",
          "original_form": "학교 가는게 나아?",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038815.1.29",
          "form": "엉...",
          "original_form": "엉...",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038815.1.30",
          "form": "말 ㄹㅇ",
          "original_form": "말 ㄹㅇ",
