        {
          "id": "MDRW2100039558.1.1",
          "form": "어제요?",
          "original_form": "어제요?",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039558.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039558.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039558.1.4",
          "form": "전 오랜만에 어제 봤어요",
          "original_form": "전 오랜만에 어제 봤어요",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039558.1.5",
          "form": "무섭던가요?",
          "original_form": "무섭던가요?",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039558.1.6",
          "form": "아뇨 ㅋㅋ 그냥..",
          "original_form": "아뇨 ㅋㅋ 그냥..",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039558.1.7",
          "form": "안그래도 요즘 욕먹더라고요",
          "original_form": "안그래도 요즘 욕먹더라고요",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.8",
          "form": "네 저번에",
          "original_form": "네 저번에",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.9",
          "form": "씨랜드 사건 다뤄서..",
          "original_form": "씨랜드 사건 다뤄서..",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.11",
          "form": "원혼취급했다구요",
          "original_form": "원혼취급했다구요",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.12",
          "form": "네 저흰 그 살인사건만 해도",
          "original_form": "네 저흰 그 살인사건만 해도",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.13",
          "form": "별로였잖아요",
          "original_form": "별로였잖아요",
          "speaker_id": "1",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.14",
          "form": "네 선넘었어요",
          "original_form": "네 선넘었어요",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100039558.1.15",
          "form": "네 좀 오바..",
          "original_form": "네 좀 오바..",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039558.1.16",
          "form": "안 보길 잘했어요",
          "original_form": "안 보길 잘했어요",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039558.1.17",
          "form": "그래도 전 봐요 ㅎㅎㅎㅎ",
          "original_form": "그래도 전 봐요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
