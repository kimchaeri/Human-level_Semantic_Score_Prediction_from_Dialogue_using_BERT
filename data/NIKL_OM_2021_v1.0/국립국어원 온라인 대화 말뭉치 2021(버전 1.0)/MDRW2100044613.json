      "utterance": [
        {
          "id": "MDRW2100044613.1.1",
          "form": "아침 날씨가 제법 쌀쌀해졌습니다",
          "original_form": "아침 날씨가 제법 쌀쌀해졌습니다",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044613.1.2",
          "form": "긴팔옷꺼내셨나요",
          "original_form": "긴팔옷꺼내셨나요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044613.1.3",
          "form": "아직요~이번 주말엔 꺼내야할것 같아요",
          "original_form": "아직요~이번 주말엔 꺼내야할것 같아요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044613.1.4",
          "form": "저도ㅠ",
          "original_form": "저도ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044613.1.5",
          "form": "비도오고 오늘은많이춥더라구요",
          "original_form": "비도오고 오늘은많이춥더라구요",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044613.1.6",
          "form": "네 ㅠㅠ 맞아요 날씨가 이러니 아이들이 감기에 잘 걸려요..",
          "original_form": "네 ㅠㅠ 맞아요 날씨가 이러니 아이들이 감기에 잘 걸려요..",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044613.1.7",
          "form": "그래도마스크덕에병원은많이안갔어요",
          "original_form": "그래도마스크덕에병원은많이안갔어요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044613.1.8",
          "form": "저희 애들은 왜 마스크를 써도 그리 아픈지 ㅠ",
          "original_form": "저희 애들은 왜 마스크를 써도 그리 아픈지 ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044613.1.9",
          "form": "아 아직어려서그런가요",
          "original_form": "아 아직어려서그런가요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044613.1.10",
          "form": "저희애는6세 1세라",
          "original_form": "저희애는6세 1세라",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044613.1.11",
          "form": "저희도 5세 3세 ㅎㅎ",
          "original_form": "저희도 5세 3세 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044613.1.12",
          "form": "3세는한참 잘아픈시기인거같아요",
          "original_form": "3세는한참 잘아픈시기인거같아요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044613.1.13",
          "form": "1세는아직태어난지얼마안돼서ㅋㅋ",
          "original_form": "1세는아직태어난지얼마안돼서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044613.1.14",
          "form": "맞아요 ㅠㅠ3세 맨날 아프고 언니한테 옮기고...",
          "original_form": "맞아요 ㅠㅠ3세 맨날 아프고 언니한테 옮기고...",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044613.1.15",
          "form": "ㅠㅠ 병원가는것도일인데",
          "original_form": "ㅠㅠ 병원가는것도일인데",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044613.1.16",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044613.1.17",
          "form": "네 ㅠㅠ 일하니까 더 힘들더라구요",
          "original_form": "네 ㅠㅠ 일하니까 더 힘들더라구요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044613.1.18",
          "form": "아이고 워킹맘이시군요",
          "original_form": "아이고 워킹맘이시군요",
          "speaker_id": "1",
          "time": "20210908 00:34"
        }
      ]
