        },
        {
          "id": "MDRW2100045416.1.5",
          "form": "어떤 다이어트 하세요?",
          "original_form": "어떤 다이어트 하세요?",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045416.1.6",
          "form": "홈트합니다!",
          "original_form": "홈트합니다!",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045416.1.7",
          "form": "오..홈트 좋네요.",
          "original_form": "오..홈트 좋네요.",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045416.1.8",
          "form": "요가 같은거 하시나요?",
          "original_form": "요가 같은거 하시나요?",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045416.1.9",
          "form": "어떤거하세요?",
          "original_form": "어떤거하세요?",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045416.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045416.1.11",
          "form": "근력이랑 유산소",
          "original_form": "근력이랑 유산소",
          "speaker_id": "1",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045416.1.12",
          "form": "오..제대로 하시네요.",
          "original_form": "오..제대로 하시네요.",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045416.1.13",
          "form": "저는 만보걷기 해요",
          "original_form": "저는 만보걷기 해요",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045416.1.14",
          "form": "효과있나요?",
          "original_form": "효과있나요?",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045416.1.15",
          "form": "효과가 있는진 모르겠는데 좀 움직여야할것 같아서요.^^",
          "original_form": "효과가 있는진 모르겠는데 좀 움직여야할것 같아서요.^^",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045416.1.16",
          "form": "더 많이 먹고싶어서ㅋㅋ",
          "original_form": "더 많이 먹고싶어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045416.1.17",
          "form": "먹고 싶은 거 먹고 운동해서 유지하면 좋죠.",
          "original_form": "먹고 싶은 거 먹고 운동해서 유지하면 좋죠.",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045416.1.18",
          "form": "얼마나 하셨어요?",
          "original_form": "얼마나 하셨어요?",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045416.1.19",
          "form": "저 작년에 배민으로도 살 많이 뺐어요",
          "original_form": "저 작년에 배민으로도 살 많이 뺐어요",
          "speaker_id": "1",
          "time": "20210908 02:37"
        },
        {
          "id": "MDRW2100045416.1.20",
          "form": "어떻게요?",
          "original_form": "어떻게요?",
          "speaker_id": "2",
