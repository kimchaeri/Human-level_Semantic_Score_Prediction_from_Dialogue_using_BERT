      "utterance": [
        {
          "id": "MDRW2100002113.1.1",
          "form": "너 재택근무 해?",
          "original_form": "너 재택근무 해?",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.2",
          "form": "나 재택 하지",
          "original_form": "나 재택 하지",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.4",
          "form": "난 프리랜서라 원래 재택이야 ㅋㅋㅋ",
          "original_form": "난 프리랜서라 원래 재택이야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.5",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.6",
          "form": "무슨 일 해?",
          "original_form": "무슨 일 해?",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.7",
          "form": "나 그냥 디자인쪽",
          "original_form": "나 그냥 디자인쪽",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002113.1.8",
          "form": "오 디자인",
          "original_form": "오 디자인",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.9",
          "form": "너는 무슨일하는데?",
          "original_form": "너는 무슨일하는데?",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.10",
          "form": "전공이 그쪽이야?",
          "original_form": "전공이 그쪽이야?",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.11",
          "form": "나는 그냥 회사 다녀",
          "original_form": "나는 그냥 회사 다녀",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.12",
          "form": "웅 전공도 디자인했는데 원래 좀 혼자 조용히 일하는거 좋아해서",
          "original_form": "웅 전공도 디자인했는데 원래 좀 혼자 조용히 일하는거 좋아해서",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.13",
          "form": "이런 일이 잘 맞아",
          "original_form": "이런 일이 잘 맞아",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.14",
          "form": "그렇구나 부럽다",
          "original_form": "그렇구나 부럽다",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.15",
          "form": "능력있네",
          "original_form": "능력있네",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.16",
          "form": "넌 재택 어때?",
          "original_form": "넌 재택 어때?",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.17",
          "form": "나는 가끔만 시켜줘",
          "original_form": "나는 가끔만 시켜줘",
          "speaker_id": "1",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.18",
          "form": "할만해?",
          "original_form": "할만해?",
          "speaker_id": "2",
          "time": "20210819 23:43"
        },
        {
          "id": "MDRW2100002113.1.19",
          "form": "하면 좋지",
          "original_form": "하면 좋지",
          "speaker_id": "1",
          "time": "20210819 23:43"
        }
      ]
