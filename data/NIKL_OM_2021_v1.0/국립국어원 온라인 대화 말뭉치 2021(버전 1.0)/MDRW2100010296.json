      "utterance": [
        {
          "id": "MDRW2100010296.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010296.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010296.1.3",
          "form": "룸메이트 있으신가요?",
          "original_form": "룸메이트 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010296.1.4",
          "form": "아뇨 부모님이랑 살아요",
          "original_form": "아뇨 부모님이랑 살아요",
          "speaker_id": "2",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010296.1.5",
          "form": "아 집안일중 어떤게 그나마 좋으신가요?",
          "original_form": "아 집안일중 어떤게 그나마 좋으신가요?",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010296.1.6",
          "form": "청소보다는 빨래가 좋은거같아요 ㅋㅋㅋ",
          "original_form": "청소보다는 빨래가 좋은거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100010296.1.7",
          "form": "빨랫감 넣기만 하면되니 ㅎ",
          "original_form": "빨랫감 넣기만 하면되니 ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100010296.1.8",
          "form": "ㅋㅋㅋㅋ 저는 그나마 설거지요",
          "original_form": "ㅋㅋㅋㅋ 저는 그나마 설거지요",
          "speaker_id": "1",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010296.1.9",
          "form": "저는이상하게 빨래가너무싫더라고요",
          "original_form": "저는이상하게 빨래가너무싫더라고요",
          "speaker_id": "1",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010296.1.10",
          "form": "아 설거지~ ㅋㅋㅋ저도 설거지 엄청많지않으면 설거지하겟어요",
          "original_form": "아 설거지~ ㅋㅋㅋ저도 설거지 엄청많지않으면 설거지하겟어요",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010296.1.11",
          "form": "기름때 제외하면",
          "original_form": "기름때 제외하면",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010296.1.12",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010296.1.13",
          "form": "집안일은 너무귀찮은거같아요",
          "original_form": "집안일은 너무귀찮은거같아요",
          "speaker_id": "1",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010296.1.14",
          "form": "만능기계가 나오면 좋으련만",
          "original_form": "만능기계가 나오면 좋으련만",
          "speaker_id": "1",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010296.1.15",
          "form": "나중에 돈많이벌면 가사도우미 부르고싶어요 ㅋㅋㅋㅋ",
          "original_form": "나중에 돈많이벌면 가사도우미 부르고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010296.1.16",
          "form": "오 좋죠",
          "original_form": "오 좋죠",
          "speaker_id": "1",
          "time": "20210823 17:03"
        },
        {
          "id": "MDRW2100010296.1.17",
          "form": "오늘도 퇴근하고나서 빨래 해야하네요 ㅠ",
          "original_form": "오늘도 퇴근하고나서 빨래 해야하네요 ㅠ",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010296.1.18",
          "form": "요즘 날이 꿉꿉해서 건조기 안돌리면 찝찝한냄새나드라구여ㅜ",
          "original_form": "요즘 날이 꿉꿉해서 건조기 안돌리면 찝찝한냄새나드라구여ㅜ",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010296.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010296.1.20",
          "form": "너무습하고ㅠ최고조예요",
          "original_form": "너무습하고ㅠ최고조예요",
