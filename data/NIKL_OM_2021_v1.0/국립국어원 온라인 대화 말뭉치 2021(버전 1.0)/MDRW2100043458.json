      "utterance": [
        {
          "id": "MDRW2100043458.1.1",
          "form": "역시 감자튀김이나 치킨요",
          "original_form": "역시 감자튀김이나 치킨요",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043458.1.2",
          "form": "오 저는 맥주 안주로는 피자나 햄버거 좋아해요",
          "original_form": "오 저는 맥주 안주로는 피자나 햄버거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043458.1.3",
          "form": "햄버거요?",
          "original_form": "햄버거요?",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043458.1.4",
          "form": "맥도날드 빅맥이랑 엄청 잘어울려요",
          "original_form": "맥도날드 빅맥이랑 엄청 잘어울려요",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043458.1.5",
          "form": "신기한 조합이네요",
          "original_form": "신기한 조합이네요",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043458.1.6",
          "form": "드셔보시면 생각보다 엄청 잘어울려요",
          "original_form": "드셔보시면 생각보다 엄청 잘어울려요",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043458.1.7",
          "form": "저는 맥주는 감자튀김이랑 먹는게 제일 맛있던데요",
          "original_form": "저는 맥주는 감자튀김이랑 먹는게 제일 맛있던데요",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043458.1.8",
          "form": "감자튀김도 진리죠... 빅맥에 감자튀김까지 ㅋㅋㅋ하면 환상",
          "original_form": "감자튀김도 진리죠... 빅맥에 감자튀김까지 ㅋㅋㅋ하면 환상",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043458.1.9",
          "form": "아 햄버거니까 그렇네요 ㅋㅋ",
          "original_form": "아 햄버거니까 그렇네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043458.1.10",
          "form": "오징어류도 맛있긴한데 역시 튀김은 못따라와요",
          "original_form": "오징어류도 맛있긴한데 역시 튀김은 못따라와요",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043458.1.11",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043458.1.12",
          "form": "저녁이라 맥주마시고 싶네요..",
          "original_form": "저녁이라 맥주마시고 싶네요..",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043458.1.13",
          "form": "시원한 맥주..",
          "original_form": "시원한 맥주..",
