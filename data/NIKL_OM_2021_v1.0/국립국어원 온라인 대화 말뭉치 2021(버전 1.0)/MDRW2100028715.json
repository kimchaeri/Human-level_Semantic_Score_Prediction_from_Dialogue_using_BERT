      "utterance": [
        {
          "id": "MDRW2100028715.1.1",
          "form": "냐낭 님은 코로나전에 마지막으로 갔던 여행지가 어디에요?",
          "original_form": "냐낭 님은 코로나전에 마지막으로 갔던 여행지가 어디에요?",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028715.1.2",
          "form": "안녕하세요~~!",
          "original_form": "안녕하세요~~!",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028715.1.3",
          "form": "저는 일본이요! 샛별님은오ㅡ?",
          "original_form": "저는 일본이요! 샛별님은오ㅡ?",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028715.1.4",
          "form": "저는 코로나 터지기 직전에 미국 캐나다 다녀온게 마지막이에요ㅠ",
          "original_form": "저는 코로나 터지기 직전에 미국 캐나다 다녀온게 마지막이에요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028715.1.5",
          "form": "처음가보는 곳인데 그때 가길 잘했다는 생각이 들어요",
          "original_form": "처음가보는 곳인데 그때 가길 잘했다는 생각이 들어요",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028715.1.6",
          "form": "오..제친구도 미국 캐나다갔었는데",
          "original_form": "오..제친구도 미국 캐나다갔었는데",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.7",
          "form": "뉴~~~욕~~~ 거울을 기념품으로 선물받았어요!",
          "original_form": "뉴~~~욕~~~ 거울을 기념품으로 선물받았어요!",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.8",
          "form": "우와~~저도 관광지 기념품 선물 받고 싶네용",
          "original_form": "우와~~저도 관광지 기념품 선물 받고 싶네용",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.9",
          "form": "저도 보고싶은데 영어권 동네는",
          "original_form": "저도 보고싶은데 영어권 동네는",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.10",
          "form": "저는 여행가서 안좋은 기억이 있어요",
          "original_form": "저는 여행가서 안좋은 기억이 있어요",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.11",
          "form": "좀 두렵더라구요",
          "original_form": "좀 두렵더라구요",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.12",
          "form": "제가 동행해드릴게요",
          "original_form": "제가 동행해드릴게요",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028715.1.13",
          "form": "헉 무슨일 있으셨나요?",
          "original_form": "헉 무슨일 있으셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028715.1.14",
          "form": "꺅 설레",
          "original_form": "꺅 설레",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028715.1.15",
          "form": "유명한 관광명소에 갔는데 인형탈 쓴 사람들이 사진찍어주겠다고 하더라구요",
          "original_form": "유명한 관광명소에 갔는데 인형탈 쓴 사람들이 사진찍어주겠다고 하더라구요",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028715.1.16",
          "form": "넘 들떠서 네 사진 찍어주세요! 했죠",
          "original_form": "넘 들떠서 네 사진 찍어주세요! 했죠",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028715.1.17",
          "form": "그런데 말이죠...사진찍어줬으니 돈을 달라는겁니다ㅜㅜ",
          "original_form": "그런데 말이죠...사진찍어줬으니 돈을 달라는겁니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028715.1.18",
          "form": "헉ㅜ 넘해ㅠㅠ",
          "original_form": "헉ㅜ 넘해ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028715.1.19",
          "form": "마치 그거같네요 유럽에",
          "original_form": "마치 그거같네요 유럽에",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028715.1.20",
          "form": "꽃주더니 받았으니 꽃 값줘!",
          "original_form": "꽃주더니 받았으니 꽃 값줘!",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028715.1.21",
          "form": "이런 날강도같은 사람들땜에",
          "original_form": "이런 날강도같은 사람들땜에",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028715.1.22",
          "form": "진짜 그렇게까지해서 여행자들 피해줘야하는지...",
          "original_form": "진짜 그렇게까지해서 여행자들 피해줘야하는지...",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028715.1.23",
          "form": "그 여행지에 대한 감정이 부정적으로 되는거",
          "original_form": "그 여행지에 대한 감정이 부정적으로 되는거",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028715.1.24",
          "form": "같아요ㅠㅠㅠ",
          "original_form": "같아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028715.1.25",
          "form": "진짜 얼탱이 없었어요",
          "original_form": "진짜 얼탱이 없었어요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        }
      ]
    }
