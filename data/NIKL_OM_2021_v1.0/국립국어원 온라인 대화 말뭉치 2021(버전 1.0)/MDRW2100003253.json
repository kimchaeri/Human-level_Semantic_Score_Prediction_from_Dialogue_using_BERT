      "utterance": [
        {
          "id": "MDRW2100003253.1.1",
          "form": "요새 듣는 신곡 있으세요?",
          "original_form": "요새 듣는 신곡 있으세요?",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003253.1.2",
          "form": "요즘 나이먹으니 음악은 잘안듣게 되네요ㅎ 예전엔 음악방송 꼬박챙겨봤는데",
          "original_form": "요즘 나이먹으니 음악은 잘안듣게 되네요ㅎ 예전엔 음악방송 꼬박챙겨봤는데",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003253.1.3",
          "form": "좋아하는 노래있으세요?",
          "original_form": "좋아하는 노래있으세요?",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003253.1.4",
          "form": "앗 저두요 근데 요 며칠 음악방송 챙겨봤어요",
          "original_form": "앗 저두요 근데 요 며칠 음악방송 챙겨봤어요",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003253.1.5",
          "form": "나름 재밌던데요?",
          "original_form": "나름 재밌던데요?",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003253.1.6",
          "form": "저 요새는 레드벨벳 노래 들어요",
          "original_form": "저 요새는 레드벨벳 노래 들어요",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003253.1.7",
          "form": "신곡 나왔나요?",
          "original_form": "신곡 나왔나요?",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003253.1.8",
          "form": "네! 들어보세요 좋아요",
          "original_form": "네! 들어보세요 좋아요",
          "speaker_id": "2",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003253.1.9",
          "form": "그리고 요새 더보이즈 스릴라이드도 잘 들어요 딱 여름노래",
          "original_form": "그리고 요새 더보이즈 스릴라이드도 잘 들어요 딱 여름노래",
          "speaker_id": "2",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003253.1.10",
          "form": "제목이 뭐예요?",
          "original_form": "제목이 뭐예요?",
          "speaker_id": "1",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003253.1.11",
          "form": "레드벨벳 노래는 퀸덤이에요!",
          "original_form": "레드벨벳 노래는 퀸덤이에요!",
          "speaker_id": "2",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003253.1.12",
          "form": "더보이즈는 그룹인가요?ㅎ 늙었나봐요",
          "original_form": "더보이즈는 그룹인가요?ㅎ 늙었나봐요",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003253.1.13",
          "form": "첨들어보네요",
          "original_form": "첨들어보네요",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003253.1.14",
          "form": "네 저도 이번에 처음 봤어요ㅋㅋㅋ",
          "original_form": "네 저도 이번에 처음 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003253.1.15",
          "form": "음방 보다가 본건데 어우 음방도 뭐가 많더라구요",
          "original_form": "음방 보다가 본건데 어우 음방도 뭐가 많더라구요",
          "speaker_id": "2",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003253.1.16",
          "form": "워낙 그룹도 많아서 예능에나와야 대충 알아요",
          "original_form": "워낙 그룹도 많아서 예능에나와야 대충 알아요",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003253.1.17",
          "form": "아 맞아요 저도 문명특급 보면서 아이돌 배워요 거의",
          "original_form": "아 맞아요 저도 문명특급 보면서 아이돌 배워요 거의",
          "speaker_id": "2",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003253.1.18",
          "form": "놀토 이런거 봐야ㅋㅋㅋ",
          "original_form": "놀토 이런거 봐야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003253.1.19",
          "form": "놀토  요즘도 하나요?",
          "original_form": "놀토  요즘도 하나요?",
          "speaker_id": "1",
          "time": "20210820 21:56"
        },
        {
          "id": "MDRW2100003253.1.20",
          "form": "초반엔 재밌었는데",
          "original_form": "초반엔 재밌었는데",
          "speaker_id": "1",
          "time": "20210820 21:56"
        },
        {
          "id": "MDRW2100003253.1.21",
          "form": "네 요즘도 해요!",
          "original_form": "네 요즘도 해요!",
          "speaker_id": "2",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003253.1.22",
          "form": "키랑 한해 군대가기 전이 진짜 재밌었던거 같아요",
          "original_form": "키랑 한해 군대가기 전이 진짜 재밌었던거 같아요",
          "speaker_id": "2",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003253.1.23",
          "form": "저도요 원멤버가 잼났는데 태연오고 잼없더라구요",
          "original_form": "저도요 원멤버가 잼났는데 태연오고 잼없더라구요",
          "speaker_id": "1",
          "time": "20210820 21:59"
        }
      ]
    }
  ]
