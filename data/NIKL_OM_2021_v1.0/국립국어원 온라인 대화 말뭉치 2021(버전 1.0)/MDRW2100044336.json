      "utterance": [
        {
          "id": "MDRW2100044336.1.1",
          "form": "빈츠는 얼웨이즈 사랑이죠~",
          "original_form": "빈츠는 얼웨이즈 사랑이죠~",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044336.1.2",
          "form": "엄마손파이는 요?",
          "original_form": "엄마손파이는 요?",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044336.1.3",
          "form": "아 그건 제스탈 아니에요ㅠㅠ 흑",
          "original_form": "아 그건 제스탈 아니에요ㅠㅠ 흑",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044336.1.4",
          "form": "저 얼마전에 편의점갔다가",
          "original_form": "저 얼마전에 편의점갔다가",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044336.1.5",
          "form": "곱창맛과자 봤는데 알아요?",
          "original_form": "곱창맛과자 봤는데 알아요?",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044336.1.6",
          "form": "헐 엄마손파이 깔끔하고 맛잇지않나요?",
          "original_form": "헐 엄마손파이 깔끔하고 맛잇지않나요?",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044336.1.7",
          "form": "헐 곱창맛이요?!?!?!?!?!",
          "original_form": "헐 곱창맛이요?!?!?!?!?!",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044336.1.8",
          "form": "저는 근데 그 패이스츄리가 뭔가 제스탈이 아니에요ㅜㅜ",
          "original_form": "저는 근데 그 패이스츄리가 뭔가 제스탈이 아니에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044336.1.9",
          "form": "아항 그렇구나 원래 패스츄리 안좋아해요?",
          "original_form": "아항 그렇구나 원래 패스츄리 안좋아해요?",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044336.1.10",
          "form": "프렌치 파이는 좋아해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "프렌치 파이는 좋아해요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044336.1.11",
          "form": "프렌치.파이는 위에 잼 올라간거죠??",
          "original_form": "프렌치.파이는 위에 잼 올라간거죠??",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044336.1.12",
          "form": "네네!! 딸기맛 사과맛 !!! 둘다 맛나용",
          "original_form": "네네!! 딸기맛 사과맛 !!! 둘다 맛나용",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044336.1.13",
          "form": "저는 오히려 그게 넘 달아서 안먹게되용ㅋㅋㅋ",
          "original_form": "저는 오히려 그게 넘 달아서 안먹게되용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044336.1.14",
          "form": "잼 부분이 좀 달긴하져 ㅋㅋㅋㅋ",
          "original_form": "잼 부분이 좀 달긴하져 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044336.1.15",
          "form": "ㅋㅋㅋ빼빼로는요??",
          "original_form": "ㅋㅋㅋ빼빼로는요??",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044336.1.16",
          "form": "빼빼로는 거의 모든 맛 다 좋아해요",
          "original_form": "빼빼로는 거의 모든 맛 다 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044336.1.17",
          "form": "요즘 편의점 갈때마다 새로운맛이 있더라구요?",
          "original_form": "요즘 편의점 갈때마다 새로운맛이 있더라구요?",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044336.1.18",
          "form": "저번주에는 오래오 티라미수맛 먹어봤어요",
          "original_form": "저번주에는 오래오 티라미수맛 먹어봤어요",
