      "utterance": [
        {
          "id": "MDRW2100015297.1.1",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015297.1.2",
          "form": "쌀떡 파세요 밀떡 파세요",
          "original_form": "쌀떡 파세요 밀떡 파세요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015297.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015297.1.4",
          "form": "쌀떡인데요",
          "original_form": "쌀떡인데요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015297.1.5",
          "form": "아 저랑 안맞으시네요",
          "original_form": "아 저랑 안맞으시네요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015297.1.6",
          "form": "쌀떡의 매력포인트가 뭔가요?",
          "original_form": "쌀떡의 매력포인트가 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015297.1.7",
          "form": "쫠깃쫠깃",
          "original_form": "쫠깃쫠깃",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015297.1.8",
          "form": "밀떡은 말ㄹ(뚝)",
          "original_form": "밀떡은 말ㄹ(뚝)",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015297.1.9",
          "form": "뭔지 아시죠",
          "original_form": "뭔지 아시죠",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015297.1.10",
          "form": "밀떡이 더 쫄깃한데요",
          "original_form": "밀떡이 더 쫄깃한데요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015297.1.11",
          "form": "떡을 혹시 구분 못하시는 거 아닌가요.",
          "original_form": "떡을 혹시 구분 못하시는 거 아닌가요.",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015297.1.12",
          "form": "저기요",
          "original_form": "저기요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015297.1.13",
          "form": "떡 원데이 투데이 드세요?",
          "original_form": "떡 원데이 투데이 드세요?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015297.1.14",
          "form": "아니요?",
          "original_form": "아니요?",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015297.1.15",
          "form": "밀떡이 더 쫄깃한 것은 사실이잖아요",
          "original_form": "밀떡이 더 쫄깃한 것은 사실이잖아요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015297.1.16",
          "form": "근데 어떻게 밀떡이 더 쫄깃하다고 말하시나요",
          "original_form": "근데 어떻게 밀떡이 더 쫄깃하다고 말하시나요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015297.1.17",
          "form": "에 다른 분들한테 물어보세요",
          "original_form": "에 다른 분들한테 물어보세요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015297.1.18",
          "form": "쌀떡은 그 특유의 식감이 너무 별로예요.",
          "original_form": "쌀떡은 그 특유의 식감이 너무 별로예요.",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015297.1.19",
          "form": "물어봐도 같을걸요",
          "original_form": "물어봐도 같을걸요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015297.1.20",
          "form": "제일 좋아하는 떡볶이 프랜차이즈가 어디세요?",
          "original_form": "제일 좋아하는 떡볶이 프랜차이즈가 어디세요?",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015297.1.21",
          "form": "전 못 골라요",
          "original_form": "전 못 골라요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015297.1.22",
          "form": "기분에 따라 다르죠",
          "original_form": "기분에 따라 다르죠",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015297.1.23",
          "form": "그럼 제일 최근에 드신 곳은요?",
          "original_form": "그럼 제일 최근에 드신 곳은요?",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015297.1.24",
          "form": "움 어딜까요",
          "original_form": "움 어딜까요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015297.1.25",
          "form": "잘 안 시켜먹어서 모루갰어요",
          "original_form": "잘 안 시켜먹어서 모루갰어요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015297.1.26",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015297.1.27",
          "form": "제일 좋아하는 음식이 뭔가요?",
          "original_form": "제일 좋아하는 음식이 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 22:39"
        }
      ]
    }
  ]
