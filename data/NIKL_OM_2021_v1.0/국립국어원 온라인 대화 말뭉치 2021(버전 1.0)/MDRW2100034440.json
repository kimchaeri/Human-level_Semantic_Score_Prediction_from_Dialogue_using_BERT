      "utterance": [
        {
          "id": "MDRW2100034440.1.1",
          "form": "티비에 뭐하는데?",
          "original_form": "티비에 뭐하는데?",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100034440.1.2",
          "form": "재밌는거는 안한다",
          "original_form": "재밌는거는 안한다",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100034440.1.3",
          "form": "참 토일에도 볼게 없어",
          "original_form": "참 토일에도 볼게 없어",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100034440.1.4",
          "form": "엉 그래서 티비 살 필요가 없는거 같기도 한데",
          "original_form": "엉 그래서 티비 살 필요가 없는거 같기도 한데",
          "speaker_id": "2",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100034440.1.5",
          "form": "그래도 티비는 있어야 함ㅋ",
          "original_form": "그래도 티비는 있어야 함ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100034440.1.6",
          "form": "맞다 어쩌다 티비봐야하니",
          "original_form": "맞다 어쩌다 티비봐야하니",
          "speaker_id": "2",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100034440.1.7",
          "form": "그리고 티비에서 유튜브도 볼 수 있고 하니",
          "original_form": "그리고 티비에서 유튜브도 볼 수 있고 하니",
          "speaker_id": "1",
          "time": "20210828 11:09"
        },
        {
          "id": "MDRW2100034440.1.8",
          "form": "ㅋㅋ유튜브 좋아하네",
          "original_form": "ㅋㅋ유튜브 좋아하네",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100034440.1.9",
          "form": "티비 볼 게 없으니 자연스레 유튜브로ㅋ",
          "original_form": "티비 볼 게 없으니 자연스레 유튜브로ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100034440.1.10",
          "form": "그래 난 인터넷소설 볼게 없으면 낮잠 자듯이  ㅋㅋ 유튜브 보는거지",
          "original_form": "그래 난 인터넷소설 볼게 없으면 낮잠 자듯이  ㅋㅋ 유튜브 보는거지",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034440.1.11",
          "form": "난 인터넷 소설이나 웹툰은 못 보겠드라",
          "original_form": "난 인터넷 소설이나 웹툰은 못 보겠드라",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100034440.1.12",
          "form": "첨에 그런데 계속 보면 괜찮다...일기예보에 오늘 부산 30도까지 올라가네",
          "original_form": "첨에 그런데 계속 보면 괜찮다...일기예보에 오늘 부산 30도까지 올라가네",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100034440.1.13",
          "form": "비도 안오고 오늘은 좋네",
          "original_form": "비도 안오고 오늘은 좋네",
          "speaker_id": "1",
          "time": "20210828 11:13"
