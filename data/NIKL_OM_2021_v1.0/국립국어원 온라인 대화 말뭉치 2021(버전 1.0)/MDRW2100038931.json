      "utterance": [
        {
          "id": "MDRW2100038931.1.1",
          "form": "커피 많이 드시나요?",
          "original_form": "커피 많이 드시나요?",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038931.1.2",
          "form": "조아해용",
          "original_form": "조아해용",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038931.1.3",
          "form": "많인 못머금 ㅠㅠ",
          "original_form": "많인 못머금 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038931.1.4",
          "form": "카페인 알러지 있으신가요??ㅠㅠㅠ",
          "original_form": "카페인 알러지 있으신가요??ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038931.1.5",
          "form": "선호하는 브랜드 있으세요?:)",
          "original_form": "선호하는 브랜드 있으세요?:)",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038931.1.6",
          "form": "아뇨...알러지없느데요 갑자기 잘 안받음",
          "original_form": "아뇨...알러지없느데요 갑자기 잘 안받음",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038931.1.7",
          "form": "넘많이마셔서그런가",
          "original_form": "넘많이마셔서그런가",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038931.1.8",
          "form": "으 브랜드는 딱히요 산미있느거조아하는데 ㅠ 잘없어요",
          "original_form": "으 브랜드는 딱히요 산미있느거조아하는데 ㅠ 잘없어요",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.9",
          "form": "아 커피가 위장에 안좋대요",
          "original_form": "아 커피가 위장에 안좋대요",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.10",
          "form": "직접 내려드세요?",
          "original_form": "직접 내려드세요?",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.11",
          "form": "아뇽ㅋㅋ",
          "original_form": "아뇽ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.12",
          "form": "찾아머거요",
          "original_form": "찾아머거요",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.13",
          "form": "카페인 마니먹어도 잘잣는데",
          "original_form": "카페인 마니먹어도 잘잣는데",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.14",
          "form": "이제 손떨려요",
          "original_form": "이제 손떨려요",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038931.1.15",
          "form": "아ㅠㅠㅠ 슬프네요ㅠㅠ",
          "original_form": "아ㅠㅠㅠ 슬프네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100038931.1.16",
          "form": "차는 안드세요??",
          "original_form": "차는 안드세요??",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100038931.1.17",
          "form": "차도 조아해용",
          "original_form": "차도 조아해용",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100038931.1.18",
          "form": "ㅋㅋㅋ 근데 카페 넘 비싸요",
          "original_form": "ㅋㅋㅋ 근데 카페 넘 비싸요",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038931.1.19",
          "form": "집에서 가끔머거요",
          "original_form": "집에서 가끔머거요",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038931.1.20",
          "form": "아 맞아요 차는 진짜 티백이 육천원 이래서",
          "original_form": "아 맞아요 차는 진짜 티백이 육천원 이래서",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038931.1.21",
          "form": "선뜻 손이 안가요 ㅋㅋㅋ",
          "original_form": "선뜻 손이 안가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038931.1.22",
          "form": "홍차 선호하시나요?",
          "original_form": "홍차 선호하시나요?",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038931.1.23",
          "form": "가격을 아니까 ㅋㅋㅋ 커피는 귀찮기라도하죠",
          "original_form": "가격을 아니까 ㅋㅋㅋ 커피는 귀찮기라도하죠",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038931.1.24",
          "form": "네 주로 홍차..",
          "original_form": "네 주로 홍차..",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038931.1.25",
          "form": "녹차도좋구요",
          "original_form": "녹차도좋구요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100038931.1.26",
          "form": "맞아요 ㅋㅋㅋㅋ 커피는 원두 갈기가 귀찮은데",
          "original_form": "맞아요 ㅋㅋㅋㅋ 커피는 원두 갈기가 귀찮은데",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038931.1.27",
          "form": "차는 아니죠 ㅋㅋㅋ",
          "original_form": "차는 아니죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038931.1.28",
          "form": "그래서차는 집에서ㅋ ㅋㅋㅋㅋ",
          "original_form": "그래서차는 집에서ㅋ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038931.1.29",
          "form": "티푸드는 어떤 거 좋아하세요?",
          "original_form": "티푸드는 어떤 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100038931.1.30",
          "form": "티푸드...걍 간단한 비스킷같음거요 ㅋㅋㅋ 랑드샤?이런거",
          "original_form": "티푸드...걍 간단한 비스킷같음거요 ㅋㅋㅋ 랑드샤?이런거",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038931.1.31",
          "form": "스콘같은거나",
          "original_form": "스콘같은거나",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100038931.1.32",
          "form": "스콘 좋네요 ㅋㅋㅋㅋ 스콘 맛집 가보고싶어요 ㅋㅋㅋㅋ",
          "original_form": "스콘 좋네요 ㅋㅋㅋㅋ 스콘 맛집 가보고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038931.1.33",
          "form": "지금까지 먹은 스콘 중 가장 맛있는게 스벅 거더라구요ㅠㅠ",
          "original_form": "지금까지 먹은 스콘 중 가장 맛있는게 스벅 거더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100038931.1.34",
          "form": "스콘 맛집.ㅠㅠ 저둥 ㅜ 전 만들어머금 가끔",
          "original_form": "스콘 맛집.ㅠㅠ 저둥 ㅜ 전 만들어머금 가끔",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038931.1.35",
          "form": "와 능력자시네요!!",
          "original_form": "와 능력자시네요!!",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038931.1.36",
          "form": "스콘 어렵지 않아요??",
          "original_form": "스콘 어렵지 않아요??",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038931.1.37",
          "form": "젤쉬워요",
          "original_form": "젤쉬워요",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038931.1.38",
          "form": "ㅋㅋㅋㅋㅋ 젤젤쉬움ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 젤젤쉬움ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038931.1.39",
          "form": "헐 진짜요????",
          "original_form": "헐 진짜요????",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038931.1.40",
          "form": "보기만 봐서는 어려워 보이던데요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "보기만 봐서는 어려워 보이던데요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038931.1.41",
          "form": "일단 버터가 많이 들어가면 맛있을 거 같긴 하더라구요",
          "original_form": "일단 버터가 많이 들어가면 맛있을 거 같긴 하더라구요",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038931.1.42",
          "form": "쉬워용 집에서  버터 조은거쓰니가 맛잇어요 ㅋㅋㅋ",
          "original_form": "쉬워용 집에서  버터 조은거쓰니가 맛잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038931.1.43",
          "form": "우와 역시 버터가 핵심이군요 ㅋㅋㅋㅋ",
          "original_form": "우와 역시 버터가 핵심이군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038931.1.44",
          "form": "딸기잼 곁들이시나요??",
          "original_form": "딸기잼 곁들이시나요??",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038931.1.45",
          "form": "넹 파는거는 버터말고 마가린 마니쓴대요",
          "original_form": "넹 파는거는 버터말고 마가린 마니쓴대요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038931.1.46",
          "form": "아뇨 걍 먹어요 차랑 ㅋㅋㅋ",
          "original_form": "아뇨 걍 먹어요 차랑 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038931.1.47",
          "form": "앗 파는 거 좀 실망이예요 어쩐지 맛이 없더라니...",
          "original_form": "앗 파는 거 좀 실망이예요 어쩐지 맛이 없더라니...",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100038931.1.48",
          "form": "스콘에 과일같은 재료는 추가하시는 편인가요???",
          "original_form": "스콘에 과일같은 재료는 추가하시는 편인가요???",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100038931.1.49",
          "form": "ㅋㅋㅋ 아뇨!아 얼그레이 넣어서 얼그레이 스콘 만들어용",
          "original_form": "ㅋㅋㅋ 아뇨!아 얼그레이 넣어서 얼그레이 스콘 만들어용",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038931.1.50",
          "form": "그리고 얼그레이차랑 같이먹어요",
          "original_form": "그리고 얼그레이차랑 같이먹어요",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038931.1.51",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038931.1.52",
          "form": "진짜 향긋하겠네요",
          "original_form": "진짜 향긋하겠네요",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100038931.1.53",
          "form": "넹...기회되시면 시도해보세용",
          "original_form": "넹...기회되시면 시도해보세용",
