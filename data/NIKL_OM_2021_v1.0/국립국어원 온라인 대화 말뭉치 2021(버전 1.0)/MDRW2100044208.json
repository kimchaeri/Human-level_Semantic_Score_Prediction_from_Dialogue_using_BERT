      "utterance": [
        {
          "id": "MDRW2100044208.1.1",
          "form": "난 매운것들 들어간 거 좋아해",
          "original_form": "난 매운것들 들어간 거 좋아해",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044208.1.2",
          "form": "고추장불고기같은게 제일 좋아",
          "original_form": "고추장불고기같은게 제일 좋아",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044208.1.3",
          "form": "근데 재료좀 더 듬뿍 넣어줬으면 좋겠어",
          "original_form": "근데 재료좀 더 듬뿍 넣어줬으면 좋겠어",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044208.1.4",
          "form": "맞아 너무",
          "original_form": "맞아 너무",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044208.1.5",
          "form": "가성비없어",
          "original_form": "가성비없어",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044208.1.6",
          "form": "그래서 난 비벼진 김밥사",
          "original_form": "그래서 난 비벼진 김밥사",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044208.1.7",
          "form": "참치마요비빔맛인가",
          "original_form": "참치마요비빔맛인가",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044208.1.8",
          "form": "참치마요도 맛있지 크악",
          "original_form": "참치마요도 맛있지 크악",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044208.1.9",
          "form": "난 삼김보다는 일반 원통령 김밥을",
          "original_form": "난 삼김보다는 일반 원통령 김밥을",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044208.1.10",
          "form": "맞아 그거랑 춘천비빔밥",
          "original_form": "맞아 그거랑 춘천비빔밥",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044208.1.11",
          "form": "원통형",
          "original_form": "원통형",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044208.1.12",
          "form": "사는 것 같어",
          "original_form": "사는 것 같어",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044208.1.13",
          "form": "진짜????!",
          "original_form": "진짜????!",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044208.1.14",
          "form": "춘천 비빔밥은 한 번도 안 먹어봤어",
          "original_form": "춘천 비빔밥은 한 번도 안 먹어봤어",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044208.1.15",
          "form": "난거기 들어간 단무지헤이터라",
          "original_form": "난거기 들어간 단무지헤이터라",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044208.1.16",
          "form": "이럴수가 단무지 안 먹는구나",
          "original_form": "이럴수가 단무지 안 먹는구나",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044208.1.17",
          "form": "난 옛날에는 다 빼고 먹었는데 요즘은 먹어",
          "original_form": "난 옛날에는 다 빼고 먹었는데 요즘은 먹어",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044208.1.18",
          "form": "춘천비빔밥은 어떻게 생긴거야?",
          "original_form": "춘천비빔밥은 어떻게 생긴거야?",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044208.1.19",
          "form": "그거 다 제거해서 먹어도 김밥자체에 단무지 향이 베여서 싫더라",
          "original_form": "그거 다 제거해서 먹어도 김밥자체에 단무지 향이 베여서 싫더라",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044208.1.20",
          "form": "고추장비빔밥처럼",
          "original_form": "고추장비빔밥처럼",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044208.1.21",
          "form": "밥이 고추장에 비벼져있고",
          "original_form": "밥이 고추장에 비벼져있고",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044208.1.22",
          "form": "중간에 소불고기 소가 들어가있어",
          "original_form": "중간에 소불고기 소가 들어가있어",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044208.1.23",
          "form": "ㅋㅋㅋㅋㅌ아 맞아 단무지가 밥에 배서",
          "original_form": "ㅋㅋㅋㅋㅌ아 맞아 단무지가 밥에 배서",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044208.1.24",
          "form": "향도 배고 누렇게 되어있잖아",
          "original_form": "향도 배고 누렇게 되어있잖아",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044208.1.25",
          "form": "호불호가 피클만큼 많이 갈리나봐",
          "original_form": "호불호가 피클만큼 많이 갈리나봐",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044208.1.26",
          "form": "그건 컵밥형식으로 나오는 거야?",
          "original_form": "그건 컵밥형식으로 나오는 거야?",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044208.1.27",
          "form": "피클도 극악의 난이도지",
          "original_form": "피클도 극악의 난이도지",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.28",
          "form": "아니 삼각김밥",
          "original_form": "아니 삼각김밥",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.29",
          "form": "ㅋㅋㄱㅋㄱㅋㅋㄲㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㅋㄲㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.30",
          "form": "아! 밥이 흰 색이 아니라",
          "original_form": "아! 밥이 흰 색이 아니라",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.31",
          "form": "학교앞에 김참치계라는 밥버거있었는데",
          "original_form": "학교앞에 김참치계라는 밥버거있었는데",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.32",
          "form": "섞인 밥이라는거구나",
          "original_form": "섞인 밥이라는거구나",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.33",
          "form": "삼김이",
          "original_form": "삼김이",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.34",
          "form": "어 맞아 양념된 밥이야",
          "original_form": "어 맞아 양념된 밥이야",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044208.1.35",
          "form": "응앙",
          "original_form": "응앙",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044208.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ응앙이래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ응앙이래",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044208.1.37",
          "form": "응가인가",
          "original_form": "응가인가",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044208.1.38",
          "form": "김참치계 ㅇㅇ",
          "original_form": "김참치계 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044208.1.39",
          "form": "김치참치치즈계란이 들어간 밥버거라",
          "original_form": "김치참치치즈계란이 들어간 밥버거라",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044208.1.40",
          "form": "아직도 가끔 기억이나",
          "original_form": "아직도 가끔 기억이나",
