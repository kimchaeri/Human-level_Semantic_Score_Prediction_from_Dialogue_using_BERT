      "utterance": [
        {
          "id": "MDRW2100002768.1.1",
          "form": "뮤지컬이나 공연 좋아하시나요?",
          "original_form": "뮤지컬이나 공연 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002768.1.2",
          "form": "좋아하는데 몇번 안가봤어요.",
          "original_form": "좋아하는데 몇번 안가봤어요.",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002768.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002768.1.4",
          "form": "네 저도 좋아해요",
          "original_form": "네 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002768.1.5",
          "form": "공연은 많이 가봤는데",
          "original_form": "공연은 많이 가봤는데",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002768.1.6",
          "form": "뮤지컬은 못 가봤네요",
          "original_form": "뮤지컬은 못 가봤네요",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002768.1.7",
          "form": "최근에 어떤 공연 보셨야요?!",
          "original_form": "최근에 어떤 공연 보셨야요?!",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002768.1.8",
          "form": "저는 몇달전에 오페라의유령 예매했는데 코로나때문에 취소되었어요ㅠ",
          "original_form": "저는 몇달전에 오페라의유령 예매했는데 코로나때문에 취소되었어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002768.1.9",
          "form": "최근에는 아쉽게도 본 것이 없어요",
          "original_form": "최근에는 아쉽게도 본 것이 없어요",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002768.1.10",
          "form": "우와 오페라의유령 재밌나요?",
          "original_form": "우와 오페라의유령 재밌나요?",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002768.1.11",
          "form": "내한공연이라서 잔뜩 기대했어요!!",
          "original_form": "내한공연이라서 잔뜩 기대했어요!!",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002768.1.12",
          "form": "그 노래를 정말 좋아해서요",
          "original_form": "그 노래를 정말 좋아해서요",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002768.1.13",
          "form": "다음에 꼭 갈거예요ㅜㅜ",
          "original_form": "다음에 꼭 갈거예요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002768.1.14",
          "form": "기억에 나는 공연은 뭐였어요?",
          "original_form": "기억에 나는 공연은 뭐였어요?",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002768.1.15",
          "form": "저도 한번 가보고 싶네요",
          "original_form": "저도 한번 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.16",
          "form": "전 콘서트를 자주 가봐서 그냥 콘서트들이 다 기억에 남아요",
          "original_form": "전 콘서트를 자주 가봐서 그냥 콘서트들이 다 기억에 남아요",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.17",
          "form": "콘서트도 가보셨나요?",
          "original_form": "콘서트도 가보셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.18",
          "form": "다음 내한공연 때.. 꼭 추천드려요",
          "original_form": "다음 내한공연 때.. 꼭 추천드려요",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.20",
          "form": "콘서트는 몇 번 안가봤어요",
          "original_form": "콘서트는 몇 번 안가봤어요",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.21",
          "form": "멀리서 공연장을 봤고, 줄이 길어서 입장에 힘들었던 기억이 있어요",
          "original_form": "멀리서 공연장을 봤고, 줄이 길어서 입장에 힘들었던 기억이 있어요",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002768.1.22",
          "form": "어떤 콘서트 가셨어요?",
          "original_form": "어떤 콘서트 가셨어요?",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002768.1.23",
          "form": "맞아요 콘서트에 사람이 많으니까 힘들죠",
          "original_form": "맞아요 콘서트에 사람이 많으니까 힘들죠",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002768.1.24",
          "form": "저는 아이돌 콘서트 가봤어요",
          "original_form": "저는 아이돌 콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002768.1.25",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002768.1.26",
          "form": "우오ㅏ!!!",
          "original_form": "우오ㅏ!!!",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002768.1.27",
          "form": "티켓팅 성공하셨네요",
          "original_form": "티켓팅 성공하셨네요",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002768.1.28",
          "form": "콘서트장 앞에서 파는 엠디들도 구입하셨나요?",
          "original_form": "콘서트장 앞에서 파는 엠디들도 구입하셨나요?",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002768.1.29",
          "form": "오 네 맞아요",
          "original_form": "오 네 맞아요",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002768.1.30",
          "form": "필수라고 생각합니댜",
          "original_form": "필수라고 생각합니댜",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002768.1.31",
          "form": "근데 비싸요 하하",
          "original_form": "근데 비싸요 하하",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002768.1.32",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002768.1.33",
          "form": "사서 모셔둬야해요...",
          "original_form": "사서 모셔둬야해요...",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002768.1.34",
          "form": "빨리 코로나가 끝나서 공연이 많아졌으면 좋겠네요ㅠ",
          "original_form": "빨리 코로나가 끝나서 공연이 많아졌으면 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002768.1.35",
          "form": "맞아요 그날만큼을 기다리고 있어요",
          "original_form": "맞아요 그날만큼을 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210820 15:34"
        }
      ]
