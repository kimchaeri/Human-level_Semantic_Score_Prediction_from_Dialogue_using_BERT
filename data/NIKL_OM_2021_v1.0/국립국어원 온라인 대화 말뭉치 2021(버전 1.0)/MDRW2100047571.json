      "utterance": [
        {
          "id": "MDRW2100047571.1.1",
          "form": "저는 해산물이요",
          "original_form": "저는 해산물이요",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047571.1.2",
          "form": "회 도 좋아해요",
          "original_form": "회 도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047571.1.3",
          "form": "저도 해산물 너무 좋아해요",
          "original_form": "저도 해산물 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047571.1.4",
          "form": "초밥도 좋아하세요?",
          "original_form": "초밥도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100047571.1.5",
          "form": "어우 없어서 못 먹죠",
          "original_form": "어우 없어서 못 먹죠",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047571.1.6",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047571.1.7",
          "form": "네 간단히 고구마먹었어요 자두랑 ㅋㅋㅋ",
          "original_form": "네 간단히 고구마먹었어요 자두랑 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100047571.1.8",
          "form": "저는 새우찜먹었어요",
          "original_form": "저는 새우찜먹었어요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100047571.1.9",
          "form": "너무 맛있었겠어요 ㅠㅠ",
          "original_form": "너무 맛있었겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100047571.1.10",
          "form": "친구가 백신을 맞아서 든든하게 먹었어요!",
          "original_form": "친구가 백신을 맞아서 든든하게 먹었어요!",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047571.1.11",
          "form": "맛있었어요!",
          "original_form": "맛있었어요!",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100047571.1.12",
          "form": "새우는 어떻게 해도 맛있는거같아요",
          "original_form": "새우는 어떻게 해도 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047571.1.13",
          "form": "그죠 간장새우도 좋고 생새우도 좋고...",
          "original_form": "그죠 간장새우도 좋고 생새우도 좋고...",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047571.1.14",
          "form": "간장새우 , 생새우 도 좋구요",
          "original_form": "간장새우 , 생새우 도 좋구요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047571.1.15",
          "form": "전 새우찜이 제일 맛있는거같아요 ㅋㅋㅋ",
          "original_form": "전 새우찜이 제일 맛있는거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047571.1.16",
          "form": "저두요! 든든 하기도 하구요",
          "original_form": "저두요! 든든 하기도 하구요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047571.1.17",
          "form": "맞아요 몸에도 좋고 맛도있어서 좋아요 ㅋㅋㅋ",
          "original_form": "맞아요 몸에도 좋고 맛도있어서 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100047571.1.18",
          "form": "소금구이 먹고싶네요",
          "original_form": "소금구이 먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 21:21"
        }
      ]
    }
