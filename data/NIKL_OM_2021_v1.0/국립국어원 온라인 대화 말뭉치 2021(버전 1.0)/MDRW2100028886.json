      "utterance": [
        {
          "id": "MDRW2100028886.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028886.1.2",
          "form": "안녕하세요! 쇼핑... 돈만 있으면 자주 하는 편이죠",
          "original_form": "안녕하세요! 쇼핑... 돈만 있으면 자주 하는 편이죠",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028886.1.3",
          "form": "ㅜㅜ 월급날 털어버리는게 낙이예요",
          "original_form": "ㅜㅜ 월급날 털어버리는게 낙이예요",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028886.1.4",
          "form": "뭘 제일 많이 사는 편이세요?",
          "original_form": "뭘 제일 많이 사는 편이세요?",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028886.1.5",
          "form": "전 굿즈도 많이 사는거 같아요",
          "original_form": "전 굿즈도 많이 사는거 같아요",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028886.1.6",
          "form": "저는 주로 화장품에 많이 투자해요 ㅠㅠ",
          "original_form": "저는 주로 화장품에 많이 투자해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028886.1.7",
          "form": "예민해서 맞는걸 찾을려고 하는중이에요 ㅎㅎ",
          "original_form": "예민해서 맞는걸 찾을려고 하는중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028886.1.8",
          "form": "굿즈 어떤거 사세요?",
          "original_form": "굿즈 어떤거 사세요?",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028886.1.9",
          "form": "저는 웹소설 굿즈 많이 사요",
          "original_form": "저는 웹소설 굿즈 많이 사요",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028886.1.10",
          "form": "전지적 독자 시점 이라고... 아세요...?",
          "original_form": "전지적 독자 시점 이라고... 아세요...?",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028886.1.11",
          "form": "앗 제가 소설을 안읽어서 ㅠㅜ",
          "original_form": "앗 제가 소설을 안읽어서 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028886.1.12",
          "form": "굿즈 다양하게 나오나요??",
          "original_form": "굿즈 다양하게 나오나요??",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028886.1.13",
          "form": "공식은 쫌 그런데 소비자들이 직접 만드는 굿즈는 잘 나와요",
          "original_form": "공식은 쫌 그런데 소비자들이 직접 만드는 굿즈는 잘 나와요",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028886.1.14",
          "form": "우아 소비자들이 만든는군요!! 파는 사이트가 따로있나요.?",
          "original_form": "우아 소비자들이 만든는군요!! 파는 사이트가 따로있나요.?",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028886.1.15",
          "form": "앗 아뇨 그냥 트위터에서 사는거에요",
          "original_form": "앗 아뇨 그냥 트위터에서 사는거에요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028886.1.16",
          "form": "우아 ! 저 예전에 아이돌 좋아햇을때 달력 샀던거 같아요 ㅋㅋ",
          "original_form": "우아 ! 저 예전에 아이돌 좋아햇을때 달력 샀던거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028886.1.17",
          "form": "제 소비의 80프로는 트위터인거 같아요ㅋㅋ",
          "original_form": "제 소비의 80프로는 트위터인거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028886.1.18",
          "form": "우아 ㅋㅋ 저도 한때 많이 삿어요",
          "original_form": "우아 ㅋㅋ 저도 한때 많이 삿어요",
