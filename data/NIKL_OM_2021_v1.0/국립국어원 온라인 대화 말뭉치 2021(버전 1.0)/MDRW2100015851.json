        {
          "id": "MDRW2100015851.1.1",
          "form": "집안일 좋아하시나요??ㅋㅋ",
          "original_form": "집안일 좋아하시나요??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015851.1.2",
          "form": "아니요!!",
          "original_form": "아니요!!",
          "speaker_id": "1",
          "time": "20210826 02:06"
        },
        {
          "id": "MDRW2100015851.1.3",
          "form": "느낌표 엄청 단호하시네요ㅋㅋㅋㅋ",
          "original_form": "느낌표 엄청 단호하시네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:06"
        },
        {
          "id": "MDRW2100015851.1.4",
          "form": "전 정리하고 청소하는거 좋아합니다ㅋㅋ",
          "original_form": "전 정리하고 청소하는거 좋아합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:07"
        },
        {
          "id": "MDRW2100015851.1.5",
          "form": "집안일중 어떤게 싫으세요?",
          "original_form": "집안일중 어떤게 싫으세요?",
          "speaker_id": "1",
          "time": "20210826 02:07"
        },
        {
          "id": "MDRW2100015851.1.6",
          "form": "음쓰버리기요ㅋㅋ",
          "original_form": "음쓰버리기요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:07"
        },
        {
          "id": "MDRW2100015851.1.7",
          "form": "다들 같을거 같아요ㅋㅋㅋ",
          "original_form": "다들 같을거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:08"
        },
        {
          "id": "MDRW2100015851.1.8",
          "form": "아 제일 시러요",
          "original_form": "아 제일 시러요",
          "speaker_id": "1",
          "time": "20210826 02:08"
        },
        {
          "id": "MDRW2100015851.1.9",
          "form": "ㅋㅋㅋ역시ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ역시ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:08"
        },
        {
          "id": "MDRW2100015851.1.10",
          "form": "일주일 방치해두면 구더기 생기더라고요",
          "original_form": "일주일 방치해두면 구더기 생기더라고요",
          "speaker_id": "1",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015851.1.11",
          "form": "뜨헉ㄷㄷㄷㄷㄷ",
          "original_form": "뜨헉ㄷㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015851.1.12",
          "form": "ㄷㄷㄷㄷㄷㄷㄷㄷㄷㄷ",
          "original_form": "ㄷㄷㄷㄷㄷㄷㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015851.1.13",
          "form": "너무싫어요ㅠ",
          "original_form": "너무싫어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015851.1.14",
          "form": "청소기 매일돌리시나요?",
          "original_form": "청소기 매일돌리시나요?",
