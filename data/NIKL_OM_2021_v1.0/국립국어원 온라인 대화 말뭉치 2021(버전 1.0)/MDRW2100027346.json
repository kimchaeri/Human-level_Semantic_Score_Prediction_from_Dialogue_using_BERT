      "utterance": [
        {
          "id": "MDRW2100027346.1.1",
          "form": "저는 알리오올리오 라면 이정도....하핫..",
          "original_form": "저는 알리오올리오 라면 이정도....하핫..",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027346.1.2",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027346.1.3",
          "form": "근데 앞에 보내주신걸 못봤어요 지워져서ㅠㅠ!!",
          "original_form": "근데 앞에 보내주신걸 못봤어요 지워져서ㅠㅠ!!",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027346.1.4",
          "form": "알리오올리오는 올리브오일에 볶나요",
          "original_form": "알리오올리오는 올리브오일에 볶나요",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027346.1.5",
          "form": "괜찮아요ㅎㅎ",
          "original_form": "괜찮아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027346.1.6",
          "form": "네네 올리브오일에 해요",
          "original_form": "네네 올리브오일에 해요",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027346.1.7",
          "form": "요리 어떤거 잘하세요?",
          "original_form": "요리 어떤거 잘하세요?",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027346.1.8",
          "form": "저는 크림소스 청정원꺼 사다가 볶아요",
          "original_form": "저는 크림소스 청정원꺼 사다가 볶아요",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027346.1.9",
          "form": "잘하진못해도 요리는 좋아해요",
          "original_form": "잘하진못해도 요리는 좋아해요",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027346.1.10",
          "form": "어젠 오리무쌈 먹었어요",
          "original_form": "어젠 오리무쌈 먹었어요",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.11",
          "form": "월남쌈처럼",
          "original_form": "월남쌈처럼",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.12",
          "form": "채소썰어서 싸서 먹어요",
          "original_form": "채소썰어서 싸서 먹어요",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.13",
          "form": "헐 세상에 침나와요",
          "original_form": "헐 세상에 침나와요",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.14",
          "form": "소스는 머스터드로 해요",
          "original_form": "소스는 머스터드로 해요",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.15",
          "form": "아 당연하죠!!!!",
          "original_form": "아 당연하죠!!!!",
          "speaker_id": "2",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.16",
          "form": "간단하고 맛있어요",
          "original_form": "간단하고 맛있어요",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027346.1.17",
          "form": "오리무쌈은 무조건 머스타드.",
          "original_form": "오리무쌈은 무조건 머스타드.",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027346.1.18",
          "form": "월남쌈은 땅콩소스나 전용칠리소스가 맛있는 것 같아요",
          "original_form": "월남쌈은 땅콩소스나 전용칠리소스가 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027346.1.19",
          "form": "네. 땅콩소스도 고소하죠",
          "original_form": "네. 땅콩소스도 고소하죠",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027346.1.20",
          "form": "사서먹기 힘드니 밀키트도 좋아요",
          "original_form": "사서먹기 힘드니 밀키트도 좋아요",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027346.1.21",
          "form": "점심 샐러드로 충분한가요?",
          "original_form": "점심 샐러드로 충분한가요?",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027346.1.22",
          "form": "아 오늘은 샐러드 먹기 싫은데 고민되네요..",
          "original_form": "아 오늘은 샐러드 먹기 싫은데 고민되네요..",
          "speaker_id": "2",
          "time": "20210902 11:44"
        }
      ]
