      "utterance": [
        {
          "id": "MDRW2100039643.1.1",
          "form": "코숏이에요 엄마가 길냥이였어요",
          "original_form": "코숏이에요 엄마가 길냥이였어요",
          "speaker_id": "2",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100039643.1.2",
          "form": "근데 하나는 엄마닮아서 터키쉬앙고라 장모처럼 생겼어요",
          "original_form": "근데 하나는 엄마닮아서 터키쉬앙고라 장모처럼 생겼어요",
          "speaker_id": "2",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039643.1.3",
          "form": "코숏은 제가 아는 고양이의 전형이네요",
          "original_form": "코숏은 제가 아는 고양이의 전형이네요",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100039643.1.4",
          "form": "맞아요 고등어에요 하나는 ㅎㅎ",
          "original_form": "맞아요 고등어에요 하나는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039643.1.5",
          "form": "고1요?",
          "original_form": "고1요?",
          "speaker_id": "1",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039643.1.6",
          "form": "앗 그게 아니고 ㅋㅋ 터키도 찾아봤는데요",
          "original_form": "앗 그게 아니고 ㅋㅋ 터키도 찾아봤는데요",
          "speaker_id": "1",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039643.1.7",
          "form": "저런 고양이는 처음 봤어요",
          "original_form": "저런 고양이는 처음 봤어요",
          "speaker_id": "1",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039643.1.8",
          "form": "ㅎㅎ 털이 길어요",
          "original_form": "ㅎㅎ 털이 길어요",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039643.1.9",
          "form": "털이 긴 고양이 처음 봤거든요~",
          "original_form": "털이 긴 고양이 처음 봤거든요~",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039643.1.10",
          "form": "털은 안빠지나요?",
          "original_form": "털은 안빠지나요?",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039643.1.11",
          "form": "고양이의 최대 단점이에요 털...ㅎㅎ",
          "original_form": "고양이의 최대 단점이에요 털...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039643.1.12",
          "form": "아.. 저 같는 비염인은 못키우겠군요 ㅠ",
          "original_form": "아.. 저 같는 비염인은 못키우겠군요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039643.1.13",
          "form": "그럴지두요 ㅜㅜ 냥 키우면 검은옷은 거의 못입어요",
          "original_form": "그럴지두요 ㅜㅜ 냥 키우면 검은옷은 거의 못입어요",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039643.1.14",
          "form": "검은 가구도 안되겠고요 ㅡ.ㅡ",
          "original_form": "검은 가구도 안되겠고요 ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039643.1.15",
          "form": "ㅎㅎ 털이 넘  잘보여요",
          "original_form": "ㅎㅎ 털이 넘  잘보여요",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039643.1.16",
          "form": "전 고양이도 보기만 할게요 ㅋㅋ",
          "original_form": "전 고양이도 보기만 할게요 ㅋㅋ",
