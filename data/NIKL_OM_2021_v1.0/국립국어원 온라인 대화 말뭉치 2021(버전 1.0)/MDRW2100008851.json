      "utterance": [
        {
          "id": "MDRW2100008851.1.1",
          "form": "요즘 건강이 트렌드로 잡은 것 같아요",
          "original_form": "요즘 건강이 트렌드로 잡은 것 같아요",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008851.1.2",
          "form": "아니면 제가 나이를 먹은 걸까요 ㅋㅋㅋ",
          "original_form": "아니면 제가 나이를 먹은 걸까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008851.1.3",
          "form": "저도 그래요ㅋㅋ 건강이 최우선",
          "original_form": "저도 그래요ㅋㅋ 건강이 최우선",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008851.1.4",
          "form": "코로나로 정말 쪄버려서^^",
          "original_form": "코로나로 정말 쪄버려서^^",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008851.1.5",
          "form": "이제는 살 빼야겠더라구요",
          "original_form": "이제는 살 빼야겠더라구요",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008851.1.6",
          "form": "ㅋㅋ안움직이니까 살이 고대로 쪄버렸어요ㅎㅎ",
          "original_form": "ㅋㅋ안움직이니까 살이 고대로 쪄버렸어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008851.1.7",
          "form": "왜 찌우는 건 쉬은데 빼는 건 힘들까요",
          "original_form": "왜 찌우는 건 쉬은데 빼는 건 힘들까요",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008851.1.8",
          "form": "제발이요...이제는 안먹어도 안빠지더라구요",
          "original_form": "제발이요...이제는 안먹어도 안빠지더라구요",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008851.1.9",
          "form": "진짜 그래요...",
          "original_form": "진짜 그래요...",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008851.1.10",
          "form": "적게 먹어도 안 빠지고 운동을 해야하나봐요 ㅎ",
          "original_form": "적게 먹어도 안 빠지고 운동을 해야하나봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008851.1.11",
          "form": "운동이 재밌는분들보면 부럽달까ㅋㅋ",
          "original_form": "운동이 재밌는분들보면 부럽달까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008851.1.12",
          "form": "운동하고 나면 개운한데... 하기까지가 참 힘들어요ㅠ",
          "original_form": "운동하고 나면 개운한데... 하기까지가 참 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008851.1.13",
          "form": "맞아요 어디서 보니까 헬스장가는게 제일 힘들다고ㅎㅎ",
          "original_form": "맞아요 어디서 보니까 헬스장가는게 제일 힘들다고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008851.1.14",
          "form": "엉덩이가 너무 무거워요ㅠ",
          "original_form": "엉덩이가 너무 무거워요ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008851.1.15",
          "form": "안 움직여요ㅠ",
          "original_form": "안 움직여요ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008851.1.16",
          "form": "ㅋㅋㅋ왜이렇게 운동보다 재밌는게 많은지...",
          "original_form": "ㅋㅋㅋ왜이렇게 운동보다 재밌는게 많은지...",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008851.1.17",
          "form": "운동은 왜 덜 재밌을까요 ㅠㅠ",
          "original_form": "운동은 왜 덜 재밌을까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008851.1.18",
          "form": "오래살고 싶은 생각은 없지만 아프고 싶지도 않은데 그럴려면 운동 해야겠죠?",
          "original_form": "오래살고 싶은 생각은 없지만 아프고 싶지도 않은데 그럴려면 운동 해야겠죠?",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008851.1.19",
          "form": "습관이 되야 한다던데...쉽지가 않네요ㅜ",
          "original_form": "습관이 되야 한다던데...쉽지가 않네요ㅜ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008851.1.20",
          "form": "3주만 하면 습관이 된다던데",
          "original_form": "3주만 하면 습관이 된다던데",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008851.1.21",
          "form": "3주가 넘 길어요 ㅋㅋㅋ",
          "original_form": "3주가 넘 길어요 ㅋㅋㅋ",
