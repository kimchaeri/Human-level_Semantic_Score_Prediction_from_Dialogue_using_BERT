      "utterance": [
        {
          "id": "MDRW2100024619.1.1",
          "form": "여행좋아하세요",
          "original_form": "여행좋아하세요",
          "speaker_id": "1",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100024619.1.2",
          "form": "네네 예전엔 진짜 자주 다녔어요",
          "original_form": "네네 예전엔 진짜 자주 다녔어요",
          "speaker_id": "2",
          "time": "20210831 15:52"
        },
        {
          "id": "MDRW2100024619.1.3",
          "form": "여행좋아하시나요???",
          "original_form": "여행좋아하시나요???",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024619.1.4",
          "form": "저도 여행좋아해요ㅜ코로나얼른끝났으면..",
          "original_form": "저도 여행좋아해요ㅜ코로나얼른끝났으면..",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024619.1.5",
          "form": "어디가 가장좋으셨어요",
          "original_form": "어디가 가장좋으셨어요",
          "speaker_id": "1",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024619.1.6",
          "form": "아 저는 제주도랑 대만이요!",
          "original_form": "아 저는 제주도랑 대만이요!",
          "speaker_id": "2",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100024619.1.7",
          "form": "오 둘다 저도갔었는데ㅜ",
          "original_form": "오 둘다 저도갔었는데ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100024619.1.8",
          "form": "대만어디가셨어요",
          "original_form": "대만어디가셨어요",
          "speaker_id": "1",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100024619.1.9",
          "form": "아 저 타이페이만 두 번 갔었어요 ㅠㅠ",
          "original_form": "아 저 타이페이만 두 번 갔었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024619.1.10",
          "form": "혹시 어디 가셨나요???",
          "original_form": "혹시 어디 가셨나요???",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024619.1.11",
          "form": "저도 타이페이만..",
          "original_form": "저도 타이페이만..",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024619.1.12",
          "form": "타이페이가실때 투어하셨나요?",
          "original_form": "타이페이가실때 투어하셨나요?",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024619.1.13",
          "form": "아 그 일일투어?만 갔었구요",
          "original_form": "아 그 일일투어?만 갔었구요",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024619.1.14",
          "form": "대만버스투어 혹시 아시나요???",
          "original_form": "대만버스투어 혹시 아시나요???",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100024619.1.15",
          "form": "시티투어요?",
          "original_form": "시티투어요?",
          "speaker_id": "1",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100024619.1.16",
          "form": "아니면 예스진지나 화련이나",
          "original_form": "아니면 예스진지나 화련이나",
          "speaker_id": "1",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100024619.1.17",
          "form": "전 예스진지랑 화련투어 돌았어요",
          "original_form": "전 예스진지랑 화련투어 돌았어요",
          "speaker_id": "1",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100024619.1.18",
          "form": "아 그 예스진지요!",
          "original_form": "아 그 예스진지요!",
          "speaker_id": "2",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100024619.1.19",
          "form": "화련이 그 기차타고 가는 데 맞나요???",
          "original_form": "화련이 그 기차타고 가는 데 맞나요???",
          "speaker_id": "2",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100024619.1.20",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100024619.1.21",
          "form": "기차타고 화련에 내리고 버스로 타고내려요 ㅋㅋㅋㅋ",
          "original_form": "기차타고 화련에 내리고 버스로 타고내려요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100024619.1.22",
          "form": "전 예스진지도 기차타고돌았어요 ㅋㅋ",
          "original_form": "전 예스진지도 기차타고돌았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100024619.1.23",
          "form": "아진짜요???? 예스진지 그럼 투어 없이 가신건가요?",
          "original_form": "아진짜요???? 예스진지 그럼 투어 없이 가신건가요?",
          "speaker_id": "2",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100024619.1.24",
          "form": "그... 무슨 드라마 촬영지가 그 기차라고 해서 가보고싶었는데",
          "original_form": "그... 무슨 드라마 촬영지가 그 기차라고 해서 가보고싶었는데",
          "speaker_id": "2",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100024619.1.25",
          "form": "네 걍 일일이 대중교통이용하면서 구경했어요 ㅋㅋㅋㅋ",
          "original_form": "네 걍 일일이 대중교통이용하면서 구경했어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100024619.1.26",
          "form": "전철잘되어있드라구여",
          "original_form": "전철잘되어있드라구여",
          "speaker_id": "1",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100024619.1.27",
          "form": "그래서 힘들었나...갔다와서 몸살이...",
          "original_form": "그래서 힘들었나...갔다와서 몸살이...",
          "speaker_id": "1",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100024619.1.28",
          "form": "와 진짜 재밌었겠어요!",
          "original_form": "와 진짜 재밌었겠어요!",
          "speaker_id": "2",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100024619.1.29",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 16:00"
        }
      ]
    }
