      "utterance": [
        {
          "id": "MDRW2100005608.1.1",
          "form": "악ㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005608.1.2",
          "form": "유럽 좋긴 했는데",
          "original_form": "유럽 좋긴 했는데",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005608.1.3",
          "form": "거기도 음식은 안 맞더라고요",
          "original_form": "거기도 음식은 안 맞더라고요",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005608.1.4",
          "form": "일주일마다 3kg 빠졌나ㅠㅋㅋㅋㅋㅋ",
          "original_form": "일주일마다 3kg 빠졌나ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005608.1.5",
          "form": "음식이 젤복병이었네요ㅜㅜ",
          "original_form": "음식이 젤복병이었네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005608.1.6",
          "form": "유럽에서 어디가 젤 좋았어요?",
          "original_form": "유럽에서 어디가 젤 좋았어요?",
          "speaker_id": "1",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005608.1.7",
          "form": "네ㅋㅋㅋㅋ 큐ㅠ 여행마다 음식 언제나 어디서나 복병이에ㅇ요 저는...",
          "original_form": "네ㅋㅋㅋㅋ 큐ㅠ 여행마다 음식 언제나 어디서나 복병이에ㅇ요 저는...",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005608.1.8",
          "form": "유럽... 서유럽은 일단 다닐 때마다",
          "original_form": "유럽... 서유럽은 일단 다닐 때마다",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005608.1.9",
          "form": "여기가 제일 좋아!! 하몀서 마음이 갈대처럼 바뀌었어요",
          "original_form": "여기가 제일 좋아!! 하몀서 마음이 갈대처럼 바뀌었어요",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005608.1.10",
          "form": "우와 언젠가는 꼭가보고싶네요",
          "original_form": "우와 언젠가는 꼭가보고싶네요",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.11",
          "form": "전 음식들 디저트류랑 빵너무 사랑해서",
          "original_form": "전 음식들 디저트류랑 빵너무 사랑해서",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.12",
          "form": "유럽디저트가 젤기대돼요ㅠ",
          "original_form": "유럽디저트가 젤기대돼요ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.13",
          "form": "그럼 서유럽 엄청 잘 맞으실 것 같아요",
          "original_form": "그럼 서유럽 엄청 잘 맞으실 것 같아요",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.14",
          "form": "젤라또 진짜 맛있습니다ㅠ",
          "original_form": "젤라또 진짜 맛있습니다ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.15",
          "form": "볼거리도 넘많겠지만",
          "original_form": "볼거리도 넘많겠지만",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.16",
          "form": "젤라또진짜 좋아하는데",
          "original_form": "젤라또진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.17",
          "form": "와플이랑ㅜ",
          "original_form": "와플이랑ㅜ",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005608.1.18",
          "form": "천국이겠네요",
          "original_form": "천국이겠네요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005608.1.19",
          "form": "와플 ㅠㅠㅠㅠ을 못 먹어봤네요",
          "original_form": "와플 ㅠㅠㅠㅠ을 못 먹어봤네요",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005608.1.20",
          "form": "너무 한이 되는 게",
          "original_form": "너무 한이 되는 게",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005608.1.21",
          "form": "프랑스에서 딱히",
          "original_form": "프랑스에서 딱히",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005608.1.22",
          "form": "빵을 많이 못 먹었어요",
          "original_form": "빵을 많이 못 먹었어요",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005608.1.23",
          "form": "ㅜ돈모아놓고 파리올림픽때맞춰서 가고싶어요",
          "original_form": "ㅜ돈모아놓고 파리올림픽때맞춰서 가고싶어요",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005608.1.24",
          "form": "빵 그냥바게트도 맛있다던데",
          "original_form": "빵 그냥바게트도 맛있다던데",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005608.1.25",
          "form": "와.... 너무 좋네요",
          "original_form": "와.... 너무 좋네요",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005608.1.26",
          "form": "저도 파리올림픽 때 갈까요... 너무 좋은 생각이에요",
          "original_form": "저도 파리올림픽 때 갈까요... 너무 좋은 생각이에요",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005608.1.27",
          "form": "올림픽이나 좋아하는 가수 콘서트 있을 때 여행 가믄 거 완전 이상이거든요ㅠ",
          "original_form": "올림픽이나 좋아하는 가수 콘서트 있을 때 여행 가믄 거 완전 이상이거든요ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005608.1.28",
          "form": "네 저두요ㅜ 가격물론 배로뛰겠지만 올림픽시즌 겹쳐서 한 두세달 있다오고싶어요  시간과여유가된다면요ㅜ",
          "original_form": "네 저두요ㅜ 가격물론 배로뛰겠지만 올림픽시즌 겹쳐서 한 두세달 있다오고싶어요  시간과여유가된다면요ㅜ",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005608.1.29",
          "form": "너무 낭만적이네요ㅜㅜㅜ",
          "original_form": "너무 낭만적이네요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005608.1.30",
          "form": "네 현실성은좀떨어지지만 생각만으로도 좋네요ㅜㅜㅜ",
          "original_form": "네 현실성은좀떨어지지만 생각만으로도 좋네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005608.1.31",
          "form": "올림픽은 생각도 못했고",
          "original_form": "올림픽은 생각도 못했고",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005608.1.32",
          "form": "저는 밴드 콜드플레이 좋아해서",
          "original_form": "저는 밴드 콜드플레이 좋아해서",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005608.1.33",
          "form": "그 밴드가 유럽에 콘서트 갈 때",
          "original_form": "그 밴드가 유럽에 콘서트 갈 때",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005608.1.34",
          "form": "콘서트랑 겸쳐서 여행 가는 걸",
          "original_form": "콘서트랑 겸쳐서 여행 가는 걸",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005608.1.35",
          "form": "어릴 때부터 꿈꿔왔어요",
          "original_form": "어릴 때부터 꿈꿔왔어요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005608.1.36",
          "form": "와 여행지에서 콘서트 진짜 감동일것같아요",
          "original_form": "와 여행지에서 콘서트 진짜 감동일것같아요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005608.1.37",
          "form": "콜드플레이ㅜ말해뭐합니까 직관하면 눈물좔좔",
          "original_form": "콜드플레이ㅜ말해뭐합니까 직관하면 눈물좔좔",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005608.1.38",
          "form": "한번 가보고싶네요ㅜㅜ",
          "original_form": "한번 가보고싶네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005608.1.39",
          "form": "그러니까요ㅠㅠㅠㅠ",
          "original_form": "그러니까요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:49"
        }
      ]
