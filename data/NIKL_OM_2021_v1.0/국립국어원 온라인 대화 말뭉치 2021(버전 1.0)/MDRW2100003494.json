      "utterance": [
        {
          "id": "MDRW2100003494.1.1",
          "form": "학교 생활 어때요?",
          "original_form": "학교 생활 어때요?",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003494.1.2",
          "form": "아.. 학생이신지 묻는게 먼저겠구나 학생인가요?",
          "original_form": "아.. 학생이신지 묻는게 먼저겠구나 학생인가요?",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003494.1.3",
          "form": "아뇨 학생은 아닌데",
          "original_form": "아뇨 학생은 아닌데",
          "speaker_id": "2",
          "time": "20210820 23:48"
        },
        {
          "id": "MDRW2100003494.1.4",
          "form": "수능준비중이에요",
          "original_form": "수능준비중이에요",
          "speaker_id": "2",
          "time": "20210820 23:48"
        },
        {
          "id": "MDRW2100003494.1.5",
          "form": "아 어디과를 준비하고있나요?",
          "original_form": "아 어디과를 준비하고있나요?",
          "speaker_id": "1",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003494.1.6",
          "form": "만화과 준비중이에요",
          "original_form": "만화과 준비중이에요",
          "speaker_id": "2",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003494.1.7",
          "form": "어디 과 나오셨어요?",
          "original_form": "어디 과 나오셨어요?",
          "speaker_id": "2",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003494.1.8",
          "form": "저는 조리쪽 전공했어요ㅎ",
          "original_form": "저는 조리쪽 전공했어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003494.1.9",
          "form": "만화과면 추후에 웹툰작가나 애니매이터 쪽을 꿈꾸시는건가요?",
          "original_form": "만화과면 추후에 웹툰작가나 애니매이터 쪽을 꿈꾸시는건가요?",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003494.1.10",
          "form": "스토리작가나..?",
          "original_form": "스토리작가나..?",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003494.1.11",
          "form": "웹툰작가 노리고 있어요",
          "original_form": "웹툰작가 노리고 있어요",
          "speaker_id": "2",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003494.1.12",
          "form": "조리쪽이면 직업도 그쪽이세요?",
          "original_form": "조리쪽이면 직업도 그쪽이세요?",
          "speaker_id": "2",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003494.1.13",
          "form": "직업은 아니구요 음 이상과 현실은 다르니까요ㅎ",
          "original_form": "직업은 아니구요 음 이상과 현실은 다르니까요ㅎ",
          "speaker_id": "1",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003494.1.14",
          "form": "웹툰을 그리게 된다면 어떤 스토리를 그리고 싶나요?",
          "original_form": "웹툰을 그리게 된다면 어떤 스토리를 그리고 싶나요?",
          "speaker_id": "1",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003494.1.15",
          "form": "스토리는 다 좋은데",
          "original_form": "스토리는 다 좋은데",
          "speaker_id": "2",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003494.1.16",
          "form": "풍경 이쁜거를 많이 그리고 싶네요",
          "original_form": "풍경 이쁜거를 많이 그리고 싶네요",
          "speaker_id": "2",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003494.1.17",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003494.1.18",
          "form": "혹시 참고하는 작가님이 계신가요",
          "original_form": "혹시 참고하는 작가님이 계신가요",
          "speaker_id": "1",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003494.1.19",
          "form": "너의이름은 보면 풍경 되게 예쁘게 나오더라구요",
          "original_form": "너의이름은 보면 풍경 되게 예쁘게 나오더라구요",
          "speaker_id": "2",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003494.1.20",
          "form": "그런 그림 그리고 싶네요",
          "original_form": "그런 그림 그리고 싶네요",
          "speaker_id": "2",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003494.1.21",
          "form": "너무 좋은 작품이에요",
          "original_form": "너무 좋은 작품이에요",
          "speaker_id": "1",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003494.1.22",
          "form": "준비하면서 어려운 점은 없나요?",
          "original_form": "준비하면서 어려운 점은 없나요?",
          "speaker_id": "1",
          "time": "20210821 11:58"
        },
        {
          "id": "MDRW2100003494.1.23",
          "form": "따로 그렇게까지 어려운점은 없더라구요",
          "original_form": "따로 그렇게까지 어려운점은 없더라구요",
          "speaker_id": "2",
          "time": "20210821 12:07"
        },
        {
          "id": "MDRW2100003494.1.24",
          "form": "앗 그러시군요",
