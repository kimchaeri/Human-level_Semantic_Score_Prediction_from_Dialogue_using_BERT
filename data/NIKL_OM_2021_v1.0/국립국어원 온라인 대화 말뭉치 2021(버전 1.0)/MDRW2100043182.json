      "utterance": [
        {
          "id": "MDRW2100043182.1.1",
          "form": "아이는 몇 살이에요? :)",
          "original_form": "아이는 몇 살이에요? :)",
          "speaker_id": "2",
          "time": "20210907 15:22"
        },
        {
          "id": "MDRW2100043182.1.2",
          "form": "둘째는 3살요 !",
          "original_form": "둘째는 3살요 !",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043182.1.3",
          "form": "큰애는 초딩 ㅎ",
          "original_form": "큰애는 초딩 ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100043182.1.4",
          "form": "우와 :) 아이가 둘이군용",
          "original_form": "우와 :) 아이가 둘이군용",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043182.1.5",
          "form": "성별은요?",
          "original_form": "성별은요?",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043182.1.6",
          "form": "딸둘이요! 하나는 외로울까봐 터울있어도 둘로..ㅎ",
          "original_form": "딸둘이요! 하나는 외로울까봐 터울있어도 둘로..ㅎ",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043182.1.7",
          "form": "오 좋네요:)",
          "original_form": "오 좋네요:)",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043182.1.8",
          "form": "저는 아들둘이에요^^^^",
          "original_form": "저는 아들둘이에요^^^^",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043182.1.9",
          "form": "와 ,... 힘드시져ㅠㅠ 화이팅해여ㅠㅠㅠㅠㅠ",
          "original_form": "와 ,... 힘드시져ㅠㅠ 화이팅해여ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043182.1.10",
          "form": "ㅋㅋ이 또한 지나가겠죠ㅠㅠㅋ",
          "original_form": "ㅋㅋ이 또한 지나가겠죠ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043182.1.11",
          "form": "둘째는 어린이집 보내고 계세요?",
          "original_form": "둘째는 어린이집 보내고 계세요?",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043182.1.12",
          "form": "아니여ㅠㅠㅠ",
          "original_form": "아니여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100043182.1.13",
          "form": "그럼 가정보육중이신거에요? ㅠ",
          "original_form": "그럼 가정보육중이신거에요? ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043182.1.14",
          "form": "아직 안보냈어여ㅠ 코로나때문에도 글코 뭔가 아직 준비가.?ㅎㅎ",
          "original_form": "아직 안보냈어여ㅠ 코로나때문에도 글코 뭔가 아직 준비가.?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043182.1.15",
          "form": "네 집앞에 대기걸어논데가 전화가 왔는데 국공립이었는데ㅠㅠ",
          "original_form": "네 집앞에 대기걸어논데가 전화가 왔는데 국공립이었는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043182.1.16",
          "form": "그쵸ㅠㅜ 코로나도 그렇구...",
          "original_form": "그쵸ㅠㅜ 코로나도 그렇구...",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043182.1.17",
          "form": "원래 대기가 되게 길었는데",
          "original_form": "원래 대기가 되게 길었는데",
          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043182.1.18",
          "form": "사건이 터져가지고ㅠㅠ 애들이 다 나가고 소문나서 대기건 사람들도 다취소해서 순번이 온거에요",
          "original_form": "사건이 터져가지고ㅠㅠ 애들이 다 나가고 소문나서 대기건 사람들도 다취소해서 순번이 온거에요",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.19",
          "form": "저도 단지내 국공립이\r\n1분거리도 안되서 보내고픈데\r\n대기때문에 꿈도 못꾸는데ㅠ",
          "original_form": "저도 단지내 국공립이\r\n1분거리도 안되서 보내고픈데\r\n대기때문에 꿈도 못꾸는데ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.20",
          "form": "근데 사람이 바꼈다해도 보내기가 싫어서 그거 놓치고 나니. ㅠ",
          "original_form": "근데 사람이 바꼈다해도 보내기가 싫어서 그거 놓치고 나니. ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.21",
          "form": "그니까여 대기가 원래 26번인가 그랬는데",
          "original_form": "그니까여 대기가 원래 26번인가 그랬는데",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.22",
          "form": "에구ㅠ 아깝긴 하네요ㅠ",
          "original_form": "에구ㅠ 아깝긴 하네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.23",
          "form": "선생이 애를 때려서는..ㅠㅠ",
          "original_form": "선생이 애를 때려서는..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.24",
          "form": "다 나갔어여",
          "original_form": "다 나갔어여",
          "speaker_id": "1",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.25",
          "form": "헉ㅜㅜ",
          "original_form": "헉ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043182.1.26",
          "form": "대기앞자리도 다 취소하고 26번이었는데 저한테 입학 전화가 왔더라구요",
