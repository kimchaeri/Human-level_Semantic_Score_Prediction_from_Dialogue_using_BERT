      "utterance": [
        {
          "id": "MDRW2100047123.1.1",
          "form": "어떤 종류의 일 하세요",
          "original_form": "어떤 종류의 일 하세요",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047123.1.2",
          "form": "현재 취업준비중이에요",
          "original_form": "현재 취업준비중이에요",
          "speaker_id": "1",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047123.1.3",
          "form": "취업준비로 고생하시네요",
          "original_form": "취업준비로 고생하시네요",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047123.1.4",
          "form": "열심히 구직활동해야죠.",
          "original_form": "열심히 구직활동해야죠.",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047123.1.5",
          "form": "어떤 업무 하시나요?",
          "original_form": "어떤 업무 하시나요?",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100047123.1.6",
          "form": "전 프리랜서라서 일감을 구해서 일하죠",
          "original_form": "전 프리랜서라서 일감을 구해서 일하죠",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100047123.1.7",
          "form": "블로그도 하고 머 말하자면 아무거나  다하고 있어요",
          "original_form": "블로그도 하고 머 말하자면 아무거나  다하고 있어요",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100047123.1.8",
          "form": "저도 영상쪽으로 프리랜서일은 하고있어요.",
          "original_form": "저도 영상쪽으로 프리랜서일은 하고있어요.",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100047123.1.9",
          "form": "영상분야 일하시군요",
          "original_form": "영상분야 일하시군요",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100047123.1.10",
          "form": "네 스펙이 될정도는 아니라 간식값 정도로만ㅎㅎ",
          "original_form": "네 스펙이 될정도는 아니라 간식값 정도로만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100047123.1.11",
          "form": "그럼 다른곳에 취업하실 생각은 있으신가요?",
          "original_form": "그럼 다른곳에 취업하실 생각은 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100047123.1.12",
          "form": "일단 코로나 좀 괜찮아지면 취업해야죠",
          "original_form": "일단 코로나 좀 괜찮아지면 취업해야죠",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100047123.1.13",
          "form": "코로나때문에 취업이 정말 힘든것같아요",
          "original_form": "코로나때문에 취업이 정말 힘든것같아요",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100047123.1.14",
          "form": "그렇죠 기업들도 경제가 안좋으니",
          "original_form": "그렇죠 기업들도 경제가 안좋으니",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100047123.1.15",
          "form": "취업난이 개선되길 바래야죠",
          "original_form": "취업난이 개선되길 바래야죠",
          "speaker_id": "1",
