      "utterance": [
        {
          "id": "MDRW2100024671.1.1",
          "form": "요즘~ 맛있게 먹은 음식 있었나요~?",
          "original_form": "요즘~ 맛있게 먹은 음식 있었나요~?",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024671.1.2",
          "form": "꽃게도 맛잇고 닭발도요ㅎ",
          "original_form": "꽃게도 맛잇고 닭발도요ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024671.1.3",
          "form": "대박 닭발 먹고싶었는데 저는 최근에 대창덮밥 먹었는데 맛있더라구요",
          "original_form": "대박 닭발 먹고싶었는데 저는 최근에 대창덮밥 먹었는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024671.1.4",
          "form": "오 그건 첨들어보네요ㅎ",
          "original_form": "오 그건 첨들어보네요ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024671.1.5",
          "form": "홍대개미인가 거기꺼인데 맛있더라고요 ㅎ 제가 대창 막창 이런거 좋아해서 그런가",
          "original_form": "홍대개미인가 거기꺼인데 맛있더라고요 ㅎ 제가 대창 막창 이런거 좋아해서 그런가",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024671.1.6",
          "form": "스테이크 덮밥도 맛있어요",
          "original_form": "스테이크 덮밥도 맛있어요",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100024671.1.7",
          "form": "오 덮밥전문인가봐요",
          "original_form": "오 덮밥전문인가봐요",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100024671.1.8",
          "form": "네 맞아요 ㅎ",
          "original_form": "네 맞아요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024671.1.9",
          "form": "파스타 좋아하세여?",
          "original_form": "파스타 좋아하세여?",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100024671.1.10",
          "form": "적당히 좋아해요ㅎ 비가오니 얼큰한게 땡기네요@",
          "original_form": "적당히 좋아해요ㅎ 비가오니 얼큰한게 땡기네요@",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024671.1.11",
          "form": "얼큰한거 좋네요",
          "original_form": "얼큰한거 좋네요",
          "speaker_id": "1",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024671.1.12",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024671.1.13",
          "form": "매운건 다좋아해요ㅎ",
          "original_form": "매운건 다좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024671.1.14",
          "form": "오 저 맵찔이라",
          "original_form": "오 저 맵찔이라",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024671.1.15",
          "form": "국밥 뼈해장국 요런거 좋아해요",
          "original_form": "국밥 뼈해장국 요런거 좋아해요",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024671.1.16",
          "form": "저도 국밥류는 다 좋아해오ㅡㅎ",
          "original_form": "저도 국밥류는 다 좋아해오ㅡㅎ",
          "speaker_id": "2",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024671.1.17",
          "form": "저는 내장국밥 제일 좋아해요 ㅎ",
          "original_form": "저는 내장국밥 제일 좋아해요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024671.1.18",
          "form": "아 먹고싶어지네요",
          "original_form": "아 먹고싶어지네요",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024671.1.19",
          "form": "저녁 뭐 드시나여?",
          "original_form": "저녁 뭐 드시나여?",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024671.1.20",
          "form": "라면먹으려구요ㅎ",
          "original_form": "라면먹으려구요ㅎ",
          "speaker_id": "2",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100024671.1.21",
          "form": "와우 맛있겠네요",
          "original_form": "와우 맛있겠네요",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024671.1.22",
          "form": "무슨라면이 제일 맛있으세요?",
          "original_form": "무슨라면이 제일 맛있으세요?",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024671.1.23",
          "form": "전 요새 틈새라면이 좋더라구요ㅎ",
          "original_form": "전 요새 틈새라면이 좋더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024671.1.24",
          "form": "오! 그거 맛있다던데",
          "original_form": "오! 그거 맛있다던데",
          "speaker_id": "1",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024671.1.25",
          "form": "저희집은 진라면만 먹어서",
          "original_form": "저희집은 진라면만 먹어서",
          "speaker_id": "1",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024671.1.26",
          "form": "기회가없네요 ㄷㄷ",
          "original_form": "기회가없네요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024671.1.27",
          "form": "저도 친구가 추천해줘서 먹어봣어요ㅎ",
          "original_form": "저도 친구가 추천해줘서 먹어봣어요ㅎ",
          "speaker_id": "2",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100024671.1.28",
          "form": "다음에 먹어봐야겠어요!!!!!!!",
          "original_form": "다음에 먹어봐야겠어요!!!!!!!",
