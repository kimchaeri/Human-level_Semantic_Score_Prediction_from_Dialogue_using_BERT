      "utterance": [
        {
          "id": "MDRW2100037676.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100037676.1.2",
          "form": "점심식사 하셨나요?",
          "original_form": "점심식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037676.1.3",
          "form": "네 쫄면이랑 만두 먹었습니다!",
          "original_form": "네 쫄면이랑 만두 먹었습니다!",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100037676.1.4",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100037676.1.5",
          "form": "네! 전 짬뽕 먹었습니다! ㅎㅎ",
          "original_form": "네! 전 짬뽕 먹었습니다! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100037676.1.6",
          "form": "쫄면에 비빔만두 먹고싶네요 ㅎㅎ",
          "original_form": "쫄면에 비빔만두 먹고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100037676.1.7",
          "form": "저녁에 짬뽕 먹어야겠네요 ㅎㅎ",
          "original_form": "저녁에 짬뽕 먹어야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037676.1.8",
          "form": "짬뽕국물만 테이크아웃 해주는 곳 있으면 좋겠어요 ㅎㅎ",
          "original_form": "짬뽕국물만 테이크아웃 해주는 곳 있으면 좋겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100037676.1.9",
          "form": "비와서 그런가 평소보다 더 맛있었어요 ㅋㅋㅋ",
          "original_form": "비와서 그런가 평소보다 더 맛있었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037676.1.10",
          "form": "비 오는 날은 유독 국물음식이 맛있는 것 같아요 ㅎㅎ\r\n저는 비 올때 우동 먹는 것도 좋아해요!",
          "original_form": "비 오는 날은 유독 국물음식이 맛있는 것 같아요 ㅎㅎ\r\n저는 비 올때 우동 먹는 것도 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100037676.1.11",
          "form": "날씨따라 찾는 음식 있나요?",
          "original_form": "날씨따라 찾는 음식 있나요?",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100037676.1.12",
          "form": "오 맞아요!! 비오는날 국물 요리가 최고죠 ㅎㅎㅎ",
          "original_form": "오 맞아요!! 비오는날 국물 요리가 최고죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:58"
        },
        {
          "id": "MDRW2100037676.1.13",
          "form": "오늘 같은 날 전골 먹으면 좋을거같아요 ㅋㅋㅋ",
          "original_form": "오늘 같은 날 전골 먹으면 좋을거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:59"
        },
        {
          "id": "MDRW2100037676.1.14",
          "form": "세상엔 왜 이렇게 맛있는 음식이 많은 걸까요ㅠㅠ 전골에 자작자작하게 밥 비벼 먹고싶어요ㅠㅠ",
          "original_form": "세상엔 왜 이렇게 맛있는 음식이 많은 걸까요ㅠㅠ 전골에 자작자작하게 밥 비벼 먹고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100037676.1.15",
          "form": "크 넘좋죠 ㅠㅠㅠㅠ 곱창전골 먹고싶네요...드시나요?!!",
          "original_form": "크 넘좋죠 ㅠㅠㅠㅠ 곱창전골 먹고싶네요...드시나요?!!",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100037676.1.16",
          "form": "그럼요!! 유독 곱창전골이 입에서 멤도는 날이 있지않나요 ㅎㅎ",
          "original_form": "그럼요!! 유독 곱창전골이 입에서 멤도는 날이 있지않나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100037676.1.17",
          "form": "맞아요!! 요며칠 엄청 땡기는데 아무래도 조만간 먹어야할 것 같아요 ㅋㅋㅋ",
          "original_form": "맞아요!! 요며칠 엄청 땡기는데 아무래도 조만간 먹어야할 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100037676.1.18",
          "form": "한번 생각나면 계속 생각나는데 이번주 안으로 드셔야겠어요 ㅋㅋㅋㅋ",
          "original_form": "한번 생각나면 계속 생각나는데 이번주 안으로 드셔야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:13"
        }
      ]
