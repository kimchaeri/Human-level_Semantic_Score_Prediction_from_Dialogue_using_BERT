      "utterance": [
        {
          "id": "MDRW2100026835.1.1",
          "form": "안녕하세요 취미가 어떻게 되세용?",
          "original_form": "안녕하세요 취미가 어떻게 되세용?",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026835.1.2",
          "form": "저는 영화랑 드라마 보기요",
          "original_form": "저는 영화랑 드라마 보기요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026835.1.3",
          "form": "요즘 동영상 스트리밍 서비스 많잖아요!",
          "original_form": "요즘 동영상 스트리밍 서비스 많잖아요!",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026835.1.4",
          "form": "아 넷플릭스나 왓챠 같은거요??",
          "original_form": "아 넷플릭스나 왓챠 같은거요??",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026835.1.5",
          "form": "저도 좋아해요!! 지금도 왓챠 보면서 폰하구 있네용ㅋㅋ",
          "original_form": "저도 좋아해요!! 지금도 왓챠 보면서 폰하구 있네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026835.1.6",
          "form": "그러시구나ㅋㅋㅋ 어떤 영화 보세요?",
          "original_form": "그러시구나ㅋㅋㅋ 어떤 영화 보세요?",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026835.1.7",
          "form": "다만 악에서 구하소서 라는 영화에요",
          "original_form": "다만 악에서 구하소서 라는 영화에요",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026835.1.8",
          "form": "황정민 나오는!",
          "original_form": "황정민 나오는!",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026835.1.9",
          "form": "아아 작년에 나온거죠?",
          "original_form": "아아 작년에 나온거죠?",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026835.1.10",
          "form": "저도 그 영화 봤네요 되게 안타깝더라구요 보면서",
          "original_form": "저도 그 영화 봤네요 되게 안타깝더라구요 보면서",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026835.1.11",
          "form": "네ㅠㅠ 이렇게 생각할 거리를 던져주는 영화가 개인적으로 좋네요",
          "original_form": "네ㅠㅠ 이렇게 생각할 거리를 던져주는 영화가 개인적으로 좋네요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100026835.1.12",
          "form": "동감입니다",
          "original_form": "동감입니다",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026835.1.13",
          "form": "전 유튜브도 자주 봐요",
          "original_form": "전 유튜브도 자주 봐요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026835.1.14",
          "form": "카페 브이로그에 푹 빠졌네요ㅋㅋㅋ",
          "original_form": "카페 브이로그에 푹 빠졌네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100026835.1.15",
          "form": "아 뭔지 알아요ㅋㅋㅋㅋㅋ 알바생들이 카페 음료수 만드는 영상이죠?",
          "original_form": "아 뭔지 알아요ㅋㅋㅋㅋㅋ 알바생들이 카페 음료수 만드는 영상이죠?",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026835.1.16",
          "form": "소소하게 힐링되서 저도 즐겨봅니다",
          "original_form": "소소하게 힐링되서 저도 즐겨봅니다",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026835.1.17",
          "form": "그쵸 신기하고 재밌어용ㅋㅋㅋ",
          "original_form": "그쵸 신기하고 재밌어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100026835.1.18",
          "form": "요즘은 정말 컨텐츠들이 다양해진거 같아요",
          "original_form": "요즘은 정말 컨텐츠들이 다양해진거 같아요",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026835.1.19",
          "form": "맞아요 예전엔 티비랑 게임 정도였는데",
          "original_form": "맞아요 예전엔 티비랑 게임 정도였는데",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026835.1.20",
          "form": "요즘엔 볼게 많아졌네요",
          "original_form": "요즘엔 볼게 많아졌네요",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100026835.1.21",
          "form": "맞아요 세상이 발달해서 그런가봐요ㅋㅋ",
          "original_form": "맞아요 세상이 발달해서 그런가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026835.1.22",
          "form": "저도 자기전에 예능이나 하나 보고 자야겠어요",
          "original_form": "저도 자기전에 예능이나 하나 보고 자야겠어요",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026835.1.23",
          "form": "어떤 예능 보시게요?",
          "original_form": "어떤 예능 보시게요?",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026835.1.24",
          "form": "놀면뭐하니요~!",
          "original_form": "놀면뭐하니요~!",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026835.1.25",
          "form": "유재석 나오는건데 완전 재밌더라고용",
          "original_form": "유재석 나오는건데 완전 재밌더라고용",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026835.1.26",
          "form": "아 저도 알아요ㅋㅋㅋㅋㅋ",
          "original_form": "아 저도 알아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026835.1.27",
          "form": "그럼 재밌게 보세용 보시고 후기도ㅎㅎ",
          "original_form": "그럼 재밌게 보세용 보시고 후기도ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:44"
        }
      ]
