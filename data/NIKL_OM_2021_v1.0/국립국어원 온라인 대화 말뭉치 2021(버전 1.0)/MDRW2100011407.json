      "utterance": [
        {
          "id": "MDRW2100011407.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "1",
          "time": "20210824 12:16"
        },
        {
          "id": "MDRW2100011407.1.2",
          "form": "애기욕조요?",
          "original_form": "애기욕조요?",
          "speaker_id": "2",
          "time": "20210824 12:20"
        },
        {
          "id": "MDRW2100011407.1.3",
          "form": "애기가 있으시군요!",
          "original_form": "애기가 있으시군요!",
          "speaker_id": "1",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011407.1.4",
          "form": "저는 운동화를 샀답니다",
          "original_form": "저는 운동화를 샀답니다",
          "speaker_id": "1",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100011407.1.5",
          "form": "어떤운동화요?",
          "original_form": "어떤운동화요?",
          "speaker_id": "2",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100011407.1.6",
          "form": "나이키요!",
          "original_form": "나이키요!",
          "speaker_id": "1",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011407.1.7",
          "form": "오 역시 나이키가 이쁘죠",
          "original_form": "오 역시 나이키가 이쁘죠",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100011407.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011407.1.9",
          "form": "오즈위고를 샀답니다",
          "original_form": "오즈위고를 샀답니다",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011407.1.10",
          "form": "그건 또 첨듣네요",
          "original_form": "그건 또 첨듣네요",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011407.1.11",
          "form": "ㅋㅋㅋ 운동화예요!",
          "original_form": "ㅋㅋㅋ 운동화예요!",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011407.1.12",
          "form": "뭐사고싶으신거있으세요?",
          "original_form": "뭐사고싶으신거있으세요?",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011407.1.13",
          "form": "전 요즘 가구요",
          "original_form": "전 요즘 가구요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011407.1.14",
          "form": "집정리사고싶어요",
          "original_form": "집정리사고싶어요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011407.1.15",
          "form": "어떤가구인가요ㅋㅋㅋ",
          "original_form": "어떤가구인가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011407.1.16",
          "form": "침대?",
          "original_form": "침대?",
          "speaker_id": "1",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011407.1.17",
          "form": "수납장?",
          "original_form": "수납장?",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011407.1.18",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011407.1.19",
          "form": "수납이 최고죠",
          "original_form": "수납이 최고죠",
          "speaker_id": "1",
          "time": "20210824 12:54"
        }
      ]
