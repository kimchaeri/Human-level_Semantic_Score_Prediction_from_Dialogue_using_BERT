      "utterance": [
        {
          "id": "MDRW2100001976.1.1",
          "form": "저녁식사 하셨나요??",
          "original_form": "저녁식사 하셨나요??",
          "speaker_id": "1",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001976.1.2",
          "form": "네 저녁은 무슨메뉴드셨나요?",
          "original_form": "네 저녁은 무슨메뉴드셨나요?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001976.1.3",
          "form": "막걸리에 김치전 먹었습니다!!",
          "original_form": "막걸리에 김치전 먹었습니다!!",
          "speaker_id": "1",
          "time": "20210820 07:29"
        },
        {
          "id": "MDRW2100001976.1.4",
          "form": "맛있는거 드셨네요",
          "original_form": "맛있는거 드셨네요",
          "speaker_id": "2",
          "time": "20210820 07:30"
        },
        {
          "id": "MDRW2100001976.1.5",
          "form": "저는 사무실에서 도시락 시켜먹었습니다",
          "original_form": "저는 사무실에서 도시락 시켜먹었습니다",
          "speaker_id": "2",
          "time": "20210820 07:30"
        },
        {
          "id": "MDRW2100001976.1.6",
          "form": "야근하셨나보네요 ㅠ 뭐하고계세요?",
          "original_form": "야근하셨나보네요 ㅠ 뭐하고계세요?",
          "speaker_id": "1",
          "time": "20210820 07:49"
        },
        {
          "id": "MDRW2100001976.1.7",
          "form": "출근중이에요  아침은 드셨나요?",
          "original_form": "출근중이에요  아침은 드셨나요?",
          "speaker_id": "2",
          "time": "20210820 07:55"
        },
        {
          "id": "MDRW2100001976.1.8",
          "form": "지금 먹는중이에요!",
          "original_form": "지금 먹는중이에요!",
          "speaker_id": "1",
          "time": "20210820 08:02"
        },
        {
          "id": "MDRW2100001976.1.9",
          "form": "된장찌개에 깻잎나물 ㅋㅋ",
          "original_form": "된장찌개에 깻잎나물 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:02"
        },
        {
          "id": "MDRW2100001976.1.10",
          "form": "오 ㅎㅎㅎ저는 간헐적단식해서 아침은 굶어요 ㅎㅎ",
          "original_form": "오 ㅎㅎㅎ저는 간헐적단식해서 아침은 굶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100001976.1.11",
          "form": "그럼 점심만 드시는거에요?",
          "original_form": "그럼 점심만 드시는거에요?",
          "speaker_id": "1",
          "time": "20210820 08:11"
        },
        {
          "id": "MDRW2100001976.1.12",
          "form": "네 특별한 일없으면 저녁6시부터 다음날점심까지 굶어요 ㅎㅎ",
          "original_form": "네 특별한 일없으면 저녁6시부터 다음날점심까지 굶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 08:14"
        },
        {
          "id": "MDRW2100001976.1.13",
          "form": "그렇게 어느정도 하셨어요?",
          "original_form": "그렇게 어느정도 하셨어요?",
          "speaker_id": "1",
          "time": "20210820 08:14"
        },
        {
          "id": "MDRW2100001976.1.14",
          "form": "지금 두달째하고있어요",
          "original_form": "지금 두달째하고있어요",
          "speaker_id": "2",
          "time": "20210820 08:30"
        },
        {
          "id": "MDRW2100001976.1.15",
          "form": "체중 많이 감량하셨어요?",
          "original_form": "체중 많이 감량하셨어요?",
          "speaker_id": "1",
          "time": "20210820 08:37"
        },
        {
          "id": "MDRW2100001976.1.16",
          "form": "한번에 확 빠지지는 않는데",
          "original_form": "한번에 확 빠지지는 않는데",
          "speaker_id": "2",
          "time": "20210820 08:45"
        },
        {
          "id": "MDRW2100001976.1.17",
          "form": "꾸준히 빠지고있어요  8키로정도 빠졌어요",
          "original_form": "꾸준히 빠지고있어요  8키로정도 빠졌어요",
          "speaker_id": "2",
          "time": "20210820 08:45"
        },
        {
          "id": "MDRW2100001976.1.18",
          "form": "와 두달만에 8키로면 엄청 많이 빼셨네요 ㅠ",
          "original_form": "와 두달만에 8키로면 엄청 많이 빼셨네요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 08:46"
        },
        {
          "id": "MDRW2100001976.1.19",
          "form": "전 10키로 빼는게 목표입니다 ㅠ",
          "original_form": "전 10키로 빼는게 목표입니다 ㅠ",
          "speaker_id": "1",
          "time": "20210820 08:47"
        },
        {
          "id": "MDRW2100001976.1.20",
          "form": "처음엔 많이 힘들었는데  적응되니까 오히려 컨디션이 좋아진거같았어요",
          "original_form": "처음엔 많이 힘들었는데  적응되니까 오히려 컨디션이 좋아진거같았어요",
          "speaker_id": "2",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100001976.1.21",
          "form": "살빠지는건 덤이고요",
          "original_form": "살빠지는건 덤이고요",
          "speaker_id": "2",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100001976.1.22",
          "form": "몸이 가벼워지니까 더 그럴꺼같아요 ㅋㅋㅋ부럽네요",
          "original_form": "몸이 가벼워지니까 더 그럴꺼같아요 ㅋㅋㅋ부럽네요",
          "speaker_id": "1",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100001976.1.23",
          "form": "점심에 맛있는거 드셔야겠어요",
          "original_form": "점심에 맛있는거 드셔야겠어요",
          "speaker_id": "1",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100001976.1.24",
          "form": "구내식당에서 주는.대로 먹고있어요 ㅎㅎ",
          "original_form": "구내식당에서 주는.대로 먹고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100001976.1.25",
          "form": "가끔 일탈(?)로 저녁때 맛잇는거먹어요 ㅎ",
          "original_form": "가끔 일탈(?)로 저녁때 맛잇는거먹어요 ㅎ",
