      "utterance": [
        {
          "id": "MDRW2100002299.1.1",
          "form": "과제할때 넘 힘들오",
          "original_form": "과제할때 넘 힘들오",
          "speaker_id": "2",
          "time": "20210820 08:42"
        },
        {
          "id": "MDRW2100002299.1.2",
          "form": "저는 교수님 수업만 듣는게 아닌데요",
          "original_form": "저는 교수님 수업만 듣는게 아닌데요",
          "speaker_id": "2",
          "time": "20210820 08:43"
        },
        {
          "id": "MDRW2100002299.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:46"
        },
        {
          "id": "MDRW2100002299.1.4",
          "form": "교수님은 그런거몰라",
          "original_form": "교수님은 그런거몰라",
          "speaker_id": "1",
          "time": "20210820 08:46"
        },
        {
          "id": "MDRW2100002299.1.5",
          "form": "학생들은 내 과제 이정도는 해야돼",
          "original_form": "학생들은 내 과제 이정도는 해야돼",
          "speaker_id": "1",
          "time": "20210820 08:46"
        },
        {
          "id": "MDRW2100002299.1.6",
          "form": "너무해요 엉엉 시험만 해도 벅찬데",
          "original_form": "너무해요 엉엉 시험만 해도 벅찬데",
          "speaker_id": "2",
          "time": "20210820 08:48"
        },
        {
          "id": "MDRW2100002299.1.7",
          "form": "갑자기 그거 생각난다",
          "original_form": "갑자기 그거 생각난다",
          "speaker_id": "1",
          "time": "20210820 08:54"
        },
        {
          "id": "MDRW2100002299.1.8",
          "form": "과제 혼자서 내멋대로 열심히 해서 제출했는데",
          "original_form": "과제 혼자서 내멋대로 열심히 해서 제출했는데",
          "speaker_id": "1",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100002299.1.9",
          "form": "교수님이 진심으로 열받았었는데",
          "original_form": "교수님이 진심으로 열받았었는데",
          "speaker_id": "1",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100002299.1.10",
          "form": "이런식으로 해오면 안되죠? 이러면서",
          "original_form": "이런식으로 해오면 안되죠? 이러면서",
          "speaker_id": "1",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100002299.1.11",
          "form": "홀ㅋㅋㅋ과제 어떻게 해야하는지 안알려줬잖아요ㅠㅠ",
          "original_form": "홀ㅋㅋㅋ과제 어떻게 해야하는지 안알려줬잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 08:57"
        },
        {
          "id": "MDRW2100002299.1.12",
          "form": "동아리활동 한거 있어?",
          "original_form": "동아리활동 한거 있어?",
          "speaker_id": "1",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002299.1.13",
          "form": "나 사진동아리 들어갔다가 하루만에 나왔어ㅋㅋㅋㅋㅋ",
          "original_form": "나 사진동아리 들어갔다가 하루만에 나왔어ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100002299.1.14",
          "form": "뭐때매ㅋㅋㅋㅋ 그정도면 탈출한거 아님?ㅋㅋㅋ",
          "original_form": "뭐때매ㅋㅋㅋㅋ 그정도면 탈출한거 아님?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:53"
        },
        {
          "id": "MDRW2100002299.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ넘 버티기 힘들도라고 나만 과가 달랐거든ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ넘 버티기 힘들도라고 나만 과가 달랐거든ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100002299.1.16",
          "form": "난 3학년때 과실보다는 동아리실이 편했어",
          "original_form": "난 3학년때 과실보다는 동아리실이 편했어",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002299.1.17",
          "form": "오히려 아는사람이 적은게 맘이 편하더라ㅋㅋㅋ",
          "original_form": "오히려 아는사람이 적은게 맘이 편하더라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002299.1.18",
          "form": "아하 그럴 수 있지",
          "original_form": "아하 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100002299.1.19",
          "form": "우리는 과실이 없었어",
          "original_form": "우리는 과실이 없었어",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100002299.1.20",
          "form": "난 어쩔 수 없는 아싸인가봐",
          "original_form": "난 어쩔 수 없는 아싸인가봐",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100002299.1.21",
          "form": "ㅋㅋㅋㅋ왜에 그래도 막상 친구들 있으면 잘 놀지 않아?",
          "original_form": "ㅋㅋㅋㅋ왜에 그래도 막상 친구들 있으면 잘 놀지 않아?",
          "speaker_id": "2",
          "time": "20210820 23:41"
        },
        {
          "id": "MDRW2100002299.1.22",
          "form": "근데 혼자있는게 좀 더 좋음ㅋㅋㅋ",
          "original_form": "근데 혼자있는게 좀 더 좋음ㅋㅋㅋ",
