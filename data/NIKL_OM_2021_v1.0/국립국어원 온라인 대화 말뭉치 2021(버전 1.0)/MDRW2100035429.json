      "utterance": [
        {
          "id": "MDRW2100035429.1.1",
          "form": "쇼핑 정보 얻기 좋은 곳 있나요?",
          "original_form": "쇼핑 정보 얻기 좋은 곳 있나요?",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100035429.1.2",
          "form": "저는 필요하면 물건 찾고 바로 사는 편이에요",
          "original_form": "저는 필요하면 물건 찾고 바로 사는 편이에요",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100035429.1.3",
          "form": "상세정보 꼼꼼하게 읽는 편이구요",
          "original_form": "상세정보 꼼꼼하게 읽는 편이구요",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100035429.1.4",
          "form": "아하 네",
          "original_form": "아하 네",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100035429.1.5",
          "form": "업체후기는 걸러보시나요?",
          "original_form": "업체후기는 걸러보시나요?",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100035429.1.6",
          "form": "별점 낮은 리뷰들을 꼼꼼히 보는 것 같아요",
          "original_form": "별점 낮은 리뷰들을 꼼꼼히 보는 것 같아요",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035429.1.7",
          "form": "앗 저도 그래요 ㅎㅎㅎ",
          "original_form": "앗 저도 그래요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035429.1.8",
          "form": "보통 화내면서 쓰는 리뷰들이 현실적이고 상세해서요 ㅎ",
          "original_form": "보통 화내면서 쓰는 리뷰들이 현실적이고 상세해서요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035429.1.9",
          "form": "ㅋㅋㅋ그죠 맞아요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ그죠 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035429.1.10",
          "form": "주로 어떤 거 구매하세요?",
          "original_form": "주로 어떤 거 구매하세요?",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035429.1.11",
          "form": "요즘에는 별로 사는게 없어요 ㅜㅜ",
          "original_form": "요즘에는 별로 사는게 없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035429.1.12",
          "form": "사고싶은 물건은 없나요?",
          "original_form": "사고싶은 물건은 없나요?",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035429.1.13",
          "form": "자동차 사고싶네요 ㅋㅋ",
          "original_form": "자동차 사고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035429.1.14",
          "form": "선호하는 기종이 있나요?",
          "original_form": "선호하는 기종이 있나요?",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035429.1.15",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100035429.1.16",
          "form": "그냥 막연하게 갖고싶으셨나봐요",
          "original_form": "그냥 막연하게 갖고싶으셨나봐요",
