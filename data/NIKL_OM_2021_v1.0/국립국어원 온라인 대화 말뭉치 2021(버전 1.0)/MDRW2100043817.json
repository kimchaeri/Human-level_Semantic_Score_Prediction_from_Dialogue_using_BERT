      "utterance": [
        {
          "id": "MDRW2100043817.1.1",
          "form": "후식으로 과자 냠냠하려고",
          "original_form": "후식으로 과자 냠냠하려고",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043817.1.2",
          "form": "저녁 먹엇어?",
          "original_form": "저녁 먹엇어?",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043817.1.3",
          "form": "조금만 먹어야지..",
          "original_form": "조금만 먹어야지..",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043817.1.4",
          "form": "ㅇㅇ 언니는?",
          "original_form": "ㅇㅇ 언니는?",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043817.1.5",
          "form": "나는 어제 남은 문어 삶은거랑",
          "original_form": "나는 어제 남은 문어 삶은거랑",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043817.1.6",
          "form": "감자탕",
          "original_form": "감자탕",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043817.1.7",
          "form": "헐 맛있겠다",
          "original_form": "헐 맛있겠다",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043817.1.8",
          "form": "다 고급음식이네 ㅋㅋㅋ",
          "original_form": "다 고급음식이네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043817.1.9",
          "form": "ㅋㅋㅋㅋ 그러겤ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그러겤ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043817.1.10",
          "form": "내돈주고 사먹기 힘든 음식ㅇ지",
          "original_form": "내돈주고 사먹기 힘든 음식ㅇ지",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043817.1.11",
          "form": "ㅇㅇ 문어 먹고싶네 ㅠ",
          "original_form": "ㅇㅇ 문어 먹고싶네 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043817.1.12",
          "form": "ㅠㅠ 문어 맛있었어 약간 제철..? 이라고 들었는데",
          "original_form": "ㅠㅠ 문어 맛있었어 약간 제철..? 이라고 들었는데",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043817.1.13",
          "form": "그건 낙지인가",
          "original_form": "그건 낙지인가",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043817.1.14",
          "form": "ㅋㅋㅋㅋㅋ 모르겠어",
          "original_form": "ㅋㅋㅋㅋㅋ 모르겠어",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043817.1.15",
          "form": "지금쯤이 철 맞을걸?",
          "original_form": "지금쯤이 철 맞을걸?",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043817.1.16",
          "form": "추석 직전이잖아",
          "original_form": "추석 직전이잖아",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043817.1.17",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043817.1.18",
          "form": "그렇군 약간 철을 가늠하기가 어려운거 같아",
          "original_form": "그렇군 약간 철을 가늠하기가 어려운거 같아",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043817.1.19",
          "form": "보통",
          "original_form": "보통",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043817.1.20",
          "form": "제사상에 올라가는건",
          "original_form": "제사상에 올라가는건",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043817.1.21",
          "form": "추석이 제철이라고 생각하면 편해 ㅋㅌㅋ",
          "original_form": "추석이 제철이라고 생각하면 편해 ㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043817.1.22",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043817.1.23",
          "form": "그렇군.. 차례상에 올라가묜 되는구나",
          "original_form": "그렇군.. 차례상에 올라가묜 되는구나",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043817.1.24",
          "form": "ㅇㅇ 꽃게도",
          "original_form": "ㅇㅇ 꽃게도",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.25",
          "form": "설 전엔 설에 나는 것들 올리나 흠 비슷하게 올리는 거 같긴 한데",
          "original_form": "설 전엔 설에 나는 것들 올리나 흠 비슷하게 올리는 거 같긴 한데",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.26",
          "form": "가을엔 숫게 봄엔 암게가 철이야",
          "original_form": "가을엔 숫게 봄엔 암게가 철이야",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.27",
          "form": "가을이니까 대체로 살이 오르는 느낌으로다가..",
          "original_form": "가을이니까 대체로 살이 오르는 느낌으로다가..",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.28",
          "form": "ㄹㅎ",
          "original_form": "ㄹㅎ",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.29",
          "form": "암게는 알 낳기 전이 맛있겠지..",
          "original_form": "암게는 알 낳기 전이 맛있겠지..",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.30",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋ 그치",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋ 그치",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.31",
          "form": "얘기하니까 게먹고싶다",
          "original_form": "얘기하니까 게먹고싶다",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.32",
          "form": "하지만 그땐 많이 못 먹겠지..",
          "original_form": "하지만 그땐 많이 못 먹겠지..",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.33",
          "form": "ㅠ 난 랍스타...",
          "original_form": "ㅠ 난 랍스타...",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043817.1.34",
          "form": "ㅋㅋㅋㅋㅋ 나 랍스타는 뭔가",
          "original_form": "ㅋㅋㅋㅋㅋ 나 랍스타는 뭔가",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043817.1.35",
          "form": "좀 비리고 거대한 새우 느낌니야",
          "original_form": "좀 비리고 거대한 새우 느낌니야",
          "speaker_id": "1",
          "time": "20210907 20:50"
        }
      ]
