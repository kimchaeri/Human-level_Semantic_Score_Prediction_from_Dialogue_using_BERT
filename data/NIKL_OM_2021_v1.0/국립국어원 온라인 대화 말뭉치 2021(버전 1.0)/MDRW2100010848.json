      "utterance": [
        {
          "id": "MDRW2100010848.1.1",
          "form": "웅장하진 않고 그녕.. 첨들으시면 시끄러율거에요",
          "original_form": "웅장하진 않고 그녕.. 첨들으시면 시끄러율거에요",
          "speaker_id": "1",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010848.1.2",
          "form": "커뮤같은거 하세요?",
          "original_form": "커뮤같은거 하세요?",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010848.1.3",
          "form": "인스타그램 해요",
          "original_form": "인스타그램 해요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010848.1.4",
          "form": "하고계신 sns있으신가요",
          "original_form": "하고계신 sns있으신가요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010848.1.5",
          "form": "저는 안해요 ㅋㅋ",
          "original_form": "저는 안해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010848.1.6",
          "form": "사람들 신경쓰는게 불편해서 ㅜ",
          "original_form": "사람들 신경쓰는게 불편해서 ㅜ",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010848.1.7",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010848.1.8",
          "form": "인스타 많이하세요?",
          "original_form": "인스타 많이하세요?",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010848.1.9",
          "form": "자주는 아니고 시간 날 때 한 번씩?",
          "original_form": "자주는 아니고 시간 날 때 한 번씩?",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010848.1.10",
          "form": "그냥 피드만 대충 둘러보는 정도로만 해요",
          "original_form": "그냥 피드만 대충 둘러보는 정도로만 해요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010848.1.11",
          "form": "릴스 한 번 보면 계속보게 되지 않나요 ㅋㅋ",
          "original_form": "릴스 한 번 보면 계속보게 되지 않나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010848.1.12",
          "form": "약간 짧게 짧게 올라와서ㅋㅋ",
          "original_form": "약간 짧게 짧게 올라와서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010848.1.13",
          "form": "계속 보게되는 거 있는 거 같아요",
          "original_form": "계속 보게되는 거 있는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010848.1.14",
          "form": "맞아요!! 중독되는듯한기분..",
          "original_form": "맞아요!! 중독되는듯한기분..",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010848.1.15",
          "form": "이래서 틱톡이 뜬건가 ㅋㅋㅋ",
          "original_form": "이래서 틱톡이 뜬건가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010848.1.16",
          "form": "별 내용은 없는데 시간떼우기는 좋아서ㅋㅋㅋ",
          "original_form": "별 내용은 없는데 시간떼우기는 좋아서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010848.1.17",
          "form": "그런가봐요 사람들이 이제는 가벼운거 위주로 좋아하니까",
          "original_form": "그런가봐요 사람들이 이제는 가벼운거 위주로 좋아하니까",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010848.1.18",
          "form": "틱톡 보시나요",
          "original_form": "틱톡 보시나요",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010848.1.19",
          "form": "틱톡은 안보고 유튜브 쇼츠? 많이봐요",
          "original_form": "틱톡은 안보고 유튜브 쇼츠? 많이봐요",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010848.1.20",
          "form": "릴스나 쇼츠나 틱톡이나 다거기서거기인곳같아욬ㅋㅋ",
          "original_form": "릴스나 쇼츠나 틱톡이나 다거기서거기인곳같아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010848.1.21",
          "form": "그건 그래요ㅋㅋ 그리고 뭔가 틱톡은 감성에 안맞아서ㅋㅋ",
          "original_form": "그건 그래요ㅋㅋ 그리고 뭔가 틱톡은 감성에 안맞아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010848.1.22",
          "form": "맞아욬ㅋㅋㅋ 틱톡은 약간 오글거리는 감성이 주류인듯해요",
          "original_form": "맞아욬ㅋㅋㅋ 틱톡은 약간 오글거리는 감성이 주류인듯해요",
