      },
      "utterance": [
        {
          "id": "MDRW2100038900.1.1",
          "form": "응 오늘 아침에 엠알에이 찍고 오셨어",
          "original_form": "응 오늘 아침에 엠알에이 찍고 오셨어",
          "speaker_id": "2",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038900.1.2",
          "form": "병원에서 뭐래? 더 안좋아진 데는 없대?",
          "original_form": "병원에서 뭐래? 더 안좋아진 데는 없대?",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100038900.1.3",
          "form": "오른쪽은 괜찮은데 왼쪽 뇌 혈관이 좀 막혔대",
          "original_form": "오른쪽은 괜찮은데 왼쪽 뇌 혈관이 좀 막혔대",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100038900.1.4",
          "form": "그래? 심해진 건 아니고?",
          "original_form": "그래? 심해진 건 아니고?",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038900.1.5",
          "form": "많이 심해진 건 아닌데 그래도 계속 조심하고 정기적으로 병원 다녀야지",
          "original_form": "많이 심해진 건 아닌데 그래도 계속 조심하고 정기적으로 병원 다녀야지",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038900.1.6",
          "form": "입원해야되는 거 아니면 그나마 다행이지 뭐..",
          "original_form": "입원해야되는 거 아니면 그나마 다행이지 뭐..",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038900.1.7",
          "form": "다른 치료 받을 건 없대?",
          "original_form": "다른 치료 받을 건 없대?",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038900.1.8",
          "form": "약만 좀 더 추가했어. 한동안 꾸준히 잘 챙겨 먹고 다시 검사해야 된대",
          "original_form": "약만 좀 더 추가했어. 한동안 꾸준히 잘 챙겨 먹고 다시 검사해야 된대",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038900.1.9",
          "form": "뇌경색이니까 약은 평생 먹어야지 뭐... 엄마는 아픈데 없고?",
          "original_form": "뇌경색이니까 약은 평생 먹어야지 뭐... 엄마는 아픈데 없고?",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038900.1.10",
          "form": "나는 내일 피검사 받으러 가",
          "original_form": "나는 내일 피검사 받으러 가",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038900.1.11",
          "form": "아침 일찍 예약해뒀어",
          "original_form": "아침 일찍 예약해뒀어",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038900.1.12",
          "form": "피검사 받고 2주 후에 또 정기검진 이랬지?",
          "original_form": "피검사 받고 2주 후에 또 정기검진 이랬지?",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038900.1.13",
          "form": "응. 요새 혈압도 계속 높아져가지고 그것도 상담해보려고",
          "original_form": "응. 요새 혈압도 계속 높아져가지고 그것도 상담해보려고",
          "speaker_id": "2",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100038900.1.14",
          "form": "집에 혈압계 있잖아",
          "original_form": "집에 혈압계 있잖아",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038900.1.15",
          "form": "그걸로 매일 혈압 재서 기록해 봐",
          "original_form": "그걸로 매일 혈압 재서 기록해 봐",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038900.1.16",
          "form": "그래. 혈압약 먹기 싫으면 미리 예방해야지",
          "original_form": "그래. 혈압약 먹기 싫으면 미리 예방해야지",
          "speaker_id": "2",
          "time": "20210902 22:52"
        }
      ]
