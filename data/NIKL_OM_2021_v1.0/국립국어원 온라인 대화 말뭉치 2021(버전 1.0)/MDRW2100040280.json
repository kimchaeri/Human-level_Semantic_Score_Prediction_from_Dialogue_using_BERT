      "utterance": [
        {
          "id": "MDRW2100040280.1.1",
          "form": "헤드스파 7 써 보셨나요",
          "original_form": "헤드스파 7 써 보셨나요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040280.1.2",
          "form": "처음 들어봅니다",
          "original_form": "처음 들어봅니다",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040280.1.3",
          "form": "제가 머릿결이 좋지 않은데 헤드스파 7 쓰고 머릿결이 많이 좋아졌거든요",
          "original_form": "제가 머릿결이 좋지 않은데 헤드스파 7 쓰고 머릿결이 많이 좋아졌거든요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040280.1.4",
          "form": "아 샴푸 군요",
          "original_form": "아 샴푸 군요",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.5",
          "form": "홈쇼핑에서 대량으로 싸게 팔대가 한 번씩 있어요",
          "original_form": "홈쇼핑에서 대량으로 싸게 팔대가 한 번씩 있어요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.6",
          "form": "트리트먼튼 가요",
          "original_form": "트리트먼튼 가요",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.7",
          "form": "트리트먼트예요",
          "original_form": "트리트먼트예요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.8",
          "form": "그 회사에서 만든 샴푸도 있긴 한데 써보진 않았어요",
          "original_form": "그 회사에서 만든 샴푸도 있긴 한데 써보진 않았어요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.9",
          "form": "최근에는 쿤 달 밖에 쓰지 않아서",
          "original_form": "최근에는 쿤 달 밖에 쓰지 않아서",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.10",
          "form": "많이 괜찮은가 보네요",
          "original_form": "많이 괜찮은가 보네요",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.11",
          "form": "탈모방지용 이라서요 그거 쓰고 머릿결도 좋아지고 탈모방지도 된다고 하더라고요",
          "original_form": "탈모방지용 이라서요 그거 쓰고 머릿결도 좋아지고 탈모방지도 된다고 하더라고요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.12",
          "form": "머릿결이 확실히 많이 좋아진",
          "original_form": "머릿결이 확실히 많이 좋아진",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.13",
          "form": "많이 강추하고 다닙니다",
          "original_form": "많이 강추하고 다닙니다",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.14",
          "form": "오세일할 때 한 번 쟁여놔야 되겠네요",
          "original_form": "오세일할 때 한 번 쟁여놔야 되겠네요",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040280.1.15",
          "form": "올리브영 세일 하고 있어요 거기에서 낱개로 팔기도 해요",
          "original_form": "올리브영 세일 하고 있어요 거기에서 낱개로 팔기도 해요",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.16",
          "form": "그런데 홈쇼핑에서 사는게 훨씬 싸게 살 수 있어요",
          "original_form": "그런데 홈쇼핑에서 사는게 훨씬 싸게 살 수 있어요",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.17",
          "form": "홈쇼핑이 훨씬 싸겠지요",
          "original_form": "홈쇼핑이 훨씬 싸겠지요",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.18",
          "form": "근데 낮게 가 좋은데",
          "original_form": "근데 낮게 가 좋은데",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.19",
          "form": "맞아요 대용량 이니까요",
          "original_form": "맞아요 대용량 이니까요",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.20",
          "form": "올 령에서 한 개만 사는 게 쌀 수도 있어요",
          "original_form": "올 령에서 한 개만 사는 게 쌀 수도 있어요",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.21",
          "form": "의령에서 하나만 사서 써 보세요",
          "original_form": "의령에서 하나만 사서 써 보세요",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040280.1.22",
          "form": "추석 선물 샀어요",
