        },
        {
          "id": "MDRW2100000922.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000922.1.3",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000922.1.4",
          "form": "안 키우지만 동물 좋아해요",
          "original_form": "안 키우지만 동물 좋아해요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000922.1.5",
          "form": "어떤동물 좋아하세요?",
          "original_form": "어떤동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000922.1.6",
          "form": "특히 강아지 좋아해요",
          "original_form": "특히 강아지 좋아해요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000922.1.7",
          "form": "오오 저는 고양이 강아지 둘다 ㅎ",
          "original_form": "오오 저는 고양이 강아지 둘다 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000922.1.8",
          "form": "고양이는 귀여운데 말을 잘 안 들어요",
          "original_form": "고양이는 귀여운데 말을 잘 안 들어요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000922.1.9",
          "form": "ㅋㅋㅋ 글죠 까다로워요",
          "original_form": "ㅋㅋㅋ 글죠 까다로워요",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000922.1.10",
          "form": "그래서 애교많은 강아지가 좋아요",
          "original_form": "그래서 애교많은 강아지가 좋아요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000922.1.11",
          "form": "고양이는 놀때가 제일 귀여워요",
          "original_form": "고양이는 놀때가 제일 귀여워요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000922.1.12",
          "form": "간식은 뭐주나요",
          "original_form": "간식은 뭐주나요",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000922.1.13",
          "form": "고양이가 먹는간식이라..",
          "original_form": "고양이가 먹는간식이라..",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000922.1.14",
          "form": "츄.르",
          "original_form": "츄.르",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000922.1.15",
          "form": "고양이 모래도 뭐살지 고민이예요",
          "original_form": "고양이 모래도 뭐살지 고민이예요",
