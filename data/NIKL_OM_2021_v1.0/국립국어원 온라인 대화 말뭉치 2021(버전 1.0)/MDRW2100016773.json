      "utterance": [
        {
          "id": "MDRW2100016773.1.1",
          "form": "겜 좋아하시나요",
          "original_form": "겜 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016773.1.2",
          "form": "가끔 플래시게임해요ㅎㅎ",
          "original_form": "가끔 플래시게임해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016773.1.3",
          "form": "저는 피파온라인해요",
          "original_form": "저는 피파온라인해요",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016773.1.4",
          "form": "제동생도 하는데 거의 다하는거같아요",
          "original_form": "제동생도 하는데 거의 다하는거같아요",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016773.1.5",
          "form": "다른게임 안하시나요?",
          "original_form": "다른게임 안하시나요?",
          "speaker_id": "1",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016773.1.6",
          "form": "심즈도 가끔해요",
          "original_form": "심즈도 가끔해요",
          "speaker_id": "2",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016773.1.7",
          "form": "오 심즈 추억의게임이네요",
          "original_form": "오 심즈 추억의게임이네요",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016773.1.8",
          "form": "재밌어요ㅎㅎ컴으로는 할게임이 없어서",
          "original_form": "재밌어요ㅎㅎ컴으로는 할게임이 없어서",
          "speaker_id": "2",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016773.1.9",
          "form": "오래하셨나요",
          "original_form": "오래하셨나요",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016773.1.10",
          "form": "시작한건 예전이니까 오래됐어요",
          "original_form": "시작한건 예전이니까 오래됐어요",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016773.1.11",
          "form": "안질리시나요ㅎㅎ",
          "original_form": "안질리시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016773.1.12",
          "form": "계속한건 아니라서요 ㅎ",
          "original_form": "계속한건 아니라서요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
