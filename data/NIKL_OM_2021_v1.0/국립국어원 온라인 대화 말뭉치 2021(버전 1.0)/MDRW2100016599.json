      "utterance": [
        {
          "id": "MDRW2100016599.1.1",
          "form": "게임 하는거 있으세요?",
          "original_form": "게임 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016599.1.2",
          "form": "모바일 게임하고 있어요",
          "original_form": "모바일 게임하고 있어요",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016599.1.3",
          "form": "어떤거하세요?",
          "original_form": "어떤거하세요?",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016599.1.4",
          "form": "저는 프렌즈타운해요!",
          "original_form": "저는 프렌즈타운해요!",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016599.1.5",
          "form": "저는 꿈의집이요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 꿈의집이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016599.1.6",
          "form": "아 저도요ㅋㅋㅋ예전에하다가 얼마전에 다시깔았는데",
          "original_form": "아 저도요ㅋㅋㅋ예전에하다가 얼마전에 다시깔았는데",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016599.1.7",
          "form": "난이도 장난아니던데요?",
          "original_form": "난이도 장난아니던데요?",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016599.1.8",
          "form": "맞아요 첨에는 쉬웠는데 갈수록 깨기 어려워지더라고요",
          "original_form": "맞아요 첨에는 쉬웠는데 갈수록 깨기 어려워지더라고요",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016599.1.9",
          "form": "아이템 없이는 어려워요 ㅋㅋㅋ",
          "original_form": "아이템 없이는 어려워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016599.1.10",
          "form": "하트 너무부족해요 진짜....",
          "original_form": "하트 너무부족해요 진짜....",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016599.1.11",
          "form": "맞아요 정말 하트 부족해요ㅠㅠ막히는판은 하다보면 가득 쌓아놓은 하트가 사라져있고 ㅋㅋㅋ",
          "original_form": "맞아요 정말 하트 부족해요ㅠㅠ막히는판은 하다보면 가득 쌓아놓은 하트가 사라져있고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016599.1.12",
          "form": "혹시 현질도 하세요?",
          "original_form": "혹시 현질도 하세요?",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016599.1.13",
          "form": "그건 제 마지막 자존심이욬ㅋㅋㅋㅋㅋㅋ 과금은 하지말자주의",
          "original_form": "그건 제 마지막 자존심이욬ㅋㅋㅋㅋㅋㅋ 과금은 하지말자주의",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016599.1.14",
          "form": "저두요ㅋㅋㅋ 근데 매번 고민하긴해서요",
          "original_form": "저두요ㅋㅋㅋ 근데 매번 고민하긴해서요",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016599.1.15",
          "form": "이번 한번만 할까 싶고ㅋㅋ",
          "original_form": "이번 한번만 할까 싶고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016599.1.16",
          "form": "오스틴의 농락에 넘어가지않으려고요!",
          "original_form": "오스틴의 농락에 넘어가지않으려고요!",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016599.1.17",
          "form": "결제하면 진짜 끝없을거같은..",
          "original_form": "결제하면 진짜 끝없을거같은..",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016599.1.18",
          "form": "오스틴의 농락 ㅋㅋㅋㅋㅋ",
          "original_form": "오스틴의 농락 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016599.1.19",
          "form": "말로는 저보고 집주인이라는데 ㅋㅋㅋ 아무리 생각해도 오스틴이 집주인 같아요 ㅋㅋ",
          "original_form": "말로는 저보고 집주인이라는데 ㅋㅋㅋ 아무리 생각해도 오스틴이 집주인 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016599.1.20",
          "form": "꿈의집은 나온지 쫌 돾는데도 꾸준히 인기있는거보면",
          "original_form": "꿈의집은 나온지 쫌 돾는데도 꾸준히 인기있는거보면",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016599.1.21",
          "form": "중독성장난아닌가봐요 다들",
          "original_form": "중독성장난아닌가봐요 다들",
          "speaker_id": "1",
          "time": "20210826 14:14"
        }
      ]
    }
  ]
