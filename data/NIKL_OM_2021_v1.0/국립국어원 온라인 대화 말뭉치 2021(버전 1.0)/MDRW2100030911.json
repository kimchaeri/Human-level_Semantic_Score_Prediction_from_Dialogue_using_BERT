      "utterance": [
        {
          "id": "MDRW2100030911.1.1",
          "form": "아진짜 삼각관계 젤 싫음요",
          "original_form": "아진짜 삼각관계 젤 싫음요",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030911.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030911.1.3",
          "form": "스토리 진행 상",
          "original_form": "스토리 진행 상",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030911.1.4",
          "form": "이해는 되는데",
          "original_form": "이해는 되는데",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030911.1.5",
          "form": "둘이 만나 사랑했습니다",
          "original_form": "둘이 만나 사랑했습니다",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030911.1.6",
          "form": "라고 하면 그냥 스토리가 끝이니",
          "original_form": "라고 하면 그냥 스토리가 끝이니",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030911.1.7",
          "form": "그건그래요",
          "original_form": "그건그래요",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.8",
          "form": "노잼인가..?",
          "original_form": "노잼인가..?",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.9",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.11",
          "form": "그 뭐지",
          "original_form": "그 뭐지",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.12",
          "form": "제가 영화 시나리오 동아리 했었다고 했었잖아요",
          "original_form": "제가 영화 시나리오 동아리 했었다고 했었잖아요",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.13",
          "form": "항상 스토리텔링 시작이",
          "original_form": "항상 스토리텔링 시작이",
          "speaker_id": "1",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030911.1.14",
          "form": "왕이 왕비를 만났습니다\r\n끝",
          "original_form": "왕이 왕비를 만났습니다\r\n끝",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100030911.1.15",
          "form": "이 두 줄 사이에 얘기를 넣는게",
          "original_form": "이 두 줄 사이에 얘기를 넣는게",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100030911.1.16",
          "form": "스토리텔링이다...",
          "original_form": "스토리텔링이다...",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100030911.1.17",
          "form": "라는 식이라",
          "original_form": "라는 식이라",
          "speaker_id": "1",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100030911.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ5살에 첫눈에 반해 행복하게 만났다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ5살에 첫눈에 반해 행복하게 만났다",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100030911.1.19",
          "form": "이럼 안되는거죠?",
          "original_form": "이럼 안되는거죠?",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030911.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030911.1.21",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030911.1.22",
          "form": "개인에게 있어서는",
          "original_form": "개인에게 있어서는",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030911.1.23",
          "form": "그게 더 행복할 거 같기도 한데..",
          "original_form": "그게 더 행복할 거 같기도 한데..",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030911.1.24",
          "form": "흠흠",
          "original_form": "흠흠",
          "speaker_id": "1",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030911.1.25",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030911.1.26",
          "form": "마자요 그게 제일 행복이죠",
          "original_form": "마자요 그게 제일 행복이죠",
          "speaker_id": "2",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030911.1.27",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030911.1.28",
          "form": "영화 등에서",
          "original_form": "영화 등에서",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030911.1.29",
          "form": "배경은",
          "original_form": "배경은",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030911.1.30",
          "form": "어떤게 제일 좋으신가요",
          "original_form": "어떤게 제일 좋으신가요",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100030911.1.31",
          "form": "배경이요?? 어떤 식의 배경이요??",
          "original_form": "배경이요?? 어떤 식의 배경이요??",
          "speaker_id": "2",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100030911.1.32",
          "form": "도시라던가",
          "original_form": "도시라던가",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100030911.1.33",
          "form": "교외지라던가",
          "original_form": "교외지라던가",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100030911.1.34",
          "form": "옛날이라던가",
          "original_form": "옛날이라던가",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100030911.1.35",
          "form": "우주라던가...",
          "original_form": "우주라던가...",
          "speaker_id": "1",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100030911.1.36",
          "form": "아 저는 옛날이요",
          "original_form": "아 저는 옛날이요",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100030911.1.37",
          "form": "한복입고 나오는",
          "original_form": "한복입고 나오는",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100030911.1.38",
          "form": "하지만 심오한 역사말고..",
          "original_form": "하지만 심오한 역사말고..",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100030911.1.39",
          "form": "약간 판타지가 들어간",
          "original_form": "약간 판타지가 들어간",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100030911.1.40",
          "form": "그런거 좋아해요",
          "original_form": "그런거 좋아해요",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100030911.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210905 14:58"
        },
        {
          "id": "MDRW2100030911.1.42",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100030911.1.43",
          "form": "사실 심오한건",
          "original_form": "사실 심오한건",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100030911.1.44",
          "form": "교과서로도 충분하죠",
          "original_form": "교과서로도 충분하죠",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100030911.1.45",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100030911.1.46",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100030911.1.47",
          "form": "역사 그래서 싫오했어욬ㅋㅋㅋㅋㅋ",
          "original_form": "역사 그래서 싫오했어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100030911.1.48",
          "form": "툭하면 전쟁이고  사람죽이고",
          "original_form": "툭하면 전쟁이고  사람죽이고",
          "speaker_id": "2",
          "time": "20210905 15:29"
