      "utterance": [
        {
          "id": "MDRW2100002185.1.1",
          "form": "3살아들 둔 워킹맘이에요 육아하시나요?",
          "original_form": "3살아들 둔 워킹맘이에요 육아하시나요?",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002185.1.2",
          "form": "육아는 아니지만 동생이 둘 있어요!",
          "original_form": "육아는 아니지만 동생이 둘 있어요!",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002185.1.3",
          "form": "우와 동생들 하고 나이차이 많이나세요?",
          "original_form": "우와 동생들 하고 나이차이 많이나세요?",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002185.1.4",
          "form": "3살 5살차이예요! 차이 많이 안나서그런지 친구처럼 지내요ㅋㅋ",
          "original_form": "3살 5살차이예요! 차이 많이 안나서그런지 친구처럼 지내요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002185.1.5",
          "form": "애기 만으로 3살이예요?",
          "original_form": "애기 만으로 3살이예요?",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002185.1.6",
          "form": "아 부럽습니다ㅎ \r\n아 아이는 만1세이고 저는 남동생하고 3살차이인데 누나동생인데도 베프처럼 잘지내요ㅎ",
          "original_form": "아 부럽습니다ㅎ \r\n아 아이는 만1세이고 저는 남동생하고 3살차이인데 누나동생인데도 베프처럼 잘지내요ㅎ",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002185.1.7",
          "form": "동생들하고 비밀없이 잘 지내세용?ㅎ",
          "original_form": "동생들하고 비밀없이 잘 지내세용?ㅎ",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002185.1.8",
          "form": "비밀까지는 잘 얘기 안하는것 같아요ㅋㅋ",
          "original_form": "비밀까지는 잘 얘기 안하는것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002185.1.9",
          "form": "그냥 같이 배달시켜먹고~가끔 놀러가고~",
          "original_form": "그냥 같이 배달시켜먹고~가끔 놀러가고~",
          "speaker_id": "2",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002185.1.10",
          "form": "저희는 둘다결혼해서 더 얘기하나바요ㅎㅎ",
          "original_form": "저희는 둘다결혼해서 더 얘기하나바요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002185.1.11",
          "form": "집안일도 다같이 잘하나요?",
          "original_form": "집안일도 다같이 잘하나요?",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002185.1.12",
          "form": "다들 20대라 서로 안하려고 미루긴 해요ㅋㅋ바쁘다면서~",
          "original_form": "다들 20대라 서로 안하려고 미루긴 해요ㅋㅋ바쁘다면서~",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002185.1.13",
          "form": "엄마가 최고죠?ㅎㅎ",
          "original_form": "엄마가 최고죠?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002185.1.14",
          "form": "ㅋㅋ그런것같아요",
          "original_form": "ㅋㅋ그런것같아요",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002185.1.15",
          "form": "애기 한살이면 넘 귀엽겠다",
          "original_form": "애기 한살이면 넘 귀엽겠다",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002185.1.16",
          "form": "엄마바라기입니다ㅎㅎ\r\n육아에 직장인에 정신없답니다 그래도 세상이뻐요",
          "original_form": "엄마바라기입니다ㅎㅎ\r\n육아에 직장인에 정신없답니다 그래도 세상이뻐요",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002185.1.17",
          "form": "주말에 가족들하고 뭐하세요?",
          "original_form": "주말에 가족들하고 뭐하세요?",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002185.1.18",
          "form": "다들 평일에 일하느라 지쳐서 각자 시간 보내는 날이 많아요!",
          "original_form": "다들 평일에 일하느라 지쳐서 각자 시간 보내는 날이 많아요!",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002185.1.19",
          "form": "가끔 시골 내려갔다오기도 하구요",
          "original_form": "가끔 시골 내려갔다오기도 하구요",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002185.1.20",
          "form": "맞아요 남동생?여동생 다 있으세요?",
          "original_form": "맞아요 남동생?여동생 다 있으세요?",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002185.1.21",
          "form": "네 바로 밑에 여동생이고 막내 남동생 있어요ㅋㅋ",
          "original_form": "네 바로 밑에 여동생이고 막내 남동생 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002185.1.22",
          "form": "엄마가 아들 가지려 노력하셨네요!\r\n여동생이랑 더친하죠?ㅎ",
          "original_form": "엄마가 아들 가지려 노력하셨네요!\r\n여동생이랑 더친하죠?ㅎ",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002185.1.23",
          "form": "약간 속상하게도 동생들끼리 더 친해요ㅋㅋ저는 안끼워줌ㅠㅠ",
          "original_form": "약간 속상하게도 동생들끼리 더 친해요ㅋㅋ저는 안끼워줌ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002185.1.24",
          "form": "앜ㅋㅋ 치사하네요 동생들이 ㅎㅎ",
          "original_form": "앜ㅋㅋ 치사하네요 동생들이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:39"
        }
      ]
