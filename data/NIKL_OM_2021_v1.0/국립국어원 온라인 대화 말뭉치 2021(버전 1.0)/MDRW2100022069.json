      "utterance": [
        {
          "id": "MDRW2100022069.1.1",
          "form": "학교생활은 잼났어요ㅎㅎ",
          "original_form": "학교생활은 잼났어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:00"
        },
        {
          "id": "MDRW2100022069.1.2",
          "form": "name1님은 학교때 뭐하셨어요?",
          "original_form": "&name1&님은 학교때 뭐하셨어요?",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022069.1.3",
          "form": "음 꼭 동아리가아니어도되고ㅎ",
          "original_form": "음 꼭 동아리가아니어도되고ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:01"
        },
        {
          "id": "MDRW2100022069.1.4",
          "form": "저 범생이요",
          "original_form": "저 범생이요",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.5",
          "form": "첫사랑 이야기나 들어볼까요?ㅎㅎㅎㄹㄹㄹㄹ",
          "original_form": "첫사랑 이야기나 들어볼까요?ㅎㅎㅎㄹㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.6",
          "form": "우왐ㅎㅎ",
          "original_form": "우왐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.7",
          "form": "뭐 안했던게",
          "original_form": "뭐 안했던게",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.8",
          "form": "범생이셨네요ㅎ",
          "original_form": "범생이셨네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.9",
          "form": "특징",
          "original_form": "특징",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.10",
          "form": "담치기도 안해봄",
          "original_form": "담치기도 안해봄",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.11",
          "form": "그럴수있을듯ㅎ",
          "original_form": "그럴수있을듯ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.12",
          "form": "하시는거보면 알겟어요ㅎ",
          "original_form": "하시는거보면 알겟어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.13",
          "form": "아 근데 느낀게요   동창회하니",
          "original_form": "아 근데 느낀게요   동창회하니",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.14",
          "form": "존재감 없음 ㅋ",
          "original_form": "존재감 없음 ㅋ",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.15",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.16",
          "form": "추억거리 1도 없더라구요",
          "original_form": "추억거리 1도 없더라구요",
          "speaker_id": "1",
          "time": "20210829 02:02"
        },
        {
          "id": "MDRW2100022069.1.17",
          "form": "아아아ㅎㅎ",
          "original_form": "아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.18",
          "form": "담치기도 안해 땡땡이도 안해",
          "original_form": "담치기도 안해 땡땡이도 안해",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.19",
          "form": "그래도 친구들이 기억은하죠?",
          "original_form": "그래도 친구들이 기억은하죠?",
          "speaker_id": "2",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.20",
          "form": "name1님 얼굴은?ㅎㅎㅎㅎㅎㅎ",
          "original_form": "&name1&님 얼굴은?ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.21",
          "form": "음 중학교땐 못해요 ㅎㅎ",
          "original_form": "음 중학교땐 못해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.22",
          "form": "근데 범생이시면",
          "original_form": "근데 범생이시면",
          "speaker_id": "2",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.23",
          "form": "중학교땐 아주 공부 잘한것도 아니구 그냥 선생님이 아 잰 조금하네 그정도고",
          "original_form": "중학교땐 아주 공부 잘한것도 아니구 그냥 선생님이 아 잰 조금하네 그정도고",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.24",
          "form": "교과서를 사랑하셨겟네요?",
          "original_form": "교과서를 사랑하셨겟네요?",
          "speaker_id": "2",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022069.1.25",
          "form": "아뇨 ㅋ",
          "original_form": "아뇨 ㅋ",
          "speaker_id": "1",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022069.1.26",
          "form": "전혀",
          "original_form": "전혀",
          "speaker_id": "1",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022069.1.27",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022069.1.28",
          "form": "수업시간외엔 안해요 스타일이",
          "original_form": "수업시간외엔 안해요 스타일이",
          "speaker_id": "1",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022069.1.29",
          "form": "교과서를 전 짝사랑만해서....",
          "original_form": "교과서를 전 짝사랑만해서....",
          "speaker_id": "2",
          "time": "20210829 02:04"
        },
        {
          "id": "MDRW2100022069.1.30",
          "form": "사랑하고싶었는데 교과선 절 안좋아하더라구요ㅎ",
          "original_form": "사랑하고싶었는데 교과선 절 안좋아하더라구요ㅎ",
