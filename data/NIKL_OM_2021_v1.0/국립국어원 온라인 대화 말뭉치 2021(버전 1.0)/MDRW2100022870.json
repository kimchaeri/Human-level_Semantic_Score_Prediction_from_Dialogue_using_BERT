      "utterance": [
        {
          "id": "MDRW2100022870.1.1",
          "form": "브라우니가 유명하다고해서",
          "original_form": "브라우니가 유명하다고해서",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022870.1.2",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022870.1.3",
          "form": "한번 사봤거든용",
          "original_form": "한번 사봤거든용",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022870.1.4",
          "form": "어떠셨나요~",
          "original_form": "어떠셨나요~",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022870.1.5",
          "form": "근데 생각보다 되게 작아서 좀",
          "original_form": "근데 생각보다 되게 작아서 좀",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022870.1.6",
          "form": "실망했는데",
          "original_form": "실망했는데",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022870.1.7",
          "form": "4조각으로 나눠서 그중 한조각을 먹었느넫",
          "original_form": "4조각으로 나눠서 그중 한조각을 먹었느넫",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022870.1.8",
          "form": "엄청 달더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "엄청 달더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022870.1.9",
          "form": "으악ㅋㅋㅋ",
          "original_form": "으악ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022870.1.10",
          "form": "저도 커 가면서 단걸 잘 못먹어서",
          "original_form": "저도 커 가면서 단걸 잘 못먹어서",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022870.1.11",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022870.1.12",
          "form": "저도 딱한입정도만 좋아요",
          "original_form": "저도 딱한입정도만 좋아요",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022870.1.13",
          "form": "머리가 아플정도로 달더라구욬ㅋㅋㅋ",
          "original_form": "머리가 아플정도로 달더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022870.1.14",
          "form": "피자는 선호하시는 브랜드 있으세요?",
          "original_form": "피자는 선호하시는 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022870.1.15",
          "form": "이것저것 잘먹습니다!",
          "original_form": "이것저것 잘먹습니다!",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022870.1.16",
          "form": "그때그때 행사하는거 먹게되더라구요ㅋㅋ",
          "original_form": "그때그때 행사하는거 먹게되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022870.1.17",
          "form": "저도 그때그때 행사하는거 먹어요ㅋㅋㅋ",
          "original_form": "저도 그때그때 행사하는거 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022870.1.18",
          "form": "최근에 도미노피자 먹었는데",
          "original_form": "최근에 도미노피자 먹었는데",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022870.1.19",
          "form": "뉴욕오리진 맛있더라구요",
          "original_form": "뉴욕오리진 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022870.1.20",
          "form": "검색해봤는데",
          "original_form": "검색해봤는데",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022870.1.21",
          "form": "맛있어보이네요!",
          "original_form": "맛있어보이네요!",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022870.1.22",
          "form": "기본 콤비네이션 피자같은 맛인데",
          "original_form": "기본 콤비네이션 피자같은 맛인데",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022870.1.23",
          "form": "토핑이 넉넉해서 맛있었어요ㅎㅎ",
          "original_form": "토핑이 넉넉해서 맛있었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:50"
        }
      ]
