      "utterance": [
        {
          "id": "MDRW2100020152.1.1",
          "form": "노는언니들에 배구선수들나오나요?",
          "original_form": "노는언니들에 배구선수들나오나요?",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020152.1.2",
          "form": "티비돌리다 언뜻 예고봤는데 아닐수도 있어요ㅋㅋㅋㅠ",
          "original_form": "티비돌리다 언뜻 예고봤는데 아닐수도 있어요ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020152.1.3",
          "form": "김희진 선수 방송으로 보고 싶은데 나와줬으면 좋겠어요ㅋㅋㅋㅋ",
          "original_form": "김희진 선수 방송으로 보고 싶은데 나와줬으면 좋겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020152.1.4",
          "form": "헐 저도 김희진선수 너무 궁금해요! ㅋㅋㅋ",
          "original_form": "헐 저도 김희진선수 너무 궁금해요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020152.1.5",
          "form": "김희진 선수 너무 잘생겼어요 ㅠㅠㅠ 그런데 또 순둥이...",
          "original_form": "김희진 선수 너무 잘생겼어요 ㅠㅠㅠ 그런데 또 순둥이...",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020152.1.6",
          "form": "진짜 잘생겼다는말이 딱이에요 ㅋㅋㅋㅋ 배구원래좋아하세요?",
          "original_form": "진짜 잘생겼다는말이 딱이에요 ㅋㅋㅋㅋ 배구원래좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020152.1.7",
          "form": "저는 가끔 재밌는 경기있단 소식 들으면 봤어요ㅋㅋㅋ",
          "original_form": "저는 가끔 재밌는 경기있단 소식 들으면 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020152.1.8",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020152.1.9",
          "form": "저는 그 만화로 배구를 배워서... ㅋㅋㅋ",
          "original_form": "저는 그 만화로 배구를 배워서... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020152.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ하이큐보셨나요,,",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하이큐보셨나요,,",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020152.1.11",
          "form": "네 ㅋㅋㅋㅋㅋㅋㅋ 다봤어요 그래도 룰은 아직 어려워요 ㅜㅜ",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋㅋ 다봤어요 그래도 룰은 아직 어려워요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020152.1.12",
          "form": "저도 다 봤는데 아직도 룰 몰라요ㅋㅋㅋ",
          "original_form": "저도 다 봤는데 아직도 룰 몰라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020152.1.13",
          "form": "리베로 이름 하나 배웠어요",
          "original_form": "리베로 이름 하나 배웠어요",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020152.1.14",
          "form": "ㅋㅋㅋㅋ 아니 자꾸 자리를 이동하더라구요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아니 자꾸 자리를 이동하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020152.1.15",
          "form": "캐릭터도 많아서 헛갈리구 ㅋㅋㅋ",
          "original_form": "캐릭터도 많아서 헛갈리구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020152.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020152.1.17",
          "form": "저는 그... 걔들 좋아했어요 네코마인가",
          "original_form": "저는 그... 걔들 좋아했어요 네코마인가",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020152.1.18",
          "form": "그 게임하던 리베로?",
          "original_form": "그 게임하던 리베로?",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020152.1.19",
          "form": "아뇨 학교 이름이었던거 같은데 구호가 피가 돼서 온몸 곳곳으로 퍼지자 했던 애들인가?",
          "original_form": "아뇨 학교 이름이었던거 같은데 구호가 피가 돼서 온몸 곳곳으로 퍼지자 했던 애들인가?",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020152.1.20",
          "form": "그그 도쿄팀이군요!",
          "original_form": "그그 도쿄팀이군요!",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020152.1.21",
          "form": "뭔가 오글거리는 구호!",
          "original_form": "뭔가 오글거리는 구호!",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020152.1.22",
          "form": "아시겠죠!",
          "original_form": "아시겠죠!",
          "speaker_id": "1",
          "time": "20210828 00:12"
        }
      ]
    }
