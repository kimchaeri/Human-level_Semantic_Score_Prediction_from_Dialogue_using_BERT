        },
        {
          "id": "MDRW2100039360.1.2",
          "form": "저녁에 뭐 먹을 거예요?",
          "original_form": "저녁에 뭐 먹을 거예요?",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039360.1.3",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.4",
          "form": "면류 먹지 말고 밥 먹어요",
          "original_form": "면류 먹지 말고 밥 먹어요",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.5",
          "form": "간단히 과일만 먹을까 싶기도 해요",
          "original_form": "간단히 과일만 먹을까 싶기도 해요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.6",
          "form": "과일로 끼니가 되나",
          "original_form": "과일로 끼니가 되나",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.7",
          "form": "과일도 많이 먹으면 괜찮더라구요",
          "original_form": "과일도 많이 먹으면 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.8",
          "form": "어떤 과일 좋아해요?",
          "original_form": "어떤 과일 좋아해요?",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.9",
          "form": "저는 과일은 다 좋아해요",
          "original_form": "저는 과일은 다 좋아해요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.11",
          "form": "어 근데 복숭아",
          "original_form": "어 근데 복숭아",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100039360.1.12",
          "form": "단 과일 아니면 신 과일?",
          "original_form": "단 과일 아니면 신 과일?",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039360.1.13",
          "form": "복숭아가 제일 좋은 거 같아요 생각해 보면",
          "original_form": "복숭아가 제일 좋은 거 같아요 생각해 보면",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039360.1.14",
          "form": "복숭아 맛있죠 단맛을 좋아하는군요",
          "original_form": "복숭아 맛있죠 단맛을 좋아하는군요",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039360.1.15",
          "form": "새콤한 것도 좋아요 새콤한 사과나 새콤한 키위 파인애플",
          "original_form": "새콤한 것도 좋아요 새콤한 사과나 새콤한 키위 파인애플",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100039360.1.16",
          "form": "복숭아가 참 좋은 과일이에요",
          "original_form": "복숭아가 참 좋은 과일이에요",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039360.1.17",
          "form": "포도나 체리도 좋고",
          "original_form": "포도나 체리도 좋고",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039360.1.18",
          "form": "괜히 옥황상제가 먹는 과일이라고 옛날 사람들이 불렀겠어요?",
          "original_form": "괜히 옥황상제가 먹는 과일이라고 옛날 사람들이 불렀겠어요?",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039360.1.19",
          "form": "맞아요 손오공이 훔쳐 먹다 벌 받았죠",
          "original_form": "맞아요 손오공이 훔쳐 먹다 벌 받았죠",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100039360.1.20",
          "form": "정말 다양한 과일을 다 좋아하는군요",
          "original_form": "정말 다양한 과일을 다 좋아하는군요",
          "speaker_id": "1",
          "time": "20210903 16:44"
        }
