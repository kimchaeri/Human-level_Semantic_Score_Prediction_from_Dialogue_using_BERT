      "utterance": [
        {
          "id": "MDRW2100043220.1.1",
          "form": "엄마 어제 몇시에 잤어",
          "original_form": "엄마 어제 몇시에 잤어",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043220.1.2",
          "form": "한 두시쯤?",
          "original_form": "한 두시쯤?",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043220.1.3",
          "form": "그때까지 중드봤고만",
          "original_form": "그때까지 중드봤고만",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043220.1.4",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043220.1.5",
          "form": "중드가 더 재밌어",
          "original_form": "중드가 더 재밌어",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043220.1.6",
          "form": "재미 없을거가튼데",
          "original_form": "재미 없을거가튼데",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043220.1.7",
          "form": "아니다 중드가 우리나라 꺼보다 요즘 더 재밌더라",
          "original_form": "아니다 중드가 우리나라 꺼보다 요즘 더 재밌더라",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043220.1.8",
          "form": "ㄲㅋㅋㅋ별거 없을 거 같은데",
          "original_form": "ㄲㅋㅋㅋ별거 없을 거 같은데",
          "speaker_id": "1",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100043220.1.9",
          "form": "재밌나보네",
          "original_form": "재밌나보네",
          "speaker_id": "1",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100043220.1.10",
          "form": "우리나라는 맨날 복수극 치정 이런거잖아",
          "original_form": "우리나라는 맨날 복수극 치정 이런거잖아",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043220.1.11",
          "form": "그런게 시청률 잘나와서 그런거아니야?",
          "original_form": "그런게 시청률 잘나와서 그런거아니야?",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043220.1.12",
          "form": "다 똑같고 질린다",
          "original_form": "다 똑같고 질린다",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043220.1.13",
          "form": "식상해서 재미없어",
          "original_form": "식상해서 재미없어",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043220.1.14",
          "form": "중드는 안그래?",
          "original_form": "중드는 안그래?",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043220.1.15",
          "form": "안 그래 완전 재밌어",
          "original_form": "안 그래 완전 재밌어",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043220.1.16",
          "form": "헐ㅋㅋㅋ대박이다",
          "original_form": "헐ㅋㅋㅋ대박이다",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043220.1.17",
          "form": "어제 하루 종일 본 거 아니야?",
          "original_form": "어제 하루 종일 본 거 아니야?",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043220.1.18",
          "form": "지금 16화 째다",
          "original_form": "지금 16화 째다",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043220.1.19",
          "form": "진짜 빠르다",
          "original_form": "진짜 빠르다",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043220.1.20",
          "form": "그것도 엄청 길지?",
          "original_form": "그것도 엄청 길지?",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043220.1.21",
          "form": "완전 길지",
          "original_form": "완전 길지",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043220.1.22",
          "form": "근데 금방 봐져",
          "original_form": "근데 금방 봐져",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043220.1.23",
          "form": "엄마 대단쓰",
          "original_form": "엄마 대단쓰",
          "speaker_id": "1",
          "time": "20210907 16:04"
        }
