      "utterance": [
        {
          "id": "MDRW2100044379.1.1",
          "form": "맞아 그거일수도 있지.. 근데 왜 여자애들은 큰데..",
          "original_form": "맞아 그거일수도 있지.. 근데 왜 여자애들은 큰데..",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044379.1.2",
          "form": "그니까 그 염색체가 문제라는거지",
          "original_form": "그니까 그 염색체가 문제라는거지",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044379.1.3",
          "form": "남자아이돌만 그럴까.. 아하!",
          "original_form": "남자아이돌만 그럴까.. 아하!",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044379.1.4",
          "form": "암튼ㅋㅋㅋ\r\n너 대탈출 이번에 한거 봤어?",
          "original_form": "암튼ㅋㅋㅋ\r\n너 대탈출 이번에 한거 봤어?",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044379.1.5",
          "form": "봤지! 근데 졸려서 좀 졸아서봤어ㅠㅠ 다시봐야할듯",
          "original_form": "봤지! 근데 졸려서 좀 졸아서봤어ㅠㅠ 다시봐야할듯",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044379.1.6",
          "form": "넌 봤어?",
          "original_form": "넌 봤어?",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044379.1.7",
          "form": "그거 볼때  졸았다고?",
          "original_form": "그거 볼때  졸았다고?",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044379.1.8",
          "form": "난 봤지!",
          "original_form": "난 봤지!",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044379.1.9",
          "form": "응 앞부분! 졸아서 휙휙 지나갔어ㅋㅋ",
          "original_form": "응 앞부분! 졸아서 휙휙 지나갔어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044379.1.10",
          "form": "근데 앞부분은 뭐..별거없었던거같아",
          "original_form": "근데 앞부분은 뭐..별거없었던거같아",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044379.1.11",
          "form": "김종민 빠지는게 최고 웃겼지ㅋㅋ",
          "original_form": "김종민 빠지는게 최고 웃겼지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044379.1.12",
          "form": "그래? 그나저나 이제 몇회 안남아서 벌써 아쉬워ㅠㅠ",
          "original_form": "그래? 그나저나 이제 몇회 안남아서 벌써 아쉬워ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044379.1.13",
          "form": "아ㅋㅋ 그건 봤지ㅋㅋ",
          "original_form": "아ㅋㅋ 그건 봤지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044379.1.14",
          "form": "진짜 심쿵했을듯ㅋㅋㅋ",
          "original_form": "진짜 심쿵했을듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044379.1.15",
          "form": "그니까 이제 이번시즌 끝이지?",
          "original_form": "그니까 이제 이번시즌 끝이지?",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044379.1.16",
          "form": "그 다음에  여고 추리반 나오려나?",
          "original_form": "그 다음에  여고 추리반 나오려나?",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044379.1.17",
          "form": "응 끝나면 내년에 또 하긴 할까? 근데 여자출연자로도 해줬으면..",
          "original_form": "응 끝나면 내년에 또 하긴 할까? 근데 여자출연자로도 해줬으면..",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044379.1.18",
          "form": "여고 추리반도 좋다!",
          "original_form": "여고 추리반도 좋다!",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044379.1.19",
          "form": "오 마자 여자출연자로도 해줬으면 좋겠다",
          "original_form": "오 마자 여자출연자로도 해줬으면 좋겠다",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044379.1.20",
          "form": "스케일에 비해 출연진이 아쉽지않냐구요\r\n너도 알고 나도 알고 우리 모두가 아는데ㅋㅋ",
          "original_form": "스케일에 비해 출연진이 아쉽지않냐구요\r\n너도 알고 나도 알고 우리 모두가 아는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044379.1.21",
          "form": "마자 진짜ㅠ 근데 계속 그대로 가겠지ㅠ",
          "original_form": "마자 진짜ㅠ 근데 계속 그대로 가겠지ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:37"
        }
      ]
    }
