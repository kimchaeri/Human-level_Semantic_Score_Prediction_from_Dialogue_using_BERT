      "utterance": [
        {
          "id": "MDRW2100012730.1.1",
          "form": "name3 데리고 자? ㅋㅋ",
          "original_form": "&name3& 데리고 자? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012730.1.2",
          "form": "아님 name3 집에서 혼자 자낭?",
          "original_form": "아님 &name3& 집에서 혼자 자낭?",
          "speaker_id": "1",
          "time": "20210825 00:28"
        },
        {
          "id": "MDRW2100012730.1.3",
          "form": "지가 자고싶은곳에서 자 ㅋㅋㅋㅋ",
          "original_form": "지가 자고싶은곳에서 자 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:03"
        },
        {
          "id": "MDRW2100012730.1.4",
          "form": "어쩔땐 옆에서 자고",
          "original_form": "어쩔땐 옆에서 자고",
          "speaker_id": "2",
          "time": "20210825 08:03"
        },
        {
          "id": "MDRW2100012730.1.5",
          "form": "아님 거실에서도 자고",
          "original_form": "아님 거실에서도 자고",
          "speaker_id": "2",
          "time": "20210825 08:03"
        },
        {
          "id": "MDRW2100012730.1.6",
          "form": "ㅋㅋname3 너무 귀엽네",
          "original_form": "ㅋㅋ&name3& 너무 귀엽네{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 08:08"
        },
        {
          "id": "MDRW2100012730.1.7",
          "form": "낮에 다들 없을땐\r\nname4랑 어떻게 지내려나 궁금ㅋ",
          "original_form": "낮에 다들 없을땐\r\n&name4&랑 어떻게 지내려나 궁금ㅋ",
          "speaker_id": "1",
          "time": "20210825 08:08"
        },
        {
          "id": "MDRW2100012730.1.8",
          "form": "호야 방에서 잘안나왕 ㅜㅠㅠ ㅋㅋ",
          "original_form": "호야 방에서 잘안나왕 ㅜㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:23"
        },
        {
          "id": "MDRW2100012730.1.9",
          "form": "문앞에서 바라보다가",
          "original_form": "문앞에서 바라보다가",
          "speaker_id": "2",
          "time": "20210825 08:24"
        },
        {
          "id": "MDRW2100012730.1.10",
          "form": "짖으면 들어가 ㅋㅋㅋ",
          "original_form": "짖으면 들어가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:24"
        },
        {
          "id": "MDRW2100012730.1.11",
          "form": "나도 낮에없을때 어떨지궁금..",
          "original_form": "나도 낮에없을때 어떨지궁금..",
          "speaker_id": "2",
          "time": "20210825 08:24"
        },
        {
          "id": "MDRW2100012730.1.12",
          "form": "힝ㅠ name4 오또케 ㅜㅜ",
          "original_form": "힝ㅠ &name4& 오또케 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 08:37"
        },
        {
          "id": "MDRW2100012730.1.13",
          "form": "집에 그런거 홈캠? \r\n설치하면 재밌겠당ㅋㅋ",
          "original_form": "집에 그런거 홈캠? \r\n설치하면 재밌겠당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 08:37"
        },
        {
          "id": "MDRW2100012730.1.14",
          "form": "안그래도 그거 하고싶긴해유 ㅋㅋㅋㅋㅋ",
          "original_form": "안그래도 그거 하고싶긴해유 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012730.1.15",
          "form": "김팀장님 통해서 우리\r\nname3랑 name4를 위해\r\n하나 가입하시죠ㅋㅋㅋ",
          "original_form": "김팀장님 통해서 우리\r\n&name3&랑 &name4&를 위해\r\n하나 가입하시죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:41"
        },
        {
          "id": "MDRW2100012730.1.16",
          "form": "혹시 강아지 용품 필요하세요?",
          "original_form": "혹시 강아지 용품 필요하세요?",
          "speaker_id": "1",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100012730.1.17",
          "form": "김팀장ㅋㅋㅋㅋ",
          "original_form": "김팀장ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100012730.1.18",
          "form": "음 안그래도 사야될거있긴해유",
          "original_form": "음 안그래도 사야될거있긴해유",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100012730.1.19",
          "form": "강아지티슈랑...뭐..쿠션이랑..",
          "original_form": "강아지티슈랑...뭐..쿠션이랑..",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100012730.1.20",
          "form": "강아지 샴푸도 필요하실까요?",
          "original_form": "강아지 샴푸도 필요하실까요?",
          "speaker_id": "1",
          "time": "20210825 12:00"
        },
        {
          "id": "MDRW2100012730.1.21",
          "form": "샘푸는있오요",
          "original_form": "샘푸는있오요",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012730.1.22",
          "form": "목욕시켜야되는뎅",
          "original_form": "목욕시켜야되는뎅",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100012730.1.23",
          "form": "아 그럼 샴푸는 필요 없으세요? ㅠ",
          "original_form": "아 그럼 샴푸는 필요 없으세요? ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100012730.1.24",
          "form": "왜욤?? ㅋㅋ",
          "original_form": "왜욤?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100012730.1.25",
          "form": "샴푸",
          "original_form": "샴푸",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100012730.1.26",
          "form": "일단 조그만한거사긴했는뎅",
          "original_form": "일단 조그만한거사긴했는뎅",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100012730.1.27",
          "form": "샴푸 보내드리려구염ㅋㅋ",
          "original_form": "샴푸 보내드리려구염ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100012730.1.28",
          "form": "넹???ㅋㅋ",
          "original_form": "넹???ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100012730.1.29",
          "form": "뭐 행사해여??",
          "original_form": "뭐 행사해여??",
          "speaker_id": "2",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100012730.1.30",
          "form": "샴푸 집으로 보내드릴게용",
          "original_form": "샴푸 집으로 보내드릴게용",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100012730.1.31",
          "form": "혹시 강아지 보습제는요?",
          "original_form": "혹시 강아지 보습제는요?",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100012730.1.32",
          "form": "아 보습제는 아직 안샀아용ㅁ",
          "original_form": "아 보습제는 아직 안샀아용ㅁ",
          "speaker_id": "2",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100012730.1.33",
          "form": "그럼 보습제도 같이 보내드릴게염ㅋㅋ",
          "original_form": "그럼 보습제도 같이 보내드릴게염ㅋㅋ",
