      "utterance": [
        {
          "id": "MDRW2100040935.1.1",
          "form": "한참 휴롬 유행할때 사볼까 했는데",
          "original_form": "한참 휴롬 유행할때 사볼까 했는데",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.2",
          "form": "홈카페 개장 완전 가능인데",
          "original_form": "홈카페 개장 완전 가능인데",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.3",
          "form": "1인 가구한테는 버거울 것 같더라구요",
          "original_form": "1인 가구한테는 버거울 것 같더라구요",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.4",
          "form": "크기보고 맘을 접었어요",
          "original_form": "크기보고 맘을 접었어요",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.5",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.6",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.7",
          "form": "놓을데가 없어요!",
          "original_form": "놓을데가 없어요!",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.8",
          "form": "그리고 가격도 만만치 않더라구요!!!",
          "original_form": "그리고 가격도 만만치 않더라구요!!!",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040935.1.9",
          "form": "근데 착즙기는 즙 짜내고나면",
          "original_form": "근데 착즙기는 즙 짜내고나면",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040935.1.10",
          "form": "건더기는 버리게 되는걸까요?!",
          "original_form": "건더기는 버리게 되는걸까요?!",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040935.1.11",
          "form": "이런거보면 그냥 믹서기가 나으려나,,,",
          "original_form": "이런거보면 그냥 믹서기가 나으려나,,,",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040935.1.12",
          "form": "음식물찌꺼기로 버리던가 밭이나 이런데 뿌린다고 하더라구요?",
          "original_form": "음식물찌꺼기로 버리던가 밭이나 이런데 뿌린다고 하더라구요?",
          "speaker_id": "2",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040935.1.13",
          "form": "아~~~",
          "original_form": "아~~~",
          "speaker_id": "1",
          "time": "20210905 13:17"
        },
        {
          "id": "MDRW2100040935.1.14",
          "form": "아니면 당근이나 그런거 있잖아요",
          "original_form": "아니면 당근이나 그런거 있잖아요",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.15",
          "form": "그러면 섬유질은 어느정도 못먹게 되는거겠네요?!",
          "original_form": "그러면 섬유질은 어느정도 못먹게 되는거겠네요?!",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.16",
          "form": "먹을수 있눈거",
          "original_form": "먹을수 있눈거",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.17",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.18",
          "form": "그런건 계란말이에 넣기도 한다는데",
          "original_form": "그런건 계란말이에 넣기도 한다는데",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.19",
          "form": "오 우와우와",
          "original_form": "오 우와우와",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.20",
          "form": "잘 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "잘 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.21",
          "form": "당근한테서도 즙을 짜낼 수 있다는 게",
          "original_form": "당근한테서도 즙을 짜낼 수 있다는 게",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.22",
          "form": "너무 신기하네요",
          "original_form": "너무 신기하네요",
          "speaker_id": "1",
          "time": "20210905 13:18"
        },
        {
          "id": "MDRW2100040935.1.23",
          "form": "믹서기에만 넣을 생각을 했는데!",
          "original_form": "믹서기에만 넣을 생각을 했는데!",
          "speaker_id": "1",
          "time": "20210905 13:18"
        }
