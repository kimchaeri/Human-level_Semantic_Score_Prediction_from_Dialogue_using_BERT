      "utterance": [
        {
          "id": "MDRW2100046013.1.1",
          "form": "저도 혼자 여행 조아해요 ㅎㅎ",
          "original_form": "저도 혼자 여행 조아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100046013.1.2",
          "form": "오 혼자여행 좋아하시는구나",
          "original_form": "오 혼자여행 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100046013.1.3",
          "form": "어디어디 혼자 다녀오셨아요?",
          "original_form": "어디어디 혼자 다녀오셨아요?",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100046013.1.4",
          "form": "전 통영 한군데만 ㅠㅠ",
          "original_form": "전 통영 한군데만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100046013.1.5",
          "form": "국내선 속초만 혼자 가봤어요 :)",
          "original_form": "국내선 속초만 혼자 가봤어요 :)",
          "speaker_id": "2",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100046013.1.6",
          "form": "오오 속초",
          "original_form": "오오 속초",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046013.1.7",
          "form": "속초 좋으셨나요?",
          "original_form": "속초 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046013.1.8",
          "form": "네 좋았어요 ㅎㅎ",
          "original_form": "네 좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046013.1.9",
          "form": "저는 속초는 그냥 그냥 다녀왔던 것 같아요",
          "original_form": "저는 속초는 그냥 그냥 다녀왔던 것 같아요",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046013.1.10",
          "form": "근데 바다는 역시 너무 ㅇㅖ쁘더라구요",
          "original_form": "근데 바다는 역시 너무 ㅇㅖ쁘더라구요",
          "speaker_id": "1",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046013.1.11",
          "form": "아하 그렇구나 카페가 좋았어요 속초는 ㅎㅎ",
          "original_form": "아하 그렇구나 카페가 좋았어요 속초는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046013.1.12",
          "form": "강릉이 속초보다 좋은 이유가 뭔지 여쭤봐두될까요?",
          "original_form": "강릉이 속초보다 좋은 이유가 뭔지 여쭤봐두될까요?",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046013.1.13",
          "form": ":)",
          "original_form": ":)",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046013.1.14",
          "form": "전 강릉에서 1박 한적은 없어서 잘 몰라요",
          "original_form": "전 강릉에서 1박 한적은 없어서 잘 몰라요",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046013.1.15",
          "form": "앗 별건 아니구",
          "original_form": "앗 별건 아니구",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046013.1.16",
          "form": "그냥 강릉이 더 여행지스러웠어요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "그냥 강릉이 더 여행지스러웠어요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046013.1.17",
          "form": "그리고 그 카페거리 가는 길에 소나무 길이 있거든요",
          "original_form": "그리고 그 카페거리 가는 길에 소나무 길이 있거든요",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100046013.1.18",
          "form": "제가 정말 좋아하는 길이에요..",
          "original_form": "제가 정말 좋아하는 길이에요..",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100046013.1.19",
          "form": "거기서 노래들으면서 걸으면 정말 행복해져요",
          "original_form": "거기서 노래들으면서 걸으면 정말 행복해져요",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100046013.1.20",
          "form": "우아 그래요??",
          "original_form": "우아 그래요??",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046013.1.21",
          "form": "꼭 가볼게요 ㅎㅎㅎㅎ",
          "original_form": "꼭 가볼게요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046013.1.22",
          "form": "말씀해주신대로 꼭 해볼게요",
          "original_form": "말씀해주신대로 꼭 해볼게요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046013.1.23",
          "form": "네네 정말 추천드려요 ㅎㅎ",
          "original_form": "네네 정말 추천드려요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046013.1.24",
          "form": "소나무길 걷는 옆에 바다도 있거든요..",
          "original_form": "소나무길 걷는 옆에 바다도 있거든요..",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046013.1.25",
          "form": "진짜 멋지겠어요",
          "original_form": "진짜 멋지겠어요",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046013.1.26",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046013.1.27",
          "form": "넵 ㅎㅎ",
          "original_form": "넵 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046013.1.28",
          "form": "속초 카페 추천 부탁드려도 될까요?",
          "original_form": "속초 카페 추천 부탁드려도 될까요?",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046013.1.29",
          "form": "어나더블루요",
          "original_form": "어나더블루요",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046013.1.30",
