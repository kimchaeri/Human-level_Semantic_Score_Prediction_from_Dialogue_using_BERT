      "utterance": [
        {
          "id": "MDRW2100035824.1.1",
          "form": "건강을 위해서요!ㅎㅎ",
          "original_form": "건강을 위해서요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:45"
        },
        {
          "id": "MDRW2100035824.1.2",
          "form": "남자친구분은 있으세용?",
          "original_form": "남자친구분은 있으세용?",
          "speaker_id": "1",
          "time": "20210830 03:45"
        },
        {
          "id": "MDRW2100035824.1.3",
          "form": "아뇨 ㅋㅋㅋ 저는 솔로에여",
          "original_form": "아뇨 ㅋㅋㅋ 저는 솔로에여",
          "speaker_id": "2",
          "time": "20210830 03:45"
        },
        {
          "id": "MDRW2100035824.1.4",
          "form": "저두 솔로에요ㅎㅎ",
          "original_form": "저두 솔로에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:45"
        },
        {
          "id": "MDRW2100035824.1.5",
          "form": "마지막 연애가 4년전쯤인데 좀 길죠ㅠㅠ",
          "original_form": "마지막 연애가 4년전쯤인데 좀 길죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 03:45"
        },
        {
          "id": "MDRW2100035824.1.6",
          "form": "ㅋㅋㅋㅋ 에이 또 누군가 생기겟져 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 에이 또 누군가 생기겟져 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:45"
        },
        {
          "id": "MDRW2100035824.1.7",
          "form": "소개팅은 하세요?",
          "original_form": "소개팅은 하세요?",
          "speaker_id": "1",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.8",
          "form": "저는 자연스러운 만남 추구합니다ㅎㅎㅎ",
          "original_form": "저는 자연스러운 만남 추구합니다ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.9",
          "form": "기회가 잇엇는데 안해여 ㅋㅋㅋ",
          "original_form": "기회가 잇엇는데 안해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.10",
          "form": "ㅋㅋㅋㅋㅋ맞아요 저도 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 저도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.11",
          "form": "뭔가 판 깔아주면",
          "original_form": "뭔가 판 깔아주면",
          "speaker_id": "1",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.13",
          "form": "부담스럽고 뚝딱이돼고 그렇더라구요ㅋㅋㅋ",
          "original_form": "부담스럽고 뚝딱이돼고 그렇더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.14",
          "form": "이상형은 어떻게 되세용?",
          "original_form": "이상형은 어떻게 되세용?",
          "speaker_id": "1",
          "time": "20210830 03:46"
        },
        {
          "id": "MDRW2100035824.1.15",
          "form": "ㅋㅋㅋㅋ 저는 딱히 생각해둔게엊ㅅ어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저는 딱히 생각해둔게엊ㅅ어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.16",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.17",
          "form": "진짜 내면이나 성품을 중요시하시는 분들이",
          "original_form": "진짜 내면이나 성품을 중요시하시는 분들이",
          "speaker_id": "1",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.18",
          "form": "이상형 없더라구요ㅋㅋㅋ",
          "original_form": "이상형 없더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.19",
          "form": "저는 똑똑한 사람이요!",
          "original_form": "저는 똑똑한 사람이요!",
          "speaker_id": "1",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아닌데여 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아닌데여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.21",
          "form": "저도 외모지상주의자에여 ㅋㅋㅋㅋ",
          "original_form": "저도 외모지상주의자에여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:47"
        },
        {
          "id": "MDRW2100035824.1.22",
          "form": "앗 아닌가요?ㅋㅋㅋㅋ 잘생기면",
          "original_form": "앗 아닌가요?ㅋㅋㅋㅋ 잘생기면",
