      "utterance": [
        {
          "id": "MDRW2100030439.1.1",
          "form": "롤하는데",
          "original_form": "롤하는데",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030439.1.2",
          "form": "미션이 일종에 미끼죠",
          "original_form": "미션이 일종에 미끼죠",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030439.1.3",
          "form": "안하면 찜찜 하죠",
          "original_form": "안하면 찜찜 하죠",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030439.1.4",
          "form": "스킨을 매번 만들어서 팔아요",
          "original_form": "스킨을 매번 만들어서 팔아요",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030439.1.5",
          "form": "맞아요 인기가 없어서 빨리 못 해도 미션 클리어 되더라고요",
          "original_form": "맞아요 인기가 없어서 빨리 못 해도 미션 클리어 되더라고요",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.6",
          "form": "스킨은 옷이죠",
          "original_form": "스킨은 옷이죠",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.7",
          "form": "스킨 하나 만들면 거기에 색깔만 다른 거 여러 개 입혀서 돈 받고 팔더라고요",
          "original_form": "스킨 하나 만들면 거기에 색깔만 다른 거 여러 개 입혀서 돈 받고 팔더라고요",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.8",
          "form": "그걸 크롬 하라고 하는데 진짜 너무 심해요",
          "original_form": "그걸 크롬 하라고 하는데 진짜 너무 심해요",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.9",
          "form": "미션의 노예",
          "original_form": "미션의 노예",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.10",
          "form": "살 수도 없고 안 살 수도 없고",
          "original_form": "살 수도 없고 안 살 수도 없고",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.11",
          "form": "두 세계의 살바엔 세트로 사면 한 번에네 다섯 개도 사거든요",
          "original_form": "두 세계의 살바엔 세트로 사면 한 번에네 다섯 개도 사거든요",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.12",
          "form": "노예 이군요",
          "original_form": "노예 이군요",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030439.1.13",
          "form": "뭐 스킨 충입니다",
          "original_form": "뭐 스킨 충입니다",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.14",
          "form": "무슨 말인지 모르겠지만",
          "original_form": "무슨 말인지 모르겠지만",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.15",
          "form": "복잡하네요",
          "original_form": "복잡하네요",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.16",
          "form": "하나의 디자인에 색깔만 다르게 입히는 거죠",
          "original_form": "하나의 디자인에 색깔만 다르게 입히는 거죠",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.17",
          "form": "색깔별로 돈 받고 팝니다",
          "original_form": "색깔별로 돈 받고 팝니다",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.18",
          "form": "저도 맨날 물고기랑 놉니다",
          "original_form": "저도 맨날 물고기랑 놉니다",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.19",
          "form": "한 때는 꽃을 키웠죠",
          "original_form": "한 때는 꽃을 키웠죠",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.20",
          "form": "색깔 다섯 개 사는 가격 인하 따로 3개 사는 가격 인하 같아요",
          "original_form": "색깔 다섯 개 사는 가격 인하 따로 3개 사는 가격 인하 같아요",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.21",
          "form": "많이 키웠죠",
          "original_form": "많이 키웠죠",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030439.1.22",
          "form": "물고기 잡는 거 재밌죠",
          "original_form": "물고기 잡는 거 재밌죠",
          "speaker_id": "2",
          "time": "20210904 01:03"
        }
