      "utterance": [
        {
          "id": "MDRW2100019326.1.1",
          "form": "네 여기도 엄청 저렴한편은 아니지만",
          "original_form": "네 여기도 엄청 저렴한편은 아니지만",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019326.1.2",
          "form": "오오오옹 가격중요해요",
          "original_form": "오오오옹 가격중요해요",
          "speaker_id": "1",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019326.1.3",
          "form": "가격대비 괜찮고",
          "original_form": "가격대비 괜찮고",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019326.1.4",
          "form": "괜찮습니다!",
          "original_form": "괜찮습니다!",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019326.1.5",
          "form": "나인에서도 옷 봐야겟어요",
          "original_form": "나인에서도 옷 봐야겟어요",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019326.1.6",
          "form": "ㅋㅋㅋㅋ저는 나인도 강추해요",
          "original_form": "ㅋㅋㅋㅋ저는 나인도 강추해요",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019326.1.7",
          "form": "덕분에 소핑사이트 많이 알아가네요",
          "original_form": "덕분에 소핑사이트 많이 알아가네요",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019326.1.8",
          "form": "옿 뿌뜻해여 ㅋㅋㅋㅋ",
          "original_form": "옿 뿌뜻해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019326.1.9",
          "form": "찐으로 구매해보고 다음에 후기남길게요",
          "original_form": "찐으로 구매해보고 다음에 후기남길게요",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019326.1.10",
          "form": "오오오 기대되네요 ㅋㅋㅋ 뭐 살건가요?",
          "original_form": "오오오 기대되네요 ㅋㅋㅋ 뭐 살건가요?",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019326.1.11",
          "form": "저는 아까 말한 맨투맨이랑 신발.......",
          "original_form": "저는 아까 말한 맨투맨이랑 신발.......",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019326.1.12",
          "form": "좋으 구매하시길 바랄게요.......",
          "original_form": "좋으 구매하시길 바랄게요.......",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019326.1.13",
          "form": "감사합니다ㅋㅋㅋㅋㅋ 꼭 건지고 싶어요 ㅜ 쇼핑이 고프네요",
          "original_form": "감사합니다ㅋㅋㅋㅋㅋ 꼭 건지고 싶어요 ㅜ 쇼핑이 고프네요",
