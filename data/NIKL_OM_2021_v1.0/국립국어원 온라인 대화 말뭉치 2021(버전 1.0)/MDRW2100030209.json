      "utterance": [
        {
          "id": "MDRW2100030209.1.1",
          "form": "음..오늘은 대학 교재 삿어",
          "original_form": "음..오늘은 대학 교재 삿어",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.2",
          "form": "사는김에 보고싶은책도 삿어 ㅋ",
          "original_form": "사는김에 보고싶은책도 삿어 ㅋ",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.3",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.4",
          "form": "소설이겠지?",
          "original_form": "소설이겠지?",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.5",
          "form": "웅 근데 보고싶은소설책 사는데에 대학교재보다 돈을 더 썻엉 ㅎㅎ",
          "original_form": "웅 근데 보고싶은소설책 사는데에 대학교재보다 돈을 더 썻엉 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.6",
          "form": "잘햇지?",
          "original_form": "잘햇지?",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.7",
          "form": "그렇겠지 뭐 ㅋㅋ",
          "original_form": "그렇겠지 뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.8",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.9",
          "form": "맞아 난 멋져 ㅎㅎ",
          "original_form": "맞아 난 멋져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.10",
          "form": "역시 사는건",
          "original_form": "역시 사는건",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030209.1.11",
          "form": "flex를 해야해",
          "original_form": "flex를 해야해",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.12",
          "form": "어차피 안쓰면",
          "original_form": "어차피 안쓰면",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.13",
          "form": "중고로 팔면되는거야",
          "original_form": "중고로 팔면되는거야",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.14",
          "form": "맞아 사고싶은걸 사는게 중요하지 ㅎㅎ",
          "original_form": "맞아 사고싶은걸 사는게 중요하지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.15",
          "form": "나중에 절판이라도 되면",
          "original_form": "나중에 절판이라도 되면",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.16",
          "form": "영영못볼수도 잇으니깐",
          "original_form": "영영못볼수도 잇으니깐",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.17",
          "form": "맞아 슬프겠지 ㅎㅎ",
          "original_form": "맞아 슬프겠지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.18",
          "form": "내가 좋아하는책도",
          "original_form": "내가 좋아하는책도",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.19",
          "form": "갖고싶을때 사야돼",
          "original_form": "갖고싶을때 사야돼",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100030209.1.20",
          "form": "절판된적 있어..",
          "original_form": "절판된적 있어..",
          "speaker_id": "2",
          "time": "20210903 22:59"
        },
