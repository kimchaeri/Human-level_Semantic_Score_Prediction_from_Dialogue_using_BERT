      "utterance": [
        {
          "id": "MDRW2100016131.1.1",
          "form": "다이어트 경험 있으신가요",
          "original_form": "다이어트 경험 있으신가요",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016131.1.2",
          "form": "평생의 숙제죠",
          "original_form": "평생의 숙제죠",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016131.1.3",
          "form": "그렇죠 저도 늘 생각만..^^;",
          "original_form": "그렇죠 저도 늘 생각만..^^;",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016131.1.4",
          "form": "어제 그래서 운동을 하려고 필라테스를 등록했어요",
          "original_form": "어제 그래서 운동을 하려고 필라테스를 등록했어요",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100016131.1.5",
          "form": "오 몇대몇으로 받으시나요",
          "original_form": "오 몇대몇으로 받으시나요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100016131.1.6",
          "form": "저 개인강습 우선해보려고요 워낙 운동을 못하기도하고..",
          "original_form": "저 개인강습 우선해보려고요 워낙 운동을 못하기도하고..",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100016131.1.7",
          "form": "오 그럼 수강료가 꽤 비싸지않나요",
          "original_form": "오 그럼 수강료가 꽤 비싸지않나요",
          "speaker_id": "2",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100016131.1.8",
          "form": "그냥 투자아 생각하고",
          "original_form": "그냥 투자아 생각하고",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100016131.1.9",
          "form": "회당 얼마인지 여쭤봐도  될까요",
          "original_form": "회당 얼마인지 여쭤봐도  될까요",
          "speaker_id": "2",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100016131.1.10",
          "form": "7만원이이요 옷까지 구매하려니 초기비용이 좀 들더라고요",
          "original_form": "7만원이이요 옷까지 구매하려니 초기비용이 좀 들더라고요",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100016131.1.11",
          "form": "와 진짜 비싸요~ 운동복은 어디서 사셨나요",
          "original_form": "와 진짜 비싸요~ 운동복은 어디서 사셨나요",
          "speaker_id": "2",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100016131.1.12",
          "form": "안다르랑 제시믹스에서 샀어요",
          "original_form": "안다르랑 제시믹스에서 샀어요",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100016131.1.13",
          "form": "양말도 한켤레에 6000원씩이에요",
          "original_form": "양말도 한켤레에 6000원씩이에요",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100016131.1.14",
          "form": "유명한건 더 비싸더라고요",
          "original_form": "유명한건 더 비싸더라고요",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100016131.1.15",
          "form": "그죠! 양말도 왜이리 비싼지 놀랐어요..ㅋ",
          "original_form": "그죠! 양말도 왜이리 비싼지 놀랐어요..ㅋ",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100016131.1.16",
          "form": "그러게요 무좀량말같은데",
          "original_form": "그러게요 무좀량말같은데",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100016131.1.17",
          "form": "여튼 운동열시미 해보려고요",
          "original_form": "여튼 운동열시미 해보려고요",
