      "utterance": [
        {
          "id": "MDRW2100023355.1.1",
          "form": "최근에 본 영화 이야기를 해볼까요?",
          "original_form": "최근에 본 영화 이야기를 해볼까요?",
          "speaker_id": "2",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100023355.1.2",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "1",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100023355.1.3",
          "form": "저는 블랙위도우 봤어요",
          "original_form": "저는 블랙위도우 봤어요",
          "speaker_id": "2",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100023355.1.4",
          "form": "마블 영화죠?",
          "original_form": "마블 영화죠?",
          "speaker_id": "1",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100023355.1.5",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100023355.1.6",
          "form": "그거랑 크루엘라요!",
          "original_form": "그거랑 크루엘라요!",
          "speaker_id": "2",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100023355.1.7",
          "form": "크루엘라는 디즈니라고 하더라구요",
          "original_form": "크루엘라는 디즈니라고 하더라구요",
          "speaker_id": "1",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100023355.1.8",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 08:14"
        },
        {
          "id": "MDRW2100023355.1.9",
          "form": "영화관에서 보셨겠네요!",
          "original_form": "영화관에서 보셨겠네요!",
          "speaker_id": "1",
          "time": "20210830 08:14"
        },
        {
          "id": "MDRW2100023355.1.10",
          "form": "저는 영화관 안간지",
          "original_form": "저는 영화관 안간지",
          "speaker_id": "1",
          "time": "20210830 08:14"
        },
        {
          "id": "MDRW2100023355.1.11",
          "form": "오래랍니다",
          "original_form": "오래랍니다",
          "speaker_id": "1",
          "time": "20210830 08:14"
        },
        {
          "id": "MDRW2100023355.1.12",
          "form": "코로나 때문에",
          "original_form": "코로나 때문에",
          "speaker_id": "2",
          "time": "20210830 08:15"
        },
        {
          "id": "MDRW2100023355.1.13",
          "form": "안가시는거군요",
          "original_form": "안가시는거군요",
          "speaker_id": "2",
          "time": "20210830 08:15"
        },
        {
          "id": "MDRW2100023355.1.14",
          "form": "이해해요!",
          "original_form": "이해해요!",
          "speaker_id": "2",
          "time": "20210830 08:15"
        },
        {
          "id": "MDRW2100023355.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210830 08:16"
        },
        {
          "id": "MDRW2100023355.1.16",
          "form": "그래도 전시회 같은데는",
          "original_form": "그래도 전시회 같은데는",
          "speaker_id": "1",
          "time": "20210830 08:16"
        },
        {
          "id": "MDRW2100023355.1.17",
          "form": "잘 다녀요",
          "original_form": "잘 다녀요",
          "speaker_id": "1",
          "time": "20210830 08:16"
        },
        {
          "id": "MDRW2100023355.1.18",
          "form": "다녀오신 데 있으세요?",
          "original_form": "다녀오신 데 있으세요?",
          "speaker_id": "2",
          "time": "20210830 08:16"
        },
        {
          "id": "MDRW2100023355.1.19",
          "form": "이건희 기증한 데요!",
          "original_form": "이건희 기증한 데요!",
          "speaker_id": "1",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100023355.1.20",
          "form": "국립중앙박물관",
          "original_form": "국립중앙박물관",
          "speaker_id": "2",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100023355.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100023355.1.22",
          "form": "국중박이요!!!",
          "original_form": "국중박이요!!!",
          "speaker_id": "1",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100023355.1.23",
          "form": "우와 그거 티켓팅",
          "original_form": "우와 그거 티켓팅",
          "speaker_id": "2",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100023355.1.24",
          "form": "어렵다던데!!!",
          "original_form": "어렵다던데!!!",
          "speaker_id": "2",
          "time": "20210830 08:17"
        },
        {
          "id": "MDRW2100023355.1.25",
          "form": "네 근데",
          "original_form": "네 근데",
          "speaker_id": "1",
          "time": "20210830 08:18"
        },
        {
          "id": "MDRW2100023355.1.26",
