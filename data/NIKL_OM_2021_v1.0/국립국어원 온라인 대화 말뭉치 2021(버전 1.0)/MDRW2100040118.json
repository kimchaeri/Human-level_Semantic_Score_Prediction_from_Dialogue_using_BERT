      "utterance": [
        {
          "id": "MDRW2100040118.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 10:42"
        },
        {
          "id": "MDRW2100040118.1.2",
          "form": "안녕하세요~ 직장인이신가요?",
          "original_form": "안녕하세요~ 직장인이신가요?",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100040118.1.3",
          "form": "네네 지금도 회사입니다",
          "original_form": "네네 지금도 회사입니다",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100040118.1.4",
          "form": "토요일인데도요??@",
          "original_form": "토요일인데도요??@",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100040118.1.5",
          "form": "네네 오늘 당직 근무에요ㅠ",
          "original_form": "네네 오늘 당직 근무에요ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100040118.1.6",
          "form": "아이고ㅜㅜㅜ 고생하시네요",
          "original_form": "아이고ㅜㅜㅜ 고생하시네요",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100040118.1.7",
          "form": "아침은 드셨나요",
          "original_form": "아침은 드셨나요",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100040118.1.8",
          "form": "토마토주스 먹었어요",
          "original_form": "토마토주스 먹었어요",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100040118.1.9",
          "form": "드셨나요",
          "original_form": "드셨나요",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100040118.1.10",
          "form": "학생이신가용?",
          "original_form": "학생이신가용?",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100040118.1.11",
          "form": "안녕 저도 직장 다니고 있어요 ㅎㅎ",
          "original_form": "안녕 저도 직장 다니고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040118.1.12",
          "form": "안녕이 아니라... 아니요...",
          "original_form": "안녕이 아니라... 아니요...",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040118.1.13",
          "form": "라고 보냈는데 폰이 지맘대로 바꿔서 보내버렸네요...?",
          "original_form": "라고 보냈는데 폰이 지맘대로 바꿔서 보내버렸네요...?",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040118.1.14",
          "form": "직장 어떠세요",
          "original_form": "직장 어떠세요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100040118.1.15",
          "form": "ㅎㅎ싫지만... 돈주니까 갑니다...",
          "original_form": "ㅎㅎ싫지만... 돈주니까 갑니다...",
          "speaker_id": "2",
          "time": "20210904 11:49"
        },
        {
          "id": "MDRW2100040118.1.16",
          "form": "저도그렇습니다 ㅋㅋ",
          "original_form": "저도그렇습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040118.1.17",
          "form": "당직근무는 어떤 일 하시는 거예요?",
          "original_form": "당직근무는 어떤 일 하시는 거예요?",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040118.1.18",
          "form": "민원 전화 처리하고 사옥 순찰하고 그래요",
          "original_form": "민원 전화 처리하고 사옥 순찰하고 그래요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040118.1.19",
          "form": "아 혹시 업무가 어떤 쪽이세요?",
          "original_form": "아 혹시 업무가 어떤 쪽이세요?",
