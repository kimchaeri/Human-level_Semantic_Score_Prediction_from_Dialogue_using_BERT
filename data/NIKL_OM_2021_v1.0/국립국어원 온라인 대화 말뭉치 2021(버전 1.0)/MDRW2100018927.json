      "utterance": [
        {
          "id": "MDRW2100018927.1.1",
          "form": "기습 제출",
          "original_form": "기습 제출",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.2",
          "form": "조심 ㅎㅎ",
          "original_form": "조심 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.3",
          "form": "축하합니다",
          "original_form": "축하합니다",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.4",
          "form": "눼",
          "original_form": "눼",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.5",
          "form": "축하합니다",
          "original_form": "축하합니다",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.6",
          "form": "뭐야 무서워요",
          "original_form": "뭐야 무서워요",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.7",
          "form": "당신의 개  강을 축하합니다",
          "original_form": "당신의 개  강을 축하합니다",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018927.1.8",
          "form": "참고로",
          "original_form": "참고로",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.9",
          "form": "개 뭐시기",
          "original_form": "개 뭐시기",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.10",
          "form": "다 걸러져요",
          "original_form": "다 걸러져요",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.11",
          "form": "오늘 마음 따뜻해지는 소식을 들었어요",
          "original_form": "오늘 마음 따뜻해지는 소식을 들었어요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.12",
          "form": "비대면...?",
          "original_form": "비대면...?",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.13",
          "form": "토론하는 비대면 강의 있었는데",
          "original_form": "토론하는 비대면 강의 있었는데",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.14",
          "form": "동영상 강의로 바뀌어서 그나마 듣는 2개 다 동영상 강의가 되었어요",
          "original_form": "동영상 강의로 바뀌어서 그나마 듣는 2개 다 동영상 강의가 되었어요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.15",
          "form": "대면 가나요",
          "original_form": "대면 가나요",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.16",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.17",
          "form": "저를 저주하시낭ㆍ",
          "original_form": "저를 저주하시낭ㆍ",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018927.1.18",
          "form": "꿀이네",
          "original_form": "꿀이네",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.19",
          "form": "저주하시나요",
          "original_form": "저주하시나요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.20",
          "form": "쳇",
          "original_form": "쳇",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.21",
          "form": "까비",
          "original_form": "까비",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.22",
          "form": "장난",
          "original_form": "장난",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.23",
          "form": "저는 행복합니다..^^",
          "original_form": "저는 행복합니다..^^",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.24",
          "form": "그거 아세요",
          "original_form": "그거 아세요",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.25",
          "form": "백신공결",
          "original_form": "백신공결",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.26",
          "form": "내년에 복학하시는 거 잊지 않았죠?",
          "original_form": "내년에 복학하시는 거 잊지 않았죠?",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.27",
          "form": "공지사항에 있던데",
          "original_form": "공지사항에 있던데",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.28",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.29",
          "form": "저 그거 하루는 해당될 것 같아요",
          "original_form": "저 그거 하루는 해당될 것 같아요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.30",
          "form": "학교 휴학 할래요",
          "original_form": "학교 휴학 할래요",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018927.1.31",
          "form": "2년 할래요",
          "original_form": "2년 할래요",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018927.1.32",
          "form": "좋네요 공결",
          "original_form": "좋네요 공결",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018927.1.33",
          "form": "어째서 2년이죠",
          "original_form": "어째서 2년이죠",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018927.1.34",
          "form": "휴학 한 번 해보세요",
          "original_form": "휴학 한 번 해보세요",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018927.1.35",
          "form": "인생이 맑아져요",
          "original_form": "인생이 맑아져요",
          "speaker_id": "1",
          "time": "20210827 13:42"
        }
      ]
