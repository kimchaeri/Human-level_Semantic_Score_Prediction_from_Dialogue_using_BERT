      "utterance": [
        {
          "id": "MDRW2100018083.1.1",
          "form": "출근하기 싫어요용",
          "original_form": "출근하기 싫어요용",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018083.1.2",
          "form": "직장다니시는군요..!!",
          "original_form": "직장다니시는군요..!!",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018083.1.3",
          "form": "힘드시겠어요ㅠ",
          "original_form": "힘드시겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018083.1.4",
          "form": "녜...!",
          "original_form": "녜...!",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018083.1.5",
          "form": "어떤일 하시나요?",
          "original_form": "어떤일 하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018083.1.6",
          "form": "저는 사무직...전화받는 일 해요",
          "original_form": "저는 사무직...전화받는 일 해요",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018083.1.7",
          "form": "저는 아직 대학생이에요!",
          "original_form": "저는 아직 대학생이에요!",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018083.1.8",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018083.1.9",
          "form": "아하 그러시군요! 진상들때문에 힘들거나 그러실때도 많겠어요..!ㅠ",
          "original_form": "아하 그러시군요! 진상들때문에 힘들거나 그러실때도 많겠어요..!ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018083.1.10",
          "form": "졸업까지 아직 남았나보네요!",
          "original_form": "졸업까지 아직 남았나보네요!",
          "speaker_id": "1",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018083.1.11",
          "form": "그래도 전문적인쪽이라 진상이 많진 않아요",
          "original_form": "그래도 전문적인쪽이라 진상이 많진 않아요",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.12",
          "form": "네 군대다녀와서 올해 복학해가지구",
          "original_form": "네 군대다녀와서 올해 복학해가지구",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.13",
          "form": "아직 2학년이에요!",
          "original_form": "아직 2학년이에요!",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.14",
          "form": "아하 그럼 아르바이트 하세요?",
          "original_form": "아하 그럼 아르바이트 하세요?",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.15",
          "form": "아하 다행이네요!",
          "original_form": "아하 다행이네요!",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.16",
          "form": "그래도 일하는거 자체가 힘들고 피곤하고 그러긴하죠ㅠ",
          "original_form": "그래도 일하는거 자체가 힘들고 피곤하고 그러긴하죠ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.17",
          "form": "알바는 안하고 있어요!",
          "original_form": "알바는 안하고 있어요!",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018083.1.18",
          "form": "용돈 받아서 생활하시나용?",
          "original_form": "용돈 받아서 생활하시나용?",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018083.1.19",
          "form": "용돈도 받을때도 있고 군대에서 모아온 돈이 아직 꽤 남아 있어요!",
          "original_form": "용돈도 받을때도 있고 군대에서 모아온 돈이 아직 꽤 남아 있어요!",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018083.1.20",
          "form": "그렇구나~ㅎㅎ아직은 취업걱정하긴 이르겠네요",
          "original_form": "그렇구나~ㅎㅎ아직은 취업걱정하긴 이르겠네요",
          "speaker_id": "1",
          "time": "20210826 22:32"
        }
      ]
