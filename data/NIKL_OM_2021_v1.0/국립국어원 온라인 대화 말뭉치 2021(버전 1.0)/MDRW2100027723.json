      "utterance": [
        {
          "id": "MDRW2100027723.1.1",
          "form": "강아지 키우고 있는데 다양한 생각이 들어요",
          "original_form": "강아지 키우고 있는데 다양한 생각이 들어요",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027723.1.2",
          "form": "저는 반려동물은 없어요",
          "original_form": "저는 반려동물은 없어요",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027723.1.3",
          "form": "강아지는 좋아해요",
          "original_form": "강아지는 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027723.1.4",
          "form": "말티폼을 키우는데 사랑스러워요",
          "original_form": "말티폼을 키우는데 사랑스러워요",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027723.1.5",
          "form": "몇살인가요?",
          "original_form": "몇살인가요?",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027723.1.6",
          "form": "11월임 두살 돼요",
          "original_form": "11월임 두살 돼요",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027723.1.7",
          "form": "예쁘겠네요",
          "original_form": "예쁘겠네요",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027723.1.8",
          "form": "저도 나중에는 고양이나 강아지 중에 키우고 싶긴해요",
          "original_form": "저도 나중에는 고양이나 강아지 중에 키우고 싶긴해요",
          "speaker_id": "2",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027723.1.9",
          "form": "그러세요 삶에 도움이 많이되네요\r\n일은 많지만요 ㅎ",
          "original_form": "그러세요 삶에 도움이 많이되네요\r\n일은 많지만요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027723.1.10",
          "form": "ㅋ 매일 배를 보이고 누워요 쓰다듬어달라고요",
          "original_form": "ㅋ 매일 배를 보이고 누워요 쓰다듬어달라고요",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027723.1.11",
          "form": "강아지 한마리 키우는데 아기처럼 용품도 많더라구요",
          "original_form": "강아지 한마리 키우는데 아기처럼 용품도 많더라구요",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100027723.1.12",
          "form": "그렇더라구요\r\n난 제일좋아하는 사랑을 주니 용품은 적어도 괜찮아할거에요",
          "original_form": "그렇더라구요\r\n난 제일좋아하는 사랑을 주니 용품은 적어도 괜찮아할거에요",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100027723.1.13",
          "form": "간식도 만들어보셨어요?",
          "original_form": "간식도 만들어보셨어요?",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100027723.1.14",
          "form": "병아리콩 단호박 섞어서 쿠키만들고\r\n닭가슴살도 말려서 줬어요",
          "original_form": "병아리콩 단호박 섞어서 쿠키만들고\r\n닭가슴살도 말려서 줬어요",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100027723.1.15",
          "form": "와! 대단하시네요",
          "original_form": "와! 대단하시네요",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100027723.1.16",
          "form": "주변에서 보면 자녀보다 더 위안을 받더라구요",
          "original_form": "주변에서 보면 자녀보다 더 위안을 받더라구요",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100027723.1.17",
          "form": "맞아요 ㅎ 늦둥이키우는 것 같아요\r\n안녕히 주무세요",
          "original_form": "맞아요 ㅎ 늦둥이키우는 것 같아요\r\n안녕히 주무세요",
          "speaker_id": "1",
          "time": "20210902 21:59"
        }
      ]
