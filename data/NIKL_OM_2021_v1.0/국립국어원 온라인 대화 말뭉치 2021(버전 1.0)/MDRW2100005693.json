      "utterance": [
        {
          "id": "MDRW2100005693.1.1",
          "form": "콩나물무침 냉장고에 넣어두면 며칠이나 갈까?",
          "original_form": "콩나물무침 냉장고에 넣어두면 며칠이나 갈까?",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005693.1.2",
          "form": "일주일?",
          "original_form": "일주일?",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005693.1.3",
          "form": "언니가 싱싱하면",
          "original_form": "언니가 싱싱하면",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005693.1.4",
          "form": "일주일이래",
          "original_form": "일주일이래",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005693.1.5",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005693.1.6",
          "form": "일주일 안에 먹어재껴야겠네",
          "original_form": "일주일 안에 먹어재껴야겠네",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005693.1.7",
          "form": "콩나물 무침은",
          "original_form": "콩나물 무침은",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005693.1.8",
          "form": "단독으로 먹기엔 밋밋해서",
          "original_form": "단독으로 먹기엔 밋밋해서",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005693.1.9",
          "form": "잘 안 먹어지는데",
          "original_form": "잘 안 먹어지는데",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005693.1.10",
          "form": "고사리랑",
          "original_form": "고사리랑",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005693.1.11",
          "form": "내가 준 그",
          "original_form": "내가 준 그",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005693.1.12",
          "form": "완자랑 같이 먹어",
          "original_form": "완자랑 같이 먹어",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005693.1.13",
          "form": "완자 해치우기에 진심이구먼",
          "original_form": "완자 해치우기에 진심이구먼",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005693.1.14",
          "form": "맛있겄네",
          "original_form": "맛있겄네",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005693.1.15",
          "form": "고사리는 걍 까먹고 있었다",
          "original_form": "고사리는 걍 까먹고 있었다",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005693.1.16",
          "form": "고사리 요리 언제할겨",
          "original_form": "고사리 요리 언제할겨",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005693.1.17",
          "form": "낼 해야겠지....",
          "original_form": "낼 해야겠지....",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005693.1.18",
          "form": "완자 이즈 마이 라잎이여",
          "original_form": "완자 이즈 마이 라잎이여",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005693.1.19",
          "form": "완자 이즈 유어 라잎",
          "original_form": "완자 이즈 유어 라잎",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005693.1.20",
          "form": "근데 이제 불가피함을 곁들인",
          "original_form": "근데 이제 불가피함을 곁들인",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005693.1.21",
          "form": "그냥 소분해서 냉동해두고 조금씩 해동해먹으면 안돼?",
          "original_form": "그냥 소분해서 냉동해두고 조금씩 해동해먹으면 안돼?",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005693.1.22",
          "form": "그냥 빨리 해치울래",
          "original_form": "그냥 빨리 해치울래",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005693.1.23",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005693.1.24",
          "form": "좋잖아",
          "original_form": "좋잖아",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005693.1.25",
          "form": "좋긴 하지",
          "original_form": "좋긴 하지",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005693.1.26",
          "form": "그럼 다른 반찬 잘 곁들여서",
          "original_form": "그럼 다른 반찬 잘 곁들여서",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005693.1.27",
          "form": "골고루 챙겨 먹더라고",
          "original_form": "골고루 챙겨 먹더라고",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005693.1.28",
          "form": "그래야 좀 덜 물리고",
          "original_form": "그래야 좀 덜 물리고",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005693.1.29",
          "form": "맛도 있고 그러지",
          "original_form": "맛도 있고 그러지",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005693.1.30",
          "form": "너도그래야해",
          "original_form": "너도그래야해",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005693.1.31",
          "form": "너도",
          "original_form": "너도",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005693.1.32",
          "form": "다양하게 멉어양배추만 먹지 말고",
          "original_form": "다양하게 멉어양배추만 먹지 말고",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100005693.1.33",
          "form": "알았으 양배추 말고도 먹을게",
          "original_form": "알았으 양배추 말고도 먹을게",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100005693.1.34",
          "form": "안그래도 나도 좀 야채를 다양하게 섭취해야 할 필요성을 느껴",
          "original_form": "안그래도 나도 좀 야채를 다양하게 섭취해야 할 필요성을 느껴",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100005693.1.35",
          "form": "모든 야채 섭취량을..양배추로 충당할 순 없으니께",
          "original_form": "모든 야채 섭취량을..양배추로 충당할 순 없으니께",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100005693.1.36",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005693.1.37",
          "form": "모든 곳에",
          "original_form": "모든 곳에",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005693.1.38",
          "form": "다른",
          "original_form": "다른",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005693.1.39",
          "form": "그게 있다고",
          "original_form": "그게 있다고",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005693.1.40",
          "form": "영양소",
          "original_form": "영양소",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100005693.1.41",
          "form": "그지그지",
          "original_form": "그지그지",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100005693.1.42",
          "form": "건강하게 챙겨먹어야겄어요",
          "original_form": "건강하게 챙겨먹어야겄어요",
          "speaker_id": "1",
          "time": "20210822 00:26"
        }
      ]
