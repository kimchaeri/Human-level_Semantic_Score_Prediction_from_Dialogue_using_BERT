      "utterance": [
        {
          "id": "MDRW2100036479.1.1",
          "form": "저두요 ㅜㅜ",
          "original_form": "저두요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100036479.1.2",
          "form": "나연 그룹에만 묶여있기엔 너무 팔방미인이라구요",
          "original_form": "나연 그룹에만 묶여있기엔 너무 팔방미인이라구요",
          "speaker_id": "1",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100036479.1.3",
          "form": "솔직히 노래 부르는걸로 봐서는",
          "original_form": "솔직히 노래 부르는걸로 봐서는",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100036479.1.4",
          "form": "솔로 라이브도 잘할거같아요 ㅋㅋ ㅜㅜ",
          "original_form": "솔로 라이브도 잘할거같아요 ㅋㅋ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036479.1.5",
          "form": "이미 지금도 거의 솔로 라이브 ^,^......",
          "original_form": "이미 지금도 거의 솔로 라이브 ^,^......",
          "speaker_id": "1",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036479.1.6",
          "form": "그거랑 별개로 트와이스는 노래 진짜 잘 뽑는 거 같아요",
          "original_form": "그거랑 별개로 트와이스는 노래 진짜 잘 뽑는 거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036479.1.7",
          "form": "하하하...",
          "original_form": "하하하...",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036479.1.8",
          "form": "맞아요 !!",
          "original_form": "맞아요 !!",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036479.1.9",
          "form": "단 한 곡도 별로라고 생각해본 적이 업어요!",
          "original_form": "단 한 곡도 별로라고 생각해본 적이 업어요!",
          "speaker_id": "1",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100036479.1.10",
          "form": "저는 시그널도",
          "original_form": "저는 시그널도",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036479.1.11",
          "form": "좋아했어요ㅋㅋ ㅜㅜ",
          "original_form": "좋아했어요ㅋㅋ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036479.1.12",
          "form": "사람들이 별로다 할때 혼자 좋은데 ..이랬어요 ㅋㅋ",
          "original_form": "사람들이 별로다 할때 혼자 좋은데 ..이랬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036479.1.13",
          "form": "헐 저도요!!!!!!!... 솔직히 전 다 너무 좋아해요 ㅠ 왓이즈 러브도 ㅠ ㅅ 너무 예쁘게 나오잖아요",
          "original_form": "헐 저도요!!!!!!!... 솔직히 전 다 너무 좋아해요 ㅠ 왓이즈 러브도 ㅠ ㅅ 너무 예쁘게 나오잖아요",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036479.1.14",
          "form": "저 최애곡은 펜시..",
          "original_form": "저 최애곡은 펜시..",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036479.1.15",
          "form": "팬시 좋아요ㅜㅜㅜ",
          "original_form": "팬시 좋아요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036479.1.16",
          "form": "팬시는 도입부 춤이 !",
          "original_form": "팬시는 도입부 춤이 !",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.17",
          "form": "대박이에요",
          "original_form": "대박이에요",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.18",
          "form": "헐헐 맞아요 글고 그 하이라이트에서",
          "original_form": "헐헐 맞아요 글고 그 하이라이트에서",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.19",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.20",
          "form": "음 점점 높아지면서 알러뷰~~~ 이럴때",
          "original_form": "음 점점 높아지면서 알러뷰~~~ 이럴때",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.21",
          "form": "넘 좋아요",
          "original_form": "넘 좋아요",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.22",
          "form": "아 맞아요 ㅜㅜ",
          "original_form": "아 맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036479.1.23",
          "form": "멤버들이 젤 이뻤던건 왓이즈러브요 !",
          "original_form": "멤버들이 젤 이뻤던건 왓이즈러브요 !",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036479.1.24",
          "form": "맞아요 ㅠ 그 뮤비 의상 입고",
          "original_form": "맞아요 ㅠ 그 뮤비 의상 입고",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036479.1.25",
          "form": "나연 교복 입었던거 넘 규ㅣ여워요 ㅠ",
          "original_form": "나연 교복 입었던거 넘 규ㅣ여워요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036479.1.26",
          "form": "앗 나연 교복 입었던 왓이즈러브 예쁘단 말 했었어욥 ㅎ.ㅎ",
          "original_form": "앗 나연 교복 입었던 왓이즈러브 예쁘단 말 했었어욥 ㅎ.ㅎ",
