      "utterance": [
        {
          "id": "MDRW2100025042.1.1",
          "form": "그거 딜 자주떠!",
          "original_form": "그거 딜 자주떠!",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025042.1.2",
          "form": "오! 나도 사봐야겠다 지금은 8천얼마네!",
          "original_form": "오! 나도 사봐야겠다 지금은 8천얼마네!",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100025042.1.3",
          "form": "지금 6400원뜨는데?",
          "original_form": "지금 6400원뜨는데?",
          "speaker_id": "1",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100025042.1.4",
          "form": "앗 어디서?",
          "original_form": "앗 어디서?",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100025042.1.5",
          "form": "십일번가..",
          "original_form": "십일번가..",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025042.1.6",
          "form": "왜 오백원더내린거야... 왜때문에....",
          "original_form": "왜 오백원더내린거야... 왜때문에....",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025042.1.7",
          "form": "그렇구나! 가격이 유동적이야ㅜㅜㅋㅋ",
          "original_form": "그렇구나! 가격이 유동적이야ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025042.1.8",
          "form": "ㅋㅋㅋ그래도 이정도선인듯 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그래도 이정도선인듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025042.1.9",
          "form": "십일번가에 그가격뜨나?",
          "original_form": "십일번가에 그가격뜨나?",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025042.1.10",
          "form": "응응! 바나나맛이 기대된다!",
          "original_form": "응응! 바나나맛이 기대된다!",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025042.1.11",
          "form": "우리집은 멜론이 호평이었다",
          "original_form": "우리집은 멜론이 호평이었다",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025042.1.12",
          "form": "앗 의외네? 나 멜론맛빵은 전에 뚜레쥬르였나",
          "original_form": "앗 의외네? 나 멜론맛빵은 전에 뚜레쥬르였나",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025042.1.13",
          "form": "그런데서 먹어보고 안먹어봤어!",
          "original_form": "그런데서 먹어보고 안먹어봤어!",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025042.1.14",
          "form": "근데 웃긴건 엄마아빠 멜론먹고 녹차맛이 제일 맛있더라 이랬디",
          "original_form": "근데 웃긴건 엄마아빠 멜론먹고 녹차맛이 제일 맛있더라 이랬디",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025042.1.15",
          "form": "약간 원효대사 해골물...",
          "original_form": "약간 원효대사 해골물...",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025042.1.16",
          "form": "아ㅋㅋㅋ멜론맛인줄 모르시고ㅋㅋ",
          "original_form": "아ㅋㅋㅋ멜론맛인줄 모르시고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025042.1.17",
          "form": "그니까 ㅋㅋㅋ색만보고 ㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋ색만보고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:46"
        }
      ]
