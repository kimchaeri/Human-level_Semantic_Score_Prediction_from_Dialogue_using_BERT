      "utterance": [
        {
          "id": "MDRW2100013684.1.1",
          "form": "종교가 어떻게되셔요",
          "original_form": "종교가 어떻게되셔요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013684.1.2",
          "form": "전 무신론자요 name1님은요?",
          "original_form": "전 무신론자요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013684.1.3",
          "form": "교회다녔는데 코로나때문에 못가고있지요",
          "original_form": "교회다녔는데 코로나때문에 못가고있지요",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013684.1.4",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013684.1.5",
          "form": "이시국이라 예배는 안하는게 좋은거같아요",
          "original_form": "이시국이라 예배는 안하는게 좋은거같아요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013684.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013684.1.7",
          "form": "종교행사많이 하세요?",
          "original_form": "종교행사많이 하세요?",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013684.1.8",
          "form": "아뇨 걍 무늬만 교회다녔지 열심히는 안했어요",
          "original_form": "아뇨 걍 무늬만 교회다녔지 열심히는 안했어요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013684.1.9",
          "form": "아 주위에 교회다니는 동기는 장기자랑도 나가더라고요",
          "original_form": "아 주위에 교회다니는 동기는 장기자랑도 나가더라고요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013684.1.10",
          "form": "맞아요 활동 많이하는분들도 있어요",
          "original_form": "맞아요 활동 많이하는분들도 있어요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013684.1.11",
          "form": "태어날때부터 교회였나요??",
          "original_form": "태어날때부터 교회였나요??",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013684.1.12",
          "form": "집안이 다??",
          "original_form": "집안이 다??",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013684.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013684.1.14",
          "form": "아 주위에 그런경우 많더라고요",
          "original_form": "아 주위에 그런경우 많더라고요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013684.1.15",
          "form": "여러가지 종교중 어떤종교를 가장 선호하시나요.",
          "original_form": "여러가지 종교중 어떤종교를 가장 선호하시나요.",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013684.1.16",
          "form": "거부감드는 종교는 없는지 궁금해요",
          "original_form": "거부감드는 종교는 없는지 궁금해요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013684.1.17",
          "form": "전 불교요",
          "original_form": "전 불교요",
          "speaker_id": "2",
