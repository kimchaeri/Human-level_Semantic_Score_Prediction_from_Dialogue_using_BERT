        {
          "id": "MDRW2100020140.1.1",
          "form": "name1은 일하신지 얼마나 되셨나요?",
          "original_form": "&name1&은 일하신지 얼마나 되셨나요?",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020140.1.2",
          "form": "너무 제 얘기만 많이 한 것 같아요ㅋㅋㅋ",
          "original_form": "너무 제 얘기만 많이 한 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020140.1.3",
          "form": "저는 6개월이욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 6개월이욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020140.1.4",
          "form": "2년 대학에서 일하다가",
          "original_form": "2년 대학에서 일하다가",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020140.1.5",
          "form": "새롭게 이쪽으로 왓어요 ㅎㅎ",
          "original_form": "새롭게 이쪽으로 왓어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020140.1.6",
          "form": "오 아직 파릇한 신입이시네요!",
          "original_form": "오 아직 파릇한 신입이시네요!",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020140.1.7",
          "form": "네 그래서 오늘도 가랄 때 제일먼저나왓어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 그래서 오늘도 가랄 때 제일먼저나왓어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100020140.1.8",
          "form": "ㅋㅋㅋㅋ저는 막 눈치줘요.. 이제 다 잡은 물고기라고 생각하는지",
          "original_form": "ㅋㅋㅋㅋ저는 막 눈치줘요.. 이제 다 잡은 물고기라고 생각하는지",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020140.1.9",
          "form": "퇴근하늗거 눈치주나요??",
          "original_form": "퇴근하늗거 눈치주나요??",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020140.1.10",
          "form": "절레절레",
          "original_form": "절레절레",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020140.1.11",
          "form": "말은 퇴근하라하고",
          "original_form": "말은 퇴근하라하고",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020140.1.12",
          "form": "뒷말에 우리는 좀 더 남아서 일하다 들어갈게^^",
          "original_form": "뒷말에 우리는 좀 더 남아서 일하다 들어갈게^^",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020140.1.13",
          "form": "이러면ㅋㅋㅋ이건 뭐 퇴근하라는게 아니잖아요",
          "original_form": "이러면ㅋㅋㅋ이건 뭐 퇴근하라는게 아니잖아요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020140.1.14",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020140.1.15",
          "form": "저는 그런말을 무시하고 갑니다",
          "original_form": "저는 그런말을 무시하고 갑니다",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100020140.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020140.1.17",
          "form": "크으...멋있네요",
          "original_form": "크으...멋있네요",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020140.1.18",
          "form": "퇴근은 소중하니까요 ㅎㅎ",
          "original_form": "퇴근은 소중하니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020140.1.19",
          "form": "저 대학에서 일할 때 팀장님이 정말 워커홀릭이셔서",
          "original_form": "저 대학에서 일할 때 팀장님이 정말 워커홀릭이셔서",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020140.1.20",
          "form": "저도 이제 얼마 안남은거",
          "original_form": "저도 이제 얼마 안남은거",
          "speaker_id": "2",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020140.1.21",
          "form": "출근할땐 인사받아주는데",
          "original_form": "출근할땐 인사받아주는데",
