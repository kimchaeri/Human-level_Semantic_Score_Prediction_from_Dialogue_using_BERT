      "utterance": [
        {
          "id": "MDRW2100033226.1.1",
          "form": "안녕~~ 종교 있니??",
          "original_form": "안녕~~ 종교 있니??",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100033226.1.2",
          "form": "아니 나는 없어~",
          "original_form": "아니 나는 없어~",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100033226.1.3",
          "form": "나도 없어ㅠㅠ",
          "original_form": "나도 없어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100033226.1.4",
          "form": "근데 불교 좋아해",
          "original_form": "근데 불교 좋아해",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100033226.1.5",
          "form": "좋아하는건 뭐야ㅋㅋㅋㅋㅋ",
          "original_form": "좋아하는건 뭐야ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100033226.1.6",
          "form": "우리는 친가가 불교고 외가가 천주교야",
          "original_form": "우리는 친가가 불교고 외가가 천주교야",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100033226.1.7",
          "form": "오 그렇구나 중간에 껴서 무교인거야?",
          "original_form": "오 그렇구나 중간에 껴서 무교인거야?",
          "speaker_id": "1",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100033226.1.8",
          "form": "절 가면 맘이 편해서 좋아해 ㅋㅋㅋ",
          "original_form": "절 가면 맘이 편해서 좋아해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100033226.1.9",
          "form": "딱히 그런건 아닌데 부모님이 종교는 관여하지 말자고? 하셨대",
          "original_form": "딱히 그런건 아닌데 부모님이 종교는 관여하지 말자고? 하셨대",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100033226.1.10",
          "form": "그건 그렇거 같다ㅋㅋㅋ 대부분 산 속에 있으니까",
          "original_form": "그건 그렇거 같다ㅋㅋㅋ 대부분 산 속에 있으니까",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100033226.1.11",
          "form": "우와부모님 멋지다",
          "original_form": "우와부모님 멋지다",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100033226.1.12",
          "form": "불교의 차분함과 향이 좋아",
          "original_form": "불교의 차분함과 향이 좋아",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100033226.1.13",
          "form": "나도 그 향 좋아해",
          "original_form": "나도 그 향 좋아해",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100033226.1.14",
          "form": "다른 사람들은 싫어하는데 좋더라고...",
          "original_form": "다른 사람들은 싫어하는데 좋더라고...",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100033226.1.15",
          "form": "백담사인가 거기는 가을에 가면 진짜 멋있더라",
          "original_form": "백담사인가 거기는 가을에 가면 진짜 멋있더라",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100033226.1.16",
          "form": "와 백담사",
          "original_form": "와 백담사",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100033226.1.17",
          "form": "나도가볼래",
          "original_form": "나도가볼래",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100033226.1.18",
          "form": "단풍이 멋있더라!",
          "original_form": "단풍이 멋있더라!",
          "speaker_id": "2",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100033226.1.19",
          "form": "와 가을에 단풍 들면 가야겠어",
          "original_form": "와 가을에 단풍 들면 가야겠어",
          "speaker_id": "1",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100033226.1.20",
          "form": "교회 같은 곳 다녀본 적 있어?",
          "original_form": "교회 같은 곳 다녀본 적 있어?",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100033226.1.21",
          "form": "어릴때 간식 받으러 가봤어 ㅋㅋ",
          "original_form": "어릴때 간식 받으러 가봤어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100033226.1.22",
          "form": "교회는 심하게 전도 해서 시로",
          "original_form": "교회는 심하게 전도 해서 시로",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033226.1.23",
          "form": "그래서 불교나 천주교가 좋아",
          "original_form": "그래서 불교나 천주교가 좋아",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033226.1.24",
          "form": "나도...ㅋㅋㅋㅋㅠㅠ",
          "original_form": "나도...ㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100033226.1.25",
          "form": "와 우리 비슷하다",
          "original_form": "와 우리 비슷하다",
