      "utterance": [
        {
          "id": "MDRW2100032536.1.1",
          "form": "네네 근데 한번만 먹어봤어요!!",
          "original_form": "네네 근데 한번만 먹어봤어요!!",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032536.1.2",
          "form": "요즘 카페가 많이 생겼드라구요 !!",
          "original_form": "요즘 카페가 많이 생겼드라구요 !!",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032536.1.3",
          "form": "맞아요 너무많아졌어요",
          "original_form": "맞아요 너무많아졌어요",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032536.1.4",
          "form": "맛은 비슷비슷한데 분위기가 있드라구요!!",
          "original_form": "맛은 비슷비슷한데 분위기가 있드라구요!!",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100032536.1.5",
          "form": "맞아요 분위기 차이",
          "original_form": "맞아요 분위기 차이",
          "speaker_id": "2",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032536.1.6",
          "form": "저는 바다 근처가 조터라구요",
          "original_form": "저는 바다 근처가 조터라구요",
          "speaker_id": "2",
          "time": "20210905 21:21"
        },
        {
          "id": "MDRW2100032536.1.7",
          "form": "오오 너무 좋아요!!",
          "original_form": "오오 너무 좋아요!!",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032536.1.8",
          "form": "바다 근처 카페 어디로 가셧어요?",
          "original_form": "바다 근처 카페 어디로 가셧어요?",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032536.1.9",
          "form": "저는 기장에 일광쪽으로 갔어요",
          "original_form": "저는 기장에 일광쪽으로 갔어요",
          "speaker_id": "2",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032536.1.10",
          "form": "맛집 잘 아세요???",
          "original_form": "맛집 잘 아세요???",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032536.1.11",
          "form": "아니요 ㅠㅠㅠ",
          "original_form": "아니요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032536.1.12",
          "form": "조개집만알걸요",
          "original_form": "조개집만알걸요",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032536.1.13",
          "form": "맛집은 웨이팅 때문에 힘들어욬ㅋㅋㅋㅋ",
          "original_form": "맛집은 웨이팅 때문에 힘들어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032536.1.14",
          "form": "맞아요 숨은맛집을 찾아야해요 ㅋㅋ",
          "original_form": "맞아요 숨은맛집을 찾아야해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032536.1.15",
          "form": "맛집 찾을때 리뷰 잘챙겨 보세요??",
          "original_form": "맛집 찾을때 리뷰 잘챙겨 보세요??",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100032536.1.16",
          "form": "아니요!!",
          "original_form": "아니요!!",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032536.1.17",
          "form": "그냥 제눈에 맛있어보이면",
          "original_form": "그냥 제눈에 맛있어보이면",
