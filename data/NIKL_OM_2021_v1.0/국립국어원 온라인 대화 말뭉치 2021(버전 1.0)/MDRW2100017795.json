      "utterance": [
        {
          "id": "MDRW2100017795.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100017795.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100017795.1.3",
          "form": "저녁드셨나요?!",
          "original_form": "저녁드셨나요?!",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100017795.1.4",
          "form": "넵 소고기 먹었습니다",
          "original_form": "넵 소고기 먹었습니다",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017795.1.5",
          "form": "저녁 드셨죠?",
          "original_form": "저녁 드셨죠?",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017795.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100017795.1.7",
          "form": "반칙이예요",
          "original_form": "반칙이예요",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017795.1.8",
          "form": "ㅋㅋ 구워먹은건 아니니 반만 반칙할게요 무웃국",
          "original_form": "ㅋㅋ 구워먹은건 아니니 반만 반칙할게요 무웃국",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100017795.1.9",
          "form": "오오 그럼 다행이네요",
          "original_form": "오오 그럼 다행이네요",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100017795.1.10",
          "form": "서운할뻔했어요",
          "original_form": "서운할뻔했어요",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100017795.1.11",
          "form": "ㅋㅋ국도 진짜 맛있는데요ㅎㅎ",
          "original_form": "ㅋㅋ국도 진짜 맛있는데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100017795.1.12",
          "form": "저녁메뉴는 무엇이었어요?",
          "original_form": "저녁메뉴는 무엇이었어요?",
          "speaker_id": "1",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100017795.1.13",
          "form": "ㅋㅋㅋㅋ 저 가볍게",
          "original_form": "ㅋㅋㅋㅋ 저 가볍게",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100017795.1.14",
          "form": "햄버거먹었답니다",
          "original_form": "햄버거먹었답니다",
          "speaker_id": "2",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100017795.1.15",
          "form": "ㅋㅋ매우 가볍군요! 저도 샐러디 먹을까했는데 집밥먹었어요",
          "original_form": "ㅋㅋ매우 가볍군요! 저도 샐러디 먹을까했는데 집밥먹었어요",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100017795.1.16",
          "form": "ㅋㅋㅋㅋ 집밥이짱이죠",
          "original_form": "ㅋㅋㅋㅋ 집밥이짱이죠",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100017795.1.17",
          "form": "내일은",
          "original_form": "내일은",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100017795.1.18",
          "form": "샤브를 먹어볼까봐요",
          "original_form": "샤브를 먹어볼까봐요",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100017795.1.19",
          "form": "샤브샤브 진짜!!! 최고의 선택! 내일 전국적으로 비가 많이 온대요",
          "original_form": "샤브샤브 진짜!!! 최고의 선택! 내일 전국적으로 비가 많이 온대요",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
