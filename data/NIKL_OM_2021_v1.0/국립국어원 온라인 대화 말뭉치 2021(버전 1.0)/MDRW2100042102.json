      "utterance": [
        {
          "id": "MDRW2100042102.1.1",
          "form": "여름 다 지나갔는데 무슨 단발이야",
          "original_form": "여름 다 지나갔는데 무슨 단발이야",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042102.1.2",
          "form": "제가 방금 그얘기 했거든요 ㅠㅠ",
          "original_form": "제가 방금 그얘기 했거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042102.1.3",
          "form": "날 추워지면 머리카락만큼 보온되는게 없어",
          "original_form": "날 추워지면 머리카락만큼 보온되는게 없어",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042102.1.4",
          "form": "맞아요 근데 단발땡겨 ㅠ",
          "original_form": "맞아요 근데 단발땡겨 ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042102.1.5",
          "form": "여름에 진즉 잘랐어야지 미련하게 아직까지 갖구있었어",
          "original_form": "여름에 진즉 잘랐어야지 미련하게 아직까지 갖구있었어",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042102.1.6",
          "form": "근데 저 머리 길어도 이쁘지 않아요? ㅎㅎ",
          "original_form": "근데 저 머리 길어도 이쁘지 않아요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042102.1.7",
          "form": "말도 참 이쁘게 하는군",
          "original_form": "말도 참 이쁘게 하는군",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042102.1.8",
          "form": "아잉 이쁘잖아요",
          "original_form": "아잉 이쁘잖아요",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042102.1.9",
          "form": "이쁘긴 하지 근데 단발이 더 이뻐",
          "original_form": "이쁘긴 하지 근데 단발이 더 이뻐",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042102.1.10",
          "form": "그릉가 저도 그런거같기두 하고",
          "original_form": "그릉가 저도 그런거같기두 하고",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042102.1.11",
          "form": "엄만 긴머리는 별로더라 치렁치렁해가지고",
          "original_form": "엄만 긴머리는 별로더라 치렁치렁해가지고",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042102.1.12",
          "form": "관리 잘하면 예쁜데 관리 못하면 엄청 더러워보이지않나요 ㅋㅋ",
          "original_form": "관리 잘하면 예쁜데 관리 못하면 엄청 더러워보이지않나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042102.1.13",
          "form": "글구 너 반곱슬이잖아 곱슬끼 엄청나서",
          "original_form": "글구 너 반곱슬이잖아 곱슬끼 엄청나서",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042102.1.14",
          "form": "하 그것만 아니었어도 청순여신 되는건데",
          "original_form": "하 그것만 아니었어도 청순여신 되는건데",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042102.1.15",
          "form": "무슨 청순여신이야",
          "original_form": "무슨 청순여신이야",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042102.1.16",
          "form": "원래부터 긴머리는 청순미의 상징이니까요",
          "original_form": "원래부터 긴머리는 청순미의 상징이니까요",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042102.1.17",
          "form": "청순 다 죽었다 엄마도 청순해 그럼",
          "original_form": "청순 다 죽었다 엄마도 청순해 그럼",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042102.1.18",
          "form": "엄마도 반곱슬이면서",
          "original_form": "엄마도 반곱슬이면서",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042102.1.19",
          "form": "아빠도 반곱슬이면서!!",
          "original_form": "아빠도 반곱슬이면서!!",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042102.1.20",
          "form": "어쩌겠니 반곱슬하고 반곱슬 만나서 곱슬이 된걸",
          "original_form": "어쩌겠니 반곱슬하고 반곱슬 만나서 곱슬이 된걸",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042102.1.21",
          "form": "다른건 다 좋은데 흑흑",
          "original_form": "다른건 다 좋은데 흑흑",
          "speaker_id": "1",
          "time": "20210906 19:51"
        }
      ]
