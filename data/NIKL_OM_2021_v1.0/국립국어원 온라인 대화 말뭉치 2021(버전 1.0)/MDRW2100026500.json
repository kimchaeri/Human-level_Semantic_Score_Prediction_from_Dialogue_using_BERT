      "utterance": [
        {
          "id": "MDRW2100026500.1.1",
          "form": "안녕~ 티비 자주 봐??",
          "original_form": "안녕~ 티비 자주 봐??",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026500.1.2",
          "form": "응 안녕",
          "original_form": "응 안녕",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026500.1.3",
          "form": "어떤 프로그램 좋아해??",
          "original_form": "어떤 프로그램 좋아해??",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026500.1.4",
          "form": "노는언니",
          "original_form": "노는언니",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026500.1.5",
          "form": "박세리 나오는 프로그램 맞나?",
          "original_form": "박세리 나오는 프로그램 맞나?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026500.1.6",
          "form": "응 본 적 있어?",
          "original_form": "응 본 적 있어?",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026500.1.7",
          "form": "응! 몇번 본적 있어",
          "original_form": "응! 몇번 본적 있어",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026500.1.8",
          "form": "다들 운동신경이 좋아서 그런가 몸쓰는거 너무 잘하더라!",
          "original_form": "다들 운동신경이 좋아서 그런가 몸쓰는거 너무 잘하더라!",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026500.1.9",
          "form": "ㅋㅋㅋㅋ 그치 아무래도 운동선수 출신이니까",
          "original_form": "ㅋㅋㅋㅋ 그치 아무래도 운동선수 출신이니까",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026500.1.10",
          "form": "너무 재밌어",
          "original_form": "너무 재밌어",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026500.1.11",
          "form": "맞아 전부 운동선수들이라 재미 많이 있을까? 싶었는데 너무 재밌더라",
          "original_form": "맞아 전부 운동선수들이라 재미 많이 있을까? 싶었는데 너무 재밌더라",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026500.1.12",
          "form": "곧 도쿄올림픽 국대도 나온다는데!",
          "original_form": "곧 도쿄올림픽 국대도 나온다는데!",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026500.1.13",
          "form": "다들 예능감도 좋아 ㅋㅋ",
          "original_form": "다들 예능감도 좋아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026500.1.14",
          "form": "너무 기대된다~~~",
          "original_form": "너무 기대된다~~~",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026500.1.15",
          "form": "나도 도쿄올림픽 국대들 나오는 편 챙겨보려고!",
          "original_form": "나도 도쿄올림픽 국대들 나오는 편 챙겨보려고!",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026500.1.16",
          "form": "올림픽 너무 재미있게 봐서 선수들 나오는 프로그램 다보고있어ㅜ",
          "original_form": "올림픽 너무 재미있게 봐서 선수들 나오는 프로그램 다보고있어ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100026500.1.17",
          "form": "유퀴즈도 봤어? 난 보고싶었는데 못봄 ㅠ",
          "original_form": "유퀴즈도 봤어? 난 보고싶었는데 못봄 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026500.1.18",
          "form": "응! 완전 재밌었어ㅋㅋㅋㅋㅋ",
          "original_form": "응! 완전 재밌었어ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026500.1.19",
          "form": "유튜브에 클립 올라온거 많더라고",
          "original_form": "유튜브에 클립 올라온거 많더라고",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026500.1.20",
          "form": "나중에 찾아봐봐!",
          "original_form": "나중에 찾아봐봐!",
          "speaker_id": "1",
          "time": "20210901 23:42"
        }
      ]
