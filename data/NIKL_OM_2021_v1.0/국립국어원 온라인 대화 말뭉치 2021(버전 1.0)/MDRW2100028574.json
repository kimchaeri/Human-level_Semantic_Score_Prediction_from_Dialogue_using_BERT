      "utterance": [
        {
          "id": "MDRW2100028574.1.1",
          "form": "저녁드셨나요",
          "original_form": "저녁드셨나요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028574.1.2",
          "form": "저녁먹었어요 생일은 아니지만 미역국먹었습니당",
          "original_form": "저녁먹었어요 생일은 아니지만 미역국먹었습니당",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028574.1.3",
          "form": "오 미역국 좋네요",
          "original_form": "오 미역국 좋네요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028574.1.4",
          "form": "손이커서 국끓이면 항상 남아서 냉동실에 얼려버리거든요",
          "original_form": "손이커서 국끓이면 항상 남아서 냉동실에 얼려버리거든요",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028574.1.5",
          "form": "그거 끓여서 먹었어요...완전 자취생바이브",
          "original_form": "그거 끓여서 먹었어요...완전 자취생바이브",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028574.1.6",
          "form": "아 얼려서 드시는구나",
          "original_form": "아 얼려서 드시는구나",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028574.1.7",
          "form": "저도 카레 미역국 많이해먹어요",
          "original_form": "저도 카레 미역국 많이해먹어요",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028574.1.8",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028574.1.9",
          "form": "냉동식품최고...",
          "original_form": "냉동식품최고...",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028574.1.10",
          "form": "맞습니댜 ..",
          "original_form": "맞습니댜 ..",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028574.1.11",
          "form": "음식이 너무 빨리 상해요",
          "original_form": "음식이 너무 빨리 상해요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028574.1.12",
          "form": "여름이니까 어쩔 수 없긴한데 진짜 콩국수해먹으려고 콩물사왔는데 하루만에 쉬더라구요",
          "original_form": "여름이니까 어쩔 수 없긴한데 진짜 콩국수해먹으려고 콩물사왔는데 하루만에 쉬더라구요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028574.1.13",
          "form": "ㅠㅠ서럽",
          "original_form": "ㅠㅠ서럽",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028574.1.14",
          "form": "아 콩국도 좋죠",
          "original_form": "아 콩국도 좋죠",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028574.1.15",
          "form": "아 맞아요ㅠㅠㅠ 냉장고 넣어두 상해요",
          "original_form": "아 맞아요ㅠㅠㅠ 냉장고 넣어두 상해요",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028574.1.16",
          "form": "그 냉동식품중에",
          "original_form": "그 냉동식품중에",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028574.1.17",
          "form": "비비고 얇은피만두드셔보셨나요?",
          "original_form": "비비고 얇은피만두드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028574.1.18",
          "form": "ㅋㅋㅋㅋㅋ이러니까무슨 판매원같기는 한데 맛있답니다...",
          "original_form": "ㅋㅋㅋㅋㅋ이러니까무슨 판매원같기는 한데 맛있답니다...",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028574.1.19",
          "form": "아니욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028574.1.20",
          "form": "얇은피만두 풀무원껀 먹거봤는데",
          "original_form": "얇은피만두 풀무원껀 먹거봤는데",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028574.1.21",
          "form": "비비고는 안먹어봤어요 궁금해요",
          "original_form": "비비고는 안먹어봤어요 궁금해요",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028574.1.22",
          "form": "풀무원이 원조였나여???",
          "original_form": "풀무원이 원조였나여???",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100028574.1.23",
          "form": "흠 그걸로 만두밥해먹었는데 맛있더라구요",
          "original_form": "흠 그걸로 만두밥해먹었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100028574.1.24",
          "form": "추천추천",
          "original_form": "추천추천",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100028574.1.25",
          "form": "만두밥!! 만두밥은 레시피가 뭐에여",
          "original_form": "만두밥!! 만두밥은 레시피가 뭐에여",
