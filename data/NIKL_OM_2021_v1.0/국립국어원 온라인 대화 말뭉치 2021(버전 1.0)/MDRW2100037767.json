      "utterance": [
        {
          "id": "MDRW2100037767.1.1",
          "form": "바다 보고싶어",
          "original_form": "바다 보고싶어",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100037767.1.2",
          "form": "제주도 가고싶다",
          "original_form": "제주도 가고싶다",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100037767.1.3",
          "form": "우리 나중에 제주도 가면",
          "original_form": "우리 나중에 제주도 가면",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100037767.1.4",
          "form": "렌트해서 다니자!",
          "original_form": "렌트해서 다니자!",
          "speaker_id": "2",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100037767.1.5",
          "form": "응 그려",
          "original_form": "응 그려",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100037767.1.6",
          "form": "이쁜 숙소 많더라",
          "original_form": "이쁜 숙소 많더라",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.7",
          "form": "단독으로 된 숙소?",
          "original_form": "단독으로 된 숙소?",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.8",
          "form": "잡아서",
          "original_form": "잡아서",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.9",
          "form": "아 너무 좋아~!",
          "original_form": "아 너무 좋아~!",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.10",
          "form": "신나게 놀자ㅏ!!",
          "original_form": "신나게 놀자ㅏ!!",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.11",
          "form": "술도 마시고",
          "original_form": "술도 마시고",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.12",
          "form": "분위기 좋은 카페도 많더라",
          "original_form": "분위기 좋은 카페도 많더라",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.13",
          "form": "맛있는것도 많이 먹고!",
          "original_form": "맛있는것도 많이 먹고!",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.14",
          "form": "마자마자!!",
          "original_form": "마자마자!!",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.15",
          "form": "맛집 다 다녀오자",
          "original_form": "맛집 다 다녀오자",
          "speaker_id": "1",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100037767.1.16",
          "form": "히히 생각만해도 좋아",
          "original_form": "히히 생각만해도 좋아",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.17",
          "form": "흑돼지도 함 먹어보자",
          "original_form": "흑돼지도 함 먹어보자",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.18",
          "form": "좋아!",
          "original_form": "좋아!",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.19",
          "form": "난 옆에서",
          "original_form": "난 옆에서",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.20",
          "form": "김치 구워먹어야징",
          "original_form": "김치 구워먹어야징",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.21",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.22",
          "form": "예에",
          "original_form": "예에",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.23",
          "form": "회도 먹고",
          "original_form": "회도 먹고",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.24",
          "form": "마트 들려서",
          "original_form": "마트 들려서",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.25",
          "form": "나 제주도에서 한 달 살기 이런 것도 하고 싶어ㅋㅋㅋㅋㅋ",
          "original_form": "나 제주도에서 한 달 살기 이런 것도 하고 싶어ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.26",
          "form": "숙소가서 숱도 마시고",
          "original_form": "숙소가서 숱도 마시고",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.27",
          "form": "나도!",
          "original_form": "나도!",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.28",
          "form": "나 되게 이쁜곳 봄",
          "original_form": "나 되게 이쁜곳 봄",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.29",
          "form": "재밌을 거 같아",
          "original_form": "재밌을 거 같아",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100037767.1.30",
          "form": "나중에 꼭 갈꺼야",
          "original_form": "나중에 꼭 갈꺼야",
          "speaker_id": "2",
          "time": "20210901 18:10"
        }
      ]
    }
