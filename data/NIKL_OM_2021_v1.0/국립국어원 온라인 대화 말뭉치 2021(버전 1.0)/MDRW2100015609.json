      "utterance": [
        {
          "id": "MDRW2100015609.1.1",
          "form": "추가하는거도 있나요??",
          "original_form": "추가하는거도 있나요??",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015609.1.2",
          "form": "가격 얼마정도 플러스돼요?",
          "original_form": "가격 얼마정도 플러스돼요?",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015609.1.3",
          "form": "황금올리브가 18,000",
          "original_form": "황금올리브가 18,000",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015609.1.4",
          "form": "닭다리두개더 세트가 23,000",
          "original_form": "닭다리두개더 세트가 23,000",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015609.1.5",
          "form": "세트라서 황올+닭다리2+감튀 에요ㅋㅋ",
          "original_form": "세트라서 황올+닭다리2+감튀 에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015609.1.6",
          "form": "5천원 플러스",
          "original_form": "5천원 플러스",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015609.1.7",
          "form": "오호 감튀까지 주면",
          "original_form": "오호 감튀까지 주면",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015609.1.8",
          "form": "5천원정도 괜찮네요ㅋㅋ",
          "original_form": "5천원정도 괜찮네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015609.1.9",
          "form": "다음에 먹어봐야겠어요",
          "original_form": "다음에 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015609.1.10",
          "form": "치킨말고는 어떤 음식 좋아하세요?",
          "original_form": "치킨말고는 어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015609.1.11",
          "form": "떡볶이 좋아해요ㅋㅋ",
          "original_form": "떡볶이 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015609.1.12",
          "form": "피자도 좋아하구요:)",
          "original_form": "피자도 좋아하구요:)",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015609.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015609.1.14",
          "form": "저도 떡볶이 좋아해요!",
          "original_form": "저도 떡볶이 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100015609.1.15",
          "form": "전 요새 로제 떡볶이 그거 먹어보고 싶더라구요",
          "original_form": "전 요새 로제 떡볶이 그거 먹어보고 싶더라구요",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100015609.1.16",
          "form": "드셔보셨어요??",
          "original_form": "드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100015609.1.17",
          "form": "네:) 로제떡볶이 먹고나면\r\n계속 생각나는 맛이에요",
          "original_form": "네:) 로제떡볶이 먹고나면\r\n계속 생각나는 맛이에요",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100015609.1.18",
          "form": "아 정말요?? 혹시 로제파스타랑은 다른맛이에요 비슷한가?",
          "original_form": "아 정말요?? 혹시 로제파스타랑은 다른맛이에요 비슷한가?",
          "speaker_id": "2",
          "time": "20210826 17:44"
        },
        {
          "id": "MDRW2100015609.1.19",
          "form": "제 입맛에는\r\n로제 파스타랑은\r\n좀 다른 맛이었어요:)",
          "original_form": "제 입맛에는\r\n로제 파스타랑은\r\n좀 다른 맛이었어요:)",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100015609.1.20",
          "form": "오 한번 먹어봐야겠어요",
          "original_form": "오 한번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100015609.1.21",
          "form": "혹시 브랜드 추천해주실수 있나요",
          "original_form": "혹시 브랜드 추천해주실수 있나요",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100015609.1.22",
          "form": "전 맨날 죠스 떡볶이만 먹어봐서ㅋㅋ",
          "original_form": "전 맨날 죠스 떡볶이만 먹어봐서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100015609.1.23",
          "form": "저는 친구가 떡볶이 가게를 운영해서\r\n달쫀에서 먹었는데 맛있더라구용",
          "original_form": "저는 친구가 떡볶이 가게를 운영해서\r\n달쫀에서 먹었는데 맛있더라구용",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100015609.1.24",
          "form": "오오 달쫀이 체인점인가요?",
          "original_form": "오오 달쫀이 체인점인가요?",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100015609.1.25",
          "form": "네ㅋㅋ 달쫀도 체인이에요:)",
          "original_form": "네ㅋㅋ 달쫀도 체인이에요:)",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100015609.1.26",
          "form": "아하ㅎㅎ 아 저 오늘 로제떡볶이 먹어봤어요!",
          "original_form": "아하ㅎㅎ 아 저 오늘 로제떡볶이 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100015609.1.27",
          "form": "달쫀은 못 멋어보고 동네에 새로생긴 집 있길래 먹어봤는데",
          "original_form": "달쫀은 못 멋어보고 동네에 새로생긴 집 있길래 먹어봤는데",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100015609.1.28",
          "form": "매콤하고 안느끼해서 맛있더라구요!ㅎㅎ",
          "original_form": "매콤하고 안느끼해서 맛있더라구요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100015609.1.29",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210827 19:19"
        }
      ]
    }
