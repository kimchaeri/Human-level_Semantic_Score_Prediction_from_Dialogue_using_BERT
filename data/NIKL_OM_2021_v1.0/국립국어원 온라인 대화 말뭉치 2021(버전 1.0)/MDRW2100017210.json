          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.28",
          "form": "아우디 디자인 좋아했어서",
          "original_form": "아우디 디자인 좋아했어서",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.29",
          "form": "아우디 suv타고 싶어",
          "original_form": "아우디 suv타고 싶어",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.30",
          "form": "가보자고",
          "original_form": "가보자고",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.32",
          "form": "그래서",
