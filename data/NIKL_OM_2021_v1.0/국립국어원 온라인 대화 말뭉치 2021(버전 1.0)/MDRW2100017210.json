        },
        {
          "id": "MDRW2100017210.1.2",
          "form": "너 생일 선물 받았던거",
          "original_form": "너 생일 선물 받았던거",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.3",
          "form": "마븡 장갑??",
          "original_form": "마븡 장갑??",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.4",
          "form": "아 마스크??",
          "original_form": "아 마스크??",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.6",
          "form": "아 마스크였낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 마스크였낰ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.7",
          "form": "다음에 그거 쓰고 와줘",
          "original_form": "다음에 그거 쓰고 와줘",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.8",
          "form": "끈 묶어서쓰는 마스크ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "끈 묶어서쓰는 마스크ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.9",
          "form": "안됨 그거 개작아서",
          "original_form": "안됨 그거 개작아서",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.10",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017210.1.12",
          "form": "다 안가려짐ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다 안가려짐ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017210.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017210.1.14",
          "form": "근데 나는 물건 살때",
          "original_form": "근데 나는 물건 살때",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017210.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017210.1.16",
          "form": "주변인들이 산거 보면 나도 갑자기 사고 싶어져서",
          "original_form": "주변인들이 산거 보면 나도 갑자기 사고 싶어져서",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017210.1.17",
          "form": "좀 위험한것 같음",
          "original_form": "좀 위험한것 같음",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017210.1.18",
          "form": "필요하지도 않은데",
          "original_form": "필요하지도 않은데",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017210.1.19",
          "form": "나도 좀 그럼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 좀 그럼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017210.1.20",
          "form": "오 이쁜데? 이러면서 찾아보고",
          "original_form": "오 이쁜데? 이러면서 찾아보고",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017210.1.21",
          "form": "지금은 좀 ㄱㅊ은데 중고딩때 충동구매 진짜심했음",
          "original_form": "지금은 좀 ㄱㅊ은데 중고딩때 충동구매 진짜심했음",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017210.1.22",
          "form": "고민하다가 결국 안사는게 다행이다 진짜",
          "original_form": "고민하다가 결국 안사는게 다행이다 진짜",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017210.1.23",
          "form": "나 나중에 아우디 사고 싶어",
          "original_form": "나 나중에 아우디 사고 싶어",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017210.1.24",
          "form": "옹오오오ㅗ오오옹",
          "original_form": "옹오오오ㅗ오오옹",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.25",
          "form": "개멋지네",
          "original_form": "개멋지네",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.26",
          "form": "어렸을때부터",
          "original_form": "어렸을때부터",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017210.1.27",
