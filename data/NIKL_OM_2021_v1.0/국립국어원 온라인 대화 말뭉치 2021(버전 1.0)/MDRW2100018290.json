        },
        {
          "id": "MDRW2100018290.1.2",
          "form": "어떤 스타일 커피 좋아해?",
          "original_form": "어떤 스타일 커피 좋아해?",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100018290.1.3",
          "form": "라면 안 불은 게 좋아 불은 게 좋아?",
          "original_form": "라면 안 불은 게 좋아 불은 게 좋아?",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018290.1.4",
          "form": "안불은거",
          "original_form": "안불은거",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018290.1.5",
          "form": "나도 꼬들한 게 좋아",
          "original_form": "나도 꼬들한 게 좋아",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100018290.1.6",
          "form": "불은거 비주얼이 별로야",
          "original_form": "불은거 비주얼이 별로야",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100018290.1.7",
          "form": "비주얼 때문에 별로야?",
          "original_form": "비주얼 때문에 별로야?",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100018290.1.8",
          "form": "그것도 그런데 뭔가 먹다 남긴거 같음;;",
          "original_form": "그것도 그런데 뭔가 먹다 남긴거 같음;;",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100018290.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ라면 얘기하니까 라면 먹고싶다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ라면 얘기하니까 라면 먹고싶다",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100018290.1.10",
          "form": "ㅋㅋㅋㅋㅋ 이따 먹어",
          "original_form": "ㅋㅋㅋㅋㅋ 이따 먹어",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100018290.1.11",
          "form": "안그래도 그렇게 될 거 같아ㅋㅋㅋ",
          "original_form": "안그래도 그렇게 될 거 같아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100018290.1.12",
