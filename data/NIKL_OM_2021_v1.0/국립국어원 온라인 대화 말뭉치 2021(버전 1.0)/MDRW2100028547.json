      "utterance": [
        {
          "id": "MDRW2100028547.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028547.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028547.1.3",
          "form": "근데 동물 정말 좋아해요",
          "original_form": "근데 동물 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028547.1.4",
          "form": "고양이 너무 좋아해서 언젠간 꼭 키울거예요",
          "original_form": "고양이 너무 좋아해서 언젠간 꼭 키울거예요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028547.1.5",
          "form": "name2님은 동물 키우시나요?",
          "original_form": "&name2&님은 동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028547.1.6",
          "form": "네 저는 강아지 키워요ㅎㅎ 푸들",
          "original_form": "네 저는 강아지 키워요ㅎㅎ 푸들",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100028547.1.7",
          "form": "헐 ㅋㅋㅋ큐ㅠ 진짜 귀여울거 같아요",
          "original_form": "헐 ㅋㅋㅋ큐ㅠ 진짜 귀여울거 같아요",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028547.1.8",
          "form": "푸들 똑똑하고 눈도 똘망똘망하고 털도 곱슬거리고",
          "original_form": "푸들 똑똑하고 눈도 똘망똘망하고 털도 곱슬거리고",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028547.1.9",
          "form": "혹시 무슨 색 푸들이에요?",
          "original_form": "혹시 무슨 색 푸들이에요?",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028547.1.10",
          "form": "갈색인데 가슴은 흰색털이예요ㅋㅋ",
          "original_form": "갈색인데 가슴은 흰색털이예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028547.1.11",
          "form": "어머 귀여워라ㅋㅋㅋㅋㅋ",
          "original_form": "어머 귀여워라ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028547.1.12",
          "form": "엄마 아빠가 갈색 푸들이랑 흰색 푸들이었을까요?",
          "original_form": "엄마 아빠가 갈색 푸들이랑 흰색 푸들이었을까요?",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028547.1.13",
          "form": "그럴 것 같아요ㅋㅋ흰색 털이 군데군데 섞어있어서",
          "original_form": "그럴 것 같아요ㅋㅋ흰색 털이 군데군데 섞어있어서",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028547.1.14",
          "form": "제 친구중에 고양이 키우는 친구있는데",
          "original_form": "제 친구중에 고양이 키우는 친구있는데",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028547.1.15",
          "form": "진짜 액체 같더라구요ㅋㅋㅋ강아지는 다 뼈라 단단한데",
          "original_form": "진짜 액체 같더라구요ㅋㅋㅋ강아지는 다 뼈라 단단한데",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028547.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 고양이 키우는 사람들은 오히려 강아지 만지면 단단해서 놀란다고 하더라고요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 고양이 키우는 사람들은 오히려 강아지 만지면 단단해서 놀란다고 하더라고요",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028547.1.17",
          "form": "단단해도 귀엽고 몰캉해도 귀여운 털친구들..",
          "original_form": "단단해도 귀엽고 몰캉해도 귀여운 털친구들..",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028547.1.18",
          "form": "아 혹시 동물 털 관련한 알러지는 없으세요?",
          "original_form": "아 혹시 동물 털 관련한 알러지는 없으세요?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028547.1.19",
          "form": "아빠가 비염이 좀 있는데 다른 가족들은 다 괜찮아요",
          "original_form": "아빠가 비염이 좀 있는데 다른 가족들은 다 괜찮아요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028547.1.20",
          "form": "푸들은 곱슬거리는 털 때문에 잘 안빠져서 아빠도 괜찮더라구요",
          "original_form": "푸들은 곱슬거리는 털 때문에 잘 안빠져서 아빠도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028547.1.21",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028547.1.22",
          "form": "아 푸들털이 그렇구나",
          "original_form": "아 푸들털이 그렇구나",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028547.1.23",
          "form": "저희도 비염있는 가족이 있어서",
          "original_form": "저희도 비염있는 가족이 있어서",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028547.1.24",
          "form": "나중에 동물 키우게된다면 푸들을 생각해봐야겠어요",
          "original_form": "나중에 동물 키우게된다면 푸들을 생각해봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028547.1.25",
          "form": "비숑도 털 곱슬이라 잘 안빠진대요ㅎㅎ",
          "original_form": "비숑도 털 곱슬이라 잘 안빠진대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028547.1.26",
          "form": "비숑! 그렇네요 비숑도 그러겠네요ㅋㅋ",
          "original_form": "비숑! 그렇네요 비숑도 그러겠네요ㅋㅋ",
