      "utterance": [
        {
          "id": "MDRW2100007295.1.1",
          "form": "곧 끝나겠죠^^",
          "original_form": "곧 끝나겠죠^^",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007295.1.2",
          "form": "일은 어떠세요?",
          "original_form": "일은 어떠세요?",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007295.1.3",
          "form": "저는 지금 휴학하고 알바 중인 학생입니다!",
          "original_form": "저는 지금 휴학하고 알바 중인 학생입니다!",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007295.1.4",
          "form": "첫 알바라 조금 힘든..? 느낌은 있네요ㅎㅎ",
          "original_form": "첫 알바라 조금 힘든..? 느낌은 있네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007295.1.5",
          "form": "아~~ 요새 알바 구하기 힘들다던데",
          "original_form": "아~~ 요새 알바 구하기 힘들다던데",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007295.1.6",
          "form": "대단하십니다!!!",
          "original_form": "대단하십니다!!!",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007295.1.7",
          "form": "아닙니다.. 처음에 계속 떨어져서,,",
          "original_form": "아닙니다.. 처음에 계속 떨어져서,,",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007295.1.8",
          "form": "힘들다고 소문난 패푸점 알바에 지원했다가 덜컥 붙은 케이스라..ㅎㅎ",
          "original_form": "힘들다고 소문난 패푸점 알바에 지원했다가 덜컥 붙은 케이스라..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007295.1.9",
          "form": "아 패푸 많이 힘든가요??",
          "original_form": "아 패푸 많이 힘든가요??",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007295.1.10",
          "form": "기본적으로 손이 빨라야하는데 알바도 처음이고 몇달 안돼서 좀 힘들더라구요..",
          "original_form": "기본적으로 손이 빨라야하는데 알바도 처음이고 몇달 안돼서 좀 힘들더라구요..",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007295.1.11",
          "form": "많이 혼나기도 하구요..",
          "original_form": "많이 혼나기도 하구요..",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007295.1.12",
          "form": "음 맞아요 힘들어보여요",
          "original_form": "음 맞아요 힘들어보여요",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007295.1.13",
          "form": "서서 일하시니까 더",
          "original_form": "서서 일하시니까 더",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007295.1.14",
          "form": "맞아요.. 보통 못 앉게 해서 다리랑 발이 좀 많이 아프죠.",
          "original_form": "맞아요.. 보통 못 앉게 해서 다리랑 발이 좀 많이 아프죠.",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007295.1.15",
          "form": "그쵸 서있는게 참 힘들죠!! 쉴땐 쉬게해주지",
          "original_form": "그쵸 서있는게 참 힘들죠!! 쉴땐 쉬게해주지",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007295.1.16",
          "form": "근데 또 시급은 최저시급을 받고 하는 거라 현타가 많이 오더라구요ㅋㅋ",
          "original_form": "근데 또 시급은 최저시급을 받고 하는 거라 현타가 많이 오더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007295.1.17",
          "form": "그거 현타 올만 하죠",
          "original_form": "그거 현타 올만 하죠",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007295.1.18",
          "form": "힘은 힘대로 들고",
          "original_form": "힘은 힘대로 들고",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007295.1.19",
          "form": "맞아요,, 그래도 대기업이라 돈은 일 한 만큼 맞춰서 주긴 하는데",
          "original_form": "맞아요,, 그래도 대기업이라 돈은 일 한 만큼 맞춰서 주긴 하는데",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007295.1.20",
          "form": "돈이 가끔 병원비로 갈 때가 있어서 그게 좀 아깝죠..",
          "original_form": "돈이 가끔 병원비로 갈 때가 있어서 그게 좀 아깝죠..",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007295.1.21",
          "form": "아유 병원비 참ㅠ",
          "original_form": "아유 병원비 참ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007295.1.22",
          "form": "먹고 살자고 하는 일인데 허헣",
          "original_form": "먹고 살자고 하는 일인데 허헣",
          "speaker_id": "2",
          "time": "20210822 01:29"
        }
      ]
