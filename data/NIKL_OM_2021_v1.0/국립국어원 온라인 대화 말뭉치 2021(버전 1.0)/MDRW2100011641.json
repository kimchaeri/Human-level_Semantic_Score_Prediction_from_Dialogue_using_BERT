      "utterance": [
        {
          "id": "MDRW2100011641.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011641.1.2",
          "form": "딱히 좋진 않아요",
          "original_form": "딱히 좋진 않아요",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011641.1.3",
          "form": "멀리 다니는게 체력이 부담이고 돈이많이없어서요",
          "original_form": "멀리 다니는게 체력이 부담이고 돈이많이없어서요",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011641.1.4",
          "form": "저도 딱히 좋아하진않아요",
          "original_form": "저도 딱히 좋아하진않아요",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011641.1.5",
          "form": "좋았던 여행지 있으세요?",
          "original_form": "좋았던 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011641.1.6",
          "form": "전주 괜찮았어요",
          "original_form": "전주 괜찮았어요",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011641.1.7",
          "form": "외국은 대만 타이페이요",
          "original_form": "외국은 대만 타이페이요",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011641.1.8",
          "form": "저는 송도가 괜찮았어요",
          "original_form": "저는 송도가 괜찮았어요",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011641.1.9",
          "form": "바다 야경예쁘고",
          "original_form": "바다 야경예쁘고",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011641.1.10",
          "form": "도심 하늘에 갈매기가 날아다니는게 웃기고요ㅋㅋ",
          "original_form": "도심 하늘에 갈매기가 날아다니는게 웃기고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011641.1.11",
          "form": "아아 네",
          "original_form": "아아 네",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011641.1.12",
          "form": "송도는 못가봤어요",
          "original_form": "송도는 못가봤어요",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011641.1.13",
          "form": "갈매기가 날아다닌다는거 첨듣네요",
          "original_form": "갈매기가 날아다닌다는거 첨듣네요",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011641.1.14",
          "form": "코로나 끝나고 스페인가고싶네요",
          "original_form": "코로나 끝나고 스페인가고싶네요",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011641.1.15",
          "form": "그러게요 넘 답답하네요",
          "original_form": "그러게요 넘 답답하네요",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011641.1.16",
          "form": "전주는 어떤게 좋았나요?",
          "original_form": "전주는 어떤게 좋았나요?",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100011641.1.17",
          "form": "도시가 정돈되고 조용조용한 분위기여서 좋았어요",
          "original_form": "도시가 정돈되고 조용조용한 분위기여서 좋았어요",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011641.1.18",
          "form": "저도 가봐야겠어요",
          "original_form": "저도 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011641.1.19",
          "form": "네 시간되실때 함가보세요",
          "original_form": "네 시간되실때 함가보세요",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011641.1.20",
          "form": "관광지 말고 약간 변두리 주택가",
          "original_form": "관광지 말고 약간 변두리 주택가",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011641.1.21",
          "form": "그런곳도좋았어요",
          "original_form": "그런곳도좋았어요",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011641.1.22",
          "form": "어떤음식 드셨나요?",
          "original_form": "어떤음식 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011641.1.23",
          "form": "혼자가서 맛있는건 못먹었어요",
          "original_form": "혼자가서 맛있는건 못먹었어요",
          "speaker_id": "2",
          "time": "20210824 15:02"
        }
      ]
