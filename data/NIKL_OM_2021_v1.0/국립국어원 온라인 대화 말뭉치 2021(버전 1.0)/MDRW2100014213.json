      "utterance": [
        {
          "id": "MDRW2100014213.1.1",
          "form": "지금 비 엄청 오지않나요 ㅠㅠ",
          "original_form": "지금 비 엄청 오지않나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014213.1.2",
          "form": "지역이 거리가 있나봐요",
          "original_form": "지역이 거리가 있나봐요",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014213.1.3",
          "form": "저희동네는 비 안와요",
          "original_form": "저희동네는 비 안와요",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014213.1.4",
          "form": "아 저희 지역은 지금 엄청 많이 와요 !",
          "original_form": "아 저희 지역은 지금 엄청 많이 와요 !",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014213.1.5",
          "form": "천둥번개도 쳐요 ㅠ",
          "original_form": "천둥번개도 쳐요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014213.1.6",
          "form": "밑지방이라 그런가봐용",
          "original_form": "밑지방이라 그런가봐용",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014213.1.7",
          "form": "혹시 밖이에요?ㅠㅠ",
          "original_form": "혹시 밖이에요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014213.1.8",
          "form": "아직은 실내인데 ㅠㅠ 곧 퇴근해서 나가야해요 !",
          "original_form": "아직은 실내인데 ㅠㅠ 곧 퇴근해서 나가야해요 !",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014213.1.9",
          "form": "아... 하필 지금 퇴근시간에 비라니ㅠㅠ",
          "original_form": "아... 하필 지금 퇴근시간에 비라니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014213.1.10",
          "form": "비오는건 좋은데 실내에서 나갈일 없을때 왔음 좋겠어요ㅋㅋㅋ",
          "original_form": "비오는건 좋은데 실내에서 나갈일 없을때 왔음 좋겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014213.1.11",
          "form": "출퇴근할때 특히 더 비가 심해지는거 같아요",
          "original_form": "출퇴근할때 특히 더 비가 심해지는거 같아요",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014213.1.12",
          "form": "저만 그런가욬ㄱㅋ",
          "original_form": "저만 그런가욬ㄱㅋ",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014213.1.13",
          "form": "저도 그래요ㅋㅋㅋ 어제 출근시간에 비가 엄청 오더니 출근하니까 그치는거 보고",
          "original_form": "저도 그래요ㅋㅋㅋ 어제 출근시간에 비가 엄청 오더니 출근하니까 그치는거 보고",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014213.1.14",
          "form": "너무 얄밉더라고요 날씨가",
          "original_form": "너무 얄밉더라고요 날씨가",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014213.1.15",
          "form": "진짜욬ㅋㅋ 그리고 요즘은 또",
          "original_form": "진짜욬ㅋㅋ 그리고 요즘은 또",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014213.1.16",
          "form": "일기예보도 안맞으니까 비 안온다해서 우산 안챙겨가면",
          "original_form": "일기예보도 안맞으니까 비 안온다해서 우산 안챙겨가면",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014213.1.17",
          "form": "비오고 그래요 ㅋㅋ",
          "original_form": "비오고 그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014213.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 약간 동남아 날씨 같기도 하고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 약간 동남아 날씨 같기도 하고",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014213.1.19",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014213.1.20",
          "form": "지금은 또 비 그쳐서",
          "original_form": "지금은 또 비 그쳐서",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014213.1.21",
          "form": "날씨가 지맘대로네요",
          "original_form": "날씨가 지맘대로네요",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014213.1.22",
          "form": "어제 저 퇴근할때는 우산썼는데도 바람이 많이 불어서",
          "original_form": "어제 저 퇴근할때는 우산썼는데도 바람이 많이 불어서",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014213.1.23",
          "form": "완전 생쥐됐었어요",
          "original_form": "완전 생쥐됐었어요",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014213.1.24",
          "form": "아 진짜 그런경우 젤 싫어요 ㅎㅎ",
          "original_form": "아 진짜 그런경우 젤 싫어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014213.1.25",
          "form": "우산을 써도 비가 대각선으로 내리니까ㅋㄲ",
          "original_form": "우산을 써도 비가 대각선으로 내리니까ㅋㄲ",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014213.1.26",
          "form": "차라리 비 안오고 더운게 나은거같아요 ㅎ",
          "original_form": "차라리 비 안오고 더운게 나은거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014213.1.27",
          "form": "맞아요 우산을 쓴 의미도 없고 불쾌지수 올라가고ㅠㅠ",
          "original_form": "맞아요 우산을 쓴 의미도 없고 불쾌지수 올라가고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 19:04"
        }
      ]
