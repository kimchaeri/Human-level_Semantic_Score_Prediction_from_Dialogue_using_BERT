      "utterance": [
        {
          "id": "MDRW2100028524.1.1",
          "form": "휴학해서 수강신청은 안하겠구나",
          "original_form": "휴학해서 수강신청은 안하겠구나",
          "speaker_id": "1",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100028524.1.2",
          "form": "친구들은 다 개학했지만요 ㅎㅎ",
          "original_form": "친구들은 다 개학했지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028524.1.3",
          "form": "휴학한 것도 학교 스트레스 때문이라며",
          "original_form": "휴학한 것도 학교 스트레스 때문이라며",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028524.1.4",
          "form": "많이 힘들었어?",
          "original_form": "많이 힘들었어?",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028524.1.5",
          "form": "좀 그랬어요 부담도 되고 불안하기도 하고",
          "original_form": "좀 그랬어요 부담도 되고 불안하기도 하고",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028524.1.6",
          "form": "미래가 안보여서 ㅠ",
          "original_form": "미래가 안보여서 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028524.1.7",
          "form": "좋은 학과인데 왜 그러니",
          "original_form": "좋은 학과인데 왜 그러니",
          "speaker_id": "1",
          "time": "20210902 22:27"
        },
        {
          "id": "MDRW2100028524.1.8",
          "form": "좋은 학과여도 제가 잘해야 하는 건데 그게 잘 안되더라고요",
          "original_form": "좋은 학과여도 제가 잘해야 하는 건데 그게 잘 안되더라고요",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028524.1.9",
          "form": "고생이 많았다 푹 쉬고 다음에 열심히 하자",
          "original_form": "고생이 많았다 푹 쉬고 다음에 열심히 하자",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028524.1.10",
          "form": "네 ㅎㅎㅠㅠ 내년부터 복학이니까요",
          "original_form": "네 ㅎㅎㅠㅠ 내년부터 복학이니까요",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028524.1.11",
          "form": "그래도 학교 다닐때는 다른건 좋았어요!!",
          "original_form": "그래도 학교 다닐때는 다른건 좋았어요!!",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028524.1.12",
          "form": "동아리 같은 것도 들었니?",
          "original_form": "동아리 같은 것도 들었니?",
          "speaker_id": "1",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100028524.1.13",
          "form": "저 학술동아리 들었거든요 요즘은 안하지만",
          "original_form": "저 학술동아리 들었거든요 요즘은 안하지만",
          "speaker_id": "2",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028524.1.14",
          "form": "예전에 방학때 와서 피피티 만들고 그랬죠 ㅋㅋㅋ",
          "original_form": "예전에 방학때 와서 피피티 만들고 그랬죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028524.1.15",
          "form": "후배들 가르치는거 힘들지",
          "original_form": "후배들 가르치는거 힘들지",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028524.1.16",
          "form": "그래도 좋은 일 했어",
          "original_form": "그래도 좋은 일 했어",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100028524.1.17",
          "form": "애들이 말 잘 들어서 좋았어요 그리고 저도 똑같이 배웠으니까",
          "original_form": "애들이 말 잘 들어서 좋았어요 그리고 저도 똑같이 배웠으니까",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028524.1.18",
          "form": "물려주는게 맞는거같더라구요",
          "original_form": "물려주는게 맞는거같더라구요",
          "speaker_id": "2",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028524.1.19",
          "form": "지금은 동아리 왜 안하니? 휴학해서?",
          "original_form": "지금은 동아리 왜 안하니? 휴학해서?",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028524.1.20",
          "form": "휴학한것도 있는데",
          "original_form": "휴학한것도 있는데",
          "speaker_id": "2",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028524.1.21",
          "form": "동아리 친구들이 대부분 남자였거든요",
          "original_form": "동아리 친구들이 대부분 남자였거든요",
          "speaker_id": "2",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100028524.1.22",
          "form": "그래서 부장도 군대가고 ㅋㅋㅋ 그랬죠",
          "original_form": "그래서 부장도 군대가고 ㅋㅋㅋ 그랬죠",
          "speaker_id": "2",
          "time": "20210902 22:31"
