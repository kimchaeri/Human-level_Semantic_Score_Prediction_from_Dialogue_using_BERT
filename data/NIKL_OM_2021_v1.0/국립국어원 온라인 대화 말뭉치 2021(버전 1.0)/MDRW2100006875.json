        },
        {
          "id": "MDRW2100006875.1.2",
          "form": "나이키",
          "original_form": "나이키",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.3",
          "form": "신발 당첨되서 첨으로 한정펀 삼",
          "original_form": "신발 당첨되서 첨으로 한정펀 삼",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.4",
          "form": "운동화?",
          "original_form": "운동화?",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.5",
          "form": "오 나는 광탈",
          "original_form": "오 나는 광탈",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.6",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.7",
          "form": "사카이 됨요",
          "original_form": "사카이 됨요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.8",
          "form": "네이비 됨\n  .",
          "original_form": "네이비 됨\n  .",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006875.1.9",
          "form": "완전부러움",
          "original_form": "완전부러움",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006875.1.10",
          "form": "로또수준",
          "original_form": "로또수준",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006875.1.11",
          "form": "첨 됨  놀람",
          "original_form": "첨 됨  놀람",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006875.1.12",
          "form": "기대안했음",
          "original_form": "기대안했음",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006875.1.13",
          "form": "신을까 팔카 고민중",
          "original_form": "신을까 팔카 고민중",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006875.1.14",
          "form": "신는다 70프로임",
          "original_form": "신는다 70프로임",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006875.1.15",
          "form": "얼마붙음?팔면?",
          "original_form": "얼마붙음?팔면?",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006875.1.16",
          "form": "21에사고 지금 50 임",
          "original_form": "21에사고 지금 50 임",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006875.1.17",
          "form": "나같으면 팜",
          "original_form": "나같으면 팜",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006875.1.18",
          "form": "사이즈 마다 가격 다름",
          "original_form": "사이즈 마다 가격 다름",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006875.1.19",
          "form": "한정신발좋아함?",
          "original_form": "한정신발좋아함?",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006875.1.20",
          "form": "내가 나이키 좋아함 근데",
          "original_form": "내가 나이키 좋아함 근데",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006875.1.21",
          "form": "한정판 첨이고 가격이 적당해서",
          "original_form": "한정판 첨이고 가격이 적당해서",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006875.1.22",
          "form": "신을까 생각둥",
          "original_form": "신을까 생각둥",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006875.1.23",
          "form": "한정모으는거아니면 판다 그걸로 나이키 2개산다",
          "original_form": "한정모으는거아니면 판다 그걸로 나이키 2개산다",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006875.1.24",
          "form": "어떰?",
          "original_form": "어떰?",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006875.1.25",
          "form": "막 비싸면 팔라고 생각",
          "original_form": "막 비싸면 팔라고 생각",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100006875.1.26",
          "form": "금액애매한가보네",
