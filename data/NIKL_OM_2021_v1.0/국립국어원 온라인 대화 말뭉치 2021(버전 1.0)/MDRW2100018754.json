      "utterance": [
        {
          "id": "MDRW2100018754.1.1",
          "form": "안녕하세요 취미가 어떻게 되세요?",
          "original_form": "안녕하세요 취미가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018754.1.2",
          "form": "좋은 아침입니다~",
          "original_form": "좋은 아침입니다~",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018754.1.3",
          "form": "저는 티비보고 음악듣기 좋아해요",
          "original_form": "저는 티비보고 음악듣기 좋아해요",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018754.1.4",
          "form": "독서도 즐깁니다",
          "original_form": "독서도 즐깁니다",
          "speaker_id": "2",
          "time": "20210827 11:48"
        },
        {
          "id": "MDRW2100018754.1.5",
          "form": "우와 저랑 되게 비슷하시네요?!",
          "original_form": "우와 저랑 되게 비슷하시네요?!",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018754.1.6",
          "form": "저는 음악 듣기랑 독서 좋아해요",
          "original_form": "저는 음악 듣기랑 독서 좋아해요",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018754.1.7",
          "form": "요즘엔 어떤 책 읽으세요?",
          "original_form": "요즘엔 어떤 책 읽으세요?",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018754.1.8",
          "form": "저는 칼빈의 기독교 강요 읽고 있어요",
          "original_form": "저는 칼빈의 기독교 강요 읽고 있어요",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018754.1.9",
          "form": "아 교회 다시는구나",
          "original_form": "아 교회 다시는구나",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018754.1.10",
          "form": "어때요 책 재밌나요?",
          "original_form": "어때요 책 재밌나요?",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018754.1.11",
          "form": "네네 정말 좋은 책이에요",
          "original_form": "네네 정말 좋은 책이에요",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018754.1.12",
          "form": "많이 배우고 알아가는!",
          "original_form": "많이 배우고 알아가는!",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018754.1.13",
          "form": "그렇군요 ㅎㅎ 저는 요즘 소설 읽어요",
          "original_form": "그렇군요 ㅎㅎ 저는 요즘 소설 읽어요",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018754.1.14",
          "form": "우와 어떤 소설책 읽으시나용?",
          "original_form": "우와 어떤 소설책 읽으시나용?",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018754.1.15",
          "form": "해리포터요!",
          "original_form": "해리포터요!",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018754.1.16",
          "form": "해리포터 유명하더라구요",
          "original_form": "해리포터 유명하더라구요",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018754.1.17",
          "form": "아ㅋㅋㅋ 저도 해리포터 좋아해요",
          "original_form": "아ㅋㅋㅋ 저도 해리포터 좋아해요",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018754.1.18",
          "form": "제가 너무 좋아하는 책이자 영화라서ㅠㅠ",
          "original_form": "제가 너무 좋아하는 책이자 영화라서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018754.1.19",
          "form": "맞아요 보는데 재밌더라고요",
          "original_form": "맞아요 보는데 재밌더라고요",
          "speaker_id": "2",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018754.1.20",
          "form": "첨엔 아동용 소설이라고 생각했는데",
          "original_form": "첨엔 아동용 소설이라고 생각했는데",
          "speaker_id": "2",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018754.1.21",
          "form": "성인이 읽어도 너무 멋지고 탄탄한거 같아요",
          "original_form": "성인이 읽어도 너무 멋지고 탄탄한거 같아요",
          "speaker_id": "2",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018754.1.22",
          "form": "네 맞아요 스토리가 굉당히 탄탄하고 짜임새도 좋더라고요",
          "original_form": "네 맞아요 스토리가 굉당히 탄탄하고 짜임새도 좋더라고요",
          "speaker_id": "1",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018754.1.23",
          "form": "저는 소설책 말고도 사회과학 도서도 좋아해요",
          "original_form": "저는 소설책 말고도 사회과학 도서도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100018754.1.24",
          "form": "사회과학 책도 좋죠",
          "original_form": "사회과학 책도 좋죠",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018754.1.25",
          "form": "요즘엔 어떤 책이 인기인가요?",
          "original_form": "요즘엔 어떤 책이 인기인가요?",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018754.1.26",
          "form": "요즘엔 주식 관련 도서가 많은 것 같아요",
          "original_form": "요즘엔 주식 관련 도서가 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018754.1.27",
          "form": "국제관계 내용도 많구요",
          "original_form": "국제관계 내용도 많구요",
          "speaker_id": "1",
          "time": "20210827 11:54"
        }
      ]
    }
  ]
