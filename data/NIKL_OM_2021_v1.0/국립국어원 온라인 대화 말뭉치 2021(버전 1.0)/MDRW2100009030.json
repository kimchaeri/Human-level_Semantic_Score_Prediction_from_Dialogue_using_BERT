        },
        {
          "id": "MDRW2100009030.1.2",
          "form": "아녀 전 그냥 학생입니당 ㅋㅋ",
          "original_form": "아녀 전 그냥 학생입니당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100009030.1.3",
          "form": "무슨 직업 가지고싶어영?",
          "original_form": "무슨 직업 가지고싶어영?",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009030.1.4",
          "form": "음 아직은 딱히 아무생각업서요..ㅠ",
          "original_form": "음 아직은 딱히 아무생각업서요..ㅠ",
          "speaker_id": "2",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100009030.1.5",
          "form": "대학생이에용?",
          "original_form": "대학생이에용?",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009030.1.6",
          "form": "아녀 중3이요!ㅋㅋㅋ",
          "original_form": "아녀 중3이요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009030.1.7",
          "form": "오호 신기하네용",
          "original_form": "오호 신기하네용",
          "speaker_id": "1",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100009030.1.8",
          "form": "저는 치과다녀용",
          "original_form": "저는 치과다녀용",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009030.1.9",
          "form": "우와 의사에요 치의생사 에용?",
          "original_form": "우와 의사에요 치의생사 에용?",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009030.1.10",
          "form": "치위생사예용 ㅎㅎ",
          "original_form": "치위생사예용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009030.1.11",
          "form": "알바는 해본적없어요?",
          "original_form": "알바는 해본적없어요?",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009030.1.12",
          "form": "네네 없어요!!",
          "original_form": "네네 없어요!!",
          "speaker_id": "2",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100009030.1.13",
          "form": "주변 친구들은 하지않아용?",
          "original_form": "주변 친구들은 하지않아용?",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009030.1.14",
          "form": "아뇽 아무도 안해욤",
          "original_form": "아뇽 아무도 안해욤",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100009030.1.15",
          "form": "오 전 중학교때 서빙알바 몇번 해봤던것같아여",
          "original_form": "오 전 중학교때 서빙알바 몇번 해봤던것같아여",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009030.1.16",
          "form": "오오 저두 알바 해보고는 싶지만 어떻게 해야될지도 모르겟고 시간도 없네용..",
          "original_form": "오오 저두 알바 해보고는 싶지만 어떻게 해야될지도 모르겟고 시간도 없네용..",
