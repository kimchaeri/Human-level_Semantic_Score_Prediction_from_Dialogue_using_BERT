      "utterance": [
        {
          "id": "MDRW2100000325.1.1",
          "form": "요새 뭐 커뮤니티하나?",
          "original_form": "요새 뭐 커뮤니티하나?",
          "speaker_id": "1",
          "time": "20210818 15:26"
        },
        {
          "id": "MDRW2100000325.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210818 15:27"
        },
        {
          "id": "MDRW2100000325.1.3",
          "form": "난 온라인 모임 안해",
          "original_form": "난 온라인 모임 안해",
          "speaker_id": "1",
          "time": "20210818 15:27"
        },
        {
          "id": "MDRW2100000325.1.4",
          "form": "재미 엄숴",
          "original_form": "재미 엄숴",
          "speaker_id": "1",
          "time": "20210818 15:27"
        },
        {
          "id": "MDRW2100000325.1.5",
          "form": "할게 뭐가 있나 폰겜 카페 종종 들어가서 잡글 남기거나",
          "original_form": "할게 뭐가 있나 폰겜 카페 종종 들어가서 잡글 남기거나",
          "speaker_id": "2",
          "time": "20210818 15:27"
        },
        {
          "id": "MDRW2100000325.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210818 15:28"
        },
        {
          "id": "MDRW2100000325.1.7",
          "form": "김프 사이트가서 한번씩 구경하거나",
          "original_form": "김프 사이트가서 한번씩 구경하거나",
          "speaker_id": "2",
          "time": "20210818 15:28"
        },
        {
          "id": "MDRW2100000325.1.8",
          "form": "코인사이트",
          "original_form": "코인사이트",
          "speaker_id": "1",
          "time": "20210818 15:28"
        },
        {
          "id": "MDRW2100000325.1.9",
          "form": "다들 미쳐 날뛴다",
          "original_form": "다들 미쳐 날뛴다",
          "speaker_id": "1",
          "time": "20210818 15:28"
        },
        {
          "id": "MDRW2100000325.1.10",
          "form": "우울하군 폰 켠게 최저점인데 그때 싹 팔다니",
          "original_form": "우울하군 폰 켠게 최저점인데 그때 싹 팔다니",
          "speaker_id": "2",
          "time": "20210818 15:29"
        },
        {
          "id": "MDRW2100000325.1.11",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210818 15:29"
        },
        {
          "id": "MDRW2100000325.1.12",
          "form": "코인 커뮤니티는 들어가나보네",
          "original_form": "코인 커뮤니티는 들어가나보네",
          "speaker_id": "2",
          "time": "20210818 15:29"
        },
        {
          "id": "MDRW2100000325.1.13",
          "form": "아까 최저점에 정리",
          "original_form": "아까 최저점에 정리",
          "speaker_id": "1",
          "time": "20210818 15:29"
        },
        {
          "id": "MDRW2100000325.1.14",
          "form": "너도 다 팜?",
          "original_form": "너도 다 팜?",
          "speaker_id": "2",
          "time": "20210818 15:29"
        },
        {
          "id": "MDRW2100000325.1.15",
          "form": "손해 두배로 봤네 ㅎㅎ 아침이나 어제 정리안해서 타이밍의 귀재다",
          "original_form": "손해 두배로 봤네 ㅎㅎ 아침이나 어제 정리안해서 타이밍의 귀재다",
          "speaker_id": "2",
          "time": "20210818 15:30"
        },
        {
          "id": "MDRW2100000325.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210818 15:30"
        },
        {
          "id": "MDRW2100000325.1.17",
          "form": "일론 도지 어찌 생각하냐",
          "original_form": "일론 도지 어찌 생각하냐",
          "speaker_id": "2",
          "time": "20210818 15:30"
        },
        {
          "id": "MDRW2100000325.1.18",
          "form": "다팔지는 않았어",
          "original_form": "다팔지는 않았어",
          "speaker_id": "1",
          "time": "20210818 15:30"
        },
        {
          "id": "MDRW2100000325.1.19",
          "form": "도지 오르지 않을까",
          "original_form": "도지 오르지 않을까",
          "speaker_id": "1",
          "time": "20210818 15:31"
        },
