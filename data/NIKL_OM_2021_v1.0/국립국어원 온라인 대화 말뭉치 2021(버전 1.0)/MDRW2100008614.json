      "utterance": [
        {
          "id": "MDRW2100008614.1.1",
          "form": "안녕하소요",
          "original_form": "안녕하소요",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008614.1.2",
          "form": "최근 쇼핑한것 중에 제일 마음에 드는 건 뭐였나요?",
          "original_form": "최근 쇼핑한것 중에 제일 마음에 드는 건 뭐였나요?",
          "speaker_id": "1",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008614.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008614.1.4",
          "form": "저 보조배터리요! 노트북도 충전이 되는거여서 편했어요 ㅎㅎ",
          "original_form": "저 보조배터리요! 노트북도 충전이 되는거여서 편했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008614.1.5",
          "form": "전자기기 좋아하시나요",
          "original_form": "전자기기 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008614.1.6",
          "form": "와 신기하네요 ㅋㅋㅋ 저는 전자기기는 잘 몰라요,, 지금 제 핸드폰이랑 노트북도 5년째 쓰고 있어요",
          "original_form": "와 신기하네요 ㅋㅋㅋ 저는 전자기기는 잘 몰라요,, 지금 제 핸드폰이랑 노트북도 5년째 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008614.1.7",
          "form": "저 카페에서 닉네임 보고 연락드렸어요",
          "original_form": "저 카페에서 닉네임 보고 연락드렸어요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008614.1.8",
          "form": "앜ㅋㅋㅋㅋ 안녕하세요",
          "original_form": "앜ㅋㅋㅋㅋ 안녕하세요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008614.1.9",
          "form": "연락주셔서 감사해요 주제 가끔 연애 맞선 이런거 걸리면 바꾸고 그런답니다..",
          "original_form": "연락주셔서 감사해요 주제 가끔 연애 맞선 이런거 걸리면 바꾸고 그런답니다..",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008614.1.10",
          "form": "핸드폰 오년이면 진짜 오래쓰시네요!",
          "original_form": "핸드폰 오년이면 진짜 오래쓰시네요!",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008614.1.11",
          "form": "안녕하^소^요 가 싸인이었어요 다른 분께도 연락드렸었는데 아직 눈치챈분은 없네요,,",
          "original_form": "안녕하^소^요 가 싸인이었어요 다른 분께도 연락드렸었는데 아직 눈치챈분은 없네요,,",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008614.1.12",
          "form": "다행이에요 편하게 얘기할수있어서 ㅎㅎ",
          "original_form": "다행이에요 편하게 얘기할수있어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008614.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008614.1.14",
          "form": "저는 최근에 휴플러스 목어깨안마기 샀어요",
          "original_form": "저는 최근에 휴플러스 목어깨안마기 샀어요",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008614.1.15",
          "form": "5만원인데 가성비 좋더라구요 ㅎㅎ",
          "original_form": "5만원인데 가성비 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008614.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 헐 대박 눈치 못챘어요 완전 영민하시다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 헐 대박 눈치 못챘어요 완전 영민하시다",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.17",
          "form": "오 저도 요새 목어깨 너무뭉치는데",
          "original_form": "오 저도 요새 목어깨 너무뭉치는데",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.18",
          "form": "ㅋㅋㅋㅋㅋ감사해요",
          "original_form": "ㅋㅋㅋㅋㅋ감사해요",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.19",
          "form": "많이 풀리나요? 저 완전 요새 거북이에요",
          "original_form": "많이 풀리나요? 저 완전 요새 거북이에요",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.20",
          "form": "거북목",
          "original_form": "거북목",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.21",
          "form": "하는 동안에는 풀리고 시원한데",
          "original_form": "하는 동안에는 풀리고 시원한데",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.22",
          "form": "담날 출근하면 원상복귀죠..",
          "original_form": "담날 출근하면 원상복귀죠..",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ슬픈 현실",
          "original_form": "ㅋㅋㅋㅋㅋㅋ슬픈 현실",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008614.1.24",
          "form": "이거 종아리에도 할 수 있어서 저는 넘 좋더라구요!!",
          "original_form": "이거 종아리에도 할 수 있어서 저는 넘 좋더라구요!!",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008614.1.25",
          "form": "저도 뽐뿌온느데요",
          "original_form": "저도 뽐뿌온느데요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008614.1.26",
          "form": "손으로 마사지하면 손목 아프잖아요 ㅎㅎ",
          "original_form": "손으로 마사지하면 손목 아프잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008614.1.27",
          "form": "맞아요 맞아요 저도 마사지건 살까 하고있었는데 추천 감사합니다",
          "original_form": "맞아요 맞아요 저도 마사지건 살까 하고있었는데 추천 감사합니다",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008614.1.28",
          "form": "아녜요 ㅋㅋㅋ저도 마사지건이랑 고민했는데",
          "original_form": "아녜요 ㅋㅋㅋ저도 마사지건이랑 고민했는데",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008614.1.29",
          "form": "마사지건하려면 제 손은 쉬질 못하잖아요,,,",
          "original_form": "마사지건하려면 제 손은 쉬질 못하잖아요,,,",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008614.1.30",
          "form": "그리고 한번에 양쪽 주물주물하는 느낌을 받고싶어서",
          "original_form": "그리고 한번에 양쪽 주물주물하는 느낌을 받고싶어서",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008614.1.31",
          "form": "맞네요.. 손목도 아파요 손목보호대도 사야할판",
          "original_form": "맞네요.. 손목도 아파요 손목보호대도 사야할판",
          "speaker_id": "2",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008614.1.32",
          "form": "휴플러스로 샀네요,,",
          "original_form": "휴플러스로 샀네요,,",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008614.1.33",
          "form": "저는 손목보호대도 차고 있어요 ㅋㅋㅋㅋ 약국에서 샀어요! 되게 좋아요",
          "original_form": "저는 손목보호대도 차고 있어요 ㅋㅋㅋㅋ 약국에서 샀어요! 되게 좋아요",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008614.1.34",
          "form": "이름이 반테린이었나 그랬을거에요",
          "original_form": "이름이 반테린이었나 그랬을거에요",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008614.1.35",
          "form": "손목보호대도 사셨구낰ㅋㅋㅋㅋㅋㅋ",
          "original_form": "손목보호대도 사셨구낰ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008614.1.36",
          "form": "앞서가시는군요",
          "original_form": "앞서가시는군요",
          "speaker_id": "2",
          "time": "20210822 17:30"
        },
        {
          "id": "MDRW2100008614.1.37",
          "form": "건강기능제품? 이런거 엄청 사게 되네요 저는 무릎보호대 얼마전에 샀어요",
          "original_form": "건강기능제품? 이런거 엄청 사게 되네요 저는 무릎보호대 얼마전에 샀어요",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008614.1.38",
          "form": "살기위해 샀어요 젓가락질이 아프더라구요..",
          "original_form": "살기위해 샀어요 젓가락질이 아프더라구요..",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008614.1.39",
          "form": "무릎보호대는 어떤가요??",
          "original_form": "무릎보호대는 어떤가요??",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008614.1.40",
          "form": "운동하셔요??",
          "original_form": "운동하셔요??",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008614.1.41",
          "form": "저 등산할때 쓰려고 샀는데 테이핑이 나은거같아요",
          "original_form": "저 등산할때 쓰려고 샀는데 테이핑이 나은거같아요",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008614.1.42",
          "form": "보호대 여름에 너무 땀차고 효과있는지 잘 모르겠어서요",
          "original_form": "보호대 여름에 너무 땀차고 효과있는지 잘 모르겠어서요",
          "speaker_id": "2",
          "time": "20210822 17:32"
        }
