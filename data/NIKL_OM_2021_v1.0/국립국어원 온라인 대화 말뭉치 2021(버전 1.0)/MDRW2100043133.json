          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043133.1.3",
          "form": "그건뭐야?",
          "original_form": "그건뭐야?",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043133.1.4",
          "form": "마블에서",
          "original_form": "마블에서",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100043133.1.5",
          "form": "나오는거",
          "original_form": "나오는거",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100043133.1.6",
          "form": "마동석나오는거있자나 안젤리나졸리",
          "original_form": "마동석나오는거있자나 안젤리나졸리",
          "speaker_id": "2",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100043133.1.7",
          "form": "아 그게 그거구나 ㅋㅋ 보고싶긴했어",
          "original_form": "아 그게 그거구나 ㅋㅋ 보고싶긴했어",
          "speaker_id": "1",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043133.1.8",
          "form": "영화샹치 흥행에 따라 달렸대",
          "original_form": "영화샹치 흥행에 따라 달렸대",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043133.1.9",
          "form": "ㅋㅋ망하면 밀릴듯",
          "original_form": "ㅋㅋ망하면 밀릴듯",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043133.1.10",
          "form": "샹치흥해서 하지않을까",
          "original_form": "샹치흥해서 하지않을까",
          "speaker_id": "1",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043133.1.11",
          "form": "할거가태",
          "original_form": "할거가태",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100043133.1.12",
