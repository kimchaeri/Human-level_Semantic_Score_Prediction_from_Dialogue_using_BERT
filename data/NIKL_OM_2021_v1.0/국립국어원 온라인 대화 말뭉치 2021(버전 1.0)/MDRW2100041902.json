      "utterance": [
        {
          "id": "MDRW2100041902.1.1",
          "form": "요즘 저탄수 다이어트가 인기더라",
          "original_form": "요즘 저탄수 다이어트가 인기더라",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041902.1.2",
          "form": "흠 저두 그거 찾아보고 있었어요",
          "original_form": "흠 저두 그거 찾아보고 있었어요",
          "speaker_id": "1",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041902.1.3",
          "form": "다이어트 잘되가니?",
          "original_form": "다이어트 잘되가니?",
          "speaker_id": "2",
          "time": "20210906 17:27"
        },
        {
          "id": "MDRW2100041902.1.4",
          "form": "돈주고 살빼놨더니 이자식이",
          "original_form": "돈주고 살빼놨더니 이자식이",
          "speaker_id": "2",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041902.1.5",
          "form": "아니 좀 맛있는거 먹고싶을수도 있구 그렇죠 뭐 너무하시네",
          "original_form": "아니 좀 맛있는거 먹고싶을수도 있구 그렇죠 뭐 너무하시네",
          "speaker_id": "1",
          "time": "20210906 17:28"
        },
        {
          "id": "MDRW2100041902.1.6",
          "form": "조심해 살 찌는거 금방이다",
          "original_form": "조심해 살 찌는거 금방이다",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041902.1.7",
          "form": "저두 나름 조심은 하구있어요 ㅠㅠ",
          "original_form": "저두 나름 조심은 하구있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041902.1.8",
          "form": "근데 너무 맛있는걸 어떡해˜˜",
          "original_form": "근데 너무 맛있는걸 어떡해˜˜",
          "speaker_id": "1",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041902.1.9",
          "form": "너두 저탄수 다이어트해",
          "original_form": "너두 저탄수 다이어트해",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041902.1.10",
          "form": "엄마가 도와주면 할수있을지두",
          "original_form": "엄마가 도와주면 할수있을지두",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041902.1.11",
          "form": "스스로 알아서 해야지",
          "original_form": "스스로 알아서 해야지",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041902.1.12",
          "form": "그래두 레시피도 많이 찾아봤어요 ㅋㅋㅋ",
          "original_form": "그래두 레시피도 많이 찾아봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100041902.1.13",
          "form": "아몬드 가루 있으니까 그걸로 빵 만들어 먹어봐",
          "original_form": "아몬드 가루 있으니까 그걸로 빵 만들어 먹어봐",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041902.1.14",
          "form": "녹차가루도 집에 있어요",
          "original_form": "녹차가루도 집에 있어요",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041902.1.15",
          "form": "물음표 빠졌다",
          "original_form": "물음표 빠졌다",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041902.1.16",
          "form": "있어 너 에전에 라떼 만들어먹는다고 사둔거",
          "original_form": "있어 너 에전에 라떼 만들어먹는다고 사둔거",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041902.1.17",
          "form": "완전 오래된거 아닌가",
          "original_form": "완전 오래된거 아닌가",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100041902.1.18",
          "form": "저 유당불내증 생겨서 안먹었는데요 우유ㅠ ㅠ",
          "original_form": "저 유당불내증 생겨서 안먹었는데요 우유ㅠ ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100041902.1.19",
          "form": "그랬나?? 모르겠다",
          "original_form": "그랬나?? 모르겠다",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041902.1.20",
          "form": "저 중학교3학년때부터 우유 못마셨다구요 ㅋㅋㅋ",
          "original_form": "저 중학교3학년때부터 우유 못마셨다구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041902.1.21",
          "form": "엄마 방금 검색해보고 왔어",
          "original_form": "엄마 방금 검색해보고 왔어",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041902.1.22",
          "form": "유당불내증에는 우유 조금씩 마셔보기 시작하는게 좋다더라",
          "original_form": "유당불내증에는 우유 조금씩 마셔보기 시작하는게 좋다더라",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041902.1.23",
          "form": "근데 우유 좋아해서 맨날 개처럼 마시게 돼요",
          "original_form": "근데 우유 좋아해서 맨날 개처럼 마시게 돼요",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041902.1.24",
          "form": "개처럼인지 소처럼인지 고양이처럼인지",
          "original_form": "개처럼인지 소처럼인지 고양이처럼인지",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041902.1.25",
          "form": "우리딸 맨날 장이 안좋아서 어떡해",
          "original_form": "우리딸 맨날 장이 안좋아서 어떡해",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041902.1.26",
          "form": "ㅠㅠ 약먹고있는데 잘 안되네요 해결이",
          "original_form": "ㅠㅠ 약먹고있는데 잘 안되네요 해결이",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041902.1.27",
          "form": "뭔데 말 똑바로 해",
          "original_form": "뭔데 말 똑바로 해",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041902.1.28",
          "form": "두부가??두부살안찌잔아",
          "original_form": "두부가??두부살안찌잔아",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041902.1.29",
          "form": "270이면 밥 한공기도 안나오는거 아니냐",
          "original_form": "270이면 밥 한공기도 안나오는거 아니냐",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041902.1.30",
          "form": "한끼에 두부 한모씩 먹는 애가 돼지인거 아냐",
          "original_form": "한끼에 두부 한모씩 먹는 애가 돼지인거 아냐",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041902.1.31",
          "form": "보통 두부만 먹지 않으니까 어쩔수없지",
          "original_form": "보통 두부만 먹지 않으니까 어쩔수없지",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041902.1.32",
          "form": "그렇게 불평불만 가지고 살면 스트레스 받아서 살찌겠다",
          "original_form": "그렇게 불평불만 가지고 살면 스트레스 받아서 살찌겠다",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041902.1.33",
          "form": "너 스프레스 받으면 먹잖아",
          "original_form": "너 스프레스 받으면 먹잖아",
