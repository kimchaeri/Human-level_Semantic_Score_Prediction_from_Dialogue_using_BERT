      "utterance": [
        {
          "id": "MDRW2100035065.1.1",
          "form": "너는 학교 가는 거 좋아하니",
          "original_form": "너는 학교 가는 거 좋아하니",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.2",
          "form": "엄청 좋아하지",
          "original_form": "엄청 좋아하지",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.3",
          "form": "나는 별로 안 좋아하는데",
          "original_form": "나는 별로 안 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.4",
          "form": "좋아하는 것들이 늘어나면 학교도 좋아져",
          "original_form": "좋아하는 것들이 늘어나면 학교도 좋아져",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.5",
          "form": "어떤 것을 좋아하는데",
          "original_form": "어떤 것을 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.6",
          "form": "우리 학교는 음식이 맛있어",
          "original_form": "우리 학교는 음식이 맛있어",
          "speaker_id": "1",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.7",
          "form": "학식 말하는 거지",
          "original_form": "학식 말하는 거지",
          "speaker_id": "2",
          "time": "20210829 01:34"
        },
        {
          "id": "MDRW2100035065.1.8",
          "form": "맞아 학식 이 값도 싸고 맛있어",
          "original_form": "맞아 학식 이 값도 싸고 맛있어",
          "speaker_id": "1",
          "time": "20210829 01:35"
        },
        {
          "id": "MDRW2100035065.1.9",
          "form": "양도 충분하니",
          "original_form": "양도 충분하니",
          "speaker_id": "2",
          "time": "20210829 01:35"
        },
        {
          "id": "MDRW2100035065.1.10",
          "form": "그럼 정말 좋다",
          "original_form": "그럼 정말 좋다",
          "speaker_id": "1",
          "time": "20210829 01:35"
        },
        {
          "id": "MDRW2100035065.1.11",
          "form": "나는 학교에 밥을 먹으러 가",
          "original_form": "나는 학교에 밥을 먹으러 가",
          "speaker_id": "2",
          "time": "20210829 01:35"
        },
        {
          "id": "MDRW2100035065.1.12",
          "form": "설마 아침부터 학교에서 먹니",
          "original_form": "설마 아침부터 학교에서 먹니",
          "speaker_id": "1",
          "time": "20210829 01:35"
        },
        {
          "id": "MDRW2100035065.1.13",
          "form": "당연하지 아침은 2000원밖에 안해",
          "original_form": "당연하지 아침은 2000원밖에 안해",
          "speaker_id": "2",
          "time": "20210829 01:35"
        },
        {
          "id": "MDRW2100035065.1.14",
          "form": "진짜 저렴하구나",
          "original_form": "진짜 저렴하구나",
