        {
          "id": "MDRW2100045675.1.2",
          "form": "나 앞머리 잘랐어",
          "original_form": "나 앞머리 잘랐어",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045675.1.3",
          "form": "미용실갔어?",
          "original_form": "미용실갔어?",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045675.1.4",
          "form": "아니 셀프로..",
          "original_form": "아니 셀프로..",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045675.1.5",
          "form": "헐 결과는 어때",
          "original_form": "헐 결과는 어때",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045675.1.6",
          "form": "ㅋㅋㅋ역시 미용실 갈걸 ㅜ",
          "original_form": "ㅋㅋㅋ역시 미용실 갈걸 ㅜ",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045675.1.7",
          "form": "ㅜㅜㅜ 괜찮아 금방 자랄거야",
          "original_form": "ㅜㅜㅜ 괜찮아 금방 자랄거야",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045675.1.8",
          "form": "몇주간 어쩌지",
          "original_form": "몇주간 어쩌지",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045675.1.9",
          "form": "너무 짧게 잘랐어?",
          "original_form": "너무 짧게 잘랐어?",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045675.1.10",
          "form": "응 게다가 삐뚤어져",
          "original_form": "응 게다가 삐뚤어져",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045675.1.11",
          "form": "ㅜㅜ 그냥 머리 넘기고다녀",
          "original_form": "ㅜㅜ 그냥 머리 넘기고다녀",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045675.1.12",
          "form": "응 안그래도 그러려고",
          "original_form": "응 안그래도 그러려고",
