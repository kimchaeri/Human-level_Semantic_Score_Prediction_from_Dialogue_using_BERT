      "utterance": [
        {
          "id": "MDRW2100033027.1.1",
          "form": "주말에 본가와 처가를 번가라 다녀오는 것입니까?",
          "original_form": "주말에 본가와 처가를 번가라 다녀오는 것입니까?",
          "speaker_id": "1",
          "time": "20210906 14:07"
        },
        {
          "id": "MDRW2100033027.1.2",
          "form": "본가는 안가구요",
          "original_form": "본가는 안가구요",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033027.1.3",
          "form": "처가만 갑니다",
          "original_form": "처가만 갑니다",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033027.1.4",
          "form": "처가는 주말에 갈 수 있는 거리에 있나봅니다",
          "original_form": "처가는 주말에 갈 수 있는 거리에 있나봅니다",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033027.1.5",
          "form": "어린아이가 있으니 보육 도움 받을 겸 가는 거겠군요",
          "original_form": "어린아이가 있으니 보육 도움 받을 겸 가는 거겠군요",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100033027.1.6",
          "form": "네 여러가지 이유가있지만 처가에서 잘 챙겨주셔서 더 자주 가게됩니다",
          "original_form": "네 여러가지 이유가있지만 처가에서 잘 챙겨주셔서 더 자주 가게됩니다",
          "speaker_id": "2",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100033027.1.7",
          "form": "처가 가는 것이 부담스럽지 않은 듯 해 보기 좋습니다",
          "original_form": "처가 가는 것이 부담스럽지 않은 듯 해 보기 좋습니다",
          "speaker_id": "1",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100033027.1.8",
          "form": "저희 집에도 딸 사위가 자주 옵니다",
          "original_form": "저희 집에도 딸 사위가 자주 옵니다",
          "speaker_id": "1",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100033027.1.9",
          "form": "요새는 처가에 더 자주가는 사회 분위기인듯 싶어요",
          "original_form": "요새는 처가에 더 자주가는 사회 분위기인듯 싶어요",
          "speaker_id": "1",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100033027.1.10",
          "form": "그런것 같습니다",
          "original_form": "그런것 같습니다",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033027.1.11",
          "form": "장모님과 등산도 가고",
          "original_form": "장모님과 등산도 가고",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033027.1.12",
          "form": "등산도 가는군요",
          "original_form": "등산도 가는군요",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100033027.1.13",
          "form": "처음 장가갈때 사위 안하고 작은아들 하기로해서",
          "original_form": "처음 장가갈때 사위 안하고 작은아들 하기로해서",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033027.1.14",
          "form": "작은 아들처럼 어머니라고 부르고 지내고 있습니다",
          "original_form": "작은 아들처럼 어머니라고 부르고 지내고 있습니다",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033027.1.15",
          "form": "둘이서 등산도 가구요 ㅎㅎ",
          "original_form": "둘이서 등산도 가구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033027.1.16",
          "form": "잘맞다는 표현이 옳은표현인지 모르겠으마",
          "original_form": "잘맞다는 표현이 옳은표현인지 모르겠으마",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033027.1.17",
          "form": "사위가 아니라 아들처럼 지내는군요",
          "original_form": "사위가 아니라 아들처럼 지내는군요",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033027.1.18",
          "form": "성향이 비슷하여",
          "original_form": "성향이 비슷하여",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033027.1.19",
          "form": "둘이서 곧 잘 다닙니다",
          "original_form": "둘이서 곧 잘 다닙니다",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033027.1.20",
          "form": "보기 좋습니다",
          "original_form": "보기 좋습니다",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033027.1.21",
          "form": "사위들은 어떤가요?",
          "original_form": "사위들은 어떤가요?",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033027.1.22",
          "form": "자상하지요",
          "original_form": "자상하지요",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033027.1.23",
          "form": "요즘 시대는 사위들이 예전 같지 않게 사근사근 합니다",
          "original_form": "요즘 시대는 사위들이 예전 같지 않게 사근사근 합니다",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033027.1.24",
          "form": "다 그런것 같습니다",
          "original_form": "다 그런것 같습니다",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033027.1.25",
          "form": "시대도 바뀌고",
          "original_form": "시대도 바뀌고",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033027.1.26",
          "form": "가정내에서 남자들의 역할도 사뭇 달라졌습니다",
          "original_form": "가정내에서 남자들의 역할도 사뭇 달라졌습니다",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033027.1.27",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210906 16:55"
        }
      ]
    }
