      "utterance": [
        {
          "id": "MDRW2100034011.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034011.1.2",
          "form": "네.. 피부 좋으신가요.\r\n제 피부는 갈수록 건강을 잃고 있네요.",
          "original_form": "네.. 피부 좋으신가요.\r\n제 피부는 갈수록 건강을 잃고 있네요.",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100034011.1.3",
          "form": "저도요ㅎㅎ",
          "original_form": "저도요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100034011.1.4",
          "form": "그냥 신경끄고산답니다ㅎㅎㅎ",
          "original_form": "그냥 신경끄고산답니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100034011.1.5",
          "form": "해서 피부 대신 패션에 신경을 써볼까 하는데요.\r\n애용하는 패션몰이 있으신가요?",
          "original_form": "해서 피부 대신 패션에 신경을 써볼까 하는데요.\r\n애용하는 패션몰이 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034011.1.6",
          "form": "저는 가볍게입는편이라",
          "original_form": "저는 가볍게입는편이라",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100034011.1.7",
          "form": "자라 구경자주가요ㅋㅋㅋ",
          "original_form": "자라 구경자주가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100034011.1.8",
          "form": "아. 자라\r\n자라 옷 소화하시는 거면\r\n패셔니스타이신거죠.",
          "original_form": "아. 자라\r\n자라 옷 소화하시는 거면\r\n패셔니스타이신거죠.",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100034011.1.9",
          "form": "ㅋㅋㅋㅋㅋ 앗",
          "original_form": "ㅋㅋㅋㅋㅋ 앗",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100034011.1.10",
          "form": "그건전혀아니예요ㅠㅠ",
          "original_form": "그건전혀아니예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100034011.1.11",
          "form": "자라는 난해한 패션의 옷이 있더라구요.\r\n일단 키가 커야겠단 생각도 들었구요.",
          "original_form": "자라는 난해한 패션의 옷이 있더라구요.\r\n일단 키가 커야겠단 생각도 들었구요.",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100034011.1.12",
          "form": "그렇긴해요ㅋㅋㅋㅋ",
          "original_form": "그렇긴해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100034011.1.13",
          "form": "전 키가작답니다ㅠ",
          "original_form": "전 키가작답니다ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100034011.1.14",
          "form": "옷을 어울리는 것으로 잘고르시나요.\r\n키는 신발로 커버하면 되지요.ㅎ",
          "original_form": "옷을 어울리는 것으로 잘고르시나요.\r\n키는 신발로 커버하면 되지요.ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100034011.1.15",
          "form": "ㅋㅋㅋ네네",
          "original_form": "ㅋㅋㅋ네네",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100034011.1.16",
          "form": "패션에 관심이 많으신가봐요?!",
          "original_form": "패션에 관심이 많으신가봐요?!",
          "speaker_id": "2",
          "time": "20210826 13:32"
        },
        {
          "id": "MDRW2100034011.1.17",
          "form": "이제 관심 좀 가지려구요.",
          "original_form": "이제 관심 좀 가지려구요.",
          "speaker_id": "1",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100034011.1.18",
          "form": "오 좋네요ㅋㅋㅋㅋ",
          "original_form": "오 좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100034011.1.19",
          "form": "저는 영 몰라서요",
          "original_form": "저는 영 몰라서요",
          "speaker_id": "2",
          "time": "20210826 13:40"
        }
      ]
