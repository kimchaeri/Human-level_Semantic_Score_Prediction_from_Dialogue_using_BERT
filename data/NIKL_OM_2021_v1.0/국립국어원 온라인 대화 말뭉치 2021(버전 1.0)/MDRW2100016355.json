      "utterance": [
        {
          "id": "MDRW2100016355.1.1",
          "form": "먹방은 잘 안 봐?",
          "original_form": "먹방은 잘 안 봐?",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016355.1.2",
          "form": "예~전에 문복희? 입짧은햇님은 좀 봤었는데",
          "original_form": "예~전에 문복희? 입짧은햇님은 좀 봤었는데",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016355.1.3",
          "form": "그외엔 잘안봐!",
          "original_form": "그외엔 잘안봐!",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016355.1.4",
          "form": "난 입짧은 햇님 요즘도 구독중인데",
          "original_form": "난 입짧은 햇님 요즘도 구독중인데",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016355.1.5",
          "form": "인기 엄청 많더라구",
          "original_form": "인기 엄청 많더라구",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016355.1.6",
          "form": "그치 너무 재밌게 잘 먹으시더라공",
          "original_form": "그치 너무 재밌게 잘 먹으시더라공",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016355.1.7",
          "form": "클립으로 돌아다니는거도 웃겨ㅋㅋㅋ",
          "original_form": "클립으로 돌아다니는거도 웃겨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016355.1.8",
          "form": "아줌마 잘먹지..? 이거",
          "original_form": "아줌마 잘먹지..? 이거",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016355.1.9",
          "form": "아 ㅋㅋㅋ 맞아 ㅋㅋㅋ유명하지",
          "original_form": "아 ㅋㅋㅋ 맞아 ㅋㅋㅋ유명하지",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016355.1.10",
          "form": "좋아하는 연예인 있어?",
          "original_form": "좋아하는 연예인 있어?",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016355.1.11",
          "form": "요즘은 좀 시들해 딱히없어",
          "original_form": "요즘은 좀 시들해 딱히없어",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016355.1.12",
          "form": "티비를 잘안봐서 그런듯ㅜㅜ",
          "original_form": "티비를 잘안봐서 그런듯ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016355.1.13",
          "form": "요새 티비 볼게없더라고 ㅠㅠ",
          "original_form": "요새 티비 볼게없더라고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016355.1.14",
          "form": "그치 딱히 볼게없어ㅜㅜ",
          "original_form": "그치 딱히 볼게없어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016355.1.15",
          "form": "유툽이 더 재밌어ㅋㅋㅋ",
          "original_form": "유툽이 더 재밌어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016355.1.16",
          "form": "맞아 아니면 넷플릭스나 ㅋㅋㅋ",
          "original_form": "맞아 아니면 넷플릭스나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016355.1.17",
          "form": "맞아 그냥ott보게되는듯ㅋㅋㅋ",
          "original_form": "맞아 그냥ott보게되는듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016355.1.18",
          "form": "아!11월에 디즈니플러스 나온대",
          "original_form": "아!11월에 디즈니플러스 나온대",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016355.1.19",
          "form": "디즈니플러스는 뭐야?? 디즈니영화?",
          "original_form": "디즈니플러스는 뭐야?? 디즈니영화?",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016355.1.20",
          "form": "넷플같은건데 디즈니에서 만드는거야!",
          "original_form": "넷플같은건데 디즈니에서 만드는거야!",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016355.1.21",
          "form": "디즈니가 마블,픽사 인수해서 가지고있는 영화가 많은데",
          "original_form": "디즈니가 마블,픽사 인수해서 가지고있는 영화가 많은데",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016355.1.22",
          "form": "거기서 다 풀거같아",
          "original_form": "거기서 다 풀거같아",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016355.1.23",
          "form": "와 인기많겠다 그러면",
          "original_form": "와 인기많겠다 그러면",
          "speaker_id": "1",
          "time": "20210826 12:18"
        }
      ]
    }
