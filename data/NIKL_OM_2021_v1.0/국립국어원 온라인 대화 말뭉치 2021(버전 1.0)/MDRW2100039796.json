      "utterance": [
        {
          "id": "MDRW2100039796.1.1",
          "form": "여행좋아하세요,",
          "original_form": "여행좋아하세요,",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039796.1.2",
          "form": "네 물론이죠",
          "original_form": "네 물론이죠",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039796.1.3",
          "form": "자주다니시나요~~?",
          "original_form": "자주다니시나요~~?",
          "speaker_id": "1",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039796.1.4",
          "form": "지금 제주살이하면서 여행중이예요",
          "original_form": "지금 제주살이하면서 여행중이예요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039796.1.5",
          "form": "제주도에 맛집있나요?ㅜㅜ",
          "original_form": "제주도에 맛집있나요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039796.1.6",
          "form": "전  다실패했었어ㅜㅜ",
          "original_form": "전  다실패했었어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039796.1.7",
          "form": "관광객 대상은 아무래도 그런것같구",
          "original_form": "관광객 대상은 아무래도 그런것같구",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039796.1.8",
          "form": "저는 동문시장에서 잘 사먹어요",
          "original_form": "저는 동문시장에서 잘 사먹어요",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039796.1.9",
          "form": "동문시장에서는 뭐가 제일 맛있어요?",
          "original_form": "동문시장에서는 뭐가 제일 맛있어요?",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039796.1.10",
          "form": "딱새우는 전 별로였어요ㅠㅠ",
          "original_form": "딱새우는 전 별로였어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100039796.1.11",
          "form": "청년몰에서 팟타이나 생선구이 솥밥 같은거",
          "original_form": "청년몰에서 팟타이나 생선구이 솥밥 같은거",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039796.1.12",
          "form": "혹은 간식 주전부리 사먹어요",
          "original_form": "혹은 간식 주전부리 사먹어요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039796.1.13",
          "form": "아! 팟타이 맛있겠다",
          "original_form": "아! 팟타이 맛있겠다",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100039796.1.14",
          "form": "제주 특산음식은 자주 안먹어요 ㅎ",
          "original_form": "제주 특산음식은 자주 안먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039796.1.15",
          "form": "근데 술안주로도 최고겠는데요ㅋㅋㅋㅋ",
          "original_form": "근데 술안주로도 최고겠는데요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039796.1.16",
          "form": "날씨가 변덕스러워서 한라산 가기전에",
          "original_form": "날씨가 변덕스러워서 한라산 가기전에",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039796.1.17",
          "form": "소주에 중독될 판예요",
          "original_form": "소주에 중독될 판예요",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039796.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039796.1.19",
          "form": "무릎보호대꼭하세요ㅋㅋ",
          "original_form": "무릎보호대꼭하세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039796.1.20",
          "form": "전 한라산올라가는건 괜찮았는데",
          "original_form": "전 한라산올라가는건 괜찮았는데",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039796.1.21",
          "form": "내려올때 무릎이 아프더라구요ㅜㅜ",
          "original_form": "내려올때 무릎이 아프더라구요ㅜㅜ",
