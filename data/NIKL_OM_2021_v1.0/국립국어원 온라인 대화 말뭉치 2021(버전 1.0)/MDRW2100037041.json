      "utterance": [
        {
          "id": "MDRW2100037041.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100037041.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037041.1.3",
          "form": "다이어트해보신적있으신가요",
          "original_form": "다이어트해보신적있으신가요",
          "speaker_id": "1",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100037041.1.4",
          "form": "아니요 다이어트보다는 평소에 꾸준히 관리하려고 하는 편이에요",
          "original_form": "아니요 다이어트보다는 평소에 꾸준히 관리하려고 하는 편이에요",
          "speaker_id": "2",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100037041.1.5",
          "form": "저도 다이어트 식단이나이런거 정말못하겠더라구여..",
          "original_form": "저도 다이어트 식단이나이런거 정말못하겠더라구여..",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037041.1.6",
          "form": "평소에 어떻게관리하세요",
          "original_form": "평소에 어떻게관리하세요",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037041.1.7",
          "form": "몸무게가 조금 늘것 같으면 \n간식을 끊고, 먹는 양을 줄이려고 해요",
          "original_form": "몸무게가 조금 늘것 같으면 \n간식을 끊고, 먹는 양을 줄이려고 해요",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100037041.1.8",
          "form": "과식이나 야식은 항상 안 하려고 하구요..",
          "original_form": "과식이나 야식은 항상 안 하려고 하구요..",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100037041.1.9",
          "form": "오오 전 먹는거 도중에 멈추는게 넘힘들어요ㅜ과식하게되고",
          "original_form": "오오 전 먹는거 도중에 멈추는게 넘힘들어요ㅜ과식하게되고",
          "speaker_id": "1",
          "time": "20210831 18:03"
        },
        {
          "id": "MDRW2100037041.1.10",
          "form": "아 그러시군요ㅠㅜ\n저는 과식하고나면 속이 별로 안 좋더라구요",
          "original_form": "아 그러시군요ㅠㅜ\n저는 과식하고나면 속이 별로 안 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 18:07"
        },
        {
          "id": "MDRW2100037041.1.11",
          "form": "네 저도그래서 과식하면안되는데 식탐이항상이기네요ㅜ",
          "original_form": "네 저도그래서 과식하면안되는데 식탐이항상이기네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100037041.1.12",
          "form": "ㅜㅜ 식욕이 많은 경우 음식을 앞에 두고 멈추는걸 많이 힘들어하시더라구요",
          "original_form": "ㅜㅜ 식욕이 많은 경우 음식을 앞에 두고 멈추는걸 많이 힘들어하시더라구요",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100037041.1.13",
          "form": "네ㅜ 먹고 소화안되고 후회하고 반복되네요",
          "original_form": "네ㅜ 먹고 소화안되고 후회하고 반복되네요",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100037041.1.14",
          "form": "맞아요 과식하고나면 꼭 후회하게되더라구요\n그래서 소식을 하려고 노력하고 있어요",
          "original_form": "맞아요 과식하고나면 꼭 후회하게되더라구요\n그래서 소식을 하려고 노력하고 있어요",
          "speaker_id": "2",
          "time": "20210831 19:27"
        },
        {
          "id": "MDRW2100037041.1.15",
          "form": "ㅜㅜ소식진짜 넘어려워요",
          "original_form": "ㅜㅜ소식진짜 넘어려워요",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100037041.1.16",
          "form": "배고파질땐 어떻게하시나요",
          "original_form": "배고파질땐 어떻게하시나요",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100037041.1.17",
          "form": "간단하게 먹을게 없나 찾아다녀요ㅋㅋㅋ\n군것질거리 있으면 조금씩 먹기도 하구요",
          "original_form": "간단하게 먹을게 없나 찾아다녀요ㅋㅋㅋ\n군것질거리 있으면 조금씩 먹기도 하구요",
          "speaker_id": "2",
          "time": "20210831 21:08"
        }
      ]
