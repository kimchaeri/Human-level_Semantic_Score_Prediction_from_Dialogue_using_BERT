      "utterance": [
        {
          "id": "MDRW2100011013.1.1",
          "form": "티비 많이보시나요?",
          "original_form": "티비 많이보시나요?",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011013.1.2",
          "form": "네 좋아해요 !",
          "original_form": "네 좋아해요 !",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011013.1.3",
          "form": "온갖 플랫폼을 다 구독중이에요 ㅋㅋ",
          "original_form": "온갖 플랫폼을 다 구독중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011013.1.4",
          "form": "와.. 한개보기도 힘든데.. \r\n넷플릭스도 많이 보세요?",
          "original_form": "와.. 한개보기도 힘든데.. \r\n넷플릭스도 많이 보세요?",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011013.1.5",
          "form": "음 많이 본다..는건 모르겠지만",
          "original_form": "음 많이 본다..는건 모르겠지만",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011013.1.6",
          "form": "넷플도 언제나 구독중이에요",
          "original_form": "넷플도 언제나 구독중이에요",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011013.1.7",
          "form": "4인팟으로 ㅋㅋㅋㅋ",
          "original_form": "4인팟으로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011013.1.8",
          "form": "넷플 좋아하세요?",
          "original_form": "넷플 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100011013.1.9",
          "form": "와.. 전 한방에 봐야해서.. 주로 넷플릭스..",
          "original_form": "와.. 전 한방에 봐야해서.. 주로 넷플릭스..",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011013.1.10",
          "form": "컴터로 보시는거에여?",
          "original_form": "컴터로 보시는거에여?",
          "speaker_id": "1",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100011013.1.11",
          "form": "아뇨 주로 패드로 봐요!",
          "original_form": "아뇨 주로 패드로 봐요!",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011013.1.12",
          "form": "어떤 시리즈 재밌게 보셨어요??",
          "original_form": "어떤 시리즈 재밌게 보셨어요??",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011013.1.13",
          "form": "최근에 오토만.. 제국..",
          "original_form": "최근에 오토만.. 제국..",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100011013.1.14",
          "form": "오스만 제국 어쩌구 봤어요..",
          "original_form": "오스만 제국 어쩌구 봤어요..",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100011013.1.15",
          "form": "1픽은 하오카..",
          "original_form": "1픽은 하오카..",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100011013.1.16",
          "form": "오 하오카 재밌나요?",
          "original_form": "오 하오카 재밌나요?",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011013.1.17",
          "form": "전 기묘한이야기...스띵이 이상하게 좋더라구요",
          "original_form": "전 기묘한이야기...스띵이 이상하게 좋더라구요",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011013.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011013.1.19",
          "form": "최애는 종이의집이에요",
          "original_form": "최애는 종이의집이에요",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011013.1.20",
          "form": "아.. 다 봣어요..",
          "original_form": "아.. 다 봣어요..",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011013.1.21",
          "form": "스띵 쫌 음침하면서 발랄하고.. 최근 젤 성공한거 같네요",
          "original_form": "스띵 쫌 음침하면서 발랄하고.. 최근 젤 성공한거 같네요",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011013.1.22",
          "form": "넷플 고인물로 인정해드립니다...",
          "original_form": "넷플 고인물로 인정해드립니다...",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011013.1.23",
          "form": "맞아요 스띵은 초반에 좀만 참으면 진짜 재밌던데",
          "original_form": "맞아요 스띵은 초반에 좀만 참으면 진짜 재밌던데",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011013.1.24",
          "form": "호불호가 좀 갈리고...",
          "original_form": "호불호가 좀 갈리고...",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011013.1.25",
          "form": "종이의집은 너무 인생드라마라 다음시즌 존버중이에요 곧 나온답니다",
          "original_form": "종이의집은 너무 인생드라마라 다음시즌 존버중이에요 곧 나온답니다",
          "speaker_id": "2",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011013.1.26",
          "form": "저도 왜 유명한가 의아했는데..\r\n뒷부분이..",
          "original_form": "저도 왜 유명한가 의아했는데..\r\n뒷부분이..",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011013.1.27",
          "form": "종이의집 한국판도 나오자나여",
          "original_form": "종이의집 한국판도 나오자나여",
          "speaker_id": "1",
          "time": "20210824 00:04"
        },
        {
          "id": "MDRW2100011013.1.28",
          "form": "그것또한 존버중입니다",
          "original_form": "그것또한 존버중입니다",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011013.1.29",
          "form": "나름 캐스팅이 잘 된것같아서",
          "original_form": "나름 캐스팅이 잘 된것같아서",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011013.1.30",
          "form": "테러..를 어떻게 풀어갈지모르겠지만 기대중이에요 ㅋㅋㅋ",
          "original_form": "테러..를 어떻게 풀어갈지모르겠지만 기대중이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011013.1.31",
          "form": "지정생존자 보셨어요?",
          "original_form": "지정생존자 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011013.1.32",
          "form": "오.. 봣죠.. ㅠㅠ",
          "original_form": "오.. 봣죠.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011013.1.33",
          "form": "저 의외로 훼인인듯.. 다봤네.. ㅋㅋㅋ",
          "original_form": "저 의외로 훼인인듯.. 다봤네.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011013.1.34",
          "form": "지정생존자 컨셉은 좋은데 용두사미.. 시즌 안나오죠?",
          "original_form": "지정생존자 컨셉은 좋은데 용두사미.. 시즌 안나오죠?",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011013.1.35",
          "form": "저는 아직 안 봤는데",
          "original_form": "저는 아직 안 봤는데",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011013.1.36",
          "form": "볼까했는데 뭔가 망설여지네요 ㅋㅋㅋㅋ",
          "original_form": "볼까했는데 뭔가 망설여지네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011013.1.37",
          "form": "진짜 넷플 고인물이세요...!!!!",
          "original_form": "진짜 넷플 고인물이세요...!!!!",
