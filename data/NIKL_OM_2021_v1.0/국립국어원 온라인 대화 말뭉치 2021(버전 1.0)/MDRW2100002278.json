      "utterance": [
        {
          "id": "MDRW2100002278.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100002278.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 08:19"
        },
        {
          "id": "MDRW2100002278.1.3",
          "form": "동호회활동하시는거 있으신가요?",
          "original_form": "동호회활동하시는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 08:21"
        },
        {
          "id": "MDRW2100002278.1.4",
          "form": "아니요 지금은 없어요~",
          "original_form": "아니요 지금은 없어요~",
          "speaker_id": "1",
          "time": "20210820 08:22"
        },
        {
          "id": "MDRW2100002278.1.5",
          "form": "님은 있으신가요?",
          "original_form": "님은 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 08:22"
        },
        {
          "id": "MDRW2100002278.1.6",
          "form": "저도 없어요.",
          "original_form": "저도 없어요.",
          "speaker_id": "2",
          "time": "20210820 08:23"
        },
        {
          "id": "MDRW2100002278.1.7",
          "form": "봉사활동은 해보고 싶어요",
          "original_form": "봉사활동은 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210820 08:23"
        },
        {
          "id": "MDRW2100002278.1.8",
          "form": "어떤 봉사활동에 관심있으세요?",
          "original_form": "어떤 봉사활동에 관심있으세요?",
          "speaker_id": "1",
          "time": "20210820 08:23"
        },
        {
          "id": "MDRW2100002278.1.9",
          "form": "아이들있는곳요.",
          "original_form": "아이들있는곳요.",
          "speaker_id": "2",
          "time": "20210820 08:24"
        },
        {
          "id": "MDRW2100002278.1.10",
          "form": "아~ 아이들 좋아하시나봐요",
          "original_form": "아~ 아이들 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210820 08:24"
        },
        {
          "id": "MDRW2100002278.1.11",
          "form": "관심있는 동호회가 있으세요?",
          "original_form": "관심있는 동호회가 있으세요?",
          "speaker_id": "2",
          "time": "20210820 08:26"
        },
        {
          "id": "MDRW2100002278.1.12",
          "form": "저는 시간이 되면 유기견 봉사활동 하고싶어요",
          "original_form": "저는 시간이 되면 유기견 봉사활동 하고싶어요",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002278.1.13",
          "form": "대학생때 몇번 나갔었는데",
          "original_form": "대학생때 몇번 나갔었는데",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002278.1.14",
          "form": "힘들긴했어도 보람차고 좋더라구요",
          "original_form": "힘들긴했어도 보람차고 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100002278.1.15",
          "form": "유기견 봉사활동도 기회가 되면 꼭 해보고 싶어요",
          "original_form": "유기견 봉사활동도 기회가 되면 꼭 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002278.1.16",
          "form": "그쵸 시간 여유가 없는게 안타까워요 ㅠ",
          "original_form": "그쵸 시간 여유가 없는게 안타까워요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 08:31"
        },
        {
          "id": "MDRW2100002278.1.17",
          "form": "지인은 세차동호회 하는데 좋다더라구요",
          "original_form": "지인은 세차동호회 하는데 좋다더라구요",
          "speaker_id": "1",
          "time": "20210820 08:32"
        },
        {
          "id": "MDRW2100002278.1.18",
          "form": "일끝나고 밤에 잠깐 모여서 세차하고 차한잔 마시고 오니까 생활에 크게 지장 안가고 오래 유지할 수 있어서 좋대요",
          "original_form": "일끝나고 밤에 잠깐 모여서 세차하고 차한잔 마시고 오니까 생활에 크게 지장 안가고 오래 유지할 수 있어서 좋대요",
          "speaker_id": "1",
          "time": "20210820 08:32"
        },
        {
          "id": "MDRW2100002278.1.19",
          "form": "셀프세차 너무 힘들더라구요ㅎㅎ",
          "original_form": "셀프세차 너무 힘들더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 08:32"
        },
        {
          "id": "MDRW2100002278.1.20",
          "form": "자꾸하면 노하우가 생기겠죠?ㅎㅎ",
          "original_form": "자꾸하면 노하우가 생기겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 08:33"
        },
        {
          "id": "MDRW2100002278.1.21",
          "form": "그럴거에요~ 세차하고 나면 기분 넘 좋을것같어요",
          "original_form": "그럴거에요~ 세차하고 나면 기분 넘 좋을것같어요",
          "speaker_id": "1",
          "time": "20210820 08:36"
        },
        {
          "id": "MDRW2100002278.1.22",
          "form": "회사동료가 클라이밍 동호회하는것도 봤었는데",
          "original_form": "회사동료가 클라이밍 동호회하는것도 봤었는데",
          "speaker_id": "1",
          "time": "20210820 08:36"
        },
        {
          "id": "MDRW2100002278.1.23",
          "form": "그게 정말 운동이 많이된대요",
          "original_form": "그게 정말 운동이 많이된대요",
          "speaker_id": "1",
          "time": "20210820 08:36"
        },
        {
          "id": "MDRW2100002278.1.24",
          "form": "준비물도 많이 없고 일끝나고 쉭 갔다올수있어서 좋다더라구요",
          "original_form": "준비물도 많이 없고 일끝나고 쉭 갔다올수있어서 좋다더라구요",
          "speaker_id": "1",
          "time": "20210820 08:36"
        },
        {
          "id": "MDRW2100002278.1.25",
          "form": "준비물많이 없는건 좋네요ㅎ",
          "original_form": "준비물많이 없는건 좋네요ㅎ",
          "speaker_id": "2",
          "time": "20210820 08:37"
        }
      ]
    }
