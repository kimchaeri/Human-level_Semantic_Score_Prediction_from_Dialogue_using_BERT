      "utterance": [
        {
          "id": "MDRW2100042479.1.1",
          "form": "아침은 챙겨먹는 편인가요?",
          "original_form": "아침은 챙겨먹는 편인가요?",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042479.1.2",
          "form": "아니요 맨날 허둥지둥",
          "original_form": "아니요 맨날 허둥지둥",
          "speaker_id": "1",
          "time": "20210907 09:54"
        },
        {
          "id": "MDRW2100042479.1.3",
          "form": "그렇군요.. 그럼 점심떄까지 너무 배고프지 않나요?",
          "original_form": "그렇군요.. 그럼 점심떄까지 너무 배고프지 않나요?",
          "speaker_id": "2",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042479.1.4",
          "form": "넹 그래서 물이나 차 엄청 마셔요",
          "original_form": "넹 그래서 물이나 차 엄청 마셔요",
          "speaker_id": "1",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042479.1.5",
          "form": "아 물배채우는 군요",
          "original_form": "아 물배채우는 군요",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042479.1.6",
          "form": "저희 가족은 다들 일찍 일어나요",
          "original_form": "저희 가족은 다들 일찍 일어나요",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042479.1.7",
          "form": "아침 안먹으면 점심까지 못버텨서 일단 먹입니다",
          "original_form": "아침 안먹으면 점심까지 못버텨서 일단 먹입니다",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042479.1.8",
          "form": "저도 어릴때부터 아침머는 버릇이있어서 안먹으면 위액때문에 배아팠는데",
          "original_form": "저도 어릴때부터 아침머는 버릇이있어서 안먹으면 위액때문에 배아팠는데",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100042479.1.9",
          "form": "요새 안먹다버릇하니까 또 거기에 적응이되네요",
          "original_form": "요새 안먹다버릇하니까 또 거기에 적응이되네요",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100042479.1.10",
          "form": "그렇죠.. 인간은 적응의 동물이라",
          "original_form": "그렇죠.. 인간은 적응의 동물이라",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100042479.1.11",
          "form": "그래도 전 식구들이 아침을 먹었으면 좋겠어서 일단 먹으라고 해요 ㅎㅎ",
          "original_form": "그래도 전 식구들이 아침을 먹었으면 좋겠어서 일단 먹으라고 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100042479.1.12",
          "form": "아침 먹는게 건강에 좋다고 하더라구요",
          "original_form": "아침 먹는게 건강에 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100042479.1.13",
          "form": "전 차려져 있어도 못 먹어서. .",
          "original_form": "전 차려져 있어도 못 먹어서. .",
          "speaker_id": "1",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100042479.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100042479.1.15",
          "form": "그럼 아침 안먹으니 점심은 든든히 먹겠네요",
          "original_form": "그럼 아침 안먹으니 점심은 든든히 먹겠네요",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100042479.1.16",
          "form": "오늘은 뭐 드셨나요?",
          "original_form": "오늘은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 14:14"
        },
        {
          "id": "MDRW2100042479.1.17",
          "form": "점심에 칼국수에 만두먹었어요",
          "original_form": "점심에 칼국수에 만두먹었어요",
          "speaker_id": "1",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100042479.1.18",
          "form": "비오는 날 어울리는 메뉴네요",
          "original_form": "비오는 날 어울리는 메뉴네요",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100042479.1.19",
          "form": "그래서 생각나서어",
          "original_form": "그래서 생각나서어",
          "speaker_id": "1",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100042479.1.20",
          "form": "저는 집에서 감자채볶음과 가지볶음 해서 먹었어요",
          "original_form": "저는 집에서 감자채볶음과 가지볶음 해서 먹었어요",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100042479.1.21",
          "form": "가지요? 가지좋아하세요?",
          "original_form": "가지요? 가지좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100042479.1.22",
          "form": "전 가지싫어해요 ㅜㅠ",
          "original_form": "전 가지싫어해요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100042479.1.23",
          "form": "가지는 호불호가 갈리죠",
          "original_form": "가지는 호불호가 갈리죠",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100042479.1.24",
          "form": "가지 튀김 먹어봤나요?",
          "original_form": "가지 튀김 먹어봤나요?",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100042479.1.25",
          "form": "텐동에 있던 가지튀김먹어봤는데 그건 그나마 먹을만했어요",
          "original_form": "텐동에 있던 가지튀김먹어봤는데 그건 그나마 먹을만했어요",
          "speaker_id": "1",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100042479.1.26",
          "form": "가지꼬치는 좋아해요",
          "original_form": "가지꼬치는 좋아해요",
          "speaker_id": "1",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100042479.1.27",
          "form": "가지나물 못먹는 사람도 가지볶음이나 튀김은 먹더라구요",
          "original_form": "가지나물 못먹는 사람도 가지볶음이나 튀김은 먹더라구요",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100042479.1.28",
          "form": "아무래도 식감의 차이가 있어서 그런가봐요",
          "original_form": "아무래도 식감의 차이가 있어서 그런가봐요",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100042479.1.29",
          "form": "흐물거림이 으윽",
          "original_form": "흐물거림이 으윽",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100042479.1.30",
          "form": "그러니까요. .전 가지나물도 좋은데 딸이 싫어해요",
          "original_form": "그러니까요. .전 가지나물도 좋은데 딸이 싫어해요",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100042479.1.31",
          "form": "그래서 가지 볶음으로 하죠",
          "original_form": "그래서 가지 볶음으로 하죠",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100042479.1.32",
          "form": "볶아도 흐물",
          "original_form": "볶아도 흐물",
          "speaker_id": "1",
          "time": "20210907 16:34"
        },
        {
          "id": "MDRW2100042479.1.33",
          "form": "볶음도 안되는군요  일단 튀겨야 하는군요 ㅎㅎ",
          "original_form": "볶음도 안되는군요  일단 튀겨야 하는군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:37"
        }
      ]
