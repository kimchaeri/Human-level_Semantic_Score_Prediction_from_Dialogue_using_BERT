      "utterance": [
        {
          "id": "MDRW2100037673.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100037673.1.2",
          "form": "홀맨 다 맘에 들지만 젤 유용하게 쓰는건 키링이요ㅎㅎ",
          "original_form": "홀맨 다 맘에 들지만 젤 유용하게 쓰는건 키링이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100037673.1.3",
          "form": "그렇군요\r\n어떤 색 받으셨어요?",
          "original_form": "그렇군요\r\n어떤 색 받으셨어요?",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100037673.1.4",
          "form": "전 빨간색이요",
          "original_form": "전 빨간색이요",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100037673.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100037673.1.6",
          "form": "전 검은색이요",
          "original_form": "전 검은색이요",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100037673.1.7",
          "form": "집에 다 검정만 왔어요..",
          "original_form": "집에 다 검정만 왔어요..",
          "speaker_id": "2",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100037673.1.8",
          "form": "아 헐..",
          "original_form": "아 헐..",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100037673.1.9",
          "form": "몇개받으심요?",
          "original_form": "몇개받으심요?",
          "speaker_id": "1",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100037673.1.10",
          "form": "저희집은 3개요",
          "original_form": "저희집은 3개요",
          "speaker_id": "2",
          "time": "20210901 15:27"
        },
        {
          "id": "MDRW2100037673.1.11",
          "form": "저희집은 2개요ㅎ",
          "original_form": "저희집은 2개요ㅎ",
          "speaker_id": "1",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100037673.1.12",
          "form": "언니꺼도 했죠",
          "original_form": "언니꺼도 했죠",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037673.1.13",
          "form": "그럼 색 당 1개씩 왔나요?",
          "original_form": "그럼 색 당 1개씩 왔나요?",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037673.1.14",
          "form": "네 근데 제꺼 거슬려요ㅋㅋ",
          "original_form": "네 근데 제꺼 거슬려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037673.1.15",
          "form": "어느부분이요?",
          "original_form": "어느부분이요?",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100037673.1.16",
          "form": "메달려있자나요 근데....",
          "original_form": "메달려있자나요 근데....",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100037673.1.17",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100037673.1.18",
          "form": "인형 윗부분에 달려있는데 그건 아닌가요?",
          "original_form": "인형 윗부분에 달려있는데 그건 아닌가요?",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100037673.1.19",
          "form": "빨간건 발이 묶인거처럼 데롱데롱 매달려 있어요",
          "original_form": "빨간건 발이 묶인거처럼 데롱데롱 매달려 있어요",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037673.1.20",
          "form": "그래서 데롱 키링",
          "original_form": "그래서 데롱 키링",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100037673.1.21",
          "form": "그것도 나름 보면 귀엽겠는데요?",
          "original_form": "그것도 나름 보면 귀엽겠는데요?",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037673.1.22",
          "form": "처음에는 어색할것 같지만",
          "original_form": "처음에는 어색할것 같지만",
          "speaker_id": "2",
          "time": "20210901 15:54"
        }
      ]
