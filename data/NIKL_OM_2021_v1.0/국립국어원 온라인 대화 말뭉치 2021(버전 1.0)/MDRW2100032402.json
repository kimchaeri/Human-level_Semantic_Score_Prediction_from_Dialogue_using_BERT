      "utterance": [
        {
          "id": "MDRW2100032402.1.1",
          "form": "왜 그렇게 생각하세요",
          "original_form": "왜 그렇게 생각하세요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032402.1.2",
          "form": "함께 살면 의지가 되고요",
          "original_form": "함께 살면 의지가 되고요",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032402.1.3",
          "form": "저도 혼자 사니까 쏠쏠하더라구요",
          "original_form": "저도 혼자 사니까 쏠쏠하더라구요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032402.1.4",
          "form": "집안일도 나누어서 할 수 있어요",
          "original_form": "집안일도 나누어서 할 수 있어요",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032402.1.5",
          "form": "아버지는 집안 일을 잘 안 하세요",
          "original_form": "아버지는 집안 일을 잘 안 하세요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032402.1.6",
          "form": "저희집도 마찬가지에요",
          "original_form": "저희집도 마찬가지에요",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032402.1.7",
          "form": "요리를 잘 하시나요",
          "original_form": "요리를 잘 하시나요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032402.1.8",
          "form": "김치볶음밥 정도만 만듭니다",
          "original_form": "김치볶음밥 정도만 만듭니다",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032402.1.9",
          "form": "저랑 표정이 비슷하시네요",
          "original_form": "저랑 표정이 비슷하시네요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032402.1.10",
          "form": "저는 집안일에 욕심이 없어요",
          "original_form": "저는 집안일에 욕심이 없어요",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032402.1.11",
          "form": "어느 정도 깔끔하게만 살면 되는 거 같아요",
          "original_form": "어느 정도 깔끔하게만 살면 되는 거 같아요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032402.1.12",
          "form": "네 안하면 티나는게 집안일이니까요",
          "original_form": "네 안하면 티나는게 집안일이니까요",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032402.1.13",
          "form": "오늘 저는 설거지를 했어요",
          "original_form": "오늘 저는 설거지를 했어요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032402.1.14",
          "form": "설거지를 그때그떄 바로 하시나요",
          "original_form": "설거지를 그때그떄 바로 하시나요",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032402.1.15",
          "form": "쌓이면 더 하기 싫더라고요",
          "original_form": "쌓이면 더 하기 싫더라고요",
          "speaker_id": "2",
