        },
        {
          "id": "MDRW2100038559.1.4",
          "form": "나비 정상",
          "original_form": "나비 정상",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100038559.1.5",
          "form": "아이고야",
          "original_form": "아이고야",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100038559.1.6",
          "form": "띄어쓰기 잘못하면 큰일났겠군요",
          "original_form": "띄어쓰기 잘못하면 큰일났겠군요",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100038559.1.7",
          "form": "악ㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038559.1.8",
          "form": "벌써 눈치를 채신건가요",
          "original_form": "벌써 눈치를 채신건가요",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038559.1.9",
          "form": "ㅎㅎ눈치는 빠릅니다",
          "original_form": "ㅎㅎ눈치는 빠릅니다",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038559.1.10",
          "form": "다음 문제를 내도 의미가 없겠죶?",
          "original_form": "다음 문제를 내도 의미가 없겠죶?",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100038559.1.11",
          "form": "이젠 없을거 같아요",
          "original_form": "이젠 없을거 같아요",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038559.1.12",
          "form": "name1님 의도를 다 알아버렸는걸요ㅋㅋ",
          "original_form": "&name1&님 의도를 다 알아버렸는걸요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038559.1.13",
          "form": "앜ㅋㅋ아쉬워라 이거 진짜 재미있는 문제 였는데",
          "original_form": "앜ㅋㅋ아쉬워라 이거 진짜 재미있는 문제 였는데",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038559.1.14",
          "form": "의도를 다 알아버리니깐",
          "original_form": "의도를 다 알아버리니깐",
          "speaker_id": "1",
          "time": "20210902 16:07"
