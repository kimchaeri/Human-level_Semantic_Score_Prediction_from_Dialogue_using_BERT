          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.14",
          "form": "로긴을 제꺼로 해놓고",
          "original_form": "로긴을 제꺼로 해놓고",
          "speaker_id": "2",
          "time": "20210903 11:28"
        },
        {
          "id": "MDRW2100029657.1.15",
          "form": "게임에 진심 ㅇㅈ",
          "original_form": "게임에 진심 ㅇㅈ",
          "speaker_id": "1",
          "time": "20210903 11:28"
        },
        {
          "id": "MDRW2100029657.1.16",
