        {
          "id": "MDRW2100029657.1.1",
          "form": "폰이랑 패드",
          "original_form": "폰이랑 패드",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.2",
          "form": "여러기게에 깔아놓으면",
          "original_form": "여러기게에 깔아놓으면",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.3",
          "form": "새로운게임 해봐야겠어요",
          "original_form": "새로운게임 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.4",
          "form": "공짜로 여러번하루 있어요",
          "original_form": "공짜로 여러번하루 있어요",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.5",
          "form": "여러개에 깔리나요",
          "original_form": "여러개에 깔리나요",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.6",
          "form": "구패드 신패드 폰 3대로 돌리고",
          "original_form": "구패드 신패드 폰 3대로 돌리고",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.7",
          "form": "아이디가 여러갠가요",
          "original_form": "아이디가 여러갠가요",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.8",
          "form": "가족 있을땐 그폰까지",
          "original_form": "가족 있을땐 그폰까지",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.9",
          "form": "하나의 아이디인가요",
          "original_form": "하나의 아이디인가요",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.10",
          "form": "4대돌려요",
          "original_form": "4대돌려요",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.11",
          "form": "무한가능",
          "original_form": "무한가능",
          "speaker_id": "2",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.12",
          "form": "오 가족아이디좋죠",
          "original_form": "오 가족아이디좋죠",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100029657.1.13",
          "form": "하나의 아이디요",
