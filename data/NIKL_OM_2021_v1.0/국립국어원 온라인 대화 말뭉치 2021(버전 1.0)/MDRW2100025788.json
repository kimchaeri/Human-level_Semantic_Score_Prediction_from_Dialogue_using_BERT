      "utterance": [
        {
          "id": "MDRW2100025788.1.1",
          "form": "여름이 다 간줄 알았더니 다시 여름이 온거같아요",
          "original_form": "여름이 다 간줄 알았더니 다시 여름이 온거같아요",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025788.1.2",
          "form": "너무 더워서 에어컨도 결국 켰네요 ㅠ",
          "original_form": "너무 더워서 에어컨도 결국 켰네요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025788.1.3",
          "form": "습해서 기온이 안높은데도  더운거 같아요",
          "original_form": "습해서 기온이 안높은데도  더운거 같아요",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025788.1.4",
          "form": "맞아요 ㅠ 너무 습해요 ㅠㅠ",
          "original_form": "맞아요 ㅠ 너무 습해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025788.1.5",
          "form": "비도 와서 밖에 나가기도 힘들구요...",
          "original_form": "비도 와서 밖에 나가기도 힘들구요...",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025788.1.6",
          "form": "신발 축축해지는거 너무 번거로워요",
          "original_form": "신발 축축해지는거 너무 번거로워요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025788.1.7",
          "form": "아 비와요??? 여긴 아직 안오는거같아요 ㅠ",
          "original_form": "아 비와요??? 여긴 아직 안오는거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025788.1.8",
          "form": "진짜 신발 축축싫어요 ㅠ",
          "original_form": "진짜 신발 축축싫어요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025788.1.9",
          "form": "여기도 이제 안와요! 어제 종일 오더라구요",
          "original_form": "여기도 이제 안와요! 어제 종일 오더라구요",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025788.1.10",
          "form": "오우.. 하루종일 ㅠ 정말 싫죠 ㅠ",
          "original_form": "오우.. 하루종일 ㅠ 정말 싫죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025788.1.11",
          "form": "태풍도 너무 심각해서 싫었어요 ㅠ",
          "original_form": "태풍도 너무 심각해서 싫었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025788.1.12",
          "form": "하필 운전하러 나간터라 ㅠㅠ",
          "original_form": "하필 운전하러 나간터라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025788.1.13",
          "form": "다시는 태풍올때 안나가려고요 ㅠ",
          "original_form": "다시는 태풍올때 안나가려고요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025788.1.14",
          "form": "헉 운전이라니 고생하셨겠다ㅠ",
          "original_form": "헉 운전이라니 고생하셨겠다ㅠ",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025788.1.15",
          "form": "저는 어제 백신 맞으러갔는데",
          "original_form": "저는 어제 백신 맞으러갔는데",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025788.1.16",
          "form": "비 흠뻑 맞고 왔어요",
          "original_form": "비 흠뻑 맞고 왔어요",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025788.1.17",
          "form": "아앗... 고생많으셨네요 하필 ㅠㅠ 그랬네요 ㅠㅠㅠ",
          "original_form": "아앗... 고생많으셨네요 하필 ㅠㅠ 그랬네요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025788.1.18",
          "form": "여름이 지나면 또 여름이 그리워질까요?",
          "original_form": "여름이 지나면 또 여름이 그리워질까요?",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025788.1.19",
          "form": "저는...여름을 그리워한적이 없어요...",
          "original_form": "저는...여름을 그리워한적이 없어요...",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025788.1.20",
          "form": "꾸준하게 싫어합니다",
          "original_form": "꾸준하게 싫어합니다",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025788.1.21",
          "form": "아... 그럼 어떤 계절 좋아하세요?",
          "original_form": "아... 그럼 어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025788.1.22",
          "form": "저는 가을 봄 겨울 여름 순으로 좋아해요",
          "original_form": "저는 가을 봄 겨울 여름 순으로 좋아해요",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025788.1.23",
          "form": "아항 저는 겨울 좋아해요",
          "original_form": "아항 저는 겨울 좋아해요",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025788.1.24",
          "form": "눈이 좋아요",
          "original_form": "눈이 좋아요",
          "speaker_id": "1",
          "time": "20210901 17:31"
        }
      ]
    }
  ]
