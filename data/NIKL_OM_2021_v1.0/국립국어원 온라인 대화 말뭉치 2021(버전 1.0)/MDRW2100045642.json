      "utterance": [
        {
          "id": "MDRW2100045642.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 04:36"
        },
        {
          "id": "MDRW2100045642.1.2",
          "form": "저는 콘서트가 너무 가고싶어요...",
          "original_form": "저는 콘서트가 너무 가고싶어요...",
          "speaker_id": "1",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045642.1.3",
          "form": "저두요.....콘서트로 일년을 버텼는데 이게 무슨일인가요",
          "original_form": "저두요.....콘서트로 일년을 버텼는데 이게 무슨일인가요",
          "speaker_id": "2",
          "time": "20210908 04:37"
        },
        {
          "id": "MDRW2100045642.1.4",
          "form": "제가 지방에 사는데 가끔 서울가서 콘서트 보고 힘을 얻었거든요...",
          "original_form": "제가 지방에 사는데 가끔 서울가서 콘서트 보고 힘을 얻었거든요...",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045642.1.5",
          "form": "근데 지금 제가 쓸애기가 된 이유도..",
          "original_form": "근데 지금 제가 쓸애기가 된 이유도..",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045642.1.6",
          "form": "콘서트가 없어서에요.. (아님)",
          "original_form": "콘서트가 없어서에요.. (아님)",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045642.1.7",
          "form": "ㅠㅠㅠㅠㅠ 진짜 콘서트 하",
          "original_form": "ㅠㅠㅠㅠㅠ 진짜 콘서트 하",
          "speaker_id": "2",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045642.1.8",
          "form": "누구 콘서튼가요",
          "original_form": "누구 콘서튼가요",
          "speaker_id": "2",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045642.1.9",
          "form": "저는 요새",
          "original_form": "저는 요새",
          "speaker_id": "1",
          "time": "20210908 04:38"
        },
        {
          "id": "MDRW2100045642.1.10",
          "form": "세븐틴 콘서트 가보고싶어요..",
          "original_form": "세븐틴 콘서트 가보고싶어요..",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045642.1.11",
          "form": "재밌을것같음..",
          "original_form": "재밌을것같음..",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045642.1.12",
          "form": "저는 방탄 콘서트요...",
          "original_form": "저는 방탄 콘서트요...",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045642.1.13",
          "form": "제 낙이었어요..",
          "original_form": "제 낙이었어요..",
          "speaker_id": "2",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045642.1.14",
          "form": "어유 방탄이면",
          "original_form": "어유 방탄이면",
          "speaker_id": "1",
          "time": "20210908 04:39"
        },
        {
          "id": "MDRW2100045642.1.15",
          "form": "콘서트 가기 힘들지않아요?",
          "original_form": "콘서트 가기 힘들지않아요?",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045642.1.16",
          "form": "하긴 세븐틴도 힘들겠다...",
          "original_form": "하긴 세븐틴도 힘들겠다...",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045642.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045642.1.18",
          "form": "저는 매번 표잡기 좋은 아이돌만",
          "original_form": "저는 매번 표잡기 좋은 아이돌만",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045642.1.19",
          "form": "좋아해봐서...",
          "original_form": "좋아해봐서...",
          "speaker_id": "1",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045642.1.20",
          "form": "힘들죠.. 근데 올콘이었는데 코로나때문에 취소됐어요",
          "original_form": "힘들죠.. 근데 올콘이었는데 코로나때문에 취소됐어요",
          "speaker_id": "2",
          "time": "20210908 04:40"
        },
        {
          "id": "MDRW2100045642.1.21",
          "form": "ㅠㅠ 너무 아쉽겠어요 ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠ 너무 아쉽겠어요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045642.1.22",
          "form": "코로나가 죄야..",
          "original_form": "코로나가 죄야..",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045642.1.23",
          "form": "코로나 나쁜놈들...",
          "original_form": "코로나 나쁜놈들...",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045642.1.24",
          "form": "우우...ㅠ",
          "original_form": "우우...ㅠ",
          "speaker_id": "1",
          "time": "20210908 04:41"
        },
        {
          "id": "MDRW2100045642.1.25",
          "form": "계속 시도하는바람에",
          "original_form": "계속 시도하는바람에",
          "speaker_id": "2",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045642.1.26",
          "form": "몇번이나 취소가 되서",
          "original_form": "몇번이나 취소가 되서",
          "speaker_id": "2",
          "time": "20210908 04:42"
        },
        {
          "id": "MDRW2100045642.1.27",
          "form": "그것도 맘아프고요...",
          "original_form": "그것도 맘아프고요...",
          "speaker_id": "2",
          "time": "20210908 04:42"
        }
      ]
    }
