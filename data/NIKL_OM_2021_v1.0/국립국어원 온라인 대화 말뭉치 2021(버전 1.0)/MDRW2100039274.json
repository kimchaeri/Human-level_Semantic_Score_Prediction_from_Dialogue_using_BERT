      "utterance": [
        {
          "id": "MDRW2100039274.1.1",
          "form": "앗 앞에 문장을 못봤어요",
          "original_form": "앗 앞에 문장을 못봤어요",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039274.1.2",
          "form": "벅차오르는 라인!!! 어떤 라인인가요 ㄷㄱㄷㄱ",
          "original_form": "벅차오르는 라인!!! 어떤 라인인가요 ㄷㄱㄷㄱ",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039274.1.3",
          "form": "앜ㅋㅋㅋ 그 우주소녀 러블리즈 여자친구",
          "original_form": "앜ㅋㅋㅋ 그 우주소녀 러블리즈 여자친구",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039274.1.4",
          "form": "이런 애니노래같은거요 ㅋㅋㅋ",
          "original_form": "이런 애니노래같은거요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039274.1.5",
          "form": "아아아 ㅋㅋㅋ맞아요 저도 그 라인 쭉 좋아해요",
          "original_form": "아아아 ㅋㅋㅋ맞아요 저도 그 라인 쭉 좋아해요",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039274.1.6",
          "form": "러블리즈는 요즘 왜 안나올까요 ㅋㅋ 노래좋았는데",
          "original_form": "러블리즈는 요즘 왜 안나올까요 ㅋㅋ 노래좋았는데",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039274.1.7",
          "form": "그러니까요ㅋㅋㅋ 여자친구는 해체했고 ㅠㅠ",
          "original_form": "그러니까요ㅋㅋㅋ 여자친구는 해체했고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039274.1.8",
          "form": "이누야샤노래같아서좋었는데",
          "original_form": "이누야샤노래같아서좋었는데",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039274.1.9",
          "form": "그니까요ㅠㅠㅠㅠㅠㅠㅠ여자친구 해체 넘 아쉬워요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "그니까요ㅠㅠㅠㅠㅠㅠㅠ여자친구 해체 넘 아쉬워요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039274.1.10",
          "form": "유리구슬때부터 좋아했는데........ㅠㅠㅠ",
          "original_form": "유리구슬때부터 좋아했는데........ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039274.1.11",
          "form": "그러니까요 ㅠㅠ 저도그때부터좋아했는데",
          "original_form": "그러니까요 ㅠㅠ 저도그때부터좋아했는데",
          "speaker_id": "1",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100039274.1.12",
          "form": "오마이걸 노래도 약간 다른느낌이지만 소녀소녀해서 좋아요 ㅋㅋㅋ",
          "original_form": "오마이걸 노래도 약간 다른느낌이지만 소녀소녀해서 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100039274.1.13",
          "form": "오 되게 구체적이시네욬ㅋㅋㅋㅋ",
          "original_form": "오 되게 구체적이시네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039274.1.14",
          "form": "ㅋㅋㅋㅋ노래듣는거 좋아해서 노래얘기해서 신났습니다",
          "original_form": "ㅋㅋㅋㅋ노래듣는거 좋아해서 노래얘기해서 신났습니다",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100039274.1.15",
          "form": "앜ㅋㅋㅋ저도 덕분에 플레이리스트 추가하고있어요",
          "original_form": "앜ㅋㅋㅋ저도 덕분에 플레이리스트 추가하고있어요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100039274.1.16",
          "form": "ㅋㅋㅋ저도 오랜만에 우주소녀 노래도 들어야겠어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저도 오랜만에 우주소녀 노래도 들어야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100039274.1.17",
          "form": "야호퇴근입니다 신나게 들어야엤어요%",
          "original_form": "야호퇴근입니다 신나게 들어야엤어요%",
