      "utterance": [
        {
          "id": "MDRW2100039073.1.1",
          "form": "요새 듣는 노래",
          "original_form": "요새 듣는 노래",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100039073.1.2",
          "form": "있으신가용",
          "original_form": "있으신가용",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100039073.1.3",
          "form": "본 조비의 이츠마이라이프 듣고있어요",
          "original_form": "본 조비의 이츠마이라이프 듣고있어요",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100039073.1.4",
          "form": "오 좋나요?!",
          "original_form": "오 좋나요?!",
          "speaker_id": "2",
          "time": "20210903 12:18"
        },
        {
          "id": "MDRW2100039073.1.5",
          "form": "락 밴드 곡이라ㅋㅋㅋ",
          "original_form": "락 밴드 곡이라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039073.1.6",
          "form": "이번에 슬의생에서 나왔더라구요",
          "original_form": "이번에 슬의생에서 나왔더라구요",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100039073.1.7",
          "form": "ㅋㅋㅋㅋ오 들어봐야겠어요",
          "original_form": "ㅋㅋㅋㅋ오 들어봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039073.1.8",
          "form": "저거 말고는 안들으세요?",
          "original_form": "저거 말고는 안들으세요?",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100039073.1.9",
          "form": "친구들한테 추천받은 아이돌곡 정도요?ㅋㅋㅋ",
          "original_form": "친구들한테 추천받은 아이돌곡 정도요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039073.1.10",
          "form": "ㅋㅋ 오 뭐있어요ㅋㅋㅋㅋ?",
          "original_form": "ㅋㅋ 오 뭐있어요ㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100039073.1.11",
          "form": "샤이니랑 온앤오프랑 루시랑... 여러개 추천 받았어요",
          "original_form": "샤이니랑 온앤오프랑 루시랑... 여러개 추천 받았어요",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039073.1.12",
          "form": "샤이니 곡은 맘에 들어서 계속 듣고 있어요ㅋㅋㅋ",
          "original_form": "샤이니 곡은 맘에 들어서 계속 듣고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039073.1.13",
          "form": "샤이니 뭐들어요?",
          "original_form": "샤이니 뭐들어요?",
          "speaker_id": "2",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100039073.1.14",
          "form": "전 원오브원 자주 듣거든요ㅋㅋㅋㅋㅋ",
          "original_form": "전 원오브원 자주 듣거든요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:07"
        },
        {
          "id": "MDRW2100039073.1.15",
          "form": "저 최근 발매된 곡들이요",
          "original_form": "저 최근 발매된 곡들이요",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039073.1.16",
          "form": "아틸란티스 좋더라구여",
          "original_form": "아틸란티스 좋더라구여",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039073.1.17",
          "form": "들어봐야겠어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "들어봐야겠어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039073.1.18",
          "form": "온앤오프는 뭐들으시나용",
          "original_form": "온앤오프는 뭐들으시나용",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039073.1.19",
          "form": "컴플리트요 제일 좋던데요ㅋㅋㅋ",
          "original_form": "컴플리트요 제일 좋던데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039073.1.20",
          "form": "나가고 싶어지는 느낌이에요",
          "original_form": "나가고 싶어지는 느낌이에요",
          "speaker_id": "1",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039073.1.21",
          "form": "ㅋㅋㅋㅋ아 뷰티풀도 있는데",
          "original_form": "ㅋㅋㅋㅋ아 뷰티풀도 있는데",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039073.1.22",
          "form": "중독성 있어요ㅋㅋㅋ",
          "original_form": "중독성 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039073.1.23",
          "form": "그거도 들어봤어요ㅋㅋㅋ 뮤비가 화려하더라구요",
          "original_form": "그거도 들어봤어요ㅋㅋㅋ 뮤비가 화려하더라구요",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039073.1.24",
          "form": "전 노래만 들었는데",
          "original_form": "전 노래만 들었는데",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039073.1.25",
          "form": "빰빰빰!! 이래서 중독송이ㅋㅋㅋㅋ",
