      "utterance": [
        {
          "id": "MDRW2100004453.1.1",
          "form": "ㅎ 샘플써서 이것저것 짬뽕이요",
          "original_form": "ㅎ 샘플써서 이것저것 짬뽕이요",
          "speaker_id": "2",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004453.1.2",
          "form": "그중 좋은거 있나요?",
          "original_form": "그중 좋은거 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004453.1.3",
          "form": "저도 샘플만ㅋ",
          "original_form": "저도 샘플만ㅋ",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004453.1.4",
          "form": "지금 헤라꺼쓰는데 비싼라인인거같아요",
          "original_form": "지금 헤라꺼쓰는데 비싼라인인거같아요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004453.1.5",
          "form": "설화수가 좋다는데 써보셨어요?",
          "original_form": "설화수가 좋다는데 써보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004453.1.6",
          "form": "설화수도 샘플만 잠깐 써봤어요",
          "original_form": "설화수도 샘플만 잠깐 써봤어요",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004453.1.7",
          "form": "한방냄새때매 싫더라구요",
          "original_form": "한방냄새때매 싫더라구요",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004453.1.8",
          "form": "비싼데 피부가 확 좋아지는 느낌도 없고",
          "original_form": "비싼데 피부가 확 좋아지는 느낌도 없고",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004453.1.9",
          "form": "다들 어디에 좋다는건지 모르겠어요",
          "original_form": "다들 어디에 좋다는건지 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004453.1.10",
          "form": "전 동인비 싱글에센스 쓰는데 괜찮은거같아요",
          "original_form": "전 동인비 싱글에센스 쓰는데 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004453.1.11",
          "form": "ㅎㅎ 그니깐요. 싼것도 비싼것도 트러블만없음 좋은거같아요",
          "original_form": "ㅎㅎ 그니깐요. 싼것도 비싼것도 트러블만없음 좋은거같아요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004453.1.12",
          "form": "저도 지금 그거써요. 갈색병 맞죠?",
          "original_form": "저도 지금 그거써요. 갈색병 맞죠?",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004453.1.13",
          "form": "지복합 피부라 이니스프리 그린티세럼도 괜찮고요",
          "original_form": "지복합 피부라 이니스프리 그린티세럼도 괜찮고요",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004453.1.14",
          "form": "갈색병은 빈티지에센스요ㅎㅎ",
          "original_form": "갈색병은 빈티지에센스요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004453.1.15",
          "form": "그린티도 어딘가에있을텐데...아직 안써봤어요",
          "original_form": "그린티도 어딘가에있을텐데...아직 안써봤어요",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004453.1.16",
          "form": "빨간색이 동인비",
          "original_form": "빨간색이 동인비",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004453.1.17",
          "form": "ㅎㅎ 빈티지인가보네요.  솜에 묻혀서 스킨대신 써요.",
          "original_form": "ㅎㅎ 빈티지인가보네요.  솜에 묻혀서 스킨대신 써요.",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004453.1.18",
          "form": "저도요 ㅎ 날짜지날까봐 그비싼걸 막 퍼발랐더니",
          "original_form": "저도요 ㅎ 날짜지날까봐 그비싼걸 막 퍼발랐더니",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004453.1.19",
          "form": "전 다썼어요ㅋ",
          "original_form": "전 다썼어요ㅋ",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004453.1.20",
          "form": "전 아직도 그대로~~",
          "original_form": "전 아직도 그대로~~",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004453.1.21",
          "form": "날짜 확인해보시고 후딱쓰셔요",
          "original_form": "날짜 확인해보시고 후딱쓰셔요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004453.1.22",
          "form": "발효에센스라 빨리쓰는게 좋을듯요",
          "original_form": "발효에센스라 빨리쓰는게 좋을듯요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004453.1.23",
          "form": "발효가 안맞는건지 이마쪽에 트러블올라오네요",
          "original_form": "발효가 안맞는건지 이마쪽에 트러블올라오네요",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004453.1.24",
          "form": "그런분들도 있대요 발효에센스가 안맞음",
          "original_form": "그런분들도 있대요 발효에센스가 안맞음",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004453.1.25",
          "form": "그럼 쓰지마시고 중단하셔야할듯",
          "original_form": "그럼 쓰지마시고 중단하셔야할듯",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004453.1.26",
          "form": "숨껀 괜찮았는데...화장품이 문제인지 안바르다 발라서그런건지",
          "original_form": "숨껀 괜찮았는데...화장품이 문제인지 안바르다 발라서그런건지",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004453.1.27",
          "form": "쫌더 써보고 계속그럼 그만써야죠",
          "original_form": "쫌더 써보고 계속그럼 그만써야죠",
          "speaker_id": "2",
          "time": "20210821 16:50"
        }
      ]
