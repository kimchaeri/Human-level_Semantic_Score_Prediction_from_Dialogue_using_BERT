      "utterance": [
        {
          "id": "MDRW2100010452.1.1",
          "form": "ㅇㅇ 퇴근 중",
          "original_form": "ㅇㅇ 퇴근 중",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010452.1.2",
          "form": "어쩌나.. 비가 많이 내리네",
          "original_form": "어쩌나.. 비가 많이 내리네",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010452.1.3",
          "form": "하아.. 비도 많이 내리고 사람도 많고 ㅠ",
          "original_form": "하아.. 비도 많이 내리고 사람도 많고 ㅠ",
          "speaker_id": "2",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010452.1.4",
          "form": "우산에서 물 뚝뚝 떨어지지 ㅠㅠ",
          "original_form": "우산에서 물 뚝뚝 떨어지지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010452.1.5",
          "form": "ㅇㅇ 바닥 다 젖었지 뭐",
          "original_form": "ㅇㅇ 바닥 다 젖었지 뭐",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010452.1.6",
          "form": "어여 조심히 와",
          "original_form": "어여 조심히 와",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010452.1.7",
          "form": "비가 앞이 안보일 정도로 내리네",
          "original_form": "비가 앞이 안보일 정도로 내리네",
          "speaker_id": "2",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010452.1.8",
          "form": "아무래도 태풍이니까..",
          "original_form": "아무래도 태풍이니까..",
          "speaker_id": "1",
          "time": "20210823 18:30"
        },
        {
          "id": "MDRW2100010452.1.9",
          "form": "바람이라도 안불면 괜찮은데 우산이 막 날아다님 ㅋㅋ",
          "original_form": "바람이라도 안불면 괜찮은데 우산이 막 날아다님 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010452.1.10",
          "form": "에궁 중심잡기 힘들겠네 ㅠ",
          "original_form": "에궁 중심잡기 힘들겠네 ㅠ",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010452.1.11",
          "form": "몸이 휘청휘청 햐",
          "original_form": "몸이 휘청휘청 햐",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010452.1.12",
          "form": "집에 오면 편안해질껴",
          "original_form": "집에 오면 편안해질껴",
          "speaker_id": "1",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010452.1.13",
          "form": "이미 양말도 다 젖어서 축축해",
          "original_form": "이미 양말도 다 젖어서 축축해",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010452.1.14",
