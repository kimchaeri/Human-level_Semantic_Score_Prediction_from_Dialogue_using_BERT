      "utterance": [
        {
          "id": "MDRW2100042780.1.1",
          "form": "자유시간이구만? 아 두찌가 아직안갔나>",
          "original_form": "자유시간이구만? 아 두찌가 아직안갔나>",
          "speaker_id": "2",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042780.1.2",
          "form": "이제보내야된다ㅋ",
          "original_form": "이제보내야된다ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042780.1.3",
          "form": "보내고올게ㅋ",
          "original_form": "보내고올게ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042780.1.4",
          "form": "응 갔다와유",
          "original_form": "응 갔다와유",
          "speaker_id": "2",
          "time": "20210907 08:59"
        },
        {
          "id": "MDRW2100042780.1.5",
          "form": "응 준비 끝 다녀올게",
          "original_form": "응 준비 끝 다녀올게",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042780.1.6",
          "form": "비안오지??",
          "original_form": "비안오지??",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042780.1.7",
          "form": "아직은 안 오는 것 같은데",
          "original_form": "아직은 안 오는 것 같은데",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042780.1.8",
          "form": "곧 올 것 같다",
          "original_form": "곧 올 것 같다",
          "speaker_id": "1",
          "time": "20210907 09:07"
        },
        {
          "id": "MDRW2100042780.1.9",
          "form": "에구궁",
          "original_form": "에구궁",
          "speaker_id": "2",
          "time": "20210907 09:08"
        },
        {
          "id": "MDRW2100042780.1.10",
          "form": "비오면 애들 데릴러 갈때 힘든데 ㅎ 안오길",
          "original_form": "비오면 애들 데릴러 갈때 힘든데 ㅎ 안오길",
          "speaker_id": "2",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042780.1.11",
          "form": "맞어 비오면 힘들다",
          "original_form": "맞어 비오면 힘들다",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042780.1.12",
          "form": "첫쨰도 우산안가져갔지 싶은데 ㅎㅎㅎ",
          "original_form": "첫쨰도 우산안가져갔지 싶은데 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042780.1.13",
          "form": "오늘 지원금 신청하는날 아냐?? 했어??",
          "original_form": "오늘 지원금 신청하는날 아냐?? 했어??",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042780.1.14",
          "form": "응응 우리 첫째도 ㅋㅋ 우산 작은거 학교에놔두라고 ㅋㅋ했었는데 ㅋㅋㅋ 그걸 가지고 있으면",
          "original_form": "응응 우리 첫째도 ㅋㅋ 우산 작은거 학교에놔두라고 ㅋㅋ했었는데 ㅋㅋㅋ 그걸 가지고 있으면",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042780.1.15",
          "form": "그거 쓰고 오라고 했어",
          "original_form": "그거 쓰고 오라고 했어",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042780.1.16",
          "form": "없으면은 전화하면 데리러가야지 ㅋㅋ 비오면",
          "original_form": "없으면은 전화하면 데리러가야지 ㅋㅋ 비오면",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042780.1.17",
          "form": "응 오늘이야 ㅋㅋ 해야지 ㅋ",
          "original_form": "응 오늘이야 ㅋㅋ 해야지 ㅋ",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042780.1.18",
          "form": "너는 했어?",
          "original_form": "너는 했어?",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042780.1.19",
          "form": "응ㅋ어제",
          "original_form": "응ㅋ어제",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042780.1.20",
          "form": "name3도 우산사물함잇는데 그냥오더라 한번은",
          "original_form": "&name3&도 우산사물함잇는데 그냥오더라 한번은",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042780.1.21",
          "form": "맞어 왠지 name4도 없다하면서 그냥 전화할듯",
          "original_form": "맞어 왠지 &name4&도 없다하면서 그냥 전화할듯",
          "speaker_id": "1",
