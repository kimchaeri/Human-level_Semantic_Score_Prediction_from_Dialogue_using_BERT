      "utterance": [
        {
          "id": "MDRW2100035705.1.1",
          "form": "아 파더 브라운봐야지",
          "original_form": "아 파더 브라운봐야지",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100035705.1.2",
          "form": "오늘은 그냥 계속 영상보고 놀음",
          "original_form": "오늘은 그냥 계속 영상보고 놀음",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100035705.1.3",
          "form": "웅 수고했다name2",
          "original_form": "웅 수고했다&name2&",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100035705.1.4",
          "form": "쉬어야한다",
          "original_form": "쉬어야한다",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100035705.1.5",
          "form": "아이고ㅋ name2님 ㅋ",
          "original_form": "아이고ㅋ &name2&님 ㅋ",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100035705.1.6",
          "form": "파더브라운이 머임",
          "original_form": "파더브라운이 머임",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035705.1.7",
          "form": "일단 그 판매자에게 물어봐놓긴함",
          "original_form": "일단 그 판매자에게 물어봐놓긴함",
          "speaker_id": "2",
          "time": "20210831 19:19"
        },
        {
          "id": "MDRW2100035705.1.8",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "1",
          "time": "20210831 19:19"
        },
        {
          "id": "MDRW2100035705.1.9",
          "form": "문의는해봐야지 새건데",
          "original_form": "문의는해봐야지 새건데",
          "speaker_id": "1",
          "time": "20210831 19:20"
        },
        {
          "id": "MDRW2100035705.1.10",
          "form": "요새 일드나 애니 보는거있음?",
          "original_form": "요새 일드나 애니 보는거있음?",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100035705.1.11",
          "form": "나 라프텔 일주일이용권 받아서 애니볼라구 시간 될때",
          "original_form": "나 라프텔 일주일이용권 받아서 애니볼라구 시간 될때",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100035705.1.12",
          "form": "머보징 몹싸이코 이거보고싶당",
          "original_form": "머보징 몹싸이코 이거보고싶당",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100035705.1.13",
          "form": "모브사이콘가먼가 골든카무이하구",
          "original_form": "모브사이콘가먼가 골든카무이하구",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100035705.1.14",
          "form": "오 공짜로 받음? 굳굳",
          "original_form": "오 공짜로 받음? 굳굳",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100035705.1.15",
          "form": "웅 게임이벤트로 히히",
          "original_form": "웅 게임이벤트로 히히",
          "speaker_id": "1",
          "time": "20210831 20:03"
        },
        {
          "id": "MDRW2100035705.1.16",
          "form": "name2님은 영어강좌같은거 안봄? 유투브로",
          "original_form": "&name2&님은 영어강좌같은거 안봄? 유투브로",
          "speaker_id": "1",
          "time": "20210831 20:08"
        },
        {
          "id": "MDRW2100035705.1.17",
          "form": "나는 파더브라운 ㅋㅋ시즌8까지 쭉보고",
          "original_form": "나는 파더브라운 ㅋㅋ시즌8까지 쭉보고",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100035705.1.18",
          "form": "오늘또 시즌 1또보는중",
          "original_form": "오늘또 시즌 1또보는중",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100035705.1.19",
          "form": "오 푹빠지셨구만",
          "original_form": "오 푹빠지셨구만",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100035705.1.20",
          "form": "애니는 음 나는 장편인게 좋은데",
          "original_form": "애니는 음 나는 장편인게 좋은데",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100035705.1.21",
          "form": "아 블랙 클로버 봤었음",
          "original_form": "아 블랙 클로버 봤었음",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100035705.1.22",
          "form": "그게무야",
          "original_form": "그게무야",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100035705.1.23",
          "form": "애니 ㅋㅋ",
          "original_form": "애니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100035705.1.24",
