      "utterance": [
        {
          "id": "MDRW2100041524.1.1",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "1",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100041524.1.2",
          "form": "우리애는 아직 4개월밖에 안돼요 ㅎㅎ",
          "original_form": "우리애는 아직 4개월밖에 안돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:01"
        },
        {
          "id": "MDRW2100041524.1.3",
          "form": "오 저는 이제 4살 생일 지났어요 ㅎㅎ",
          "original_form": "오 저는 이제 4살 생일 지났어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 09:02"
        },
        {
          "id": "MDRW2100041524.1.4",
          "form": "저도 남자애기에요",
          "original_form": "저도 남자애기에요",
          "speaker_id": "2",
          "time": "20210906 09:20"
        },
        {
          "id": "MDRW2100041524.1.5",
          "form": "곧 중성화 해야할것 같아요",
          "original_form": "곧 중성화 해야할것 같아요",
          "speaker_id": "2",
          "time": "20210906 09:20"
        },
        {
          "id": "MDRW2100041524.1.6",
          "form": "2.5키로인가 넘을때 하라더라구요",
          "original_form": "2.5키로인가 넘을때 하라더라구요",
          "speaker_id": "1",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041524.1.7",
          "form": "오 꿀티집 감사합니다",
          "original_form": "오 꿀티집 감사합니다",
          "speaker_id": "2",
          "time": "20210906 09:25"
        },
        {
          "id": "MDRW2100041524.1.8",
          "form": "꿀팁",
          "original_form": "꿀팁",
          "speaker_id": "2",
          "time": "20210906 09:25"
        },
        {
          "id": "MDRW2100041524.1.9",
          "form": "제칭구 강쥐는 중성화 안해서 유선종양 생겼다는",
          "original_form": "제칭구 강쥐는 중성화 안해서 유선종양 생겼다는",
          "speaker_id": "1",
          "time": "20210906 09:38"
        },
        {
          "id": "MDRW2100041524.1.10",
          "form": "헉 중성화가 생각보다 중요한가 보네요",
          "original_form": "헉 중성화가 생각보다 중요한가 보네요",
          "speaker_id": "2",
          "time": "20210906 09:45"
        },
        {
          "id": "MDRW2100041524.1.11",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210906 09:46"
        },
        {
          "id": "MDRW2100041524.1.12",
          "form": "특히 암컷은 더 중요할것같아요",
          "original_form": "특히 암컷은 더 중요할것같아요",
          "speaker_id": "1",
          "time": "20210906 09:46"
        },
        {
          "id": "MDRW2100041524.1.13",
          "form": "일단 수컷이긴한데 잘먹여야 겠네용",
          "original_form": "일단 수컷이긴한데 잘먹여야 겠네용",
          "speaker_id": "2",
          "time": "20210906 10:10"
        },
        {
          "id": "MDRW2100041524.1.14",
          "form": "맞아요 ㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 10:47"
        },
        {
          "id": "MDRW2100041524.1.15",
          "form": "혹시 간식은 뭐주세요?",
          "original_form": "혹시 간식은 뭐주세요?",
          "speaker_id": "1",
          "time": "20210906 10:48"
        },
        {
          "id": "MDRW2100041524.1.16",
          "form": "저는 말린 닭가슴살 많이줘요 샤오루 님은요",
          "original_form": "저는 말린 닭가슴살 많이줘요 샤오루 님은요",
          "speaker_id": "2",
          "time": "20210906 10:48"
        },
        {
          "id": "MDRW2100041524.1.17",
          "form": "사람용 닭가슴 삶아서 주네요",
          "original_form": "사람용 닭가슴 삶아서 주네요",
          "speaker_id": "1",
