      "utterance": [
        {
          "id": "MDRW2100033917.1.1",
          "form": "생크림 케이크 vs 치즈 케이크 어떠세요?",
          "original_form": "생크림 케이크 vs 치즈 케이크 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100033917.1.2",
          "form": "네 ㅎㅎ전 치즈요",
          "original_form": "네 ㅎㅎ전 치즈요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100033917.1.3",
          "form": "근데 솔직히 둘다 잘먹어요 ㅋㅋ",
          "original_form": "근데 솔직히 둘다 잘먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100033917.1.4",
          "form": "ㅋㅋㅋ전 생크림이랍니다",
          "original_form": "ㅋㅋㅋ전 생크림이랍니다",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100033917.1.5",
          "form": "딸기케익vs초코케익은요?",
          "original_form": "딸기케익vs초코케익은요?",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100033917.1.6",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100033917.1.7",
          "form": "초코요 ㅋㅋ",
          "original_form": "초코요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100033917.1.8",
          "form": "초코초코 좋아해요 ㅋㅋ",
          "original_form": "초코초코 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100033917.1.9",
          "form": "ㅋㅋㅋ저도 초코요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저도 초코요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100033917.1.10",
          "form": "호박고구마vs밤고구마는 어떠세요?",
          "original_form": "호박고구마vs밤고구마는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100033917.1.11",
          "form": "밤이죠",
          "original_form": "밤이죠",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100033917.1.12",
          "form": "전 호박 못먹어요 ㅡㅡ",
          "original_form": "전 호박 못먹어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100033917.1.13",
          "form": "ㅋㅋㅋㅋ 전호박고구마요",
          "original_form": "ㅋㅋㅋㅋ 전호박고구마요",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033917.1.14",
          "form": "달달하니",
          "original_form": "달달하니",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033917.1.15",
          "form": "오 그래요",
          "original_form": "오 그래요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033917.1.16",
          "form": "전 워낙 밤고구마 팬이예요 ㅎㅎ",
          "original_form": "전 워낙 밤고구마 팬이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033917.1.17",
          "form": "식감이 밤이 좋아요",
          "original_form": "식감이 밤이 좋아요",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100033917.1.18",
          "form": "아들 떡볶이 보내야 하는데 안오네요 ㅋㅋ",
          "original_form": "아들 떡볶이 보내야 하는데 안오네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100033917.1.19",
          "form": "복숭아와 사과는요?",
          "original_form": "복숭아와 사과는요?",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100033917.1.20",
          "form": "전 복숭아파 예요 ㅎㅎ",
          "original_form": "전 복숭아파 예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100033917.1.21",
          "form": "ㅋㅋㅋ아드님이 아직이신가요? 떡볶이가 와야하는데",
          "original_form": "ㅋㅋㅋ아드님이 아직이신가요? 떡볶이가 와야하는데",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033917.1.22",
          "form": "저도 복숭아요 사실전 사과를 별로",
          "original_form": "저도 복숭아요 사실전 사과를 별로",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033917.1.23",
          "form": "안좋아해요ㅋㅋㅋ",
          "original_form": "안좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033917.1.24",
          "form": "사과 요새 별로예요",
          "original_form": "사과 요새 별로예요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033917.1.25",
          "form": "그러니까요 ㅜㅜ",
          "original_form": "그러니까요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033917.1.26",
          "form": "오늘늦네요",
          "original_form": "오늘늦네요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033917.1.27",
          "form": "이러다 출동합니다 ㅋㅋ",
          "original_form": "이러다 출동합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100033917.1.28",
          "form": "ㅠㅠ아직인가요?????",
          "original_form": "ㅠㅠ아직인가요?????",
          "speaker_id": "1",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100033917.1.29",
          "form": "놀다오는건가요 연락이안되나요",
          "original_form": "놀다오는건가요 연락이안되나요",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100033917.1.30",
          "form": "전 물복좋아해요",
          "original_form": "전 물복좋아해요",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100033917.1.31",
          "form": "물복이요?",
          "original_form": "물복이요?",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100033917.1.32",
          "form": "저 복국 잘먹었는데",
          "original_form": "저 복국 잘먹었는데",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100033917.1.33",
          "form": "물복이 뭔가요?",
          "original_form": "물복이 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100033917.1.34",
          "form": "말랑말랑한",
          "original_form": "말랑말랑한",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100033917.1.35",
          "form": "복숭아요ㅋㅋㅋㅋ",
          "original_form": "복숭아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100033917.1.36",
          "form": "딱복은 안돼요!!!",
          "original_form": "딱복은 안돼요!!!",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100033917.1.37",
          "form": "아 그렇게 말해요 ㅎㅎ",
          "original_form": "아 그렇게 말해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033917.1.38",
          "form": "전 딱딱이 물렁이 라고해요 ㅎㅎ",
          "original_form": "전 딱딱이 물렁이 라고해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033917.1.39",
          "form": "전 딱복이요 ㅋㅋ",
          "original_form": "전 딱복이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033917.1.40",
          "form": "근데 캔복숭아 너무 좋아해요",
