        },
        {
          "id": "MDRW2100014192.1.2",
          "form": "가자 언제 가지",
          "original_form": "가자 언제 가지",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014192.1.3",
          "form": "다음 달에 가자",
          "original_form": "다음 달에 가자",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014192.1.4",
          "form": "구램 날씨도 딱 좋고",
          "original_form": "구램 날씨도 딱 좋고",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014192.1.5",
          "form": "그래 가서 맛있는 거 많이 먹고 오자",
          "original_form": "그래 가서 맛있는 거 많이 먹고 오자",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014192.1.6",
          "form": "구램 ㅋㅋ 흙돼지도 먹고",
          "original_form": "구램 ㅋㅋ 흙돼지도 먹고",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014192.1.7",
          "form": "엄마 좋아하는 국수도 먹고",
          "original_form": "엄마 좋아하는 국수도 먹고",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014192.1.8",
          "form": "그래 좋다",
          "original_form": "그래 좋다",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014192.1.9",
          "form": "담달에 가려면 빨리 계획을 짜야겠군",
          "original_form": "담달에 가려면 빨리 계획을 짜야겠군",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014192.1.10",
          "form": "그래 계획 세워 봐라",
          "original_form": "그래 계획 세워 봐라",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014192.1.11",
          "form": "옹옹 맡겨주시오 ㅋ",
          "original_form": "옹옹 맡겨주시오 ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014192.1.12",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014192.1.13",
          "form": "빨리 가고싶다 ㅠㅠ",
          "original_form": "빨리 가고싶다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014192.1.14",
          "form": "시간 금방 간다",
          "original_form": "시간 금방 간다",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014192.1.15",
          "form": "옹옹.. 열일하면서 기다려야겠군",
          "original_form": "옹옹.. 열일하면서 기다려야겠군",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014192.1.16",
          "form": "미국여행은 언제 갈수있을까 ㅠㅠ 언니 보러가구싶다",
