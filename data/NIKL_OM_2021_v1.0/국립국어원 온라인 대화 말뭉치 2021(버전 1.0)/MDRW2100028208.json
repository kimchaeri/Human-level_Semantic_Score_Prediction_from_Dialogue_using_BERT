      "utterance": [
        {
          "id": "MDRW2100028208.1.1",
          "form": "저는 봉사활동 다녀요",
          "original_form": "저는 봉사활동 다녀요",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028208.1.2",
          "form": "연탄배달 해보셯나요",
          "original_form": "연탄배달 해보셯나요",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028208.1.3",
          "form": "전 고딩 이후로 안한거 같아요",
          "original_form": "전 고딩 이후로 안한거 같아요",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.4",
          "form": "아뇨 저는 퇴근후에",
          "original_form": "아뇨 저는 퇴근후에",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.5",
          "form": "요양원 가서 돌봐드렸어요",
          "original_form": "요양원 가서 돌봐드렸어요",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.6",
          "form": "항상 봉사하고잇어요",
          "original_form": "항상 봉사하고잇어요",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.7",
          "form": "어떤거요",
          "original_form": "어떤거요",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.8",
          "form": "유료봉사입니다.근데 힘드네요",
          "original_form": "유료봉사입니다.근데 힘드네요",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.9",
          "form": "가서 전 풀도 뜯고 그랬어요",
          "original_form": "가서 전 풀도 뜯고 그랬어요",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028208.1.10",
          "form": "그냥 일종의 몸종 같은 거에요",
          "original_form": "그냥 일종의 몸종 같은 거에요",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.11",
          "form": "저도 시중 들어요",
          "original_form": "저도 시중 들어요",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.12",
          "form": "풀뜯는 소리 해보셨나요?",
          "original_form": "풀뜯는 소리 해보셨나요?",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.13",
          "form": "퇴근 후에 더 힘드네요",
          "original_form": "퇴근 후에 더 힘드네요",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.14",
          "form": "시중은 봉사가 아니죠",
          "original_form": "시중은 봉사가 아니죠",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.15",
          "form": "봉사하는 마음으로 인내해야죠",
          "original_form": "봉사하는 마음으로 인내해야죠",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.16",
          "form": "저는 장애인봉사해요",
          "original_form": "저는 장애인봉사해요",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.17",
          "form": "봉사정신으로",
          "original_form": "봉사정신으로",
          "speaker_id": "2",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028208.1.18",
          "form": "퇴근후에 장애인돌봐요",
          "original_form": "퇴근후에 장애인돌봐요",
