      "utterance": [
        {
          "id": "MDRW2100043050.1.1",
          "form": "방금 택배로 오겹살이 와서 저녁은 오겹살 ㅋㅋㅋ",
          "original_form": "방금 택배로 오겹살이 와서 저녁은 오겹살 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043050.1.2",
          "form": "하 완벽.....",
          "original_form": "하 완벽.....",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043050.1.3",
          "form": "고기는 사랑입니다.",
          "original_form": "고기는 사랑입니다.",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043050.1.4",
          "form": "저는 또 밥없이 고기못먹는사람이라",
          "original_form": "저는 또 밥없이 고기못먹는사람이라",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043050.1.5",
          "form": "밥 된찌 고기",
          "original_form": "밥 된찌 고기",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043050.1.6",
          "form": "사랑이에요ㅜㅜ",
          "original_form": "사랑이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043050.1.7",
          "form": "3종셋트 완벽",
          "original_form": "3종셋트 완벽",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043050.1.8",
          "form": "그쳐그쳐",
          "original_form": "그쳐그쳐",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043050.1.9",
          "form": "마늘 넣고 고추넣고",
          "original_form": "마늘 넣고 고추넣고",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043050.1.10",
          "form": "쌈딱",
          "original_form": "쌈딱",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043050.1.11",
          "form": "오겹살 조으다.....ㅎㅎ",
          "original_form": "오겹살 조으다.....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043050.1.12",
          "form": "오겹살이 삼겹보다 싸길래...원래 오겹이 더비싼거 아닌가여 ㅎㅎ",
          "original_form": "오겹살이 삼겹보다 싸길래...원래 오겹이 더비싼거 아닌가여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100043050.1.13",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043050.1.14",
          "form": "더비싼걸로아는뎅",
          "original_form": "더비싼걸로아는뎅",
          "speaker_id": "2",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043050.1.15",
          "form": "몇백원 더 싸고 오예 하고 오겹으로 ㅋㅋ",
          "original_form": "몇백원 더 싸고 오예 하고 오겹으로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:41"
        },
        {
          "id": "MDRW2100043050.1.16",
          "form": "에프에 구우시나용?",
          "original_form": "에프에 구우시나용?",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043050.1.17",
          "form": "후라이팬?",
          "original_form": "후라이팬?",
          "speaker_id": "2",
          "time": "20210907 13:42"
        },
        {
          "id": "MDRW2100043050.1.18",
          "form": "에프는 청소하기 싫어서 후라이팬에이요 ㅠ",
