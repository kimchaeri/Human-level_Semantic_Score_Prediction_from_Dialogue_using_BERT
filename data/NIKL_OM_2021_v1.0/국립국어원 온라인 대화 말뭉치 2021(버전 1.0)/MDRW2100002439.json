      "utterance": [
        {
          "id": "MDRW2100002439.1.1",
          "form": "마트 장 볼 때 주로 사는게 있나요?",
          "original_form": "마트 장 볼 때 주로 사는게 있나요?",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002439.1.2",
          "form": "요즘은 아이스크림에 빠져서",
          "original_form": "요즘은 아이스크림에 빠져서",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002439.1.3",
          "form": "아이스크림 위주로 사고 있어요 ㅎ",
          "original_form": "아이스크림 위주로 사고 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002439.1.4",
          "form": "가장좋아하는 아이스크림은 뭔가요?",
          "original_form": "가장좋아하는 아이스크림은 뭔가요?",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002439.1.5",
          "form": "저는 요즘 젤루좋아 청포도 좋아해요",
          "original_form": "저는 요즘 젤루좋아 청포도 좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002439.1.6",
          "form": "오 젤루좋아 맛있죠 전 오렌지맛 좋아했어요",
          "original_form": "오 젤루좋아 맛있죠 전 오렌지맛 좋아했어요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002439.1.7",
          "form": "요즘 왕모히또 이거 먹는데 맛있어요",
          "original_form": "요즘 왕모히또 이거 먹는데 맛있어요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002439.1.8",
          "form": "추천드릴게요",
          "original_form": "추천드릴게요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002439.1.9",
          "form": "헐 모히또 완전좋아",
          "original_form": "헐 모히또 완전좋아",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002439.1.10",
          "form": "꼭 먹어볼게요",
          "original_form": "꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002439.1.11",
          "form": "진짜 추천합니다",
          "original_form": "진짜 추천합니다",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002439.1.12",
          "form": "요새 빠져서 1일 1아이스크림도 했어요",
          "original_form": "요새 빠져서 1일 1아이스크림도 했어요",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002439.1.13",
          "form": "아이스크림 말고는 다른거 뭐 사시나요?",
          "original_form": "아이스크림 말고는 다른거 뭐 사시나요?",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002439.1.14",
          "form": "이건 꼭 산다 하는거",
          "original_form": "이건 꼭 산다 하는거",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002439.1.15",
          "form": "저는 두부 꼭 사는거같아요",
          "original_form": "저는 두부 꼭 사는거같아요",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002439.1.16",
          "form": "웃기지만 전 과자요 하하",
          "original_form": "웃기지만 전 과자요 하하",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002439.1.17",
          "form": "두부 많이 좋아하세요?",
          "original_form": "두부 많이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002439.1.18",
          "form": "음 좋아하지는 않는데 활용도가 높아서..",
          "original_form": "음 좋아하지는 않는데 활용도가 높아서..",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002439.1.19",
          "form": "과자 어떤거 좋아해요?",
          "original_form": "과자 어떤거 좋아해요?",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002439.1.20",
          "form": "과자 허니버터칩 좋아합니다",
          "original_form": "과자 허니버터칩 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002439.1.21",
          "form": "두부로 뭐 많이 해드세요,?",
          "original_form": "두부로 뭐 많이 해드세요,?",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002439.1.22",
          "form": "그냥 국끓일 때나 두부부침 조림 이런거?",
          "original_form": "그냥 국끓일 때나 두부부침 조림 이런거?",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002439.1.23",
          "form": "근데 웃긴건 저 두부안좋아해요ㅎㅎㅎㅎ",
          "original_form": "근데 웃긴건 저 두부안좋아해요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002439.1.24",
          "form": "아ㅋㅋㅋㅋㅋ 두부 안좋아하는데 그래도 자주 구매하시군요",
          "original_form": "아ㅋㅋㅋㅋㅋ 두부 안좋아하는데 그래도 자주 구매하시군요",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002439.1.25",
          "form": "ㅋㅋㅋ어이없죠ㅋㅋ 저도 그래요 그래서 항상 니는 두부싫어하면서 두부사더라 소리들어요",
          "original_form": "ㅋㅋㅋ어이없죠ㅋㅋ 저도 그래요 그래서 항상 니는 두부싫어하면서 두부사더라 소리들어요",
