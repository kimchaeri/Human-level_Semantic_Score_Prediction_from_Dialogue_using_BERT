      "utterance": [
        {
          "id": "MDRW2100009606.1.1",
          "form": "요새 선선하네요ㅎㅎ",
          "original_form": "요새 선선하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009606.1.2",
          "form": "안주무시나요?ㅎ",
          "original_form": "안주무시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009606.1.3",
          "form": "이제 슬슬 자려구요~~~!!",
          "original_form": "이제 슬슬 자려구요~~~!!",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009606.1.4",
          "form": "아까 박스 버리러 나갔다왔는데 선선하더라구요",
          "original_form": "아까 박스 버리러 나갔다왔는데 선선하더라구요",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009606.1.5",
          "form": "태풍올라온대서 비올거같아요",
          "original_form": "태풍올라온대서 비올거같아요",
          "speaker_id": "1",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009606.1.6",
          "form": "창 닫고 자야겠어요 ㅎㅎ",
          "original_form": "창 닫고 자야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009606.1.7",
          "form": "창문열어놓으셨어요? 비들이닥칠지도 모르니 꼭닫고 주무셔요",
          "original_form": "창문열어놓으셨어요? 비들이닥칠지도 모르니 꼭닫고 주무셔요",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009606.1.8",
          "form": "네 감사해요ㅎㅎ 언제쯤 주무시나요",
          "original_form": "네 감사해요ㅎㅎ 언제쯤 주무시나요",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009606.1.9",
          "form": "세시쯤?ㅎ 이제설거지하고 누웠네요",
          "original_form": "세시쯤?ㅎ 이제설거지하고 누웠네요",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009606.1.10",
          "form": "좀 늦게자는편이라",
          "original_form": "좀 늦게자는편이라",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009606.1.11",
          "form": "ㅋㅋ 비올지도 모르니 창문은 닫으셨나요",
          "original_form": "ㅋㅋ 비올지도 모르니 창문은 닫으셨나요",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009606.1.12",
          "form": "창문안열어놔서 집이 더운가봐요ㅎ",
          "original_form": "창문안열어놔서 집이 더운가봐요ㅎ",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009606.1.13",
          "form": "에어컨 들었는데",
          "original_form": "에어컨 들었는데",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009606.1.14",
          "form": "서울은 태풍영향권은 아닌가봐요",
          "original_form": "서울은 태풍영향권은 아닌가봐요",
          "speaker_id": "1",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009606.1.15",
          "form": "에어컨 키기에는 추운데요ㅎㅎ",
          "original_form": "에어컨 키기에는 추운데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 02:44"
        },
        {
          "id": "MDRW2100009606.1.16",
          "form": "그런가요? 오피스텔이라 좀 더워요ㅎ",
          "original_form": "그런가요? 오피스텔이라 좀 더워요ㅎ",
          "speaker_id": "1",
          "time": "20210823 02:44"
        },
        {
          "id": "MDRW2100009606.1.17",
          "form": "바람이 안통해서",
          "original_form": "바람이 안통해서",
          "speaker_id": "1",
          "time": "20210823 02:45"
        },
        {
          "id": "MDRW2100009606.1.18",
          "form": "아파트는 맞바람 쳐서 시원할듯요",
          "original_form": "아파트는 맞바람 쳐서 시원할듯요",
          "speaker_id": "1",
          "time": "20210823 02:45"
        },
        {
          "id": "MDRW2100009606.1.19",
          "form": "잠들었네요 ㅎㅎ",
          "original_form": "잠들었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:16"
        },
        {
          "id": "MDRW2100009606.1.20",
          "form": "좋은 아침이에요",
          "original_form": "좋은 아침이에요",
          "speaker_id": "2",
          "time": "20210823 09:16"
        },
        {
          "id": "MDRW2100009606.1.21",
          "form": "선선합니다 역시!",
          "original_form": "선선합니다 역시!",
          "speaker_id": "2",
          "time": "20210823 09:16"
        },
        {
          "id": "MDRW2100009606.1.22",
          "form": "선선한데 비가오네요ㅎ해가  안떠서 늦잠자는바람에",
          "original_form": "선선한데 비가오네요ㅎ해가  안떠서 늦잠자는바람에",
          "speaker_id": "1",
          "time": "20210823 09:30"
        },
        {
          "id": "MDRW2100009606.1.23",
          "form": "반차쓸뻔",
          "original_form": "반차쓸뻔",
          "speaker_id": "1",
          "time": "20210823 09:30"
        },
        {
          "id": "MDRW2100009606.1.24",
          "form": "후덥지근한거같아요",
          "original_form": "후덥지근한거같아요",
