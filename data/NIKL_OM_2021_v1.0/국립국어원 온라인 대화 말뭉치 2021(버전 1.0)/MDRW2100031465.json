      "utterance": [
        {
          "id": "MDRW2100031465.1.1",
          "form": "믿을수가 없다.",
          "original_form": "믿을수가 없다.",
          "speaker_id": "1",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031465.1.2",
          "form": "민초는 사회악이다",
          "original_form": "민초는 사회악이다",
          "speaker_id": "2",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031465.1.3",
          "form": "옳소 민초는 그저 치약초코",
          "original_form": "옳소 민초는 그저 치약초코",
          "speaker_id": "1",
          "time": "20210904 21:47"
        },
        {
          "id": "MDRW2100031465.1.4",
          "form": "호랑이 커피는 먹어밧나",
          "original_form": "호랑이 커피는 먹어밧나",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031465.1.5",
          "form": "이름이 익숙한데",
          "original_form": "이름이 익숙한데",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031465.1.6",
          "form": "서울어디에서 유명한 커피점이라던데",
          "original_form": "서울어디에서 유명한 커피점이라던데",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031465.1.7",
          "form": "아 나 그거 내가",
          "original_form": "아 나 그거 내가",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031465.1.8",
          "form": "편의점에 한군데밖에못봤네..",
          "original_form": "편의점에 한군데밖에못봤네..",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100031465.1.9",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031465.1.10",
          "form": "먹진않앗구",
          "original_form": "먹진않앗구",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031465.1.11",
          "form": "누가 먹은거봣어ㅋㅋㅋㅋㅋ 그냥 커피맛이래",
          "original_form": "누가 먹은거봣어ㅋㅋㅋㅋㅋ 그냥 커피맛이래",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031465.1.12",
          "form": "아닌데 부드럽던데",
          "original_form": "아닌데 부드럽던데",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031465.1.13",
          "form": "그사람은 가게가서 먹엇던데",
          "original_form": "그사람은 가게가서 먹엇던데",
          "speaker_id": "1",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031465.1.14",
          "form": "아..가게는 안가봤다 ㅜㅜ",
          "original_form": "아..가게는 안가봤다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 21:49"
        },
        {
          "id": "MDRW2100031465.1.15",
          "form": "궁금쓰",
          "original_form": "궁금쓰",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100031465.1.16",
          "form": "호불호가 잇나부당",
          "original_form": "호불호가 잇나부당",
