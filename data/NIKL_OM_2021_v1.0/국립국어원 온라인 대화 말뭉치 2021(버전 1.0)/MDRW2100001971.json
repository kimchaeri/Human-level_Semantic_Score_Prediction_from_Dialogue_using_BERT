      "utterance": [
        {
          "id": "MDRW2100001971.1.1",
          "form": "어제 티비보니까",
          "original_form": "어제 티비보니까",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001971.1.2",
          "form": "경마 재밌어보이더랑!!",
          "original_form": "경마 재밌어보이더랑!!",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001971.1.3",
          "form": "경마 재밌지",
          "original_form": "경마 재밌지",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001971.1.4",
          "form": "한번 보러가자!",
          "original_form": "한번 보러가자!",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001971.1.5",
          "form": "코로나 때문에 지금 못보나 ㅠㅠ",
          "original_form": "코로나 때문에 지금 못보나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001971.1.6",
          "form": "그럴듯",
          "original_form": "그럴듯",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001971.1.7",
          "form": "무관중으로 하지 않을까",
          "original_form": "무관중으로 하지 않을까",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001971.1.8",
          "form": "아하.. 그렇겠네 ㅠ",
          "original_form": "아하.. 그렇겠네 ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001971.1.9",
          "form": "연예인들 응원하면서 보는거 재밌어보이던데",
          "original_form": "연예인들 응원하면서 보는거 재밌어보이던데",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001971.1.10",
          "form": "코로나 잠잠해지면 보러 가자",
          "original_form": "코로나 잠잠해지면 보러 가자",
          "speaker_id": "2",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001971.1.11",
          "form": "구래욤 ㅎㅎ 왠지",
          "original_form": "구래욤 ㅎㅎ 왠지",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001971.1.12",
          "form": "1등 말 잘 찝을 수 있을것 같다 ㅋㅋ",
          "original_form": "1등 말 잘 찝을 수 있을것 같다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001971.1.13",
          "form": "그래 ㅋㅋ",
