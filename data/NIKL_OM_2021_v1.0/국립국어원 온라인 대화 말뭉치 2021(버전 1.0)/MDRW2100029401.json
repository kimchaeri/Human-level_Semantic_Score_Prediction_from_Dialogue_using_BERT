      "utterance": [
        {
          "id": "MDRW2100029401.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 06:57"
        },
        {
          "id": "MDRW2100029401.1.2",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 07:08"
        },
        {
          "id": "MDRW2100029401.1.3",
          "form": "전 그다지 안 좋아해용",
          "original_form": "전 그다지 안 좋아해용",
          "speaker_id": "1",
          "time": "20210903 07:08"
        },
        {
          "id": "MDRW2100029401.1.4",
          "form": "움직이는게 싫어서ㅋㄱㅋ",
          "original_form": "움직이는게 싫어서ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 07:08"
        },
        {
          "id": "MDRW2100029401.1.5",
          "form": "그래도 말리부는 함 가보고 싶어요",
          "original_form": "그래도 말리부는 함 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210903 07:10"
        },
        {
          "id": "MDRW2100029401.1.6",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.7",
          "form": "집나가면 고생이라지만",
          "original_form": "집나가면 고생이라지만",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.8",
          "form": "좋은 곳은 가보고싶어요",
          "original_form": "좋은 곳은 가보고싶어요",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.9",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.10",
          "form": "꼭 가보고 싶은곳있으세요?",
          "original_form": "꼭 가보고 싶은곳있으세요?",
          "speaker_id": "1",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.11",
          "form": "말리부요ㅋㅋㅋㅋ",
          "original_form": "말리부요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.12",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100029401.1.13",
          "form": "저도 말리부",
          "original_form": "저도 말리부",
          "speaker_id": "1",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100029401.1.14",
          "form": "모리셔스도 궁금해요",
          "original_form": "모리셔스도 궁금해요",
          "speaker_id": "1",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100029401.1.15",
          "form": "모리셔스는 어떤곳이에요??",
          "original_form": "모리셔스는 어떤곳이에요??",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029401.1.16",
          "form": "피지섬도 그렇구 고래만나고 싶어요",
          "original_form": "피지섬도 그렇구 고래만나고 싶어요",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029401.1.17",
          "form": "아 남대서양에 있는데",
          "original_form": "아 남대서양에 있는데",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029401.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029401.1.19",
          "form": "해양관광하기 좋대요",
          "original_form": "해양관광하기 좋대요",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029401.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029401.1.21",
          "form": "섬인가봐요",
          "original_form": "섬인가봐요",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029401.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029401.1.23",
          "form": "신혼여행지로도",
          "original_form": "신혼여행지로도",
          "speaker_id": "1",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029401.1.24",
          "form": "많이 가더라고요",
          "original_form": "많이 가더라고요",
          "speaker_id": "1",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029401.1.25",
          "form": "그럼 무지 예쁘겠네요ㅋㅋ",
          "original_form": "그럼 무지 예쁘겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029401.1.26",
          "form": "거기도 찾아봐야겠어요",
          "original_form": "거기도 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029401.1.27",
          "form": "코시국이 끝나야 어딜 가도 갈텐데",
          "original_form": "코시국이 끝나야 어딜 가도 갈텐데",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029401.1.28",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
