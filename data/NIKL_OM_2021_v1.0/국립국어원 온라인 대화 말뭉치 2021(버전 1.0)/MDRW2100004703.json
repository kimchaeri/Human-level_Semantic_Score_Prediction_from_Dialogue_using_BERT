      "utterance": [
        {
          "id": "MDRW2100004703.1.1",
          "form": "최근에 사신 거 있으세요?",
          "original_form": "최근에 사신 거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004703.1.2",
          "form": "저는 어제 가을에 입을 연청바지 샀어요",
          "original_form": "저는 어제 가을에 입을 연청바지 샀어요",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004703.1.3",
          "form": "전 사고싶은건 있어요",
          "original_form": "전 사고싶은건 있어요",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004703.1.4",
          "form": "접이식 우산..",
          "original_form": "접이식 우산..",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004703.1.5",
          "form": "오 마음에 드는 거 있으세요??",
          "original_form": "오 마음에 드는 거 있으세요??",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004703.1.6",
          "form": "그냥 튼튼한걸로 사고싶어요. 접이식우산이 왜케 약한지 바람 불면 훼까닥해요ㅋㅋ쿠ㅜ",
          "original_form": "그냥 튼튼한걸로 사고싶어요. 접이식우산이 왜케 약한지 바람 불면 훼까닥해요ㅋㅋ쿠ㅜ",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004703.1.7",
          "form": "우산이랑 책도 사고싶엇는데 비싸서 도서관에 신청해놧어요",
          "original_form": "우산이랑 책도 사고싶엇는데 비싸서 도서관에 신청해놧어요",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004703.1.8",
          "form": "진짜 약하죠... 그냥 가방에 넣고 다니고 싶은데 금방 고장나니까 ㅠㅠ",
          "original_form": "진짜 약하죠... 그냥 가방에 넣고 다니고 싶은데 금방 고장나니까 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004703.1.9",
          "form": "비싼 책은 도서관! 완전 꿀팁이네요",
          "original_form": "비싼 책은 도서관! 완전 꿀팁이네요",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004703.1.10",
          "form": "책 사고 싶은 게 많은데... 이사할 때 골치라서 이북을 더 많이 보게되는 거 같아요 ㅠㅠ",
          "original_form": "책 사고 싶은 게 많은데... 이사할 때 골치라서 이북을 더 많이 보게되는 거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004703.1.11",
          "form": "그쵸ㅎㅎ 왠만하면 다 사주니까 이용하세욥!!",
          "original_form": "그쵸ㅎㅎ 왠만하면 다 사주니까 이용하세욥!!",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004703.1.12",
          "form": "저는 이북은 잘 안보게 되도라구요",
          "original_form": "저는 이북은 잘 안보게 되도라구요",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004703.1.13",
          "form": "이북 리더기 사셨나요",
          "original_form": "이북 리더기 사셨나요",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004703.1.14",
          "form": "아뇨 태블릿 있어서 그걸로 봐요!",
          "original_form": "아뇨 태블릿 있어서 그걸로 봐요!",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004703.1.15",
          "form": "저는 오히려 실물책 펼치기가 더 힘들더라구요 ㅠ",
          "original_form": "저는 오히려 실물책 펼치기가 더 힘들더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004703.1.16",
          "form": "오 저도 태블릿 사고싶너요",
          "original_form": "오 저도 태블릿 사고싶너요",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004703.1.17",
          "form": "작년에 태블릿 샀는데 저는 너무 만족스러워요!",
          "original_form": "작년에 태블릿 샀는데 저는 너무 만족스러워요!",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004703.1.18",
          "form": "갤럭시 짱!",
          "original_form": "갤럭시 짱!",
          "speaker_id": "2",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004703.1.19",
          "form": "오 다들 아이패드만 이야기하던데",
          "original_form": "오 다들 아이패드만 이야기하던데",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004703.1.20",
          "form": "갤럭시도 봐야겟엉ㅎ",
          "original_form": "갤럭시도 봐야겟엉ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004703.1.21",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004703.1.22",
          "form": "제가 폰도 갤럭시 이어폰도 버즈라서 맞추는게 더 좋더라구요!",
          "original_form": "제가 폰도 갤럭시 이어폰도 버즈라서 맞추는게 더 좋더라구요!",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004703.1.23",
          "form": "아 그렇겠네요!",
          "original_form": "아 그렇겠네요!",
          "speaker_id": "1",
          "time": "20210821 18:35"
        }
      ]
