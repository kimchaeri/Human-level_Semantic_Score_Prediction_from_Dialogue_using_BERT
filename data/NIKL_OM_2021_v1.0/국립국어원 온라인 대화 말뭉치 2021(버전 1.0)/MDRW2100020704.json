      "utterance": [
        {
          "id": "MDRW2100020704.1.1",
          "form": "넷플에 볼만한 거 없나요?",
          "original_form": "넷플에 볼만한 거 없나요?",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020704.1.2",
          "form": "넷플은 삼진영어토익반?",
          "original_form": "넷플은 삼진영어토익반?",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020704.1.3",
          "form": "그거 괜찮더라구요",
          "original_form": "그거 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020704.1.4",
          "form": "에밀리파리가자 이것도 가벼운 로코 좋아하시면 추천해요",
          "original_form": "에밀리파리가자 이것도 가벼운 로코 좋아하시면 추천해요",
          "speaker_id": "1",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020704.1.5",
          "form": "삼진영어토익반 그거 뜨자마자 봤어요 ㅎㅎ",
          "original_form": "삼진영어토익반 그거 뜨자마자 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020704.1.6",
          "form": "에밀리파리가다 그거 드라마죠?",
          "original_form": "에밀리파리가다 그거 드라마죠?",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020704.1.7",
          "form": "네네 드라마요!",
          "original_form": "네네 드라마요!",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020704.1.8",
          "form": "그거 그럼 찜해놔야겠어요",
          "original_form": "그거 그럼 찜해놔야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020704.1.9",
          "form": "요새 너무 볼게 없지 않나요 ?ㅠㅠ",
          "original_form": "요새 너무 볼게 없지 않나요 ?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020704.1.10",
          "form": "저는 요즘 넷플 안 보고 왓챠봐요 왓챠가 영화가 많더라구요",
          "original_form": "저는 요즘 넷플 안 보고 왓챠봐요 왓챠가 영화가 많더라구요",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020704.1.11",
          "form": "넷플은 오리지널 보고나면 볼게없는 느낌이에요",
          "original_form": "넷플은 오리지널 보고나면 볼게없는 느낌이에요",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020704.1.12",
          "form": "왓챠 최신영화 많아요?",
          "original_form": "왓챠 최신영화 많아요?",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020704.1.13",
          "form": "넷플 너무 옛날거밖에 없어서 다 봤던 것들..",
          "original_form": "넷플 너무 옛날거밖에 없어서 다 봤던 것들..",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100020704.1.14",
          "form": "저는 최신영화는 잘 모르겠고 ott 검색 어플 같은거 있더라구요 보시고 싶은거 검색해보시고 있으면 결제하셔도 될거같아요",
          "original_form": "저는 최신영화는 잘 모르겠고 ott 검색 어플 같은거 있더라구요 보시고 싶은거 검색해보시고 있으면 결제하셔도 될거같아요",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020704.1.15",
          "form": "왓챠는 처음이면 이주 무료체험도 있어요",
          "original_form": "왓챠는 처음이면 이주 무료체험도 있어요",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020704.1.16",
          "form": "와 요새 그런것도 있구나",
          "original_form": "와 요새 그런것도 있구나",
          "speaker_id": "2",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100020704.1.17",
          "form": "좀 있으면 디즈니플러스 들어온다하더라구요 그거 기다리고 있어요",
          "original_form": "좀 있으면 디즈니플러스 들어온다하더라구요 그거 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210828 11:39"
        },
        {
          "id": "MDRW2100020704.1.18",
          "form": "디즈니랑 마블 좋아하시면 디플이 좋겠네요! 저도 들어와서 반응 괜찮으면 생각해봐야겠어요",
          "original_form": "디즈니랑 마블 좋아하시면 디플이 좋겠네요! 저도 들어와서 반응 괜찮으면 생각해봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 11:40"
        },
        {
          "id": "MDRW2100020704.1.19",
          "form": "11월쯤에 들어온대서 저도 반응 보려구요",
          "original_form": "11월쯤에 들어온대서 저도 반응 보려구요",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020704.1.20",
          "form": "그래도 넷플에 신작 꾸준히 올려줘서 아직 볼만해요",
          "original_form": "그래도 넷플에 신작 꾸준히 올려줘서 아직 볼만해요",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020704.1.21",
          "form": "네네 저도 넷플은 신작 볼만한거 나옴 한달 결제해서 봐요",
          "original_form": "네네 저도 넷플은 신작 볼만한거 나옴 한달 결제해서 봐요",
          "speaker_id": "1",
          "time": "20210828 11:44"
        },
        {
          "id": "MDRW2100020704.1.22",
          "form": "아이디 혼자 써요?",
          "original_form": "아이디 혼자 써요?",
          "speaker_id": "2",
          "time": "20210828 11:45"
        },
        {
          "id": "MDRW2100020704.1.23",
          "form": "네! 한달도 구하시던데 인터넷으로",
          "original_form": "네! 한달도 구하시던데 인터넷으로",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020704.1.24",
          "form": "같이하는거 해요",
          "original_form": "같이하는거 해요",
          "speaker_id": "1",
          "time": "20210828 11:46"
        },
        {
          "id": "MDRW2100020704.1.25",
          "form": "아아 맞아요 그렇게 봐야 좀 덜아깝더라구요",
          "original_form": "아아 맞아요 그렇게 봐야 좀 덜아깝더라구요",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020704.1.26",
          "form": "전 가족끼리 아이디 공유해요 돈 나눠내고 ㅋㅋ",
          "original_form": "전 가족끼리 아이디 공유해요 돈 나눠내고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020704.1.27",
          "form": "네 혼자서 다 결제하기엔 부담 ㅠ",
          "original_form": "네 혼자서 다 결제하기엔 부담 ㅠ",
          "speaker_id": "1",
          "time": "20210828 11:47"
        },
        {
          "id": "MDRW2100020704.1.28",
          "form": "가족끼리면 너무 좋죠",
          "original_form": "가족끼리면 너무 좋죠",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020704.1.29",
          "form": "근데 거의 저만 봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 거의 저만 봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:48"
        }
      ]
