      "utterance": [
        {
          "id": "MDRW2100012902.1.1",
          "form": "언냐 우린 요새 계속 캠핑용품 샀음",
          "original_form": "언냐 우린 요새 계속 캠핑용품 샀음",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012902.1.2",
          "form": "텐트 작은걸로 바꾼다고",
          "original_form": "텐트 작은걸로 바꾼다고",
          "speaker_id": "2",
          "time": "20210825 07:52"
        },
        {
          "id": "MDRW2100012902.1.3",
          "form": "캠핑용품좋다",
          "original_form": "캠핑용품좋다",
          "speaker_id": "1",
          "time": "20210825 09:03"
        },
        {
          "id": "MDRW2100012902.1.4",
          "form": "타프사고, 타프옆에 가림막 사고",
          "original_form": "타프사고, 타프옆에 가림막 사고",
          "speaker_id": "2",
          "time": "20210825 09:07"
        },
        {
          "id": "MDRW2100012902.1.5",
          "form": "요새 뭐 산거없슈??",
          "original_form": "요새 뭐 산거없슈??",
          "speaker_id": "2",
          "time": "20210825 09:07"
        },
        {
          "id": "MDRW2100012902.1.6",
          "form": "쌀샀다",
          "original_form": "쌀샀다",
          "speaker_id": "1",
          "time": "20210825 09:09"
        },
        {
          "id": "MDRW2100012902.1.7",
          "form": "스텐드사고",
          "original_form": "스텐드사고",
          "speaker_id": "1",
          "time": "20210825 09:09"
        },
        {
          "id": "MDRW2100012902.1.8",
          "form": "책상사고ㅋㅋ",
          "original_form": "책상사고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:09"
        },
        {
          "id": "MDRW2100012902.1.9",
          "form": "둘째책상 사야되는데",
          "original_form": "둘째책상 사야되는데",
          "speaker_id": "1",
          "time": "20210825 09:09"
        },
        {
          "id": "MDRW2100012902.1.10",
          "form": "아 책상 ㅋㅋㅋㅋ",
          "original_form": "아 책상 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:10"
        },
        {
          "id": "MDRW2100012902.1.11",
          "form": "어떻다던데요??",
          "original_form": "어떻다던데요??",
          "speaker_id": "2",
          "time": "20210825 09:11"
        },
        {
          "id": "MDRW2100012902.1.12",
          "form": "둘째도 그거 사려고??",
          "original_form": "둘째도 그거 사려고??",
          "speaker_id": "2",
          "time": "20210825 09:11"
        },
        {
          "id": "MDRW2100012902.1.13",
          "form": "좋아함ㅋㅋ",
          "original_form": "좋아함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:17"
        },
        {
          "id": "MDRW2100012902.1.14",
          "form": "둘째도 사야지ㅋㅋ",
          "original_form": "둘째도 사야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:18"
        },
        {
          "id": "MDRW2100012902.1.15",
          "form": "똑같은걸로??",
          "original_form": "똑같은걸로??",
          "speaker_id": "2",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012902.1.16",
          "form": "대박 ㅋㅋㄱㄱㄱㅋ",
          "original_form": "대박 ㅋㅋㄱㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012902.1.17",
          "form": "웃긴다 정말 ㅋㅋㅋㅋ",
          "original_form": "웃긴다 정말 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012902.1.18",
          "form": "어 똑같은거ㅋㅋ",
          "original_form": "어 똑같은거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100012902.1.19",
          "form": "정말좋다ㅋㅋ",
          "original_form": "정말좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100012902.1.20",
          "form": "아 진짜요?? ㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜요?? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:24"
        },
        {
          "id": "MDRW2100012902.1.21",
          "form": "정말 인테리어는 1도 신경안쓰고 사네 ㅋㅋㅋ",
          "original_form": "정말 인테리어는 1도 신경안쓰고 사네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012902.1.22",
          "form": "어ㅋㅋ",
          "original_form": "어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012902.1.23",
          "form": "집은 실용적으로ㅋㅋ",
          "original_form": "집은 실용적으로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012902.1.24",
          "form": "진짜 실용 만 있음",
          "original_form": "진짜 실용 만 있음",
          "speaker_id": "2",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100012902.1.25",
          "form": "스탠드는 왜 샀어요??",
          "original_form": "스탠드는 왜 샀어요??",
          "speaker_id": "2",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100012902.1.26",
          "form": "몰라ㅋ 울신랑이샀다",
          "original_form": "몰라ㅋ 울신랑이샀다",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100012902.1.27",
          "form": "내쓰라고ㅋㅋ",
          "original_form": "내쓰라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100012902.1.28",
          "form": "뭔데 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "뭔데 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012902.1.29",
          "form": "언니 공부한다고??",
          "original_form": "언니 공부한다고??",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100012902.1.30",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100012902.1.31",
          "form": "니도공부해라ㅋ",
          "original_form": "니도공부해라ㅋ",
          "speaker_id": "1",
          "time": "20210825 13:25"
        }
      ]
    }
