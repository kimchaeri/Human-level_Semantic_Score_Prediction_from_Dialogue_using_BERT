      "utterance": [
        {
          "id": "MDRW2100010891.1.1",
          "form": "응 오늘 저녁이 참 맛나더라",
          "original_form": "응 오늘 저녁이 참 맛나더라",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010891.1.2",
          "form": "반찬으로 뭐먹은지 맞춰볼래?",
          "original_form": "반찬으로 뭐먹은지 맞춰볼래?",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010891.1.3",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010891.1.4",
          "form": "장조림은 없었겠고",
          "original_form": "장조림은 없었겠고",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010891.1.5",
          "form": "김치는 있었니..?",
          "original_form": "김치는 있었니..?",
          "speaker_id": "1",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010891.1.6",
          "form": "응 맞아 역시 똑똑하구나",
          "original_form": "응 맞아 역시 똑똑하구나",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010891.1.7",
          "form": "그냥 답을 알려줘.",
          "original_form": "그냥 답을 알려줘.",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010891.1.8",
          "form": "그리고 돈육오징어볶음이 나왔어",
          "original_form": "그리고 돈육오징어볶음이 나왔어",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010891.1.9",
          "form": "음. 전형적인 급식메뉴구나.",
          "original_form": "음. 전형적인 급식메뉴구나.",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.10",
          "form": "그리고...미역국도 먹었단다",
          "original_form": "그리고...미역국도 먹었단다",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.11",
          "form": "누구의 생일이었을까",
          "original_form": "누구의 생일이었을까",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.12",
          "form": "오늘 미역국 먹은 사람 왜이렇게 많지m",
          "original_form": "오늘 미역국 먹은 사람 왜이렇게 많지m",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ또 누가먹었대?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ또 누가먹었대?",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.14",
          "form": "내친구 네명도 각자 집이나 회사에서 미역국을 먹었대",
          "original_form": "내친구 네명도 각자 집이나 회사에서 미역국을 먹었대",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.15",
          "form": "오늘 무슨 날일까?",
          "original_form": "오늘 무슨 날일까?",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.16",
          "form": "세상에...정말 누구의 생일인가봐",
          "original_form": "세상에...정말 누구의 생일인가봐",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.17",
          "form": "친구 두명은 잡채도 먹었대",
          "original_form": "친구 두명은 잡채도 먹었대",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.18",
          "form": "신기해..생일이었나봐",
          "original_form": "신기해..생일이었나봐",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.19",
          "form": "완전 잔칫상이네",
          "original_form": "완전 잔칫상이네",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100010891.1.20",
          "form": "나도 잡채가 땡긴다 갑자기",
          "original_form": "나도 잡채가 땡긴다 갑자기",
          "speaker_id": "2",
          "time": "20210823 23:24"
        }
