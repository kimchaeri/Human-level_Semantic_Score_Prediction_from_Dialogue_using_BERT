      "utterance": [
        {
          "id": "MDRW2100020344.1.1",
          "form": "싸이월드혹시 하셨나요,,?",
          "original_form": "싸이월드혹시 하셨나요,,?",
          "speaker_id": "1",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100020344.1.2",
          "form": "넵! 싸이월드 세대입니다",
          "original_form": "넵! 싸이월드 세대입니다",
          "speaker_id": "2",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100020344.1.3",
          "form": "이미 부활했나요..?ㅎ",
          "original_form": "이미 부활했나요..?ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100020344.1.4",
          "form": "아뇨 한다고한다고",
          "original_form": "아뇨 한다고한다고",
          "speaker_id": "1",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100020344.1.5",
          "form": "말만하고 대기타게하고",
          "original_form": "말만하고 대기타게하고",
          "speaker_id": "1",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100020344.1.6",
          "form": "안여는 그놈들",
          "original_form": "안여는 그놈들",
          "speaker_id": "1",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100020344.1.7",
          "form": "비공개로오픈하겠다던데",
          "original_form": "비공개로오픈하겠다던데",
          "speaker_id": "1",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100020344.1.8",
          "form": "왜부활하지",
          "original_form": "왜부활하지",
          "speaker_id": "1",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100020344.1.9",
          "form": "비공개요? 혼자만 볼 수 있는건가요?",
          "original_form": "비공개요? 혼자만 볼 수 있는건가요?",
          "speaker_id": "2",
          "time": "20210828 01:45"
        },
        {
          "id": "MDRW2100020344.1.10",
          "form": "예전에 백업하라고 했을때 백업한걸로 기억하는데",
          "original_form": "예전에 백업하라고 했을때 백업한걸로 기억하는데",
          "speaker_id": "2",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100020344.1.11",
          "form": "정말 굳이네요...",
          "original_form": "정말 굳이네요...",
          "speaker_id": "2",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100020344.1.12",
          "form": "저는 귀찮다고 안했다가",
          "original_form": "저는 귀찮다고 안했다가",
          "speaker_id": "1",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100020344.1.13",
          "form": "진짜 그시절감성 그대로 남아있는거",
          "original_form": "진짜 그시절감성 그대로 남아있는거",
          "speaker_id": "1",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100020344.1.14",
          "form": "안보고싶어요,,",
          "original_form": "안보고싶어요,,",
          "speaker_id": "1",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100020344.1.15",
          "form": "게시물 보면 항마력 딸릴듯ㅠㅠ",
          "original_form": "게시물 보면 항마력 딸릴듯ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:47"
        },
        {
          "id": "MDRW2100020344.1.16",
          "form": "저막 고등학생~대학생초 이때라섴ㅋㅋㅋㅋ",
          "original_form": "저막 고등학생~대학생초 이때라섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100020344.1.17",
          "form": "뭐올렸는지 기억나서 항마력 못참지,,",
          "original_form": "뭐올렸는지 기억나서 항마력 못참지,,",
          "speaker_id": "1",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100020344.1.18",
          "form": "저그때 선덕여왕봐서",
          "original_form": "저그때 선덕여왕봐서",
          "speaker_id": "1",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100020344.1.19",
          "form": "비담 아 왜 좋아했지",
          "original_form": "비담 아 왜 좋아했지",
          "speaker_id": "1",
          "time": "20210828 01:48"
        },
        {
          "id": "MDRW2100020344.1.20",
          "form": "그시절 비담은 누구나ㅏ...",
          "original_form": "그시절 비담은 누구나ㅏ...",
          "speaker_id": "2",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100020344.1.21",
          "form": "저는 꽃남 한참 좋아해서 그걸로 올린거같은뎈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 꽃남 한참 좋아해서 그걸로 올린거같은뎈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020344.1.22",
          "form": "그거보다 친구들하고 찍은 과사가 더 문제죠^_^",
          "original_form": "그거보다 친구들하고 찍은 과사가 더 문제죠^_^",
          "speaker_id": "2",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020344.1.23",
          "form": "아 세상에 그시절 꽃남 못참죠",
          "original_form": "아 세상에 그시절 꽃남 못참죠",
          "speaker_id": "1",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020344.1.24",
          "form": "과사진짜",
          "original_form": "과사진짜",
          "speaker_id": "1",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020344.1.25",
          "form": "어떡해요,,",
          "original_form": "어떡해요,,",
          "speaker_id": "1",
          "time": "20210828 01:50"
        },
        {
          "id": "MDRW2100020344.1.26",
          "form": "대학 졸업하고 라식수술해서 더 못볼꼴일텐데ㅠㅠ",
          "original_form": "대학 졸업하고 라식수술해서 더 못볼꼴일텐데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020344.1.27",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020344.1.28",
          "form": "라식하셨구나 좋겠다",
          "original_form": "라식하셨구나 좋겠다",
          "speaker_id": "1",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020344.1.29",
          "form": "밝은눈으로 더 명확히 볼수있는 흑역사,,,",
          "original_form": "밝은눈으로 더 명확히 볼수있는 흑역사,,,",
          "speaker_id": "1",
          "time": "20210828 01:52"
        },
        {
          "id": "MDRW2100020344.1.30",
          "form": "삐젬도 하셨어요?",
          "original_form": "삐젬도 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 01:53"
        },
        {
          "id": "MDRW2100020344.1.31",
          "form": "프리스타일 와이 국룰",
          "original_form": "프리스타일 와이 국룰",
          "speaker_id": "1",
          "time": "20210828 01:53"
        },
        {
          "id": "MDRW2100020344.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 저 버즈좋아했어서 버즈 했을듯요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 저 버즈좋아했어서 버즈 했을듯요",
          "speaker_id": "2",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020344.1.33",
          "form": "나에게로,,떠나는,,,가시돋힌,,,겁쟁이,,",
          "original_form": "나에게로,,떠나는,,,가시돋힌,,,겁쟁이,,",
