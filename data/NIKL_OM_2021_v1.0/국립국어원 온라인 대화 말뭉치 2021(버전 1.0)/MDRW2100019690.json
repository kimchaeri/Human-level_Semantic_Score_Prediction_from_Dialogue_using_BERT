      "utterance": [
        {
          "id": "MDRW2100019690.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019690.1.2",
          "form": "아니요 가족이 반대해서 못키우고 있어요",
          "original_form": "아니요 가족이 반대해서 못키우고 있어요",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019690.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019690.1.4",
          "form": "헐 ㅠㅠ 왜 반대하실까요 ?",
          "original_form": "헐 ㅠㅠ 왜 반대하실까요 ?",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019690.1.5",
          "form": "저는 안키워요 ㅎㅎ ㅠㅠ",
          "original_form": "저는 안키워요 ㅎㅎ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019690.1.6",
          "form": "청소할 거리 늘어난대요 ㅋㅋㅋ",
          "original_form": "청소할 거리 늘어난대요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019690.1.7",
          "form": "아 ㅋㅋ 털때문에 그런가봐요 ..",
          "original_form": "아 ㅋㅋ 털때문에 그런가봐요 ..",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019690.1.8",
          "form": "네네 ㅋㅋㅋ 그리고 동물 특유의 냄새?도 싫다고 하시더라구요",
          "original_form": "네네 ㅋㅋㅋ 그리고 동물 특유의 냄새?도 싫다고 하시더라구요",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019690.1.9",
          "form": "안키우는 이유 있으세여?",
          "original_form": "안키우는 이유 있으세여?",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019690.1.10",
          "form": "저는 엄마가 털알러지가 있어요 ㅠ",
          "original_form": "저는 엄마가 털알러지가 있어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019690.1.11",
          "form": "헉 알러지가 ㅠㅠ",
          "original_form": "헉 알러지가 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019690.1.12",
          "form": "그럼 독립하고 키우고싶은 동물 있으신가요?",
          "original_form": "그럼 독립하고 키우고싶은 동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019690.1.13",
          "form": "네 저는 강아지요 ㅠㅠ 의지가 될꺼같아요 ㅎㅎ",
          "original_form": "네 저는 강아지요 ㅠㅠ 의지가 될꺼같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019690.1.14",
          "form": "아 맞아요 저도 큰 강아지 키우고 싶어요",
          "original_form": "아 맞아요 저도 큰 강아지 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019690.1.15",
          "form": "리트리버 같은 종이요",
          "original_form": "리트리버 같은 종이요",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019690.1.16",
          "form": "아 맞아요 ㅠㅠ 대형견인데 앵기는거 너무 좋아요 ㅋㅋ",
          "original_form": "아 맞아요 ㅠㅠ 대형견인데 앵기는거 너무 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019690.1.17",
          "form": "자기 덩치 모르구 ㅋㅋㅋ",
          "original_form": "자기 덩치 모르구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019690.1.18",
          "form": "그쵸 ㅋㅋㅋㅋ 그리고 덩치 커서 안을때 푹신한 것도 좋아요",
          "original_form": "그쵸 ㅋㅋㅋㅋ 그리고 덩치 커서 안을때 푹신한 것도 좋아요",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019690.1.19",
          "form": "맞아요 ㅋㅋ 그 외국처럼 큰 집에서 키우고싶어요 ㅠㅠ",
          "original_form": "맞아요 ㅋㅋ 그 외국처럼 큰 집에서 키우고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019690.1.20",
          "form": "맞아요 ㅋㅋㅋ 대형견은 마당있는 집이 필수인거같아요",
          "original_form": "맞아요 ㅋㅋㅋ 대형견은 마당있는 집이 필수인거같아요",
