      "utterance": [
        {
          "id": "MDRW2100013792.1.1",
          "form": "자취 해보신적 있으세요?",
          "original_form": "자취 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013792.1.2",
          "form": "네 대학교때 해봤어요",
          "original_form": "네 대학교때 해봤어요",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013792.1.3",
          "form": "자취하신적있으세요?",
          "original_form": "자취하신적있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013792.1.4",
          "form": "네 6개월정도 해봤어요",
          "original_form": "네 6개월정도 해봤어요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013792.1.5",
          "form": "전 자취하는동안 진짜 좋았는데",
          "original_form": "전 자취하는동안 진짜 좋았는데",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013792.1.6",
          "form": "자취 어떠셨어요?",
          "original_form": "자취 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013792.1.7",
          "form": "마자요 자취하면 좋죠",
          "original_form": "마자요 자취하면 좋죠",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013792.1.8",
          "form": "물론 안좋은점도 있겠지만",
          "original_form": "물론 안좋은점도 있겠지만",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013792.1.9",
          "form": "자유롭게 있을수있어서 좋은거같아요",
          "original_form": "자유롭게 있을수있어서 좋은거같아요",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013792.1.10",
          "form": "금전적으로만 해결되면",
          "original_form": "금전적으로만 해결되면",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013792.1.11",
          "form": "자취만큼 좋은게 없는것같아요",
          "original_form": "자취만큼 좋은게 없는것같아요",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013792.1.12",
          "form": "마자요!",
          "original_form": "마자요!",
          "speaker_id": "2",
          "time": "20210825 16:40"
        },
        {
          "id": "MDRW2100013792.1.13",
          "form": "근데 진짜 자취해보니까 돈나갈데가 이렇게 많구나 싶더라구요ㅠ",
          "original_form": "근데 진짜 자취해보니까 돈나갈데가 이렇게 많구나 싶더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013792.1.14",
          "form": "청소도 내가 모르는사이에 엄마가 맨날 했던거구나싶구요ㅠ",
          "original_form": "청소도 내가 모르는사이에 엄마가 맨날 했던거구나싶구요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013792.1.15",
          "form": "크흐흑",
          "original_form": "크흐흑",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013792.1.16",
          "form": "내가 머리카락이 이렇게 많이 나왔다니",
          "original_form": "내가 머리카락이 이렇게 많이 나왔다니",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013792.1.17",
          "form": "마자요ㅋㅋㅋ\n진짜 하루만에 엄청 쌓이더라구요\n먼지도 그렇구요",
          "original_form": "마자요ㅋㅋㅋ\n진짜 하루만에 엄청 쌓이더라구요\n먼지도 그렇구요",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013792.1.18",
          "form": "자취는 혼자 하셨었나요?",
          "original_form": "자취는 혼자 하셨었나요?",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013792.1.19",
          "form": "여자친구랑 같이살려고 투룸 구했는데",
          "original_form": "여자친구랑 같이살려고 투룸 구했는데",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013792.1.20",
          "form": "한달만에 헤어졌어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "한달만에 헤어졌어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013792.1.21",
          "form": "아하..그러셨군요..",
          "original_form": "아하..그러셨군요..",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013792.1.22",
          "form": "그래서 6개월만 사셨나봐요",
          "original_form": "그래서 6개월만 사셨나봐요",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013792.1.23",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013792.1.24",
          "form": "정확하시네요",
          "original_form": "정확하시네요",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013792.1.25",
          "form": "보통계약 일년은 할텐데 짧아서요ㅋㅋ\n그런 슬픈사연이 있으셨군요ㅠ",
          "original_form": "보통계약 일년은 할텐데 짧아서요ㅋㅋ\n그런 슬픈사연이 있으셨군요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013792.1.26",
          "form": "예리하십니다",
          "original_form": "예리하십니다",
          "speaker_id": "1",
          "time": "20210825 16:52"
        }
      ]
    }
  ]
