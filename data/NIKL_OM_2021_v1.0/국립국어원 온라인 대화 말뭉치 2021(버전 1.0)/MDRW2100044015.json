        },
        {
          "id": "MDRW2100044015.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044015.1.3",
          "form": "혹시 화장품이나 향수 좋아하시나요?",
          "original_form": "혹시 화장품이나 향수 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044015.1.4",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044015.1.5",
          "form": "만다린이 귤향나는거맞나요?",
          "original_form": "만다린이 귤향나는거맞나요?",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044015.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044015.1.7",
          "form": "네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044015.1.8",
          "form": "귤향이에요",
          "original_form": "귤향이에요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044015.1.9",
          "form": "와ㅋㅋㅋㅋ 혹시 아니려나 두근두근했어요",
          "original_form": "와ㅋㅋㅋㅋ 혹시 아니려나 두근두근했어요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044015.1.10",
          "form": "저 시트러스향 너무좋아해요",
          "original_form": "저 시트러스향 너무좋아해요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044015.1.11",
          "form": "록시땅 버베나두요",
          "original_form": "록시땅 버베나두요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044015.1.12",
          "form": "라임바질앤만다린...!!",
          "original_form": "라임바질앤만다린...!!",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044015.1.13",
          "form": "시트러스 너무 좋죠 ㅠㅠ",
          "original_form": "시트러스 너무 좋죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044015.1.14",
          "form": "비록 뿌리자마자 사라지지만 ㅠㅠ",
          "original_form": "비록 뿌리자마자 사라지지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044015.1.15",
          "form": "아 ㅜㅜㅜ너무좋아용",
          "original_form": "아 ㅜㅜㅜ너무좋아용",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044015.1.16",
          "form": "저는 저렴이지만 어디껀지 기억안나는데\r\n그린티 향수도 좋더라구요",
          "original_form": "저는 저렴이지만 어디껀지 기억안나는데\r\n그린티 향수도 좋더라구요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044015.1.17",
          "form": "식물향들은 진짜 다좋은거같아요",
          "original_form": "식물향들은 진짜 다좋은거같아요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044015.1.18",
          "form": "녹차향도 좋죠",
          "original_form": "녹차향도 좋죠",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044015.1.19",
          "form": "맞아요!! 마시는 녹차향은 그냥그런데",
          "original_form": "맞아요!! 마시는 녹차향은 그냥그런데",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044015.1.20",
          "form": "저도 파우더리한 향보다 식물쪽 향을좋아해요",
          "original_form": "저도 파우더리한 향보다 식물쪽 향을좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044015.1.21",
          "form": "향수나 바디미스트 녹차향은 너무좋아요ㅎㅎ",
          "original_form": "향수나 바디미스트 녹차향은 너무좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044015.1.22",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044015.1.23",
          "form": "아예 우디한것도 괜찮고 ㅋㅋㅋㅋㅋ",
          "original_form": "아예 우디한것도 괜찮고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044015.1.24",
          "form": "오 우디한거가 잘못맡으면",
          "original_form": "오 우디한거가 잘못맡으면",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044015.1.25",
          "form": "절냄새같다는 향인가요?",
          "original_form": "절냄새같다는 향인가요?",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044015.1.26",
          "form": "아니면 진짜 나무냄새 숲냄새인가요?",
          "original_form": "아니면 진짜 나무냄새 숲냄새인가요?",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044015.1.27",
          "form": "맞아요 ㅋㅋㅋㅋㅋ절냄새같기도 하다는 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ절냄새같기도 하다는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044015.1.28",
          "form": "앜ㅋㅋㅋ열심히 서치한 보람이 있네요",
          "original_form": "앜ㅋㅋㅋ열심히 서치한 보람이 있네요",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044015.1.29",
          "form": "요즘은 시향이어려워서ㅜㅜ",
          "original_form": "요즘은 시향이어려워서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044015.1.30",
          "form": "우디향은 아직 못샀는데 어디꺼가 유명해요?",
          "original_form": "우디향은 아직 못샀는데 어디꺼가 유명해요?",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044015.1.31",
          "form": "화장도 좋아하는데",
          "original_form": "화장도 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044015.1.32",
          "form": "코로나때문에 마스크끼고 살아서 ㅋㅋㅋㅋㅋㅋㅋ흥미를 잃어버린ㅋㅋㅋㅋㅋㅋ",
          "original_form": "코로나때문에 마스크끼고 살아서 ㅋㅋㅋㅋㅋㅋㅋ흥미를 잃어버린ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044015.1.33",
          "form": "아ㅜㅜㅜㅜ맞아요ㅜㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜㅜ맞아요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044015.1.34",
          "form": "마스크쓰니까 마음도 풀어져서 화장도 빡세게안하고 옷도 막입고 안꾸미게돼요ㅠㅠ",
          "original_form": "마스크쓰니까 마음도 풀어져서 화장도 빡세게안하고 옷도 막입고 안꾸미게돼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044015.1.35",
          "form": "저도 우디향쪽은 그렇게 잘 아는 편이 아니라서 ㅋㅋㅋㅋㅋ 개인적으로 좋앗던건 딥디크 탐다오예요",
          "original_form": "저도 우디향쪽은 그렇게 잘 아는 편이 아니라서 ㅋㅋㅋㅋㅋ 개인적으로 좋앗던건 딥디크 탐다오예요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044015.1.36",
          "form": "우와 탐다오!!이름들어봤어요",
          "original_form": "우와 탐다오!!이름들어봤어요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044015.1.37",
          "form": "화장에 흥미를 아예 잃어버렷어요 ㅋㅋㅋㅋㅋ",
          "original_form": "화장에 흥미를 아예 잃어버렷어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044015.1.38",
          "form": "장미향하고 무화과향도 시향했었는데",
          "original_form": "장미향하고 무화과향도 시향했었는데",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044015.1.39",
          "form": "무화과향도 좋더라구요",
          "original_form": "무화과향도 좋더라구요",
          "speaker_id": "1",
