      "utterance": [
        {
          "id": "MDRW2100029453.1.1",
          "form": "최근에 잘샀다고 생각하시는거있으세요?",
          "original_form": "최근에 잘샀다고 생각하시는거있으세요?",
          "speaker_id": "2",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100029453.1.2",
          "form": "음 저는 전기모기채요 ㅋㅋ",
          "original_form": "음 저는 전기모기채요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100029453.1.3",
          "form": "여름밤 요긴하게 썼어요",
          "original_form": "여름밤 요긴하게 썼어요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100029453.1.4",
          "form": "와 저는 지금 필요해요 그거",
          "original_form": "와 저는 지금 필요해요 그거",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100029453.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ모기 너무많아요요새",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ모기 너무많아요요새",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100029453.1.6",
          "form": "요새 모기많아요?",
          "original_form": "요새 모기많아요?",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.7",
          "form": "다이소에 파는 모기채도 효과좋대요",
          "original_form": "다이소에 파는 모기채도 효과좋대요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.8",
          "form": "ㅠㅠ 저는 모기는 잡는건 손으로도 잘잡는데",
          "original_form": "ㅠㅠ 저는 모기는 잡는건 손으로도 잘잡는데",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.9",
          "form": "오정말요",
          "original_form": "오정말요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.10",
          "form": "모기 수가 너무 많아서",
          "original_form": "모기 수가 너무 많아서",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.11",
          "form": "미칠거같아여",
          "original_form": "미칠거같아여",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.12",
          "form": "아하ㅠㅠ 그럼 플러그 모기약 사셔야겠다",
          "original_form": "아하ㅠㅠ 그럼 플러그 모기약 사셔야겠다",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100029453.1.13",
          "form": "유칼립투스 오일도 뿌려보셔요",
          "original_form": "유칼립투스 오일도 뿌려보셔요",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100029453.1.14",
          "form": "화장실 배수구 막으시구",
          "original_form": "화장실 배수구 막으시구",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100029453.1.15",
          "form": "오 모기 쫓아준대요??",
          "original_form": "오 모기 쫓아준대요??",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100029453.1.16",
          "form": "제방 창문 열면 자면 밤에 열마리씩 벽에 붙어있어요",
          "original_form": "제방 창문 열면 자면 밤에 열마리씩 벽에 붙어있어요",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100029453.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100029453.1.18",
          "form": "열고자면",
          "original_form": "열고자면",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100029453.1.19",
          "form": "으억 큰일이네유",
          "original_form": "으억 큰일이네유",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100029453.1.20",
          "form": "네네 쫓아준대요 계피도요",
          "original_form": "네네 쫓아준대요 계피도요",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100029453.1.21",
          "form": "오 집에 계피있는데 써봐야겠어요a",
          "original_form": "오 집에 계피있는데 써봐야겠어요a",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100029453.1.22",
          "form": "저는 최근에 포스터 샀는데 너무너무 이뻐서 맘에 드네여ㅎㅎ",
          "original_form": "저는 최근에 포스터 샀는데 너무너무 이뻐서 맘에 드네여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100029453.1.23",
          "form": "오호 벽에 붙이셨나요",
          "original_form": "오호 벽에 붙이셨나요",
          "speaker_id": "1",
          "time": "20210904 06:32"
        },
        {
          "id": "MDRW2100029453.1.24",
          "form": "원래붙어있던거 좀 정리하고 붙일려구요",
          "original_form": "원래붙어있던거 좀 정리하고 붙일려구요",
          "speaker_id": "2",
          "time": "20210904 09:53"
        },
        {
          "id": "MDRW2100029453.1.25",
          "form": "인테리어 재밌는거 같아요",
          "original_form": "인테리어 재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210904 09:54"
        },
        {
          "id": "MDRW2100029453.1.26",
          "form": "맞아요 저도 이번에 방 인테리어했어요",
          "original_form": "맞아요 저도 이번에 방 인테리어했어요",
          "speaker_id": "1",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100029453.1.27",
          "form": "오 어떻게요?? 페인트 칠하셨나여??",
          "original_form": "오 어떻게요?? 페인트 칠하셨나여??",
          "speaker_id": "2",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100029453.1.28",
          "form": "저는 이불 싹다 바꾸고 침구",
          "original_form": "저는 이불 싹다 바꾸고 침구",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100029453.1.29",
          "form": "조명사고",
          "original_form": "조명사고",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100029453.1.30",
          "form": "서랍 조립 했어요 이케아",
          "original_form": "서랍 조립 했어요 이케아",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100029453.1.31",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210904 10:08"
        },
        {
          "id": "MDRW2100029453.1.32",
          "form": "스웨덴에 신과함께가 있었다면",
          "original_form": "스웨덴에 신과함께가 있었다면",
          "speaker_id": "2",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100029453.1.33",
          "form": "지옥에 조립지옥이 있었을거라던",
          "original_form": "지옥에 조립지옥이 있었을거라던",
          "speaker_id": "2",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100029453.1.34",
          "form": "이키아...",
          "original_form": "이키아...",
          "speaker_id": "2",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100029453.1.35",
          "form": "저도 제방가구 침대빼고 이케아거든여",
          "original_form": "저도 제방가구 침대빼고 이케아거든여",
          "speaker_id": "2",
          "time": "20210904 10:09"
        },
        {
          "id": "MDRW2100029453.1.36",
          "form": "저도 제방에 침대빼고 전부 이케아거든요",
          "original_form": "저도 제방에 침대빼고 전부 이케아거든요",
