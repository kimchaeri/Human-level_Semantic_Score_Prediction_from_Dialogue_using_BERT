          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020040.1.9",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020040.1.10",
          "form": "전 사실 피자는 가리지않아요",
          "original_form": "전 사실 피자는 가리지않아요",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.11",
          "form": "빵이잖아요 다 맛있다구요",
          "original_form": "빵이잖아요 다 맛있다구요",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.13",
          "form": "정말 빵 사랑꾼이셧어요",
          "original_form": "정말 빵 사랑꾼이셧어요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.14",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.15",
          "form": "피자먹을 때",
          "original_form": "피자먹을 때",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.16",
          "form": "디핑소스",
          "original_form": "디핑소스",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020040.1.17",
          "form": "먹는다 안먹는다",
          "original_form": "먹는다 안먹는다",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
