          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020040.1.5",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020040.1.6",
          "form": "우린 더 가까워졋어요",
