      "utterance": [
        {
          "id": "MDRW2100040224.1.1",
          "form": "라떼는 산미죠",
          "original_form": "라떼는 산미죠",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040224.1.2",
          "form": "알았어요 저도 담부턴 라떼는 산미 명심할게요",
          "original_form": "알았어요 저도 담부턴 라떼는 산미 명심할게요",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100040224.1.3",
          "form": "명심또명심",
          "original_form": "명심또명심",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040224.1.4",
          "form": "파란보틀 가밧어요?",
          "original_form": "파란보틀 가밧어요?",
          "speaker_id": "2",
          "time": "20210904 14:23"
        },
        {
          "id": "MDRW2100040224.1.5",
          "form": "아뇨 근데 블루보틀 삼청동점은 예약안하면 못간대요",
          "original_form": "아뇨 근데 블루보틀 삼청동점은 예약안하면 못간대요",
          "speaker_id": "1",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100040224.1.6",
          "form": "헉 ...",
          "original_form": "헉 ...",
          "speaker_id": "2",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100040224.1.7",
          "form": "글케인기많나요",
          "original_form": "글케인기많나요",
          "speaker_id": "2",
          "time": "20210904 14:39"
        },
        {
          "id": "MDRW2100040224.1.8",
          "form": "그니까요 한옥이라 그런가",
          "original_form": "그니까요 한옥이라 그런가",
          "speaker_id": "1",
          "time": "20210904 14:40"
        },
        {
          "id": "MDRW2100040224.1.9",
          "form": "예약 해주세용 저대신",
          "original_form": "예약 해주세용 저대신",
          "speaker_id": "2",
          "time": "20210904 14:43"
        },
        {
          "id": "MDRW2100040224.1.10",
          "form": "요즘엔 음식점도 다 예약해야하더라구요..",
          "original_form": "요즘엔 음식점도 다 예약해야하더라구요..",
          "speaker_id": "1",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100040224.1.11",
          "form": "밥 한번 먹기 참 힘드네요",
          "original_form": "밥 한번 먹기 참 힘드네요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040224.1.12",
          "form": "먹기어렵네요",
          "original_form": "먹기어렵네요",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040224.1.13",
          "form": "사람은 없고 맛집은 많은 동네 없나요",
          "original_form": "사람은 없고 맛집은 많은 동네 없나요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040224.1.14",
          "form": "그런데가 잇을까요",
          "original_form": "그런데가 잇을까요",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040224.1.15",
          "form": "대전어때요",
          "original_form": "대전어때요",
          "speaker_id": "2",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040224.1.16",
          "form": "대전은 맛집 웨이팅 없나요",
          "original_form": "대전은 맛집 웨이팅 없나요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040224.1.17",
          "form": "잇긴해요",
          "original_form": "잇긴해요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040224.1.18",
          "form": "님 가시면 프리패스요",
          "original_form": "님 가시면 프리패스요",
          "speaker_id": "2",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040224.1.19",
          "form": "제가 부자도 아니고 프리패스라뇨",
          "original_form": "제가 부자도 아니고 프리패스라뇨",
          "speaker_id": "1",
          "time": "20210904 14:53"
        },
        {
          "id": "MDRW2100040224.1.20",
          "form": "착한분은 프리패스요",
          "original_form": "착한분은 프리패스요",
          "speaker_id": "2",
          "time": "20210904 14:54"
        }
      ]
