      "utterance": [
        {
          "id": "MDRW2100027404.1.1",
          "form": "근래 쇼핑한거있어?",
          "original_form": "근래 쇼핑한거있어?",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027404.1.2",
          "form": "오 나 요즘 쇼핑 대폭발이지 ㅋㅋ",
          "original_form": "오 나 요즘 쇼핑 대폭발이지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027404.1.3",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027404.1.4",
          "form": "샀어?",
          "original_form": "샀어?",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027404.1.5",
          "form": "굳지않는떡 샀구 앙꼬쑥절편이랑 꿀떡",
          "original_form": "굳지않는떡 샀구 앙꼬쑥절편이랑 꿀떡",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027404.1.6",
          "form": "1키로 2400원 1키로 3400원 해서 샀는데 떡 괜춘",
          "original_form": "1키로 2400원 1키로 3400원 해서 샀는데 떡 괜춘",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027404.1.7",
          "form": "떡집 떡처럼 완전고퀄은 아닌데 굳지않아서 말랑한게 장점!",
          "original_form": "떡집 떡처럼 완전고퀄은 아닌데 굳지않아서 말랑한게 장점!",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027404.1.8",
          "form": "떡 왔오?",
          "original_form": "떡 왔오?",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027404.1.9",
          "form": "가격 너무 좋다ㅋㅋ",
          "original_form": "가격 너무 좋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027404.1.10",
          "form": "말랑말랑",
          "original_form": "말랑말랑",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027404.1.11",
          "form": "떡 잘 안 먹는데 엄마 먹으라고 샀어",
          "original_form": "떡 잘 안 먹는데 엄마 먹으라고 샀어",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027404.1.12",
          "form": "떡은 잘 안먹는데",
          "original_form": "떡은 잘 안먹는데",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027404.1.13",
          "form": "엄마먹으라고샀오",
          "original_form": "엄마먹으라고샀오",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027404.1.14",
          "form": "아항 먹음 고소해서 좋던데",
          "original_form": "아항 먹음 고소해서 좋던데",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027404.1.15",
          "form": "떡은 천천히 음미?하듯이 먹는게 맛있더라",
          "original_form": "떡은 천천히 음미?하듯이 먹는게 맛있더라",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027404.1.16",
          "form": "근데 내가 먹고있어ㅜㅜ 뭐든 사두면 살찌는데 안살수가없넹",
          "original_form": "근데 내가 먹고있어ㅜㅜ 뭐든 사두면 살찌는데 안살수가없넹",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027404.1.17",
          "form": "맛난떡은 급히먹어도 맛나지만ㅋ",
          "original_form": "맛난떡은 급히먹어도 맛나지만ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027404.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027404.1.19",
          "form": "근데 나도 쟁이는? 스탈이라",
          "original_form": "근데 나도 쟁이는? 스탈이라",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027404.1.20",
          "form": "사둬야하는 성향ㅜㅠ",
          "original_form": "사둬야하는 성향ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027404.1.21",
          "form": "떡 씹을수록 단맛이 있지 탄수라 ㅋㅋ",
          "original_form": "떡 씹을수록 단맛이 있지 탄수라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027404.1.22",
          "form": "나도 생필품 먹을거  엄청 쟁인다",
          "original_form": "나도 생필품 먹을거  엄청 쟁인다",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027404.1.23",
          "form": "그래도 쌀때 사야지 싶어서",
          "original_form": "그래도 쌀때 사야지 싶어서",
          "speaker_id": "1",
          "time": "20210902 13:08"
        },
        {
          "id": "MDRW2100027404.1.24",
          "form": "그치ㅋㅋㅋ",
          "original_form": "그치ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027404.1.25",
          "form": "미니멀 못해ㅎ",
          "original_form": "미니멀 못해ㅎ",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027404.1.26",
          "form": "ㅇㅇ오늘도택배 여러게왔다",
          "original_form": "ㅇㅇ오늘도택배 여러게왔다",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027404.1.27",
          "form": "내 소소한 행복임 ㅋㅋ",
          "original_form": "내 소소한 행복임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027404.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027404.1.29",
          "form": "소소  아인거같은데ㅋ",
          "original_form": "소소  아인거같은데ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027404.1.30",
          "form": "택배는 기다렺고",
          "original_form": "택배는 기다렺고",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027404.1.31",
