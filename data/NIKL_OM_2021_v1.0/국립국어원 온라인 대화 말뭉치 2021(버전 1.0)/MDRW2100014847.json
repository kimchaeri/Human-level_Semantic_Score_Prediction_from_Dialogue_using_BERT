      "utterance": [
        {
          "id": "MDRW2100014847.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014847.1.2",
          "form": "좋아하는 맛집있으세요?",
          "original_form": "좋아하는 맛집있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014847.1.3",
          "form": "최근에 로우앤슬로우 다녀왔는데 맛있었어요",
          "original_form": "최근에 로우앤슬로우 다녀왔는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100014847.1.4",
          "form": "어떤 음식인가요?",
          "original_form": "어떤 음식인가요?",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100014847.1.5",
          "form": "브리스킷이라고 낮은온도에서 아주 오래 요리한 고기요리 랑 비프립 먹었어요",
          "original_form": "브리스킷이라고 낮은온도에서 아주 오래 요리한 고기요리 랑 비프립 먹었어요",
          "speaker_id": "2",
          "time": "20210825 22:07"
        },
        {
          "id": "MDRW2100014847.1.6",
          "form": "서울이랑 부산에 지점이 있어요",
          "original_form": "서울이랑 부산에 지점이 있어요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014847.1.7",
          "form": "와 말만들어도 군침이 싹도네요",
          "original_form": "와 말만들어도 군침이 싹도네요",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014847.1.8",
          "form": "한번쯤 드셔보는걸 추천드려요 색달라서 경험삼아 먹어볼만한거 같아요",
          "original_form": "한번쯤 드셔보는걸 추천드려요 색달라서 경험삼아 먹어볼만한거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014847.1.9",
          "form": "좋아하는 맛집 있으세요?",
          "original_form": "좋아하는 맛집 있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014847.1.10",
          "form": "저는 이대 주변에 까이식당이요",
          "original_form": "저는 이대 주변에 까이식당이요",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014847.1.11",
          "form": "뭐 파는 식당인가요?",
          "original_form": "뭐 파는 식당인가요?",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100014847.1.12",
          "form": "싱가포르식 닭고기 덮밥이에요",
          "original_form": "싱가포르식 닭고기 덮밥이에요",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100014847.1.13",
          "form": "독특한데 제 취향일거 같아요",
          "original_form": "독특한데 제 취향일거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100014847.1.14",
          "form": "추천감사해요",
          "original_form": "추천감사해요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100014847.1.15",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014847.1.16",
          "form": "네 삼격살먹었어요",
          "original_form": "네 삼격살먹었어요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
