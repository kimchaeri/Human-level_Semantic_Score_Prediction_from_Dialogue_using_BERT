      "utterance": [
        {
          "id": "MDRW2100016792.1.1",
          "form": "닭갈비와 낙지수제비",
          "original_form": "닭갈비와 낙지수제비",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016792.1.2",
          "form": "아니 왤케 메뉴 화려해",
          "original_form": "아니 왤케 메뉴 화려해",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016792.1.3",
          "form": "근데 수제비에 낙지가 2조각밖에...",
          "original_form": "근데 수제비에 낙지가 2조각밖에...",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016792.1.4",
          "form": "닭갈비랑 수제비 맛있었겠다",
          "original_form": "닭갈비랑 수제비 맛있었겠다",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016792.1.5",
          "form": "오늘 날씨에 잘어울리고",
          "original_form": "오늘 날씨에 잘어울리고",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016792.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016792.1.7",
          "form": "저녁은 뭐먹고 싶어?",
          "original_form": "저녁은 뭐먹고 싶어?",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016792.1.8",
          "form": "아니 뭐 먹어야해가 맞나",
          "original_form": "아니 뭐 먹어야해가 맞나",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016792.1.9",
          "form": "일단 아빠가 보쌈을 만들었고",
          "original_form": "일단 아빠가 보쌈을 만들었고",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.10",
          "form": "보쌈?!!!!!!",
          "original_form": "보쌈?!!!!!!",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.11",
          "form": "점심에 남은 부속을 털어서 순대국도 끓여뒀고",
          "original_form": "점심에 남은 부속을 털어서 순대국도 끓여뒀고",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.12",
          "form": "(오늘 점심으로 먹음)",
          "original_form": "(오늘 점심으로 먹음)",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.13",
          "form": "대단한 메뉴 라인업인데?",
          "original_form": "대단한 메뉴 라인업인데?",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.14",
          "form": "오늘 잔치날인가ㅋㅋ",
          "original_form": "오늘 잔치날인가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.15",
          "form": "그리고 삼치도 도착했지",
          "original_form": "그리고 삼치도 도착했지",
          "speaker_id": "1",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100016792.1.16",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016792.1.17",
          "form": "냉동실에 넣긴 했지만",
          "original_form": "냉동실에 넣긴 했지만",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016792.1.18",
          "form": "음식 파티네 파티",
          "original_form": "음식 파티네 파티",
          "speaker_id": "2",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016792.1.19",
