      "utterance": [
        {
          "id": "MDRW2100031098.1.1",
          "form": "님 회사가 망하면",
          "original_form": "님 회사가 망하면",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031098.1.2",
          "form": "저는 이미 공사장서 일용직  뛰고 있을 걸요",
          "original_form": "저는 이미 공사장서 일용직  뛰고 있을 걸요",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031098.1.3",
          "form": "ㅋㅋ자꾸 서울시가 갑질합니다",
          "original_form": "ㅋㅋ자꾸 서울시가 갑질합니다",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031098.1.4",
          "form": "인원 줄이라고ㅋㅋㅋ",
          "original_form": "인원 줄이라고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031098.1.5",
          "form": "민노총들 짜르고 청년채용 합시다!!!",
          "original_form": "민노총들 짜르고 청년채용 합시다!!!",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031098.1.6",
          "form": "(머리띠를 두른다)",
          "original_form": "(머리띠를 두른다)",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031098.1.7",
          "form": "솔직히 하기는 개쉬울거 같은데",
          "original_form": "솔직히 하기는 개쉬울거 같은데",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031098.1.8",
          "form": "대충 빨간 머리띠 두르고",
          "original_form": "대충 빨간 머리띠 두르고",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031098.1.9",
          "form": "투쟁 하면 되는거 아닌가",
          "original_form": "투쟁 하면 되는거 아닌가",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031098.1.10",
          "form": "ㅎㅎ 선봉에 서세요!",
          "original_form": "ㅎㅎ 선봉에 서세요!",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031098.1.11",
          "form": "코시국이니 비대면으로 동참하겠슴돠",
          "original_form": "코시국이니 비대면으로 동참하겠슴돠",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031098.1.12",
          "form": "저는 민노총 아닙니다",
          "original_form": "저는 민노총 아닙니다",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031098.1.13",
          "form": "엇 철도노조 그쪽은 민노총 아녔어요?",
          "original_form": "엇 철도노조 그쪽은 민노총 아녔어요?",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031098.1.14",
          "form": "ㅋㅋ그건 철도노조죠",
          "original_form": "ㅋㅋ그건 철도노조죠",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031098.1.15",
          "form": "저는 다른쪽입니다ㅋㅋ",
          "original_form": "저는 다른쪽입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031098.1.16",
          "form": "제가 그쪽을 전혀 모르니ㅎㅎ",
          "original_form": "제가 그쪽을 전혀 모르니ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031098.1.17",
          "form": "근데 이 시국에 서울시도 참 여러모로 대단하네요ㅎㅎㅎ",
          "original_form": "근데 이 시국에 서울시도 참 여러모로 대단하네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031098.1.18",
          "form": "애초에 적자 원인이 직원 월급이 아닌것을....",
          "original_form": "애초에 적자 원인이 직원 월급이 아닌것을....",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031098.1.19",
          "form": "답답합니다 ㅋㅋㅋㅋ",
          "original_form": "답답합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031098.1.20",
          "form": "뭐 어케든 되겠지 뭐~~",
          "original_form": "뭐 어케든 되겠지 뭐~~",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031098.1.21",
          "form": "해탈해 득도하셨군요ㅜㅜㅜ",
          "original_form": "해탈해 득도하셨군요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100031098.1.22",
          "form": "니나노입니다~~",
          "original_form": "니나노입니다~~",
          "speaker_id": "2",
          "time": "20210904 21:37"
        },
        {
          "id": "MDRW2100031098.1.23",
          "form": "똑똑똑 역장님",
          "original_form": "똑똑똑 역장님",
          "speaker_id": "2",
          "time": "20210905 10:04"
        },
        {
          "id": "MDRW2100031098.1.24",
          "form": "저는 역장이 아녜요ㅜ",
          "original_form": "저는 역장이 아녜요ㅜ",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031098.1.25",
          "form": "일개 사노비일뿐",
          "original_form": "일개 사노비일뿐",
          "speaker_id": "1",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100031098.1.26",
          "form": "라이언 못샀죠?",
          "original_form": "라이언 못샀죠?",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031098.1.27",
          "form": "다 팔렸을거 같음",
          "original_form": "다 팔렸을거 같음",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031098.1.28",
          "form": "염장 지르시나욧!",
          "original_form": "염장 지르시나욧!",
          "speaker_id": "1",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100031098.1.29",
          "form": "라이언 팝니다",
          "original_form": "라이언 팝니다",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031098.1.30",
          "form": "역장님 데려갈 찬스",
          "original_form": "역장님 데려갈 찬스",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031098.1.31",
          "form": "ㅎ... 제 것은 스스로 쟁취하는 새나라의 어른이로서",
          "original_form": "ㅎ... 제 것은 스스로 쟁취하는 새나라의 어른이로서",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031098.1.32",
          "form": "아직 포기 안 했어요",
          "original_form": "아직 포기 안 했어요",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031098.1.33",
