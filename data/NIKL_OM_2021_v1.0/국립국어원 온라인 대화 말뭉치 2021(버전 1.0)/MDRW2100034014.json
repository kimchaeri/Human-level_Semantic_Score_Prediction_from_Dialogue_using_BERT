      "utterance": [
        {
          "id": "MDRW2100034014.1.1",
          "form": "동물 키으세요?",
          "original_form": "동물 키으세요?",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100034014.1.2",
          "form": "아니요 name2님은요?",
          "original_form": "아니요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100034014.1.3",
          "form": "저도요 ㅎ",
          "original_form": "저도요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034014.1.4",
          "form": "좋아하는 동물은",
          "original_form": "좋아하는 동물은",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034014.1.5",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100034014.1.6",
          "form": "동물은 거의 다 좋아해요",
          "original_form": "동물은 거의 다 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100034014.1.7",
          "form": "만약 키우신다면 어떤 동물을 키우고싶으세요?",
          "original_form": "만약 키우신다면 어떤 동물을 키우고싶으세요?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100034014.1.8",
          "form": "저 완전 고양이요",
          "original_form": "저 완전 고양이요",
          "speaker_id": "2",
          "time": "20210826 08:25"
        },
        {
          "id": "MDRW2100034014.1.9",
          "form": "고양이 좋아하시나요",
          "original_form": "고양이 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 08:25"
        },
        {
          "id": "MDRW2100034014.1.10",
          "form": "저두 고양이요",
          "original_form": "저두 고양이요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100034014.1.11",
          "form": "고양이 좋죠ㅠㅠ",
          "original_form": "고양이 좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100034014.1.12",
          "form": "고양이 진짜 너뮤너무 귀여워요",
          "original_form": "고양이 진짜 너뮤너무 귀여워요",
          "speaker_id": "2",
          "time": "20210826 11:28"
        },
        {
          "id": "MDRW2100034014.1.13",
          "form": "잘놀다가도 혼자 쉬러가는거 너무 웃기고귀엽죠ㅠㅠ",
          "original_form": "잘놀다가도 혼자 쉬러가는거 너무 웃기고귀엽죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100034014.1.14",
          "form": "ㅋㅋㅋㅋㅋ 네 너무 귀엽고 흑",
          "original_form": "ㅋㅋㅋㅋㅋ 네 너무 귀엽고 흑",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100034014.1.15",
          "form": "친구집 고양이를 보는게 고작이지만",
          "original_form": "친구집 고양이를 보는게 고작이지만",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100034014.1.16",
          "form": "그냥 옆에 지나가기만 해도 좋아요 ㅠㅠ",
          "original_form": "그냥 옆에 지나가기만 해도 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100034014.1.17",
          "form": "헉 ㅠㅠㅠㅠ 부러워요",
          "original_form": "헉 ㅠㅠㅠㅠ 부러워요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100034014.1.18",
          "form": "손님맞이 냥 아시나요",
          "original_form": "손님맞이 냥 아시나요",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100034014.1.19",
          "form": "그래서 되게 잘 와줘서 좋아요ㅠㅠ",
          "original_form": "그래서 되게 잘 와줘서 좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100034014.1.20",
          "form": "완전 개냥이군요 ㅠㅠㅠㅠ",
          "original_form": "완전 개냥이군요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:25"
        }
