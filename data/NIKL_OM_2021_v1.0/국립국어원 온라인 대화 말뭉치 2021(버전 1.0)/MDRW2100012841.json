      "utterance": [
        {
          "id": "MDRW2100012841.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210825 02:26"
        },
        {
          "id": "MDRW2100012841.1.2",
          "form": "저는 무교요",
          "original_form": "저는 무교요",
          "speaker_id": "2",
          "time": "20210825 02:27"
        },
        {
          "id": "MDRW2100012841.1.3",
          "form": "저도 무교예요",
          "original_form": "저도 무교예요",
          "speaker_id": "1",
          "time": "20210825 02:27"
        },
        {
          "id": "MDRW2100012841.1.4",
          "form": "무교인 이유 있으신가요?",
          "original_form": "무교인 이유 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 02:27"
        },
        {
          "id": "MDRW2100012841.1.5",
          "form": "그냥 안믿어요 현실적인 편",
          "original_form": "그냥 안믿어요 현실적인 편",
          "speaker_id": "2",
          "time": "20210825 02:28"
        },
        {
          "id": "MDRW2100012841.1.6",
          "form": "이유가 같네요 제 앞가림 잘하고 자신을 믿는 편이에요",
          "original_form": "이유가 같네요 제 앞가림 잘하고 자신을 믿는 편이에요",
          "speaker_id": "1",
          "time": "20210825 02:28"
        },
        {
          "id": "MDRW2100012841.1.7",
          "form": "절이나 교회 가보신적있나요?",
          "original_form": "절이나 교회 가보신적있나요?",
          "speaker_id": "1",
          "time": "20210825 02:29"
        },
        {
          "id": "MDRW2100012841.1.8",
          "form": "어릴 때 교회 가봤어요",
          "original_form": "어릴 때 교회 가봤어요",
          "speaker_id": "2",
          "time": "20210825 02:30"
        },
        {
          "id": "MDRW2100012841.1.9",
          "form": "저는 초딩때 문상준다고해서 가봤어요",
          "original_form": "저는 초딩때 문상준다고해서 가봤어요",
          "speaker_id": "1",
          "time": "20210825 02:30"
        },
        {
          "id": "MDRW2100012841.1.10",
          "form": "저랑 똑같네요 ㅋㅋㅋ",
          "original_form": "저랑 똑같네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 02:31"
        },
        {
          "id": "MDRW2100012841.1.11",
          "form": "근데 낯가려서 한번 가고 말았어요",
          "original_form": "근데 낯가려서 한번 가고 말았어요",
          "speaker_id": "2",
          "time": "20210825 02:32"
        },
        {
          "id": "MDRW2100012841.1.12",
          "form": "절은 봉은사 가봤어요",
          "original_form": "절은 봉은사 가봤어요",
          "speaker_id": "1",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100012841.1.13",
          "form": "절 가면 뭐 해요?",
          "original_form": "절 가면 뭐 해요?",
          "speaker_id": "2",
          "time": "20210825 02:33"
        },
        {
          "id": "MDRW2100012841.1.14",
          "form": "구경하고 스님이 종치는거 봤어요",
          "original_form": "구경하고 스님이 종치는거 봤어요",
          "speaker_id": "1",
          "time": "20210825 02:35"
        },
        {
          "id": "MDRW2100012841.1.15",
          "form": "종교적인 활동은 안하나요?",
          "original_form": "종교적인 활동은 안하나요?",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012841.1.16",
          "form": "못봤어요",
          "original_form": "못봤어요",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012841.1.17",
          "form": "그냥 우연히 가봤어요 유명하다해서",
          "original_form": "그냥 우연히 가봤어요 유명하다해서",
          "speaker_id": "1",
