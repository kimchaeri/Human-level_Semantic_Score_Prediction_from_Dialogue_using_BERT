      "utterance": [
        {
          "id": "MDRW2100034635.1.1",
          "form": "저도 탕수육 좋아해요",
          "original_form": "저도 탕수육 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034635.1.2",
          "form": "중식도 빼놓을 수 없죠",
          "original_form": "중식도 빼놓을 수 없죠",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034635.1.3",
          "form": "탕수육도 맛집이 있어서 진짜맛난곳이 따로있더라구요~",
          "original_form": "탕수육도 맛집이 있어서 진짜맛난곳이 따로있더라구요~",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034635.1.4",
          "form": "부먹파세요 찍먹파세요?ㅋㅋ",
          "original_form": "부먹파세요 찍먹파세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034635.1.5",
          "form": "찍먹파예요",
          "original_form": "찍먹파예요",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034635.1.6",
          "form": "저랑 똑같으시네요 ㅋㅋ",
          "original_form": "저랑 똑같으시네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034635.1.7",
          "form": "그런데 중식당에서 아예 소스째 볶아나오는 탕수육도 엄청 좋아해요",
          "original_form": "그런데 중식당에서 아예 소스째 볶아나오는 탕수육도 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034635.1.8",
          "form": "저도 탕수육은 찍먹이 좋아요ㅋㅋ",
          "original_form": "저도 탕수육은 찍먹이 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034635.1.9",
          "form": "오~  저도요 ㅋㅋㅋㅋ  그건 또 그렇게 먹어줘야해요!ㅋㅋㅋ",
          "original_form": "오~  저도요 ㅋㅋㅋㅋ  그건 또 그렇게 먹어줘야해요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034635.1.10",
          "form": "맞아요ㅋㅋㅋㅋ 그건 그거대로 너무 맛잇어요",
          "original_form": "맞아요ㅋㅋㅋㅋ 그건 그거대로 너무 맛잇어요",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034635.1.11",
          "form": "자꾸 이야기하니까 또 먹고싶어지네요 ㅋㅋ",
          "original_form": "자꾸 이야기하니까 또 먹고싶어지네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034635.1.12",
          "form": "저녁메뉴로..슬쩍 넣고싶네요 ㅋㅋㅋ",
          "original_form": "저녁메뉴로..슬쩍 넣고싶네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034635.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ슬쩍 추가 고고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ슬쩍 추가 고고",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100034635.1.14",
          "form": "아...저 바본가봐요ㅠㅠ\r\n 두부집 가기로 해놓고 ㅋㄱㅋ... 내일저녁으로...ㅠㅠ",
          "original_form": "아...저 바본가봐요ㅠㅠ\r\n 두부집 가기로 해놓고 ㅋㄱㅋ... 내일저녁으로...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100034635.1.15",
          "form": "직접 두부 만드시고 순두부 콩국수 다 맛나다고해서  가보려고용ㅎㅎ",
          "original_form": "직접 두부 만드시고 순두부 콩국수 다 맛나다고해서  가보려고용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100034635.1.16",
          "form": "와 맛잇겟다",
          "original_form": "와 맛잇겟다",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100034635.1.17",
          "form": "두부맛집 의외로 별로 없는데 ㅠㅠ",
          "original_form": "두부맛집 의외로 별로 없는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100034635.1.18",
          "form": "귀한 맛집이네요",
          "original_form": "귀한 맛집이네요",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100034635.1.19",
          "form": "저도 아직안가봐서 맛있어요!는 모르겠네요ㅋㅋ",
          "original_form": "저도 아직안가봐서 맛있어요!는 모르겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100034635.1.20",
          "form": "맛잇길 바랄게요 ㅋㅋㅋ",
          "original_form": "맛잇길 바랄게요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100034635.1.21",
          "form": "네 ㅋㅋ감사해요ㅎㅎ 내일아침도 고민해보고 재료들을 장봐와야겠어요ㅠㅠㅋ  먹고돌아서면 먹을시간이네요",
          "original_form": "네 ㅋㅋ감사해요ㅎㅎ 내일아침도 고민해보고 재료들을 장봐와야겠어요ㅠㅠㅋ  먹고돌아서면 먹을시간이네요",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034635.1.22",
          "form": "메뉴 고르는것도 너무 큰 일이에요 ㅋㅋㅋ",
          "original_form": "메뉴 고르는것도 너무 큰 일이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034635.1.23",
          "form": "아침엔 뭘먹어야할까요?!ㅋㅋㄱ",
          "original_form": "아침엔 뭘먹어야할까요?!ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034635.1.24",
          "form": "저녁에 두부집에가서 콩물을 사서 올까요 ㅋㅋㄱ\r\n콩국수를 또먹게용..ㅍㅎㅎ",
          "original_form": "저녁에 두부집에가서 콩물을 사서 올까요 ㅋㅋㄱ\r\n콩국수를 또먹게용..ㅍㅎㅎ",
