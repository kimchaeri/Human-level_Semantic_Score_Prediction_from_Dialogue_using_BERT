      "utterance": [
        {
          "id": "MDRW2100018317.1.1",
          "form": "name2은 최근에 산 물건 있으세요?",
          "original_form": "&name2&은 최근에 산 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018317.1.2",
          "form": "옷 샀어요!! 기본옷이 없어가지구ㅋㅋㅋ",
          "original_form": "옷 샀어요!! 기본옷이 없어가지구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018317.1.3",
          "form": "저도 며칠 전에 옷 샀어요!",
          "original_form": "저도 며칠 전에 옷 샀어요!",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018317.1.4",
          "form": "재킷이랑 트레이닝 셋업 샀어요",
          "original_form": "재킷이랑 트레이닝 셋업 샀어요",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018317.1.5",
          "form": "트레이닝복 편하죵ㅋㅋㅋ 전 니트랑 긴팔티 샀어요ㅋㅋㅋ",
          "original_form": "트레이닝복 편하죵ㅋㅋㅋ 전 니트랑 긴팔티 샀어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018317.1.6",
          "form": "오 니트 좋아하세요?",
          "original_form": "오 니트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100018317.1.7",
          "form": "아니요ㅋㅋㅋ 니트는 몇개없어요ㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ 니트는 몇개없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100018317.1.8",
          "form": "그래서 이번에 새로 장만했어용ㅋㅋ",
          "original_form": "그래서 이번에 새로 장만했어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100018317.1.9",
          "form": "어떤 색 사셨어요?",
          "original_form": "어떤 색 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100018317.1.10",
          "form": "저도 니트 되게 좋아해요",
          "original_form": "저도 니트 되게 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018317.1.11",
          "form": "아정말요?ㅎㅎ 레드 사려다가 아이보리로 샀어요!!",
          "original_form": "아정말요?ㅎㅎ 레드 사려다가 아이보리로 샀어요!!",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100018317.1.12",
          "form": "아이보리가 무난해서 자주 입긴 하죠",
          "original_form": "아이보리가 무난해서 자주 입긴 하죠",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100018317.1.13",
          "form": "전 아이보리 계열 옷이 많아서 빨간색 옷이 너무 사고 싶어요ㅋㅋ",
          "original_form": "전 아이보리 계열 옷이 많아서 빨간색 옷이 너무 사고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100018317.1.14",
          "form": "빨간색 너무 이쁘지 않나요??ㅋㅋㅋ",
          "original_form": "빨간색 너무 이쁘지 않나요??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100018317.1.15",
          "form": "저는 흰색이나 검정색같은 기본색이 없고 다 알록달록한거밖에 없어서 요즘은 기본을 사고 있어요ㅋㅋㅋ",
          "original_form": "저는 흰색이나 검정색같은 기본색이 없고 다 알록달록한거밖에 없어서 요즘은 기본을 사고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100018317.1.16",
          "form": "저랑 딱 반대네요ㅋㅋㅋ",
          "original_form": "저랑 딱 반대네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100018317.1.17",
          "form": "전 무채색만 가득해요 원래는 빨간색을 제일 좋아하는데ㅋㅋ",
          "original_form": "전 무채색만 가득해요 원래는 빨간색을 제일 좋아하는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100018317.1.18",
          "form": "그래도 빨간색 제일 좋아하는거는 똑같네요ㅋㅋㅋ",
          "original_form": "그래도 빨간색 제일 좋아하는거는 똑같네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100018317.1.19",
          "form": "근데 나이드니까 점점 무채색을 사게 돼요ㅋㅋㅋ",
          "original_form": "근데 나이드니까 점점 무채색을 사게 돼요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100018317.1.20",
          "form": "그런가요ㅋㅋㅋ무채색 옷을 오래입게 되긴 하더라고요ㅋㅋ",
          "original_form": "그런가요ㅋㅋㅋ무채색 옷을 오래입게 되긴 하더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100018317.1.21",
          "form": "코디하기도 쉽고 편한거같아요ㅋㅋㅋ",
