      "utterance": [
        {
          "id": "MDRW2100035645.1.1",
          "form": "그래도 부모님이 재밌으셨ㄷㅏ니 다행이에요",
          "original_form": "그래도 부모님이 재밌으셨ㄷㅏ니 다행이에요",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035645.1.2",
          "form": "제주도 수학여행때..?",
          "original_form": "제주도 수학여행때..?",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035645.1.3",
          "form": "가고싶어요ㅠ",
          "original_form": "가고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035645.1.4",
          "form": "아하 제주도 가보고싶은곳이나",
          "original_form": "아하 제주도 가보고싶은곳이나",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.5",
          "form": "넘 좋겠죠..?ㅜ",
          "original_form": "넘 좋겠죠..?ㅜ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.6",
          "form": "드셔보시고 싶은 음식 있으세용?",
          "original_form": "드셔보시고 싶은 음식 있으세용?",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.7",
          "form": "저 산도 타보고싶고",
          "original_form": "저 산도 타보고싶고",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.8",
          "form": "고기국수 먹고싶어요",
          "original_form": "고기국수 먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.9",
          "form": "저는 사촌이 제주도에 살아서 어릴때부터 많이 갔어서",
          "original_form": "저는 사촌이 제주도에 살아서 어릴때부터 많이 갔어서",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.10",
          "form": "후기 들려드릴수있어용ㅎㅎㅎ",
          "original_form": "후기 들려드릴수있어용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.11",
          "form": "아아 고기국수!",
          "original_form": "아아 고기국수!",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.12",
          "form": "아하 좋으시겠어요",
          "original_form": "아하 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.13",
          "form": "고기국수 블로그나 관광지 근처에 있는 고기국수집 가지마시고",
          "original_form": "고기국수 블로그나 관광지 근처에 있는 고기국수집 가지마시고",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035645.1.14",
          "form": "그리고 흑돼지고기요",
          "original_form": "그리고 흑돼지고기요",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.15",
          "form": "제주도민들이 사는 아파트 단지 그런곳에있는 시장에서 드세요",
          "original_form": "제주도민들이 사는 아파트 단지 그런곳에있는 시장에서 드세요",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.16",
          "form": "아핯꿀팁 감사합니다",
          "original_form": "아핯꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.17",
          "form": "흑돼지고기",
          "original_form": "흑돼지고기",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.18",
          "form": "넘 맛있죠ㅠㅠ",
          "original_form": "넘 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.19",
          "form": "두껍게 잘라서 소금쳐서 드시면 꿀맛이에요",
          "original_form": "두껍게 잘라서 소금쳐서 드시면 꿀맛이에요",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.20",
          "form": "어우 넘 맛있겠어요",
          "original_form": "어우 넘 맛있겠어요",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035645.1.21",
          "form": "2년전쯤 3월에 갔을때는 중국 관광객분들이",
          "original_form": "2년전쯤 3월에 갔을때는 중국 관광객분들이",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035645.1.22",
          "form": "많이 계셔서 좀 번잡했는데",
          "original_form": "많이 계셔서 좀 번잡했는데",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035645.1.23",
          "form": "아마 지금 가면 그나마 널널할 것 같아요",
          "original_form": "아마 지금 가면 그나마 널널할 것 같아요",
          "speaker_id": "1",
          "time": "20210829 21:52"
        }
      ]
