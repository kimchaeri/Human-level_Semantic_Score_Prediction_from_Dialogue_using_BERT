      "utterance": [
        {
          "id": "MDRW2100041107.1.1",
          "form": "전 예랑가랑이랑 해쭈 자주 봐요",
          "original_form": "전 예랑가랑이랑 해쭈 자주 봐요",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100041107.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100041107.1.3",
          "form": "앗 어제 어떤분이 예랑가랑 추천해주셨는데 ㅎ",
          "original_form": "앗 어제 어떤분이 예랑가랑 추천해주셨는데 ㅎ",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041107.1.4",
          "form": "저는 요즘에 먹방봐요",
          "original_form": "저는 요즘에 먹방봐요",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041107.1.5",
          "form": "히밥님요",
          "original_form": "히밥님요",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100041107.1.6",
          "form": "아 그 분 진짜 많이 드시더라고요",
          "original_form": "아 그 분 진짜 많이 드시더라고요",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100041107.1.7",
          "form": "그 밥 먹으면 8킬로 찌시는 분 맞죠",
          "original_form": "그 밥 먹으면 8킬로 찌시는 분 맞죠",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100041107.1.8",
          "form": "네 먹는양도 많은데 속도도 엄청 빨라요",
          "original_form": "네 먹는양도 많은데 속도도 엄청 빨라요",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100041107.1.9",
          "form": "그 남자 먹방 유튜버랑도 친하지 않나요?",
          "original_form": "그 남자 먹방 유튜버랑도 친하지 않나요?",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100041107.1.10",
          "form": "이름이 기억 안 나네",
          "original_form": "이름이 기억 안 나네",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100041107.1.11",
          "form": "아네네 웅이님이요",
          "original_form": "아네네 웅이님이요",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100041107.1.12",
          "form": "그분거는 몇개안보긴했어요",
          "original_form": "그분거는 몇개안보긴했어요",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041107.1.13",
          "form": "해쭈는 무슨 채널이예요?",
          "original_form": "해쭈는 무슨 채널이예요?",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041107.1.14",
          "form": "브이로그도 섞여있는데 혹시 햄튜브 아시나요?",
          "original_form": "브이로그도 섞여있는데 혹시 햄튜브 아시나요?",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041107.1.15",
          "form": "약간 그런 바이브에요",
          "original_form": "약간 그런 바이브에요",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100041107.1.16",
          "form": "햄튜브 몰라요ㅜㅜ",
          "original_form": "햄튜브 몰라요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100041107.1.17",
          "form": "아 그럼 그냥 약간 발랄한? 느낌의 브이로그에요",
          "original_form": "아 그럼 그냥 약간 발랄한? 느낌의 브이로그에요",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100041107.1.18",
          "form": "오 브이로그 힐링하시려고 보시는거여요?",
          "original_form": "오 브이로그 힐링하시려고 보시는거여요?",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100041107.1.19",
          "form": "어 아뇨 그래서 약간 시끄러운? 사람들 거 봐요",
          "original_form": "어 아뇨 그래서 약간 시끄러운? 사람들 거 봐요",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100041107.1.20",
          "form": "그냥 재밌으려고 보는 거라",
          "original_form": "그냥 재밌으려고 보는 거라",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100041107.1.21",
          "form": "아 재밌으면 찾아봐야겠네요",
          "original_form": "아 재밌으면 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100041107.1.22",
          "form": "브이로그가 웃기기 힘들거같은데",
          "original_form": "브이로그가 웃기기 힘들거같은데",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100041107.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100041107.1.24",
          "form": "보면 계솓 웃게될 정도로",
          "original_form": "보면 계솓 웃게될 정도로",
          "speaker_id": "2",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100041107.1.25",
          "form": "웃기더라고요 특히 해쭈랑 예랑가랑이요",
          "original_form": "웃기더라고요 특히 해쭈랑 예랑가랑이요",
          "speaker_id": "2",
          "time": "20210905 17:14"
        }
      ]
    }
