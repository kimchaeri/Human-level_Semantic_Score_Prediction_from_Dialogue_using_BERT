      "utterance": [
        {
          "id": "MDRW2100008286.1.1",
          "form": "종교라",
          "original_form": "종교라",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100008286.1.2",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100008286.1.3",
          "form": "아니요 ㅎㅎ없어요",
          "original_form": "아니요 ㅎㅎ없어요",
          "speaker_id": "2",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100008286.1.4",
          "form": "저는 어디 체크하라하면 카톨릭에 체크하긴해요",
          "original_form": "저는 어디 체크하라하면 카톨릭에 체크하긴해요",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100008286.1.5",
          "form": "카톨릭 아니시죠?",
          "original_form": "카톨릭 아니시죠?",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100008286.1.6",
          "form": "저도 그런적 있어요 ㅋㅋ",
          "original_form": "저도 그런적 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100008286.1.7",
          "form": "종교가 무교신가요?",
          "original_form": "종교가 무교신가요?",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100008286.1.8",
          "form": "아님 예전에 다니셨어요?",
          "original_form": "아님 예전에 다니셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100008286.1.9",
          "form": "모태신앙이긴해요",
          "original_form": "모태신앙이긴해요",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100008286.1.10",
          "form": "어머니께선",
          "original_form": "어머니께선",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100008286.1.11",
          "form": "미사도 가시고",
          "original_form": "미사도 가시고",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100008286.1.12",
          "form": "묵주기도도 매일 드리시고요",
          "original_form": "묵주기도도 매일 드리시고요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100008286.1.13",
          "form": "아 그럼 카톨릭 맞는데요",
          "original_form": "아 그럼 카톨릭 맞는데요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100008286.1.14",
          "form": "무교신줄 알았어요",
          "original_form": "무교신줄 알았어요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100008286.1.15",
          "form": "근데 전 음 성당도 안가고",
          "original_form": "근데 전 음 성당도 안가고",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100008286.1.16",
          "form": "딱히 예수님 안믿는다고 지옥간다 이런생각도 안해요 ㅋㅋ",
          "original_form": "딱히 예수님 안믿는다고 지옥간다 이런생각도 안해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100008286.1.17",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100008286.1.18",
          "form": "근데 저도 그런 생각은 안해요",
          "original_form": "근데 저도 그런 생각은 안해요",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100008286.1.19",
          "form": "안갈수도 있죠",
          "original_form": "안갈수도 있죠",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100008286.1.20",
          "form": "종교는 원하는대로 본인 마음이 끌려야죠",
          "original_form": "종교는 원하는대로 본인 마음이 끌려야죠",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100008286.1.21",
          "form": "원하실때 가면좋을것 같아요",
          "original_form": "원하실때 가면좋을것 같아요",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100008286.1.22",
          "form": "성당은 교회보다 전 좋은거 같아요",
          "original_form": "성당은 교회보다 전 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100008286.1.23",
          "form": "강요 안하고",
          "original_form": "강요 안하고",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100008286.1.24",
          "form": "전 엄마가 불교신데 연등 달아주세요",
          "original_form": "전 엄마가 불교신데 연등 달아주세요",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100008286.1.25",
          "form": "저도 절에서 공부하는 친구가 있었거든요",
          "original_form": "저도 절에서 공부하는 친구가 있었거든요",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100008286.1.26",
          "form": "불경공부?",
          "original_form": "불경공부?",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100008286.1.27",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100008286.1.28",
          "form": "그친구따라 절갔을땐 그냥 108배도 하고 그랬어요",
          "original_form": "그친구따라 절갔을땐 그냥 108배도 하고 그랬어요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100008286.1.29",
          "form": "첨봐요",
          "original_form": "첨봐요",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100008286.1.30",
          "form": "그성당에서 미사볼때 여자들은 미사보를  쓰는데",
          "original_form": "그성당에서 미사볼때 여자들은 미사보를  쓰는데",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100008286.1.31",
          "form": "어릴땐 그거좋아했어요",
          "original_form": "어릴땐 그거좋아했어요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100008286.1.32",
          "form": "네 봤어요",
          "original_form": "네 봤어요",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100008286.1.33",
          "form": "예뻐요",
          "original_form": "예뻐요",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100008286.1.34",
          "form": "묵주같은거도 액세서리같이 끼긴하고요",
          "original_form": "묵주같은거도 액세서리같이 끼긴하고요",
          "speaker_id": "1",
          "time": "20210822 23:41"
        }
      ]
