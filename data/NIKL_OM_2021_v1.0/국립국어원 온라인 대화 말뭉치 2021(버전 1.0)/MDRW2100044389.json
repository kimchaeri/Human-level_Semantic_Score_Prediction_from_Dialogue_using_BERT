      "utterance": [
        {
          "id": "MDRW2100044389.1.1",
          "form": "갑자기 그렇단 생각이 드네요",
          "original_form": "갑자기 그렇단 생각이 드네요",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044389.1.2",
          "form": "ㅋㅋㅋㅋ맥세권 스세권처럼 ㅋㅋ문세권 책세권!!",
          "original_form": "ㅋㅋㅋㅋ맥세권 스세권처럼 ㅋㅋ문세권 책세권!!",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044389.1.3",
          "form": "ㅋㅋㅋ문세권 책세권이라니",
          "original_form": "ㅋㅋㅋ문세권 책세권이라니",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044389.1.4",
          "form": "작명센스가 좋으시네요",
          "original_form": "작명센스가 좋으시네요",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044389.1.5",
          "form": "아아 그릭요거트도 좋은 겅 같아요 단백질섭취로",
          "original_form": "아아 그릭요거트도 좋은 겅 같아요 단백질섭취로",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044389.1.6",
          "form": "ㅋㅋㅋ아아 그릭요거트 넘 좋은데 비싸고 맛있는거 사기가 힘들어요",
          "original_form": "ㅋㅋㅋ아아 그릭요거트 넘 좋은데 비싸고 맛있는거 사기가 힘들어요",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044389.1.7",
          "form": "전 인터넷으로 주문해 먹어요",
          "original_form": "전 인터넷으로 주문해 먹어요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044389.1.8",
          "form": "꾸덕한게 좋아서",
          "original_form": "꾸덕한게 좋아서",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044389.1.9",
          "form": "오 어디서 주문하세요?!",
          "original_form": "오 어디서 주문하세요?!",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044389.1.10",
          "form": "네이버쇼핑인데",
          "original_form": "네이버쇼핑인데",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044389.1.11",
          "form": "푸드앤플랜이란 곳이요",
          "original_form": "푸드앤플랜이란 곳이요",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044389.1.12",
          "form": "오 저지금 미리 장바구니 담아둘래요 ㅋㅋ 추천해주셔서 감사해요!!!",
          "original_form": "오 저지금 미리 장바구니 담아둘래요 ㅋㅋ 추천해주셔서 감사해요!!!",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044389.1.13",
          "form": "아 도움이 되셨다니 좋네요",
          "original_form": "아 도움이 되셨다니 좋네요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044389.1.14",
          "form": "다른 그릭요거트에비해 엄청 비싸지도않고 좋네요!!! ㅋㅋㅋㅋ",
          "original_form": "다른 그릭요거트에비해 엄청 비싸지도않고 좋네요!!! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044389.1.15",
          "form": "네 엄청 꾸덕해요 근데",
          "original_form": "네 엄청 꾸덕해요 근데",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044389.1.16",
          "form": "추천합니다",
          "original_form": "추천합니다",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044389.1.17",
          "form": "요거트에는 어떤거 넣어드세요?",
          "original_form": "요거트에는 어떤거 넣어드세요?",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044389.1.18",
          "form": "차치키라는 소스 만들기도 하고",
          "original_form": "차치키라는 소스 만들기도 하고",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044389.1.19",
          "form": "잼이나 그래놀라 곁들여 먹기도 해요",
          "original_form": "잼이나 그래놀라 곁들여 먹기도 해요",
          "speaker_id": "2",
          "time": "20210907 22:45"
