      "utterance": [
        {
          "id": "MDRW2100038330.1.1",
          "form": "재우는것만 안해도",
          "original_form": "재우는것만 안해도",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038330.1.2",
          "form": "스트레스가 덜하지",
          "original_form": "스트레스가 덜하지",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038330.1.3",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038330.1.4",
          "form": "진짜 피곤해서",
          "original_form": "진짜 피곤해서",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038330.1.5",
          "form": "바로자면좋은데",
          "original_form": "바로자면좋은데",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100038330.1.6",
          "form": "우리 투도 엄청 뒹굴거리거든",
          "original_form": "우리 투도 엄청 뒹굴거리거든",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100038330.1.7",
          "form": "근데 원이 바로 잠드니까",
          "original_form": "근데 원이 바로 잠드니까",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100038330.1.8",
          "form": "그냥 지도 얘기할 사람이 없어서 그런지 바로 자는 거 같애",
          "original_form": "그냥 지도 얘기할 사람이 없어서 그런지 바로 자는 거 같애",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100038330.1.9",
          "form": "지금 삼일 됐어 둘이 잔지",
          "original_form": "지금 삼일 됐어 둘이 잔지",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100038330.1.10",
          "form": "좋다좋다",
          "original_form": "좋다좋다",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100038330.1.11",
          "form": "나두 둘이재우고싶다ㅋ",
          "original_form": "나두 둘이재우고싶다ㅋ",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100038330.1.12",
          "form": "그래 한 5분에서 10분 정도 같이 누워 있다가",
          "original_form": "그래 한 5분에서 10분 정도 같이 누워 있다가",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038330.1.13",
          "form": "누워서 얘기 좀 하고",
          "original_form": "누워서 얘기 좀 하고",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.14",
          "form": "엄마 이제 씻고 올게 하고 나가거든",
          "original_form": "엄마 이제 씻고 올게 하고 나가거든",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.15",
          "form": "웅 좋다ㅋㅋㅋㅋㅋ",
          "original_form": "웅 좋다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.16",
          "form": "다키웠네",
          "original_form": "다키웠네",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.17",
          "form": "원래는 투는 오빠랑 자는 거 좋아해서 괜찮은데",
          "original_form": "원래는 투는 오빠랑 자는 거 좋아해서 괜찮은데",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.18",
          "form": "원이 맨날 무섭다고 징징거려",
          "original_form": "원이 맨날 무섭다고 징징거려",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.19",
          "form": "응응ㅋㅋㅋㅋ",
          "original_form": "응응ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.20",
          "form": "근데 지금까지는 원이 별 말 안 하고 투랑 잘자 네",
          "original_form": "근데 지금까지는 원이 별 말 안 하고 투랑 잘자 네",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.21",
          "form": "우리애들은 자꾸 장난쳐서ㅡㅡ",
          "original_form": "우리애들은 자꾸 장난쳐서ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.22",
          "form": "제발 이대로 쭉 가줬으면 좋겠다",
          "original_form": "제발 이대로 쭉 가줬으면 좋겠다",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.23",
          "form": "아효",
          "original_form": "아효",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.24",
          "form": "ㅋㅋㅋ 그러니깐",
          "original_form": "ㅋㅋㅋ 그러니깐",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.25",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.26",
          "form": "부럽노ㅋ",
          "original_form": "부럽노ㅋ",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038330.1.27",
          "form": "첫날은 둘이 얘기하고 수다 떤다고 안 자는 거야",
          "original_form": "첫날은 둘이 얘기하고 수다 떤다고 안 자는 거야",
