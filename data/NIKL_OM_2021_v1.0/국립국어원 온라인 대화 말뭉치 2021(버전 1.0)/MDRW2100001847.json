      "utterance": [
        {
          "id": "MDRW2100001847.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100001847.1.2",
          "form": "네 코로나전에는 여행많이다녔어요",
          "original_form": "네 코로나전에는 여행많이다녔어요",
          "speaker_id": "2",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001847.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001847.1.4",
          "form": "저도 여행 정말 좋아해요",
          "original_form": "저도 여행 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001847.1.5",
          "form": "국내건 해외건",
          "original_form": "국내건 해외건",
          "speaker_id": "1",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001847.1.6",
          "form": "가장 인상깊었던 여행지는 어디에요?",
          "original_form": "가장 인상깊었던 여행지는 어디에요?",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001847.1.7",
          "form": "저는 태국이요...",
          "original_form": "저는 태국이요...",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001847.1.8",
          "form": "백번 물어도 태국일거같아요",
          "original_form": "백번 물어도 태국일거같아요",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001847.1.9",
          "form": "와 태국 어떤부분에서요? 저도 태국가보고싶었는데 못가봤어요",
          "original_form": "와 태국 어떤부분에서요? 저도 태국가보고싶었는데 못가봤어요",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001847.1.10",
          "form": "음식도 맛있고 사람들도 친절하고 길거리 시장도 너무 재밌고",
          "original_form": "음식도 맛있고 사람들도 친절하고 길거리 시장도 너무 재밌고",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001847.1.11",
          "form": "모든 호텔에 수영장이 같이 있는데 값이 싸요 ㅋㅋㅋ",
          "original_form": "모든 호텔에 수영장이 같이 있는데 값이 싸요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001847.1.12",
          "form": "오 호캉스진짜 넘좋아요 음식도",
          "original_form": "오 호캉스진짜 넘좋아요 음식도",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001847.1.13",
          "form": "베트남도 좋았었는데 태국은진짜 화려하고 볼거리많다고해서 기대쁌뿜",
          "original_form": "베트남도 좋았었는데 태국은진짜 화려하고 볼거리많다고해서 기대쁌뿜",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001847.1.14",
          "form": "베트남 ㅜㅜ 원래 태국 다음이 베트남이었는데....ㅜㅜ",
          "original_form": "베트남 ㅜㅜ 원래 태국 다음이 베트남이었는데....ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001847.1.15",
          "form": "동남아가 저는 정말 잘 맞더라구요",
          "original_form": "동남아가 저는 정말 잘 맞더라구요",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001847.1.16",
          "form": "저도 휴양지를좋아해서 동남아가 잘맞을것같은데 미주를 더돌았어요ㅠ",
          "original_form": "저도 휴양지를좋아해서 동남아가 잘맞을것같은데 미주를 더돌았어요ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001847.1.17",
          "form": "휴양지 너무 좋죠 ㅜㅜ",
          "original_form": "휴양지 너무 좋죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001847.1.18",
          "form": "오디쪽이 제일 좋으셨어요??",
          "original_form": "오디쪽이 제일 좋으셨어요??",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001847.1.19",
          "form": "전 남미 볼리비아 우유니요ㅜ 자연이주는 감동이있었어요",
          "original_form": "전 남미 볼리비아 우유니요ㅜ 자연이주는 감동이있었어요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001847.1.20",
          "form": "와... 우유니 사막 알죠 알죠",
          "original_form": "와... 우유니 사막 알죠 알죠",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001847.1.21",
          "form": "그 바닥에 비치는 거기 맞죠??",
          "original_form": "그 바닥에 비치는 거기 맞죠??",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001847.1.22",
          "form": "보면 대게 별거없는데 그 소금사막에 하늘이 비치는데 넘이뻐서 눈물나더라구요ㅜ",
          "original_form": "보면 대게 별거없는데 그 소금사막에 하늘이 비치는데 넘이뻐서 눈물나더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001847.1.23",
          "form": "구름이 둥둥떠있고 하늘위를 걷는것같았어요",
          "original_form": "구름이 둥둥떠있고 하늘위를 걷는것같았어요",
          "speaker_id": "2",
          "time": "20210819 22:11"
        }
      ]
