      "utterance": [
        {
          "id": "MDRW2100018388.1.1",
          "form": "안녕 잘 지내?",
          "original_form": "안녕 잘 지내?",
          "speaker_id": "1",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018388.1.2",
          "form": "혹시 그 이야기 들었어?",
          "original_form": "혹시 그 이야기 들었어?",
          "speaker_id": "1",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018388.1.3",
          "form": "안녕하세요 언니ㅎㅎ",
          "original_form": "안녕하세요 언니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018388.1.4",
          "form": "어떤 이야기요?",
          "original_form": "어떤 이야기요?",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018388.1.5",
          "form": "싸이월드라는 커뮤니티가 이번에 다시 서비스된대",
          "original_form": "싸이월드라는 커뮤니티가 이번에 다시 서비스된대",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018388.1.6",
          "form": "싸이월드요? 아 이름 들어봤어요!",
          "original_form": "싸이월드요? 아 이름 들어봤어요!",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018388.1.7",
          "form": "우와 신기하다 언니 싸이월드 예전에 많이 하셨어요?",
          "original_form": "우와 신기하다 언니 싸이월드 예전에 많이 하셨어요?",
          "speaker_id": "2",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018388.1.8",
          "form": "응응 나는 예전에 싸이월드 자주 했었지ㅎㅎ",
          "original_form": "응응 나는 예전에 싸이월드 자주 했었지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018388.1.9",
          "form": "아 넌 싸이월드 세대가 아닌가...?",
          "original_form": "아 넌 싸이월드 세대가 아닌가...?",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100018388.1.10",
          "form": "싸이월드 잘 몰라?ㅠㅠ",
          "original_form": "싸이월드 잘 몰라?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018388.1.11",
          "form": "네ㅜㅜ 저는 싸이월드 세대는 아니었어요",
          "original_form": "네ㅜㅜ 저는 싸이월드 세대는 아니었어요",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100018388.1.12",
          "form": "그치만 사촌 언니들이 하던거 자주 봐서 대충은 알아요!!",
          "original_form": "그치만 사촌 언니들이 하던거 자주 봐서 대충은 알아요!!",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018388.1.13",
          "form": "싸이월드랑 버디버디랑 그런거 있었잖아요ㅋㅋㅋ",
          "original_form": "싸이월드랑 버디버디랑 그런거 있었잖아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018388.1.14",
          "form": "오오 맞앜ㅋㅋㅋㅋ 되게 잘 아는구나",
          "original_form": "오오 맞앜ㅋㅋㅋㅋ 되게 잘 아는구나",
          "speaker_id": "1",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100018388.1.15",
          "form": "그래서 나한테는 싸이월드가 굉장한 추억이야",
          "original_form": "그래서 나한테는 싸이월드가 굉장한 추억이야",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100018388.1.16",
          "form": "그쵸 추억이죠ㅠㅠ",
          "original_form": "그쵸 추억이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018388.1.17",
          "form": "이렇게 싸이월드가 부활한다니 신기하고 감회가 새롭겠어요?!",
          "original_form": "이렇게 싸이월드가 부활한다니 신기하고 감회가 새롭겠어요?!",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100018388.1.18",
          "form": "맞아 정확해! 기분이 묘하고 신기해",
          "original_form": "맞아 정확해! 기분이 묘하고 신기해",
          "speaker_id": "1",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100018388.1.19",
          "form": "내 홈피 들어가보면 이상한 사진 많을듯...ㅋㅋ",
          "original_form": "내 홈피 들어가보면 이상한 사진 많을듯...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:03"
        },
        {
          "id": "MDRW2100018388.1.20",
          "form": "앗ㅋㅋㅋㅋㅋ 언니 흑역사 얼른 지워야하는거 아니에요..?ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ 언니 흑역사 얼른 지워야하는거 아니에요..?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018388.1.21",
          "form": "그래도 추억이니까 좋개 생각합시다 ㅎㅎ",
          "original_form": "그래도 추억이니까 좋개 생각합시다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100018388.1.22",
          "form": "알았어ㅋㅋㅋ 얼른 들어가서 지워야겠다",
          "original_form": "알았어ㅋㅋㅋ 얼른 들어가서 지워야겠다",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018388.1.23",
          "form": "싸이윌드 부활해서 좋은데 흑역사는 골치 아프구먼ㅎ",
          "original_form": "싸이윌드 부활해서 좋은데 흑역사는 골치 아프구먼ㅎ",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100018388.1.24",
          "form": "예전 글들 지우기 전에 사진은 저한테 살짝만 보여주세요!ㅎㅎ",
          "original_form": "예전 글들 지우기 전에 사진은 저한테 살짝만 보여주세요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018388.1.25",
          "form": "언니 예전 젊을 때 모습 보고싶어요~",
          "original_form": "언니 예전 젊을 때 모습 보고싶어요~{emoji:♡}",
          "speaker_id": "2",
          "time": "20210827 01:06"
        },
        {
          "id": "MDRW2100018388.1.26",
          "form": "오케이 알겠어",
          "original_form": "오케이 알겠어",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100018388.1.27",
