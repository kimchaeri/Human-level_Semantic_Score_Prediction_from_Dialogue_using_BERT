      "utterance": [
        {
          "id": "MDRW2100010808.1.1",
          "form": "가장 좋아하는 음식은 무엇이세요?",
          "original_form": "가장 좋아하는 음식은 무엇이세요?",
          "speaker_id": "1",
          "time": "20210823 22:04"
        },
        {
          "id": "MDRW2100010808.1.2",
          "form": "요즘은 인도 커리다 당겨요",
          "original_form": "요즘은 인도 커리다 당겨요",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010808.1.3",
          "form": "헉 저도 인도커리 좋아해요.",
          "original_form": "헉 저도 인도커리 좋아해요.",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010808.1.4",
          "form": "그 강한 향미가 좋아요",
          "original_form": "그 강한 향미가 좋아요",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100010808.1.5",
          "form": "제가 또 밀가루를 좋아하는데 난....난 너무 좋아해요ㅠㅜ",
          "original_form": "제가 또 밀가루를 좋아하는데 난....난 너무 좋아해요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 22:13"
        },
        {
          "id": "MDRW2100010808.1.6",
          "form": "갈릭난 커리난 ㅠㅠ",
          "original_form": "갈릭난 커리난 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010808.1.7",
          "form": "저도 급땡기네요 그 끝부분 바삭한거... 엄청 맛있잖아요",
          "original_form": "저도 급땡기네요 그 끝부분 바삭한거... 엄청 맛있잖아요",
          "speaker_id": "1",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010808.1.8",
          "form": "갈릭난도 좋고 버터난도 좋아해요! 탄산음료도 좋지만 라씨로 끝내면 정말 배부르죠ㅋㅋ",
          "original_form": "갈릭난도 좋고 버터난도 좋아해요! 탄산음료도 좋지만 라씨로 끝내면 정말 배부르죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010808.1.9",
          "form": "어떤 라씨 젤 좋아하세요? 저는 딸기요 ㅎㅎ",
          "original_form": "어떤 라씨 젤 좋아하세요? 저는 딸기요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010808.1.10",
          "form": "저는 망고랑 딸기 중 그 날에 끌리는걸로 먹어요ㅋㅋ",
          "original_form": "저는 망고랑 딸기 중 그 날에 끌리는걸로 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010808.1.11",
          "form": "이 대화를 하니 에베레스트 식당 가고싶네요 ㅠㅠ",
          "original_form": "이 대화를 하니 에베레스트 식당 가고싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010808.1.12",
          "form": "식당 이름이 에베레스트인가요?",
          "original_form": "식당 이름이 에베레스트인가요?",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010808.1.13",
          "form": "네네 체인인데 나름 괜찮아요. 강가보다 나은 것 같아요~",
          "original_form": "네네 체인인데 나름 괜찮아요. 강가보다 나은 것 같아요~",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010808.1.14",
          "form": "커리 잘하는 식당 추천 부탁드려요! ㅎㅎ",
          "original_form": "커리 잘하는 식당 추천 부탁드려요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010808.1.15",
          "form": "저는 옷살, 아마 잘 가요!",
          "original_form": "저는 옷살, 아마 잘 가요!",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010808.1.16",
          "form": "멀지 않고 예약도 가능해서 종종 생각나면 들려요ㅎㅎ",
          "original_form": "멀지 않고 예약도 가능해서 종종 생각나면 들려요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010808.1.17",
          "form": "와 적어두겠습니다 ㅋㅋ",
          "original_form": "와 적어두겠습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010808.1.18",
          "form": "저 낼 뵐게요! 너무 졸려서 ㅋㅋ 좋은 밤 보내세요",
          "original_form": "저 낼 뵐게요! 너무 졸려서 ㅋㅋ 좋은 밤 보내세요",
          "speaker_id": "1",
          "time": "20210823 23:09"
        }
      ]
