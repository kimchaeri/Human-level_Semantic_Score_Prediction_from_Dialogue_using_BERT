      "utterance": [
        {
          "id": "MDRW2100001694.1.1",
          "form": "재택근무 하니까 좋다",
          "original_form": "재택근무 하니까 좋다",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001694.1.2",
          "form": "출퇴근 시간 아껴서",
          "original_form": "출퇴근 시간 아껴서",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001694.1.3",
          "form": "집에서 일하는게 최고",
          "original_form": "집에서 일하는게 최고",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001694.1.4",
          "form": "코로나로 마스크 안쓰고 일할수 있으니 좋치",
          "original_form": "코로나로 마스크 안쓰고 일할수 있으니 좋치",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001694.1.5",
          "form": "맞당~ 마스크 값 아끼기 ㅋㅋ",
          "original_form": "맞당~ 마스크 값 아끼기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001694.1.6",
          "form": "직장에서 마스크 하루종일 착용하는게 얼마나 힘들꼬 ㅠ",
          "original_form": "직장에서 마스크 하루종일 착용하는게 얼마나 힘들꼬 ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001694.1.7",
          "form": "그니까 ㅠ",
          "original_form": "그니까 ㅠ",
          "speaker_id": "1",
          "time": "20210819 18:11"
        },
        {
          "id": "MDRW2100001694.1.8",
          "form": "엄만 마스크 쓰고 다녀서 얼굴에 뽀로지도 생기네 ㅠ",
          "original_form": "엄만 마스크 쓰고 다녀서 얼굴에 뽀로지도 생기네 ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001694.1.9",
          "form": "이런 ㅠㅠ 나도 턱에 나더라",
          "original_form": "이런 ㅠㅠ 나도 턱에 나더라",
          "speaker_id": "1",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001694.1.10",
          "form": "집에서 일하니 이제 그럴일없어서 좋당",
          "original_form": "집에서 일하니 이제 그럴일없어서 좋당",
          "speaker_id": "1",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001694.1.11",
          "form": "코로나 땜에 재택근무도 다 해보고",
          "original_form": "코로나 땜에 재택근무도 다 해보고",
          "speaker_id": "2",
          "time": "20210819 18:12"
        },
        {
          "id": "MDRW2100001694.1.12",
          "form": "그러게 ㅋㅋㅋ 그런거 절대 없을것같은 회사였는뎅~",
          "original_form": "그러게 ㅋㅋㅋ 그런거 절대 없을것같은 회사였는뎅~",
          "speaker_id": "1",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100001694.1.13",
          "form": "아무튼 재택근무로 더 열심히 일해여~",
          "original_form": "아무튼 재택근무로 더 열심히 일해여~",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100001694.1.14",
          "form": "구래용~! 집중해서 일해야겠따",
          "original_form": "구래용~! 집중해서 일해야겠따",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001694.1.15",
          "form": "집에서도 성실하게!",
          "original_form": "집에서도 성실하게!",
          "speaker_id": "2",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001694.1.16",
          "form": "오키!!",
          "original_form": "오키!!",
          "speaker_id": "1",
          "time": "20210819 18:14"
        },
        {
          "id": "MDRW2100001694.1.17",
          "form": "재택근무 하니까 일하는 티가 잘 안나서 별로인듯",
          "original_form": "재택근무 하니까 일하는 티가 잘 안나서 별로인듯",
