      "utterance": [
        {
          "id": "MDRW2100008863.1.1",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008863.1.2",
          "form": "막창 사왔어요",
          "original_form": "막창 사왔어요",
          "speaker_id": "2",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008863.1.3",
          "form": "부추 레시피 보고 무쳐서 먹어보려고요",
          "original_form": "부추 레시피 보고 무쳐서 먹어보려고요",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008863.1.4",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008863.1.5",
          "form": "그 서스도 만들어야 하는데 ㅋㅋ",
          "original_form": "그 서스도 만들어야 하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008863.1.6",
          "form": "소스",
          "original_form": "소스",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008863.1.7",
          "form": "아! 맛있겠어요ㅎㅎ\r\n저도 막창좋아하는데!",
          "original_form": "아! 맛있겠어요ㅎㅎ\r\n저도 막창좋아하는데!",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008863.1.8",
          "form": "얼마전에 에어프라이기로 처음했다가 과자만들어 먹었어요ㅋㅋㅋ",
          "original_form": "얼마전에 에어프라이기로 처음했다가 과자만들어 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008863.1.9",
          "form": "ㅋㅋ똑같네요",
          "original_form": "ㅋㅋ똑같네요",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008863.1.10",
          "form": "저도 바삭",
          "original_form": "저도 바삭",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008863.1.11",
          "form": "과자되던데요 ㅋㅋ",
          "original_form": "과자되던데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008863.1.12",
          "form": "저만 그런거 아니군요 ㅋㅋ",
          "original_form": "저만 그런거 아니군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008863.1.13",
          "form": "저도 원락 빠.싹 익힌거 좋아하는데",
          "original_form": "저도 원락 빠.싹 익힌거 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008863.1.14",
          "form": "넣은양에 3분의1이되어서 나왔어요ㅋㅋ",
          "original_form": "넣은양에 3분의1이되어서 나왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008863.1.15",
          "form": "맞아요 쪼매나게 되서",
          "original_form": "맞아요 쪼매나게 되서",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008863.1.16",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008863.1.17",
          "form": "근데 뭐 먹을만 했어요",
          "original_form": "근데 뭐 먹을만 했어요",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008863.1.18",
          "form": "그후론 기냥 구워요",
          "original_form": "그후론 기냥 구워요",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008863.1.19",
          "form": "저두요ㅋㅋㄱㄱ",
          "original_form": "저두요ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008863.1.20",
          "form": "저는 오늘 이연복 쉐프 짜장",
          "original_form": "저는 오늘 이연복 쉐프 짜장",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008863.1.21",
          "form": "반조리 해서 먹었어요",
          "original_form": "반조리 해서 먹었어요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.22",
          "form": "오 맛난가요?",
          "original_form": "오 맛난가요?",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.23",
          "form": "처음 해먹어봤는데",
          "original_form": "처음 해먹어봤는데",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.24",
          "form": "시중파는거랑 비슷한거 같앴어요",
          "original_form": "시중파는거랑 비슷한거 같앴어요",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.25",
          "form": "저도 반조리 가끔 사는데 맛나고 좋아요",
          "original_form": "저도 반조리 가끔 사는데 맛나고 좋아요",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.27",
          "form": "맛있더라구요ㅎㅎ",
          "original_form": "맛있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.28",
          "form": "짜장은 안먹어 봤는데 먹어봐야겠네요",
          "original_form": "짜장은 안먹어 봤는데 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.29",
          "form": "전..가끔아니고.... 반조리를 선호해요ㅋㅋㄱ",
          "original_form": "전..가끔아니고.... 반조리를 선호해요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.30",
          "form": "집에서 편하게 먹어 좋아요",
          "original_form": "집에서 편하게 먹어 좋아요",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008863.1.31",
          "form": "괜찮죠",
          "original_form": "괜찮죠",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.32",
          "form": "딱 조리가 편해서",
          "original_form": "딱 조리가 편해서",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.33",
          "form": "된장찌개를 끓이더라고..  항상 야채 다사면",
          "original_form": "된장찌개를 끓이더라고..  항상 야채 다사면",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.34",
          "form": "그리고 사실 넘 맛나요",
          "original_form": "그리고 사실 넘 맛나요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.35",
          "form": "남으니깐 버리게되더라구요",
          "original_form": "남으니깐 버리게되더라구요",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.36",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.37",
          "form": "아까워요",
          "original_form": "아까워요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.38",
          "form": "전 가지 ㅜㅜ",
          "original_form": "전 가지 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.39",
          "form": "그양에맞게 딱딱할수 있어서 편하고",
          "original_form": "그양에맞게 딱딱할수 있어서 편하고",
          "speaker_id": "1",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.40",
          "form": "맨날 버려요",
          "original_form": "맨날 버려요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008863.1.41",
          "form": "전 제가 이것저것 조합해서 요리를 못하니..  죄다 버리더라구요ㅜㅜ",
          "original_form": "전 제가 이것저것 조합해서 요리를 못하니..  죄다 버리더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:20"
        }
      ]
    }
