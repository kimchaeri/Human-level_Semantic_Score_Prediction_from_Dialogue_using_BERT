      "utterance": [
        {
          "id": "MDRW2100018101.1.1",
          "form": "손맛이 좋아서 재밌어요",
          "original_form": "손맛이 좋아서 재밌어요",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018101.1.2",
          "form": "크으... 한조 스킬 보는 거 좋아해서 유튭으로 종종 봐요",
          "original_form": "크으... 한조 스킬 보는 거 좋아해서 유튭으로 종종 봐요",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018101.1.3",
          "form": "궁쓰는거 말씀하시나요?",
          "original_form": "궁쓰는거 말씀하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018101.1.4",
          "form": "그 반사되는 화살?",
          "original_form": "그 반사되는 화살?",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018101.1.5",
          "form": "아 리메이크되기전에 있던",
          "original_form": "아 리메이크되기전에 있던",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018101.1.6",
          "form": "스킬말하시는구나!",
          "original_form": "스킬말하시는구나!",
          "speaker_id": "1",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018101.1.7",
          "form": "그거 속시원하고 좋죠",
          "original_form": "그거 속시원하고 좋죠",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018101.1.8",
          "form": "저는 그때 한조가 더 좋아요",
          "original_form": "저는 그때 한조가 더 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018101.1.9",
          "form": "헉 아니 그게 벌써 과거라구요?",
          "original_form": "헉 아니 그게 벌써 과거라구요?",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018101.1.10",
          "form": "ㅜ 시대가 너무 빠르게 달라집니다 ㅠㅠㅠ",
          "original_form": "ㅜ 시대가 너무 빠르게 달라집니다 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018101.1.11",
          "form": "이게 게임도 멀리서 보면 세월이 참빠르죠...",
          "original_form": "이게 게임도 멀리서 보면 세월이 참빠르죠...",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018101.1.12",
          "form": "사이퍼즈 캐릭터중에 누가제일 좋으세요?",
          "original_form": "사이퍼즈 캐릭터중에 누가제일 좋으세요?",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018101.1.13",
          "form": "저 로라스요",
          "original_form": "저 로라스요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018101.1.14",
          "form": "로라스 너무 사랑이에요.... 얼굴이",
          "original_form": "로라스 너무 사랑이에요.... 얼굴이",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018101.1.15",
          "form": "갑옷입은 캐릭터 맞나요?",
          "original_form": "갑옷입은 캐릭터 맞나요?",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018101.1.16",
          "form": "네 맞아요 로딩포스터 라고 쳐주시면 투구 벗은 거 있는데 그게 사랑입니다.",
          "original_form": "네 맞아요 로딩포스터 라고 쳐주시면 투구 벗은 거 있는데 그게 사랑입니다.",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018101.1.17",
          "form": "걔 아니었으면 게임 이미 나갔는데 말이죠 ㅠㅠ",
          "original_form": "걔 아니었으면 게임 이미 나갔는데 말이죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018101.1.18",
          "form": "우와...투구벗으면 너무 미남이네요",
          "original_form": "우와...투구벗으면 너무 미남이네요",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018101.1.19",
          "form": "네 맞아요! 진짜 잘샌겼어요. 완전 애정합니다. 근데 손이 똥이라 로라스 못 해요",
          "original_form": "네 맞아요! 진짜 잘샌겼어요. 완전 애정합니다. 근데 손이 똥이라 로라스 못 해요",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018101.1.20",
          "form": "손타는 캐릭터군요...",
          "original_form": "손타는 캐릭터군요...",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018101.1.21",
          "form": "엄청 타죠...",
          "original_form": "엄청 타죠...",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018101.1.22",
          "form": "자리야는 어때요?",
          "original_form": "자리야는 어때요?",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018101.1.23",
          "form": "자리야도 어느정도는 타는데 심하진않아요",
          "original_form": "자리야도 어느정도는 타는데 심하진않아요",
          "speaker_id": "1",
          "time": "20210826 22:53"
        }
      ]
    }
  ]
