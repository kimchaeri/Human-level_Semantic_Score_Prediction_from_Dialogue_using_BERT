      "utterance": [
        {
          "id": "MDRW2100017659.1.1",
          "form": "얼마전에 배스킨라빈스 행사할때 구매하셨어요?",
          "original_form": "얼마전에 배스킨라빈스 행사할때 구매하셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017659.1.2",
          "form": "네! 청키몽키는 안샀네요ㅠ",
          "original_form": "네! 청키몽키는 안샀네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017659.1.3",
          "form": "아니요 근처에 없어서 못샀어요ㅠ",
          "original_form": "아니요 근처에 없어서 못샀어요ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017659.1.4",
          "form": "가장 좋아하는 맛이 뭐에용?",
          "original_form": "가장 좋아하는 맛이 뭐에용?",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017659.1.5",
          "form": "배라도 참좋아하는데..ㅜ",
          "original_form": "배라도 참좋아하는데..ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017659.1.6",
          "form": "ㅎㅎ 저두요 거의 다 맛있어요",
          "original_form": "ㅎㅎ 저두요 거의 다 맛있어요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017659.1.7",
          "form": "전 엄마는외계인 맛 좋아해요",
          "original_form": "전 엄마는외계인 맛 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017659.1.8",
          "form": "저는 아몬드봉봉이랑 엄마는 외계인, 솜사탕, 레인보우샤베트좋아해요",
          "original_form": "저는 아몬드봉봉이랑 엄마는 외계인, 솜사탕, 레인보우샤베트좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017659.1.9",
          "form": "오!ㅋㅋ저두요",
          "original_form": "오!ㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017659.1.10",
          "form": "다른거는 뭐좋아하세요?",
          "original_form": "다른거는 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017659.1.11",
          "form": "다 쉐이크로 먹어도 맛있는 맛들이네용 ㅣㅎㅎ",
          "original_form": "다 쉐이크로 먹어도 맛있는 맛들이네용 ㅣㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017659.1.12",
          "form": "민트초코도 좋아해요",
          "original_form": "민트초코도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017659.1.13",
          "form": "오호! 민초파시군요",
          "original_form": "오호! 민초파시군요",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017659.1.14",
          "form": "저는 아직 민초까지는 못갔어요ㅋㅋ",
          "original_form": "저는 아직 민초까지는 못갔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017659.1.15",
          "form": "넹ㅋㅋㅋ 민초 상쾌해서 좋아해요",
          "original_form": "넹ㅋㅋㅋ 민초 상쾌해서 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017659.1.16",
          "form": "치약맛이라고 생각하시나용ㅋ",
          "original_form": "치약맛이라고 생각하시나용ㅋ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017659.1.17",
          "form": "마자요ㅋㅋ처음에는 치약같았는데",
          "original_form": "마자요ㅋㅋ처음에는 치약같았는데",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017659.1.18",
          "form": "먹다보니까 괜찮더라구요",
          "original_form": "먹다보니까 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017659.1.19",
          "form": "앞으로 민초파가 될수도 있을거같아요ㅋㅋㅋ",
          "original_form": "앞으로 민초파가 될수도 있을거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017659.1.20",
          "form": "민초단이 많아졌으면 좋겠네용ㅋㅋ",
          "original_form": "민초단이 많아졌으면 좋겠네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017659.1.21",
          "form": "ㅋㅋㅋㅋㅋ그러게요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017659.1.22",
          "form": "일반아이스크림은 뭐 좋아하세요?",
          "original_form": "일반아이스크림은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017659.1.23",
          "form": "생귤탱귤 좋아해요!",
          "original_form": "생귤탱귤 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017659.1.24",
          "form": "슈퍼콘도 맛있어용",
          "original_form": "슈퍼콘도 맛있어용",
          "speaker_id": "1",
          "time": "20210826 20:41"
        }
      ]
    }
  ]
