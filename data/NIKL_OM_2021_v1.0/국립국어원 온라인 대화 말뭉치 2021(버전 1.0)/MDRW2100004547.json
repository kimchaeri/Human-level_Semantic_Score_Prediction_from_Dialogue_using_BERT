      "utterance": [
        {
          "id": "MDRW2100004547.1.1",
          "form": "안녕하세요ㅎ 혹시 게임좋아하세요?",
          "original_form": "안녕하세요ㅎ 혹시 게임좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004547.1.2",
          "form": "요즘은 쿠키런 킹덤만 해요!",
          "original_form": "요즘은 쿠키런 킹덤만 해요!",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004547.1.3",
          "form": "우왐ㅎ 쿠키런 재밌나요? 전 모바일 게임은 잘못해서ㅎ",
          "original_form": "우왐ㅎ 쿠키런 재밌나요? 전 모바일 게임은 잘못해서ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004547.1.4",
          "form": "킹덤은뭔가요? 인기 게임인가보네요ㅎ",
          "original_form": "킹덤은뭔가요? 인기 게임인가보네요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004547.1.5",
          "form": "쿠키런 캐릭터들이 마을을 짓고 물건만들고 싸움해요!",
          "original_form": "쿠키런 캐릭터들이 마을을 짓고 물건만들고 싸움해요!",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004547.1.6",
          "form": "아ㅎ 그럼 클래시오브킹덤 이런것 같은거네요ㅎ",
          "original_form": "아ㅎ 그럼 클래시오브킹덤 이런것 같은거네요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004547.1.7",
          "form": "게임 순위는 높죠?",
          "original_form": "게임 순위는 높죠?",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004547.1.8",
          "form": "클래시 킹덤 안해보았지만 그럴수있어요!",
          "original_form": "클래시 킹덤 안해보았지만 그럴수있어요!",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004547.1.9",
          "form": "예전에는 높았는데 지금은 잘 모르겠어요",
          "original_form": "예전에는 높았는데 지금은 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004547.1.10",
          "form": "인기 게임이었던것 같아요ㅎ 그럼 신작 게임중에는 뭐 좋아하세요?",
          "original_form": "인기 게임이었던것 같아요ㅎ 그럼 신작 게임중에는 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004547.1.11",
          "form": "쿠키런 킹덤 외에는 안해서 잘모르는데 지브리와 합작한 게임 좋아보이더라구요",
          "original_form": "쿠키런 킹덤 외에는 안해서 잘모르는데 지브리와 합작한 게임 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004547.1.12",
          "form": "아ㅎㅎ 뭔지알것같아요ㅎㅎ",
          "original_form": "아ㅎㅎ 뭔지알것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004547.1.13",
          "form": "예전에 e-sports좋아하셨어요? 프로게이머들 잘생겻었잖아요ㅎ",
          "original_form": "예전에 e-sports좋아하셨어요? 프로게이머들 잘생겻었잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004547.1.14",
          "form": "그쵸 많이 광고해서",
          "original_form": "그쵸 많이 광고해서",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004547.1.15",
          "form": "스타할때 엄청나게 인기있었던것 같은데ㅎ",
          "original_form": "스타할때 엄청나게 인기있었던것 같은데ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004547.1.16",
          "form": "저는 그쪽 분야를 잘 모르는데 유튜버 꽃빈님 좋아해서",
          "original_form": "저는 그쪽 분야를 잘 모르는데 유튜버 꽃빈님 좋아해서",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004547.1.17",
          "form": "러너님만 알아요!",
          "original_form": "러너님만 알아요!",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004547.1.18",
          "form": "아ㅎㅎ 저도 잘몰라요ㅎㅎ",
          "original_form": "아ㅎㅎ 저도 잘몰라요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004547.1.19",
          "form": "프로게이머 연봉이 엄청나다고 들었긴했어요",
          "original_form": "프로게이머 연봉이 엄청나다고 들었긴했어요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004547.1.20",
          "form": "스타는 예전에 조금했어요!",
          "original_form": "스타는 예전에 조금했어요!",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004547.1.21",
          "form": "우와 스타 하셨어요?",
          "original_form": "우와 스타 하셨어요?",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004547.1.22",
          "form": "그쵸!엄청 높다고 저도 들은 것 같아요",
          "original_form": "그쵸!엄청 높다고 저도 들은 것 같아요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004547.1.23",
          "form": "전 정말 못할것같던데ㅎㅎ",
          "original_form": "전 정말 못할것같던데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004547.1.24",
          "form": "매일 무한만 했던것같애요ㅎ",
          "original_form": "매일 무한만 했던것같애요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004547.1.25",
          "form": "네네 저그 좋아했었어요",
          "original_form": "네네 저그 좋아했었어요",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004547.1.26",
          "form": "무한이 뭐죠??",
          "original_form": "무한이 뭐죠??",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004547.1.27",
          "form": "무한맵이요ㅎㅎ 보통 스타는 미네랄 캐면 없어지는데 무한은 자원무한이라는 뜻이에요ㅎ",
          "original_form": "무한맵이요ㅎㅎ 보통 스타는 미네랄 캐면 없어지는데 무한은 자원무한이라는 뜻이에요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004547.1.28",
          "form": "저도 저그였어요 ㅡㅎㅎ",
          "original_form": "저도 저그였어요 ㅡㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004547.1.29",
          "form": "저그가 초반에 가장 좋았던것같애요ㅎ",
          "original_form": "저그가 초반에 가장 좋았던것같애요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004547.1.30",
          "form": "오오오 몰랐던 정보네요!",
          "original_form": "오오오 몰랐던 정보네요!",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004547.1.31",
          "form": "맞아요 저그가 좋았어요",
          "original_form": "맞아요 저그가 좋았어요",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004547.1.32",
          "form": "아마 게임순위가 몇년돈안 1위가 스타였었죠?ㅎ",
          "original_form": "아마 게임순위가 몇년돈안 1위가 스타였었죠?ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004547.1.33",
          "form": "정말 인기 게임이었는데 후속 시리즈들이 1탄을 못따라가네요",
          "original_form": "정말 인기 게임이었는데 후속 시리즈들이 1탄을 못따라가네요",
          "speaker_id": "1",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004547.1.34",
          "form": "후속 게임도 냈었군요",
          "original_form": "후속 게임도 냈었군요",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004547.1.35",
          "form": "스타2했었는데 망한거죠 뭐ㅎ",
          "original_form": "스타2했었는데 망한거죠 뭐ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004547.1.36",
          "form": "1탄의 인기 게임의 위엄은 2탄에서는 초반에 잠깐ㅎ",
          "original_form": "1탄의 인기 게임의 위엄은 2탄에서는 초반에 잠깐ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004547.1.37",
          "form": "쿠키런은 저 아시는분이 개발자로 이번에 들어갔다고 하는데 엄청좋더라구요",
          "original_form": "쿠키런은 저 아시는분이 개발자로 이번에 들어갔다고 하는데 엄청좋더라구요",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004547.1.38",
          "form": "아 2가 나왔었군요!!",
          "original_form": "아 2가 나왔었군요!!",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004547.1.39",
          "form": "쿠키런도 리뉴얼했나요??킹덤만 해서 ㅋㅋㅋ",
          "original_form": "쿠키런도 리뉴얼했나요??킹덤만 해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004547.1.40",
          "form": "쿠키런 직원 정말 어마어마 합니다ㅎㅎ",
          "original_form": "쿠키런 직원 정말 어마어마 합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004547.1.41",
          "form": "대리인데 연봉 5천이래요ㅎ 그리고 스톡옵션행사가능해요ㅎ",
          "original_form": "대리인데 연봉 5천이래요ㅎ 그리고 스톡옵션행사가능해요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004547.1.42",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004547.1.43",
          "form": "인기 게임이니깐 그런가봐요ㅎ 직원들 복지가 허덜덜",
          "original_form": "인기 게임이니깐 그런가봐요ㅎ 직원들 복지가 허덜덜",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004547.1.44",
          "form": "요번에 게임 주식 올랐다고 들었어요",
          "original_form": "요번에 게임 주식 올랐다고 들었어요",
          "speaker_id": "2",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004547.1.45",
          "form": "아무래도 집에있는 시간이 많으니깐 그런것같아요ㅎ",
          "original_form": "아무래도 집에있는 시간이 많으니깐 그런것같아요ㅎ",
