      "utterance": [
        {
          "id": "MDRW2100033096.1.1",
          "form": "어떤 일 하세여?",
          "original_form": "어떤 일 하세여?",
          "speaker_id": "1",
          "time": "20210906 15:59"
        },
        {
          "id": "MDRW2100033096.1.2",
          "form": "공공기관애서일해여 ㅋㅋ",
          "original_form": "공공기관애서일해여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100033096.1.3",
          "form": "오오 저도요",
          "original_form": "오오 저도요",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033096.1.4",
          "form": "부서원들은 좋으세여?",
          "original_form": "부서원들은 좋으세여?",
          "speaker_id": "1",
          "time": "20210906 16:03"
        },
        {
          "id": "MDRW2100033096.1.5",
          "form": "네 일은 거지같은데  부서원들은 좋아요 ㅎㅎㅎㅎ",
          "original_form": "네 일은 거지같은데  부서원들은 좋아요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 16:04"
        },
        {
          "id": "MDRW2100033096.1.6",
          "form": "우와 그게 차라리 좋아요",
          "original_form": "우와 그게 차라리 좋아요",
          "speaker_id": "1",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100033096.1.7",
          "form": "저는 일은 괜찮고 부서원도 괜찮은데",
          "original_form": "저는 일은 괜찮고 부서원도 괜찮은데",
          "speaker_id": "1",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100033096.1.8",
          "form": "같은 팀 한명만 좀 미워요 빌런이에요",
          "original_form": "같은 팀 한명만 좀 미워요 빌런이에요",
          "speaker_id": "1",
          "time": "20210906 16:05"
        },
        {
          "id": "MDRW2100033096.1.9",
          "form": "ㅎㅎ 저도 맘에 안드는사람이 있죠",
          "original_form": "ㅎㅎ 저도 맘에 안드는사람이 있죠",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100033096.1.10",
          "form": "그래도 좋은사람들이 더 많아서 버티고 있어요 ㅎㅎ",
          "original_form": "그래도 좋은사람들이 더 많아서 버티고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100033096.1.11",
          "form": "와 그러시구나",
          "original_form": "와 그러시구나",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033096.1.12",
          "form": "저도그래요ㅠㅠ 한사람 땜에 기분 망치기 싫어서",
          "original_form": "저도그래요ㅠㅠ 한사람 땜에 기분 망치기 싫어서",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033096.1.13",
          "form": "잊으려고 노력해요!!",
          "original_form": "잊으려고 노력해요!!",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033096.1.14",
          "form": "맞아요  그사람은 자기땜에 그러는지도모를텐데",
          "original_form": "맞아요  그사람은 자기땜에 그러는지도모를텐데",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033096.1.15",
          "form": "혼자 스트레스 받을 필요 없죠 ㅎㅎ",
          "original_form": "혼자 스트레스 받을 필요 없죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033096.1.16",
          "form": "저도그래요 ...",
          "original_form": "저도그래요 ...",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033096.1.17",
          "form": "그냥 받아칠거 받아치고 무시합니다...",
          "original_form": "그냥 받아칠거 받아치고 무시합니다...",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033096.1.18",
          "form": "그래도 힘들긴 하네요 ㅋㅋ",
          "original_form": "그래도 힘들긴 하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033096.1.19",
          "form": "맞아요  근데 어딜가나 그런 사람들은 꼭 있어서 어쩔 수 없는거같아요 ㅎ",
          "original_form": "맞아요  근데 어딜가나 그런 사람들은 꼭 있어서 어쩔 수 없는거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100033096.1.20",
          "form": "맞아요 ㅋㅋㅌ어딜가나 있어요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅌ어딜가나 있어요 ㅋㅋㅋ",
