      "utterance": [
        {
          "id": "MDRW2100002825.1.1",
          "form": "오늘 불금인데 계획있어?",
          "original_form": "오늘 불금인데 계획있어?",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002825.1.2",
          "form": "퇴근하고 친구 만날거야",
          "original_form": "퇴근하고 친구 만날거야",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002825.1.3",
          "form": "너는 머할거야,?",
          "original_form": "너는 머할거야,?",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002825.1.4",
          "form": "나 고민중이야",
          "original_form": "나 고민중이야",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002825.1.5",
          "form": "이대로 보내긴 아쉬운데",
          "original_form": "이대로 보내긴 아쉬운데",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002825.1.6",
          "form": "오늘은 치킨을 시켜먹느냐",
          "original_form": "오늘은 치킨을 시켜먹느냐",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.7",
          "form": "특별한 요리를 해야하나",
          "original_form": "특별한 요리를 해야하나",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.8",
          "form": "요리하는거좋아해??",
          "original_form": "요리하는거좋아해??",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.9",
          "form": "먹고사는게 중하니까 메뉴고민중 ㅋㅋㅋ",
          "original_form": "먹고사는게 중하니까 메뉴고민중 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.10",
          "form": "아니아니아니!",
          "original_form": "아니아니아니!",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.11",
          "form": "예전에는 좋은엄마 좋은아내로 살고파서",
          "original_form": "예전에는 좋은엄마 좋은아내로 살고파서",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.12",
          "form": "나름 열심히 햿는데",
          "original_form": "나름 열심히 햿는데",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.13",
          "form": "지금은 까졌어ㅜㅎ",
          "original_form": "지금은 까졌어ㅜㅎ",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.14",
          "form": "나는 자취했었어서 생존형 요리는 해 ㅋㅋ",
          "original_form": "나는 자취했었어서 생존형 요리는 해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002825.1.15",
          "form": "예전엔 집에서 육개장 감자탕 이런걸 끓였다면",
          "original_form": "예전엔 집에서 육개장 감자탕 이런걸 끓였다면",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002825.1.16",
          "form": "지금은 카레 주물럭 정도만",
          "original_form": "지금은 카레 주물럭 정도만",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002825.1.17",
          "form": "대충해서 해치우고픈",
          "original_form": "대충해서 해치우고픈",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002825.1.18",
          "form": "육개장 감자탕 멋있다",
          "original_form": "육개장 감자탕 멋있다",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002825.1.19",
          "form": "나는 생존형이라 김치찌개 카레 이런거 ㅎ",
          "original_form": "나는 생존형이라 김치찌개 카레 이런거 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002825.1.20",
          "form": "근데 고기요리좋아해서",
          "original_form": "근데 고기요리좋아해서",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002825.1.21",
          "form": "오 제법이야",
          "original_form": "오 제법이야",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002825.1.22",
          "form": "수비드해먹거나 오븐이용해서 스테이크 해먹어",
          "original_form": "수비드해먹거나 오븐이용해서 스테이크 해먹어",
          "speaker_id": "2",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002825.1.23",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002825.1.24",
          "form": "니가 더 멋있다",
          "original_form": "니가 더 멋있다",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002825.1.25",
          "form": "혼자 있어도 잘지냈겠어 ㅎ",
          "original_form": "혼자 있어도 잘지냈겠어 ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002825.1.26",
          "form": "고기에 대해선 진심이야 ㅎㅎㅎㅎ",
          "original_form": "고기에 대해선 진심이야 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002825.1.27",
          "form": "그건 나도 마찬가지야",
          "original_form": "그건 나도 마찬가지야",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002825.1.28",
          "form": "집에서 고기 먹고 뒷정리 싫어도 \r\n자주 고기구워 ㅋ",
          "original_form": "집에서 고기 먹고 뒷정리 싫어도 \r\n자주 고기구워 ㅋ",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002825.1.29",
          "form": "맞아  고기먹어야 힘이 나더라고 ㅎㅎ",
          "original_form": "맞아  고기먹어야 힘이 나더라고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002825.1.30",
          "form": "그래서 오늘 저녁 친구 만나 맛난거 뭐 먹을거야",
          "original_form": "그래서 오늘 저녁 친구 만나 맛난거 뭐 먹을거야",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002825.1.31",
          "form": "나도 육퇴하고 나가고 싶다 ㅎ",
          "original_form": "나도 육퇴하고 나가고 싶다 ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002825.1.32",
          "form": "오늘은 깐풍기",
          "original_form": "오늘은 깐풍기",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002825.1.33",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002825.1.34",
          "form": "지금 부모님이랑 살아?",
          "original_form": "지금 부모님이랑 살아?",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002825.1.35",
          "form": "아님 본가에 가나?",
          "original_form": "아님 본가에 가나?",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002825.1.36",
          "form": "친구랑 놀고 집에 혼자 가명 쓸쓸하잖아",
          "original_form": "친구랑 놀고 집에 혼자 가명 쓸쓸하잖아",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002825.1.37",
          "form": "지금은 캥거루족",
          "original_form": "지금은 캥거루족",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002825.1.38",
          "form": "집값이 너무 비싸서 부모님집에 얹혀살지",
          "original_form": "집값이 너무 비싸서 부모님집에 얹혀살지",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002825.1.39",
          "form": "엄마 말씀 잘 듣도록 ㅎ",
          "original_form": "엄마 말씀 잘 듣도록 ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002825.1.40",
          "form": "ㅋㅋㅋㅋ 하숙생처럼 아침에 출근했다 밤에 퇴근하고 집들어가서",
          "original_form": "ㅋㅋㅋㅋ 하숙생처럼 아침에 출근했다 밤에 퇴근하고 집들어가서",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002825.1.41",
          "form": "엄마얼굴 볼 시간도 잘 업어",
          "original_form": "엄마얼굴 볼 시간도 잘 업어",
          "speaker_id": "2",
          "time": "20210820 16:36"
        }
      ]
    }
  ]
