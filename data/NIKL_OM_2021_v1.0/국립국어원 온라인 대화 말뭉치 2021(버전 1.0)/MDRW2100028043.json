          "original_form": "아랏따",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.34",
          "form": "얼마 안 해요",
          "original_form": "얼마 안 해요",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
