      "utterance": [
        {
          "id": "MDRW2100028043.1.1",
          "form": "안봤을 것 같긴 한데",
          "original_form": "안봤을 것 같긴 한데",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028043.1.2",
          "form": "대탈출 보셨나요",
          "original_form": "대탈출 보셨나요",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028043.1.3",
          "form": "당연히 안 봤죠",
          "original_form": "당연히 안 봤죠",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028043.1.4",
          "form": "여고추리반은?",
          "original_form": "여고추리반은?",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028043.1.5",
          "form": "그건... 티빙을 안 써서",
          "original_form": "그건... 티빙을 안 써서",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028043.1.6",
          "form": "그렇군...",
          "original_form": "그렇군...",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028043.1.7",
          "form": "넷플릭스에서 해주면 좋을텐데...",
          "original_form": "넷플릭스에서 해주면 좋을텐데...",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028043.1.8",
          "form": "즐겨보는 tv 프로그램이 있으신지...",
          "original_form": "즐겨보는 tv 프로그램이 있으신지...",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.9",
          "form": "tv 잘 안 봐요...",
          "original_form": "tv 잘 안 봐요...",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.10",
          "form": "나랑 똑같네...",
          "original_form": "나랑 똑같네...",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.11",
          "form": "스우파도 거실에 틀어져있길래 잠깐 보다 눌러앉은 거라",
          "original_form": "스우파도 거실에 틀어져있길래 잠깐 보다 눌러앉은 거라",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.12",
          "form": "지금도 본방 안 보는",
          "original_form": "지금도 본방 안 보는",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.13",
          "form": "나랑똑같네",
          "original_form": "나랑똑같네",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.14",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028043.1.15",
          "form": "넷플릭스도 안 보지만",
          "original_form": "넷플릭스도 안 보지만",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.16",
          "form": "넷플 최고",
          "original_form": "넷플 최고",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.17",
          "form": "나도 넷플릭스 패밀리 가입은 해뒀는데",
          "original_form": "나도 넷플릭스 패밀리 가입은 해뒀는데",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.18",
          "form": "내가 보고 싶을 때 몰아서 볼 수 있다는 점이 최고라고",
          "original_form": "내가 보고 싶을 때 몰아서 볼 수 있다는 점이 최고라고",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.19",
          "form": "맨날 보고 싶은 거 있을 때만 들어가서",
          "original_form": "맨날 보고 싶은 거 있을 때만 들어가서",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.20",
          "form": "결국 안 보지만",
          "original_form": "결국 안 보지만",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.21",
          "form": "그냥 하나 보려고 3000원 내는 사람 됨",
          "original_form": "그냥 하나 보려고 3000원 내는 사람 됨",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.22",
          "form": "가성비 최악",
          "original_form": "가성비 최악",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028043.1.23",
          "form": "ㅠㅠ그니까",
          "original_form": "ㅠㅠ그니까",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.24",
          "form": "반역 아직 넷플에 있나",
          "original_form": "반역 아직 넷플에 있나",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.25",
          "form": "아직도 못본",
          "original_form": "아직도 못본",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.26",
          "form": "무슨 반역",
          "original_form": "무슨 반역",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.27",
          "form": "마마마?",
          "original_form": "마마마?",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.28",
          "form": "마마마",
          "original_form": "마마마",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.29",
          "form": "그건 첨부터 없었는데",
          "original_form": "그건 첨부터 없었는데",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028043.1.30",
          "form": "아니그러냐",
          "original_form": "아니그러냐",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
