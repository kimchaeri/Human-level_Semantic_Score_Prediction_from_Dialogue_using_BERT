      "utterance": [
        {
          "id": "MDRW2100013336.1.1",
          "form": "티비 자주 보세요?",
          "original_form": "티비 자주 보세요?",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013336.1.2",
          "form": "네 전 넷플릭스 티비로 자주 봐요ㅎㅎ",
          "original_form": "네 전 넷플릭스 티비로 자주 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013336.1.3",
          "form": "챙겨보는 프로는 놀면뭐하니랑 런닝맨 2개ㅋㅋㅋ",
          "original_form": "챙겨보는 프로는 놀면뭐하니랑 런닝맨 2개ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013336.1.4",
          "form": "전금쪽이ㅋ",
          "original_form": "전금쪽이ㅋ",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013336.1.5",
          "form": "아 저도 금쪽이 애청자 ㅋㅋㅋ",
          "original_form": "아 저도 금쪽이 애청자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013336.1.6",
          "form": "남일이 아닌ㅠ",
          "original_form": "남일이 아닌ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013336.1.7",
          "form": "맞아요..저희 신랑도 잘 보더라구요\n자기도 많이 배운다면서",
          "original_form": "맞아요..저희 신랑도 잘 보더라구요\n자기도 많이 배운다면서",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013336.1.8",
          "form": "앗 저와같으셔요",
          "original_form": "앗 저와같으셔요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100013336.1.9",
          "form": "ㅋㅋㅋㅋㅋ역시 오은영박사님은 육아의 신!",
          "original_form": "ㅋㅋㅋㅋㅋ역시 오은영박사님은 육아의 신!",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100013336.1.10",
          "form": "진짜 대단해요",
          "original_form": "진짜 대단해요",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100013336.1.11",
          "form": "그니까요~",
          "original_form": "그니까요~",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100013336.1.12",
          "form": "유투브도 자주 보시나요?",
          "original_form": "유투브도 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100013336.1.13",
          "form": "네 자주봐요",
          "original_form": "네 자주봐요",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100013336.1.14",
          "form": "저도 맨날 봐요ㅎㅎ\n유투브 한번 보기 시작하니 끝이 없더라고요",
          "original_form": "저도 맨날 봐요ㅎㅎ\n유투브 한번 보기 시작하니 끝이 없더라고요",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100013336.1.15",
          "form": "유투브.프리미엄도 결제했다니까요...ㅋㅋ",
          "original_form": "유투브.프리미엄도 결제했다니까요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100013336.1.16",
          "form": "애기 때문에 시작했는데 제가 더 잘 봐요",
          "original_form": "애기 때문에 시작했는데 제가 더 잘 봐요",
          "speaker_id": "2",
          "time": "20210825 19:39"
