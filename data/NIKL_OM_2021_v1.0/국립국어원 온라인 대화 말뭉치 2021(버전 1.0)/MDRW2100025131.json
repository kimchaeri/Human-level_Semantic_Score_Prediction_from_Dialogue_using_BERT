      "utterance": [
        {
          "id": "MDRW2100025131.1.1",
          "form": "흠 많아",
          "original_form": "흠 많아",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.2",
          "form": "총균쇠가 그 중 하나야",
          "original_form": "총균쇠가 그 중 하나야",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.3",
          "form": "이기적인 유전자",
          "original_form": "이기적인 유전자",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.4",
          "form": "같은 책 말하는거야",
          "original_form": "같은 책 말하는거야",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.5",
          "form": "총균쇠 읽어봤어?",
          "original_form": "총균쇠 읽어봤어?",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.6",
          "form": "어머 나도야 총균쇠는 항상 궁금해",
          "original_form": "어머 나도야 총균쇠는 항상 궁금해",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.7",
          "form": "아 그거 집에 있는데 읽다 말았어",
          "original_form": "아 그거 집에 있는데 읽다 말았어",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.8",
          "form": "맞아 근대 너무 두꺼워",
          "original_form": "맞아 근대 너무 두꺼워",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.9",
          "form": "우리 다음에 소규모 독서클럽을 만들어보자",
          "original_form": "우리 다음에 소규모 독서클럽을 만들어보자",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025131.1.10",
          "form": "읽기싫은 책을 같이 읽는거지",
          "original_form": "읽기싫은 책을 같이 읽는거지",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.11",
          "form": "사람들은 정말 대단해",
          "original_form": "사람들은 정말 대단해",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.12",
          "form": "좋은 생각이다",
          "original_form": "좋은 생각이다",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.13",
          "form": "그리고 가끔 좋아하는 책도 추천해주저",
          "original_form": "그리고 가끔 좋아하는 책도 추천해주저",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.14",
          "form": "또 고전들도 읽어봐야하는데",
          "original_form": "또 고전들도 읽어봐야하는데",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.15",
          "form": "너는 데미안 읽어봤니?",
          "original_form": "너는 데미안 읽어봤니?",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.16",
          "form": "황순원 책들도 귱금해",
          "original_form": "황순원 책들도 귱금해",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.17",
          "form": "데미안 읽었는데 기억이 안나",
          "original_form": "데미안 읽었는데 기억이 안나",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.18",
          "form": "소나기 작가 말하는거야??",
          "original_form": "소나기 작가 말하는거야??",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.19",
          "form": "너무 어려웠어",
          "original_form": "너무 어려웠어",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.20",
          "form": "또 읽으려니 엄두가ㅜ안나더라",
          "original_form": "또 읽으려니 엄두가ㅜ안나더라",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.21",
          "form": "나는 안읽어봤는데 너무 손이안간다",
          "original_form": "나는 안읽어봤는데 너무 손이안간다",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.22",
          "form": "응 맞아 다른 좋은 작품들이ㅜ많더라",
          "original_form": "응 맞아 다른 좋은 작품들이ㅜ많더라",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025131.1.23",
          "form": "잉 잘못누름",
          "original_form": "잉 잘못누름",
          "speaker_id": "1",
          "time": "20210901 23:21"
        }
      ]
    }
