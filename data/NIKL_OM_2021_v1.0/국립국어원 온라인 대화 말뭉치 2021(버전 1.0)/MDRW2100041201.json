      "utterance": [
        {
          "id": "MDRW2100041201.1.1",
          "form": "네, 별로안배고파서 디저트로 대신 먹었어요~",
          "original_form": "네, 별로안배고파서 디저트로 대신 먹었어요~",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100041201.1.2",
          "form": "그럴 때도 있죠",
          "original_form": "그럴 때도 있죠",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100041201.1.3",
          "form": "저는 갈치구이해서 식구들과 맛있게 먹었어요",
          "original_form": "저는 갈치구이해서 식구들과 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100041201.1.4",
          "form": "생선요리 좋아해요?",
          "original_form": "생선요리 좋아해요?",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100041201.1.5",
          "form": "아~ 저 어제 갈치구이먹었어요ㅎ",
          "original_form": "아~ 저 어제 갈치구이먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100041201.1.6",
          "form": "생선구이나 조림등 좋아해요~",
          "original_form": "생선구이나 조림등 좋아해요~",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100041201.1.7",
          "form": "그렇구나~ 생선은 구이도 맛있고 조림도 맛있죠",
          "original_form": "그렇구나~ 생선은 구이도 맛있고 조림도 맛있죠",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041201.1.8",
          "form": "가장 선호하는 생선은 뭔가요?",
          "original_form": "가장 선호하는 생선은 뭔가요?",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100041201.1.9",
          "form": "갈치,고등어요~",
          "original_form": "갈치,고등어요~",
          "speaker_id": "2",
          "time": "20210906 00:39"
        },
        {
          "id": "MDRW2100041201.1.10",
          "form": "둘다 맛있는 생선이죠",
          "original_form": "둘다 맛있는 생선이죠",
          "speaker_id": "1",
          "time": "20210906 06:57"
        },
        {
          "id": "MDRW2100041201.1.11",
          "form": "육고기랑 생선 중 어느것을 더 좋아하세요?",
          "original_form": "육고기랑 생선 중 어느것을 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 06:57"
        },
        {
          "id": "MDRW2100041201.1.12",
          "form": "고기를 더 좋아하는 편이에요",
          "original_form": "고기를 더 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210906 10:58"
        },
        {
          "id": "MDRW2100041201.1.13",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041201.1.14",
          "form": "그래서 아무래도 요리가 소 돼지 닭 오리 위주로 하게되는데",
          "original_form": "그래서 아무래도 요리가 소 돼지 닭 오리 위주로 하게되는데",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041201.1.15",
          "form": "남편은 생선요리를 좋아하거든요",
          "original_form": "남편은 생선요리를 좋아하거든요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041201.1.16",
          "form": "아,그렇군요~",
          "original_form": "아,그렇군요~",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041201.1.17",
          "form": "그래서 자꾸 생선은 언제 해줄꺼냐고 하니까",
          "original_form": "그래서 자꾸 생선은 언제 해줄꺼냐고 하니까",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041201.1.18",
          "form": "가끔씩 해줘야 합니다",
          "original_form": "가끔씩 해줘야 합니다",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041201.1.19",
          "form": "오늘 그래서 갈치구이를 먹었네요",
          "original_form": "오늘 그래서 갈치구이를 먹었네요",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100041201.1.20",
          "form": "가끔씩 그렇게 해드셔야겠네요~",
          "original_form": "가끔씩 그렇게 해드셔야겠네요~",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041201.1.21",
          "form": "전 귀찮기도하고 한그릇 요리로 자주먹는거 같아요",
          "original_form": "전 귀찮기도하고 한그릇 요리로 자주먹는거 같아요",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041201.1.22",
          "form": "한그릇 요리 좋죠",
          "original_form": "한그릇 요리 좋죠",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041201.1.23",
          "form": "전 반찬수를 늘리는 타입이라 식구들에게 구박을 좀 듣죠",
          "original_form": "전 반찬수를 늘리는 타입이라 식구들에게 구박을 좀 듣죠",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041201.1.24",
          "form": "반찬수를 줄여야 한다고 놓을데도 없다고",
          "original_form": "반찬수를 줄여야 한다고 놓을데도 없다고",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041201.1.25",
          "form": "근데 요리하는 제가 반찬 많은걸 좋아하다보니 그렇게 돼요",
          "original_form": "근데 요리하는 제가 반찬 많은걸 좋아하다보니 그렇게 돼요",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100041201.1.26",
          "form": "가족함께먹다보면 그러죠ㅎㅎ",
          "original_form": "가족함께먹다보면 그러죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:18"
        }
      ]
    }
  ]
