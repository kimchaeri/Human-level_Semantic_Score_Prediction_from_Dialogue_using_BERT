      "utterance": [
        {
          "id": "MDRW2100034721.1.1",
          "form": "ㅋㅋㅋ 콩깍지 씌였으니",
          "original_form": "ㅋㅋㅋ 콩깍지 씌였으니",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034721.1.2",
          "form": "그런갑다 말맞춰주면 기분좋잖아",
          "original_form": "그런갑다 말맞춰주면 기분좋잖아",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034721.1.3",
          "form": "지 사주에 물이 부족하댔는데",
          "original_form": "지 사주에 물이 부족하댔는데",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034721.1.4",
          "form": "그남자애가 바다해를 쓴대요 이름에",
          "original_form": "그남자애가 바다해를 쓴대요 이름에",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034721.1.5",
          "form": "지는 그리고 바다가좋대 ㅋㅋ",
          "original_form": "지는 그리고 바다가좋대 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034721.1.6",
          "form": "ㅋㅋ아줌마같아 벌써",
          "original_form": "ㅋㅋ아줌마같아 벌써",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034721.1.7",
          "form": "사주와 가족력이라니",
          "original_form": "사주와 가족력이라니",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034721.1.8",
          "form": "난 연애세포가 사멸됐는지",
          "original_form": "난 연애세포가 사멸됐는지",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034721.1.9",
          "form": "ㅋㅋ 222",
          "original_form": "ㅋㅋ 222",
          "speaker_id": "2",
          "time": "20210828 18:16"
        },
        {
          "id": "MDRW2100034721.1.10",
          "form": "헐 별 개 다 챠밍포인트야 ㅋㅋ",
          "original_form": "헐 별 개 다 챠밍포인트야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:16"
        },
        {
          "id": "MDRW2100034721.1.11",
          "form": "싶어서 콧웃음 나옴 ㅋㅋ",
          "original_form": "싶어서 콧웃음 나옴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:16"
        },
        {
          "id": "MDRW2100034721.1.12",
          "form": "ㅋㅋㅋㅋ상상도 못해봤다",
          "original_form": "ㅋㅋㅋㅋ상상도 못해봤다",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034721.1.13",
          "form": "그런걸봤다니..",
          "original_form": "그런걸봤다니..",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034721.1.14",
          "form": "아직 궁합본건 아니래여",
          "original_form": "아직 궁합본건 아니래여",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034721.1.15",
          "form": "태어난 시간을 안 알려줬다나 ㅋㅋ",
          "original_form": "태어난 시간을 안 알려줬다나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034721.1.16",
          "form": "ㅋㅋㅋ 아니 머그런걸봐",
          "original_form": "ㅋㅋㅋ 아니 머그런걸봐",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034721.1.17",
          "form": "ㅋㅋㅋ오컬트스러워  초반부터",
