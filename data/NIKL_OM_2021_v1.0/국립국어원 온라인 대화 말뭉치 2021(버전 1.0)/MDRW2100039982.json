      "utterance": [
        {
          "id": "MDRW2100039982.1.1",
          "form": "저도 다시 보고 눈물닦고 올게여",
          "original_form": "저도 다시 보고 눈물닦고 올게여",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039982.1.2",
          "form": "(웃낌주의)",
          "original_form": "(웃낌주의)",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039982.1.3",
          "form": "앗ㅋㅋㅋㅋㅋㅋ 너무웃기다고 하니 궁금하지만...ㅜㅜ",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ 너무웃기다고 하니 궁금하지만...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039982.1.4",
          "form": "우울할때를 위해 참을게요ㅋㅋㅋ",
          "original_form": "우울할때를 위해 참을게요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039982.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039982.1.6",
          "form": "유튜브나 영화는 어떤거 보세여",
          "original_form": "유튜브나 영화는 어떤거 보세여",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039982.1.7",
          "form": "영화는 잘안보고 유튜브로는 고양이 봐요ㅎㅎ",
          "original_form": "영화는 잘안보고 유튜브로는 고양이 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039982.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039982.1.9",
          "form": "재태크도 좀 보고요ㅎㅎ",
          "original_form": "재태크도 좀 보고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039982.1.10",
          "form": "신사임당님 같은?",
          "original_form": "신사임당님 같은?",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039982.1.11",
          "form": "네ㅎ  맞아요ㅋㅋㅋㄱ",
          "original_form": "네ㅎ  맞아요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039982.1.12",
          "form": "저는 좀 웃긴거 좋아해서",
          "original_form": "저는 좀 웃긴거 좋아해서",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100039982.1.13",
          "form": "침착맨 봐요",
          "original_form": "침착맨 봐요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100039982.1.14",
          "form": "침착맨 재밌나요?",
          "original_form": "침착맨 재밌나요?",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039982.1.15",
          "form": "짤로는 몇번 봤는데ㅎㅎ",
          "original_form": "짤로는 몇번 봤는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100039982.1.16",
          "form": "네에 게임은 안하는데",
          "original_form": "네에 게임은 안하는데",
          "speaker_id": "1",
          "time": "20210904 14:42"
        },
        {
          "id": "MDRW2100039982.1.17",
          "form": "최근에 입덕해버렸어요",
          "original_form": "최근에 입덕해버렸어요",
          "speaker_id": "1",
          "time": "20210904 14:42"
        },
        {
          "id": "MDRW2100039982.1.18",
          "form": "게임도 하나요?ㅎㅎㅎ",
          "original_form": "게임도 하나요?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100039982.1.19",
          "form": "네 그러시더라구요",
          "original_form": "네 그러시더라구요",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100039982.1.20",
          "form": "전 침펄풍 완전체 팬이예여",
          "original_form": "전 침펄풍 완전체 팬이예여",
          "speaker_id": "1",
