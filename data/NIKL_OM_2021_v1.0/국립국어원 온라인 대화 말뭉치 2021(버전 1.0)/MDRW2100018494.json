      "utterance": [
        {
          "id": "MDRW2100018494.1.1",
          "form": "오징어게임이 뭔지 궁금해졌네요!ㅎ.ㅎ",
          "original_form": "오징어게임이 뭔지 궁금해졌네요!ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210827 06:24"
        },
        {
          "id": "MDRW2100018494.1.2",
          "form": "저는 최근에 미추리를 다시봤는데 보신적 있으세요?",
          "original_form": "저는 최근에 미추리를 다시봤는데 보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210827 06:24"
        },
        {
          "id": "MDRW2100018494.1.3",
          "form": "아 유재석 나오는 프로그램이죠?",
          "original_form": "아 유재석 나오는 프로그램이죠?",
          "speaker_id": "1",
          "time": "20210827 06:24"
        },
        {
          "id": "MDRW2100018494.1.4",
          "form": "아맞아요 천만원 찾는 프로그램요!",
          "original_form": "아맞아요 천만원 찾는 프로그램요!",
          "speaker_id": "2",
          "time": "20210827 06:25"
        },
        {
          "id": "MDRW2100018494.1.5",
          "form": "이것도 시즌제인가요? 저는 아직 못보긴했어요ㅠㅠ",
          "original_form": "이것도 시즌제인가요? 저는 아직 못보긴했어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 06:25"
        },
        {
          "id": "MDRW2100018494.1.6",
          "form": "천만원 찾는 사람이",
          "original_form": "천만원 찾는 사람이",
          "speaker_id": "1",
          "time": "20210827 06:25"
        },
        {
          "id": "MDRW2100018494.1.7",
          "form": "가지는거에요??",
          "original_form": "가지는거에요??",
          "speaker_id": "1",
          "time": "20210827 06:25"
        },
        {
          "id": "MDRW2100018494.1.8",
          "form": "네 소속사랑 배분없이 순수 천만원이요! 방탈출같은 퀴즈들을 푸는건데",
          "original_form": "네 소속사랑 배분없이 순수 천만원이요! 방탈출같은 퀴즈들을 푸는건데",
          "speaker_id": "2",
          "time": "20210827 06:25"
        },
        {
          "id": "MDRW2100018494.1.9",
          "form": "헐 매회 천만원씩 줘요?",
          "original_form": "헐 매회 천만원씩 줘요?",
          "speaker_id": "1",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.10",
          "form": "예능적 요소가 많아서 보기 편해요",
          "original_form": "예능적 요소가 많아서 보기 편해요",
          "speaker_id": "2",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.11",
          "form": "찾으면요!",
          "original_form": "찾으면요!",
          "speaker_id": "2",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.12",
          "form": "우와 저도 방탈출 좋아해서",
          "original_form": "우와 저도 방탈출 좋아해서",
          "speaker_id": "1",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.13",
          "form": "저는 대탈출봐요!",
          "original_form": "저는 대탈출봐요!",
          "speaker_id": "1",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.14",
          "form": "근데 대신 찾아도 사람들한테 자기가 천만원 찾은사람인거 걸리면 못가져가요",
          "original_form": "근데 대신 찾아도 사람들한테 자기가 천만원 찾은사람인거 걸리면 못가져가요",
          "speaker_id": "2",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.15",
          "form": "아 대탈출 저 시즌 1만 다봤는데",
          "original_form": "아 대탈출 저 시즌 1만 다봤는데",
          "speaker_id": "2",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.16",
          "form": "저두 좋아해요!",
          "original_form": "저두 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.17",
          "form": "헉 얘기만 들어도 재밌네요ㅋㅋㅋ",
          "original_form": "헉 얘기만 들어도 재밌네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.18",
          "form": "꼭 봐야겠어용",
          "original_form": "꼭 봐야겠어용",
          "speaker_id": "1",
          "time": "20210827 06:26"
        },
        {
          "id": "MDRW2100018494.1.19",
          "form": "시즌 1 뭐가 제일 재밌으셨어요?",
          "original_form": "시즌 1 뭐가 제일 재밌으셨어요?",
          "speaker_id": "1",
          "time": "20210827 06:27"
        },
        {
          "id": "MDRW2100018494.1.20",
          "form": "최근 대탈출 다시 시작했죠?",
          "original_form": "최근 대탈출 다시 시작했죠?",
          "speaker_id": "2",
          "time": "20210827 06:27"
        },
        {
          "id": "MDRW2100018494.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 06:27"
        },
        {
          "id": "MDRW2100018494.1.22",
          "form": "지금 방영중이에요",
          "original_form": "지금 방영중이에요",
          "speaker_id": "1",
          "time": "20210827 06:27"
        },
        {
          "id": "MDRW2100018494.1.23",
          "form": "저는 그 좀비특집이요! 최근 대탈출 시즌1보다 재밌나요?",
          "original_form": "저는 그 좀비특집이요! 최근 대탈출 시즌1보다 재밌나요?",
          "speaker_id": "2",
          "time": "20210827 06:27"
        }
      ]
