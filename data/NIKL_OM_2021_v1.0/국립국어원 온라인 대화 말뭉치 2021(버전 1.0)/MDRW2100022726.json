      "utterance": [
        {
          "id": "MDRW2100022726.1.1",
          "form": "아아 그거 짤로 조금봤어요",
          "original_form": "아아 그거 짤로 조금봤어요",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022726.1.2",
          "form": "재밋던가요??",
          "original_form": "재밋던가요??",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022726.1.3",
          "form": "ㅋㅋㅋㅋㅋ전 재밌었어요",
          "original_form": "ㅋㅋㅋㅋㅋ전 재밌었어요",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.4",
          "form": "거기서 신천지 같은 장면 나오는데 너무 공감됐어요",
          "original_form": "거기서 신천지 같은 장면 나오는데 너무 공감됐어요",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.5",
          "form": "오 그래요 한번봐야겟어요 ㅋㅋㅋㅋ",
          "original_form": "오 그래요 한번봐야겟어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.6",
          "form": "아 신천지요? ㅋㅋㅋㅋㅋ",
          "original_form": "아 신천지요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.7",
          "form": "안녕하세요~~인상이 참 좋아보이세요~~",
          "original_form": "안녕하세요~~인상이 참 좋아보이세요~~",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.8",
          "form": "원래 소재인가요?",
          "original_form": "원래 소재인가요?",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.9",
          "form": "이러면서 말 거는 도를 아십니까 있잖아요",
          "original_form": "이러면서 말 거는 도를 아십니까 있잖아요",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.11",
          "form": "아 왜안쳐져",
          "original_form": "아 왜안쳐져",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022726.1.12",
          "form": "원래 소재는요",
          "original_form": "원래 소재는요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.13",
          "form": "분노조절에 어려움 겪고 있는 남주와",
          "original_form": "분노조절에 어려움 겪고 있는 남주와",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.14",
          "form": "피해 망상 있는 여주의 케미에요",
          "original_form": "피해 망상 있는 여주의 케미에요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.15",
          "form": "오오 독특하네요 ㅋㅋㅋㅋㅋ",
          "original_form": "오오 독특하네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.16",
          "form": "봐봐야겟어요",
          "original_form": "봐봐야겟어요",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.17",
          "form": "근데 그 남주에게 인상이 참좋으세요~ 하니까",
          "original_form": "근데 그 남주에게 인상이 참좋으세요~ 하니까",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.18",
          "form": "남주 막 화가 나가지고 ㅋㅋㅋㅋ",
          "original_form": "남주 막 화가 나가지고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.20",
          "form": "근데 그 둘이가 또 바로 옆집이어가지고",
          "original_form": "근데 그 둘이가 또 바로 옆집이어가지고",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.21",
          "form": "생활하면서 일어나는 에피소드로 이루어져있어요",
          "original_form": "생활하면서 일어나는 에피소드로 이루어져있어요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022726.1.22",
          "form": "ㅋㅋㅋㅋㅋ재밋겟는데요?",
          "original_form": "ㅋㅋㅋㅋㅋ재밋겟는데요?",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.23",
          "form": "근데 막 우와 엄청 재밌다! 라기보다는",
          "original_form": "근데 막 우와 엄청 재밌다! 라기보다는",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.24",
          "form": "안그래두 저 내일 백신맞는데",
          "original_form": "안그래두 저 내일 백신맞는데",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.25",
          "form": "볼거찾는중이엇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "볼거찾는중이엇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.26",
          "form": "뭔가 가볍게 보기 좋았어요",
          "original_form": "뭔가 가볍게 보기 좋았어요",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.27",
          "form": "쉬면서 보려구요",
          "original_form": "쉬면서 보려구요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.28",
          "form": "그냥 아무 생각없이 ~ 이 씁슬한 현실 잘 반영한거 같은 느낌이였어요",
          "original_form": "그냥 아무 생각없이 ~ 이 씁슬한 현실 잘 반영한거 같은 느낌이였어요",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.29",
          "form": "그런거 좋아해여 ㅋㅋㅋㅋ",
          "original_form": "그런거 좋아해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022726.1.30",
          "form": "그래서 개인적으로 전 좀 맘에 들더라구요",
          "original_form": "그래서 개인적으로 전 좀 맘에 들더라구요",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022726.1.31",
          "form": "근데 나중에 얘기가 나와요 왜 이 둘이가 그런 아픔을 갖게 됐는지 후속스토리?",
          "original_form": "근데 나중에 얘기가 나와요 왜 이 둘이가 그런 아픔을 갖게 됐는지 후속스토리?",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022726.1.32",
          "form": "현실감있는 내용 좋아하세요?",
          "original_form": "현실감있는 내용 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022726.1.33",
          "form": "그거 보니까 주인공들을 좀 잘 이해하게 됐어요",
          "original_form": "그거 보니까 주인공들을 좀 잘 이해하게 됐어요",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022726.1.34",
          "form": "네네 좋아해요",
          "original_form": "네네 좋아해요",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022726.1.35",
          "form": "아아 그런거구나",
          "original_form": "아아 그런거구나",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022726.1.36",
          "form": "저 ISTJ여서 말도 안되는 스토리 보면 집중 안돼요",
          "original_form": "저 ISTJ여서 말도 안되는 스토리 보면 집중 안돼요",
          "speaker_id": "1",
