      "utterance": [
        {
          "id": "MDRW2100008902.1.1",
          "form": "쿠킹덤 요즘 자주 하냐",
          "original_form": "쿠킹덤 요즘 자주 하냐",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100008902.1.2",
          "form": "난 요즘 귀찮아서",
          "original_form": "난 요즘 귀찮아서",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100008902.1.3",
          "form": "되게 관성적으로 하고있어...",
          "original_form": "되게 관성적으로 하고있어...",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100008902.1.4",
          "form": "나 1일 1접속만 겨우 한다 ㅋㅋㅋㅋ",
          "original_form": "나 1일 1접속만 겨우 한다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008902.1.5",
          "form": "솔직히 할 겜 없어서 하는 거지",
          "original_form": "솔직히 할 겜 없어서 하는 거지",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008902.1.6",
          "form": "쿠킹덤 노잼이야 ㅠㅠ",
          "original_form": "쿠킹덤 노잼이야 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008902.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나도 나도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나도 나도",
          "speaker_id": "1",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008902.1.8",
          "form": "진짜 할거없어서...꾸역꾸역 하는 느낌...?",
          "original_form": "진짜 할거없어서...꾸역꾸역 하는 느낌...?",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100008902.1.9",
          "form": "+이때까지 키워놓은거 아까워서ㅠㅠㅋㅋㅋㅋ",
          "original_form": "+이때까지 키워놓은거 아까워서ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100008902.1.10",
          "form": "ㅁㅈ.... 그냥... 하는 거지 뭐...",
          "original_form": "ㅁㅈ.... 그냥... 하는 거지 뭐...",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100008902.1.11",
          "form": "근데 대체가능 게임 나오든가 이것조차도 귀찮아지면 걍 접을 거 같아 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 대체가능 게임 나오든가 이것조차도 귀찮아지면 걍 접을 거 같아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100008902.1.12",
          "form": "근데 아직 뭐...할만한 모바일겜 나온게 없어 ㅠㅠ",
          "original_form": "근데 아직 뭐...할만한 모바일겜 나온게 없어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100008902.1.13",
          "form": "마자 ㅋㅋㅋ 이것도 귀차나지면 접을듯",
          "original_form": "마자 ㅋㅋㅋ 이것도 귀차나지면 접을듯",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100008902.1.14",
          "form": "안그래도 우리 길드 접는 사람들이 슬슬 많아져서ㅜ 얼마전에 길마 부길마까지 접어부림...",
          "original_form": "안그래도 우리 길드 접는 사람들이 슬슬 많아져서ㅜ 얼마전에 길마 부길마까지 접어부림...",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100008902.1.15",
          "form": "ㅇㅇ....진짜 할게없음ㅠ 모바일배그 재밌다던데 난...컨트롤이 영...그래서....",
          "original_form": "ㅇㅇ....진짜 할게없음ㅠ 모바일배그 재밌다던데 난...컨트롤이 영...그래서....",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100008902.1.16",
          "form": "ㅁㅈ... 난 동생이 길마였는데 접고 나가서 내가 길마됐따... 길드 망해감 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅁㅈ... 난 동생이 길마였는데 접고 나가서 내가 길마됐따... 길드 망해감 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100008902.1.17",
          "form": "쿠킹덤도 슬슬 라이트하게 겜하는 사람들한텐 끝물인듯",
          "original_form": "쿠킹덤도 슬슬 라이트하게 겜하는 사람들한텐 끝물인듯",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100008902.1.18",
          "form": "배그같은건 난 ㅠㅠㅠ 컨트롤이 구려서 못하겠어...ㄸㄹㄹ...",
          "original_form": "배그같은건 난 ㅠㅠㅠ 컨트롤이 구려서 못하겠어...ㄸㄹㄹ...",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100008902.1.19",
          "form": "억지로 길마됐뿟네 ㅋㅋㅋㅋㅋ",
          "original_form": "억지로 길마됐뿟네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100008902.1.20",
          "form": "ㅁㅈㅁㅈ... 진짜 점점 게임이... 고인물들이 넘쳐나게됏어",
          "original_form": "ㅁㅈㅁㅈ... 진짜 점점 게임이... 고인물들이 넘쳐나게됏어",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100008902.1.21",
          "form": "나도....^^ 양학 당할게 틀림업따...",
          "original_form": "나도....^^ 양학 당할게 틀림업따...",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100008902.1.22",
          "form": "나중에 너네 길드 망하면 우리길드 와라 ㅋㅋㅋㅋ",
          "original_form": "나중에 너네 길드 망하면 우리길드 와라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008902.1.23",
          "form": "우리길드도...망해가고 있지만 ㅎ...",
          "original_form": "우리길드도...망해가고 있지만 ㅎ...",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008902.1.24",
          "form": "암튼 뭔가 모바일겜 할만한게 안나오는 느낌이야 요즘 ㅜㅠ",
          "original_form": "암튼 뭔가 모바일겜 할만한게 안나오는 느낌이야 요즘 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008902.1.25",
          "form": "앜ㅋㅋㅋㅋㅋ 다행히...누군가 이어받긴했오",
          "original_form": "앜ㅋㅋㅋㅋㅋ 다행히...누군가 이어받긴했오",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100008902.1.26",
          "form": "근데 내가 게임을 그만두는게 더 빠를거같아 ㅋㅋㅋㅋ",
          "original_form": "근데 내가 게임을 그만두는게 더 빠를거같아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100008902.1.27",
          "form": "ㅇㅈㅇㅈㅜㅠ 진짜 할만한게 없름........반성해랏",
          "original_form": "ㅇㅈㅇㅈㅜㅠ 진짜 할만한게 없름........반성해랏",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008902.1.28",
          "form": "너 제5인격 알아?",
          "original_form": "너 제5인격 알아?",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008902.1.29",
          "form": "ㅇㅇ 알아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 알아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100008902.1.30",
          "form": "근데 해본적은 없다 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 해본적은 없다 ㅋㅋㅋㅋㅋ",
