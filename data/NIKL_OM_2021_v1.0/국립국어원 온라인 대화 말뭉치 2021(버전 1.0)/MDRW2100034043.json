      "utterance": [
        {
          "id": "MDRW2100034043.1.1",
          "form": "아는 프로게이머가 있으신가요?",
          "original_form": "아는 프로게이머가 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100034043.1.2",
          "form": "오... 그 콩진호님만 알아요",
          "original_form": "오... 그 콩진호님만 알아요",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100034043.1.3",
          "form": "좋아하는 프로기이머 있으세요?",
          "original_form": "좋아하는 프로기이머 있으세요?",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100034043.1.4",
          "form": "저는 페이커만 알아요ㅋㅋ",
          "original_form": "저는 페이커만 알아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100034043.1.5",
          "form": "아 저도 그 분 들어봤어요 ..! 잘은 모르지만요..ㅎㅎ",
          "original_form": "아 저도 그 분 들어봤어요 ..! 잘은 모르지만요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100034043.1.6",
          "form": "프로게이머에 대해서는 저희 모두 관심이 없나봐요 ㅎㅎ",
          "original_form": "프로게이머에 대해서는 저희 모두 관심이 없나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100034043.1.7",
          "form": "그러게요ㅋㅋ",
          "original_form": "그러게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100034043.1.8",
          "form": "예전에 닌텐도 해보셨어요?",
          "original_form": "예전에 닌텐도 해보셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100034043.1.9",
          "form": "포켓몬 많이 했어요ㅎㅎ",
          "original_form": "포켓몬 많이 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100034043.1.10",
          "form": "닌텐도로 포켓몬은 안해봤네요 저는 동물의숲이랑 그 무슨 .. 미니게임 여러가지 있는거 많이했어요",
          "original_form": "닌텐도로 포켓몬은 안해봤네요 저는 동물의숲이랑 그 무슨 .. 미니게임 여러가지 있는거 많이했어요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100034043.1.11",
          "form": "슈퍼마리오도 많이 했어요",
          "original_form": "슈퍼마리오도 많이 했어요",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100034043.1.12",
          "form": "오 맞아요 마리오도 있었군요",
          "original_form": "오 맞아요 마리오도 있었군요",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100034043.1.13",
          "form": "재밌었어요ㅋㅋ",
          "original_form": "재밌었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100034043.1.14",
          "form": "슈퍼마리오, 뉴슈퍼마리오 많이 했죠ㅋㅋ",
          "original_form": "슈퍼마리오, 뉴슈퍼마리오 많이 했죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100034043.1.15",
          "form": "맞아요 닌텐도위로 마리오카트도 재밌었죠ㅎㅎ",
          "original_form": "맞아요 닌텐도위로 마리오카트도 재밌었죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100034043.1.16",
          "form": "추억의 게임들이었죠ㅋㅋ",
          "original_form": "추억의 게임들이었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100034043.1.17",
          "form": "그쵸 ㅋㅋ 예전에 완전 유행이었죠",
          "original_form": "그쵸 ㅋㅋ 예전에 완전 유행이었죠",
