      "utterance": [
        {
          "id": "MDRW2100029559.1.1",
          "form": "근데 이제는 스펙이 걱정이다",
          "original_form": "근데 이제는 스펙이 걱정이다",
          "speaker_id": "1",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029559.1.2",
          "form": "내 스펙으로 멀쩡하게 취업할 수 있을까",
          "original_form": "내 스펙으로 멀쩡하게 취업할 수 있을까",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029559.1.3",
          "form": "어우 취업은 다 된다야",
          "original_form": "어우 취업은 다 된다야",
          "speaker_id": "2",
          "time": "20210903 10:22"
        },
        {
          "id": "MDRW2100029559.1.4",
          "form": "그래서 울면서 서포터즈 하고 있잖아",
          "original_form": "그래서 울면서 서포터즈 하고 있잖아",
          "speaker_id": "1",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100029559.1.5",
          "form": "그거도 스펙티라면 스펙이지 어디서 서포터츠를 하겠어...",
          "original_form": "그거도 스펙티라면 스펙이지 어디서 서포터츠를 하겠어...",
          "speaker_id": "2",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100029559.1.6",
          "form": "넌 꽤 잘찾아서 한거라고 생각함",
          "original_form": "넌 꽤 잘찾아서 한거라고 생각함",
          "speaker_id": "2",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100029559.1.7",
          "form": "친구가 일했으니까...ㅎㅎ",
          "original_form": "친구가 일했으니까...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100029559.1.8",
          "form": "얼레벌레라도 국가기관 일 받아서 다행이라고 생각해",
          "original_form": "얼레벌레라도 국가기관 일 받아서 다행이라고 생각해",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029559.1.9",
          "form": "역시 주변에 사람들이 있으면 뭐라도 알게 된다...",
          "original_form": "역시 주변에 사람들이 있으면 뭐라도 알게 된다...",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029559.1.10",
          "form": "이래서 인맥이 중요하다고 하나봐",
          "original_form": "이래서 인맥이 중요하다고 하나봐",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029559.1.11",
          "form": "진짜 인맥인맥 얘기하는거 심하다고 생각했는데",
          "original_form": "진짜 인맥인맥 얘기하는거 심하다고 생각했는데",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029559.1.12",
          "form": "그럴만한 것 같아",
          "original_form": "그럴만한 것 같아",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029559.1.13",
          "form": "나 진쩌 관광과 간거도 친척언니가 관광일본어과여서 관광과 간거고",
          "original_form": "나 진쩌 관광과 간거도 친척언니가 관광일본어과여서 관광과 간거고",
          "speaker_id": "2",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100029559.1.14",
          "form": "심리학도 주변에서 학점은행제 한다는 사람 있으니까 나도 해볼까 하고 한거구...",
          "original_form": "심리학도 주변에서 학점은행제 한다는 사람 있으니까 나도 해볼까 하고 한거구...",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029559.1.15",
          "form": "학교 행정 업무도 소개시켜줘서 한거고",
          "original_form": "학교 행정 업무도 소개시켜줘서 한거고",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029559.1.16",
          "form": "이게 정말 친구의 친구도 다 인맥이다... 건너건너 소개 가능하잖아",
          "original_form": "이게 정말 친구의 친구도 다 인맥이다... 건너건너 소개 가능하잖아",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029559.1.17",
          "form": "당신도...주변사람 도움을 많이 받는구나",
          "original_form": "당신도...주변사람 도움을 많이 받는구나",
          "speaker_id": "1",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029559.1.18",
          "form": "그치만 이런 사람들 있는게 소중한거지",
          "original_form": "그치만 이런 사람들 있는게 소중한거지",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029559.1.19",
          "form": "그리고 name3 언니도 따지고 보면 복지 계열이니",
          "original_form": "그리고 &name3& 언니도 따지고 보면 복지 계열이니",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029559.1.20",
          "form": "영향을 받았다면 받은 거 아닐까나",
          "original_form": "영향을 받았다면 받은 거 아닐까나",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029559.1.21",
          "form": "그럴수도 있겠다",
          "original_form": "그럴수도 있겠다",
          "speaker_id": "1",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029559.1.22",
          "form": "나는...진짜 내 하고 싶은대로 하나봐ㅋㅋ",
          "original_form": "나는...진짜 내 하고 싶은대로 하나봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029559.1.23",
          "form": "왜잉 자기가 하고 싶은거 찾아서 하는 건 좋은 일이야",
          "original_form": "왜잉 자기가 하고 싶은거 찾아서 하는 건 좋은 일이야",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029559.1.24",
          "form": "남이 시켜서 하는 건 아니잖아",
          "original_form": "남이 시켜서 하는 건 아니잖아",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029559.1.25",
          "form": "그냥 내 직업 뭐할지 고민하는 과정이지 전부",
          "original_form": "그냥 내 직업 뭐할지 고민하는 과정이지 전부",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029559.1.26",
          "form": "영향을 받았다면 예전에 지인이 뮤지컬 좋아해서",
          "original_form": "영향을 받았다면 예전에 지인이 뮤지컬 좋아해서",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029559.1.27",
          "form": "데뷔한 최애를 자주 보여줬는데",
          "original_form": "데뷔한 최애를 자주 보여줬는데",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029559.1.28",
          "form": "그 사람 덕에 연극 입문하고 배우 좋아하면서 기획과 생각하게 된 거니까",
          "original_form": "그 사람 덕에 연극 입문하고 배우 좋아하면서 기획과 생각하게 된 거니까",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029559.1.29",
          "form": "그 영향은 하나 있는듯",
          "original_form": "그 영향은 하나 있는듯",
          "speaker_id": "1",
          "time": "20210903 10:42"
        }
