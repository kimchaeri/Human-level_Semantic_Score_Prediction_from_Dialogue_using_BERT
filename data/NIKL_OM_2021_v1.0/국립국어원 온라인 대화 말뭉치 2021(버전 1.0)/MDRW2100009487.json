      "utterance": [
        {
          "id": "MDRW2100009487.1.1",
          "form": "ㅋㅋㅋㅋㅋ우리 건강 중요할 나이지",
          "original_form": "ㅋㅋㅋㅋㅋ우리 건강 중요할 나이지",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009487.1.2",
          "form": "손목도 아프고 모가지도 아프고 허리도 아프고",
          "original_form": "손목도 아프고 모가지도 아프고 허리도 아프고",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009487.1.3",
          "form": "새롭게 발등도 아프다",
          "original_form": "새롭게 발등도 아프다",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009487.1.4",
          "form": "있잖아 애기볼때말야",
          "original_form": "있잖아 애기볼때말야",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009487.1.5",
          "form": "애기를 양손으로 안다보니까 어깨가 안쪽으로 굽어지거든??",
          "original_form": "애기를 양손으로 안다보니까 어깨가 안쪽으로 굽어지거든??",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009487.1.6",
          "form": "근데 그걸 내가 인식을 못했나봐",
          "original_form": "근데 그걸 내가 인식을 못했나봐",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009487.1.7",
          "form": "나 지금 완전 굽은어깨랑 거북목이랑 등때문에 고생중",
          "original_form": "나 지금 완전 굽은어깨랑 거북목이랑 등때문에 고생중",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009487.1.8",
          "form": "그니까 꼭 틈틈히 스트레칭 하구 ㅠㅠㅠㅠㅠㅠㅠ 폼롤러 같은걸로 등 펴주고 ㅠㅠㅠㅠ",
          "original_form": "그니까 꼭 틈틈히 스트레칭 하구 ㅠㅠㅠㅠㅠㅠㅠ 폼롤러 같은걸로 등 펴주고 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009487.1.9",
          "form": "이걸 말해주고 싶었어",
          "original_form": "이걸 말해주고 싶었어",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009487.1.10",
          "form": "헐....내 얘기네 나도 이렇게 둥글게 아기 받쳐 안거든",
          "original_form": "헐....내 얘기네 나도 이렇게 둥글게 아기 받쳐 안거든",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009487.1.11",
          "form": "서럽네",
          "original_form": "서럽네",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009487.1.12",
          "form": "운동하니?",
          "original_form": "운동하니?",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009487.1.13",
          "form": "나 집에 있는 폼롤러 한번도 안 써봤다 ㅋㅋㅋ",
          "original_form": "나 집에 있는 폼롤러 한번도 안 써봤다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.14",
          "form": "아니요 그런걸 할리가....",
          "original_form": "아니요 그런걸 할리가....",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ모야 나보고만 하랴",
          "original_form": "ㅋㅋㅋㅋㅋㅋ모야 나보고만 하랴",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.16",
          "form": "우리 꼭 하자",
          "original_form": "우리 꼭 하자",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.17",
          "form": "그래도 스트레칭은 틈틈히 해 ㅠㅠㅠㅠ",
          "original_form": "그래도 스트레칭은 틈틈히 해 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.18",
          "form": "내일부터...",
          "original_form": "내일부터...",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.19",
          "form": "언제였지... 이걸 둘째 낳고 복직하면서 인식한듯",
          "original_form": "언제였지... 이걸 둘째 낳고 복직하면서 인식한듯",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009487.1.20",
          "form": "아 스트레칭은 운동으로 안 쳐줘? 그럼 나 운동 못 해",
          "original_form": "아 스트레칭은 운동으로 안 쳐줘? 그럼 나 운동 못 해",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009487.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009487.1.22",
          "form": "노인",
          "original_form": "노인",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009487.1.23",
          "form": "티비보니까",
          "original_form": "티비보니까",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009487.1.24",
          "form": "80노인도 헬스하더라",
          "original_form": "80노인도 헬스하더라",
          "speaker_id": "2",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009487.1.25",
          "form": "80먹으면 헬스할 시간이 나나봐",
          "original_form": "80먹으면 헬스할 시간이 나나봐",
          "speaker_id": "1",
          "time": "20210822 23:53"
        },
        {
          "id": "MDRW2100009487.1.26",
          "form": "울끈불끈",
          "original_form": "울끈불끈",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009487.1.27",
          "form": "아 ㅋㅋㅋㅋ 너 너무 바쁘구나",
          "original_form": "아 ㅋㅋㅋㅋ 너 너무 바쁘구나",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009487.1.28",
          "form": "하긴 출근하고 애보고 언제 운동하냐",
          "original_form": "하긴 출근하고 애보고 언제 운동하냐",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009487.1.29",
          "form": "이게 주기적으로 운동욕구가 샘솟아서",
          "original_form": "이게 주기적으로 운동욕구가 샘솟아서",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009487.1.30",
          "form": "애기 남편한테 맡기고 한밤중에 헬스한적있음",
          "original_form": "애기 남편한테 맡기고 한밤중에 헬스한적있음",
          "speaker_id": "1",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009487.1.31",
          "form": "이열 대단한데",
          "original_form": "이열 대단한데",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.32",
          "form": "멋있다",
          "original_form": "멋있다",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.33",
          "form": "10시에 나가서 한시간 운동하고 거의 12시쯤 귀가",
          "original_form": "10시에 나가서 한시간 운동하고 거의 12시쯤 귀가",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.34",
          "form": "한달 끊고 딱 3주나감",
          "original_form": "한달 끊고 딱 3주나감",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.35",
          "form": "오래하지도 않았는데 너무 피곤한 일정이다",
          "original_form": "오래하지도 않았는데 너무 피곤한 일정이다",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.36",
          "form": "계단 오르기도 한달정도 했었구",
          "original_form": "계단 오르기도 한달정도 했었구",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.37",
          "form": "못할짓이네...",
          "original_form": "못할짓이네...",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.38",
          "form": "땅끄부부 유튜브도 한달정도 했나",
          "original_form": "땅끄부부 유튜브도 한달정도 했나",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.39",
          "form": "일주어터가 아니라 한달어터",
          "original_form": "일주어터가 아니라 한달어터",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.40",
          "form": "뭘 많이 했다야",
          "original_form": "뭘 많이 했다야",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.41",
          "form": "멋져멋져",
          "original_form": "멋져멋져",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009487.1.42",
          "form": "아기와 운동하기",
          "original_form": "아기와 운동하기",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.43",
          "form": "근데 지금 아마 인생 최대 몸무게를 찍어가는것같거든",
          "original_form": "근데 지금 아마 인생 최대 몸무게를 찍어가는것같거든",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.44",
          "form": "임신중에",
          "original_form": "임신중에",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.45",
          "form": "임신중 제외",
          "original_form": "임신중 제외",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.46",
          "form": "검색햇는데",
          "original_form": "검색햇는데",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.47",
          "form": "출산 후 하지 않지",
          "original_form": "출산 후 하지 않지",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.48",
          "form": "그런게 가능할리가",
          "original_form": "그런게 가능할리가",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100009487.1.49",
          "form": "아니 나 앉자마자 머리 얇아지고 앞머리쪽 탈모왔다고 ㅠㅠㅠ",
          "original_form": "아니 나 앉자마자 머리 얇아지고 앞머리쪽 탈모왔다고 ㅠㅠㅠ",
          "speaker_id": "1",
