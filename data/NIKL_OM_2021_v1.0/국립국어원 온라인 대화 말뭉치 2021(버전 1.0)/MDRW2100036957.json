      "utterance": [
        {
          "id": "MDRW2100036957.1.1",
          "form": "유튜브 자주보세요?",
          "original_form": "유튜브 자주보세요?",
          "speaker_id": "1",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100036957.1.2",
          "form": "종종 봐요ㅎ",
          "original_form": "종종 봐요ㅎ",
          "speaker_id": "2",
          "time": "20210831 15:40"
        },
        {
          "id": "MDRW2100036957.1.3",
          "form": "보시는 유튜버 있나요",
          "original_form": "보시는 유튜버 있나요",
          "speaker_id": "2",
          "time": "20210831 15:40"
        },
        {
          "id": "MDRW2100036957.1.4",
          "form": "구독하고보시는 유튜브누구있어요",
          "original_form": "구독하고보시는 유튜브누구있어요",
          "speaker_id": "1",
          "time": "20210831 15:40"
        },
        {
          "id": "MDRW2100036957.1.5",
          "form": "전 해쭈 하 하하  똥괭이네",
          "original_form": "전 해쭈 하 하하  똥괭이네",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036957.1.6",
          "form": "등등많아봐여",
          "original_form": "등등많아봐여",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036957.1.7",
          "form": "저는 참피디하고 옥뷰티랑 땅끄부부 요정도...?",
          "original_form": "저는 참피디하고 옥뷰티랑 땅끄부부 요정도...?",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036957.1.8",
          "form": "땅끄부부로 홈트도 하시나봐여",
          "original_form": "땅끄부부로 홈트도 하시나봐여",
          "speaker_id": "1",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036957.1.9",
          "form": "네ㅋㅋ홈트합니당",
          "original_form": "네ㅋㅋ홈트합니당",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036957.1.10",
          "form": "홈트유튜브보세요?",
          "original_form": "홈트유튜브보세요?",
          "speaker_id": "2",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036957.1.11",
          "form": "전 운동을...안합니다...!",
          "original_form": "전 운동을...안합니다...!",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036957.1.12",
          "form": "ㅋㅋㅋㅋㅋ홈트하시면 몇분정도하시나요",
          "original_form": "ㅋㅋㅋㅋㅋ홈트하시면 몇분정도하시나요",
          "speaker_id": "1",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036957.1.13",
          "form": "아하...!",
          "original_form": "아하...!",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036957.1.14",
          "form": "30분...?ㅜㅜㅋㅋㅋ",
          "original_form": "30분...?ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036957.1.15",
          "form": "이것도 매일하는게 아니고 하고싶을 때나 시간날 때 만해서...",
          "original_form": "이것도 매일하는게 아니고 하고싶을 때나 시간날 때 만해서...",
          "speaker_id": "2",
          "time": "20210831 15:47"
        },
        {
          "id": "MDRW2100036957.1.16",
          "form": "그쳐ㅜ하고나면 땀쫙빠지나여?",
          "original_form": "그쳐ㅜ하고나면 땀쫙빠지나여?",
          "speaker_id": "1",
          "time": "20210831 15:48"
        },
        {
          "id": "MDRW2100036957.1.17",
          "form": "아후...엄청 더워요ㅜㅜ",
          "original_form": "아후...엄청 더워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100036957.1.18",
          "form": "땅끄부부 영상에 쉬운 영상 위주로 하는 편인데도 힘든건 힘들어요ㅋㅋㅋㅋㅋ",
          "original_form": "땅끄부부 영상에 쉬운 영상 위주로 하는 편인데도 힘든건 힘들어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:49"
        },
        {
          "id": "MDRW2100036957.1.19",
          "form": "운동 지대로한다는 증거져",
          "original_form": "운동 지대로한다는 증거져",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100036957.1.20",
          "form": "저 한번시도는해봤었는데",
          "original_form": "저 한번시도는해봤었는데",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100036957.1.21",
          "form": "그뒤로 땅크부부...영상쳐다도안봐요 넘힘들어서",
          "original_form": "그뒤로 땅크부부...영상쳐다도안봐요 넘힘들어서",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100036957.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:50"
        },
        {
          "id": "MDRW2100036957.1.23",
          "form": "ㅋㅋㅋㅋㅋㅜㅠ 땅끄부부가 강력하긴해요...ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅜㅠ 땅끄부부가 강력하긴해요...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100036957.1.24",
          "form": "숨쉬기만하던저에겐 너무 하드했어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "숨쉬기만하던저에겐 너무 하드했어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:54"
        }
      ]
    }
