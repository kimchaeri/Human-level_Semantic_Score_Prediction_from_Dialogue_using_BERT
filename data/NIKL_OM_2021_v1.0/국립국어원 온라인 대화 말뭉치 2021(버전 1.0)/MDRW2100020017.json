        },
        {
          "id": "MDRW2100020017.1.4",
          "form": "나 이사 준비중이야",
          "original_form": "나 이사 준비중이야",
          "speaker_id": "1",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100020017.1.5",
          "form": "벌써 이사하려고 어디인데",
          "original_form": "벌써 이사하려고 어디인데",
          "speaker_id": "2",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100020017.1.6",
          "form": "벌써가 아니지",
          "original_form": "벌써가 아니지",
          "speaker_id": "1",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100020017.1.7",
          "form": "이사짐싸고 있는 거야",
          "original_form": "이사짐싸고 있는 거야",
          "speaker_id": "2",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100020017.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100020017.1.9",
          "form": "맞아 보통 힘든 집안일이 아니네",
          "original_form": "맞아 보통 힘든 집안일이 아니네",
          "speaker_id": "1",
          "time": "20210829 02:05"
        },
        {
          "id": "MDRW2100020017.1.10",
          "form": "짐 정리하려면 힘들겠다",
          "original_form": "짐 정리하려면 힘들겠다",
          "speaker_id": "2",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100020017.1.11",
          "form": "우리집이 이렇게 더러운 줄 몰랐어",
          "original_form": "우리집이 이렇게 더러운 줄 몰랐어",
          "speaker_id": "1",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100020017.1.12",
          "form": "이사하는 김에 청소하는 거지 뭐",
          "original_form": "이사하는 김에 청소하는 거지 뭐",
          "speaker_id": "2",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100020017.1.13",
          "form": "버려얗ㄹ 것도 많아",
          "original_form": "버려얗ㄹ 것도 많아",
          "speaker_id": "1",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100020017.1.14",
          "form": "안 쓰는 물건들 위주로 버리니?",
          "original_form": "안 쓰는 물건들 위주로 버리니?",
          "speaker_id": "2",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100020017.1.15",
          "form": "맞아 친구들 나눠주기도 하고",
          "original_form": "맞아 친구들 나눠주기도 하고",
          "speaker_id": "1",
          "time": "20210829 02:06"
        },
        {
          "id": "MDRW2100020017.1.16",
          "form": "요즘은 재활용도 잘된다더라",
          "original_form": "요즘은 재활용도 잘된다더라",
