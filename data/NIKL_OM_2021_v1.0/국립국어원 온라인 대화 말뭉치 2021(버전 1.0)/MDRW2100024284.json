      "utterance": [
        {
          "id": "MDRW2100024284.1.1",
          "form": "남동생  아직 독립전인가보군요",
          "original_form": "남동생  아직 독립전인가보군요",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024284.1.2",
          "form": "네...독립전인데백수예요",
          "original_form": "네...독립전인데백수예요",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024284.1.3",
          "form": "집안일도 안해요ㅎㅎ",
          "original_form": "집안일도 안해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024284.1.4",
          "form": "부모님이 이해심이넓으신가봐요",
          "original_form": "부모님이 이해심이넓으신가봐요",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024284.1.5",
          "form": "철없는 동생이다 생각하고 싶지만 도를 넘네요...",
          "original_form": "철없는 동생이다 생각하고 싶지만 도를 넘네요...",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024284.1.6",
          "form": "맨날 승질내요ㅡㅡ",
          "original_form": "맨날 승질내요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024284.1.7",
          "form": "집안분위기엉망",
          "original_form": "집안분위기엉망",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024284.1.8",
          "form": "진짜 할 말 많아요ㅋ이거관련은...",
          "original_form": "진짜 할 말 많아요ㅋ이거관련은...",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024284.1.9",
          "form": "동생이 빨리 철이들어야할텐데",
          "original_form": "동생이 빨리 철이들어야할텐데",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024284.1.10",
          "form": "가족들 사이가 안좋아지는 느낌!!느낌이 아니라 사실!!!!",
          "original_form": "가족들 사이가 안좋아지는 느낌!!느낌이 아니라 사실!!!!",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024284.1.11",
          "form": "남자는 자기자식을낳아야 어른되는듯",
          "original_form": "남자는 자기자식을낳아야 어른되는듯",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024284.1.12",
          "form": "서로 뾰족하게 말하더라구요",
          "original_form": "서로 뾰족하게 말하더라구요",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024284.1.13",
          "form": "가족들끼리...스트레스받으니깐",
          "original_form": "가족들끼리...스트레스받으니깐",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024284.1.14",
          "form": "결혼도 못할듯요ㅡㅡㅋ",
          "original_form": "결혼도 못할듯요ㅡㅡㅋ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.15",
          "form": "서로상처주는거알지만 그말을 해버려야 속이시원함",
          "original_form": "서로상처주는거알지만 그말을 해버려야 속이시원함",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.16",
          "form": "결혼이란그런것",
          "original_form": "결혼이란그런것",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.17",
          "form": "가족이란그런것ㄱㅋ",
          "original_form": "가족이란그런것ㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.18",
          "form": "문제는 남동생빼고 부모님들끼리 그러세요",
          "original_form": "문제는 남동생빼고 부모님들끼리 그러세요",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.19",
          "form": "진짜 속상함...",
          "original_form": "진짜 속상함...",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.20",
          "form": "빨리 결혼하면 후련할텐데",
          "original_form": "빨리 결혼하면 후련할텐데",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.21",
          "form": "미안할뿐 그 누군가에게",
          "original_form": "미안할뿐 그 누군가에게",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024284.1.22",
          "form": "어디계신가요 그분",
          "original_form": "어디계신가요 그분",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024284.1.23",
          "form": "집안일도 1도 안함ㅠ",
          "original_form": "집안일도 1도 안함ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024284.1.24",
          "form": "결혼하면 하드라구요",
          "original_form": "결혼하면 하드라구요",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024284.1.25",
          "form": "근데 누군가를 데리고 와도 제가 미안할듯ㅠㅠ",
          "original_form": "근데 누군가를 데리고 와도 제가 미안할듯ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024284.1.26",
          "form": "친정오빠도",
          "original_form": "친정오빠도",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024284.1.27",
          "form": "아니 근데 새언니없이만났을때",
          "original_form": "아니 근데 새언니없이만났을때",
          "speaker_id": "2",
          "time": "20210831 00:09"
        }
      ]
    }
