      "utterance": [
        {
          "id": "MDRW2100030388.1.1",
          "form": "비뢰도 기억나시나요",
          "original_form": "비뢰도 기억나시나요",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030388.1.2",
          "form": "몇 년 동안 책을 안 쓰다가",
          "original_form": "몇 년 동안 책을 안 쓰다가",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.3",
          "form": "독자들이 엄청 기다렸는데 책을 안 썼거든요",
          "original_form": "독자들이 엄청 기다렸는데 책을 안 썼거든요",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.4",
          "form": "그런데 그 비뢰도를 기본으로 해서 웹툰이 나왔더라고요",
          "original_form": "그런데 그 비뢰도를 기본으로 해서 웹툰이 나왔더라고요",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.5",
          "form": "책 내용이 결말이 안 났지만",
          "original_form": "책 내용이 결말이 안 났지만",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.6",
          "form": "이름만 기억나요",
          "original_form": "이름만 기억나요",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.7",
          "form": "웹툰에서는 나름 결말을 정해 놓고 만든다고 하더라고요",
          "original_form": "웹툰에서는 나름 결말을 정해 놓고 만든다고 하더라고요",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.8",
          "form": "안 읽었어요",
          "original_form": "안 읽었어요",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.9",
          "form": "사람들이 욕 엄청 했는데",
          "original_form": "사람들이 욕 엄청 했는데",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030388.1.10",
          "form": "인기가 많았나 봐요",
          "original_form": "인기가 많았나 봐요",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.11",
          "form": "웹툰은 볼만한 것 같아요",
          "original_form": "웹툰은 볼만한 것 같아요",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.12",
          "form": "만화책도 완결 안 된게 많죠",
          "original_form": "만화책도 완결 안 된게 많죠",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.13",
          "form": "묵향처럼 인기 많았던 거예요",
          "original_form": "묵향처럼 인기 많았던 거예요",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.14",
          "form": "옛날 만화책 들이요",
          "original_form": "옛날 만화책 들이요",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.15",
          "form": "독자들 만 불쌍해요",
          "original_form": "독자들 만 불쌍해요",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.16",
          "form": "맞아요 결말이 궁금해요",
          "original_form": "맞아요 결말이 궁금해요",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.17",
          "form": "나의 아름다운 그대에게 정말 좋아했었는데",
          "original_form": "나의 아름다운 그대에게 정말 좋아했었는데",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.18",
          "form": "묵향은 다 봤나요",
          "original_form": "묵향은 다 봤나요",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030388.1.19",
          "form": "일본에서 드라마로 만든 거 봤는데 오글거리 더라고요",
          "original_form": "일본에서 드라마로 만든 거 봤는데 오글거리 더라고요",
          "speaker_id": "2",
          "time": "20210904 00:28"
        }
      ]
