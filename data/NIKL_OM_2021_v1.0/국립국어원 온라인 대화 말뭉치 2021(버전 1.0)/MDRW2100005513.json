      "utterance": [
        {
          "id": "MDRW2100005513.1.1",
          "form": "가족들도 원래 다들 주식 하셨어요?",
          "original_form": "가족들도 원래 다들 주식 하셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005513.1.2",
          "form": "아니요 저 하는거 보고 첨해요 ㅋㅋ",
          "original_form": "아니요 저 하는거 보고 첨해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005513.1.3",
          "form": "아빠는 자금이 좀 있으셔서 자꾸 주식을 따로사서 걱정이예요",
          "original_form": "아빠는 자금이 좀 있으셔서 자꾸 주식을 따로사서 걱정이예요",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005513.1.4",
          "form": "엇ㅋㅋㅋ뭔가 개미는 작은돈 넣으면 많이 오르고",
          "original_form": "엇ㅋㅋㅋ뭔가 개미는 작은돈 넣으면 많이 오르고",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005513.1.5",
          "form": "많이넣으면 똘어지는 경우가 많다구",
          "original_form": "많이넣으면 똘어지는 경우가 많다구",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005513.1.6",
          "form": "ㅜㅜ항상 조심하게 되던데..!!",
          "original_form": "ㅜㅜ항상 조심하게 되던데..!!",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005513.1.7",
          "form": "맞아요 진리죠",
          "original_form": "맞아요 진리죠",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005513.1.8",
          "form": "큰돈을 잘 투척하셔요",
          "original_form": "큰돈을 잘 투척하셔요",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005513.1.9",
          "form": "제가 혼자 걱정중요",
          "original_form": "제가 혼자 걱정중요",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005513.1.10",
          "form": "그럼 더 열심히 알아보게 되시겠네요..",
          "original_form": "그럼 더 열심히 알아보게 되시겠네요..",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005513.1.11",
          "form": "저 모르게 사세요 ㅋㅋ",
          "original_form": "저 모르게 사세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005513.1.12",
          "form": "사실 저희 아버지도 자꾸 살짝살짝 단타하시는거 같아요 비밀로ㅋㅋㅋㅋ",
          "original_form": "사실 저희 아버지도 자꾸 살짝살짝 단타하시는거 같아요 비밀로ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005513.1.13",
          "form": "요번에 다이소 된거 알았네요 ㅡㅡ",
          "original_form": "요번에 다이소 된거 알았네요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.14",
          "form": "종목이 다양해요",
          "original_form": "종목이 다양해요",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.15",
          "form": "다이소?는 뭐에요?",
          "original_form": "다이소?는 뭐에요?",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.16",
          "form": "아ㅋㅋㅋㅋ다이솤ㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ다이솤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 다이소 운영중이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 다이소 운영중이에요",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.18",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.19",
          "form": "저도 있어요",
          "original_form": "저도 있어요",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.20",
          "form": "몇년전에 들어간거 못빠진게 있어서ㅋㅋㅋㅋ",
          "original_form": "몇년전에 들어간거 못빠진게 있어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.21",
          "form": "해외주식도 ㅜㅜ",
          "original_form": "해외주식도 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005513.1.22",
          "form": "저도 존버중요 ㅋㅋ",
          "original_form": "저도 존버중요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005513.1.23",
          "form": "가족분들이랑은 카톡창에서 대화하시는거에요?",
          "original_form": "가족분들이랑은 카톡창에서 대화하시는거에요?",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005513.1.24",
          "form": "아니면 오프라인으로?",
          "original_form": "아니면 오프라인으로?",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005513.1.25",
          "form": "엄마 아빠가 카톡을 잘못하셔서",
          "original_form": "엄마 아빠가 카톡을 잘못하셔서",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005513.1.26",
          "form": "영상통화요",
          "original_form": "영상통화요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005513.1.27",
          "form": "요금이 헉이요",
          "original_form": "요금이 헉이요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005513.1.28",
          "form": "300분무료라",
          "original_form": "300분무료라",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005513.1.29",
          "form": "으잉? 무료 지나서 통화하신적도 많아요?",
          "original_form": "으잉? 무료 지나서 통화하신적도 많아요?",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005513.1.30",
          "form": "차라리 페이스톡을 쓰시지!!!",
          "original_form": "차라리 페이스톡을 쓰시지!!!",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005513.1.31",
          "form": "몇번돼요 ㅋㅋ",
          "original_form": "몇번돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005513.1.32",
          "form": "그러려고 하는데 영상통화가 좋으시데요",
          "original_form": "그러려고 하는데 영상통화가 좋으시데요",
