          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038749.1.4",
          "form": "뭐드셨어요 name1님",
          "original_form": "뭐드셨어요 &name1&님",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038749.1.5",
          "form": "신라면볶음면이요",
          "original_form": "신라면볶음면이요",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038749.1.6",
          "form": "엘에이갈비",
          "original_form": "엘에이갈비",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038749.1.7",
          "form": "오이",
          "original_form": "오이",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038749.1.8",
          "form": "맛있는거 드셨네요",
          "original_form": "맛있는거 드셨네요",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038749.1.9",
          "form": "재택근무라 하루종일 맛난거 마니 드셧겟어오",
          "original_form": "재택근무라 하루종일 맛난거 마니 드셧겟어오",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038749.1.10",
          "form": "근데 좀 다 별로였어요",
          "original_form": "근데 좀 다 별로였어요",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038749.1.11",
          "form": "오잉 왜요",
          "original_form": "오잉 왜요",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038749.1.12",
          "form": "몰라요 입맛이없나",
          "original_form": "몰라요 입맛이없나",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038749.1.13",
          "form": "그냥 별맛없는 애들 이었어용",
          "original_form": "그냥 별맛없는 애들 이었어용",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038749.1.14",
          "form": "ㅋㅋㅋㅋ요즘 머가 젤 맛잇엇어여",
          "original_form": "ㅋㅋㅋㅋ요즘 머가 젤 맛잇엇어여",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038749.1.15",
          "form": "요즘 마시는거용",
          "original_form": "요즘 마시는거용",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038749.1.16",
          "form": "아니면 과일",
          "original_form": "아니면 과일",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100038749.1.17",
          "form": "역시 마른 사람들은 그런게 맛잇군여",
          "original_form": "역시 마른 사람들은 그런게 맛잇군여",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100038749.1.18",
          "form": "ㅇㅖ?",
          "original_form": "ㅇㅖ?",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038749.1.19",
          "form": "우ㅏㄴ랜 마라샹궈좋아해욤",
          "original_form": "우ㅏㄴ랜 마라샹궈좋아해욤",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038749.1.20",
          "form": "머좋아하세요",
          "original_form": "머좋아하세요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038749.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038749.1.22",
          "form": "저는 요즘 입맛 다시 돌아오는 중이라",
          "original_form": "저는 요즘 입맛 다시 돌아오는 중이라",
