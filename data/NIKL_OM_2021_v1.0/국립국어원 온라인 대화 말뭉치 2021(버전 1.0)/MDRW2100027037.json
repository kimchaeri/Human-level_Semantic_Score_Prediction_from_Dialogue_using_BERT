        {
          "id": "MDRW2100027037.1.1",
          "form": "영화 좋아해요?",
          "original_form": "영화 좋아해요?",
          "speaker_id": "2",
          "time": "20210902 08:26"
        },
        {
          "id": "MDRW2100027037.1.2",
          "form": "영화는 잘안봐요 영상을 잘 못봐서ㅜㅜ",
          "original_form": "영화는 잘안봐요 영상을 잘 못봐서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100027037.1.3",
          "form": "영화좋아하세요?",
          "original_form": "영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100027037.1.4",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210902 08:31"
        },
        {
          "id": "MDRW2100027037.1.5",
          "form": "왜 잘 못보세요?",
          "original_form": "왜 잘 못보세요?",
          "speaker_id": "2",
          "time": "20210902 08:31"
        },
        {
          "id": "MDRW2100027037.1.6",
          "form": "그냥 집중이 잘안돼요 ㅋㅋㅋ",
          "original_form": "그냥 집중이 잘안돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100027037.1.7",
          "form": "드라마도 그래서 잘안보구요",
          "original_form": "드라마도 그래서 잘안보구요",
          "speaker_id": "1",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100027037.1.8",
          "form": "아하 그럼 콘서트",
          "original_form": "아하 그럼 콘서트",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100027037.1.9",
          "form": "뮤지컬 이런것도",
          "original_form": "뮤지컬 이런것도",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100027037.1.10",
          "form": "별로세요?",
          "original_form": "별로세요?",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100027037.1.11",
          "form": "아 그런건 괜찮아요 ㅋㅋㅋ",
          "original_form": "아 그런건 괜찮아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027037.1.12",
          "form": "영화도 영화관에서보는건 나름 괜찮아요",
          "original_form": "영화도 영화관에서보는건 나름 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027037.1.13",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100027037.1.14",
          "form": "티비나 휴대폰 이렁게 별로궁요?",
          "original_form": "티비나 휴대폰 이렁게 별로궁요?",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100027037.1.15",
          "form": "네네 잘 안봐요ㅠ 집중이 안되는 느낌이에요",
          "original_form": "네네 잘 안봐요ㅠ 집중이 안되는 느낌이에요",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100027037.1.16",
          "form": "유튜브는 좀 봐요 ㅎㅎ",
          "original_form": "유튜브는 좀 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100027037.1.17",
          "form": "유투브는 뭐 보세뇨?",
          "original_form": "유투브는 뭐 보세뇨?",
