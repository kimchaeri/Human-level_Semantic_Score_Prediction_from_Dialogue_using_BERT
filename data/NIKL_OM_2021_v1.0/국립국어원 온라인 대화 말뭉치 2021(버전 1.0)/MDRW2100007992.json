      "utterance": [
        {
          "id": "MDRW2100007992.1.1",
          "form": "무슨 일 하는지 여쭤봐도 될까요?",
          "original_form": "무슨 일 하는지 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007992.1.2",
          "form": "그냥 회사다녀요~",
          "original_form": "그냥 회사다녀요~",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100007992.1.3",
          "form": "일반직장인이죠",
          "original_form": "일반직장인이죠",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100007992.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100007992.1.5",
          "form": "저는 대학생이구 디자인 관련해서 취업 준비하고 있어요",
          "original_form": "저는 대학생이구 디자인 관련해서 취업 준비하고 있어요",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100007992.1.6",
          "form": "아 아직 졸업안하셨군여",
          "original_form": "아 아직 졸업안하셨군여",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100007992.1.7",
          "form": "올해 신입생이에요ㅋㅋ 21학번",
          "original_form": "올해 신입생이에요ㅋㅋ 21학번",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100007992.1.8",
          "form": "그럼 그냥 대학생이시구나 ㅋㅋㅋ",
          "original_form": "그럼 그냥 대학생이시구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100007992.1.9",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100007992.1.10",
          "form": "방학인데 요즘은 뭐하시나요? 알바?",
          "original_form": "방학인데 요즘은 뭐하시나요? 알바?",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100007992.1.11",
          "form": "사람 상대하는걸 힘들어해서 알바는 안하고 있고",
          "original_form": "사람 상대하는걸 힘들어해서 알바는 안하고 있고",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100007992.1.12",
          "form": "포트폴리오 쌓고 있어요",
          "original_form": "포트폴리오 쌓고 있어요",
          "speaker_id": "1",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100007992.1.13",
          "form": "알차게 보내고 계시네요",
          "original_form": "알차게 보내고 계시네요",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100007992.1.14",
          "form": "나중에 외주받는게 목표에요",
          "original_form": "나중에 외주받는게 목표에요",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100007992.1.15",
          "form": "외주요?",
          "original_form": "외주요?",
          "speaker_id": "2",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100007992.1.16",
          "form": "디자인 관련해서 공부하고 있거든요",
          "original_form": "디자인 관련해서 공부하고 있거든요",
          "speaker_id": "1",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100007992.1.17",
          "form": "열심히 하세요~ 그렇게 목표가 있으면 잘될거에요",
          "original_form": "열심히 하세요~ 그렇게 목표가 있으면 잘될거에요",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100007992.1.18",
          "form": "ㅎㅎ 좋은 말씀 감사합니다",
          "original_form": "ㅎㅎ 좋은 말씀 감사합니다",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100007992.1.19",
          "form": "대학생이면 방학이라 시간여유 많아서 좋겠어요",
          "original_form": "대학생이면 방학이라 시간여유 많아서 좋겠어요",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100007992.1.20",
          "form": "학기중보단 여유있긴 한데 그렇다고 편하지는 않네요",
          "original_form": "학기중보단 여유있긴 한데 그렇다고 편하지는 않네요",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100007992.1.21",
          "form": "2학기 내용도 예습하고 포트폴리오 작성하느라ㅠ",
          "original_form": "2학기 내용도 예습하고 포트폴리오 작성하느라ㅠ",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100007992.1.22",
          "form": "전 대학다닐때 취업걱정안하고 겁나 놀았는데",
          "original_form": "전 대학다닐때 취업걱정안하고 겁나 놀았는데",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100007992.1.23",
          "form": "주변에서 압박을 많이 주셔서 가만히 있기 참 그렇더라구요ㅠ",
          "original_form": "주변에서 압박을 많이 주셔서 가만히 있기 참 그렇더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100007992.1.24",
          "form": "1학년인데요?",
          "original_form": "1학년인데요?",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100007992.1.25",
          "form": "압박을주나요?",
          "original_form": "압박을주나요?",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100007992.1.26",
          "form": "교수님이 선배들 실패사례를 자주 얘기하셔서",
          "original_form": "교수님이 선배들 실패사례를 자주 얘기하셔서",
          "speaker_id": "1",
          "time": "20210824 09:05"
        },
        {
          "id": "MDRW2100007992.1.27",
          "form": "성공사례를 얘기해야지",
          "original_form": "성공사례를 얘기해야지",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100007992.1.28",
          "form": "왜 실패사례를...",
          "original_form": "왜 실패사례를...",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100007992.1.29",
          "form": "애들 노력하라고 일부러 자극주신거 같아요",
          "original_form": "애들 노력하라고 일부러 자극주신거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100007992.1.30",
          "form": "그럴수도 있겠네요",
          "original_form": "그럴수도 있겠네요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        }
      ]
    }
