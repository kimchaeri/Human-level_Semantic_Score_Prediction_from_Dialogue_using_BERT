      "utterance": [
        {
          "id": "MDRW2100024928.1.1",
          "form": "올리브 치아바타... 좋아하세요?",
          "original_form": "올리브 치아바타... 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024928.1.2",
          "form": "네 좋아해여 ㅋㅋㅋ 마이 페이보릿 디절트",
          "original_form": "네 좋아해여 ㅋㅋㅋ 마이 페이보릿 디절트",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024928.1.3",
          "form": "완전 맛있어 쫄깃쫄깃",
          "original_form": "완전 맛있어 쫄깃쫄깃",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024928.1.4",
          "form": "디저트는 달달한 거 아닌가요?",
          "original_form": "디저트는 달달한 거 아닌가요?",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024928.1.5",
          "form": "난 칼국수가좋은데",
          "original_form": "난 칼국수가좋은데",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024928.1.6",
          "form": "원래 단걸 별로 안좋아해서 식사빵같은게 좋아영",
          "original_form": "원래 단걸 별로 안좋아해서 식사빵같은게 좋아영",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024928.1.7",
          "form": "국수도 맛있지",
          "original_form": "국수도 맛있지",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024928.1.8",
          "form": "콩국수 칼국수",
          "original_form": "콩국수 칼국수",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024928.1.9",
          "form": "아니 나오늘 바지락칼국수 먹었는디 진짜 맛있어서 공중제비뛸뻔",
          "original_form": "아니 나오늘 바지락칼국수 먹었는디 진짜 맛있어서 공중제비뛸뻔",
          "speaker_id": "2",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024928.1.10",
          "form": "명동쪽에 있는 명동교자 가보셈 완전 존맛",
          "original_form": "명동쪽에 있는 명동교자 가보셈 완전 존맛",
          "speaker_id": "1",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100024928.1.11",
          "form": "난 명동칼국수 스타일을 더 좋아해",
          "original_form": "난 명동칼국수 스타일을 더 좋아해",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024928.1.12",
          "form": "와 교자 맛있겠당",
          "original_form": "와 교자 맛있겠당",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024928.1.13",
          "form": "오늘 9000원짜리 만두 먹었는데",
          "original_form": "오늘 9000원짜리 만두 먹었는데",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024928.1.14",
          "form": "왕만두",
          "original_form": "왕만두",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024928.1.15",
          "form": "양많고 맛있었음",
          "original_form": "양많고 맛있었음",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024928.1.16",
          "form": "왕만두 짱좋아",
          "original_form": "왕만두 짱좋아",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024928.1.17",
          "form": "두부많이 들어간거",
          "original_form": "두부많이 들어간거",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024928.1.18",
          "form": "다먹고 프릳츠 갔었는데",
          "original_form": "다먹고 프릳츠 갔었는데",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024928.1.19",
          "form": "빵보단 커피",
          "original_form": "빵보단 커피",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024928.1.20",
          "form": "추천",
          "original_form": "추천",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024928.1.21",
          "form": "빵이 뭐가있었는데",
          "original_form": "빵이 뭐가있었는데",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024928.1.22",
          "form": "멀라 그 커스터드올라간거 먹어보고싶었음",
          "original_form": "멀라 그 커스터드올라간거 먹어보고싶었음",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024928.1.23",
          "form": "바게트랑 뺑오쇼콜라먹었는데 초코가 개미똥꼬만큼 들어있어서",
          "original_form": "바게트랑 뺑오쇼콜라먹었는데 초코가 개미똥꼬만큼 들어있어서",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024928.1.24",
          "form": "약간 실망",
          "original_form": "약간 실망",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024928.1.25",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.26",
          "form": "나 노티드도넛 먹고싶어",
          "original_form": "나 노티드도넛 먹고싶어",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.27",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.28",
          "form": "노티드 도넛 내취향 아닐거같은데",
          "original_form": "노티드 도넛 내취향 아닐거같은데",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.29",
          "form": "순수 탄수화물에",
          "original_form": "순수 탄수화물에",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.30",
          "form": "먹어보고싶긴해",
          "original_form": "먹어보고싶긴해",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.31",
          "form": "기름",
          "original_form": "기름",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024928.1.32",
          "form": "티긴거",
          "original_form": "티긴거",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024928.1.33",
          "form": "아니근데 거기는 티긴건ㅇ ㅏ닌거같은데",
          "original_form": "아니근데 거기는 티긴건ㅇ ㅏ닌거같은데",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024928.1.34",
          "form": "빵먼저 튀기나",
          "original_form": "빵먼저 튀기나",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024928.1.35",
          "form": "나 오늘 효뜨갈거야",
          "original_form": "나 오늘 효뜨갈거야",
          "speaker_id": "1",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100024928.1.36",
          "form": "효뜨가서 쌀국수 먹을거야",
          "original_form": "효뜨가서 쌀국수 먹을거야",
