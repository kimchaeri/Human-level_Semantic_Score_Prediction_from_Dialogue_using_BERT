        {
          "id": "MDRW2100002632.1.1",
          "form": "여행 좋아요?",
          "original_form": "여행 좋아요?",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002632.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002632.1.3",
          "form": "주로 바닷가쪽 가는 거 선호해요",
          "original_form": "주로 바닷가쪽 가는 거 선호해요",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002632.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002632.1.5",
          "form": "저도 바다요",
          "original_form": "저도 바다요",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002632.1.6",
          "form": "어디 좋아하세요?",
          "original_form": "어디 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002632.1.7",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002632.1.8",
          "form": "남해 너무 좋죠",
          "original_form": "남해 너무 좋죠",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002632.1.9",
          "form": "가면 주로 뭐 하시나요",
          "original_form": "가면 주로 뭐 하시나요",
          "speaker_id": "2",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002632.1.10",
          "form": "바다가요",
          "original_form": "바다가요",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002632.1.11",
          "form": "음식은 어떤거 드세요",
          "original_form": "음식은 어떤거 드세요",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002632.1.12",
          "form": "멸치 쌈밥이요",
          "original_form": "멸치 쌈밥이요",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002632.1.13",
          "form": "남해에 멸치쌈밥이 유명한가요?",
          "original_form": "남해에 멸치쌈밥이 유명한가요?",
          "speaker_id": "2",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002632.1.14",
          "form": "멸치쌈밥은 먹어본적이 없어서 궁금하네요",
          "original_form": "멸치쌈밥은 먹어본적이 없어서 궁금하네요",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002632.1.15",
          "form": "맛나요",
          "original_form": "맛나요",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002632.1.16",
          "form": "그렇군요 바다가면 바닷가 걸으세요?",
          "original_form": "그렇군요 바다가면 바닷가 걸으세요?",
          "speaker_id": "2",
          "time": "20210820 14:38"
        },
        {
