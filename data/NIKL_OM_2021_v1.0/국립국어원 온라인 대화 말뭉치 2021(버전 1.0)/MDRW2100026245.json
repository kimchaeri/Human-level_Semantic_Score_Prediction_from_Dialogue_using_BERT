      "utterance": [
        {
          "id": "MDRW2100026245.1.1",
          "form": "드라마 자주 보시나요??",
          "original_form": "드라마 자주 보시나요??",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026245.1.2",
          "form": "네 요즘은 슬의생에 빠져있어요",
          "original_form": "네 요즘은 슬의생에 빠져있어요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026245.1.3",
          "form": "슬의생 너무 재밌죠!!",
          "original_form": "슬의생 너무 재밌죠!!",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.4",
          "form": "이번주 결방이라",
          "original_form": "이번주 결방이라",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.5",
          "form": "너무 아쉬워요",
          "original_form": "너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.6",
          "form": "아 맞아요 ㅠㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.7",
          "form": "아니 왜 이 타이밍에서 결방을 하조..?",
          "original_form": "아니 왜 이 타이밍에서 결방을 하조..?",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.8",
          "form": "준완이 진짜 ㅠㅜㅜ 그런 대사 날려놓고",
          "original_form": "준완이 진짜 ㅠㅜㅜ 그런 대사 날려놓고",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.9",
          "form": "결방...",
          "original_form": "결방...",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.10",
          "form": "그니까요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "그니까요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.11",
          "form": "일주일을 어떻게 버틸지...",
          "original_form": "일주일을 어떻게 버틸지...",
          "speaker_id": "1",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100026245.1.12",
          "form": "...그쵸..? 정말 괴로워요",
          "original_form": "...그쵸..? 정말 괴로워요",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.13",
          "form": "요즘 그거 말곤...볼거도없어서",
          "original_form": "요즘 그거 말곤...볼거도없어서",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.14",
          "form": "그거 하나보는데 ㅠㅠㅠ",
          "original_form": "그거 하나보는데 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.15",
          "form": "님은 다른 드라마 보시나요?",
          "original_form": "님은 다른 드라마 보시나요?",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.16",
          "form": "맞아요 ㅠㅠㅠ 저도 넷플릭스 결제하고",
          "original_form": "맞아요 ㅠㅠㅠ 저도 넷플릭스 결제하고",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.17",
          "form": "슬의생 하나 보고 있네요",
          "original_form": "슬의생 하나 보고 있네요",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.18",
          "form": "저는 미드도 좋아하는데 요새는 재미있는게 없어서 잘 안보는 것 같아요",
          "original_form": "저는 미드도 좋아하는데 요새는 재미있는게 없어서 잘 안보는 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.19",
          "form": "아이고 ㅋㅋㅋㅋ 요즘 디피 재밌다고하더라구요",
          "original_form": "아이고 ㅋㅋㅋㅋ 요즘 디피 재밌다고하더라구요",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100026245.1.20",
          "form": "아...그렇구나 ㅠ",
          "original_form": "아...그렇구나 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.21",
          "form": "저도 그래요 ㅠ",
          "original_form": "저도 그래요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.22",
          "form": "그거랑 펜트하우스 마라탕 먹는 기분으로 보고 있어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거랑 펜트하우스 마라탕 먹는 기분으로 보고 있어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.23",
          "form": "볼게없어요",
          "original_form": "볼게없어요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.24",
          "form": "아앗..ㅋㅋㅋㅋㅋ",
          "original_form": "아앗..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.25",
          "form": "아 디피 들었는데 한 번 봐야겠네요",
          "original_form": "아 디피 들었는데 한 번 봐야겠네요",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.27",
          "form": "그거 시즌1 1화보고..너무 충격먹어서",
          "original_form": "그거 시즌1 1화보고..너무 충격먹어서",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.28",
          "form": "못보겟더라구요",
          "original_form": "못보겟더라구요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.29",
          "form": "포기했슴다",
          "original_form": "포기했슴다",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.30",
          "form": "디피 원작이 웹툰이래요",
          "original_form": "디피 원작이 웹툰이래요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026245.1.31",
          "form": "충격적인가요?! 주변에서 재밌다해서",
          "original_form": "충격적인가요?! 주변에서 재밌다해서",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.32",
          "form": "보려했는데",
          "original_form": "보려했는데",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.33",
          "form": "웹툰이라도 슬쩍 보고",
          "original_form": "웹툰이라도 슬쩍 보고",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.34",
          "form": "볼지말지 생각해봐야겠네요",
          "original_form": "볼지말지 생각해봐야겠네요",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.35",
          "form": "아 저건 팬트하우스요",
          "original_form": "아 저건 팬트하우스요",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.36",
          "form": "디피는 아직...",
          "original_form": "디피는 아직...",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.37",
          "form": "재미잇는데 차마 재미있다고 못한다고 평하던걸요",
          "original_form": "재미잇는데 차마 재미있다고 못한다고 평하던걸요",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026245.1.38",
          "form": "아 그렇군요 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100026245.1.39",
          "form": "펜트하우스는 진짜 마라탕 먹는 기분으로 보다가 점점 끝에는 계연성과 현실성 아무것도 없는",
          "original_form": "펜트하우스는 진짜 마라탕 먹는 기분으로 보다가 점점 끝에는 계연성과 현실성 아무것도 없는",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100026245.1.40",
          "form": "완벽한 판타지 같다는 소감 뿐이에요 ㅎㅎ",
          "original_form": "완벽한 판타지 같다는 소감 뿐이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100026245.1.41",
          "form": "시간이 소중하다면 다른 걸 보시는 것을 추천합니다 ㅎㅎ",
          "original_form": "시간이 소중하다면 다른 걸 보시는 것을 추천합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026245.1.42",
          "form": "네 ㅎㅎㅎ 다른걸 볼께요 ㅎㅎ",
          "original_form": "네 ㅎㅎㅎ 다른걸 볼께요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        }
      ]
