      "utterance": [
        {
          "id": "MDRW2100045138.1.1",
          "form": "있잖아 이상형이 뭐야?",
          "original_form": "있잖아 이상형이 뭐야?",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045138.1.2",
          "form": "나는 .. 가정에충실한 돈잘벌어오는남자",
          "original_form": "나는 .. 가정에충실한 돈잘벌어오는남자",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045138.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045138.1.4",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ 나는 몸짱인 사람!!",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ 나는 몸짱인 사람!!",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045138.1.5",
          "form": "마치 김종국이나 군인들처럼 그런 근육 가진 사람들이 좋더라",
          "original_form": "마치 김종국이나 군인들처럼 그런 근육 가진 사람들이 좋더라",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045138.1.6",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045138.1.7",
          "form": "운동하는사람 좋아해?",
          "original_form": "운동하는사람 좋아해?",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045138.1.8",
          "form": "응! 운동하는 사람들 좋아해!!",
          "original_form": "응! 운동하는 사람들 좋아해!!",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045138.1.9",
          "form": "너는 운동하는 사람들은 어때?",
          "original_form": "너는 운동하는 사람들은 어때?",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045138.1.10",
          "form": "나도 좋은거같애 에너제틱하고 자기관리 잘하자낭",
          "original_form": "나도 좋은거같애 에너제틱하고 자기관리 잘하자낭",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045138.1.11",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045138.1.12",
          "form": "근데 운동만 잘하고 성격이 안맞으면 별로징",
          "original_form": "근데 운동만 잘하고 성격이 안맞으면 별로징",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045138.1.13",
          "form": "그건 맞아 성격이 안맞으면 영 꽝이지",
          "original_form": "그건 맞아 성격이 안맞으면 영 꽝이지",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045138.1.14",
          "form": "현재 애인 있어?",
          "original_form": "현재 애인 있어?",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045138.1.15",
          "form": "응 애인 있는데 넌?",
          "original_form": "응 애인 있는데 넌?",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045138.1.16",
          "form": "윽... 슬프게도 난 없어...",
          "original_form": "윽... 슬프게도 난 없어...",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045138.1.17",
          "form": "악 ㅋㅋ큐ㅠ 곧 좋은사람이 찾아올거야",
          "original_form": "악 ㅋㅋ큐ㅠ 곧 좋은사람이 찾아올거야",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045138.1.18",
          "form": "ㅠㅜ 그랬으면 좋겠다",
          "original_form": "ㅠㅜ 그랬으면 좋겠다",
          "speaker_id": "1",
          "time": "20210908 01:10"
