      "utterance": [
        {
          "id": "MDRW2100034779.1.1",
          "form": "안녕하세요! 식사하셨나요?",
          "original_form": "안녕하세요! 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034779.1.2",
          "form": "안녕하세용 저녁 방금 먹었어요!",
          "original_form": "안녕하세용 저녁 방금 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034779.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100034779.1.4",
          "form": "네 저도 좀 전에 먹었어요",
          "original_form": "네 저도 좀 전에 먹었어요",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034779.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034779.1.6",
          "form": "파리바게트에서 빵 사와서 빵 먹었어요ㅎㅎ",
          "original_form": "파리바게트에서 빵 사와서 빵 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100034779.1.7",
          "form": "개인적으로 뚜레쥬르가 입맛에 더 맞는것같아용",
          "original_form": "개인적으로 뚜레쥬르가 입맛에 더 맞는것같아용",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034779.1.8",
          "form": "뚜레쥬르 빵 맛있죠!",
          "original_form": "뚜레쥬르 빵 맛있죠!",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034779.1.9",
          "form": "뻥 좋아하시나봐요",
          "original_form": "뻥 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034779.1.10",
          "form": "네네 좋아해요ㅋㅋㅋ name1님은 무슨 음식 좋아하세요?",
          "original_form": "네네 좋아해요ㅋㅋㅋ &name1&님은 무슨 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100034779.1.11",
          "form": "아이고 오타가 났네요ㅋㅋㅋ 죄송해요",
          "original_form": "아이고 오타가 났네요ㅋㅋㅋ 죄송해요",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034779.1.12",
          "form": "저는 요즘 마라탕 자주 먹어요!",
          "original_form": "저는 요즘 마라탕 자주 먹어요!",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034779.1.13",
          "form": "괜찮아요!ㅎㅎ 아 마라탕 너무 맛있죠",
          "original_form": "괜찮아요!ㅎㅎ 아 마라탕 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034779.1.14",
          "form": "매운거 잘 못먹지만 마라탕은 꾸역꾸역 먹어요ㅋㅋㅋ",
          "original_form": "매운거 잘 못먹지만 마라탕은 꾸역꾸역 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100034779.1.15",
          "form": "최근에 입문했는데 너무 맛있더라고요~",
          "original_form": "최근에 입문했는데 너무 맛있더라고요~",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034779.1.16",
          "form": "마라샹궈도 드셔보셨나요?",
          "original_form": "마라샹궈도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034779.1.17",
          "form": "헉 아직이요!",
          "original_form": "헉 아직이요!",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034779.1.18",
          "form": "훠궈는 먹어봤는데",
          "original_form": "훠궈는 먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034779.1.19",
          "form": "마라샹궈도 맛있나요?",
          "original_form": "마라샹궈도 맛있나요?",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034779.1.20",
          "form": "저도 아직 안 먹어봤는데 마라샹궈 먹어본 친구들이 다 탕보다 샹궈가 더 맛있다고 하더라고요!",
          "original_form": "저도 아직 안 먹어봤는데 마라샹궈 먹어본 친구들이 다 탕보다 샹궈가 더 맛있다고 하더라고요!",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100034779.1.21",
          "form": "꼭 먹어볼게요ㅋㅋㅋㅋ",
          "original_form": "꼭 먹어볼게요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034779.1.22",
          "form": "제가 중식을 워낙 좋아해서",
          "original_form": "제가 중식을 워낙 좋아해서",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034779.1.23",
          "form": "입맛에 잘 맞을것같아요ㅋㅋ",
          "original_form": "입맛에 잘 맞을것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100034779.1.24",
          "form": "오 그럼 맛있게 드실 거 같아요!",
          "original_form": "오 그럼 맛있게 드실 거 같아요!",
          "speaker_id": "1",
          "time": "20210828 19:30"
        }
      ]
    }
  ]
