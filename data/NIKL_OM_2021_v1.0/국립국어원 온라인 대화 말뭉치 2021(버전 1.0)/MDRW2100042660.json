      "utterance": [
        {
          "id": "MDRW2100042660.1.1",
          "form": "집안일은 하시나요?",
          "original_form": "집안일은 하시나요?",
          "speaker_id": "1",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100042660.1.2",
          "form": "네~~~ 거의 하려고 해요! name1님은요??",
          "original_form": "네~~~ 거의 하려고 해요! &name1&님은요??",
          "speaker_id": "2",
          "time": "20210907 00:53"
        },
        {
          "id": "MDRW2100042660.1.3",
          "form": "저도 설거지랑 빨래합니다",
          "original_form": "저도 설거지랑 빨래합니다",
          "speaker_id": "1",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042660.1.4",
          "form": "집안일 힘들죠",
          "original_form": "집안일 힘들죠",
          "speaker_id": "1",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042660.1.5",
          "form": "네ㅜㅜ 집안일은 힘든데 티가 안나서 화가나요",
          "original_form": "네ㅜㅜ 집안일은 힘든데 티가 안나서 화가나요",
          "speaker_id": "2",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042660.1.6",
          "form": "맞아여 그래서 더 속상하죠",
          "original_form": "맞아여 그래서 더 속상하죠",
          "speaker_id": "1",
          "time": "20210907 00:56"
        },
        {
          "id": "MDRW2100042660.1.7",
          "form": "하 집안일 중 뭐가 제일 싫으세요?",
          "original_form": "하 집안일 중 뭐가 제일 싫으세요?",
          "speaker_id": "2",
          "time": "20210907 00:57"
        },
        {
          "id": "MDRW2100042660.1.8",
          "form": "저는 설거지요 귀찮더라고요",
          "original_form": "저는 설거지요 귀찮더라고요",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042660.1.9",
          "form": "와 저도요ㅠ 저는 바닥 청소가 좀 더 싫긴하지만요",
          "original_form": "와 저도요ㅠ 저는 바닥 청소가 좀 더 싫긴하지만요",
          "speaker_id": "2",
          "time": "20210907 01:00"
        },
        {
          "id": "MDRW2100042660.1.10",
          "form": "아 바닥청소 진짜 티가 안나죠",
          "original_form": "아 바닥청소 진짜 티가 안나죠",
          "speaker_id": "1",
          "time": "20210907 01:00"
        },
        {
          "id": "MDRW2100042660.1.11",
          "form": "맞아요 제일 티가 안나는 집안일 같아용",
          "original_form": "맞아요 제일 티가 안나는 집안일 같아용",
          "speaker_id": "2",
          "time": "20210907 01:02"
        },
        {
          "id": "MDRW2100042660.1.12",
          "form": "맞아요 머리카락도 계속보이고요",
          "original_form": "맞아요 머리카락도 계속보이고요",
          "speaker_id": "1",
          "time": "20210907 01:02"
        },
        {
          "id": "MDRW2100042660.1.13",
          "form": "악 맞아요 그래서 머리 밀고 싶엇다니까요",
          "original_form": "악 맞아요 그래서 머리 밀고 싶엇다니까요",
          "speaker_id": "2",
          "time": "20210907 01:02"
        },
        {
          "id": "MDRW2100042660.1.14",
          "form": "그정도로 귀찮죠",
          "original_form": "그정도로 귀찮죠",
          "speaker_id": "1",
          "time": "20210907 01:02"
        }
