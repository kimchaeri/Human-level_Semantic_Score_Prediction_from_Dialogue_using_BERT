      "utterance": [
        {
          "id": "MDRW2100045830.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045830.1.2",
          "form": "넹 좋아해요",
          "original_form": "넹 좋아해요",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045830.1.3",
          "form": "가장 최근에 간 여행지가 어디인가요?",
          "original_form": "가장 최근에 간 여행지가 어디인가요?",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045830.1.4",
          "form": "제주도 갓어요",
          "original_form": "제주도 갓어요",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045830.1.5",
          "form": "오 저도요!",
          "original_form": "오 저도요!",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045830.1.6",
          "form": "우도가 정말 예쁘더라고요",
          "original_form": "우도가 정말 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100045830.1.7",
          "form": "저는 우도까지 안 들어가봤어요",
          "original_form": "저는 우도까지 안 들어가봤어요",
          "speaker_id": "2",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045830.1.8",
          "form": "한 번 꼭 가보세요! 너무 예뻐요",
          "original_form": "한 번 꼭 가보세요! 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100045830.1.9",
          "form": "네ㅋㅋ 땅콩 사오셨나요?",
          "original_form": "네ㅋㅋ 땅콩 사오셨나요?",
          "speaker_id": "2",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045830.1.10",
          "form": "네! 땅콩 너무 맛있더라구요ㅋㅋ",
          "original_form": "네! 땅콩 너무 맛있더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:47"
        },
        {
          "id": "MDRW2100045830.1.11",
          "form": "땅콩 아이스크림두요",
          "original_form": "땅콩 아이스크림두요",
          "speaker_id": "1",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045830.1.12",
          "form": "제주도에서는 머 하셨어요?",
          "original_form": "제주도에서는 머 하셨어요?",
          "speaker_id": "2",
          "time": "20210908 07:48"
        },
        {
          "id": "MDRW2100045830.1.13",
          "form": "말도 타고, 보트도 타고..",
          "original_form": "말도 타고, 보트도 타고..",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045830.1.14",
          "form": "계속 먹었어요ㅋㅋㅋ",
          "original_form": "계속 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045830.1.15",
          "form": "와 보트도 타셨어요?",
          "original_form": "와 보트도 타셨어요?",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045830.1.16",
          "form": "돌고래도 보셨나요?",
          "original_form": "돌고래도 보셨나요?",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100045830.1.17",
          "form": "우도 내에서 탔어요!",
          "original_form": "우도 내에서 탔어요!",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045830.1.18",
          "form": "막 꺾어주는데 재밌더라고요! 근데 돌고래는 못봤어요ㅠㅠ",
          "original_form": "막 꺾어주는데 재밌더라고요! 근데 돌고래는 못봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100045830.1.19",
          "form": "제주도에서 돌고래 관광 투어? 상품이 있대성ㅅ",
          "original_form": "제주도에서 돌고래 관광 투어? 상품이 있대성ㅅ",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045830.1.20",
          "form": "운 좋으면 해안도로에섣ㅎ 볼 수 있대요",
          "original_form": "운 좋으면 해안도로에섣ㅎ 볼 수 있대요",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100045830.1.21",
          "form": "우와., , 언젠가 꼭 보고싶네요!",
          "original_form": "우와., , 언젠가 꼭 보고싶네요!",
          "speaker_id": "1",
          "time": "20210908 07:52"
        }
      ]
