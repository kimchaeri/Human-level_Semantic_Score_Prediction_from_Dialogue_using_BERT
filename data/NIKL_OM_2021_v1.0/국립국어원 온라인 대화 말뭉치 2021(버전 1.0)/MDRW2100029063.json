      "utterance": [
        {
          "id": "MDRW2100029063.1.1",
          "form": "손님이 없어 수입이 적은거죠.",
          "original_form": "손님이 없어 수입이 적은거죠.",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029063.1.2",
          "form": "저는 요즘 재택근무해요.",
          "original_form": "저는 요즘 재택근무해요.",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029063.1.3",
          "form": "네.어떤?",
          "original_form": "네.어떤?",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029063.1.4",
          "form": "재택이라도 해서 부럽네요.",
          "original_form": "재택이라도 해서 부럽네요.",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029063.1.5",
          "form": "직장에 안나가고 집에만",
          "original_form": "직장에 안나가고 집에만",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029063.1.6",
          "form": "수입은 줄었어요",
          "original_form": "수입은 줄었어요",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029063.1.7",
          "form": "대신 카페로 거의 출근하다시피 해요.",
          "original_form": "대신 카페로 거의 출근하다시피 해요.",
          "speaker_id": "1",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029063.1.8",
          "form": "재택이닌까 집에서 일을 하시잖아요.",
          "original_form": "재택이닌까 집에서 일을 하시잖아요.",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029063.1.9",
          "form": "집에서는 집중이 안되요",
          "original_form": "집에서는 집중이 안되요",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029063.1.10",
          "form": "어떤 재택근무에요.",
          "original_form": "어떤 재택근무에요.",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029063.1.11",
          "form": "직업이...?",
          "original_form": "직업이...?",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029063.1.12",
          "form": "보고서를 위한 조사",
          "original_form": "보고서를 위한 조사",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029063.1.13",
          "form": "조사?",
          "original_form": "조사?",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029063.1.14",
          "form": "선택형이에요.",
          "original_form": "선택형이에요.",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029063.1.15",
          "form": "뭘요?",
          "original_form": "뭘요?",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029063.1.16",
          "form": "출근할수도 있어요.",
          "original_form": "출근할수도 있어요.",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029063.1.17",
          "form": "아~ 무슨일을 하세요?",
          "original_form": "아~ 무슨일을 하세요?",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029063.1.18",
          "form": "문화재 관련 일자리에요",
          "original_form": "문화재 관련 일자리에요",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029063.1.19",
          "form": "우리는 재택을 할수 있는 직업이 아니라 ㅠ",
          "original_form": "우리는 재택을 할수 있는 직업이 아니라 ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029063.1.20",
          "form": "아~~~",
          "original_form": "아~~~",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029063.1.21",
          "form": "하신는 일은?",
          "original_form": "하신는 일은?",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029063.1.22",
          "form": "가게하세요?",
          "original_form": "가게하세요?",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029063.1.23",
          "form": "네~~\r\n개인 사업자요",
          "original_form": "네~~\r\n개인 사업자요",
          "speaker_id": "2",
