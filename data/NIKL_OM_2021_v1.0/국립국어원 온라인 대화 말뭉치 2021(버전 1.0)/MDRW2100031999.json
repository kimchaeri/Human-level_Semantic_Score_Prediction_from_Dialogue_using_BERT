      "utterance": [
        {
          "id": "MDRW2100031999.1.1",
          "form": "아침 뭐드셨어요",
          "original_form": "아침 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031999.1.2",
          "form": "아직 안 먹었어요.",
          "original_form": "아직 안 먹었어요.",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100031999.1.3",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100031999.1.4",
          "form": "남편이 빵구워줬어요",
          "original_form": "남편이 빵구워줬어요",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100031999.1.5",
          "form": "어머 부럽다",
          "original_form": "어머 부럽다",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.6",
          "form": "그리고 오븐은 엉망진창이되었다고 합니다",
          "original_form": "그리고 오븐은 엉망진창이되었다고 합니다",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.7",
          "form": "아...저런",
          "original_form": "아...저런",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.8",
          "form": "짜증...",
          "original_form": "짜증...",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.9",
          "form": "아 왜 밑에 흘린건 안닦는겨",
          "original_form": "아 왜 밑에 흘린건 안닦는겨",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.10",
          "form": "저는 맛보다 효율입니다",
          "original_form": "저는 맛보다 효율입니다",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.11",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.12",
          "form": "그때그때 닦아야 쉬운데",
          "original_form": "그때그때 닦아야 쉬운데",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.13",
          "form": "차라리 굶고말지",
          "original_form": "차라리 굶고말지",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.14",
          "form": "꼭 요리한 티를 팍팍 내요",
          "original_form": "꼭 요리한 티를 팍팍 내요",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.15",
          "form": "아 ptsd",
          "original_form": "아 ptsd",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100031999.1.16",
          "form": "맨날 가사도우미 어플 켰다가 백수라 끕니다",
          "original_form": "맨날 가사도우미 어플 켰다가 백수라 끕니다",
          "speaker_id": "1",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031999.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 백수아닌데 월급이 쥐꼬리같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 백수아닌데 월급이 쥐꼬리같아요",
          "speaker_id": "2",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031999.1.18",
          "form": "승진을 안시켜줘",
          "original_form": "승진을 안시켜줘",
          "speaker_id": "2",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031999.1.19",
          "form": "ㅋㅋㅋㅋㅋ살림살이 어렵구만",
          "original_form": "ㅋㅋㅋㅋㅋ살림살이 어렵구만",
          "speaker_id": "1",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031999.1.20",
          "form": "서울에서 자취방 이사하고 딱 한번 불렀는데",
          "original_form": "서울에서 자취방 이사하고 딱 한번 불렀는데",
          "speaker_id": "1",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100031999.1.21",
          "form": "완전 편하고 깨끗했다",
          "original_form": "완전 편하고 깨끗했다",
          "speaker_id": "1",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031999.1.22",
          "form": "3만원 냈던가.",
          "original_form": "3만원 냈던가.",
          "speaker_id": "1",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031999.1.23",
          "form": "이 시골엔 아마 안 되겠지",
          "original_form": "이 시골엔 아마 안 되겠지",
          "speaker_id": "1",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031999.1.24",
          "form": "그치그치 생각보다 저렴한데 막상 부르려니 아깝구 ㅋㅋㅋㅋ",
          "original_form": "그치그치 생각보다 저렴한데 막상 부르려니 아깝구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031999.1.25",
          "form": "일주일에 한번만 부를까 고민중이야",
          "original_form": "일주일에 한번만 부를까 고민중이야",
          "speaker_id": "2",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100031999.1.26",
          "form": "아까워 내가 하면 공짠데. 이 생각 계속 들어",
          "original_form": "아까워 내가 하면 공짠데. 이 생각 계속 들어",
          "speaker_id": "1",
          "time": "20210905 10:52"
        }
      ]
    }
  ]
