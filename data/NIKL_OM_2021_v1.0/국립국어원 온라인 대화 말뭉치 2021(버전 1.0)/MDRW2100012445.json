      "utterance": [
        {
          "id": "MDRW2100012445.1.1",
          "form": "저는 제로콜라좋아합니다",
          "original_form": "저는 제로콜라좋아합니다",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012445.1.2",
          "form": "저두요 저는 제로사이다도 좋아하구요",
          "original_form": "저두요 저는 제로사이다도 좋아하구요",
          "speaker_id": "1",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100012445.1.3",
          "form": "이제 그냥 감미료랑 사귀는 것 같습니다",
          "original_form": "이제 그냥 감미료랑 사귀는 것 같습니다",
          "speaker_id": "1",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100012445.1.4",
          "form": "감미료에 매료되버렷",
          "original_form": "감미료에 매료되버렷",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012445.1.5",
          "form": "곤약이나 감미료먹고 설사하거나 탈나는 사람도 많다던데",
          "original_form": "곤약이나 감미료먹고 설사하거나 탈나는 사람도 많다던데",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012445.1.6",
          "form": "저는 잘 맞나봐여",
          "original_form": "저는 잘 맞나봐여",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012445.1.7",
          "form": "저도잘 맞는거같아요",
          "original_form": "저도잘 맞는거같아요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012445.1.8",
          "form": "다른음식은",
          "original_form": "다른음식은",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012445.1.9",
          "form": "뭘좋아하시나요",
          "original_form": "뭘좋아하시나요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012445.1.10",
          "form": "아이스크림을 사랑합니다",
          "original_form": "아이스크림을 사랑합니다",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012445.1.11",
          "form": "정말 속세 음식 다 버린다해도",
          "original_form": "정말 속세 음식 다 버린다해도",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012445.1.12",
          "form": "아이스크림..못버리겠습니다",
          "original_form": "아이스크림..못버리겠습니다",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012445.1.13",
          "form": "저 아이스씨와 분가하겠습니다",
          "original_form": "저 아이스씨와 분가하겠습니다",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012445.1.14",
          "form": "베스킨라빈스",
          "original_form": "베스킨라빈스",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012445.1.15",
          "form": "최애 메뉴를 말해주세요",
          "original_form": "최애 메뉴를 말해주세요",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012445.1.16",
          "form": "하 그건 너무 힘든 문젠데...",
          "original_form": "하 그건 너무 힘든 문젠데...",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012445.1.17",
          "form": "갈 때 마다 사랑하는 레이디가 바뀐다구요",
          "original_form": "갈 때 마다 사랑하는 레이디가 바뀐다구요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012445.1.18",
          "form": "ㅋㅋㅋㅋㄱㄱㅋ저는 항상먹는 메뉴가있어요",
          "original_form": "ㅋㅋㅋㅋㄱㄱㅋ저는 항상먹는 메뉴가있어요",
          "speaker_id": "2",
          "time": "20210825 05:52"
        },
        {
          "id": "MDRW2100012445.1.19",
          "form": "뭔데용",
          "original_form": "뭔데용",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100012445.1.20",
          "form": "바로 엄마는 외계인입니당",
          "original_form": "바로 엄마는 외계인입니당",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100012445.1.21",
          "form": "호에엥~베스트메뉴죠",
          "original_form": "호에엥~베스트메뉴죠",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100012445.1.22",
          "form": "안에 들어있는 초코볼이 바삭바삭",
          "original_form": "안에 들어있는 초코볼이 바삭바삭",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100012445.1.23",
          "form": "어렸을때는 오히려 초코볼을 싫어했거든요",
          "original_form": "어렸을때는 오히려 초코볼을 싫어했거든요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100012445.1.24",
          "form": "하지만 나이를먹고",
          "original_form": "하지만 나이를먹고",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100012445.1.25",
          "form": "초코볼의 참맛을 알았답니다",
          "original_form": "초코볼의 참맛을 알았답니다",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100012445.1.26",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100012445.1.27",
          "form": "그 엄마는 외계인안에 초코볼이랑 비슷하다고 하는",
          "original_form": "그 엄마는 외계인안에 초코볼이랑 비슷하다고 하는",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100012445.1.28",
          "form": "초코볼 있던데",
          "original_form": "초코볼 있던데",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100012445.1.29",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100012445.1.30",
          "form": "몰티져스요",
          "original_form": "몰티져스요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100012445.1.31",
          "form": "제 최애 초코볼입니다",
          "original_form": "제 최애 초코볼입니다",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100012445.1.32",
          "form": "저도 한번밖에 안먹어봤지만",
          "original_form": "저도 한번밖에 안먹어봤지만",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100012445.1.33",
          "form": "맛있었던 것 같네요",
          "original_form": "맛있었던 것 같네요",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100012445.1.34",
          "form": "먹고싶당",
          "original_form": "먹고싶당",
          "speaker_id": "1",
          "time": "20210825 19:57"
        }
      ]
    }
