        },
        {
          "id": "MDRW2100029068.1.2",
          "form": "넵 좋아합니당",
          "original_form": "넵 좋아합니당",
          "speaker_id": "2",
          "time": "20210902 23:31"
        },
        {
          "id": "MDRW2100029068.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029068.1.4",
          "form": "저는 친구가 알아와주면 따라가는편이예요 !!",
          "original_form": "저는 친구가 알아와주면 따라가는편이예요 !!",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029068.1.5",
          "form": "최근에 어디 가셧어요 ?",
          "original_form": "최근에 어디 가셧어요 ?",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029068.1.6",
          "form": "최든에",
          "original_form": "최든에",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029068.1.7",
          "form": "돈까스집 갔었어요!",
          "original_form": "돈까스집 갔었어요!",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029068.1.8",
          "form": "요즘유행하는 안심돈까스!",
          "original_form": "요즘유행하는 안심돈까스!",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029068.1.9",
          "form": "우아!! 돈까스가 유행인가요??",
          "original_form": "우아!! 돈까스가 유행인가요??",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029068.1.10",
          "form": "저그 연돈 가보고 싶어요!!",
          "original_form": "저그 연돈 가보고 싶어요!!",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029068.1.11",
          "form": "아아 연돈도 엄청나죠ㅠㅠ",
          "original_form": "아아 연돈도 엄청나죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029068.1.12",
          "form": "거긴 아직도 티켓팅 힘들다던데여!!",
          "original_form": "거긴 아직도 티켓팅 힘들다던데여!!",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029068.1.13",
          "form": "그쵸그쵸 텐트치고 그러드라구요 ㅠㅠ",
          "original_form": "그쵸그쵸 텐트치고 그러드라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029068.1.14",
          "form": "비슷한 치즈 돈가스집 가봣는데 맛있더라구요",
          "original_form": "비슷한 치즈 돈가스집 가봣는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029068.1.15",
          "form": "연돈",
          "original_form": "연돈",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029068.1.16",
          "form": "코로나 때문에 어플티켓팅으로 바뀐지 꽤됐어요!",
          "original_form": "코로나 때문에 어플티켓팅으로 바뀐지 꽤됐어요!",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029068.1.17",
          "form": "우아 점점 맛집도 발전 하네요 !!!!!!",
          "original_form": "우아 점점 맛집도 발전 하네요 !!!!!!",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029068.1.18",
          "form": "그쳐그쳐",
          "original_form": "그쳐그쳐",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029068.1.19",
          "form": "하여튼 연돈 사장님부부 정말 대단하신거같아요",
          "original_form": "하여튼 연돈 사장님부부 정말 대단하신거같아요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029068.1.20",
          "form": "네네 맛도 안변하나봐요 ㅠㅠ 대단해요",
          "original_form": "네네 맛도 안변하나봐요 ㅠㅠ 대단해요",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029068.1.21",
          "form": "가격 안 올리시는 게",
          "original_form": "가격 안 올리시는 게",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029068.1.22",
          "form": "제일대단한것같아요ㅠㅠ",
          "original_form": "제일대단한것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029068.1.23",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029068.1.24",
          "form": "맛집 보면 맛도 변하고 가격도 바뀌는곳 있드라구요",
