      "utterance": [
        {
          "id": "MDRW2100000957.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000957.1.2",
          "form": "요즘 운동 뭐 하시는거잇어요??ㅎㅎ",
          "original_form": "요즘 운동 뭐 하시는거잇어요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000957.1.3",
          "form": "저는 요새 근처 공원 걷거나 근력운동 하고있어요",
          "original_form": "저는 요새 근처 공원 걷거나 근력운동 하고있어요",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000957.1.4",
          "form": "안더우세요? 저도 걸으려고 노력하는데 더워서 나가기가 싫으네요ㅜㅜ",
          "original_form": "안더우세요? 저도 걸으려고 노력하는데 더워서 나가기가 싫으네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000957.1.5",
          "form": "그래도 요며칠 시원해져서 좀 걷긴 하지만ㅋㅋ",
          "original_form": "그래도 요며칠 시원해져서 좀 걷긴 하지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000957.1.6",
          "form": "7시 넘어서 나가면 그래도 시원해서 걷기  괜찮더라구요",
          "original_form": "7시 넘어서 나가면 그래도 시원해서 걷기  괜찮더라구요",
          "speaker_id": "2",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000957.1.7",
          "form": "낮에는 더워서 못나가요ㅠ",
          "original_form": "낮에는 더워서 못나가요ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000957.1.8",
          "form": "그쳐ㅠㅠ 해도 너무 강하고ㅋㅋ",
          "original_form": "그쳐ㅠㅠ 해도 너무 강하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000957.1.9",
          "form": "전 그래서 요즘 그냥 홈트만해요ㅠ",
          "original_form": "전 그래서 요즘 그냥 홈트만해요ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000957.1.10",
          "form": "저도 근력운동은 집에서  하고있어요",
          "original_form": "저도 근력운동은 집에서  하고있어요",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000957.1.11",
          "form": "사실 집에서 에어컨틀고 운동하는게 더울때는 제일 나은거같아요",
          "original_form": "사실 집에서 에어컨틀고 운동하는게 더울때는 제일 나은거같아요",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000957.1.12",
          "form": "그쵸ㅋㅋㅋ 코로나때문에 헬스장도 못가고ㅋㅋ 집에서 에어컨트는게 제일 나은것같아요ㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ 코로나때문에 헬스장도 못가고ㅋㅋ 집에서 에어컨트는게 제일 나은것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000957.1.13",
          "form": "근력운동은 어떻게해요? 아령드시나요?",
          "original_form": "근력운동은 어떻게해요? 아령드시나요?",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000957.1.14",
          "form": "마자요 헬스장 가기도 애매하죠ㅠ",
          "original_form": "마자요 헬스장 가기도 애매하죠ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000957.1.15",
          "form": "네 운동하려고 아령사서하고있어요",
          "original_form": "네 운동하려고 아령사서하고있어요",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000957.1.16",
          "form": "아령이 제일 간단한 근력운동이겠죠??",
          "original_form": "아령이 제일 간단한 근력운동이겠죠??",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000957.1.17",
          "form": "전 근육량이 진짜부족해서...아령도 넘 힘들더라구요ㅋㅋ",
          "original_form": "전 근육량이 진짜부족해서...아령도 넘 힘들더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000957.1.18",
          "form": "그쵸? 아무래도 피티쌤한테 운동자세 배울필요없는 운동중에하나죠",
          "original_form": "그쵸? 아무래도 피티쌤한테 운동자세 배울필요없는 운동중에하나죠",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000957.1.19",
          "form": "저도 근력 진짜 없었는데 자주 하다보니까 팔에 근육이 막 생기는게 느껴지더라구요",
          "original_form": "저도 근력 진짜 없었는데 자주 하다보니까 팔에 근육이 막 생기는게 느껴지더라구요",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000957.1.20",
          "form": "ㅋㅋ 저도 아령 가벼운거라도 사서 조금씩 늘려가면서 해야겠어요!ㅋㅋ",
          "original_form": "ㅋㅋ 저도 아령 가벼운거라도 사서 조금씩 늘려가면서 해야겠어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000957.1.21",
          "form": "진짜 나이먹을수록 근력이 중요한거같아요",
          "original_form": "진짜 나이먹을수록 근력이 중요한거같아요",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000957.1.22",
          "form": "운동 안하면 체력도 떨어져서 점점 힘들어지는거같아요",
          "original_form": "운동 안하면 체력도 떨어져서 점점 힘들어지는거같아요",
          "speaker_id": "2",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000957.1.23",
          "form": "맞아요ㅋㅋ 건강을 위해 운동 열심히 하자구요ㅎㅎ",
          "original_form": "맞아요ㅋㅋ 건강을 위해 운동 열심히 하자구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:13"
        },
        {
          "id": "MDRW2100000957.1.24",
          "form": "저도 오늘부터 열심히 시작!!",
          "original_form": "저도 오늘부터 열심히 시작!!",
          "speaker_id": "1",
          "time": "20210819 21:13"
        },
        {
          "id": "MDRW2100000957.1.25",
          "form": "네! 운동해야 안아프게 사니까요ㅠ\r\n화이팅이에요!",
