      "utterance": [
        {
          "id": "MDRW2100031176.1.1",
          "form": "저녁먹나?",
          "original_form": "저녁먹나?",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031176.1.2",
          "form": "ㅇㅇ저녁먹었다 ㅋㅋ",
          "original_form": "ㅇㅇ저녁먹었다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100031176.1.3",
          "form": "폭립?",
          "original_form": "폭립?",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031176.1.4",
          "form": "ㅇㅇ난 그저께먹었던 바베큐가 젤 맛있더라!",
          "original_form": "ㅇㅇ난 그저께먹었던 바베큐가 젤 맛있더라!",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031176.1.5",
          "form": "고추장도 괜찮긴한데 걍  평범한 고추장 갈비찜같음",
          "original_form": "고추장도 괜찮긴한데 걍  평범한 고추장 갈비찜같음",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031176.1.6",
          "form": "바베큐가 최고",
          "original_form": "바베큐가 최고",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031176.1.7",
          "form": "ㅋㅋㅋ단짠ㅋ",
          "original_form": "ㅋㅋㅋ단짠ㅋ",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031176.1.8",
          "form": "ㅇㅇ조금 덜 달면 좋을것같긴한데",
          "original_form": "ㅇㅇ조금 덜 달면 좋을것같긴한데",
          "speaker_id": "1",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100031176.1.9",
          "form": "ㅇㅇ조금 덜 달면",
          "original_form": "ㅇㅇ조금 덜 달면",
          "speaker_id": "1",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100031176.1.10",
          "form": "좋을것같ㅇ긴한데",
          "original_form": "좋을것같ㅇ긴한데",
          "speaker_id": "1",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100031176.1.11",
          "form": "새콤달콤 맛있엉",
          "original_form": "새콤달콤 맛있엉",
          "speaker_id": "1",
          "time": "20210904 20:02"
        },
        {
          "id": "MDRW2100031176.1.12",
          "form": "후기 적어주뿌라\r\n바베큐맛이 젤 괜찮다고ㅎ",
          "original_form": "후기 적어주뿌라\r\n바베큐맛이 젤 괜찮다고ㅎ",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031176.1.13",
          "form": "난 지금 쌈장 사러 이마트 가는중",
          "original_form": "난 지금 쌈장 사러 이마트 가는중",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031176.1.14",
          "form": "그냥 쌈장은 순대에 안어울리지않아?",
          "original_form": "그냥 쌈장은 순대에 안어울리지않아?",
          "speaker_id": "1",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031176.1.15",
          "form": "막장 만드는게 쌈장이래",
          "original_form": "막장 만드는게 쌈장이래",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031176.1.16",
          "form": "아 막장 만들게?",
          "original_form": "아 막장 만들게?",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031176.1.17",
          "form": "쌈장에 사이다 섞더라 순대 막장은",
          "original_form": "쌈장에 사이다 섞더라 순대 막장은",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031176.1.18",
          "form": "순대 찍어먹을려고ㅎ",
          "original_form": "순대 찍어먹을려고ㅎ",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031176.1.19",
          "form": "ㅇㅇ그래서 물어본고 ㅋㅋ",
          "original_form": "ㅇㅇ그래서 물어본고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031176.1.20",
          "form": "ㅋㅋㅋ순대 한번 더 먹을거 남았어ㅎ",
          "original_form": "ㅋㅋㅋ순대 한번 더 먹을거 남았어ㅎ",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031176.1.21",
          "form": "ㅇㅇ 맛있게 머겅 ㅋㅋ",
          "original_form": "ㅇㅇ 맛있게 머겅 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031176.1.22",
          "form": "청도복숭 2키로\r\n쌈장 딸기요거트,어묵 나온다ㅋ",
          "original_form": "청도복숭 2키로\r\n쌈장 딸기요거트,어묵 나온다ㅋ",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031176.1.23",
          "form": "사온다",
          "original_form": "사온다",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031176.1.24",
          "form": "복숭 무겁네",
          "original_form": "복숭 무겁네",
