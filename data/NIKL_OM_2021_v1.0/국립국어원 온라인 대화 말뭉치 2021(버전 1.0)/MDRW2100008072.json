      "utterance": [
        {
          "id": "MDRW2100008072.1.1",
          "form": "ㅇㅇ 그래. 파도 좀 사고 양파도",
          "original_form": "ㅇㅇ 그래. 파도 좀 사고 양파도",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008072.1.2",
          "form": "돈까스도 사고 콩나물이랑 두부도 사야겠어",
          "original_form": "돈까스도 사고 콩나물이랑 두부도 사야겠어",
          "speaker_id": "1",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008072.1.3",
          "form": "산책 겸 애들이랑 같이 가야겠다",
          "original_form": "산책 겸 애들이랑 같이 가야겠다",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008072.1.4",
          "form": "ㅇㅇ 그랴요",
          "original_form": "ㅇㅇ 그랴요",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008072.1.5",
          "form": "과일은 집에 있나?",
          "original_form": "과일은 집에 있나?",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008072.1.6",
          "form": "복숭아 다 먹었는데 다음주에 사과 올 거야",
          "original_form": "복숭아 다 먹었는데 다음주에 사과 올 거야",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008072.1.7",
          "form": "ㅇㅇ 그럼 그걸로 먹으면 되겠다 ㅎㅎ",
          "original_form": "ㅇㅇ 그럼 그걸로 먹으면 되겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008072.1.8",
          "form": "복숭아 맛있는데 진짜 너무 비싸더라 ㅎㅎ",
          "original_form": "복숭아 맛있는데 진짜 너무 비싸더라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008072.1.9",
          "form": "ㅎㅎ 다른 과일도 먹으면 되지 뭐",
          "original_form": "ㅎㅎ 다른 과일도 먹으면 되지 뭐",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008072.1.10",
          "form": "집에 자두도 있으니 다음주 사과오면 그거 먹고 ㅎㅎ",
          "original_form": "집에 자두도 있으니 다음주 사과오면 그거 먹고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008072.1.11",
          "form": "식재료 많이 쟁여놔 다음주 애들 개학이니 ㅎㅎ",
          "original_form": "식재료 많이 쟁여놔 다음주 애들 개학이니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008072.1.12",
          "form": "안그래도 장 많이 봐놔야 할 것 같아 ㅎㅎ",
          "original_form": "안그래도 장 많이 봐놔야 할 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008072.1.13",
          "form": "ㅇㅇ 두고 먹을 수 있는 것 좀 사오자",
          "original_form": "ㅇㅇ 두고 먹을 수 있는 것 좀 사오자",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100008072.1.14",
