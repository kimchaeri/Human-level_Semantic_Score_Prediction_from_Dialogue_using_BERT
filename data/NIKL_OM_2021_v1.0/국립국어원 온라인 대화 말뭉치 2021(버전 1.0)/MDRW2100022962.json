      "utterance": [
        {
          "id": "MDRW2100022962.1.1",
          "form": "저의 오늘 점심 메뉴를 맞춰보시죠",
          "original_form": "저의 오늘 점심 메뉴를 맞춰보시죠",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022962.1.2",
          "form": "음 계란에 소세지를 곁들인 김치볶음밥",
          "original_form": "음 계란에 소세지를 곁들인 김치볶음밥",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022962.1.3",
          "form": "정답인가요?",
          "original_form": "정답인가요?",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022962.1.4",
          "form": "땡 틀렸습니다~ 야채가 들어가요",
          "original_form": "땡 틀렸습니다~ 야채가 들어가요",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022962.1.5",
          "form": "아 이런 다시 생각해보지요!",
          "original_form": "아 이런 다시 생각해보지요!",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022962.1.6",
          "form": "면 종류인가요?",
          "original_form": "면 종류인가요?",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022962.1.7",
          "form": "면 종류는 아닙니다",
          "original_form": "면 종류는 아닙니다",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100022962.1.8",
          "form": "으으으으으음 뭘까",
          "original_form": "으으으으으음 뭘까",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022962.1.9",
          "form": "쌀로 만든 요리인가요?",
          "original_form": "쌀로 만든 요리인가요?",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022962.1.10",
          "form": "쌀이 들어갑니다!!!",
          "original_form": "쌀이 들어갑니다!!!",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022962.1.11",
          "form": "오 아싸",
          "original_form": "오 아싸",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022962.1.12",
          "form": "그럼 국물이 있는 요리인가요?",
          "original_form": "그럼 국물이 있는 요리인가요?",
          "speaker_id": "1",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022962.1.13",
          "form": "아뇨 국물 없습니다!",
          "original_form": "아뇨 국물 없습니다!",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022962.1.14",
          "form": "고기가 들어가나요? 으아아아아아",
          "original_form": "고기가 들어가나요? 으아아아아아",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022962.1.15",
          "form": "맞추고 말겠어",
          "original_form": "맞추고 말겠어",
          "speaker_id": "1",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022962.1.16",
          "form": "네 고기 들어갑니다!!!",
          "original_form": "네 고기 들어갑니다!!!",
          "speaker_id": "2",
          "time": "20210829 19:40"
        },
        {
          "id": "MDRW2100022962.1.17",
          "form": "양념이 빨간 색인가요?",
          "original_form": "양념이 빨간 색인가요?",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022962.1.18",
          "form": "예!!!! 빨간색 입니다",
          "original_form": "예!!!! 빨간색 입니다",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022962.1.19",
          "form": "주요한 힌트 하나 더 드릴까요",
          "original_form": "주요한 힌트 하나 더 드릴까요",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022962.1.20",
          "form": "으악 네 힌트좀 주세요",
          "original_form": "으악 네 힌트좀 주세요",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022962.1.21",
          "form": "도저히 모르겠시야",
          "original_form": "도저히 모르겠시야",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022962.1.22",
          "form": "취향에 따라 초록색 소스를 추가할 수 있어요",
          "original_form": "취향에 따라 초록색 소스를 추가할 수 있어요",
          "speaker_id": "2",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022962.1.23",
          "form": "으으으으으으으으으으으으으음",
          "original_form": "으으으으으으으으으으으으으음",
