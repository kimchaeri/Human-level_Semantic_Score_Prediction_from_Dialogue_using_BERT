      "utterance": [
        {
          "id": "MDRW2100007819.1.1",
          "form": "뭐 일 하는거 있어요?",
          "original_form": "뭐 일 하는거 있어요?",
          "speaker_id": "2",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007819.1.2",
          "form": "저는 학생이라 일하는건 없는데",
          "original_form": "저는 학생이라 일하는건 없는데",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007819.1.3",
          "form": "취업에 대해선 생각해보게 되더라구요",
          "original_form": "취업에 대해선 생각해보게 되더라구요",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007819.1.4",
          "form": "오오 어느쪽으로 생각하고있는데요?",
          "original_form": "오오 어느쪽으로 생각하고있는데요?",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007819.1.5",
          "form": "사실 저는 졸업예정 대학생이라",
          "original_form": "사실 저는 졸업예정 대학생이라",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007819.1.6",
          "form": "막 취업준비중이거든요 ㅎㅎ",
          "original_form": "막 취업준비중이거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007819.1.7",
          "form": "저는 교직 쪽 생각하고 있어요!",
          "original_form": "저는 교직 쪽 생각하고 있어요!",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007819.1.8",
          "form": "근데 요즘 굉장히 빡세니까 고민도 되고 하네요",
          "original_form": "근데 요즘 굉장히 빡세니까 고민도 되고 하네요",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007819.1.9",
          "form": "우와 교직",
          "original_form": "우와 교직",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007819.1.10",
          "form": "멋있어요!",
          "original_form": "멋있어요!",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007819.1.11",
          "form": "요즘 취업난이 심하긴해요 ㅠㅠ",
          "original_form": "요즘 취업난이 심하긴해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007819.1.12",
          "form": "근데 어딜가나 다 그런것같아요..",
          "original_form": "근데 어딜가나 다 그런것같아요..",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007819.1.13",
          "form": "저는 공대 다니고있는데",
          "original_form": "저는 공대 다니고있는데",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007819.1.14",
          "form": "취업 잘되는 과라더니 막상 그렇지도 않네요 ㅜ",
          "original_form": "취업 잘되는 과라더니 막상 그렇지도 않네요 ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007819.1.15",
          "form": "공대면 요즘 취업의 희망아니었나요 현실이 녹록치 않군요 ㅠ",
          "original_form": "공대면 요즘 취업의 희망아니었나요 현실이 녹록치 않군요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100007819.1.16",
          "form": "네 맞아요 ..ㅠㅠ",
          "original_form": "네 맞아요 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100007819.1.17",
          "form": "인턴은 해봤는데",
          "original_form": "인턴은 해봤는데",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100007819.1.18",
          "form": "그것도 만만치가 않았네요",
          "original_form": "그것도 만만치가 않았네요",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100007819.1.19",
          "form": "사회생활 시작하는것부터 힘든가보네요 얼른 좀 괜찮아져야할텐데",
          "original_form": "사회생활 시작하는것부터 힘든가보네요 얼른 좀 괜찮아져야할텐데",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100007819.1.20",
          "form": "맞아요 대인관계가 진짜 제일 어려워요",
          "original_form": "맞아요 대인관계가 진짜 제일 어려워요",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100007819.1.21",
          "form": "소문도 진짜 잘나고..",
          "original_form": "소문도 진짜 잘나고..",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100007819.1.22",
          "form": "대인관계는 학교도 그렇고 사회도 그렇고 너무 힘든 문제네요",
          "original_form": "대인관계는 학교도 그렇고 사회도 그렇고 너무 힘든 문제네요",
          "speaker_id": "1",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100007819.1.23",
          "form": "학교에서는 무슨일이 힘들었나요?",
          "original_form": "학교에서는 무슨일이 힘들었나요?",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100007819.1.24",
          "form": "학교에선 아무래도 친구를 못사귀면 지내기 힘드니까요 제가 내성적인 성격이라서 친구만들기를 잘 못했거든요 ㅠ",
          "original_form": "학교에선 아무래도 친구를 못사귀면 지내기 힘드니까요 제가 내성적인 성격이라서 친구만들기를 잘 못했거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100007819.1.25",
          "form": "아아 ㅠㅠ 친구만들기 힘들죠..",
          "original_form": "아아 ㅠㅠ 친구만들기 힘들죠..",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100007819.1.26",
          "form": "그리고 친구사귀는 것도 진짜 타이밍이랑 운이 중요한것같아요 ..",
          "original_form": "그리고 친구사귀는 것도 진짜 타이밍이랑 운이 중요한것같아요 ..",
          "speaker_id": "2",
          "time": "20210822 17:07"
        }
      ]
