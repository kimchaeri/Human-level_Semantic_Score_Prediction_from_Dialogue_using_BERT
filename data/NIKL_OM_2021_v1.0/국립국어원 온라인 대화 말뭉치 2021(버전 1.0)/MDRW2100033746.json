      "utterance": [
        {
          "id": "MDRW2100033746.1.1",
          "form": "나 오늘 노래를 들었어",
          "original_form": "나 오늘 노래를 들었어",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.2",
          "form": "그 볼빨간사춘기의 여행.",
          "original_form": "그 볼빨간사춘기의 여행.",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.3",
          "form": "아아 그노래를 들으면 여행가고싶어지지",
          "original_form": "아아 그노래를 들으면 여행가고싶어지지",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.4",
          "form": "근데 여행가고싶더라.",
          "original_form": "근데 여행가고싶더라.",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.5",
          "form": "맞아 부산여행갔었지.",
          "original_form": "맞아 부산여행갔었지.",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.6",
          "form": "또 가고싶다는 말이야?",
          "original_form": "또 가고싶다는 말이야?",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.7",
          "form": "그때 좋았던 점이 뭐였어?",
          "original_form": "그때 좋았던 점이 뭐였어?",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.8",
          "form": "난 또 가고싶어.",
          "original_form": "난 또 가고싶어.",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.9",
          "form": "아무래도 숟소가 좋았디",
          "original_form": "아무래도 숟소가 좋았디",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100033746.1.10",
          "form": "그냥 추억을 회상하려고 물어봤어",
          "original_form": "그냥 추억을 회상하려고 물어봤어",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.11",
          "form": "밥도 맛있었어",
          "original_form": "밥도 맛있었어",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.12",
          "form": "시간이 되면 꼭 다시가고싶지",
          "original_form": "시간이 되면 꼭 다시가고싶지",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.13",
          "form": "우리 다음엔 사진을 많이 찍자.",
          "original_form": "우리 다음엔 사진을 많이 찍자.",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.14",
          "form": "디저트도 맛있었어",
          "original_form": "디저트도 맛있었어",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.15",
          "form": "맞아 거기서 먹은 회는 역대급이야",
          "original_form": "맞아 거기서 먹은 회는 역대급이야",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.16",
          "form": "거긴 천국이야",
          "original_form": "거긴 천국이야",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.17",
          "form": "그래 다음에는 내가 셀카봉도 들고갈께",
          "original_form": "그래 다음에는 내가 셀카봉도 들고갈께",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.18",
          "form": "아무래도 우리가 돈 쓰러 간거니까.",
          "original_form": "아무래도 우리가 돈 쓰러 간거니까.",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.19",
          "form": "그리고 우리 돼지국밥도 먹자",
          "original_form": "그리고 우리 돼지국밥도 먹자",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.20",
          "form": "좋아. 삼각대가 필요해.",
          "original_form": "좋아. 삼각대가 필요해.",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.21",
          "form": "나 부산에서 돼지국밥 먹어본적이 없어",
          "original_form": "나 부산에서 돼지국밥 먹어본적이 없어",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100033746.1.22",
          "form": "나는 기억이 안나.",
          "original_form": "나는 기억이 안나.",
          "speaker_id": "2",
          "time": "20210825 00:03"
        }
      ]
    }
