        },
        {
          "id": "MDRW2100035271.1.2",
          "form": "안녕하세요운동하시나요",
          "original_form": "안녕하세요운동하시나요",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100035271.1.3",
          "form": "안녕하세요? 아니오ㅋㅋ name2님은 구준히 하시는게 있늕지요",
          "original_form": "안녕하세요? 아니오ㅋㅋ &name2&님은 구준히 하시는게 있늕지요",
          "speaker_id": "1",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100035271.1.4",
          "form": "저는 매일 30분 산책과 주말엔 댄스레슨받아요",
          "original_form": "저는 매일 30분 산책과 주말엔 댄스레슨받아요",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100035271.1.5",
          "form": "아 산책정도는 한 이삼일에 한 번 해요",
          "original_form": "아 산책정도는 한 이삼일에 한 번 해요",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100035271.1.6",
          "form": "댄스는 어떤 종류인가요? 발레? 방송댄스?",
          "original_form": "댄스는 어떤 종류인가요? 발레? 방송댄스?",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100035271.1.7",
          "form": "케이팝댄스해요 ㅋㅋ",
          "original_form": "케이팝댄스해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100035271.1.8",
          "form": "와 ㅋㅋ 좋아하는 아이돌 댄스를 따라할 수 있겠네요",
          "original_form": "와 ㅋㅋ 좋아하는 아이돌 댄스를 따라할 수 있겠네요",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100035271.1.9",
          "form": "어떤 춤을 배웠나요?",
          "original_form": "어떤 춤을 배웠나요?",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100035271.1.10",
          "form": "아스트로 노래랑 있지노래 블랙핑크 노래 다양하게했어요",
          "original_form": "아스트로 노래랑 있지노래 블랙핑크 노래 다양하게했어요",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100035271.1.11",
          "form": "와 그 춤들이 다 춰지세요? 대단해요!!!!",
          "original_form": "와 그 춤들이 다 춰지세요? 대단해요!!!!",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100035271.1.12",
          "form": "그냥 뚝딱뚝딱 제멋대로 해요 ㅋㅋ",
          "original_form": "그냥 뚝딱뚝딱 제멋대로 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100035271.1.13",
          "form": "전 그런 춤은 속도를 4배 줄여도 따라갈동말동이에욬ㅋ",
          "original_form": "전 그런 춤은 속도를 4배 줄여도 따라갈동말동이에욬ㅋ",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100035271.1.14",
          "form": "저도 거의 0.8배속 0.9배속으로 해요 ㅋㅋ",
          "original_form": "저도 거의 0.8배속 0.9배속으로 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:27"
        },
        {
          "id": "MDRW2100035271.1.15",
          "form": "1배속 어케하는지모르겠어요 아이돌들",
          "original_form": "1배속 어케하는지모르겠어요 아이돌들",
          "speaker_id": "2",
          "time": "20210829 18:27"
        },
        {
          "id": "MDRW2100035271.1.16",
          "form": "저보다 3배 빠르신거에욬ㅋㅋ",
          "original_form": "저보다 3배 빠르신거에욬ㅋㅋ",
