      "utterance": [
        {
          "id": "MDRW2100022733.1.1",
          "form": "그렇게 말할게 아니라",
          "original_form": "그렇게 말할게 아니라",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022733.1.2",
          "form": "내말이..",
          "original_form": "내말이..",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022733.1.3",
          "form": "국가에서 교육같은거 만들어서",
          "original_form": "국가에서 교육같은거 만들어서",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022733.1.4",
          "form": "유기견도 잘 입양될 수 있도록 되면 좋겠다",
          "original_form": "유기견도 잘 입양될 수 있도록 되면 좋겠다",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.5",
          "form": "이래도 모자랄 판에",
          "original_form": "이래도 모자랄 판에",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.6",
          "form": "맞아 말이 아다르고 어다른데",
          "original_form": "맞아 말이 아다르고 어다른데",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.7",
          "form": "그 리사도",
          "original_form": "그 리사도",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.8",
          "form": "논란 있던데",
          "original_form": "논란 있던데",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.9",
          "form": "유기견은 입양하지마라는 뉘앙스로말하면ㅋㅋㅋ",
          "original_form": "유기견은 입양하지마라는 뉘앙스로말하면ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.10",
          "form": "무슨논란?",
          "original_form": "무슨논란?",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.11",
          "form": "그 품종묘랑 품종견만 데려온다고",
          "original_form": "그 품종묘랑 품종견만 데려온다고",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022733.1.12",
          "form": "근데 그 고양이가",
          "original_form": "근데 그 고양이가",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.13",
          "form": "덜덜..",
          "original_form": "덜덜..",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.14",
          "form": "사람들이 무분별하게 교배시켜서",
          "original_form": "사람들이 무분별하게 교배시켜서",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.15",
          "form": "유전병이 심한 고양이 인것 같더라고",
          "original_form": "유전병이 심한 고양이 인것 같더라고",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.16",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.17",
          "form": "막 고양이 중에 뼈가 녹고 그런 애들도 있잖아..",
          "original_form": "막 고양이 중에 뼈가 녹고 그런 애들도 있잖아..",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.18",
          "form": "그런거 보면 진짜.....",
          "original_form": "그런거 보면 진짜.....",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.19",
          "form": "맞아 걍 연예인들 전반적으로",
          "original_form": "맞아 걍 연예인들 전반적으로",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.20",
          "form": "그 로제랑 더 비교돼서",
          "original_form": "그 로제랑 더 비교돼서",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.21",
          "form": "품종견품종묘 전시 진짜심함",
          "original_form": "품종견품종묘 전시 진짜심함",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022733.1.22",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022733.1.23",
          "form": "태연도",
          "original_form": "태연도",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022733.1.24",
          "form": "근데 김희철",
          "original_form": "근데 김희철",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022733.1.25",
          "form": "제로 유명해지고나서",
          "original_form": "제로 유명해지고나서",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022733.1.26",
          "form": "예전부터 별로였어서 ㅎ",
          "original_form": "예전부터 별로였어서 ㅎ",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022733.1.27",
          "form": "펫샵에 실버푸들 엄청 많아졌다며..",
          "original_form": "펫샵에 실버푸들 엄청 많아졌다며..",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022733.1.28",
          "form": "하.....",
          "original_form": "하.....",
          "speaker_id": "2",
          "time": "20210829 17:33"
        }
      ]
    }
