        {
          "id": "MDRW2100043363.1.1",
          "form": "네 궁금해서 사먹었어요",
          "original_form": "네 궁금해서 사먹었어요",
          "speaker_id": "2",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100043363.1.2",
          "form": "저녁 머먹엉",
          "original_form": "저녁 머먹엉",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100043363.1.3",
          "form": "라면이요ㅋㅋ",
          "original_form": "라면이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043363.1.4",
          "form": "와 오눌 라면 먹는사람 많다",
          "original_form": "와 오눌 라면 먹는사람 많다",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043363.1.5",
          "form": "난 쌀국수",
          "original_form": "난 쌀국수",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043363.1.6",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043363.1.7",
          "form": "배달 기다려요",
          "original_form": "배달 기다려요",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043363.1.8",
          "form": "배달 자주 시켜요?",
          "original_form": "배달 자주 시켜요?",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043363.1.9",
          "form": "네너무 자주 시켜서 오늘 안시킬랬는데",
          "original_form": "네너무 자주 시켜서 오늘 안시킬랬는데",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043363.1.10",
          "form": "뭐 만들게 없어서 시켜요ㅠㅠ",
          "original_form": "뭐 만들게 없어서 시켜요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043363.1.11",
          "form": "배달 편하니까 자주 쓰게 되져",
          "original_form": "배달 편하니까 자주 쓰게 되져",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043363.1.12",
          "form": "그니까요 ㅋㅋㅋ 돈 너무 쉽게 써요ㅠ",
          "original_form": "그니까요 ㅋㅋㅋ 돈 너무 쉽게 써요ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043363.1.13",
          "form": "배달도 그렇고 간식 사고 하느라 다 써요",
          "original_form": "배달도 그렇고 간식 사고 하느라 다 써요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043363.1.14",
          "form": "그러게요 돈 너무 많이써서 아겨야하는데",
          "original_form": "그러게요 돈 너무 많이써서 아겨야하는데",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043363.1.15",
          "form": "배들을 안시켜야하는데",
          "original_form": "배들을 안시켜야하는데",
          "speaker_id": "1",
