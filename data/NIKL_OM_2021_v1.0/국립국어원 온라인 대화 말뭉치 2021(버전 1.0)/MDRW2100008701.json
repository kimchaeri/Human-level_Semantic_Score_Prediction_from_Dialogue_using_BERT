      "utterance": [
        {
          "id": "MDRW2100008701.1.1",
          "form": "name1님 좋아하는 맛집 있으세요?",
          "original_form": "&name1&님 좋아하는 맛집 있으세요?",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100008701.1.2",
          "form": "자주 먹게 되는 곳은 등촌칼국수요!",
          "original_form": "자주 먹게 되는 곳은 등촌칼국수요!",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100008701.1.3",
          "form": "고기도 먹을 수 있고 칼국수도 먹고 볶음밥도 먹고!",
          "original_form": "고기도 먹을 수 있고 칼국수도 먹고 볶음밥도 먹고!",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100008701.1.4",
          "form": "오 저도 칼국수 좋아해요",
          "original_form": "오 저도 칼국수 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100008701.1.5",
          "form": "등촌은 근처에 없어서 넘 슬퍼요ㅠㅠ",
          "original_form": "등촌은 근처에 없어서 넘 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100008701.1.6",
          "form": "요즘에는 배달도 되어서 너무 편하더라구요",
          "original_form": "요즘에는 배달도 되어서 너무 편하더라구요",
          "speaker_id": "1",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100008701.1.7",
          "form": "지점이 흔하지는 않죠ㅠㅠ",
          "original_form": "지점이 흔하지는 않죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100008701.1.8",
          "form": "name2님은 자주 가는 맛집 있으세요?",
          "original_form": "&name2&님은 자주 가는 맛집 있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100008701.1.9",
          "form": "저는 요새는 마라탕집!",
          "original_form": "저는 요새는 마라탕집!",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100008701.1.10",
          "form": "그리고 샤브샤브집 좋아해요",
          "original_form": "그리고 샤브샤브집 좋아해요",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100008701.1.11",
          "form": "오오 저도 한때 마라탕 많이 먹었었는데ㅋㅋㅋㅋ 맛이 중독성있죠",
          "original_form": "오오 저도 한때 마라탕 많이 먹었었는데ㅋㅋㅋㅋ 맛이 중독성있죠",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100008701.1.12",
          "form": "저는 마라탕도 고기 많이 넣어먹어요ㅋㅋㅋㅋ",
          "original_form": "저는 마라탕도 고기 많이 넣어먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100008701.1.13",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100008701.1.14",
          "form": "근데 전 양고기보다 소고기를 좋아해요ㅋㅋㅋ",
          "original_form": "근데 전 양고기보다 소고기를 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100008701.1.15",
          "form": "저도요! 양고기는 특유의 누린내가 힘들어요ㅠㅠ",
          "original_form": "저도요! 양고기는 특유의 누린내가 힘들어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100008701.1.16",
          "form": "그쵸 특히 탕에는 더 그렇더라고요ㅠㅠ",
          "original_form": "그쵸 특히 탕에는 더 그렇더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100008701.1.17",
          "form": "그래서 항상 소고기로..ㅎㅎ",
          "original_form": "그래서 항상 소고기로..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100008701.1.18",
          "form": "하이디라오 처음가서 양고기로 도전했다가 먹기 힘들었던 경험이 있네요ㅠㅠ",
          "original_form": "하이디라오 처음가서 양고기로 도전했다가 먹기 힘들었던 경험이 있네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:21"
        },
        {
          "id": "MDRW2100008701.1.19",
          "form": "오 저 하이디라오는 안가봤어요",
          "original_form": "오 저 하이디라오는 안가봤어요",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100008701.1.20",
          "form": "근처에 없어서ㅠㅠ서울가면 가봐야지 한답니다ㅎ",
          "original_form": "근처에 없어서ㅠㅠ서울가면 가봐야지 한답니다ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100008701.1.21",
          "form": "제친구들은 엄청 좋아하는데 저는 좀 안맞았어요",
          "original_form": "제친구들은 엄청 좋아하는데 저는 좀 안맞았어요",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100008701.1.22",
          "form": "토종 한국인 입맛인가봐요ㅋㅋㅋ",
          "original_form": "토종 한국인 입맛인가봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100008701.1.23",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100008701.1.24",
          "form": "향신료 맛이 좀 센가",
          "original_form": "향신료 맛이 좀 센가",
          "speaker_id": "2",
          "time": "20210827 19:31"
        }
      ]
    }
