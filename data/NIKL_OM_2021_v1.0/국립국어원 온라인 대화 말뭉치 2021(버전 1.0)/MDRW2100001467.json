        },
        {
          "id": "MDRW2100001467.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211014 18:42"
        },
        {
          "id": "MDRW2100001467.1.29",
