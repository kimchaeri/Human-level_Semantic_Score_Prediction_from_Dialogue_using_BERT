        },
        {
          "id": "MDRW2100021198.1.3",
          "form": "양귀자 작가님도",
          "original_form": "양귀자 작가님도",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021198.1.4",
          "form": "좋은데",
          "original_form": "좋은데",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021198.1.5",
          "form": "위저드베이커리들어봄",
          "original_form": "위저드베이커리들어봄",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021198.1.6",
          "form": "아 이거쓴분이시구나",
          "original_form": "아 이거쓴분이시구나",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021198.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021198.1.8",
          "form": "아 나 그거 들어봄",
          "original_form": "아 나 그거 들어봄",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021198.1.9",
          "form": "그거 청소년 필독 도서 아니야?",
          "original_form": "그거 청소년 필독 도서 아니야?",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.10",
          "form": "뭐??",
          "original_form": "뭐??",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.11",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.12",
          "form": "구병모작가님",
          "original_form": "구병모작가님",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.13",
          "form": "작품이라는대",
          "original_form": "작품이라는대",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.14",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.15",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.16",
          "form": "그거 쓰시고 상받았으셨던 걸로 기억함",
          "original_form": "그거 쓰시고 상받았으셨던 걸로 기억함",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.17",
          "form": "어떻게 이런 작가님을",
          "original_form": "어떻게 이런 작가님을",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.18",
          "form": "성별하나로떨어뜨리냐",
          "original_form": "성별하나로떨어뜨리냐",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.19",
          "form": "말이되나",
          "original_form": "말이되나",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021198.1.20",
          "form": "그니까 가장 열려 있어야 할 사람들이",
          "original_form": "그니까 가장 열려 있어야 할 사람들이",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021198.1.21",
          "form": "고여있으니까 생기는 문제지 진짜",
          "original_form": "고여있으니까 생기는 문제지 진짜",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021198.1.22",
          "form": "책 주요 소비자들 보면 여자잖아",
          "original_form": "책 주요 소비자들 보면 여자잖아",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021198.1.23",
          "form": "아예싹갈아엎아야됨",
          "original_form": "아예싹갈아엎아야됨",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021198.1.24",
          "form": "ㅇㅈㅇㅈ 그래도 요즘 그런 문학이 감소세를 보여서 참 다행이긴 한데",
          "original_form": "ㅇㅈㅇㅈ 그래도 요즘 그런 문학이 감소세를 보여서 참 다행이긴 한데",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021198.1.25",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021198.1.26",
          "form": "점점 책 읽는 성별차이도",
          "original_form": "점점 책 읽는 성별차이도",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021198.1.27",
          "form": "커지더라",
          "original_form": "커지더라",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021198.1.28",
          "form": "남자들은라노벨?",
          "original_form": "남자들은라노벨?",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021198.1.29",
          "form": "이것만보고",
          "original_form": "이것만보고",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021198.1.30",
          "form": "여자들은책열심히읽고",
          "original_form": "여자들은책열심히읽고",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021198.1.31",
          "form": "그니까 그래서",
          "original_form": "그니까 그래서",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021198.1.32",
          "form": "아예 출판사에서",
          "original_form": "아예 출판사에서",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021198.1.33",
          "form": "그런 남성 대상 이벤트 같은 거 사라진지 오래잖아",
          "original_form": "그런 남성 대상 이벤트 같은 거 사라진지 오래잖아",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021198.1.34",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021198.1.35",
