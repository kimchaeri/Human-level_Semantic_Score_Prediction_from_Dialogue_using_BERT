      "utterance": [
        {
          "id": "MDRW2100036704.1.1",
          "form": "본인이 못한다고 남을 괴롭히면 되나",
          "original_form": "본인이 못한다고 남을 괴롭히면 되나",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.2",
          "form": "화나는것도 니마음",
          "original_form": "화나는것도 니마음",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.3",
          "form": "응응 그렇긴 하지..",
          "original_form": "응응 그렇긴 하지..",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.4",
          "form": "좋은마음도 니마음이니",
          "original_form": "좋은마음도 니마음이니",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.5",
          "form": "근데 애들이 아직은 감정컨트롤이 잘 안되니깐 ㅠㅠ",
          "original_form": "근데 애들이 아직은 감정컨트롤이 잘 안되니깐 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.6",
          "form": "우리 첫째두 그래...",
          "original_form": "우리 첫째두 그래...",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.7",
          "form": "스스로",
          "original_form": "스스로",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.8",
          "form": "자존심상해하구...막 달려든다마",
          "original_form": "자존심상해하구...막 달려든다마",
          "speaker_id": "1",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.9",
          "form": "해결하라고...",
          "original_form": "해결하라고...",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.10",
          "form": "그래 ㅠ",
          "original_form": "그래 ㅠ",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.11",
          "form": "그놈의 자존심",
          "original_form": "그놈의 자존심",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036704.1.12",
          "form": "맞다 맞다",
          "original_form": "맞다 맞다",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.13",
          "form": "오늘 아침에도 칫솔질하다가 내가 입술에 있다고",
          "original_form": "오늘 아침에도 칫솔질하다가 내가 입술에 있다고",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.14",
          "form": "했다고",
          "original_form": "했다고",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.15",
          "form": "요즘 나는 첫째 보면은 싸움닭인줄.....애들 다",
          "original_form": "요즘 나는 첫째 보면은 싸움닭인줄.....애들 다",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.16",
          "form": "발을 동동구르고",
          "original_form": "발을 동동구르고",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아구 ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아구 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.19",
          "form": "싸움닭 인정",
          "original_form": "싸움닭 인정",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.20",
          "form": "그건 아프지 ㅠㅠ",
          "original_form": "그건 아프지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.21",
          "form": "진짜 ㅋㅋㅋㅋㄱㄱ",
          "original_form": "진짜 ㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.22",
          "form": "맨날 싸운다",
          "original_form": "맨날 싸운다",
          "speaker_id": "2",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036704.1.23",
