      "utterance": [
        {
          "id": "MDRW2100022368.1.1",
          "form": "혹시 고양이 좋아하시나요?",
          "original_form": "혹시 고양이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022368.1.2",
          "form": "네 좋아해요.",
          "original_form": "네 좋아해요.",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022368.1.3",
          "form": "그럼 고양이 키우시나요?",
          "original_form": "그럼 고양이 키우시나요?",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022368.1.4",
          "form": "네 키우고 있어요",
          "original_form": "네 키우고 있어요",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022368.1.5",
          "form": "어떤 종을 키우고 계시나요",
          "original_form": "어떤 종을 키우고 계시나요",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022368.1.6",
          "form": "전 코리안 숏헤어 키우고 있어요",
          "original_form": "전 코리안 숏헤어 키우고 있어요",
          "speaker_id": "2",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022368.1.7",
          "form": "저도 코숏키우는데 반가워요 ㅎㅎ",
          "original_form": "저도 코숏키우는데 반가워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022368.1.8",
          "form": "아 님도 고양이 키우시나보네요",
          "original_form": "아 님도 고양이 키우시나보네요",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022368.1.9",
          "form": "네! 고양이 정말 좋죠",
          "original_form": "네! 고양이 정말 좋죠",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022368.1.10",
          "form": "혹시 사료는 뭐 먹이고 있나요?",
          "original_form": "혹시 사료는 뭐 먹이고 있나요?",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022368.1.11",
          "form": "전 로얄캐닌 먹이고 있어요",
          "original_form": "전 로얄캐닌 먹이고 있어요",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022368.1.12",
          "form": "전 토우 먹이는데",
          "original_form": "전 토우 먹이는데",
          "speaker_id": "2",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022368.1.13",
          "form": "캣타워도 있나요?",
          "original_form": "캣타워도 있나요?",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022368.1.14",
          "form": "네 캣타워도 있어요",
          "original_form": "네 캣타워도 있어요",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022368.1.15",
          "form": "저도 캣타워 있는데 고양이 키우려면 캣타워는 필수죠",
          "original_form": "저도 캣타워 있는데 고양이 키우려면 캣타워는 필수죠",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022368.1.16",
          "form": "맞아요 애들이 정말 좋아하더라구요 거기서 장난도 많이치고",
          "original_form": "맞아요 애들이 정말 좋아하더라구요 거기서 장난도 많이치고",
          "speaker_id": "2",
          "time": "20210829 13:18"
        }
