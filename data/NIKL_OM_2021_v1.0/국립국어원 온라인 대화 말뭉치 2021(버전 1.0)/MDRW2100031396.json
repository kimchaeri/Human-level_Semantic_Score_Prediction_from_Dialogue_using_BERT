      "utterance": [
        {
          "id": "MDRW2100031396.1.1",
          "form": "엄마 월욜에",
          "original_form": "엄마 월욜에",
          "speaker_id": "1",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031396.1.2",
          "form": "내 케이크 넣으려면",
          "original_form": "내 케이크 넣으려면",
          "speaker_id": "1",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031396.1.3",
          "form": "냉장고 비워야함",
          "original_form": "냉장고 비워야함",
          "speaker_id": "1",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031396.1.4",
          "form": "냉장고 지금 가득차있는데",
          "original_form": "냉장고 지금 가득차있는데",
          "speaker_id": "2",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031396.1.5",
          "form": "큰일이네",
          "original_form": "큰일이네",
          "speaker_id": "2",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100031396.1.6",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031396.1.7",
          "form": "오늘 산 피칸파이 부지런히 먹어",
          "original_form": "오늘 산 피칸파이 부지런히 먹어",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031396.1.8",
          "form": "그거라도 상자 빼면 낫지",
          "original_form": "그거라도 상자 빼면 낫지",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031396.1.9",
          "form": "요거트도 먹어서 치워라",
          "original_form": "요거트도 먹어서 치워라",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031396.1.10",
          "form": "그래야겠다",
          "original_form": "그래야겠다",
          "speaker_id": "1",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100031396.1.11",
          "form": "낼 먹어야지 얼마 안 남았지?",
          "original_form": "낼 먹어야지 얼마 안 남았지?",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031396.1.12",
          "form": "그럴거 가튼데",
          "original_form": "그럴거 가튼데",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031396.1.13",
          "form": "한번 먹을정도 남은듯",
          "original_form": "한번 먹을정도 남은듯",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031396.1.14",
          "form": "ㅇㅋ 낼 먹어 치워야지",
          "original_form": "ㅇㅋ 낼 먹어 치워야지",
          "speaker_id": "1",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031396.1.15",
          "form": "냉장고에 또 뭐있지..",
          "original_form": "냉장고에 또 뭐있지..",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031396.1.16",
          "form": "뭐가 가득한데 뭔지 모르겠다",
          "original_form": "뭐가 가득한데 뭔지 모르겠다",
          "speaker_id": "1",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031396.1.17",
          "form": "그러게말이다",
          "original_form": "그러게말이다",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031396.1.18",
          "form": "반찬도 없는데 지금",
          "original_form": "반찬도 없는데 지금",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031396.1.19",
          "form": "낼까지 꽃게 다 먹어치우면",
          "original_form": "낼까지 꽃게 다 먹어치우면",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031396.1.20",
          "form": "자리 날거 같다",
          "original_form": "자리 날거 같다",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031396.1.21",
          "form": "꽃게 통 지금 크지?",
          "original_form": "꽃게 통 지금 크지?",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031396.1.22",
          "form": "낼 싹 비워지겠네",
          "original_form": "낼 싹 비워지겠네",
          "speaker_id": "1",
          "time": "20210904 21:20"
        }
      ]
    }
