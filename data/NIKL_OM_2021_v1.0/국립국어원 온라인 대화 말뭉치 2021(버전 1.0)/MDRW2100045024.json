      "utterance": [
        {
          "id": "MDRW2100045024.1.1",
          "form": "ㅜㅠㅠㅠㅠㅠㅠ아 저도 쏘맥 가장 좋아했는데ㅠㅠㅠ",
          "original_form": "ㅜㅠㅠㅠㅠㅠㅠ아 저도 쏘맥 가장 좋아했는데ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045024.1.2",
          "form": "ㅜㅜ 둘다 아련...ㅜ",
          "original_form": "ㅜㅜ 둘다 아련...ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045024.1.3",
          "form": "와인은 건강에 좋다던데요ㅜ",
          "original_form": "와인은 건강에 좋다던데요ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045024.1.4",
          "form": "아니근데 맥주도",
          "original_form": "아니근데 맥주도",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045024.1.5",
          "form": "막 한잔은 건강에 좋댔다가 아니랬다가 ㅋㅋㅋㅋㅋ",
          "original_form": "막 한잔은 건강에 좋댔다가 아니랬다가 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045024.1.6",
          "form": "그래서 와인 건강썰도 ㅋㅋ그냥 흘려듣게 되었습니다 ㅋㅋㅋㅋ",
          "original_form": "그래서 와인 건강썰도 ㅋㅋ그냥 흘려듣게 되었습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045024.1.7",
          "form": "ㅋㅋ좋다는거 다 광고ㅋㅋ",
          "original_form": "ㅋㅋ좋다는거 다 광고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045024.1.8",
          "form": "카페가면 단 음료 드시나요?",
          "original_form": "카페가면 단 음료 드시나요?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045024.1.9",
          "form": "ㅋㅋㅋ아 아니요 ㅠ 너무단건 잘 못마셔서",
          "original_form": "ㅋㅋㅋ아 아니요 ㅠ 너무단건 잘 못마셔서",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045024.1.10",
          "form": "과일쥬스 시킬때 시럽은 빼달라고해여 ㅋㅋㅋ",
          "original_form": "과일쥬스 시킬때 시럽은 빼달라고해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045024.1.11",
          "form": "저도 건강때매ㅋㅋ",
          "original_form": "저도 건강때매ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045024.1.12",
          "form": "아메나 티 먹어요ㅜ",
          "original_form": "아메나 티 먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045024.1.13",
          "form": "ㅜㅜㅜ저두요ㅠㅠ",
          "original_form": "ㅜㅜㅜ저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045024.1.14",
          "form": "카페인약해서..",
          "original_form": "카페인약해서..",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045024.1.15",
          "form": "페퍼민트티 주로마셔요ㅠㅠ",
          "original_form": "페퍼민트티 주로마셔요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045024.1.16",
          "form": "아ㅜ 전 카모마일이랑 유스베리",
          "original_form": "아ㅜ 전 카모마일이랑 유스베리",
