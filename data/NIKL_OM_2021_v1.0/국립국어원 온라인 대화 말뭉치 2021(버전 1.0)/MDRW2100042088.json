      "utterance": [
        {
          "id": "MDRW2100042088.1.1",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042088.1.2",
          "form": "그 쪽은 비가 오나요?",
          "original_form": "그 쪽은 비가 오나요?",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042088.1.3",
          "form": "여긴 안오네요",
          "original_form": "여긴 안오네요",
          "speaker_id": "2",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042088.1.4",
          "form": "앗 그런가요?",
          "original_form": "앗 그런가요?",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042088.1.5",
          "form": "저희는 장대비가 내리고 있어요",
          "original_form": "저희는 장대비가 내리고 있어요",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042088.1.6",
          "form": "서울인가요??",
          "original_form": "서울인가요??",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100042088.1.7",
          "form": "경기요",
          "original_form": "경기요",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042088.1.8",
          "form": "전국에 다 비가 내리는 건 아닌가보네요",
          "original_form": "전국에 다 비가 내리는 건 아닌가보네요",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042088.1.9",
          "form": "저도 경긴데 혹시 남부신가요?",
          "original_form": "저도 경긴데 혹시 남부신가요?",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100042088.1.10",
          "form": "앗 저는 중서부?",
          "original_form": "앗 저는 중서부?",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100042088.1.11",
          "form": "경기가 넓어서 날씨 차이가 있긴 하죠ㅎㅎ",
          "original_form": "경기가 넓어서 날씨 차이가 있긴 하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100042088.1.12",
          "form": "그래서 그런가봐요ㅎㅎㅎ",
          "original_form": "그래서 그런가봐요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100042088.1.13",
          "form": "좋아하는 계절이나 날씨 있으신강ㅅ?",
          "original_form": "좋아하는 계절이나 날씨 있으신강ㅅ?",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100042088.1.14",
          "form": "가요?",
          "original_form": "가요?",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100042088.1.15",
          "form": "저는 봄가을 날씨 좋아해요",
          "original_form": "저는 봄가을 날씨 좋아해요",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100042088.1.16",
          "form": "따뜻 선선",
          "original_form": "따뜻 선선",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100042088.1.17",
          "form": "저도 날씨는 봄가을이 좋은데 먼지때문에 계절은 겨울을 더 좋아해요",
          "original_form": "저도 날씨는 봄가을이 좋은데 먼지때문에 계절은 겨울을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100042088.1.18",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100042088.1.19",
          "form": "요즘 봄은 미세먼지 황사가ㅜ",
          "original_form": "요즘 봄은 미세먼지 황사가ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042088.1.20",
          "form": "그럼 지금 날씨 좋아하시겠어요",
          "original_form": "그럼 지금 날씨 좋아하시겠어요",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042088.1.21",
          "form": "네 선선한 날씨 좋아해요ㅎㅎ",
          "original_form": "네 선선한 날씨 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100042088.1.22",
          "form": "이번 주말 파란 하늘도 넘 예쁘고",
          "original_form": "이번 주말 파란 하늘도 넘 예쁘고",
          "speaker_id": "1",
          "time": "20210906 20:08"
        }
