      "utterance": [
        {
          "id": "MDRW2100038059.1.1",
          "form": "좋아해",
          "original_form": "좋아해",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.2",
          "form": "사고싶다",
          "original_form": "사고싶다",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.3",
          "form": "근데 가격 너무 오르지 않았어m",
          "original_form": "근데 가격 너무 오르지 않았어m",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.4",
          "form": "최근에 구매하고싶은거 있니?",
          "original_form": "최근에 구매하고싶은거 있니?",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.5",
          "form": "나는 그래서 인터넥으로 카카오샵 들어가서",
          "original_form": "나는 그래서 인터넥으로 카카오샵 들어가서",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.6",
          "form": "나는 항상 사고싶은결 하트해둬",
          "original_form": "나는 항상 사고싶은결 하트해둬",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.7",
          "form": "20퍼 할인받아서 사둬",
          "original_form": "20퍼 할인받아서 사둬",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.8",
          "form": "그래도 비싸",
          "original_form": "그래도 비싸",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.9",
          "form": "예전에 초코 ㅁㅎ았을 땐 좋았어",
          "original_form": "예전에 초코 ㅁㅎ았을 땐 좋았어",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.10",
          "form": "어우 그때ㅜ너무 좋았어",
          "original_form": "어우 그때ㅜ너무 좋았어",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.11",
          "form": "귀여운 이모티콘이 참 많아",
          "original_form": "귀여운 이모티콘이 참 많아",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.12",
          "form": "다 갖고싶다",
          "original_form": "다 갖고싶다",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.13",
          "form": "공짜로 이모티콘을 사다니 옛날이여",
          "original_form": "공짜로 이모티콘을 사다니 옛날이여",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.14",
          "form": "맞아.  열심히 모았었지",
          "original_form": "맞아.  열심히 모았었지",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.15",
          "form": "옛날이여 ㅋㅋㅋㅋㅋ 재밌다 말투가.",
          "original_form": "옛날이여 ㅋㅋㅋㅋㅋ 재밌다 말투가.",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.16",
          "form": "너 오리 이모티콘 좋아하지?",
          "original_form": "너 오리 이모티콘 좋아하지?",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038059.1.17",
          "form": "너는 요즘 사고싶은 이모티콘 있오m",
          "original_form": "너는 요즘 사고싶은 이모티콘 있오m",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038059.1.18",
          "form": "응 난 오리 새 비둘기 다 좋아해",
          "original_form": "응 난 오리 새 비둘기 다 좋아해",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038059.1.19",
          "form": "새 안에 다 들어가는거지만...",
          "original_form": "새 안에 다 들어가는거지만...",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038059.1.20",
          "form": "나도 오리 이모티콘 좋아해 나중에 귀여운거 공유해줘",
          "original_form": "나도 오리 이모티콘 좋아해 나중에 귀여운거 공유해줘",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038059.1.21",
          "form": "강아지도 좋아해",
          "original_form": "강아지도 좋아해",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
