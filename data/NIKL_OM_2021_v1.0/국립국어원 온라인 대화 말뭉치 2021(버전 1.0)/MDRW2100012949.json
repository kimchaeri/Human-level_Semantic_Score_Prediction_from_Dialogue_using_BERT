      "utterance": [
        {
          "id": "MDRW2100012949.1.1",
          "form": "좀 더 쉴라면 유튜브가 필요하니 ㅋㅋㅋㅋㅋ",
          "original_form": "좀 더 쉴라면 유튜브가 필요하니 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:15"
        },
        {
          "id": "MDRW2100012949.1.2",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210825 09:15"
        },
        {
          "id": "MDRW2100012949.1.3",
          "form": "근데 쉴때 폰만 보니까",
          "original_form": "근데 쉴때 폰만 보니까",
          "speaker_id": "2",
          "time": "20210825 09:15"
        },
        {
          "id": "MDRW2100012949.1.4",
          "form": "폰 안보는시간은",
          "original_form": "폰 안보는시간은",
          "speaker_id": "2",
          "time": "20210825 09:15"
        },
        {
          "id": "MDRW2100012949.1.5",
          "form": "애가 뭘 해야될지 몰라하는 느낌...",
          "original_form": "애가 뭘 해야될지 몰라하는 느낌...",
          "speaker_id": "2",
          "time": "20210825 09:15"
        },
        {
          "id": "MDRW2100012949.1.6",
          "form": "그래가 언니가 데리고 나가는거 아니라요??",
          "original_form": "그래가 언니가 데리고 나가는거 아니라요??",
          "speaker_id": "1",
          "time": "20210825 09:19"
        },
        {
          "id": "MDRW2100012949.1.7",
          "form": "난도 느끼는데..데리고 나가기꺼지가 참 힘들어여 ㅋㅋㅋㅋㅋ",
          "original_form": "난도 느끼는데..데리고 나가기꺼지가 참 힘들어여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:19"
        },
        {
          "id": "MDRW2100012949.1.8",
          "form": "그래 매일 할게없다하니",
          "original_form": "그래 매일 할게없다하니",
          "speaker_id": "2",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012949.1.9",
          "form": "차라리 밖에서 굴리는게 낫지",
          "original_form": "차라리 밖에서 굴리는게 낫지",
          "speaker_id": "2",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012949.1.10",
          "form": "폰만 보여줄수도 없고",
          "original_form": "폰만 보여줄수도 없고",
          "speaker_id": "2",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012949.1.11",
          "form": "맞아요~둘찌는 보다가 정도껏보면 끄눈데",
          "original_form": "맞아요~둘찌는 보다가 정도껏보면 끄눈데",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012949.1.12",
          "form": "요즘 첫찌는 컨트롤 안되여~",
          "original_form": "요즘 첫찌는 컨트롤 안되여~",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012949.1.13",
          "form": "어릴땐 관심없디 폭발함 ㅋㅋㅋㅋ",
          "original_form": "어릴땐 관심없디 폭발함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012949.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 뒤늦게 폭발했네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 뒤늦게 폭발했네",
          "speaker_id": "2",
          "time": "20210825 10:28"
        },
        {
          "id": "MDRW2100012949.1.15",
          "form": "그러니깐요~지금도 뭐..",
          "original_form": "그러니깐요~지금도 뭐..",
          "speaker_id": "1",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100012949.1.16",
          "form": "엄마야 일은하던말던 ㅋㅋㅋ유튜브 봅니다,,",
          "original_form": "엄마야 일은하던말던 ㅋㅋㅋ유튜브 봅니다,,",
          "speaker_id": "1",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100012949.1.17",
          "form": "일도와준다고 맨날 뻥치고",
          "original_form": "일도와준다고 맨날 뻥치고",
          "speaker_id": "1",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100012949.1.18",
          "form": "ㄹㅋㅋㅋ 오늘은 같이왔구먼",
          "original_form": "ㄹㅋㅋㅋ 오늘은 같이왔구먼",
          "speaker_id": "2",
          "time": "20210825 11:24"
        },
        {
          "id": "MDRW2100012949.1.19",
          "form": "아부지 집에 가셨나?",
          "original_form": "아부지 집에 가셨나?",
          "speaker_id": "2",
          "time": "20210825 11:24"
        },
        {
          "id": "MDRW2100012949.1.20",
          "form": "어제 차 밧데리.나가서 갈고 저녁먹으로 온다더니",
          "original_form": "어제 차 밧데리.나가서 갈고 저녁먹으로 온다더니",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100012949.1.21",
          "form": "내뺐어요 ㅋㅋㅋㅋㅋ",
          "original_form": "내뺐어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100012949.1.22",
          "form": "첫찌 노발대발 ㅋㅋㅋㅋㅋ",
          "original_form": "첫찌 노발대발 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:31"
        },
        {
          "id": "MDRW2100012949.1.23",
          "form": "힘드셨군",
          "original_form": "힘드셨군",
          "speaker_id": "2",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100012949.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100012949.1.25",
          "form": "쉽지않음",
          "original_form": "쉽지않음",
          "speaker_id": "2",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100012949.1.26",
          "form": "웃긴게 혹시나 자다가 열나나 싶어서 ㅋㅋㅋ",
          "original_form": "웃긴게 혹시나 자다가 열나나 싶어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100012949.1.27",
          "form": "신랑이랑 내랑 첫찌랑 돌아가면서 계속 이마짚음 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "신랑이랑 내랑 첫찌랑 돌아가면서 계속 이마짚음 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100012949.1.28",
          "form": "잠 한숨도 못잤다고 ㅋㅋㅋㅋㅋ",
          "original_form": "잠 한숨도 못잤다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100012949.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100012949.1.30",
          "form": "아부지\r\n자상하시노 ㅎㅎㅎ",
          "original_form": "아부지\r\n자상하시노 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100012949.1.31",
          "form": "언니..아부지가 당한거요 ㅋㅋㅋㅋㅋ",
          "original_form": "언니..아부지가 당한거요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100012949.1.32",
          "form": "울아빠는 내 옆에도 잘 안와요 ㅋㅋㅋㅋ",
          "original_form": "울아빠는 내 옆에도 잘 안와요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012949.1.33",
          "form": "아 ㅋㅋㅋㅋ 너거가 아부지 이마를 짚었다고 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 너거가 아부지 이마를 짚었다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012949.1.34",
          "form": "백신맞고 열나실까봐",
          "original_form": "백신맞고 열나실까봐",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012949.1.35",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 웃기노 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 웃기노 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012949.1.36",
          "form": "그와중에 첫찌 까지 ㅋㅋㅋㅋㄱ",
          "original_form": "그와중에 첫찌 까지 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012949.1.37",
          "form": "질려가 떠났어요 ㅋㅋㅋㅋㅋ",
          "original_form": "질려가 떠났어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012949.1.38",
          "form": "밥다차려놨는데 잠수 ㅋㅋㅋㅋ",
          "original_form": "밥다차려놨는데 잠수 ㅋㅋㅋㅋ",
