      "utterance": [
        {
          "id": "MDRW2100029358.1.1",
          "form": "책조아하세여?",
          "original_form": "책조아하세여?",
          "speaker_id": "1",
          "time": "20210903 02:45"
        },
        {
          "id": "MDRW2100029358.1.2",
          "form": "조금 좋아하는 편이에요ㅎ",
          "original_form": "조금 좋아하는 편이에요ㅎ",
          "speaker_id": "2",
          "time": "20210903 02:45"
        },
        {
          "id": "MDRW2100029358.1.3",
          "form": "오 머조아해요",
          "original_form": "오 머조아해요",
          "speaker_id": "1",
          "time": "20210903 02:45"
        },
        {
          "id": "MDRW2100029358.1.4",
          "form": "좀 재미없는 책들이라.. 자기계발서나 에세이 읽어요",
          "original_form": "좀 재미없는 책들이라.. 자기계발서나 에세이 읽어요",
          "speaker_id": "2",
          "time": "20210903 02:46"
        },
        {
          "id": "MDRW2100029358.1.5",
          "form": "학생때는 소설류만 읽었는데 지금은 안읽게되더라구요",
          "original_form": "학생때는 소설류만 읽었는데 지금은 안읽게되더라구요",
          "speaker_id": "2",
          "time": "20210903 02:47"
        },
        {
          "id": "MDRW2100029358.1.6",
          "form": "투자책도 자주 읽는편이구..",
          "original_form": "투자책도 자주 읽는편이구..",
          "speaker_id": "2",
          "time": "20210903 02:47"
        },
        {
          "id": "MDRW2100029358.1.7",
          "form": "오 ...전 소설만 읽음",
          "original_form": "오 ...전 소설만 읽음",
          "speaker_id": "1",
          "time": "20210903 02:49"
        },
        {
          "id": "MDRW2100029358.1.8",
          "form": "투자책 머요?",
          "original_form": "투자책 머요?",
          "speaker_id": "1",
          "time": "20210903 02:49"
        },
        {
          "id": "MDRW2100029358.1.9",
          "form": "주로 주식이지만 부동산, 채권 달러, 짠테크 두루두루 봅니다ㅎㅎ",
          "original_form": "주로 주식이지만 부동산, 채권 달러, 짠테크 두루두루 봅니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:50"
        },
        {
          "id": "MDRW2100029358.1.10",
          "form": "오 직접 투자하시며?",
          "original_form": "오 직접 투자하시며?",
          "speaker_id": "1",
          "time": "20210903 02:51"
        },
        {
          "id": "MDRW2100029358.1.11",
          "form": "주식만 조금하고 있어요ㅎ",
          "original_form": "주식만 조금하고 있어요ㅎ",
          "speaker_id": "2",
          "time": "20210903 06:03"
        },
        {
          "id": "MDRW2100029358.1.12",
          "form": "책이 도움이 되나용",
          "original_form": "책이 도움이 되나용",
          "speaker_id": "1",
          "time": "20210903 08:06"
        },
        {
          "id": "MDRW2100029358.1.13",
          "form": "처음 시작할때는 큰도움이 되더라구요",
          "original_form": "처음 시작할때는 큰도움이 되더라구요",
          "speaker_id": "2",
          "time": "20210903 08:45"
        },
        {
          "id": "MDRW2100029358.1.14",
          "form": "오 ..머읽으셧어요",
          "original_form": "오 ..머읽으셧어요",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100029358.1.15",
          "form": "몇권을 짬뽕해서 읽었더니 마땅히 떠오르는게 없네요ㅎㅎ",
          "original_form": "몇권을 짬뽕해서 읽었더니 마땅히 떠오르는게 없네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100029358.1.16",
          "form": "급등 조건을 다루는 책도 있었고 투자마인드를 다루는 책도 있었어요",
          "original_form": "급등 조건을 다루는 책도 있었고 투자마인드를 다루는 책도 있었어요",
          "speaker_id": "2",
          "time": "20210903 08:47"
        },
        {
          "id": "MDRW2100029358.1.17",
          "form": "오호 ㅋㅋㅋ 투자마인드",
          "original_form": "오호 ㅋㅋㅋ 투자마인드",
          "speaker_id": "1",
          "time": "20210903 08:55"
        },
        {
          "id": "MDRW2100029358.1.18",
          "form": "저도부자되고싶네요",
          "original_form": "저도부자되고싶네요",
          "speaker_id": "1",
          "time": "20210903 08:55"
        }
      ]
