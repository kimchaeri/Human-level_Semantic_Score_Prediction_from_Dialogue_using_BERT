      "utterance": [
        {
          "id": "MDRW2100030237.1.1",
          "form": "그래도 니가 끓인건",
          "original_form": "그래도 니가 끓인건",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030237.1.2",
          "form": "편견없이 먹어줄게",
          "original_form": "편견없이 먹어줄게",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030237.1.3",
          "form": "정말 아량이 하해와 같구나",
          "original_form": "정말 아량이 하해와 같구나",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030237.1.4",
          "form": "웃이셔",
          "original_form": "웃이셔",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030237.1.5",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030237.1.6",
          "form": "내가 좀 그런면으로는 인정적이야",
          "original_form": "내가 좀 그런면으로는 인정적이야",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030237.1.7",
          "form": "진라면으로 라죽 끓여먹어",
          "original_form": "진라면으로 라죽 끓여먹어",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030237.1.8",
          "form": "넌 라죽 먹어봤니",
          "original_form": "넌 라죽 먹어봤니",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030237.1.9",
          "form": "라면 죽...?",
          "original_form": "라면 죽...?",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030237.1.10",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030237.1.11",
          "form": "처음 들어봐 어떻게 만드는거야",
          "original_form": "처음 들어봐 어떻게 만드는거야",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030237.1.12",
          "form": "라면끓일때 밥도 같이 넣고 끓여서",
          "original_form": "라면끓일때 밥도 같이 넣고 끓여서",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030237.1.13",
          "form": "죽처럼 만들어",
          "original_form": "죽처럼 만들어",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030237.1.14",
          "form": "그럼 면이랑 밥이랑",
          "original_form": "그럼 면이랑 밥이랑",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030237.1.15",
          "form": "따로 안 놀아?",
          "original_form": "따로 안 놀아?",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030237.1.16",
          "form": "탄탄의 조합이지",
          "original_form": "탄탄의 조합이지",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030237.1.17",
          "form": "은근 맛있어",
          "original_form": "은근 맛있어",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030237.1.18",
          "form": "밥이랑 면이 같이 씹혀서",
          "original_form": "밥이랑 면이 같이 씹혀서",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030237.1.19",
          "form": "흐미 생각지못한 조리법이여",
          "original_form": "흐미 생각지못한 조리법이여",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030237.1.20",
          "form": "면 다 먹고 밥 말아먹는 건",
          "original_form": "면 다 먹고 밥 말아먹는 건",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030237.1.21",
          "form": "해봤는데",
          "original_form": "해봤는데",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030237.1.22",
          "form": "동시에 같이 먹는 건ㅌㅋㅋㅋㅋ참신하다",
          "original_form": "동시에 같이 먹는 건ㅌㅋㅋㅋㅋ참신하다",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030237.1.23",
          "form": "한번 도전해봐",
          "original_form": "한번 도전해봐",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030237.1.24",
          "form": "면을 잘게 부셔서끓여야돼",
          "original_form": "면을 잘게 부셔서끓여야돼",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030237.1.25",
          "form": "아 긴 채로 두는 게 아니구나",
          "original_form": "아 긴 채로 두는 게 아니구나",
          "speaker_id": "1",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030237.1.26",
          "form": "난 면을 그대로 끓이는 건줄 알고",
          "original_form": "난 면을 그대로 끓이는 건줄 알고",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030237.1.27",
          "form": "그걸 동시에 같이 먹기가 되나?",
          "original_form": "그걸 동시에 같이 먹기가 되나?",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030237.1.28",
          "form": "이러고 있었는데 ㅋㅋㅋ",
          "original_form": "이러고 있었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030237.1.29",
          "form": "자르는거였구나",
          "original_form": "자르는거였구나",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030237.1.30",
          "form": "근데 맛은 비슷하지않냐",
          "original_form": "근데 맛은 비슷하지않냐",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030237.1.31",
          "form": "기냐 짧냐 차이",
          "original_form": "기냐 짧냐 차이",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030237.1.32",
          "form": "맛은 비슷할텐데 뭐랄까",
          "original_form": "맛은 비슷할텐데 뭐랄까",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.33",
          "form": "먹을때 편의성이 문제이지앟을까 싶었거든",
          "original_form": "먹을때 편의성이 문제이지앟을까 싶었거든",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.34",
          "form": "밥은 숟가락으로 먹는데",
          "original_form": "밥은 숟가락으로 먹는데",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.35",
          "form": "숟가락은 면이 고정이 안 되잖아",
          "original_form": "숟가락은 면이 고정이 안 되잖아",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.36",
          "form": "그것도 그래",
          "original_form": "그것도 그래",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.37",
          "form": "근데 난 길때도 나름 매력적이라",
          "original_form": "근데 난 길때도 나름 매력적이라",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.38",
          "form": "주르륵 흘러서 얼굴에 국물발사되는걸",
          "original_form": "주르륵 흘러서 얼굴에 국물발사되는걸",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030237.1.39",
          "form": "긴채로도 끓여",
          "original_form": "긴채로도 끓여",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030237.1.40",
          "form": "상상했는데",
          "original_form": "상상했는데",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030237.1.41",
          "form": "먹을때 불편해?",
          "original_form": "먹을때 불편해?",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100030237.1.42",
          "form": "먹을때 면이 70프로 씹혀서",
          "original_form": "먹을때 면이 70프로 씹혀서",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030237.1.43",
          "form": "그게 좋아",
          "original_form": "그게 좋아",
          "speaker_id": "2",
          "time": "20210903 23:17"
        }
      ]
    }
  ]
