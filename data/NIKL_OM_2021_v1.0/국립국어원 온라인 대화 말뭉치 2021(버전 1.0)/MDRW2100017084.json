      "utterance": [
        {
          "id": "MDRW2100017084.1.1",
          "form": "저 가리는건 없는데 어.. 김밥?",
          "original_form": "저 가리는건 없는데 어.. 김밥?",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017084.1.2",
          "form": "김밥좋아해요 name1은 떡볶이 좋아하나요!",
          "original_form": "김밥좋아해요 &name1&은 떡볶이 좋아하나요!",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017084.1.3",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017084.1.4",
          "form": "김밥도 맛있죠 ㅋㅋㅋ",
          "original_form": "김밥도 맛있죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017084.1.5",
          "form": "떡볶이는 쌀떡좋아하세요 밀떡 좋아하세쇼",
          "original_form": "떡볶이는 쌀떡좋아하세요 밀떡 좋아하세쇼",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017084.1.6",
          "form": "무조건 밀떡이요 ㅋㅋㅋ",
          "original_form": "무조건 밀떡이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017084.1.7",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017084.1.8",
          "form": "떡은 밀떡이 양념이 잘배서 좋아요",
          "original_form": "떡은 밀떡이 양념이 잘배서 좋아요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017084.1.9",
          "form": "맞아요 최고장점ㅋㅋㅋ",
          "original_form": "맞아요 최고장점ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017084.1.10",
          "form": "떡볶이는 어떤 떡볶이 좋아하셔요",
          "original_form": "떡볶이는 어떤 떡볶이 좋아하셔요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017084.1.11",
          "form": "다 좋아하는데 요샌 신전이 맛있더라구요 신참도요",
          "original_form": "다 좋아하는데 요샌 신전이 맛있더라구요 신참도요",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017084.1.12",
          "form": "신전 맛있어요",
          "original_form": "신전 맛있어요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017084.1.13",
