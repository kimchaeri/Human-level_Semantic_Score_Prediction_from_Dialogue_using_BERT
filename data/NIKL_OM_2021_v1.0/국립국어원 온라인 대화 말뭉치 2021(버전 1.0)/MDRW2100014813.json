      "utterance": [
        {
          "id": "MDRW2100014813.1.1",
          "form": "최근에 보신 영화 있나요?",
          "original_form": "최근에 보신 영화 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014813.1.2",
          "form": "음..영화안본지 너무 오래됐네요..",
          "original_form": "음..영화안본지 너무 오래됐네요..",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014813.1.3",
          "form": "아 크루엘라 본것 같아요 ㅎㅎ",
          "original_form": "아 크루엘라 본것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014813.1.4",
          "form": "오 크루엘라 궁금했던건데",
          "original_form": "오 크루엘라 궁금했던건데",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014813.1.5",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014813.1.6",
          "form": "넵 혹시 101마리 강아지 애니메이셬 보셨나요.?",
          "original_form": "넵 혹시 101마리 강아지 애니메이셬 보셨나요.?",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014813.1.7",
          "form": "네 그건봤어요!!",
          "original_form": "네 그건봤어요!!",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014813.1.8",
          "form": "전 그 애니메이션을 제일 좋아해서 볼만했던것 같아요!!",
          "original_form": "전 그 애니메이션을 제일 좋아해서 볼만했던것 같아요!!",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014813.1.9",
          "form": "귀여운 강아지들도 나오고 좋아요",
          "original_form": "귀여운 강아지들도 나오고 좋아요",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014813.1.10",
          "form": "오 엠마스톤포스터가 아주 강렬하더라고요",
          "original_form": "오 엠마스톤포스터가 아주 강렬하더라고요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014813.1.11",
          "form": "연기도 잘하죠?",
          "original_form": "연기도 잘하죠?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014813.1.12",
          "form": "믿고 보는 연기죠 ㅎㅎ",
          "original_form": "믿고 보는 연기죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014813.1.13",
          "form": "발음이 정말 멋지더라구요",
          "original_form": "발음이 정말 멋지더라구요",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014813.1.14",
          "form": "꼭 봐야겠네요 ㅋㅋㅋ",
          "original_form": "꼭 봐야겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014813.1.15",
          "form": "약간 패션영화인가요??",
          "original_form": "약간 패션영화인가요??",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014813.1.16",
          "form": "맞아요 패션+강아지",
          "original_form": "맞아요 패션+강아지",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014813.1.17",
          "form": "이런느낌 ㅎㅎ??",
          "original_form": "이런느낌 ㅎㅎ??",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014813.1.18",
          "form": "나중에 시간되시면 꼭 보세요 ㅎㅎ",
          "original_form": "나중에 시간되시면 꼭 보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014813.1.19",
          "form": "아하 꼭 볼게요",
          "original_form": "아하 꼭 볼게요",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014813.1.20",
          "form": "마블영화는 안보세요?",
          "original_form": "마블영화는 안보세요?",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014813.1.21",
          "form": "앗 전 마블 영화 잘 안봐요",
          "original_form": "앗 전 마블 영화 잘 안봐요",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014813.1.22",
          "form": "시리즈가 있어서 다 챙겨보는게 버겁더라구요",
          "original_form": "시리즈가 있어서 다 챙겨보는게 버겁더라구요",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014813.1.23",
          "form": "아 블위 추천해주려고했는데 아쉽네요 ㅋㅋ",
          "original_form": "아 블위 추천해주려고했는데 아쉽네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        }
      ]
