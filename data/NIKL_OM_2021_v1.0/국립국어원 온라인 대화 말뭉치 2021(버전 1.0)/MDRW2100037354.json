      "utterance": [
        {
          "id": "MDRW2100037354.1.1",
          "form": "무슨 다 살인사건 아니면 치정이야",
          "original_form": "무슨 다 살인사건 아니면 치정이야",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100037354.1.2",
          "form": "내용이 다 우울해",
          "original_form": "내용이 다 우울해",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037354.1.3",
          "form": "그러게 안은진이 주연이다",
          "original_form": "그러게 안은진이 주연이다",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037354.1.4",
          "form": "슬의생 나온 애 맞죠?",
          "original_form": "슬의생 나온 애 맞죠?",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037354.1.5",
          "form": "슬기로운 의사생활에서 추민하로 조연이다 바로 주연이네",
          "original_form": "슬기로운 의사생활에서 추민하로 조연이다 바로 주연이네",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037354.1.6",
          "form": "주인공까지..  많이 떳네요",
          "original_form": "주인공까지..  많이 떳네요",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037354.1.7",
          "form": "박성훈도 나오고",
          "original_form": "박성훈도 나오고",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037354.1.8",
          "form": "아까 그 박성훈이죠?",
          "original_form": "아까 그 박성훈이죠?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037354.1.9",
          "form": "레드벨벳의 조이도 나오나봐 헐",
          "original_form": "레드벨벳의 조이도 나오나봐 헐",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037354.1.10",
          "form": "아ㅠ 별로 보고 싶지 않네요",
          "original_form": "아ㅠ 별로 보고 싶지 않네요",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037354.1.11",
          "form": "응 아까 그 박성훈 ㅋㅋ",
          "original_form": "응 아까 그 박성훈 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037354.1.12",
          "form": "이분도 대게 바쁘시네요ㅋ",
          "original_form": "이분도 대게 바쁘시네요ㅋ",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037354.1.13",
          "form": "여기저기 다 출연이네 전소민도 그렇고",
          "original_form": "여기저기 다 출연이네 전소민도 그렇고",
