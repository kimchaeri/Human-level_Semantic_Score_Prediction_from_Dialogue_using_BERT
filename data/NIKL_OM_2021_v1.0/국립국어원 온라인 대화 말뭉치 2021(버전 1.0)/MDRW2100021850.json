      "utterance": [
        {
          "id": "MDRW2100021850.1.1",
          "form": "출석이벤트로 한 2만점 줬나봐요  쿠폰으로 할인 2~3만원되고  거기에 또 + 포인트 이벤트해서",
          "original_form": "출석이벤트로 한 2만점 줬나봐요  쿠폰으로 할인 2~3만원되고  거기에 또 + 포인트 이벤트해서",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021850.1.2",
          "form": "꿀이였어요",
          "original_form": "꿀이였어요",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021850.1.3",
          "form": "와 진짜 득템이네요ㅋㅋ",
          "original_form": "와 진짜 득템이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021850.1.4",
          "form": "한 2~3개월에 한번씩 하더라구요",
          "original_form": "한 2~3개월에 한번씩 하더라구요",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021850.1.5",
          "form": "어느 사이트에서 하는거에요?",
          "original_form": "어느 사이트에서 하는거에요?",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021850.1.6",
          "form": "h 패션몰요",
          "original_form": "h 패션몰요",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021850.1.7",
          "form": "지금도 하긴하는데 이번에 다른거 해요",
          "original_form": "지금도 하긴하는데 이번에 다른거 해요",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021850.1.8",
          "form": "아 저도 함 찾아봐야겠네요!ㅋㅋ",
          "original_form": "아 저도 함 찾아봐야겠네요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021850.1.9",
          "form": "좋은 정보 감사해요ㅎㅎ",
          "original_form": "좋은 정보 감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021850.1.10",
          "form": "근데 이번껀  50퍼 쿠폰인데  선착 50명이라 힘들더라구요",
          "original_form": "근데 이번껀  50퍼 쿠폰인데  선착 50명이라 힘들더라구요",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021850.1.11",
          "form": "아침 10시에  쿠폰은 선착이고  1만포는 그냥 줘요",
          "original_form": "아침 10시에  쿠폰은 선착이고  1만포는 그냥 줘요",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021850.1.12",
          "form": "아 50명 안에 드는건 정말 힘들죠ㅜㅜㅋㅋ",
          "original_form": "아 50명 안에 드는건 정말 힘들죠ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021850.1.13",
          "form": "그런거 받는 사람들은 어떻게 하는건지ㅋㅋ",
          "original_form": "그런거 받는 사람들은 어떻게 하는건지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021850.1.14",
          "form": "손들이 넘나 빠른 그래서전 출첵이 좋아요",
          "original_form": "손들이 넘나 빠른 그래서전 출첵이 좋아요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021850.1.15",
          "form": "콘서트 티케팅급이겠네요ㅋㅋ",
          "original_form": "콘서트 티케팅급이겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021850.1.16",
          "form": "1~2초컷 같더라구요 ㅎㅎㅎ",
          "original_form": "1~2초컷 같더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021850.1.17",
          "form": "치열하네요ㅜㅜㅋㅋ",
          "original_form": "치열하네요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021850.1.18",
          "form": "대단한 사람들",
          "original_form": "대단한 사람들",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021850.1.19",
          "form": "저도 존경 하네요 그분들? ㅋㅋ",
          "original_form": "저도 존경 하네요 그분들? ㅋㅋ",
