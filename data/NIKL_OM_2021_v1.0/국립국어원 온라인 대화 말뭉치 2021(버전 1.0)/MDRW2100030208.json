      "utterance": [
        {
          "id": "MDRW2100030208.1.1",
          "form": "그거보단 달아요",
          "original_form": "그거보단 달아요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030208.1.2",
          "form": "과일가게인가요",
          "original_form": "과일가게인가요",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030208.1.3",
          "form": "부드럽고요",
          "original_form": "부드럽고요",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030208.1.4",
          "form": "그건아니에요",
          "original_form": "그건아니에요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.5",
          "form": "바나나는 없나요",
          "original_form": "바나나는 없나요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.6",
          "form": "식감믄좋네요",
          "original_form": "식감믄좋네요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.7",
          "form": "바나나다먹은듯해요",
          "original_form": "바나나다먹은듯해요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.8",
          "form": "복숭아는 있나요",
          "original_form": "복숭아는 있나요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.9",
          "form": "복숭아는있는데",
          "original_form": "복숭아는있는데",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.10",
          "form": "귤도 없죠?",
          "original_form": "귤도 없죠?",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.11",
          "form": "지금은 안먹어요",
          "original_form": "지금은 안먹어요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.12",
          "form": "귤은없어요",
          "original_form": "귤은없어요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.13",
          "form": "아껴두나요",
          "original_form": "아껴두나요",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.14",
          "form": "감귤시럽은있어요",
          "original_form": "감귤시럽은있어요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.15",
          "form": "새거에요",
          "original_form": "새거에요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.16",
          "form": "복숭아는 아까먹은듯요",
          "original_form": "복숭아는 아까먹은듯요",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030208.1.17",
          "form": "울집에 황도 있어요",
          "original_form": "울집에 황도 있어요",
