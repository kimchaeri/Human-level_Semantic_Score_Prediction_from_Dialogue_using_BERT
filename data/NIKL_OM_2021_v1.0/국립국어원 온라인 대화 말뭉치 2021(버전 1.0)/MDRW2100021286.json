      "utterance": [
        {
          "id": "MDRW2100021286.1.1",
          "form": "운동 좋아하시나요",
          "original_form": "운동 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021286.1.2",
          "form": "운동 조아하시나요",
          "original_form": "운동 조아하시나요",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021286.1.3",
          "form": "운동 저는 안좋아하지만 계속 해야한단 마음의 숙제죠",
          "original_form": "운동 저는 안좋아하지만 계속 해야한단 마음의 숙제죠",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021286.1.4",
          "form": "건강을위해서ㅠㅠ",
          "original_form": "건강을위해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021286.1.5",
          "form": "그쵸 그래서 요가쪼끔해요",
          "original_form": "그쵸 그래서 요가쪼끔해요",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021286.1.6",
          "form": "오 요가~ 유연하신가봐요",
          "original_form": "오 요가~ 유연하신가봐요",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021286.1.7",
          "form": "전혀요 안유연하니까 몸 좀이라도 늘리려고 하는거에요 안쓰는 근육이ㅣ 많아서",
          "original_form": "전혀요 안유연하니까 몸 좀이라도 늘리려고 하는거에요 안쓰는 근육이ㅣ 많아서",
          "speaker_id": "2",
          "time": "20210829 10:09"
        },
        {
          "id": "MDRW2100021286.1.8",
          "form": "필라테스랑 비슷한가요",
          "original_form": "필라테스랑 비슷한가요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021286.1.9",
          "form": "아뇨 ㅋㅋ 필라테스보다 훨씬 약한 운동입니다.. 스트레칭과 비슷",
          "original_form": "아뇨 ㅋㅋ 필라테스보다 훨씬 약한 운동입니다.. 스트레칭과 비슷",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100021286.1.10",
          "form": "헉 필라테스가 다 어렵군옵",
          "original_form": "헉 필라테스가 다 어렵군옵",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100021286.1.11",
          "form": "나 필라는 무지 힘들어요",
          "original_form": "나 필라는 무지 힘들어요",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100021286.1.12",
          "form": "뭔가 요가가더 힘든것같은데ㅎㅎ",
          "original_form": "뭔가 요가가더 힘든것같은데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100021286.1.13",
          "form": "아니에요ㅎㅎ",
          "original_form": "아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100021286.1.14",
          "form": "기간은 얼마나하셨나요",
          "original_form": "기간은 얼마나하셨나요",
