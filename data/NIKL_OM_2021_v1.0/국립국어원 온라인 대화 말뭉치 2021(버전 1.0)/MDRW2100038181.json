        },
        {
          "id": "MDRW2100038181.1.2",
          "form": "7월에 시작했는데 시청률이 17%",
          "original_form": "7월에 시작했는데 시청률이 17%",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100038181.1.3",
          "form": "높네 시청률이",
          "original_form": "높네 시청률이",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038181.1.4",
          "form": "그러게 일일저녁드라마인데",
          "original_form": "그러게 일일저녁드라마인데",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038181.1.5",
          "form": "이게 소이현 나오지",
          "original_form": "이게 소이현 나오지",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038181.1.6",
          "form": "엉 비정한 엄마와 그 딸의 복수 이야기",
          "original_form": "엉 비정한 엄마와 그 딸의 복수 이야기",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100038181.1.7",
          "form": "소이현이 딸이고 엄마가",
          "original_form": "소이현이 딸이고 엄마가",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100038181.1.8",
          "form": "엄마가 최명길",
          "original_form": "엄마가 최명길",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100038181.1.9",
          "form": "맞다 최명길",
          "original_form": "맞다 최명길",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100038181.1.10",
          "form": "엉 최명길이 가족을 버리고 구두 브랜드를 런칭해서 부자가 됨",
          "original_form": "엉 최명길이 가족을 버리고 구두 브랜드를 런칭해서 부자가 됨",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100038181.1.11",
          "form": "또 부자가 되네",
          "original_form": "또 부자가 되네",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100038181.1.12",
          "form": "불법과 악행을 저지르는데 딸 소이현이 나타나서 파멸로",
          "original_form": "불법과 악행을 저지르는데 딸 소이현이 나타나서 파멸로",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100038181.1.13",
          "form": "저런 과정이 악행이니",
          "original_form": "저런 과정이 악행이니",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100038181.1.14",
          "form": "박윤재도 빨강구두에 나옴",
          "original_form": "박윤재도 빨강구두에 나옴",
