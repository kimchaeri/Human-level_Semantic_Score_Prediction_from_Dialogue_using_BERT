      "utterance": [
        {
          "id": "MDRW2100002627.1.1",
          "form": "강아지 좋아하시나요?",
          "original_form": "강아지 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002627.1.2",
          "form": "네 동물 다좋아해요ㅎ 냥이도 귀엽고",
          "original_form": "네 동물 다좋아해요ㅎ 냥이도 귀엽고",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002627.1.3",
          "form": "강아지 좋아하세요?",
          "original_form": "강아지 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002627.1.4",
          "form": "저 너무좋아합니다ㅎㅎ",
          "original_form": "저 너무좋아합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002627.1.5",
          "form": "키우고싶은데 지금은 사정이안되네요ㅠ",
          "original_form": "키우고싶은데 지금은 사정이안되네요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002627.1.6",
          "form": "키우는게 쉬운일은 아닌거같아요 돈도들고  혼자 넘 오래 두면 외롭고",
          "original_form": "키우는게 쉬운일은 아닌거같아요 돈도들고  혼자 넘 오래 두면 외롭고",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002627.1.7",
          "form": "맞아요ㅠㅠ 참 7시간이상혼자두면",
          "original_form": "맞아요ㅠㅠ 참 7시간이상혼자두면",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002627.1.8",
          "form": "그것도안좋으니 키우기도어렵네요",
          "original_form": "그것도안좋으니 키우기도어렵네요",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002627.1.9",
          "form": "밤새 주인안들어와서 우는애들있더라구요",
          "original_form": "밤새 주인안들어와서 우는애들있더라구요",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002627.1.10",
          "form": "헐ㅠㅠ안타까워요",
          "original_form": "헐ㅠㅠ안타까워요",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002627.1.11",
          "form": "그리고키우면",
          "original_form": "그리고키우면",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002627.1.12",
          "form": "어디 1박여행가기도 힘드니까요ㅠ",
          "original_form": "어디 1박여행가기도 힘드니까요ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002627.1.13",
          "form": "그래서  호텔처럼 맡기는게 생겼나봐요",
          "original_form": "그래서  호텔처럼 맡기는게 생겼나봐요",
          "speaker_id": "2",
          "time": "20210820 14:23"
        },
        {
          "id": "MDRW2100002627.1.14",
          "form": "근데 그거도 불안해하는사람들 많더라고요ㅠㅠ",
          "original_form": "근데 그거도 불안해하는사람들 많더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002627.1.15",
          "form": "참 진짜 애기키우는거같아요ㅠㅠ 강아지키우는거도",
          "original_form": "참 진짜 애기키우는거같아요ㅠㅠ 강아지키우는거도",
          "speaker_id": "1",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002627.1.16",
          "form": "맞아요 밥다챙겨줘야되고 보험도안되니 아프면 돈도 더들어요",
          "original_form": "맞아요 밥다챙겨줘야되고 보험도안되니 아프면 돈도 더들어요",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002627.1.17",
          "form": "그래도 애교부리고 집에오면 반겨주니 기쁨조예요",
          "original_form": "그래도 애교부리고 집에오면 반겨주니 기쁨조예요",
          "speaker_id": "2",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002627.1.18",
          "form": "맞아요ㅠㅠ 저는 꼭",
          "original_form": "맞아요ㅠㅠ 저는 꼭",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002627.1.19",
          "form": "나중에 키우려구요",
          "original_form": "나중에 키우려구요",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002627.1.20",
          "form": "포메 너무귀엽더라고요",
          "original_form": "포메 너무귀엽더라고요",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002627.1.21",
          "form": "여건되면 저도 키워보고싶어요ㅎ",
          "original_form": "여건되면 저도 키워보고싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002627.1.22",
          "form": "어떤 종 생각하시나요??",
          "original_form": "어떤 종 생각하시나요??",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002627.1.23",
          "form": "저도 포메리안 아님 그 뽀끌뽀글한 강아지 이름이뭐죠?",
          "original_form": "저도 포메리안 아님 그 뽀끌뽀글한 강아지 이름이뭐죠?",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002627.1.24",
          "form": "비숑ㅎ",
          "original_form": "비숑ㅎ",
          "speaker_id": "2",
          "time": "20210820 14:41"
        }
      ]
    }
