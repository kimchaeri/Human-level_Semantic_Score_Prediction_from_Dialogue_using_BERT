      "utterance": [
        {
          "id": "MDRW2100033894.1.1",
          "form": "보쌈vs족발은 어떠세요?",
          "original_form": "보쌈vs족발은 어떠세요?",
          "speaker_id": "2",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033894.1.2",
          "form": "전 요샌 족발이요! 원랜 보쌈파지만..!!name2님은요?",
          "original_form": "전 요샌 족발이요! 원랜 보쌈파지만..!!&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033894.1.3",
          "form": "보쌈 궁예해봅니다",
          "original_form": "보쌈 궁예해봅니다",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033894.1.4",
          "form": "어우 대박",
          "original_form": "어우 대박",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100033894.1.5",
          "form": "어떻게아셨죠?",
          "original_form": "어떻게아셨죠?",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100033894.1.6",
          "form": "와ㅋㅋㅋ신기",
          "original_form": "와ㅋㅋㅋ신기",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100033894.1.7",
          "form": "name2님 집에서도 요리 자주하세요?",
          "original_form": "&name2&님 집에서도 요리 자주하세요?",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100033894.1.8",
          "form": "ㅋㅋㅋ 가끔한답니다",
          "original_form": "ㅋㅋㅋ 가끔한답니다",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100033894.1.9",
          "form": "레시피보고?",
          "original_form": "레시피보고?",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100033894.1.10",
          "form": "오 어떤 음식이 가장 자신있으세요?",
          "original_form": "오 어떤 음식이 가장 자신있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033894.1.11",
          "form": "닭볶음탕?!",
          "original_form": "닭볶음탕?!",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033894.1.12",
          "form": "님은요???!",
          "original_form": "님은요???!",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033894.1.13",
          "form": "오! 저는 한식종류?",
          "original_form": "오! 저는 한식종류?",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100033894.1.14",
          "form": "김치볶음밥 잘하고 싶은데 뭔가 쉬우면서도 어렵네요...",
          "original_form": "김치볶음밥 잘하고 싶은데 뭔가 쉬우면서도 어렵네요...",
          "speaker_id": "1",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100033894.1.15",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100033894.1.16",
          "form": "김볶밥이 은근",
          "original_form": "김볶밥이 은근",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100033894.1.17",
          "form": "성공하기힘들어요",
          "original_form": "성공하기힘들어요",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100033894.1.18",
          "form": "맞아요 클래식한 메뉸데 이상하게 제가하면 원하는 맛이 안나오더라고요?",
          "original_form": "맞아요 클래식한 메뉸데 이상하게 제가하면 원하는 맛이 안나오더라고요?",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100033894.1.19",
          "form": "그거랑 은근 계란말이..",
          "original_form": "그거랑 은근 계란말이..",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100033894.1.20",
          "form": "ㅋㅋㅋ전",
          "original_form": "ㅋㅋㅋ전",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100033894.1.21",
          "form": "계란말이는",
          "original_form": "계란말이는",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100033894.1.22",
          "form": "자신있어요",
          "original_form": "자신있어요",
          "speaker_id": "2",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100033894.1.23",
          "form": "오~~~ 똥똥하게 잘 마시나봐요!!!",
          "original_form": "오~~~ 똥똥하게 잘 마시나봐요!!!",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100033894.1.24",
          "form": "ㅋㅋㅋㅋ 사각팬만있으면",
          "original_form": "ㅋㅋㅋㅋ 사각팬만있으면",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100033894.1.25",
          "form": "어디든갈수있어요!",
          "original_form": "어디든갈수있어요!",
          "speaker_id": "2",
          "time": "20210826 17:02"
        }
      ]
