      "utterance": [
        {
          "id": "MDRW2100012021.1.1",
          "form": "아ㅜ 전 완전 세꼬시파라",
          "original_form": "아ㅜ 전 완전 세꼬시파라",
          "speaker_id": "2",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100012021.1.2",
          "form": "아나고 도다리 전어 세꼬시류는 다 좋아요",
          "original_form": "아나고 도다리 전어 세꼬시류는 다 좋아요",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012021.1.3",
          "form": "광어도 세꼬시 팔면 세꼬시로ㅎ",
          "original_form": "광어도 세꼬시 팔면 세꼬시로ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012021.1.4",
          "form": "오~ 저는 잘 안씹혀서 먹기가 힘들더라구요",
          "original_form": "오~ 저는 잘 안씹혀서 먹기가 힘들더라구요",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012021.1.5",
          "form": "연어는 보들보들 그냥 넘어가는데 비해서ㅠㅎㅎ",
          "original_form": "연어는 보들보들 그냥 넘어가는데 비해서ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012021.1.6",
          "form": "안그래도 아버지께 전어 권했더니 씹기 힘들다고 거부하셨어요ㅜ",
          "original_form": "안그래도 아버지께 전어 권했더니 씹기 힘들다고 거부하셨어요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012021.1.7",
          "form": "근데 먹잘알은 세꼬시 쪽일거 같아요 ㅎㅎ",
          "original_form": "근데 먹잘알은 세꼬시 쪽일거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012021.1.8",
          "form": "전어도 좀 질긴가봐요!",
          "original_form": "전어도 좀 질긴가봐요!",
          "speaker_id": "1",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012021.1.9",
          "form": "아버지가 치아가 안 좋으세요ㅜ",
          "original_form": "아버지가 치아가 안 좋으세요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012021.1.10",
          "form": "그래도 전어는 가끔 먹으니 맛있는거고 스테디는 연어광어죠",
          "original_form": "그래도 전어는 가끔 먹으니 맛있는거고 스테디는 연어광어죠",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012021.1.11",
          "form": "광어가 예전엔 돔처럼 비싼 생선이었대요",
          "original_form": "광어가 예전엔 돔처럼 비싼 생선이었대요",
          "speaker_id": "2",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012021.1.12",
          "form": "양식 성공 전에는",
          "original_form": "양식 성공 전에는",
          "speaker_id": "2",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012021.1.13",
          "form": "그렇다고 하더라구요!!!",
          "original_form": "그렇다고 하더라구요!!!",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012021.1.14",
          "form": "흔해져서 다행인 ㅋㅋㅋㅋ",
          "original_form": "흔해져서 다행인 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012021.1.15",
          "form": "연어 알기전엔 어릴때 늘 먹던게 광어였는데",
          "original_form": "연어 알기전엔 어릴때 늘 먹던게 광어였는데",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012021.1.16",
          "form": "그래서 가격 대비 제일 좋은 생선이 광어라고ㅎ",
          "original_form": "그래서 가격 대비 제일 좋은 생선이 광어라고ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012021.1.17",
          "form": "연어의 세계 빠지면 못 헤어나죠ㅎ",
          "original_form": "연어의 세계 빠지면 못 헤어나죠ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012021.1.18",
          "form": "우리 가족은 연어도 비리고 물컹하다고 별로 안 좋아해요",
          "original_form": "우리 가족은 연어도 비리고 물컹하다고 별로 안 좋아해요",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012021.1.19",
          "form": "저만 식성이 이래서ㅜㅜ 가끔 아쉽",
          "original_form": "저만 식성이 이래서ㅜㅜ 가끔 아쉽",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012021.1.20",
          "form": "연어덮밥 너무 좋아요!!",
          "original_form": "연어덮밥 너무 좋아요!!",
          "speaker_id": "2",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100012021.1.21",
          "form": "아 정말여?? 저는 연어는 호불호없는 음식인줄 알았어요!!",
          "original_form": "아 정말여?? 저는 연어는 호불호없는 음식인줄 알았어요!!",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100012021.1.22",
          "form": "와하우 ㅠㅠㅠㅠㅠ 연어랑 아보카도랑 ㅠㅠ",
          "original_form": "와하우 ㅠㅠㅠㅠㅠ 연어랑 아보카도랑 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100012021.1.23",
          "form": "아 그 조합은 정말 못 참죠",
          "original_form": "아 그 조합은 정말 못 참죠",
          "speaker_id": "2",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100012021.1.24",
          "form": "제 주변에 연어에 환장하는 친구가 있었는데 아무리 맛있어도 그정돈가? 이해를 못했거든요",
          "original_form": "제 주변에 연어에 환장하는 친구가 있었는데 아무리 맛있어도 그정돈가? 이해를 못했거든요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012021.1.25",
          "form": "말하고나니 연어도 당기네요",
          "original_form": "말하고나니 연어도 당기네요",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012021.1.26",
          "form": "그쵸.",
          "original_form": "그쵸.",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012021.1.27",
          "form": "제말이...",
          "original_form": "제말이...",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012021.1.28",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012021.1.29",
          "form": "ㅎ먹으면서 먹는 얘기 하는 음식의 굴레",
          "original_form": "ㅎ먹으면서 먹는 얘기 하는 음식의 굴레",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012021.1.30",
          "form": "어떻게 연어에 입덕하셨나요",
          "original_form": "어떻게 연어에 입덕하셨나요",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012021.1.31",
          "form": "근데 ... 어쩌다보니..? ㅋㅋㅋㅋ",
          "original_form": "근데 ... 어쩌다보니..? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012021.1.32",
          "form": "맛있긴해도 제돈주고 막 혼자서 사먹고 이러진않았었는데",
          "original_form": "맛있긴해도 제돈주고 막 혼자서 사먹고 이러진않았었는데",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012021.1.33",
          "form": "기운떨어질때 치킨시켜먹을거 연어사먹고 그랬었거든요",
          "original_form": "기운떨어질때 치킨시켜먹을거 연어사먹고 그랬었거든요",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012021.1.34",
          "form": "그렇게 중독이 되어버린건지 ㅋㅋㅋㅋ",
          "original_form": "그렇게 중독이 되어버린건지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012021.1.35",
          "form": "아니ㅎㅎ 충분히 연어마니아인데요",
          "original_form": "아니ㅎㅎ 충분히 연어마니아인데요",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012021.1.36",
          "form": "치킨 대신 연어할 정도면ㅎ",
          "original_form": "치킨 대신 연어할 정도면ㅎ",
          "speaker_id": "2",
