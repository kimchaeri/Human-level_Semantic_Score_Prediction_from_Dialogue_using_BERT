      "utterance": [
        {
          "id": "MDRW2100025358.1.1",
          "form": "건강을 위해 하고계신거 있나요??",
          "original_form": "건강을 위해 하고계신거 있나요??",
          "speaker_id": "1",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025358.1.2",
          "form": "아무것도안해요...ㅠㅠㅠㅠ",
          "original_form": "아무것도안해요...ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025358.1.3",
          "form": "일마치고 집가면 피곤해서 자기바쁘네요",
          "original_form": "일마치고 집가면 피곤해서 자기바쁘네요",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025358.1.4",
          "form": "그나마 하는게 일찍자기??",
          "original_form": "그나마 하는게 일찍자기??",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025358.1.5",
          "form": "건강 챙기시나요?",
          "original_form": "건강 챙기시나요?",
          "speaker_id": "2",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025358.1.6",
          "form": "일찍 자는거 너무 중요한 것 같아요",
          "original_form": "일찍 자는거 너무 중요한 것 같아요",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025358.1.7",
          "form": "저는 맨날 폰하다가 늦게 자서",
          "original_form": "저는 맨날 폰하다가 늦게 자서",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025358.1.8",
          "form": "저는 영양제 챙겨먹고 있어요!",
          "original_form": "저는 영양제 챙겨먹고 있어요!",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025358.1.9",
          "form": "저도 억지로 자는거라서 ㅠㅠㅠ",
          "original_form": "저도 억지로 자는거라서 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025358.1.10",
          "form": "저도 유산균은 먹네요 ㅋㅋㅋ",
          "original_form": "저도 유산균은 먹네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025358.1.11",
          "form": "확실히 안먹는것보다는 나은것 같아요ㅎㅎ",
          "original_form": "확실히 안먹는것보다는 나은것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025358.1.12",
          "form": "저는 사실 잘 모르겠어요..",
          "original_form": "저는 사실 잘 모르겠어요..",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025358.1.13",
          "form": "엄마가 먹으라 해서 억지로 먹는거라...",
          "original_form": "엄마가 먹으라 해서 억지로 먹는거라...",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025358.1.14",
          "form": "너무 부러워요ㅜㅜㅜ",
          "original_form": "너무 부러워요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025358.1.15",
          "form": "장이 건강하셔서 그런가봐요!ㅋㅋㅋ",
          "original_form": "장이 건강하셔서 그런가봐요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025358.1.16",
          "form": "저는 장이 안좋아서ㅋㅋㅋㅋ",
          "original_form": "저는 장이 안좋아서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025358.1.17",
          "form": "아진짜요?? 저는 소화기관이 약하긴해요",
          "original_form": "아진짜요?? 저는 소화기관이 약하긴해요",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025358.1.18",
          "form": "잘체하더라구요 ㅠㅠ",
          "original_form": "잘체하더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025358.1.19",
          "form": "저두요!!!!",
          "original_form": "저두요!!!!",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025358.1.20",
          "form": "나이 한살한살 먹으면서 몸 안좋아지는걸 느껴요ㅠㅜ",
          "original_form": "나이 한살한살 먹으면서 몸 안좋아지는걸 느껴요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025358.1.21",
          "form": "저도커가면서 나쁜습관들때문에 그런거같아요",
          "original_form": "저도커가면서 나쁜습관들때문에 그런거같아요",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025358.1.22",
          "form": "왜 건강에 안좋은건 너무 편하고 좋을까요....??ㅠㅠ",
          "original_form": "왜 건강에 안좋은건 너무 편하고 좋을까요....??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025358.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 누워있기라던지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 누워있기라던지",
          "speaker_id": "2",
          "time": "20210901 10:29"
        },
        {
          "id": "MDRW2100025358.1.24",
          "form": "운동하는건 너무 싫고 누워서 있는게 세상 좋아요",
          "original_form": "운동하는건 너무 싫고 누워서 있는게 세상 좋아요",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100025358.1.25",
          "form": "맞아요 ㅠㅠ진짜운동 너무 힘들어요 ㅠ",
