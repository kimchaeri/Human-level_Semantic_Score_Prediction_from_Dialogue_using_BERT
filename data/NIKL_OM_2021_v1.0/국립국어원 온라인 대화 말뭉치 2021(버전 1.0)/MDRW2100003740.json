      "utterance": [
        {
          "id": "MDRW2100003740.1.1",
          "form": "온라인 전 카페 이용해요",
          "original_form": "온라인 전 카페 이용해요",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003740.1.2",
          "form": "아 어떤거요?",
          "original_form": "아 어떤거요?",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003740.1.3",
          "form": "전 장난감 카페",
          "original_form": "전 장난감 카페",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003740.1.4",
          "form": "지역카페요",
          "original_form": "지역카페요",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003740.1.5",
          "form": "광석카페도요 ㅋㅋ",
          "original_form": "광석카페도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003740.1.6",
          "form": "중고나라 기본이구요",
          "original_form": "중고나라 기본이구요",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.7",
          "form": "뭐이용하세요?",
          "original_form": "뭐이용하세요?",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.8",
          "form": "아~그런건 어떤카페인가요?",
          "original_form": "아~그런건 어떤카페인가요?",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.9",
          "form": "중고나라는 알아요!",
          "original_form": "중고나라는 알아요!",
          "speaker_id": "2",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.10",
          "form": "네이버 카페예요",
          "original_form": "네이버 카페예요",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.11",
          "form": "장난감은 미니어쳐 모으는 데고요 가챠나요",
          "original_form": "장난감은 미니어쳐 모으는 데고요 가챠나요",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.12",
          "form": "광석은 호박 거래 하려고 했어요",
          "original_form": "광석은 호박 거래 하려고 했어요",
          "speaker_id": "1",
          "time": "20210821 11:36"
        },
        {
          "id": "MDRW2100003740.1.13",
          "form": "지역카페는 정보 교환과 핫딜 정보받고 지역맛집 정보 얻어요",
          "original_form": "지역카페는 정보 교환과 핫딜 정보받고 지역맛집 정보 얻어요",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003740.1.14",
          "form": "요샌 코로나라 정모 못하고 교류 못해서 넘 속상해요?",
          "original_form": "요샌 코로나라 정모 못하고 교류 못해서 넘 속상해요?",
          "speaker_id": "1",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003740.1.15",
          "form": "아~~여러카페가 많네요",
          "original_form": "아~~여러카페가 많네요",
          "speaker_id": "2",
          "time": "20210821 11:37"
        },
        {
          "id": "MDRW2100003740.1.16",
          "form": "오 정모도 나가세요?",
          "original_form": "오 정모도 나가세요?",
          "speaker_id": "2",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003740.1.17",
          "form": "무슨 활동하세?",
          "original_form": "무슨 활동하세?",
          "speaker_id": "1",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003740.1.18",
          "form": "저예전에 장난감카페 정모 짱 좋아했어요",
          "original_form": "저예전에 장난감카페 정모 짱 좋아했어요",
          "speaker_id": "1",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003740.1.19",
          "form": "오~~모이면 어떤거 하셨어요?",
          "original_form": "오~~모이면 어떤거 하셨어요?",
          "speaker_id": "2",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003740.1.20",
          "form": "서로 장난감보고 앙 좋았는데 그때친구가 젤 핀한 친구가 됐어요",
          "original_form": "서로 장난감보고 앙 좋았는데 그때친구가 젤 핀한 친구가 됐어요",
          "speaker_id": "1",
          "time": "20210821 11:38"
        },
        {
          "id": "MDRW2100003740.1.21",
          "form": "장난감 자랑 ㅋㅋ사진 찍죠",
          "original_form": "장난감 자랑 ㅋㅋ사진 찍죠",
          "speaker_id": "1",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003740.1.22",
          "form": "아~~~재밌겠어요!!",
          "original_form": "아~~~재밌겠어요!!",
          "speaker_id": "2",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003740.1.23",
          "form": "맛난거먹기 경매",
          "original_form": "맛난거먹기 경매",
          "speaker_id": "1",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003740.1.24",
          "form": "어떤 피규어 모으세요?",
          "original_form": "어떤 피규어 모으세요?",
          "speaker_id": "2",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003740.1.25",
          "form": "가챠 미니어쳐",
          "original_form": "가챠 미니어쳐",
          "speaker_id": "1",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003740.1.26",
          "form": "올드토이요",
          "original_form": "올드토이요",
          "speaker_id": "1",
          "time": "20210821 11:39"
        },
        {
          "id": "MDRW2100003740.1.27",
          "form": "요번에 싹정리했는데 다시 달리려고요 ㅋㅋ",
          "original_form": "요번에 싹정리했는데 다시 달리려고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003740.1.28",
          "form": "뭐 모임 없으세요?",
          "original_form": "뭐 모임 없으세요?",
          "speaker_id": "1",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003740.1.29",
          "form": "아~~저는 없어요ㅠㅠ",
          "original_form": "아~~저는 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003740.1.30",
          "form": "좋아하는거나 수집은요?",
          "original_form": "좋아하는거나 수집은요?",
          "speaker_id": "1",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003740.1.31",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003740.1.32",
          "form": "지역카페는 안하세요?",
          "original_form": "지역카페는 안하세요?",
          "speaker_id": "1",
          "time": "20210821 11:40"
        },
        {
          "id": "MDRW2100003740.1.33",
          "form": "전 인스타 페북은 정말 모르겠어요 ㅋㅋ암호 같아요",
          "original_form": "전 인스타 페북은 정말 모르겠어요 ㅋㅋ암호 같아요",
          "speaker_id": "1",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100003740.1.34",
          "form": "전 옛날사람 ㅋㅋ",
          "original_form": "전 옛날사람 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100003740.1.35",
          "form": "네~지역카페도요~ 취미가 없어요 ㅎ",
          "original_form": "네~지역카페도요~ 취미가 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100003740.1.36",
          "form": "모임이란게 진짜 화력소인데 코로나가 사람을 잡네요",
          "original_form": "모임이란게 진짜 화력소인데 코로나가 사람을 잡네요",
          "speaker_id": "1",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100003740.1.37",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100003740.1.38",
          "form": "아 수다떨기가 취미예요 ㅋㅋ",
          "original_form": "아 수다떨기가 취미예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100003740.1.39",
          "form": "통화 무제한",
          "original_form": "통화 무제한",
          "speaker_id": "1",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100003740.1.40",
          "form": "전 카페활동 좋아해요",
          "original_form": "전 카페활동 좋아해요",
          "speaker_id": "1",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100003740.1.41",
          "form": "ㅋㅋ 정모때 분들이랑요?",
          "original_form": "ㅋㅋ 정모때 분들이랑요?",
          "speaker_id": "2",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100003740.1.42",
          "form": "주식카페도 가입해서 활동해요 ㅋㅋ",
          "original_form": "주식카페도 가입해서 활동해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100003740.1.43",
          "form": "그때만난 친구랑요 ㅋㅋ",
          "original_form": "그때만난 친구랑요 ㅋㅋ",
          "speaker_id": "1",
