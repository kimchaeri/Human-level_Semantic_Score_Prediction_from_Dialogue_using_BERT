      "utterance": [
        {
          "id": "MDRW2100045837.1.1",
          "form": "통조림 안좋은 이유가 멀까요",
          "original_form": "통조림 안좋은 이유가 멀까요",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100045837.1.2",
          "form": "살쪄서 그럴려나요",
          "original_form": "살쪄서 그럴려나요",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045837.1.3",
          "form": "아이스크림도 구현한 ㄷㄷ",
          "original_form": "아이스크림도 구현한 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045837.1.4",
          "form": "냉장고가 돌아가나봐용",
          "original_form": "냉장고가 돌아가나봐용",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100045837.1.5",
          "form": "아직 발전기가 돌아가는가요",
          "original_form": "아직 발전기가 돌아가는가요",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045837.1.6",
          "form": "그런게 아닐까요",
          "original_form": "그런게 아닐까요",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045837.1.7",
          "form": "충전은 안되겠죠",
          "original_form": "충전은 안되겠죠",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045837.1.8",
          "form": "방법이 없진 않을것 같기도 하고",
          "original_form": "방법이 없진 않을것 같기도 하고",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100045837.1.9",
          "form": "이제 렙올리러 가나봐요",
          "original_form": "이제 렙올리러 가나봐요",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045837.1.10",
          "form": "발전이 없긴 하다는",
          "original_form": "발전이 없긴 하다는",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045837.1.11",
          "form": "다이하나요 ㅜㅜㅋㅋ",
          "original_form": "다이하나요 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045837.1.12",
          "form": "살수있울지 걱정",
          "original_form": "살수있울지 걱정",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100045837.1.13",
          "form": "창고도 한두개가 아니네요 ㅋㅋㅋ",
