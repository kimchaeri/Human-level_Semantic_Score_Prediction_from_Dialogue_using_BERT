      "utterance": [
        {
          "id": "MDRW2100040604.1.1",
          "form": "다행히도 제가 술먹을때 안주 잘 안먹어서",
          "original_form": "다행히도 제가 술먹을때 안주 잘 안먹어서",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.3",
          "form": "안시키게 되더라구요",
          "original_form": "안시키게 되더라구요",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.4",
          "form": "저는 하도 맛있다는 말만 들어서",
          "original_form": "저는 하도 맛있다는 말만 들어서",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.5",
          "form": "그냥 프레첼이면 됨 ㅋㅋㅋ",
          "original_form": "그냥 프레첼이면 됨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.6",
          "form": "궁금했는데",
          "original_form": "궁금했는데",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.7",
          "form": "양이 적다는 말만 들었어요",
          "original_form": "양이 적다는 말만 들었어요",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.9",
          "form": "name2님 진짜 위장튼튼이신가봐요",
          "original_form": "&name2&님 진짜 위장튼튼이신가봐요",
          "speaker_id": "1",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100040604.1.10",
          "form": "ㅋㅋㅋㅋ아저는 배부른게 싱ㅎ어요",
          "original_form": "ㅋㅋㅋㅋ아저는 배부른게 싱ㅎ어요",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.11",
          "form": "안주많이먹고 술도 많이먹으면",
          "original_form": "안주많이먹고 술도 많이먹으면",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.12",
          "form": "엄청 토하기땜에..",
          "original_form": "엄청 토하기땜에..",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.14",
          "form": "헉 그런가요?!",
          "original_form": "헉 그런가요?!",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.15",
          "form": "저는 위장을 보호한다는 개념으로",
          "original_form": "저는 위장을 보호한다는 개념으로",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.16",
          "form": "안주를 많이 먹었는데!!!",
          "original_form": "안주를 많이 먹었는데!!!",
          "speaker_id": "1",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.17",
          "form": "술을 많이 안드시니까 괜찮으신거같아요",
          "original_form": "술을 많이 안드시니까 괜찮으신거같아요",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.18",
          "form": "저는. .술을...ㅎ...",
          "original_form": "저는. .술을...ㅎ...",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.19",
          "form": "에..",
          "original_form": "에..",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:09"
        },
        {
          "id": "MDRW2100040604.1.21",
          "form": "저는 위스키를 제일 좋아해요",
          "original_form": "저는 위스키를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040604.1.22",
          "form": "맥주랑",
          "original_form": "맥주랑",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040604.1.23",
          "form": "name2님 진짜 멋지다",
          "original_form": "&name2&님 진짜 멋지다",
          "speaker_id": "1",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040604.1.24",
          "form": "춤도 잘추고",
          "original_form": "춤도 잘추고",
          "speaker_id": "1",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040604.1.25",
          "form": "술도 잘마시고",
          "original_form": "술도 잘마시고",
          "speaker_id": "1",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040604.1.26",
          "form": "에? 아닙니닼ㅋㅋㅋ그저 술쟁이일 뿐인걸요",
          "original_form": "에? 아닙니닼ㅋㅋㅋ그저 술쟁이일 뿐인걸요",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.27",
          "form": "저는 제가 좀 정말 술을 안좋아했으면 해요 ㅠ",
          "original_form": "저는 제가 좀 정말 술을 안좋아했으면 해요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.28",
          "form": "너무 먹어서 ㅠ",
          "original_form": "너무 먹어서 ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.29",
          "form": "건강검진에서",
          "original_form": "건강검진에서",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.30",
          "form": "술을 줄이라는 소견을 들으셨나요?!",
          "original_form": "술을 줄이라는 소견을 들으셨나요?!",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.31",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.32",
          "form": "당연라죠",
          "original_form": "당연라죠",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.33",
          "form": "헉 그렇다면",
          "original_form": "헉 그렇다면",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.34",
          "form": "진짜 줄이셔야겟네요ㅠ",
          "original_form": "진짜 줄이셔야겟네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ근데 그 기준",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ근데 그 기준",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040604.1.36",
