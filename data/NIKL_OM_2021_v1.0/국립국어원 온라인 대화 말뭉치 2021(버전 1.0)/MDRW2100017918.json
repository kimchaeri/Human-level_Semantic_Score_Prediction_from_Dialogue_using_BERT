      "utterance": [
        {
          "id": "MDRW2100017918.1.1",
          "form": "공연 관람 좋아하세요?",
          "original_form": "공연 관람 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017918.1.2",
          "form": "공연 관람 해보고는 싶은데 기회가 없었어요",
          "original_form": "공연 관람 해보고는 싶은데 기회가 없었어요",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017918.1.3",
          "form": "뮤지컬만 몇 번 봤던거같아요 ㅋㅋㅋㅋ",
          "original_form": "뮤지컬만 몇 번 봤던거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017918.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017918.1.5",
          "form": "저는 뮤지컬 한 번 보러가보고싶어요",
          "original_form": "저는 뮤지컬 한 번 보러가보고싶어요",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017918.1.6",
          "form": "전시회만 몇 번 가봤어요ㅠ",
          "original_form": "전시회만 몇 번 가봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017918.1.7",
          "form": "뮤지컬은 직접 가서 보면 확실히 배우의 아우라가 느껴지는거같아요",
          "original_form": "뮤지컬은 직접 가서 보면 확실히 배우의 아우라가 느껴지는거같아요",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017918.1.8",
          "form": "저도 자주 보고싶은데 티켓 너무 비싸요 ㅠㅠ",
          "original_form": "저도 자주 보고싶은데 티켓 너무 비싸요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017918.1.9",
          "form": "한번 느껴보고 싶어요\r\n맞아요ㅠㅠ 쉽게 접하기엔 비싸요ㅜ",
          "original_form": "한번 느껴보고 싶어요\r\n맞아요ㅠㅠ 쉽게 접하기엔 비싸요ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017918.1.10",
          "form": "전시회는 보통 어떤 주제로 가세요?",
          "original_form": "전시회는 보통 어떤 주제로 가세요?",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017918.1.11",
          "form": "사진전이요!",
          "original_form": "사진전이요!",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017918.1.12",
          "form": "전시회 보러 가시나요?",
          "original_form": "전시회 보러 가시나요?",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017918.1.13",
          "form": "저는 회화 전시회 몇번 갔었어요!",
          "original_form": "저는 회화 전시회 몇번 갔었어요!",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017918.1.14",
          "form": "알폰스 무하전이 기억나요",
          "original_form": "알폰스 무하전이 기억나요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017918.1.15",
          "form": "기억에 남는 사진전 있으세요?",
          "original_form": "기억에 남는 사진전 있으세요?",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017918.1.16",
          "form": "퓰리처상 사진전이요",
          "original_form": "퓰리처상 사진전이요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017918.1.17",
          "form": "신비롭더라구요",
          "original_form": "신비롭더라구요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017918.1.18",
          "form": "전시회에서 도슨트나 해설 같이 듣는 편이신가요?",
          "original_form": "전시회에서 도슨트나 해설 같이 듣는 편이신가요?",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017918.1.19",
          "form": "혼자 보는게 좋았는데 들어보니 듣는것도 매력적이더라구요",
          "original_form": "혼자 보는게 좋았는데 들어보니 듣는것도 매력적이더라구요",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017918.1.20",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017918.1.21",
          "form": "저는 꼭 챙겨듣는 편이에요! 비하인드나 작품 배경 설명듣는거 좋아해서요 ㅎㅎ",
          "original_form": "저는 꼭 챙겨듣는 편이에요! 비하인드나 작품 배경 설명듣는거 좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017918.1.22",
          "form": "아 그거 진짜 좋죠ㅎㅎ 쉽게 알 수 없는거라 더 좋았어요",
          "original_form": "아 그거 진짜 좋죠ㅎㅎ 쉽게 알 수 없는거라 더 좋았어요",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017918.1.23",
          "form": "그쵸 그리고 혼자 감상하면 작품의 분위기나 느낌위주로 보는거같고",
          "original_form": "그쵸 그리고 혼자 감상하면 작품의 분위기나 느낌위주로 보는거같고",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017918.1.24",
          "form": "해설이랑 같이 보면 구성요소나 구조 중심으로 보게돼서 각자 다른 재미가 있더라구요",
          "original_form": "해설이랑 같이 보면 구성요소나 구조 중심으로 보게돼서 각자 다른 재미가 있더라구요",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
