      "utterance": [
        {
          "id": "MDRW2100038708.1.1",
          "form": "그럼 식단말고 운동도 하시는건가요?",
          "original_form": "그럼 식단말고 운동도 하시는건가요?",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100038708.1.2",
          "form": "제가 석달동안 운동하면서 찌웠어요 대박이죠? ㅋ기",
          "original_form": "제가 석달동안 운동하면서 찌웠어요 대박이죠? ㅋ기",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038708.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038708.1.4",
          "form": "어떤운동 하셨는데요?!",
          "original_form": "어떤운동 하셨는데요?!",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100038708.1.5",
          "form": "스트레스 받으면 그럴것같아요 ㅠㅠ",
          "original_form": "스트레스 받으면 그럴것같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038708.1.6",
          "form": "하루 이만보씩 걸었어요 ㅋㅋ",
          "original_form": "하루 이만보씩 걸었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038708.1.7",
          "form": "근데 결과가 허허",
          "original_form": "근데 결과가 허허",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100038708.1.8",
          "form": "헉 저도 한시간 걷기햇는데 제자리더라규요",
          "original_form": "헉 저도 한시간 걷기햇는데 제자리더라규요",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100038708.1.9",
          "form": "걷기가 별 도움이 안되는 느낌",
          "original_form": "걷기가 별 도움이 안되는 느낌",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100038708.1.10",
          "form": "전 그걸 깨달았어요 걷기는 건강을위한거지",
          "original_form": "전 그걸 깨달았어요 걷기는 건강을위한거지",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038708.1.11",
          "form": "살을빼는 용도가 아니란거 ㅋㅋ",
          "original_form": "살을빼는 용도가 아니란거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038708.1.12",
          "form": "그래서 저는 실내자전거 사고싶더라구요",
          "original_form": "그래서 저는 실내자전거 사고싶더라구요",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100038708.1.13",
          "form": "앗 저희집에 있어요 실내자전거 ㅋㅋ숀리의 바이크 ㅋㅋㅋ",
          "original_form": "앗 저희집에 있어요 실내자전거 ㅋㅋ숀리의 바이크 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100038708.1.14",
          "form": "헉 그거 엄청 빨리 달리면 살빼는데 도움된데요",
          "original_form": "헉 그거 엄청 빨리 달리면 살빼는데 도움된데요",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100038708.1.15",
          "form": "그거 인터벌 이랄까 빠르게 보통 빠르게 보통하면더좋대요",
          "original_form": "그거 인터벌 이랄까 빠르게 보통 빠르게 보통하면더좋대요",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100038708.1.16",
          "form": "근데 ㅋㅋ잘못하면 무릎나가요",
          "original_form": "근데 ㅋㅋ잘못하면 무릎나가요",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100038708.1.17",
          "form": "오오 그거맞아요",
          "original_form": "오오 그거맞아요",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100038708.1.18",
          "form": "심박수를 올리라더라구요",
          "original_form": "심박수를 올리라더라구요",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100038708.1.19",
          "form": "제가 무릎나간다고 그거조심해야한다니까",
          "original_form": "제가 무릎나간다고 그거조심해야한다니까",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038708.1.20",
          "form": "엄마가 살은 오지게찌워놓고 몸사린다고 하셨던 명언이 생각나네요",
          "original_form": "엄마가 살은 오지게찌워놓고 몸사린다고 하셨던 명언이 생각나네요",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038708.1.21",
          "form": "ㅜㅜ 가족이라도 그런말 하면 상처인데",
          "original_form": "ㅜㅜ 가족이라도 그런말 하면 상처인데",
