      "utterance": [
        {
          "id": "MDRW2100016749.1.1",
          "form": "여행가고싶어~~",
          "original_form": "여행가고싶어~~",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016749.1.2",
          "form": "맞아 여행이 너무 필요해ㅜ",
          "original_form": "맞아 여행이 너무 필요해ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016749.1.3",
          "form": "가까운데라도 가고 싶어ㅠ",
          "original_form": "가까운데라도 가고 싶어ㅠ",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016749.1.4",
          "form": "맞아ㅠㅠㅠㅠ 진짜 코로나 때문에 안간지 너무 오래됐어",
          "original_form": "맞아ㅠㅠㅠㅠ 진짜 코로나 때문에 안간지 너무 오래됐어",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016749.1.5",
          "form": "바다도 가고싶다ㅠㅠ",
          "original_form": "바다도 가고싶다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016749.1.6",
          "form": "맞아맞아 그냥 바닷바람 쐬면서 모래사장 걷고 싶고..",
          "original_form": "맞아맞아 그냥 바닷바람 쐬면서 모래사장 걷고 싶고..",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016749.1.7",
          "form": "다녀본 여행지중에서 어디가 제일 그리워?",
          "original_form": "다녀본 여행지중에서 어디가 제일 그리워?",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016749.1.8",
          "form": "맞아 여유롭게 걷고싶다",
          "original_form": "맞아 여유롭게 걷고싶다",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016749.1.9",
          "form": "미국 마이에미",
          "original_form": "미국 마이에미",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016749.1.10",
          "form": "여유로웠던거 생각하면 젤좋았어.... 진심..미국인들이 노후보내는 곳인 이유가 있어..",
          "original_form": "여유로웠던거 생각하면 젤좋았어.... 진심..미국인들이 노후보내는 곳인 이유가 있어..",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016749.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016749.1.12",
          "form": "아아 그곳이 보통 미국인들이 여유를 즐기는 곳인가 보구나",
          "original_form": "아아 그곳이 보통 미국인들이 여유를 즐기는 곳인가 보구나",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016749.1.13",
          "form": "나도 그런 여유를 느낄 수 있는 곳이 좋더라",
          "original_form": "나도 그런 여유를 느낄 수 있는 곳이 좋더라",
          "speaker_id": "1",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016749.1.14",
          "form": "지금 제일 가보고싶은 곳은 어디야 그럼?",
          "original_form": "지금 제일 가보고싶은 곳은 어디야 그럼?",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016749.1.15",
          "form": "내가 좋았던 여행지는 대만이 좋았던 거 같아ㅋㅋ",
          "original_form": "내가 좋았던 여행지는 대만이 좋았던 거 같아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016749.1.16",
          "form": "대만 못가봐서너무 궁금해",
          "original_form": "대만 못가봐서너무 궁금해",
          "speaker_id": "2",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016749.1.17",
          "form": "나는 가보고 싶은 곳은 인도네시아 휴양지?? 해변가ㅋㅋ",
          "original_form": "나는 가보고 싶은 곳은 인도네시아 휴양지?? 해변가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016749.1.18",
          "form": "한국이랑 비슷해서 편안한 느낌이긴 해",
          "original_form": "한국이랑 비슷해서 편안한 느낌이긴 해",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016749.1.19",
          "form": "또 다른 곳 어디 가고싶니?",
          "original_form": "또 다른 곳 어디 가고싶니?",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016749.1.20",
          "form": "아하 대만은 좀 도시느낌이지?",
          "original_form": "아하 대만은 좀 도시느낌이지?",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016749.1.21",
          "form": "난 제주도 ...ㅋㅋㅋㅋ 성인되고 못가봐서",
          "original_form": "난 제주도 ...ㅋㅋㅋㅋ 성인되고 못가봐서",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016749.1.22",
          "form": "차타고 드라이브하고싶어",
          "original_form": "차타고 드라이브하고싶어",
          "speaker_id": "2",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016749.1.23",
          "form": "타이페이는 완전 도시느낌이야 ㅡㅋㅋ 근데 약간 홍콩영화스러운 감성도 좀 있었던 것 같아ㅋㅋ",
          "original_form": "타이페이는 완전 도시느낌이야 ㅡㅋㅋ 근데 약간 홍콩영화스러운 감성도 좀 있었던 것 같아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016749.1.24",
          "form": "그거도 오래됐지만..",
          "original_form": "그거도 오래됐지만..",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016749.1.25",
          "form": "제주도 고등학생때 간게ㅔ끝이구나#~",
          "original_form": "제주도 고등학생때 간게ㅔ끝이구나#~",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016749.1.26",
          "form": "다음에 기회되면 같이 가자ㅎㅎ",
          "original_form": "다음에 기회되면 같이 가자ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016749.1.27",
          "form": "ㅋㅋㅋ 중국 영화? 그런곳 감성인가",
          "original_form": "ㅋㅋㅋ 중국 영화? 그런곳 감성인가",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016749.1.28",
          "form": "근데 뚜벅이아 드라이브는 난 못해..ㅎ",
          "original_form": "근데 뚜벅이아 드라이브는 난 못해..ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016749.1.29",
          "form": "좋아 제주도도 대만도 인도네시아도 다가고싶어ㅠㅠ",
          "original_form": "좋아 제주도도 대만도 인도네시아도 다가고싶어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016749.1.30",
          "form": "약간 그런 감성같은게 있긴하더라구",
          "original_form": "약간 그런 감성같은게 있긴하더라구",
          "speaker_id": "1",
          "time": "20210826 15:15"
        }
      ]
