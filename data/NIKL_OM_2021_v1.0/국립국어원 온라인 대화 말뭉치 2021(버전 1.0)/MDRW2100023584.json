      "utterance": [
        {
          "id": "MDRW2100023584.1.1",
          "form": "여행가기에 날씨가 별로일텐데 요즘비와서 못가시죠?",
          "original_form": "여행가기에 날씨가 별로일텐데 요즘비와서 못가시죠?",
          "speaker_id": "1",
          "time": "20210830 11:54"
        },
        {
          "id": "MDRW2100023584.1.2",
          "form": "네 ㅠ 금요일  텐트 치는 날 비 오고",
          "original_form": "네 ㅠ 금요일  텐트 치는 날 비 오고",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023584.1.3",
          "form": "일요일 텐트 걷는 날 비 왔어요",
          "original_form": "일요일 텐트 걷는 날 비 왔어요",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100023584.1.4",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023584.1.5",
          "form": "텐트에 비 맞으면 안되는거 아니예요?",
          "original_form": "텐트에 비 맞으면 안되는거 아니예요?",
          "speaker_id": "1",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023584.1.6",
          "form": "힘드셨겠엉ㆍ",
          "original_form": "힘드셨겠엉ㆍ",
          "speaker_id": "1",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100023584.1.7",
          "form": "네 ㅠ 힘들었습니다",
          "original_form": "네 ㅠ 힘들었습니다",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023584.1.8",
          "form": "갔다와서 말리느라 또 쳤어요 ㅠ",
          "original_form": "갔다와서 말리느라 또 쳤어요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023584.1.9",
          "form": "일기예보가 계속 바껴가지고",
          "original_form": "일기예보가 계속 바껴가지고",
          "speaker_id": "2",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023584.1.10",
          "form": "집이 넓으시나봐요\n그걸 다시 치시고 그니까요 요즘 날씨에는 캠핑하기 까다로울거같아요",
          "original_form": "집이 넓으시나봐요\n그걸 다시 치시고 그니까요 요즘 날씨에는 캠핑하기 까다로울거같아요",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023584.1.11",
          "form": "아니요 ㅎㅎ 집에 친거아니고",
          "original_form": "아니요 ㅎㅎ 집에 친거아니고",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023584.1.12",
          "form": "공터에 쳤습니다 ㅎㅎㅎ",
          "original_form": "공터에 쳤습니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023584.1.13",
          "form": "아 열정이 대단하시네요",
          "original_form": "아 열정이 대단하시네요",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023584.1.14",
          "form": "새텐트 샀는데 곰팡이 생길까봐요 ㅠ",
          "original_form": "새텐트 샀는데 곰팡이 생길까봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023584.1.15",
          "form": "비맞아서 ㅠ",
          "original_form": "비맞아서 ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023584.1.16",
          "form": "날씨가 오늘은 후덥지근하네요",
          "original_form": "날씨가 오늘은 후덥지근하네요",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023584.1.17",
          "form": "그쵸 날씨가 왜 이모양인지ㅜㅜ",
          "original_form": "그쵸 날씨가 왜 이모양인지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023584.1.18",
          "form": "네\n오락가락하니",
          "original_form": "네\n오락가락하니",
          "speaker_id": "2",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100023584.1.19",
          "form": "종 잡을수가 없네요",
          "original_form": "종 잡을수가 없네요",
          "speaker_id": "2",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100023584.1.20",
          "form": "내일부터 계속 비소식 있던데",
          "original_form": "내일부터 계속 비소식 있던데",
          "speaker_id": "2",
          "time": "20210830 15:34"
        },
        {
          "id": "MDRW2100023584.1.21",
          "form": "그니까요 내일 이사하는데 걱정이네요",
          "original_form": "그니까요 내일 이사하는데 걱정이네요",
          "speaker_id": "1",
          "time": "20210830 15:50"
        },
        {
          "id": "MDRW2100023584.1.22",
          "form": "아 이사하시는구나 ㅠ",
          "original_form": "아 이사하시는구나 ㅠ",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100023584.1.23",
          "form": "부디 비가 안오길...",
          "original_form": "부디 비가 안오길...",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100023584.1.24",
          "form": "네 비가 안오길 바라고 있어요",
          "original_form": "네 비가 안오길 바라고 있어요",
          "speaker_id": "1",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023584.1.25",
          "form": "요즘 날씨가 안좋아 걱정이예요",
          "original_form": "요즘 날씨가 안좋아 걱정이예요",
          "speaker_id": "1",
          "time": "20210830 16:27"
        }
      ]
    }
