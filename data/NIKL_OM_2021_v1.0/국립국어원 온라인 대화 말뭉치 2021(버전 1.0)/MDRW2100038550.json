      "utterance": [
        {
          "id": "MDRW2100038550.1.1",
          "form": "점심 안 먹은 사람처럼 얘기한다",
          "original_form": "점심 안 먹은 사람처럼 얘기한다",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100038550.1.2",
          "form": "하지만... 배고파",
          "original_form": "하지만... 배고파",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100038550.1.3",
          "form": "간식먹고싶다",
          "original_form": "간식먹고싶다",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100038550.1.4",
          "form": "먹으세요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹으세요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100038550.1.5",
          "form": "간식이 없다구",
          "original_form": "간식이 없다구",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038550.1.6",
          "form": "물만 들이키는중",
          "original_form": "물만 들이키는중",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038550.1.7",
          "form": "바로 앞에가 시장인디....",
          "original_form": "바로 앞에가 시장인디....",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100038550.1.8",
          "form": "놀랍게도 근무시간엔 갈 수 없답니다",
          "original_form": "놀랍게도 근무시간엔 갈 수 없답니다",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100038550.1.9",
          "form": "사장님놈이",
          "original_form": "사장님놈이",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100038550.1.10",
          "form": "사오지도않구요",
          "original_form": "사오지도않구요",
          "speaker_id": "1",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100038550.1.11",
          "form": "정말 놀랍다... 탕비실 채워두라고 해...",
          "original_form": "정말 놀랍다... 탕비실 채워두라고 해...",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100038550.1.12",
          "form": "피자먹고싶다",
          "original_form": "피자먹고싶다",
          "speaker_id": "1",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100038550.1.13",
          "form": "어디 브랜드?",
          "original_form": "어디 브랜드?",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100038550.1.14",
          "form": "피자마루",
          "original_form": "피자마루",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038550.1.15",
          "form": "치즈폭탄페퍼로니",
          "original_form": "치즈폭탄페퍼로니",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100038550.1.16",
          "form": "신성당 떡보ㄲ이도 또 가거 싶다",
          "original_form": "신성당 떡보ㄲ이도 또 가거 싶다",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038550.1.17",
          "form": "그 튀김 전골 먹어보고시퍼",
          "original_form": "그 튀김 전골 먹어보고시퍼",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038550.1.18",
          "form": "튀김 전골은 머야",
          "original_form": "튀김 전골은 머야",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100038550.1.19",
          "form": "멀라 메뉴판엔 떡볶이보다 크게 그려져있던데",
          "original_form": "멀라 메뉴판엔 떡볶이보다 크게 그려져있던데",
          "speaker_id": "1",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100038550.1.20",
          "form": "밀어주는 메뉴인가?",
          "original_form": "밀어주는 메뉴인가?",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100038550.1.21",
          "form": "신성당이 구 시내에 잌ㅅ는 거지?",
          "original_form": "신성당이 구 시내에 잌ㅅ는 거지?",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100038550.1.22",
          "form": "ㅇㅇ시내에 있는거",
          "original_form": "ㅇㅇ시내에 있는거",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100038550.1.23",
          "form": "서원대 근처랑 헷갈려",
          "original_form": "서원대 근처랑 헷갈려",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100038550.1.24",
          "form": "떡볶이도 조으니까 서원대 근처에서 먹어도 되지",
          "original_form": "떡볶이도 조으니까 서원대 근처에서 먹어도 되지",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100038550.1.25",
          "form": "거긴 전골 없겠지만",
          "original_form": "거긴 전골 없겠지만",
          "speaker_id": "1",
          "time": "20210902 16:07"
        }
      ]
