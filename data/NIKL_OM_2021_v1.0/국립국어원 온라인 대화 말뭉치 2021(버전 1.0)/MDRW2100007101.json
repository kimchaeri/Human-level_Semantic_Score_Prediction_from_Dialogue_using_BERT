      "utterance": [
        {
          "id": "MDRW2100007101.1.1",
          "form": "오늘 뭐드셨어요?",
          "original_form": "오늘 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007101.1.2",
          "form": "돈가스 배달해 먹었어요!",
          "original_form": "돈가스 배달해 먹었어요!",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007101.1.3",
          "form": "처음 시켜본 곳이었는데 엄-청 맛있지는 않았어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "처음 시켜본 곳이었는데 엄-청 맛있지는 않았어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007101.1.4",
          "form": "와 저도 돈까스 먹었어요! 대신 안튀겨진거 산거라 직접튀겨먹었어요",
          "original_form": "와 저도 돈까스 먹었어요! 대신 안튀겨진거 산거라 직접튀겨먹었어요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007101.1.5",
          "form": "오호 통했네요",
          "original_form": "오호 통했네요",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007101.1.6",
          "form": "집에서 튀기면 번거롭지 않아요?",
          "original_form": "집에서 튀기면 번거롭지 않아요?",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007101.1.7",
          "form": "집에서 튀긴 마지막 돈가스는",
          "original_form": "집에서 튀긴 마지막 돈가스는",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007101.1.8",
          "form": "옛날에 정형돈 씨가 광고하던 도니도니 돈가스ㅋㅋㅋㅋㅋㅋ",
          "original_form": "옛날에 정형돈 씨가 광고하던 도니도니 돈가스ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007101.1.9",
          "form": "번거롭긴한데 확실히 맛있더라고요 아ㅋㅋ도니도니 돈가스 진짜 오랜만에 들어보네요",
          "original_form": "번거롭긴한데 확실히 맛있더라고요 아ㅋㅋ도니도니 돈가스 진짜 오랜만에 들어보네요",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007101.1.10",
          "form": "맞아요 갓 튀긴 돈가스라니!!!!",
          "original_form": "맞아요 갓 튀긴 돈가스라니!!!!",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007101.1.11",
          "form": "너무 맛있지요",
          "original_form": "너무 맛있지요",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007101.1.12",
          "form": "도니도니 돈가스 치돈이 진짜 맛있었는데..",
          "original_form": "도니도니 돈가스 치돈이 진짜 맛있었는데..",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007101.1.13",
          "form": "한동안 매일 먹었어요 진짜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "한동안 매일 먹었어요 진짜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007101.1.14",
          "form": "와 치돈 생각만해도 군침도네요 내일은 뭐드실시 생각해보셨나요?",
          "original_form": "와 치돈 생각만해도 군침도네요 내일은 뭐드실시 생각해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007101.1.15",
          "form": "내일은 외출 예정이라 엄청 많이 먹을 것 같아요",
          "original_form": "내일은 외출 예정이라 엄청 많이 먹을 것 같아요",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007101.1.16",
          "form": "커피도 많이 마시고",
          "original_form": "커피도 많이 마시고",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007101.1.17",
          "form": "영등포쪽 맛집 아시나요!",
          "original_form": "영등포쪽 맛집 아시나요!",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007101.1.18",
          "form": "제가 집콕러라ㅋㅋ 그냥 늘 인스타 찾아보고 가는편이에요",
          "original_form": "제가 집콕러라ㅋㅋ 그냥 늘 인스타 찾아보고 가는편이에요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007101.1.19",
          "form": "저도 영등포쪽은 갈 일이 없어서 모르는데",
          "original_form": "저도 영등포쪽은 갈 일이 없어서 모르는데",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007101.1.20",
          "form": "내일 약속이 영등포예요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내일 약속이 영등포예요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007101.1.21",
          "form": "카페 가시면 보통 어떤거 드시나요?",
          "original_form": "카페 가시면 보통 어떤거 드시나요?",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007101.1.22",
          "form": "저는 단 음료 좋아해서",
          "original_form": "저는 단 음료 좋아해서",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007101.1.23",
          "form": "아인슈페너나 연유라떼 이런 거 주로 마셔요!",
          "original_form": "아인슈페너나 연유라떼 이런 거 주로 마셔요!",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007101.1.24",
          "form": "단거 좋아하시는구나 저는 무조건 아메리카노 마셔요ㅋㅋ",
          "original_form": "단거 좋아하시는구나 저는 무조건 아메리카노 마셔요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007101.1.25",
          "form": "좋아하시는 카페 있나요?",
          "original_form": "좋아하시는 카페 있나요?",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007101.1.26",
          "form": "진짜 덥고 목마르면 아메리카노 마시는데 보통은 단 걸로ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 덥고 목마르면 아메리카노 마시는데 보통은 단 걸로ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007101.1.27",
          "form": "개인 카페 좋아해서 보통 그런 곳 가는데",
          "original_form": "개인 카페 좋아해서 보통 그런 곳 가는데",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007101.1.28",
          "form": "없으면 스벅가요",
          "original_form": "없으면 스벅가요",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007101.1.29",
          "form": "혹시 가보셨던 개인카페중에 추천하시는곳 있나요?",
          "original_form": "혹시 가보셨던 개인카페중에 추천하시는곳 있나요?",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007101.1.30",
          "form": "혜화에 온더모먼트라는 곳이 있는데",
          "original_form": "혜화에 온더모먼트라는 곳이 있는데",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007101.1.31",
          "form": "여기 아인슈페너가 진짜 최고예요",
          "original_form": "여기 아인슈페너가 진짜 최고예요",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007101.1.32",
          "form": "대박 혜화 갈일있는데 꼭 가봐야겠네요",
          "original_form": "대박 혜화 갈일있는데 꼭 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007101.1.33",
          "form": "네 왕 추천이에요!!",
          "original_form": "네 왕 추천이에요!!",
          "speaker_id": "2",
          "time": "20210822 01:08"
        }
      ]
    }
  ]
