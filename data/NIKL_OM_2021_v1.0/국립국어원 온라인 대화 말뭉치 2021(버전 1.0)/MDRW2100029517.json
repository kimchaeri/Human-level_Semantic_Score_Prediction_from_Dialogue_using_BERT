      "utterance": [
        {
          "id": "MDRW2100029517.1.1",
          "form": "먹는 즐거움이 넘 커서",
          "original_form": "먹는 즐거움이 넘 커서",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100029517.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029517.1.3",
          "form": "ㅎㅎ잠시 나갔다  왔어요",
          "original_form": "ㅎㅎ잠시 나갔다  왔어요",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029517.1.4",
          "form": "원하는 몸매로 살기엔 의지박약이라 힘드네요",
          "original_form": "원하는 몸매로 살기엔 의지박약이라 힘드네요",
          "speaker_id": "2",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029517.1.5",
          "form": "세상에 맛난게 넘 많잖아요",
          "original_form": "세상에 맛난게 넘 많잖아요",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029517.1.6",
          "form": "근데 또, 늘 먹던것만 먹는건 왜일까요?",
          "original_form": "근데 또, 늘 먹던것만 먹는건 왜일까요?",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029517.1.7",
          "form": "나만 그런가..ㅎㅎ",
          "original_form": "나만 그런가..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029517.1.8",
          "form": "원래 아는 맛이 젤 맛있다잖아요",
          "original_form": "원래 아는 맛이 젤 맛있다잖아요",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029517.1.9",
          "form": "주로 어떤 거 많이 드세요?",
          "original_form": "주로 어떤 거 많이 드세요?",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029517.1.10",
          "form": "탄수화물 위주의 고칼로리음식들..?",
          "original_form": "탄수화물 위주의 고칼로리음식들..?",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029517.1.11",
          "form": "도넛?",
          "original_form": "도넛?",
          "speaker_id": "1",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029517.1.12",
          "form": "면사랑 ~칼국수 수제비 냉면",
          "original_form": "면사랑 ~칼국수 수제비 냉면",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029517.1.13",
          "form": "빵까지..",
          "original_form": "빵까지..",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029517.1.14",
          "form": "아..양심 찔려요",
          "original_form": "아..양심 찔려요",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029517.1.15",
          "form": "면 빵 진짜 맛있죠",
          "original_form": "면 빵 진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210903 11:29"
        },
        {
          "id": "MDRW2100029517.1.16",
          "form": "저도 베이커리류 완전 좋아해요",
          "original_form": "저도 베이커리류 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029517.1.17",
          "form": "크리스피도넛",
          "original_form": "크리스피도넛",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029517.1.18",
          "form": "생크림케이크",
          "original_form": "생크림케이크",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029517.1.19",
          "form": "마카롱",
          "original_form": "마카롱",
          "speaker_id": "1",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029517.1.20",
          "form": "꽈배기",
          "original_form": "꽈배기",
          "speaker_id": "2",
          "time": "20210903 11:31"
        },
        {
          "id": "MDRW2100029517.1.21",
          "form": "애들이 생크림케익 그만 사오래요",
          "original_form": "애들이 생크림케익 그만 사오래요",
