      "utterance": [
        {
          "id": "MDRW2100030610.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210904 10:13"
        },
        {
          "id": "MDRW2100030610.1.2",
          "form": "네 요즘 책 거래 중이에요",
          "original_form": "네 요즘 책 거래 중이에요",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030610.1.3",
          "form": "어떤 내용일지?",
          "original_form": "어떤 내용일지?",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030610.1.4",
          "form": "전공책이요ㅠㅠㅋㅋ",
          "original_form": "전공책이요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100030610.1.5",
          "form": "팔리나여?",
          "original_form": "팔리나여?",
          "speaker_id": "1",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100030610.1.6",
          "form": "네 잘 팔리더라고요",
          "original_form": "네 잘 팔리더라고요",
          "speaker_id": "2",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100030610.1.7",
          "form": "책도ㅜ수요가 있네여",
          "original_form": "책도ㅜ수요가 있네여",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100030610.1.8",
          "form": "맞아요! 어차피 그 돈으로 다시 책 사야되지만 말이에요ㅠ",
          "original_form": "맞아요! 어차피 그 돈으로 다시 책 사야되지만 말이에요ㅠ",
          "speaker_id": "2",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100030610.1.9",
          "form": "하긴",
          "original_form": "하긴",
          "speaker_id": "1",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100030610.1.10",
          "form": "요즘 사고 싶은 거 있으신가요",
          "original_form": "요즘 사고 싶은 거 있으신가요",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100030610.1.11",
          "form": "생각중이오",
          "original_form": "생각중이오",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100030610.1.12",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
