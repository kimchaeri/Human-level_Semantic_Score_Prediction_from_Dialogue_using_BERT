      "utterance": [
        {
          "id": "MDRW2100036460.1.1",
          "form": "너 인스타마켓 알아?",
          "original_form": "너 인스타마켓 알아?",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.2",
          "form": "너 요즘 뭐 구매한거없어?",
          "original_form": "너 요즘 뭐 구매한거없어?",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.3",
          "form": "사람들이 인스타에서 물건 팔더라",
          "original_form": "사람들이 인스타에서 물건 팔더라",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.4",
          "form": "응 나 석고방향제를 샀어",
          "original_form": "응 나 석고방향제를 샀어",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.5",
          "form": "아아 응 아는데 나는 잘 이용을 안해",
          "original_form": "아아 응 아는데 나는 잘 이용을 안해",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.6",
          "form": "엄마가 원하는 디자인이 있어서",
          "original_form": "엄마가 원하는 디자인이 있어서",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.7",
          "form": "너는 그걸 자주 이용하니?",
          "original_form": "너는 그걸 자주 이용하니?",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.8",
          "form": "인스타마켓 셀러들 너무 불친절해",
          "original_form": "인스타마켓 셀러들 너무 불친절해",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.9",
          "form": "아니 가격을 안알려주지뭐야?",
          "original_form": "아니 가격을 안알려주지뭐야?",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.10",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.11",
          "form": "매일 합장하는 이모티콘만 보내",
          "original_form": "매일 합장하는 이모티콘만 보내",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.12",
          "form": "자기 아파서 9월 1일에 주문해달래",
          "original_form": "자기 아파서 9월 1일에 주문해달래",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.13",
          "form": "나는 단지 가격이 궁금했어",
          "original_form": "나는 단지 가격이 궁금했어",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036460.1.14",
          "form": "그걸 감내할만큼 디자인이 예쁘니?",
          "original_form": "그걸 감내할만큼 디자인이 예쁘니?",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.15",
          "form": "그런 불친절한 쇼핑몰ㅇ.ㄴ 이용하지 않을거야",
          "original_form": "그런 불친절한 쇼핑몰ㅇ.ㄴ 이용하지 않을거야",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.16",
          "form": "모양이 특이해",
          "original_form": "모양이 특이해",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.17",
          "form": "일반마켓에선 팔지않는 모양이었거든",
          "original_form": "일반마켓에선 팔지않는 모양이었거든",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.18",
          "form": "맞아 나도 그래서 인스타마켓은 이용안해",
          "original_form": "맞아 나도 그래서 인스타마켓은 이용안해",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.19",
          "form": "인스타마켓은 신기한 거 같아",
          "original_form": "인스타마켓은 신기한 거 같아",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.20",
          "form": "너는 차릴 생각 있어?",
          "original_form": "너는 차릴 생각 있어?",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036460.1.21",
          "form": "흠 그럼 좀 소통이 어려워도 노력해야겠다",
          "original_form": "흠 그럼 좀 소통이 어려워도 노력해야겠다",
          "speaker_id": "2",
          "time": "20210830 23:28"
        }
      ]
