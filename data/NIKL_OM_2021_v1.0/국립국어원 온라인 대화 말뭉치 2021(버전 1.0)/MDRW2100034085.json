      "utterance": [
        {
          "id": "MDRW2100034085.1.1",
          "form": "반려동물 기르시나요",
          "original_form": "반려동물 기르시나요",
          "speaker_id": "2",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100034085.1.2",
          "form": "예전에 청거북이 길러봤어요",
          "original_form": "예전에 청거북이 길러봤어요",
          "speaker_id": "1",
          "time": "20210827 17:02"
        },
        {
          "id": "MDRW2100034085.1.3",
          "form": "와 청거북이요~ 얼마나 기르셨나요",
          "original_form": "와 청거북이요~ 얼마나 기르셨나요",
          "speaker_id": "2",
          "time": "20210827 17:40"
        },
        {
          "id": "MDRW2100034085.1.4",
          "form": "6-7년 정도 길렀어요",
          "original_form": "6-7년 정도 길렀어요",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100034085.1.5",
          "form": "그러셨군요 1마리만 기르셨나요?",
          "original_form": "그러셨군요 1마리만 기르셨나요?",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100034085.1.6",
          "form": "두마리 길렀어요 ㅎㅎ",
          "original_form": "두마리 길렀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100034085.1.7",
          "form": "각각의 이름도 있었겠어요",
          "original_form": "각각의 이름도 있었겠어요",
          "speaker_id": "2",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100034085.1.8",
          "form": "이름은 없었어요",
          "original_form": "이름은 없었어요",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100034085.1.9",
          "form": "그랬군요 거북이 물은 얼마나 자주 갈아줘야하나요",
          "original_form": "그랬군요 거북이 물은 얼마나 자주 갈아줘야하나요",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100034085.1.10",
          "form": "일주일 한번정도 갈아줬어요",
          "original_form": "일주일 한번정도 갈아줬어요",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034085.1.11",
          "form": "먹이외에는 돈 들어가는 일은 없었나요?",
          "original_form": "먹이외에는 돈 들어가는 일은 없었나요?",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100034085.1.12",
          "form": "네 없었어요",
          "original_form": "네 없었어요",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100034085.1.13",
          "form": "거북이를 기르시게 된 계기가 어찌되나요",
          "original_form": "거북이를 기르시게 된 계기가 어찌되나요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034085.1.14",
          "form": "다들 기르길래요 ㅎㅎ",
          "original_form": "다들 기르길래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034085.1.15",
          "form": "어린시절 생각하면 거북이 기르는 집이 많았던 것 같아요 지금보다",
          "original_form": "어린시절 생각하면 거북이 기르는 집이 많았던 것 같아요 지금보다",
