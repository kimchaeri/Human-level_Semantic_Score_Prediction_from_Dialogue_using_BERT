      "utterance": [
        {
          "id": "MDRW2100013370.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013370.1.2",
          "form": "네! 좋아해요ㅎㅎ 뮤지컬도 좋아하고 영화보는것도 콘서트도 전시회도요",
          "original_form": "네! 좋아해요ㅎㅎ 뮤지컬도 좋아하고 영화보는것도 콘서트도 전시회도요",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013370.1.3",
          "form": "name2은 어떠세요?",
          "original_form": "&name2&은 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013370.1.4",
          "form": "네 저도 다 좋아해요",
          "original_form": "네 저도 다 좋아해요",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013370.1.5",
          "form": "이번주 금요일",
          "original_form": "이번주 금요일",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013370.1.6",
          "form": "옥상달빛 공연이있는데",
          "original_form": "옥상달빛 공연이있는데",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013370.1.7",
          "form": "무료티켓 당첨돼서 관람갈거에요",
          "original_form": "무료티켓 당첨돼서 관람갈거에요",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100013370.1.8",
          "form": "우와 축하해요 어떻게 당첨되신거에요?",
          "original_form": "우와 축하해요 어떻게 당첨되신거에요?",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013370.1.9",
          "form": "사연 보내야해서",
          "original_form": "사연 보내야해서",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013370.1.10",
          "form": "임 신한 아내가",
          "original_form": "임 신한 아내가",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013370.1.11",
          "form": "집에만 있어서 힘들어한다고 썼어요",
          "original_form": "집에만 있어서 힘들어한다고 썼어요",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013370.1.12",
          "form": "줄수밖에 없네요",
          "original_form": "줄수밖에 없네요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013370.1.13",
          "form": "가장 최근에 본공연은 뭐에요?",
          "original_form": "가장 최근에 본공연은 뭐에요?",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013370.1.14",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013370.1.15",
          "form": "제가 담당자라도 뽑았을거에요",
          "original_form": "제가 담당자라도 뽑았을거에요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013370.1.16",
          "form": "어 제일 최근... 온라인 콘서트도 되나요?",
          "original_form": "어 제일 최근... 온라인 콘서트도 되나요?",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013370.1.17",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013370.1.18",
          "form": "제가 방탄소년단팬이어서 온라인으로 봤어요ㅋㅋㅋ",
          "original_form": "제가 방탄소년단팬이어서 온라인으로 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013370.1.19",
          "form": "원래 콘서트가 잡혀있었는데 코로나때문에 전부 취소가 되가지구..",
          "original_form": "원래 콘서트가 잡혀있었는데 코로나때문에 전부 취소가 되가지구..",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013370.1.20",
          "form": "아하 저도 한때 유튜브로 bts영상 많이 봤었는데",
          "original_form": "아하 저도 한때 유튜브로 bts영상 많이 봤었는데",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013370.1.21",
          "form": "그래도 금요일날 가신다니까 너무 잘됐어요",
          "original_form": "그래도 금요일날 가신다니까 너무 잘됐어요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013370.1.22",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210825 15:34"
        }
      ]
