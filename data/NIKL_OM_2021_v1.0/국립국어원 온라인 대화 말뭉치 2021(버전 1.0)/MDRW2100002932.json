      "utterance": [
        {
          "id": "MDRW2100002932.1.1",
          "form": "11번가 탈퇴할까 고민스러워요",
          "original_form": "11번가 탈퇴할까 고민스러워요",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002932.1.2",
          "form": "쇼핑 어플들 다 삭제할까싶고",
          "original_form": "쇼핑 어플들 다 삭제할까싶고",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002932.1.3",
          "form": "그쳐 계속 할인행사 뭔알람도오고",
          "original_form": "그쳐 계속 할인행사 뭔알람도오고",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002932.1.4",
          "form": "근데또 비교해서 구입하는맛에 지우지는못하고요ㅜ",
          "original_form": "근데또 비교해서 구입하는맛에 지우지는못하고요ㅜ",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002932.1.5",
          "form": "보통 어디서구입하세요",
          "original_form": "보통 어디서구입하세요",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002932.1.6",
          "form": "쿠폰 준다하니까 장바구니 그득그듣 채워요 막",
          "original_form": "쿠폰 준다하니까 장바구니 그득그듣 채워요 막",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002932.1.7",
          "form": "11번가요 ㅠㅠ",
          "original_form": "11번가요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002932.1.8",
          "form": "마자요 근데 요즘11번가도 혜택이 많이줄더라구여ㅜ",
          "original_form": "마자요 근데 요즘11번가도 혜택이 많이줄더라구여ㅜ",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002932.1.9",
          "form": "옛날엔 쿠폰할인율도 좋았는데",
          "original_form": "옛날엔 쿠폰할인율도 좋았는데",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002932.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002932.1.11",
          "form": "그리고 11번가에서 머지를 사가지고 ㅠ",
          "original_form": "그리고 11번가에서 머지를 사가지고 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002932.1.12",
          "form": "아아..!",
          "original_form": "아아..!",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002932.1.13",
          "form": "너무 속상해요 내돈 ㅠ",
          "original_form": "너무 속상해요 내돈 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002932.1.14",
          "form": "그쳐 지금 어떻게 환불해주는얘기도없는거죠?",
          "original_form": "그쳐 지금 어떻게 환불해주는얘기도없는거죠?",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002932.1.15",
          "form": "물건너간듯해요 ㅠ",
          "original_form": "물건너간듯해요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002932.1.16",
          "form": "안사셨어요? 잘하셨어요",
          "original_form": "안사셨어요? 잘하셨어요",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002932.1.17",
          "form": "네 전 좀 그렇게 할인너무해주는거 미심쩍어서요ㅜ",
          "original_form": "네 전 좀 그렇게 할인너무해주는거 미심쩍어서요ㅜ",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002932.1.18",
          "form": "얼마나 남겨먹었나 평소엔",
          "original_form": "얼마나 남겨먹었나 평소엔",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002932.1.19",
          "form": "전 의심되던데요 ㅎ",
          "original_form": "전 의심되던데요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002932.1.20",
          "form": "돌려막기한거같던데 이상하긴했어요",
          "original_form": "돌려막기한거같던데 이상하긴했어요",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002932.1.21",
          "form": "ㅠㅠ 전 사놓고 쓰지도 못하고 아껴놓다 이 사단 난거에요",
          "original_form": "ㅠㅠ 전 사놓고 쓰지도 못하고 아껴놓다 이 사단 난거에요",
          "speaker_id": "2",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002932.1.22",
          "form": "싸게 풀길래 두번 사서 등록했는데",
          "original_form": "싸게 풀길래 두번 사서 등록했는데",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002932.1.23",
          "form": "사는곳이 시골이다 보니 이디야에서만 써지길래",
          "original_form": "사는곳이 시골이다 보니 이디야에서만 써지길래",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002932.1.24",
          "form": "카페 두번 간게 전부 ㅠ",
          "original_form": "카페 두번 간게 전부 ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002932.1.25",
          "form": "ㅜㅜㅜ환불꼭 해줬으면 좋겠네요",
          "original_form": "ㅜㅜㅜ환불꼭 해줬으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210820 16:59"
        }
      ]
    }
