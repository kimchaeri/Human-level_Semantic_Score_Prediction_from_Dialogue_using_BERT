      "utterance": [
        {
          "id": "MDRW2100024228.1.1",
          "form": "에디그램은 뭐 파는 곳인가용 ?",
          "original_form": "에디그램은 뭐 파는 곳인가용 ?",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024228.1.2",
          "form": "다시적어요ㅎㅎ못보셧을까봥",
          "original_form": "다시적어요ㅎㅎ못보셧을까봥",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024228.1.3",
          "form": "아아ㅎ 애디그램은 광고보고 적립쌓는거에요ㅎ",
          "original_form": "아아ㅎ 애디그램은 광고보고 적립쌓는거에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024228.1.4",
          "form": "20개 광고보면 하루에 500~600원?",
          "original_form": "20개 광고보면 하루에 500~600원?",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024228.1.5",
          "form": "전 25개해서 한달에 2만원정도? 버는듯요ㅎ",
          "original_form": "전 25개해서 한달에 2만원정도? 버는듯요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024228.1.6",
          "form": "헐적립율이괜찮네요?!",
          "original_form": "헐적립율이괜찮네요?!",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024228.1.7",
          "form": "제가 앱테크 초반에 출첵을 많이했었어요ㅎ",
          "original_form": "제가 앱테크 초반에 출첵을 많이했었어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024228.1.8",
          "form": "근데 시간이 아까워요",
          "original_form": "근데 시간이 아까워요",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024228.1.9",
          "form": "지금은 수익률나는거 위주로만 해요ㅎ",
          "original_form": "지금은 수익률나는거 위주로만 해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024228.1.10",
          "form": "저도 저런거는 애즐이나 퀘스트마인 하고있어용ㅎㅎ",
          "original_form": "저도 저런거는 애즐이나 퀘스트마인 하고있어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024228.1.11",
          "form": "우와ㅎ 애즐ㅎ 퀘스트마인은 처음등어요ㅎ",
          "original_form": "우와ㅎ 애즐ㅎ 퀘스트마인은 처음등어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024228.1.12",
          "form": "찾아봐야겟어요ㅎㅎ",
          "original_form": "찾아봐야겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024228.1.13",
          "form": "링켓은하시구요?",
          "original_form": "링켓은하시구요?",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024228.1.14",
          "form": "네네 링켓은하구있어요ㅋㅋ좋더라구용!",
          "original_form": "네네 링켓은하구있어요ㅋㅋ좋더라구용!",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024228.1.15",
          "form": "맞아요ㅎㅎ 두달째 이득보는중ㅎㅎ",
          "original_form": "맞아요ㅎㅎ 두달째 이득보는중ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024228.1.16",
          "form": "고수시구나ㅎㅎ",
          "original_form": "고수시구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024228.1.17",
          "form": "좋은정보가 있어야하는데ㅎ 요즘에는 출첵이 매리트가없어요ㅠ 쇼핑할인도 해야하는데ㅠ",
          "original_form": "좋은정보가 있어야하는데ㅎ 요즘에는 출첵이 매리트가없어요ㅠ 쇼핑할인도 해야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100024228.1.18",
          "form": "맞아요출첵이그렇게까진이득이없어요..ㅋㅋ근데하다보니 습관돼서 그냥하구있어요ㅋㅋ",
          "original_form": "맞아요출첵이그렇게까진이득이없어요..ㅋㅋ근데하다보니 습관돼서 그냥하구있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024228.1.19",
          "form": "전 은행앱들 엘포인트 뭐 이런거 우리가 아는 가장 기본적인 출첵앱을 안해요ㅎ",
          "original_form": "전 은행앱들 엘포인트 뭐 이런거 우리가 아는 가장 기본적인 출첵앱을 안해요ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024228.1.20",
          "form": "그러니깐 시간이남아서ㅎ 간편응모를 많이하게되더라구요ㄹ",
          "original_form": "그러니깐 시간이남아서ㅎ 간편응모를 많이하게되더라구요ㄹ",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024228.1.21",
          "form": "그래거 구매도하고 할인도 받고해요ㅎ",
          "original_form": "그래거 구매도하고 할인도 받고해요ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024228.1.22",
          "form": "이렇든저렇든 이벤트참여하면 좋은거같아용",
          "original_form": "이렇든저렇든 이벤트참여하면 좋은거같아용",
          "speaker_id": "1",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100024228.1.23",
          "form": "맞아요ㅎㅎㅎ",
          "original_form": "맞아요ㅎㅎㅎ",
          "speaker_id": "2",
