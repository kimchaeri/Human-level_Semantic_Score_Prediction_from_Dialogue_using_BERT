        },
        {
          "id": "MDRW2100006438.1.2",
          "form": "지금꺼 잘먹긴하는데 질릴까봐 ㅋ",
          "original_form": "지금꺼 잘먹긴하는데 질릴까봐 ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100006438.1.3",
          "form": "안질릴거같은데",
          "original_form": "안질릴거같은데",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100006438.1.4",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100006438.1.5",
          "form": "그러면 안먹었겠지",
          "original_form": "그러면 안먹었겠지",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006438.1.6",
          "form": "그건그렇군 ㅋㅋ",
          "original_form": "그건그렇군 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006438.1.7",
          "form": "지금 잘 먹잖아",
          "original_form": "지금 잘 먹잖아",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006438.1.8",
          "form": "웅웅..",
          "original_form": "웅웅..",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006438.1.9",
          "form": "그럼 안바꿔도 될듯",
          "original_form": "그럼 안바꿔도 될듯",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006438.1.10",
          "form": "웅 그대로 담에도 이거 사야겠다",
          "original_form": "웅 그대로 담에도 이거 사야겠다",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
