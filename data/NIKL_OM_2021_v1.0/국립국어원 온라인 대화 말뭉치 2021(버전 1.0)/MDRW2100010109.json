      "utterance": [
        {
          "id": "MDRW2100010109.1.1",
          "form": "뭐 즐겨듣는 가수는 있나요?",
          "original_form": "뭐 즐겨듣는 가수는 있나요?",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010109.1.2",
          "form": "요즘 슬기로운의사생활ost 자주 들어요!",
          "original_form": "요즘 슬기로운의사생활ost 자주 들어요!",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010109.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010109.1.4",
          "form": "와 슬의 ost!",
          "original_form": "와 슬의 ost!",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010109.1.5",
          "form": "엄창좋죠 ㅠㅠ",
          "original_form": "엄창좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010109.1.6",
          "form": "악뮤 낙하 열심히 듣고있어요!",
          "original_form": "악뮤 낙하 열심히 듣고있어요!",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010109.1.7",
          "form": "트와이스도 좋아해서 노래 많이 듣고요",
          "original_form": "트와이스도 좋아해서 노래 많이 듣고요",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010109.1.8",
          "form": "저도 낙하 너무 좋아서 매일 듣고 있어요~",
          "original_form": "저도 낙하 너무 좋아서 매일 듣고 있어요~",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010109.1.9",
          "form": "트와이스노래도 신나서 좋죠!",
          "original_form": "트와이스노래도 신나서 좋죠!",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010109.1.10",
          "form": "이번 신곡도 너무 제취향이었어요?",
          "original_form": "이번 신곡도 너무 제취향이었어요?",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010109.1.11",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010109.1.12",
          "form": "가장 좋은 곡이 뭔것같나요 ㅎㅎ",
          "original_form": "가장 좋은 곡이 뭔것같나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010109.1.13",
          "form": "타이틀곡 다 좋지만 제일 많이 들었던 노래는 메리앤해피 많이 들었어요",
          "original_form": "타이틀곡 다 좋지만 제일 많이 들었던 노래는 메리앤해피 많이 들었어요",
          "speaker_id": "2",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010109.1.14",
          "form": "겨울아닌데도 들으면 겨울생각나고 너무 좋아요",
          "original_form": "겨울아닌데도 들으면 겨울생각나고 너무 좋아요",
          "speaker_id": "2",
          "time": "20210823 20:23"
        },
        {
          "id": "MDRW2100010109.1.15",
          "form": "아 너무좋죠 ㅠㅠ",
          "original_form": "아 너무좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 21:05"
        },
        {
          "id": "MDRW2100010109.1.16",
          "form": "근데 전 겨울노래 여름에 많이 생각나는것같아요",
          "original_form": "근데 전 겨울노래 여름에 많이 생각나는것같아요",
          "speaker_id": "1",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100010109.1.17",
          "form": "너무 푹푹쪄서그런가 기분이라도 상큼하게",
          "original_form": "너무 푹푹쪄서그런가 기분이라도 상큼하게",
          "speaker_id": "1",
          "time": "20210823 21:06"
        },
        {
          "id": "MDRW2100010109.1.18",
          "form": "그 종소리들으면 .. 기분이 뭔가 좋아져요 캐롤송 너무 좋아요ㅎㅎ",
          "original_form": "그 종소리들으면 .. 기분이 뭔가 좋아져요 캐롤송 너무 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:08"
        },
        {
          "id": "MDRW2100010109.1.19",
          "form": "저도요 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010109.1.20",
          "form": "어떤 곡이 제일 좋으세요?",
          "original_form": "어떤 곡이 제일 좋으세요?",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010109.1.21",
          "form": "전 산타텔미 너무 좋아해요",
          "original_form": "전 산타텔미 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010109.1.22",
          "form": "저도 그노래 진짜 좋아해요~",
          "original_form": "저도 그노래 진짜 좋아해요~",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010109.1.23",
          "form": "저는 라스트크리스마스랑 핑클이 불렀던 화이트 좋아해요!!",
          "original_form": "저는 라스트크리스마스랑 핑클이 불렀던 화이트 좋아해요!!",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010109.1.24",
          "form": "화이트는 들어본적 없는데",
          "original_form": "화이트는 들어본적 없는데",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100010109.1.25",
          "form": "이번 크리스마스엔 꼭 틀어야겠네요 ㅎㅎ",
          "original_form": "이번 크리스마스엔 꼭 틀어야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:37"
        },
        {
          "id": "MDRW2100010109.1.26",
          "form": "네네 오마이걸이 리메이크한 버전도 있는데 이것도 좋아요ㅎㅎ",
          "original_form": "네네 오마이걸이 리메이크한 버전도 있는데 이것도 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:59"
        },
        {
          "id": "MDRW2100010109.1.27",
          "form": "한번 들어보셔요!",
          "original_form": "한번 들어보셔요!",
          "speaker_id": "2",
          "time": "20210824 00:59"
        },
        {
          "id": "MDRW2100010109.1.28",
          "form": "하아 너무 좋아요 ㅠㅠ",
          "original_form": "하아 너무 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100010109.1.29",
          "form": "캐롤음악은 듣기만해도 사르르 녹아요 정말",
          "original_form": "캐롤음악은 듣기만해도 사르르 녹아요 정말",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100010109.1.30",
          "form": "참.. 녹아요 라는 트와이스곡 정말 좋답니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "참.. 녹아요 라는 트와이스곡 정말 좋답니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100010109.1.31",
          "form": "좀 갑자기죠 ㅎㅎㅎ",
          "original_form": "좀 갑자기죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100010109.1.32",
          "form": "한번 들어볼게요ㅋㅋ",
          "original_form": "한번 들어볼게요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100010109.1.33",
          "form": "저는 트와이스 에스닉 옷입은게 정말 잘어울린다고 생각해요!",
          "original_form": "저는 트와이스 에스닉 옷입은게 정말 잘어울린다고 생각해요!",
          "speaker_id": "2",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100010109.1.34",
          "form": "느낌있죠! 저도 좋아해요",
          "original_form": "느낌있죠! 저도 좋아해요",
