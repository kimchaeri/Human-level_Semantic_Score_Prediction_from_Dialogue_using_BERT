      "utterance": [
        {
          "id": "MDRW2100002896.1.1",
          "form": "사실 저는 과일을 거의 다 좋아해요 ㅎ",
          "original_form": "사실 저는 과일을 거의 다 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002896.1.2",
          "form": "과일로 만든 청이나 음료같은것도 좋아하시나요?",
          "original_form": "과일로 만든 청이나 음료같은것도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002896.1.3",
          "form": "저는 좋아해서 종종 만들어먹어요",
          "original_form": "저는 좋아해서 종종 만들어먹어요",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002896.1.4",
          "form": "네~과일음료도 좋아해요 ㅎ",
          "original_form": "네~과일음료도 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002896.1.5",
          "form": "오~대단하시네요",
          "original_form": "오~대단하시네요",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002896.1.6",
          "form": "과일음료는 어떤거 좋아하세요?",
          "original_form": "과일음료는 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002896.1.7",
          "form": "음료도 딸기로 만든 것을 좋아하시나요?",
          "original_form": "음료도 딸기로 만든 것을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002896.1.8",
          "form": "저는 생과일쥬스는 수박쥬스요 ㅎ",
          "original_form": "저는 생과일쥬스는 수박쥬스요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002896.1.9",
          "form": "수박쥬스 진짜 좋아",
          "original_form": "수박쥬스 진짜 좋아",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002896.1.10",
          "form": "한번도 안만들어봤어요",
          "original_form": "한번도 안만들어봤어요",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002896.1.11",
          "form": "여름에 먹으면 진짜",
          "original_form": "여름에 먹으면 진짜",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002896.1.12",
          "form": "사실 만들 생각을 안했죠 ㅎㅎ",
          "original_form": "사실 만들 생각을 안했죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002896.1.13",
          "form": "아핫.. 사실 만들어 먹는 사람이 드물긴하죠",
          "original_form": "아핫.. 사실 만들어 먹는 사람이 드물긴하죠",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002896.1.14",
          "form": "요즘은 카페도 잘 되어있으니까요",
          "original_form": "요즘은 카페도 잘 되어있으니까요",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002896.1.15",
          "form": "과일청 만들기 어렵죠?",
          "original_form": "과일청 만들기 어렵죠?",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002896.1.16",
          "form": "어렵지는 않아요. 그냥 과일에 설탕 대박 때려붓고 졸이면되는거라ㅋㅋ",
          "original_form": "어렵지는 않아요. 그냥 과일에 설탕 대박 때려붓고 졸이면되는거라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002896.1.17",
          "form": "어렵기보다는 정성인거 같아요",
          "original_form": "어렵기보다는 정성인거 같아요",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002896.1.18",
          "form": "오~~그 두개만 넣으면 되요?",
          "original_form": "오~~그 두개만 넣으면 되요?",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002896.1.19",
          "form": "물은 과일이 약간 잠길정도로? 넣어주고 끓여주면되는데",
          "original_form": "물은 과일이 약간 잠길정도로? 넣어주고 끓여주면되는데",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002896.1.20",
          "form": "음 눌러붙으니 계속 저어줘야 하는게",
          "original_form": "음 눌러붙으니 계속 저어줘야 하는게",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002896.1.21",
          "form": "아 끓여야 하는구나ㅠㅠ",
          "original_form": "아 끓여야 하는구나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002896.1.22",
          "form": "음.. 끓이는거 말고 그냥 하는것도 있어요",
          "original_form": "음.. 끓이는거 말고 그냥 하는것도 있어요",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002896.1.23",
          "form": "오 그건 좀 편하겠어요",
          "original_form": "오 그건 좀 편하겠어요",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002896.1.24",
          "form": "과일잘라서 병에담고 설탕 때려부으면 한 2주~한달정도면",
          "original_form": "과일잘라서 병에담고 설탕 때려부으면 한 2주~한달정도면",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002896.1.25",
          "form": "되더라구요",
          "original_form": "되더라구요",
          "speaker_id": "1",
          "time": "20210820 16:25"
        }
      ]
