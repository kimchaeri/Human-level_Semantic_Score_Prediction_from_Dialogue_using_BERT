      "utterance": [
        {
          "id": "MDRW2100033881.1.1",
          "form": "사신 것 중에 삶의 질 상승템은 뭐가 있나요?",
          "original_form": "사신 것 중에 삶의 질 상승템은 뭐가 있나요?",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100033881.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100033881.1.3",
          "form": "아무래도 패드겠죠",
          "original_form": "아무래도 패드겠죠",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100033881.1.4",
          "form": "아 저도요 ㅋㅋㅋㅋ",
          "original_form": "아 저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033881.1.5",
          "form": "펜슬이랑 패드 진짜 너무편해요",
          "original_form": "펜슬이랑 패드 진짜 너무편해요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033881.1.6",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033881.1.7",
          "form": "요즘은 노트북사고싶어요",
          "original_form": "요즘은 노트북사고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033881.1.8",
          "form": "갤럭시북이나 그램이요",
          "original_form": "갤럭시북이나 그램이요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033881.1.9",
          "form": "저도 갤럭시북이요 ㅋㅋㅋ 터치가 되더라구요",
          "original_form": "저도 갤럭시북이요 ㅋㅋㅋ 터치가 되더라구요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033881.1.10",
          "form": "얼마나편할까요",
          "original_form": "얼마나편할까요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033881.1.11",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100033881.1.12",
          "form": "근데엄청비싸더라고요",
          "original_form": "근데엄청비싸더라고요",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100033881.1.13",
          "form": "ㅜㅜ 패드랑 노트북 따로 안쓰고 통합하고싶어요 ㅋㅋㅋㅋ",
          "original_form": "ㅜㅜ 패드랑 노트북 따로 안쓰고 통합하고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100033881.1.14",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033881.1.15",
          "form": "근데둘다",
          "original_form": "근데둘다",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033881.1.16",
          "form": "장단점이 너무명확해서",
          "original_form": "장단점이 너무명확해서",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033881.1.17",
          "form": "맞아욬ㅋㅋㅋ 노트북 너무무겁고.. 패드는 한글이 안되고",
          "original_form": "맞아욬ㅋㅋㅋ 노트북 너무무겁고.. 패드는 한글이 안되고",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033881.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100033881.1.19",
          "form": "그램은 왤케비싼지모르겠어여",
          "original_form": "그램은 왤케비싼지모르겠어여",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100033881.1.20",
          "form": "전자기기 한번사면 만족도 되게 오래가는데",
          "original_form": "전자기기 한번사면 만족도 되게 오래가는데",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033881.1.21",
          "form": "너무 비싸서 엄두가안나요 ㅋㅋ ㅜ",
          "original_form": "너무 비싸서 엄두가안나요 ㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100033881.1.22",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033881.1.23",
          "form": "전자기기는 다비싸더라고요",
          "original_form": "전자기기는 다비싸더라고요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033881.1.24",
          "form": "블루투스이어폰 있으신가요",
          "original_form": "블루투스이어폰 있으신가요",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100033881.1.25",
          "form": "네네 있죠 버즈",
          "original_form": "네네 있죠 버즈",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033881.1.26",
          "form": "다갤럭시네요 제껀ㅋㅋㅋ",
          "original_form": "다갤럭시네요 제껀ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100033881.1.27",
          "form": "삼성 깖맞춤 하셨군요 ㅋㅋㅋ",
          "original_form": "삼성 깖맞춤 하셨군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100033881.1.28",
          "form": "블투 이어폰도 비오는날에 편한거같아요",
          "original_form": "블투 이어폰도 비오는날에 편한거같아요",
