      "utterance": [
        {
          "id": "MDRW2100015903.1.1",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "2",
          "time": "20210826 02:39"
        },
        {
          "id": "MDRW2100015903.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 02:42"
        },
        {
          "id": "MDRW2100015903.1.3",
          "form": "엇 안녕하세요ㅋㅋㅋㅋ",
          "original_form": "엇 안녕하세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:42"
        },
        {
          "id": "MDRW2100015903.1.4",
          "form": "제가 책을 잘 안읽어가지고",
          "original_form": "제가 책을 잘 안읽어가지고",
          "speaker_id": "1",
          "time": "20210826 02:42"
        },
        {
          "id": "MDRW2100015903.1.5",
          "form": "책을 읽어야 하는데 걱정이네요ㅋㅋㅋ",
          "original_form": "책을 읽어야 하는데 걱정이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:42"
        },
        {
          "id": "MDRW2100015903.1.6",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:43"
        },
        {
          "id": "MDRW2100015903.1.7",
          "form": "인상깊게 읽은 책있으세요?",
          "original_form": "인상깊게 읽은 책있으세요?",
          "speaker_id": "2",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015903.1.8",
          "form": "이시국이라 조금 그렇긴한데",
          "original_form": "이시국이라 조금 그렇긴한데",
          "speaker_id": "1",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015903.1.9",
          "form": "히가시노 게이고 가면산장 살인사건",
          "original_form": "히가시노 게이고 가면산장 살인사건",
          "speaker_id": "1",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015903.1.10",
          "form": "이거 재밌게 읽었어요!ㅋㅋ",
          "original_form": "이거 재밌게 읽었어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015903.1.11",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 02:44"
        },
        {
          "id": "MDRW2100015903.1.12",
          "form": "아ㅋㅋ 엘리어트 파동 이론이요!",
          "original_form": "아ㅋㅋ 엘리어트 파동 이론이요!",
          "speaker_id": "2",
          "time": "20210826 02:46"
        },
        {
          "id": "MDRW2100015903.1.13",
          "form": "헉 제목만 봐도 어려워보여요ㅋㅋ",
          "original_form": "헉 제목만 봐도 어려워보여요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:47"
        },
        {
          "id": "MDRW2100015903.1.14",
          "form": "열심히 공부하고 있어요ㅋㅋ",
          "original_form": "열심히 공부하고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:48"
        },
        {
          "id": "MDRW2100015903.1.15",
          "form": "전공이 그쪽이세요?",
          "original_form": "전공이 그쪽이세요?",
          "speaker_id": "1",
          "time": "20210826 02:49"
        },
        {
          "id": "MDRW2100015903.1.16",
          "form": "아니요! 부자되고 싶어서 봅니다",
          "original_form": "아니요! 부자되고 싶어서 봅니다",
          "speaker_id": "2",
          "time": "20210826 02:50"
        },
        {
          "id": "MDRW2100015903.1.17",
          "form": "아하ㅋㅋ 저는 주식 경제 이런쪽 넘 어려워요ㅠㅠ",
          "original_form": "아하ㅋㅋ 저는 주식 경제 이런쪽 넘 어려워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:51"
        },
        {
          "id": "MDRW2100015903.1.18",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:52"
        },
        {
          "id": "MDRW2100015903.1.19",
          "form": "이국종 골든타임 보셨나요?",
          "original_form": "이국종 골든타임 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015903.1.20",
          "form": "헛 아니요ㅠㅠ",
          "original_form": "헛 아니요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:53"
        },
        {
          "id": "MDRW2100015903.1.21",
          "form": "대신 이국종 교수님을 모델로한",
          "original_form": "대신 이국종 교수님을 모델로한",
          "speaker_id": "1",
          "time": "20210826 02:54"
        },
        {
          "id": "MDRW2100015903.1.22",
          "form": "웹툰은 봤어요!",
          "original_form": "웹툰은 봤어요!",
          "speaker_id": "1",
          "time": "20210826 02:54"
        }
      ]
