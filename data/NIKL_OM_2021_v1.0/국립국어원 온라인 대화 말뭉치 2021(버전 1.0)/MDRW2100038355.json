      "utterance": [
        {
          "id": "MDRW2100038355.1.1",
          "form": "아 진짜 의지의 한국인이심!",
          "original_form": "아 진짜 의지의 한국인이심!",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100038355.1.2",
          "form": "앞으로도 결국 이렇게 먹는양을 줄여야하는거겠죠ㅜㅜ",
          "original_form": "앞으로도 결국 이렇게 먹는양을 줄여야하는거겠죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038355.1.3",
          "form": "아니 저도 그런 체질로 태어나고 싶어요",
          "original_form": "아니 저도 그런 체질로 태어나고 싶어요",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038355.1.4",
          "form": "왜 살찌는게 소원이라는",
          "original_form": "왜 살찌는게 소원이라는",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038355.1.5",
          "form": "먹어도 살 안찌는 체질로ㅜㅜㅜ",
          "original_form": "먹어도 살 안찌는 체질로ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038355.1.6",
          "form": "ㅜㅇㅜ",
          "original_form": "ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100038355.1.7",
          "form": "그 체질 바꾸는 습관? 그런것도 있던데요",
          "original_form": "그 체질 바꾸는 습관? 그런것도 있던데요",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100038355.1.8",
          "form": "기초대사량 늘리는",
          "original_form": "기초대사량 늘리는",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100038355.1.9",
          "form": "그것도 제눈엔 좀 까다로워보였지만",
          "original_form": "그것도 제눈엔 좀 까다로워보였지만",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100038355.1.10",
          "form": "자몽을 이긴 님이면 가능할것 같아요!",
          "original_form": "자몽을 이긴 님이면 가능할것 같아요!",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100038355.1.11",
          "form": "공부에 왕도가 없듯이 다이어트에도 왕도는 없네여",
          "original_form": "공부에 왕도가 없듯이 다이어트에도 왕도는 없네여",
          "speaker_id": "1",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100038355.1.12",
          "form": "결국 다 본질은 적게먹고 많이 움직여라인듯요ㅜㅜ 흑",
          "original_form": "결국 다 본질은 적게먹고 많이 움직여라인듯요ㅜㅜ 흑",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100038355.1.13",
          "form": "그쵸ㅜ 그 유명한 쥬비스 있잖아요",
          "original_form": "그쵸ㅜ 그 유명한 쥬비스 있잖아요",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100038355.1.14",
          "form": "그거 엄청 고가 다이어트인데도 결론은",
          "original_form": "그거 엄청 고가 다이어트인데도 결론은",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100038355.1.15",
          "form": "쥐똥만큼 먹고 운동해라 던데요ㅜ",
          "original_form": "쥐똥만큼 먹고 운동해라 던데요ㅜ",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100038355.1.16",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100038355.1.17",
          "form": "근데 굳이 따지면 거긴 밥좋아는사람들한테 식단이좋대요",
          "original_form": "근데 굳이 따지면 거긴 밥좋아는사람들한테 식단이좋대요",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100038355.1.18",
          "form": "밥 고기 쌈채소 이런식 식단이라고들었어요ㅡ",
          "original_form": "밥 고기 쌈채소 이런식 식단이라고들었어요ㅡ",
          "speaker_id": "1",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100038355.1.19",
          "form": "어 근데ㅜ 저아는 분이 그거 했는데 쌈 사이즈가 극악이래요",
          "original_form": "어 근데ㅜ 저아는 분이 그거 했는데 쌈 사이즈가 극악이래요",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100038355.1.20",
          "form": "그거 몇개 먹음 더 허기진다고 욕하던데요ㅜ",
          "original_form": "그거 몇개 먹음 더 허기진다고 욕하던데요ㅜ",
          "speaker_id": "2",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100038355.1.21",
          "form": "진짜요?사실 그거 자세한 프로그램",
          "original_form": "진짜요?사실 그거 자세한 프로그램",
          "speaker_id": "1",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100038355.1.22",
          "form": "인터넷엔 안올라오잖아요",
          "original_form": "인터넷엔 안올라오잖아요",
          "speaker_id": "1",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100038355.1.23",
          "form": "그래서 좀 궁금했는데ㅣ",
          "original_form": "그래서 좀 궁금했는데ㅣ",
          "speaker_id": "1",
          "time": "20210902 11:23"
        },
        {
          "id": "MDRW2100038355.1.24",
          "form": "ㅎㅎ 애기가 먹을 쌈 사이즈래요ㅎ",
          "original_form": "ㅎㅎ 애기가 먹을 쌈 사이즈래요ㅎ",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100038355.1.25",
          "form": "그 대단하단 쥬비스도 별거없네요",
          "original_form": "그 대단하단 쥬비스도 별거없네요",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100038355.1.26",
          "form": "개수라도많이먹나요?",
          "original_form": "개수라도많이먹나요?",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100038355.1.27",
          "form": "아뇨 그냥 크기도 적고 개수도 적고",
          "original_form": "아뇨 그냥 크기도 적고 개수도 적고",
          "speaker_id": "2",
          "time": "20210902 11:59"
        },
        {
          "id": "MDRW2100038355.1.28",
          "form": "간혹 연예인들이 살 못빼서 돈물어줬단 뉴스 왜 나오는지 알겠다던데요ㅎ",
          "original_form": "간혹 연예인들이 살 못빼서 돈물어줬단 뉴스 왜 나오는지 알겠다던데요ㅎ",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038355.1.29",
          "form": "하하 역시 제생각이 맞네요",
          "original_form": "하하 역시 제생각이 맞네요",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038355.1.30",
          "form": "결국 내 위장을 줄여야한다는ㅜ",
          "original_form": "결국 내 위장을 줄여야한다는ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038355.1.31",
          "form": "마음껏 먹고 살뺀다는건 거짓부렁이에요 으헝헝ㅜ",
          "original_form": "마음껏 먹고 살뺀다는건 거짓부렁이에요 으헝헝ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038355.1.32",
          "form": "ㅜㅇㅜ 자 기초대사량을 늘리는 겁니다ㅜ",
          "original_form": "ㅜㅇㅜ 자 기초대사량을 늘리는 겁니다ㅜ",
          "speaker_id": "2",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038355.1.33",
          "form": "그래야 그나마ㅜ",
          "original_form": "그래야 그나마ㅜ",
          "speaker_id": "2",
          "time": "20210902 12:18"
        }
      ]
    }
  ]
