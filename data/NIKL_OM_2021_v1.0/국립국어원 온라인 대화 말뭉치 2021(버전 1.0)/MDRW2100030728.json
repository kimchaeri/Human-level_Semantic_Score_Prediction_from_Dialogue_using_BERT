      "utterance": [
        {
          "id": "MDRW2100030728.1.1",
          "form": "비가 와서 잘 안 마르면 어떡하죠",
          "original_form": "비가 와서 잘 안 마르면 어떡하죠",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.2",
          "form": "한국 올 때 들고 올 수가 없었어요",
          "original_form": "한국 올 때 들고 올 수가 없었어요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.3",
          "form": "별말은 축축한 옷을 입어야 하나요",
          "original_form": "별말은 축축한 옷을 입어야 하나요",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.4",
          "form": "캐리어 두 개에 넣어야 했거든요",
          "original_form": "캐리어 두 개에 넣어야 했거든요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.5",
          "form": "그래서 다 정리해 찌요",
          "original_form": "그래서 다 정리해 찌요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.6",
          "form": "제주도에서 시키면 배송료가 비싸지요",
          "original_form": "제주도에서 시키면 배송료가 비싸지요",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.7",
          "form": "그 캐리어 두 개 중에 한 개는 컴퓨터에요",
          "original_form": "그 캐리어 두 개 중에 한 개는 컴퓨터에요",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100030728.1.8",
          "form": "추가 배송료가 붙나요",
          "original_form": "추가 배송료가 붙나요",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.9",
          "form": "짐이 얼마나 줄었는지 아시겠죠",
          "original_form": "짐이 얼마나 줄었는지 아시겠죠",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.10",
          "form": "컴퓨터는 놓칠 수가 없죠",
          "original_form": "컴퓨터는 놓칠 수가 없죠",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.11",
          "form": "캐리어 한 개당 10만 원",
          "original_form": "캐리어 한 개당 10만 원",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.12",
          "form": "너무 슬프네요",
          "original_form": "너무 슬프네요",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.13",
          "form": "근데 10년 동안 사용한 거라",
          "original_form": "근데 10년 동안 사용한 거라",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.14",
          "form": "그게 미련은 없었어요",
          "original_form": "그게 미련은 없었어요",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.15",
          "form": "그건 다행이네요",
          "original_form": "그건 다행이네요",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.16",
          "form": "항상 여름이니까",
          "original_form": "항상 여름이니까",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030728.1.17",
          "form": "마르고 닳도록 입었어요 같은 옷을",
          "original_form": "마르고 닳도록 입었어요 같은 옷을",
          "speaker_id": "1",
