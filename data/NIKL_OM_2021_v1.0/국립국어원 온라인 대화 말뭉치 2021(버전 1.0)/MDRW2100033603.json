      "utterance": [
        {
          "id": "MDRW2100033603.1.1",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100033603.1.2",
          "form": "점심때오더라구요",
          "original_form": "점심때오더라구요",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100033603.1.3",
          "form": "거긴어때요?",
          "original_form": "거긴어때요?",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100033603.1.4",
          "form": "여긴 이제 그쳤어요",
          "original_form": "여긴 이제 그쳤어요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033603.1.5",
          "form": "태풍은 지나갔다던데요",
          "original_form": "태풍은 지나갔다던데요",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033603.1.6",
          "form": "오전에 운동갔다나오는데",
          "original_form": "오전에 운동갔다나오는데",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033603.1.7",
          "form": "무섭게 오더라구요",
          "original_form": "무섭게 오더라구요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033603.1.8",
          "form": "빨리  그치고 맑은  하늘보고싶네요",
          "original_form": "빨리  그치고 맑은  하늘보고싶네요",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100033603.1.9",
          "form": "우산쓰고 차로가는데 등은 다젖었어요",
          "original_form": "우산쓰고 차로가는데 등은 다젖었어요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033603.1.10",
          "form": "태풍가고 장마온다고 하던데 맞나요?",
          "original_form": "태풍가고 장마온다고 하던데 맞나요?",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033603.1.11",
          "form": "장마가 또온대요?",
          "original_form": "장마가 또온대요?",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033603.1.12",
          "form": "오전엔 그렇게 오더니 지금은 언제 그랬냐는듯이",
          "original_form": "오전엔 그렇게 오더니 지금은 언제 그랬냐는듯이",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033603.1.13",
          "form": "맑아지네요",
          "original_form": "맑아지네요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033603.1.14",
          "form": "네 어제 티비에서 그러더라구요",
          "original_form": "네 어제 티비에서 그러더라구요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033603.1.15",
          "form": "여긴 아직 흐려요",
          "original_form": "여긴 아직 흐려요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033603.1.16",
          "form": "비와서 시원할줄알았는데",
          "original_form": "비와서 시원할줄알았는데",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033603.1.17",
          "form": "안그래요 ㅜㅠ",
          "original_form": "안그래요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033603.1.18",
          "form": "비와서 습하죠",
          "original_form": "비와서 습하죠",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033603.1.19",
          "form": "습해서 계속 에어컨켰네요",
          "original_form": "습해서 계속 에어컨켰네요",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033603.1.20",
          "form": "전 아직 덥더라구요",
          "original_form": "전 아직 덥더라구요",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033603.1.21",
          "form": "집에서 에어컨 계속 켜고있네요",
          "original_form": "집에서 에어컨 계속 켜고있네요",
          "speaker_id": "1",
          "time": "20210824 16:55"
        }
