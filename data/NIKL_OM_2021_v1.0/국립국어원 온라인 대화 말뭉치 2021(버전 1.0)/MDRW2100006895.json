      "utterance": [
        {
          "id": "MDRW2100006895.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006895.1.2",
          "form": "안녕하세용?",
          "original_form": "안녕하세용?",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006895.1.3",
          "form": "어떤 음식을 좋아하시나요?",
          "original_form": "어떤 음식을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006895.1.4",
          "form": "저는 떡볶이 좋아합니다. 그리고 매운 음식들 전반적으로 좋아해요ㅎㅎ",
          "original_form": "저는 떡볶이 좋아합니다. 그리고 매운 음식들 전반적으로 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006895.1.5",
          "form": "와아 저도 매운음식 좋아해요",
          "original_form": "와아 저도 매운음식 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006895.1.6",
          "form": "무슨 매운음식 좋아하세요?",
          "original_form": "무슨 매운음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006895.1.7",
          "form": "떡볶이는 개인적으로 엽떡을 조아해요",
          "original_form": "떡볶이는 개인적으로 엽떡을 조아해요",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006895.1.8",
          "form": "불닭에 치즈 넣어먹는걸 제일 좋아해요",
          "original_form": "불닭에 치즈 넣어먹는걸 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006895.1.9",
          "form": "오호 불닭 정말 맛있죠",
          "original_form": "오호 불닭 정말 맛있죠",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006895.1.10",
          "form": "저는 소스도 사놨어요ㅋㅋㅋ",
          "original_form": "저는 소스도 사놨어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.11",
          "form": "떡볶이 저도 엽떡 좋아합니다!!",
          "original_form": "떡볶이 저도 엽떡 좋아합니다!!",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.12",
          "form": "우와 불닭소스 맛있나요?",
          "original_form": "우와 불닭소스 맛있나요?",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.13",
          "form": "로제는 배떡도 좋아해요",
          "original_form": "로제는 배떡도 좋아해요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.14",
          "form": "여러음식에 넣어먹기좋나요?",
          "original_form": "여러음식에 넣어먹기좋나요?",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.15",
          "form": "배떡 로제 아직못먹봐서 너무 궁금하네요",
          "original_form": "배떡 로제 아직못먹봐서 너무 궁금하네요",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.16",
          "form": "넵 밥 볶아 먹을 때 한두스푼씩 넣으면 맛있어져요",
          "original_form": "넵 밥 볶아 먹을 때 한두스푼씩 넣으면 맛있어져요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006895.1.17",
          "form": "생각보다 굴소스처럼 여러군데 활용하게 돼요",
          "original_form": "생각보다 굴소스처럼 여러군데 활용하게 돼요",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006895.1.18",
          "form": "배떡 꼭 드셔보세요 맛나요",
          "original_form": "배떡 꼭 드셔보세요 맛나요",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006895.1.19",
          "form": "저도 소스 구매의향이 생기네요",
          "original_form": "저도 소스 구매의향이 생기네요",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006895.1.20",
          "form": "넵 꼭 먹어볼게요!! 너무 맛있을거 같아요",
          "original_form": "넵 꼭 먹어볼게요!! 너무 맛있을거 같아요",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006895.1.21",
          "form": "또 다른 좋아하는 음식 있으신가요?  저는 마라탕 좋아합니다.",
          "original_form": "또 다른 좋아하는 음식 있으신가요?  저는 마라탕 좋아합니다.",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006895.1.22",
          "form": "와 저두 마라탕 좋아해요",
          "original_form": "와 저두 마라탕 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100006895.1.23",
          "form": "한달전쯤 친구랑 같이 얌이마라탕을 먹었는데",
          "original_form": "한달전쯤 친구랑 같이 얌이마라탕을 먹었는데",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100006895.1.24",
          "form": "너무 맛있더라구요",
          "original_form": "너무 맛있더라구요",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100006895.1.25",
          "form": "배가불러서 밥 못 말아 먹고 나온게 아쉬웠어요",
          "original_form": "배가불러서 밥 못 말아 먹고 나온게 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100006895.1.26",
          "form": "얌이마라탕 기억해둘게요ㅎㅎ",
          "original_form": "얌이마라탕 기억해둘게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100006895.1.27",
          "form": "저는 라화쿵부 좋았어요",
          "original_form": "저는 라화쿵부 좋았어요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100006895.1.28",
          "form": "약간 사골맛 나면서(기분탓인가ㅎㅎ;;) 한국인 입맛에 맞고 맛있었던 것 같아요",
          "original_form": "약간 사골맛 나면서(기분탓인가ㅎㅎ;;) 한국인 입맛에 맞고 맛있었던 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100006895.1.29",
          "form": "사실 마라탕이",
          "original_form": "사실 마라탕이",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100006895.1.30",
          "form": "밥 말아먹는 음식이 아니라는데 밥 말아먹으면 왤케 맛있는지.. 사실 국밥 아닌지..",
          "original_form": "밥 말아먹는 음식이 아니라는데 밥 말아먹으면 왤케 맛있는지.. 사실 국밥 아닌지..",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100006895.1.31",
          "form": "와 정말 인정해요",
          "original_form": "와 정말 인정해요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100006895.1.32",
          "form": "중국특유의 향신료가 이게뭐지 싶다가도 구수하고 깊은맛이 나는게",
          "original_form": "중국특유의 향신료가 이게뭐지 싶다가도 구수하고 깊은맛이 나는게",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100006895.1.33",
          "form": "너무맛나요 라화쿵부도 먹고싶네요",
          "original_form": "너무맛나요 라화쿵부도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        }
      ]
