        },
        {
          "id": "MDRW2100039140.1.3",
          "form": "name1씨가 원하는 건",
          "original_form": "&name1&씨가 원하는 건",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100039140.1.4",
          "form": "전 공학같죠???",
          "original_form": "전 공학같죠???",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100039140.1.5",
          "form": "공학에서 하는 것처럼",
          "original_form": "공학에서 하는 것처럼",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100039140.1.6",
          "form": "실용적인거",
