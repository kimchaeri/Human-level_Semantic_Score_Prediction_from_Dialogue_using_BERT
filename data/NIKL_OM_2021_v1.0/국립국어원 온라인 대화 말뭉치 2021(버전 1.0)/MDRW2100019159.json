      "utterance": [
        {
          "id": "MDRW2100019159.1.1",
          "form": "요즘 유튭이나 티비는",
          "original_form": "요즘 유튭이나 티비는",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019159.1.2",
          "form": "잘안보니?",
          "original_form": "잘안보니?",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019159.1.3",
          "form": "티비는 아예 안보고",
          "original_form": "티비는 아예 안보고",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019159.1.4",
          "form": "유튭은 노래듣기용 & 홈트용으로만 봐",
          "original_form": "유튭은 노래듣기용 & 홈트용으로만 봐",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019159.1.5",
          "form": "티비는 진짜 언젠가부터 엄청 안보게되더라",
          "original_form": "티비는 진짜 언젠가부터 엄청 안보게되더라",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019159.1.6",
          "form": "홈트는 무슨홈트하니?",
          "original_form": "홈트는 무슨홈트하니?",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100019159.1.7",
          "form": "비타민 신지니라는 채널거 보고",
          "original_form": "비타민 신지니라는 채널거 보고",
          "speaker_id": "2",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019159.1.8",
          "form": "20분 유산소 20분 배근력 운동 해",
          "original_form": "20분 유산소 20분 배근력 운동 해",
          "speaker_id": "2",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019159.1.9",
          "form": "와 유산소랑 근력이라니",
          "original_form": "와 유산소랑 근력이라니",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019159.1.10",
          "form": "대단한걸",
          "original_form": "대단한걸",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019159.1.11",
          "form": "시간이 얼마 안돼서 ㅋㅋㅋ 체력 좀 늘면 시간도 늘려보려고",
          "original_form": "시간이 얼마 안돼서 ㅋㅋㅋ 체력 좀 늘면 시간도 늘려보려고",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.12",
          "form": "너는 유튭 보는거 있어?",
          "original_form": "너는 유튭 보는거 있어?",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.13",
          "form": "음악은 유튜브프리미엄쓰는거야?",
          "original_form": "음악은 유튜브프리미엄쓰는거야?",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.14",
          "form": "ㄴㄴ 걍 광고 나오는대로 써 ㅋㅋㅋ",
          "original_form": "ㄴㄴ 걍 광고 나오는대로 써 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.15",
          "form": "아 유튜브뮤직을 쓰는게아니구나",
          "original_form": "아 유튜브뮤직을 쓰는게아니구나",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.16",
          "form": "ㅇㅇ 그냥 영상 틀어놓고 듣는거",
          "original_form": "ㅇㅇ 그냥 영상 틀어놓고 듣는거",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.17",
          "form": "것두 잘 안해서",
          "original_form": "것두 잘 안해서",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019159.1.19",
          "form": "아 근데 나 넷플은 정기구독중",
          "original_form": "아 근데 나 넷플은 정기구독중",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019159.1.20",
          "form": "띠요옹",
          "original_form": "띠요옹",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019159.1.21",
          "form": "ott는 또 잘보는거니",
          "original_form": "ott는 또 잘보는거니",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019159.1.22",
          "form": "그림그릴때 켜두고 봐서 ㅋㅋㅋ",
          "original_form": "그림그릴때 켜두고 봐서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019159.1.23",
          "form": "가끔 영화땡기면 한 편씩 보고",
          "original_form": "가끔 영화땡기면 한 편씩 보고",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019159.1.24",
          "form": "오 호화로워",
          "original_form": "오 호화로워",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019159.1.25",
          "form": "난 ott 끊어놓으면",
          "original_form": "난 ott 끊어놓으면",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.26",
          "form": "달에 3750원인가 써서",
          "original_form": "달에 3750원인가 써서",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.27",
          "form": "소개만읽다가 몇시간날려서",
          "original_form": "소개만읽다가 몇시간날려서",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.28",
          "form": "부담없어",
          "original_form": "부담없어",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.29",
          "form": "돈이아깝던데",
          "original_form": "돈이아깝던데",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.30",
          "form": "어억 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어억 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.31",
          "form": "ㅋㅋㅋ활자중독이또",
          "original_form": "ㅋㅋㅋ활자중독이또",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.32",
          "form": "글만보면 다읽고있음",
          "original_form": "글만보면 다읽고있음",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100019159.1.33",
          "form": "그럼 뭐 영상은 딱히 보는게 없겠구만",
          "original_form": "그럼 뭐 영상은 딱히 보는게 없겠구만",
          "speaker_id": "2",
          "time": "20210827 15:54"
        }
      ]
