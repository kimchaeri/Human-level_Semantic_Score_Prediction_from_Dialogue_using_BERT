      "utterance": [
        {
          "id": "MDRW2100033455.1.1",
          "form": "혹시 애로 부부 이런것도 보시나요?",
          "original_form": "혹시 애로 부부 이런것도 보시나요?",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033455.1.2",
          "form": "아뇨 ㅋㅌ",
          "original_form": "아뇨 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033455.1.3",
          "form": "한번도 못봤러요",
          "original_form": "한번도 못봤러요",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100033455.1.4",
          "form": "근데 맨날 채널 돌리면 나와요",
          "original_form": "근데 맨날 채널 돌리면 나와요",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033455.1.5",
          "form": "그거 사랑과 전쟁 같은건가요?",
          "original_form": "그거 사랑과 전쟁 같은건가요?",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033455.1.6",
          "form": "ㅋㅋ사랑과전쟁 다봤는데",
          "original_form": "ㅋㅋ사랑과전쟁 다봤는데",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033455.1.7",
          "form": "네 비슷해요.. ㅋㅋㅋ",
          "original_form": "네 비슷해요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033455.1.8",
          "form": "진짜 상상도 할수 없는 이야기들이 나와요 ㅎㅎ",
          "original_form": "진짜 상상도 할수 없는 이야기들이 나와요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033455.1.9",
          "form": "그런거 같더라구요 ㅋㅋ",
          "original_form": "그런거 같더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033455.1.10",
          "form": "사랑과 전쟁도 예전에 본방사수였는데\r\n유튜브에도 많더라구요 ㅎㅎㅎ\r\n다시 다 보고 그랬네요 ㅎㅎ",
          "original_form": "사랑과 전쟁도 예전에 본방사수였는데\r\n유튜브에도 많더라구요 ㅎㅎㅎ\r\n다시 다 보고 그랬네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033455.1.11",
          "form": "한번 봐야겠네요",
          "original_form": "한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033455.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033455.1.13",
          "form": "네 재밌어요",
          "original_form": "네 재밌어요",
          "speaker_id": "1",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033455.1.14",
          "form": "막장 진짜 잼나요 법원에서 판결도 실감나고 ㅋㅋ",
          "original_form": "막장 진짜 잼나요 법원에서 판결도 실감나고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033455.1.15",
          "form": "그죠 ㅋㅋ\r\n사랑과 전쟁 최근거보다 제일처음 나온것들이 더 재밌더라구요",
          "original_form": "그죠 ㅋㅋ\r\n사랑과 전쟁 최근거보다 제일처음 나온것들이 더 재밌더라구요",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100033455.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100033455.1.17",
          "form": "항상 점점더 갈수록 영 아니예요",
          "original_form": "항상 점점더 갈수록 영 아니예요",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100033455.1.18",
          "form": "그래서 1편보다 나은 2편 없다 하나봐요",
          "original_form": "그래서 1편보다 나은 2편 없다 하나봐요",
          "speaker_id": "2",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100033455.1.19",
          "form": "네네 ! 그걸 팬트하우스 보면서도 느겼어요",
          "original_form": "네네 ! 그걸 팬트하우스 보면서도 느겼어요",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100033455.1.20",
          "form": "느꼈어요!",
          "original_form": "느꼈어요!",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100033455.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100033455.1.22",
          "form": "이게 요번주 ㅋㅋ완전 멘붕이였어요",
          "original_form": "이게 요번주 ㅋㅋ완전 멘붕이였어요",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033455.1.23",
          "form": "이번꺼는 ㅋㅋㅋㅋ 정말.. 정말 말도 안되게 내용이",
          "original_form": "이번꺼는 ㅋㅋㅋㅋ 정말.. 정말 말도 안되게 내용이",
          "speaker_id": "1",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033455.1.24",
          "form": "그렇죠 진짜 마지막 엔딩도",
          "original_form": "그렇죠 진짜 마지막 엔딩도",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033455.1.25",
          "form": "그게 뭐래요",
          "original_form": "그게 뭐래요",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033455.1.26",
          "form": "참나 배위로",
          "original_form": "참나 배위로",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033455.1.27",
          "form": "갑자기것도",
          "original_form": "갑자기것도",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100033455.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ급 생각났네요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ급 생각났네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100033455.1.29",
          "form": "코미디인줄...",
          "original_form": "코미디인줄...",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100033455.1.30",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100033455.1.31",
          "form": "다들 그렇군요",
          "original_form": "다들 그렇군요",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100033455.1.32",
          "form": "저도요 어이가 없어서 생각나요",
          "original_form": "저도요 어이가 없어서 생각나요",
          "speaker_id": "2",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100033455.1.33",
          "form": "김소연이 불쌍하게 느껴졌러요",
          "original_form": "김소연이 불쌍하게 느껴졌러요",
          "speaker_id": "2",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100033455.1.34",
          "form": "그나마 팬트하우스 1.2편은 한편한편 기다려졌는데...",
          "original_form": "그나마 팬트하우스 1.2편은 한편한편 기다려졌는데...",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100033455.1.35",
          "form": "폭발에서 살아남는 것도 그렇고...",
          "original_form": "폭발에서 살아남는 것도 그렇고...",
          "speaker_id": "1",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100033455.1.36",
          "form": "ㅋㅋ헛웃음나죠",
          "original_form": "ㅋㅋ헛웃음나죠",
          "speaker_id": "2",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100033455.1.37",
          "form": "말도 안되긴해요",
          "original_form": "말도 안되긴해요",
          "speaker_id": "2",
          "time": "20210823 23:11"
        },
        {
          "id": "MDRW2100033455.1.38",
          "form": "근데 또봐요 ㅋㅋ",
          "original_form": "근데 또봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100033455.1.39",
          "form": "맞아요........ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요........ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100033455.1.40",
          "form": "그러면서 블새2020도 다 챙겨보고",
          "original_form": "그러면서 블새2020도 다 챙겨보고",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100033455.1.41",
          "form": "ㅋㅋ그건 전보다가 넘길고 패스 했어요",
          "original_form": "ㅋㅋ그건 전보다가 넘길고 패스 했어요",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100033455.1.42",
          "form": "그래도... 그건 매일 퇴근후에 바로바로 봤어요 ㅎㅎ",
          "original_form": "그래도... 그건 매일 퇴근후에 바로바로 봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100033455.1.43",
          "form": "아 그래요 그럼 안밀리고 봤겠어요",
          "original_form": "아 그래요 그럼 안밀리고 봤겠어요",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100033455.1.44",
          "form": "아침드라마나 막장인 맛에 봐야죠 ㅎㅎ",
          "original_form": "아침드라마나 막장인 맛에 봐야죠 ㅎㅎ",
          "speaker_id": "1",
