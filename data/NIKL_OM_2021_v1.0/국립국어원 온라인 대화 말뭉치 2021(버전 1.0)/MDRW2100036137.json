      "utterance": [
        {
          "id": "MDRW2100036137.1.1",
          "form": "부자되어 감빵가서 재용생활이뤄보자!",
          "original_form": "부자되어 감빵가서 재용생활이뤄보자!",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.2",
          "form": "네이버 페이 모아서",
          "original_form": "네이버 페이 모아서",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.3",
          "form": "반포자이 사는거 어디쯤 되어가요",
          "original_form": "반포자이 사는거 어디쯤 되어가요",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.4",
          "form": "아 저 470개라구요?!",
          "original_form": "아 저 470개라구요?!",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.6",
          "form": "많이햇조.",
          "original_form": "많이햇조.",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.8",
          "form": "헐 많이하셨네?!!",
          "original_form": "헐 많이하셨네?!!",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.9",
          "form": "후후...",
          "original_form": "후후...",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.10",
          "form": "반포자이 기다려라 내가간다",
          "original_form": "반포자이 기다려라 내가간다",
          "speaker_id": "2",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.11",
          "form": "누구랑 100개의 밸런스게임을 했을까",
          "original_form": "누구랑 100개의 밸런스게임을 했을까",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100036137.1.12",
          "form": "아쉽게도 밸런스없이 달려서 저 사회성에 문제있는 줄 알앗다고요",
          "original_form": "아쉽게도 밸런스없이 달려서 저 사회성에 문제있는 줄 알앗다고요",
          "speaker_id": "2",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036137.1.13",
          "form": "건강 챙기면서 하시라구요(?)",
          "original_form": "건강 챙기면서 하시라구요(?)",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036137.1.14",
          "form": "간신히...채워간....대화.",
          "original_form": "간신히...채워간....대화.",
          "speaker_id": "2",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036137.1.15",
          "form": "허억",
          "original_form": "허억",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036137.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036137.1.17",
          "form": "나랑 30개 가보자고",
          "original_form": "나랑 30개 가보자고",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100036137.1.18",
