      "utterance": [
        {
          "id": "MDRW2100028417.1.1",
          "form": "파리바게트 빵먹고싶네",
          "original_form": "파리바게트 빵먹고싶네",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028417.1.2",
          "form": "나 갓나온 빵 먹고 싶어",
          "original_form": "나 갓나온 빵 먹고 싶어",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028417.1.3",
          "form": "식빵 뜯어서 김 모락모락 나오는거",
          "original_form": "식빵 뜯어서 김 모락모락 나오는거",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028417.1.4",
          "form": "허얼맛있겠다",
          "original_form": "허얼맛있겠다",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028417.1.5",
          "form": "나도 갓 나온빵 좋아해",
          "original_form": "나도 갓 나온빵 좋아해",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028417.1.6",
          "form": "따끈하고 부드러워서 먹기좋아",
          "original_form": "따끈하고 부드러워서 먹기좋아",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028417.1.7",
          "form": "빵은 식혀먹는거라고 했는데",
          "original_form": "빵은 식혀먹는거라고 했는데",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028417.1.8",
          "form": "늘 이해하지 못했어",
          "original_form": "늘 이해하지 못했어",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028417.1.9",
          "form": "갓나온게 맛있단 말이야",
          "original_form": "갓나온게 맛있단 말이야",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028417.1.10",
          "form": "몰라몰라 갓나온게 맛있다고",
          "original_form": "몰라몰라 갓나온게 맛있다고",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028417.1.11",
          "form": "당신네들이 빵맛을 알어??",
          "original_form": "당신네들이 빵맛을 알어??",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028417.1.12",
          "form": "그럼 빵을 왜 구워!",
          "original_form": "그럼 빵을 왜 구워!",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028417.1.13",
          "form": "차가운거 좋아하면 얼려서 먹지!!",
          "original_form": "차가운거 좋아하면 얼려서 먹지!!",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028417.1.14",
          "form": "정말웃겨",
          "original_form": "정말웃겨",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028417.1.15",
          "form": "얼려먹는빵!",
          "original_form": "얼려먹는빵!",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028417.1.16",
          "form": "얼빵!",
          "original_form": "얼빵!",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028417.1.17",
          "form": "뭔가 귀여운 말인걸",
          "original_form": "뭔가 귀여운 말인걸",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028417.1.18",
          "form": "얼려먹는 초코도 있는데",
          "original_form": "얼려먹는 초코도 있는데",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028417.1.19",
          "form": "얼려먹는 빵은 왜 없지",
          "original_form": "얼려먹는 빵은 왜 없지",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028417.1.20",
          "form": "빵은 얼리면 너무 딱딱하니까 아무래도",
          "original_form": "빵은 얼리면 너무 딱딱하니까 아무래도",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028417.1.21",
          "form": "하지만 네가 만들면 가능.",
          "original_form": "하지만 네가 만들면 가능.",
          "speaker_id": "2",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028417.1.22",
          "form": "크루통처럼 수분기 빼서 스프에 넣으면 맛있을거같아",
          "original_form": "크루통처럼 수분기 빼서 스프에 넣으면 맛있을거같아",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028417.1.23",
          "form": "우리엄마 빵 보관도 냉동실에 해",
          "original_form": "우리엄마 빵 보관도 냉동실에 해",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028417.1.24",
          "form": "크루통은 근데 한번 버터에 볶으니까 맛있은게 아닐까 싶어",
          "original_form": "크루통은 근데 한번 버터에 볶으니까 맛있은게 아닐까 싶어",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028417.1.25",
          "form": "볶으면 되는거지!",
          "original_form": "볶으면 되는거지!",
          "speaker_id": "1",
          "time": "20210902 21:44"
        }
      ]
    }
  ]
