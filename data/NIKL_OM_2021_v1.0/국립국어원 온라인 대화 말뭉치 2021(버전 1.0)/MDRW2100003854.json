      "utterance": [
        {
          "id": "MDRW2100003854.1.1",
          "form": "유투브 자주보세요?",
          "original_form": "유투브 자주보세요?",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100003854.1.2",
          "form": "유튜브 잘안보게 되더라구요 ㅎ",
          "original_form": "유튜브 잘안보게 되더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100003854.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100003854.1.4",
          "form": "저는 공모주땜에 한번씩봐요",
          "original_form": "저는 공모주땜에 한번씩봐요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100003854.1.5",
          "form": "그래서 프리미엄 등록했어요 ㅎ",
          "original_form": "그래서 프리미엄 등록했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100003854.1.6",
          "form": "아 주식 공부하시느라 ㅎㅎ",
          "original_form": "아 주식 공부하시느라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100003854.1.7",
          "form": "그건얼마예요? 유료 채널도있어요?",
          "original_form": "그건얼마예요? 유료 채널도있어요?",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100003854.1.8",
          "form": "그냥 광고봐도되는데 프리미엄 싸게할수있다고해서 ㅎ",
          "original_form": "그냥 광고봐도되는데 프리미엄 싸게할수있다고해서 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100003854.1.9",
          "form": "아뇨 똑같은데 광고가 안나와요",
          "original_form": "아뇨 똑같은데 광고가 안나와요",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100003854.1.10",
          "form": "아 광고없이 편하게 보는거군요",
          "original_form": "아 광고없이 편하게 보는거군요",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100003854.1.11",
          "form": "신랑이 인도계정으로하면 싸다고 하는방법을 알아와서 가족계정으로 묶었어요",
          "original_form": "신랑이 인도계정으로하면 싸다고 하는방법을 알아와서 가족계정으로 묶었어요",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100003854.1.12",
          "form": "전 펭수 핫할때 유튜브로 찾아보긴했는데 ㅎㅎ",
          "original_form": "전 펭수 핫할때 유튜브로 찾아보긴했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100003854.1.13",
          "form": "참 사람들 대단한거같아요",
          "original_form": "참 사람들 대단한거같아요",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100003854.1.14",
          "form": "펭수 귀엽죠 ㅎㅎ",
          "original_form": "펭수 귀엽죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100003854.1.15",
          "form": "요즘도 핫한가요?",
          "original_form": "요즘도 핫한가요?",
          "speaker_id": "2",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100003854.1.16",
          "form": "요즘은 인기 시들해진듯ㅎ",
          "original_form": "요즘은 인기 시들해진듯ㅎ",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100003854.1.17",
          "form": "세계로 진출하고싶어했는데 ㅋ",
          "original_form": "세계로 진출하고싶어했는데 ㅋ",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100003854.1.18",
          "form": "해외로는 못갔나요?",
          "original_form": "해외로는 못갔나요?",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100003854.1.19",
          "form": "아마도요ㅋㅋ 펭수티비는 그래도 아직하는거같아요",
          "original_form": "아마도요ㅋㅋ 펭수티비는 그래도 아직하는거같아요",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100003854.1.20",
          "form": "이비에스에 아직 한번씩나오던데요",
          "original_form": "이비에스에 아직 한번씩나오던데요",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100003854.1.21",
          "form": "네 아직 교육방송에 나오더라구요ㅎㅎ 귀여워요 펭수",
          "original_form": "네 아직 교육방송에 나오더라구요ㅎㅎ 귀여워요 펭수",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100003854.1.22",
          "form": "저는 말투가 좀 거슬리던데 ㅎㅎ",
          "original_form": "저는 말투가 좀 거슬리던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100003854.1.23",
          "form": "그게 매력이예요",
          "original_form": "그게 매력이예요",
          "speaker_id": "1",
          "time": "20210821 16:11"
        }
      ]
