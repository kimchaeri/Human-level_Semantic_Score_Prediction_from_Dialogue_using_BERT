      "utterance": [
        {
          "id": "MDRW2100014616.1.1",
          "form": "금방차겟네여ㅎㅎㅎ",
          "original_form": "금방차겟네여ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014616.1.2",
          "form": "비오는날 생각나는영화는?",
          "original_form": "비오는날 생각나는영화는?",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014616.1.3",
          "form": "전 일본영화인데 생각이안나네요ㅠ",
          "original_form": "전 일본영화인데 생각이안나네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014616.1.4",
          "form": "저는 너의 이름은",
          "original_form": "저는 너의 이름은",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.5",
          "form": "제가 좀 이런거는 약해요ㅠ 찾아보면 기억날텐데...",
          "original_form": "제가 좀 이런거는 약해요ㅠ 찾아보면 기억날텐데...",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.6",
          "form": "너의 이름은 좋죠ㅎㅎㄹ",
          "original_form": "너의 이름은 좋죠ㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.7",
          "form": "애니말하는거죠?",
          "original_form": "애니말하는거죠?",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.8",
          "form": "네넹 ㅎㅎㅎ",
          "original_form": "네넹 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.9",
          "form": "아 저 참 애니 좋아하는거 같아요 ㅋㅋ",
          "original_form": "아 저 참 애니 좋아하는거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.10",
          "form": "좋죠ㅎㅎ",
          "original_form": "좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.11",
          "form": "남자 여자 애가 몸이바뀐내용이었죠?",
          "original_form": "남자 여자 애가 몸이바뀐내용이었죠?",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014616.1.12",
          "form": "정확히 기억은 안나네요ㅎㅎ",
          "original_form": "정확히 기억은 안나네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014616.1.13",
          "form": "음 뭐지 뭐지",
          "original_form": "음 뭐지 뭐지",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014616.1.14",
          "form": "검색 해봐야지~",
          "original_form": "검색 해봐야지~",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014616.1.15",
          "form": "아ㅎㅎ 저 일본영화 생각났어요ㅎㅎ",
          "original_form": "아ㅎㅎ 저 일본영화 생각났어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014616.1.16",
          "form": "지금 만나러 갑니다ㅎ",
          "original_form": "지금 만나러 갑니다ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014616.1.17",
          "form": "아항 !!!",
          "original_form": "아항 !!!",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014616.1.18",
          "form": "비가오는날만 엄마가 찾아오잖아요ㅎㅎ",
          "original_form": "비가오는날만 엄마가 찾아오잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014616.1.19",
          "form": "너무 좋았는데ㅎㅎ",
          "original_form": "너무 좋았는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014616.1.20",
          "form": "전 본거 같긴한데 자세히 기억이 안나네요",
          "original_form": "전 본거 같긴한데 자세히 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014616.1.21",
          "form": "주말에 한번 찾아서 봐야 겠어요 ㅎㄹ",
          "original_form": "주말에 한번 찾아서 봐야 겠어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014616.1.22",
          "form": "ㅎㅎ  병으로 아내가 죽었는데...",
          "original_form": "ㅎㅎ  병으로 아내가 죽었는데...",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014616.1.23",
          "form": "아이가 한명있었어요ㅎ",
          "original_form": "아이가 한명있었어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014616.1.24",
          "form": "엄마가 죽으면서 다음 생일날 다시 온다고 했었나?ㅎㅎ",
          "original_form": "엄마가 죽으면서 다음 생일날 다시 온다고 했었나?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014616.1.25",
          "form": "저도 기억이 잘안나네요ㅎㅎ 제 인생영화중 하나에요ㅎㅎ",
          "original_form": "저도 기억이 잘안나네요ㅎㅎ 제 인생영화중 하나에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014616.1.26",
          "form": "오홍 꼭 봐볼게요 ㅎㅎ",
          "original_form": "오홍 꼭 봐볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014616.1.27",
          "form": "ㅎㅎ 한국꺼도 나왔어요ㅎ 손예진이랑 소간지랑ㅎㅎ",
          "original_form": "ㅎㅎ 한국꺼도 나왔어요ㅎ 손예진이랑 소간지랑ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014616.1.28",
          "form": "근데 원작은 못따라 오더라구요ㅎㅎ",
          "original_form": "근데 원작은 못따라 오더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014616.1.29",
          "form": "아하!! 두개 보고 비교하는것도 재밋겠네요 ㅎㅎ",
          "original_form": "아하!! 두개 보고 비교하는것도 재밋겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:08"
        }
      ]
