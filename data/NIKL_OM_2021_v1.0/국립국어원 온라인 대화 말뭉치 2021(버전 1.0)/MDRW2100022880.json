          "original_form": "안좋다고생각하면돼",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.18",
          "form": "생리용품도",
          "original_form": "생리용품도",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.19",
          "form": "비싸고화학약품많고",
          "original_form": "비싸고화학약품많고",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.20",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.21",
          "form": "그거 진심 에바임",
          "original_form": "그거 진심 에바임",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.22",
          "form": "그거나온거봤음?",
          "original_form": "그거나온거봤음?",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.23",
          "form": "면세 용품인데도 그 가걱인 것도 에바고",
          "original_form": "면세 용품인데도 그 가걱인 것도 에바고",
          "speaker_id": "2",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.24",
          "form": "여자레깅스?",
          "original_form": "여자레깅스?",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.25",
