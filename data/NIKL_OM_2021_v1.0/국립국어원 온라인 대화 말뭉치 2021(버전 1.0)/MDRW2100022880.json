      "utterance": [
        {
          "id": "MDRW2100022880.1.1",
          "form": "아 그래? 다꾸 비쌌음?",
          "original_form": "아 그래? 다꾸 비쌌음?",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022880.1.2",
          "form": "안해서 몰랐음",
          "original_form": "안해서 몰랐음",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022880.1.3",
          "form": "스티커가격",
          "original_form": "스티커가격",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022880.1.4",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022880.1.5",
          "form": "재료비?",
          "original_form": "재료비?",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022880.1.6",
          "form": "원가?",
          "original_form": "원가?",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.7",
          "form": "그런거에비해비싼편이잖아",
          "original_form": "그런거에비해비싼편이잖아",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.8",
          "form": "점점비싸기도하고",
          "original_form": "점점비싸기도하고",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.9",
          "form": "비싸지기도",
          "original_form": "비싸지기도",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.10",
          "form": "어휴 진짜",
          "original_form": "어휴 진짜",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.11",
          "form": "나 그래서",
          "original_form": "나 그래서",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.12",
          "form": "스포츠 용품 살때도 남자 부분 먼저 보잖아",
          "original_form": "스포츠 용품 살때도 남자 부분 먼저 보잖아",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.13",
          "form": "진짜로",
          "original_form": "진짜로",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.14",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022880.1.15",
          "form": "20대여성대상으로나온",
          "original_form": "20대여성대상으로나온",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.16",
          "form": "상품은",
          "original_form": "상품은",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100022880.1.17",
