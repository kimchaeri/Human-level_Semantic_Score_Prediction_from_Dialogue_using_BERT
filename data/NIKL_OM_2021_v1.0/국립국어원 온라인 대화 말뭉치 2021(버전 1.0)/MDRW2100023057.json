        },
        {
          "id": "MDRW2100023057.1.2",
          "form": "과일인가요?",
          "original_form": "과일인가요?",
          "speaker_id": "2",
          "time": "20210829 21:13"
        },
        {
          "id": "MDRW2100023057.1.3",
          "form": "꼭 하나씩만 말하기!",
          "original_form": "꼭 하나씩만 말하기!",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023057.1.4",
          "form": "과일 아닙니다",
          "original_form": "과일 아닙니다",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023057.1.5",
          "form": "빵 종류인가요?",
          "original_form": "빵 종류인가요?",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100023057.1.6",
          "form": "빵! 아닙니다",
          "original_form": "빵! 아닙니다",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023057.1.7",
          "form": "조오금 어려우실 것입니다~",
          "original_form": "조오금 어려우실 것입니다~",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023057.1.8",
          "form": "과자인가요? 바삭바삭",
          "original_form": "과자인가요? 바삭바삭",
          "speaker_id": "2",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023057.1.9",
          "form": "앗 바삭바삭 맞습니다",
          "original_form": "앗 바삭바삭 맞습니다",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100023057.1.10",
          "form": "무슨 색깔인가요?",
          "original_form": "무슨 색깔인가요?",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023057.1.11",
          "form": "노란색? 이라고 할 수 있겠습니다",
          "original_form": "노란색? 이라고 할 수 있겠습니다",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023057.1.12",
          "form": "엄마손파이!",
          "original_form": "엄마손파이!",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100023057.1.13",
          "form": "아 아닙니다 그것보다 더 바삭합니다",
          "original_form": "아 아닙니다 그것보다 더 바삭합니다",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023057.1.14",
          "form": "ㅋㅋㅋㅋㅋ먹느라 좀 늦었어요...^_^",
          "original_form": "ㅋㅋㅋㅋㅋ먹느라 좀 늦었어요...^_^",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023057.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ맛난 간식타임을 가지셨군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맛난 간식타임을 가지셨군요",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023057.1.16",
          "form": "아직도 먹고 있어요ㅋㅋㅋㅋ",
          "original_form": "아직도 먹고 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023057.1.17",
          "form": "으아 맛나겠다\n포카칩이나 새우깡같은 봉지과자인가요?",
          "original_form": "으아 맛나겠다\n포카칩이나 새우깡같은 봉지과자인가요?",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023057.1.18",
          "form": "앗 네 비슷합니다 봉지에 들어있어요",
          "original_form": "앗 네 비슷합니다 봉지에 들어있어요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
