        },
        {
          "id": "MDRW2100021763.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021763.1.3",
          "form": "좋아하는 스포츠 잇나여?",
          "original_form": "좋아하는 스포츠 잇나여?",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021763.1.4",
          "form": "전 특별히 즐기는 스포츠는 없어요",
          "original_form": "전 특별히 즐기는 스포츠는 없어요",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021763.1.5",
          "form": "좋아하는거 있으세요?",
          "original_form": "좋아하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021763.1.6",
          "form": "전 배구 야구 좋아해여 ㅎ",
          "original_form": "전 배구 야구 좋아해여 ㅎ",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021763.1.7",
          "form": "직관도 가세요?",
          "original_form": "직관도 가세요?",
          "speaker_id": "2",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021763.1.8",
          "form": "예전엔요 ㅎㅎ",
          "original_form": "예전엔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021763.1.9",
          "form": "지금은 뭐 코로나",
          "original_form": "지금은 뭐 코로나",
          "speaker_id": "1",
          "time": "20210828 22:26"
        },
        {
          "id": "MDRW2100021763.1.10",
          "form": "ㅜㅠㅋㅋ야구는 한번 가봤어요",
          "original_form": "ㅜㅠㅋㅋ야구는 한번 가봤어요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021763.1.11",
          "form": "ㅎㅎ 재밌엇어요 ? 아님  지루하셨어요 ?",
          "original_form": "ㅎㅎ 재밌엇어요 ? 아님  지루하셨어요 ?",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021763.1.12",
          "form": "하필 그때 추모해야하는 날이라 응원도 노래도 없었어요ㅋㅋ 치맥만 하고ㅋㅋ",
          "original_form": "하필 그때 추모해야하는 날이라 응원도 노래도 없었어요ㅋㅋ 치맥만 하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021763.1.13",
          "form": "읔 그럼 지루쪽이셨겠어여",
          "original_form": "읔 그럼 지루쪽이셨겠어여",
          "speaker_id": "1",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021763.1.14",
          "form": "ㅋㅋ그리고 요즘 하루가 멀다하고 사건들이 많아서ㅜㅠ",
          "original_form": "ㅋㅋ그리고 요즘 하루가 멀다하고 사건들이 많아서ㅜㅠ",
          "speaker_id": "2",
