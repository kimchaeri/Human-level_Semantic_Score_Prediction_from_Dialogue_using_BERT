      "utterance": [
        {
          "id": "MDRW2100015715.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015715.1.2",
          "form": "단밤 좋아하시나봐요",
          "original_form": "단밤 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015715.1.3",
          "form": "아뇨 ㅋ",
          "original_form": "아뇨 ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100015715.1.4",
          "form": "이태원클라스 가게이름요 ㅋ",
          "original_form": "이태원클라스 가게이름요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100015715.1.5",
          "form": "아 그렇군요 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100015715.1.6",
          "form": "슬슬 또 가을이 오니까 밤을 곧 먹을 수 있을 것 같네용",
          "original_form": "슬슬 또 가을이 오니까 밤을 곧 먹을 수 있을 것 같네용",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100015715.1.7",
          "form": "밤은 그래도 별미기인해요 ㅎㅎ",
          "original_form": "밤은 그래도 별미기인해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100015715.1.8",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100015715.1.9",
          "form": "파먹기가 귀찮을 뿐이죠 ㅎㅎ",
          "original_form": "파먹기가 귀찮을 뿐이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100015715.1.10",
          "form": "티수푼 필수 ㅋ",
          "original_form": "티수푼 필수 ㅋ",
          "speaker_id": "2",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100015715.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ공감이네용",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ공감이네용",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100015715.1.12",
          "form": "가을은 먹고싶은게 넘많아서 ㅠㅠ",
          "original_form": "가을은 먹고싶은게 넘많아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100015715.1.13",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100015715.1.14",
          "form": "살이 많이 찌네요",
          "original_form": "살이 많이 찌네요",
          "speaker_id": "1",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100015715.1.15",
          "form": "다이어트 시작 해야 겟어여ㅠ슬픈",
          "original_form": "다이어트 시작 해야 겟어여ㅠ슬픈",
          "speaker_id": "2",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100015715.1.16",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "1",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100015715.1.17",
          "form": "그래도..맛있으니까요~",
          "original_form": "그래도..맛있으니까요~",
          "speaker_id": "1",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100015715.1.18",
          "form": "먹으면서 ㅋㅋ",
          "original_form": "먹으면서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:50"
        },
        {
          "id": "MDRW2100015715.1.19",
          "form": "운동과 함께~",
          "original_form": "운동과 함께~",
          "speaker_id": "1",
          "time": "20210901 00:51"
        },
        {
          "id": "MDRW2100015715.1.20",
          "form": "건강한 돼지가 되려나요...ㅠㅎㅎ",
          "original_form": "건강한 돼지가 되려나요...ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 00:51"
        },
        {
          "id": "MDRW2100015715.1.21",
