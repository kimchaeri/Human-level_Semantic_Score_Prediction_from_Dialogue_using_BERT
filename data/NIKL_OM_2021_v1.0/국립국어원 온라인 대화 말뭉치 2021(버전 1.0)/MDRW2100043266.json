      "utterance": [
        {
          "id": "MDRW2100043266.1.1",
          "form": "비 계속와",
          "original_form": "비 계속와",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.2",
          "form": "여기도 날씨그래 ㅋㅋㅋ",
          "original_form": "여기도 날씨그래 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.3",
          "form": "주말에바짝좋더니 왜이래",
          "original_form": "주말에바짝좋더니 왜이래",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.4",
          "form": "부슬부슬와",
          "original_form": "부슬부슬와",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.5",
          "form": "흠냐 마트가야하는데 왜오냐",
          "original_form": "흠냐 마트가야하는데 왜오냐",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.6",
          "form": "목요일부터는 좋아진대",
          "original_form": "목요일부터는 좋아진대",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.7",
          "form": "다행이다..근데 내일도 구리다는애기?",
          "original_form": "다행이다..근데 내일도 구리다는애기?",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043266.1.8",
          "form": "응 내일도 비온대",
          "original_form": "응 내일도 비온대",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.9",
          "form": "너네 밭은 어때? 가뭄은 아니지??",
          "original_form": "너네 밭은 어때? 가뭄은 아니지??",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.10",
          "form": "금액이 너무 저렴ㅎ래",
          "original_form": "금액이 너무 저렴ㅎ래",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.11",
          "form": "왜 사람들이 가뭄애기하더라고...어리둥절했어 비마니온거같은데",
          "original_form": "왜 사람들이 가뭄애기하더라고...어리둥절했어 비마니온거같은데",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.12",
          "form": "어떤 금액?",
          "original_form": "어떤 금액?",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.13",
          "form": "농작물?",
          "original_form": "농작물?",
          "speaker_id": "2",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.14",
          "form": "아 그 전에는 안왔어 장마치고는",
          "original_form": "아 그 전에는 안왔어 장마치고는",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043266.1.15",
          "form": "요즘은 비가 너무와서 농작물이 안익어 추워져서",
          "original_form": "요즘은 비가 너무와서 농작물이 안익어 추워져서",
          "speaker_id": "1",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100043266.1.16",
          "form": "헐 큰일이네",
          "original_form": "헐 큰일이네",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100043266.1.17",
          "form": "비가덜와야겟따",
          "original_form": "비가덜와야겟따",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100043266.1.18",
          "form": "맞아 비가 와도 너무와서 지겨워",
          "original_form": "맞아 비가 와도 너무와서 지겨워",
          "speaker_id": "1",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100043266.1.19",
          "form": "오늘은 기분도 우울하다",
          "original_form": "오늘은 기분도 우울하다",
          "speaker_id": "1",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100043266.1.20",
          "form": "...그럴땐 고기앞인데",
          "original_form": "...그럴땐 고기앞인데",
          "speaker_id": "2",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100043266.1.21",
          "form": "이제 가을 겨울 옷 사야돼",
          "original_form": "이제 가을 겨울 옷 사야돼",
          "speaker_id": "2",
          "time": "20210908 11:13"
        }
      ]
