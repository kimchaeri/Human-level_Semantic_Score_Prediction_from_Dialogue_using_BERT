      "utterance": [
        {
          "id": "MDRW2100015122.1.1",
          "form": "안녕하세요\r\n저녁 드셨어요?",
          "original_form": "안녕하세요\r\n저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100015122.1.2",
          "form": "아뇨 오늘 저녁은 밀크티로",
          "original_form": "아뇨 오늘 저녁은 밀크티로",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100015122.1.3",
          "form": "한잔 마시니까 배부르네요",
          "original_form": "한잔 마시니까 배부르네요",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100015122.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100015122.1.5",
          "form": "어머나ㅎ\r\n전 배부른데도 저녁이라 순대국밥 먹으려 하고 있어요",
          "original_form": "어머나ㅎ\r\n전 배부른데도 저녁이라 순대국밥 먹으려 하고 있어요",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100015122.1.6",
          "form": "ㅋㅋ 순대국밥!",
          "original_form": "ㅋㅋ 순대국밥!",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100015122.1.7",
          "form": "날씨랑 딱이네요",
          "original_form": "날씨랑 딱이네요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100015122.1.8",
          "form": "날씨 화창했는데 갑자기 비와서ㅎ뜨끈하니 좋을거같아요",
          "original_form": "날씨 화창했는데 갑자기 비와서ㅎ뜨끈하니 좋을거같아요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100015122.1.9",
          "form": "밀크티 제조해드셨어요?",
          "original_form": "밀크티 제조해드셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100015122.1.10",
          "form": "저 아는 동생이 밀크티 만들어 먹더라구요ㅎ",
          "original_form": "저 아는 동생이 밀크티 만들어 먹더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100015122.1.11",
          "form": "아뇨 페트병에 있는거",
          "original_form": "아뇨 페트병에 있는거",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100015122.1.12",
          "form": "간단하게 마시려면 홍차우려서 우유랑 연유 넣으면 되는데",
          "original_form": "간단하게 마시려면 홍차우려서 우유랑 연유 넣으면 되는데",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015122.1.13",
          "form": "ㅎㅎㅎ맛있죠ㅋ",
          "original_form": "ㅎㅎㅎ맛있죠ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015122.1.14",
          "form": "본격적인 분들은 끓여서 짜고 내리고 하시더라구요",
          "original_form": "본격적인 분들은 끓여서 짜고 내리고 하시더라구요",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015122.1.15",
          "form": "그렇게 까진 못할듯",
          "original_form": "그렇게 까진 못할듯",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015122.1.16",
          "form": "그쵸 근데 막상 만들기 번거로워서 잘 안해져요ㅋ",
          "original_form": "그쵸 근데 막상 만들기 번거로워서 잘 안해져요ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100015122.1.17",
          "form": "밀크티 좋아하시나봐요\r\n제조법은 알고 계시네요ㅎ",
          "original_form": "밀크티 좋아하시나봐요\r\n제조법은 알고 계시네요ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100015122.1.18",
          "form": "전 동생이 말할때까지 그리 준비물이 간단한지 몰랐어요ㅎ",
          "original_form": "전 동생이 말할때까지 그리 준비물이 간단한지 몰랐어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100015122.1.19",
          "form": "네 차마시는 거 좋아해서 이것저것 보니까 알게되더라구요",
          "original_form": "네 차마시는 거 좋아해서 이것저것 보니까 알게되더라구요",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100015122.1.20",
          "form": "오 그래서 저녁도 차로 한끼ㅎㅎ",
          "original_form": "오 그래서 저녁도 차로 한끼ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100015122.1.21",
          "form": "이따 배고파서 야식먹겠죠?",
          "original_form": "이따 배고파서 야식먹겠죠?",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100015122.1.22",
          "form": "전 찾아먹진않고 커피 맛 없어서 잘 안먹어서 카폐가면 쥬로 음료 먹게돼요ㅎ",
          "original_form": "전 찾아먹진않고 커피 맛 없어서 잘 안먹어서 카폐가면 쥬로 음료 먹게돼요ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100015122.1.23",
          "form": "근데 순대국밥하니까 급 뼈해장국 먹고파요",
          "original_form": "근데 순대국밥하니까 급 뼈해장국 먹고파요",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100015122.1.24",
          "form": "에이 드시지 마요\r\n참아요",
          "original_form": "에이 드시지 마요\r\n참아요",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100015122.1.25",
          "form": "이왕 참은거낼 주중에 드세요",
          "original_form": "이왕 참은거낼 주중에 드세요",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100015122.1.26",
          "form": "네! 낼 점심은 뼈해장국입니다 스포스포",
          "original_form": "네! 낼 점심은 뼈해장국입니다 스포스포",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100015122.1.27",
          "form": "참을수있길 바랄께요",
          "original_form": "참을수있길 바랄께요",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100015122.1.28",
          "form": "전 저녁 다먹었어요ㅋㅋ\r\n순대국밥이 진하게 맛나네요",
          "original_form": "전 저녁 다먹었어요ㅋㅋ\r\n순대국밥이 진하게 맛나네요",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100015122.1.29",
          "form": "음~ 맛있다니 다행이네요",
          "original_form": "음~ 맛있다니 다행이네요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        }
      ]
    }
