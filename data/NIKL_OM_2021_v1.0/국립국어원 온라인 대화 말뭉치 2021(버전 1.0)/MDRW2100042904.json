      "utterance": [
        {
          "id": "MDRW2100042904.1.1",
          "form": "점심은 어떵거드세요?",
          "original_form": "점심은 어떵거드세요?",
          "speaker_id": "1",
          "time": "20210907 11:09"
        },
        {
          "id": "MDRW2100042904.1.2",
          "form": "저는 백신맞아서",
          "original_form": "저는 백신맞아서",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042904.1.3",
          "form": "곰국먹었어요!",
          "original_form": "곰국먹었어요!",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042904.1.4",
          "form": "오늘 날씨도 그렇구 곰국 딱이네요!!",
          "original_form": "오늘 날씨도 그렇구 곰국 딱이네요!!",
          "speaker_id": "1",
          "time": "20210907 11:27"
        },
        {
          "id": "MDRW2100042904.1.5",
          "form": "ㅋㅋㅋ맙어요 비와가지고 ㅠㅠ",
          "original_form": "ㅋㅋㅋ맙어요 비와가지고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100042904.1.6",
          "form": "저는 오늘 짜파게티 먹어요 ㅠㅠ",
          "original_form": "저는 오늘 짜파게티 먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100042904.1.7",
          "form": "어제 넘 먹고싶었는데 오늘 먹으니 약간 볗로례요",
          "original_form": "어제 넘 먹고싶었는데 오늘 먹으니 약간 볗로례요",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100042904.1.8",
          "form": "맛있겠어요!!+",
          "original_form": "맛있겠어요!!+",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100042904.1.9",
          "form": "어젠 그래보욨는데..! 오늘는 약간 별로네요!",
          "original_form": "어젠 그래보욨는데..! 오늘는 약간 별로네요!",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100042904.1.10",
          "form": "원래",
          "original_form": "원래",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100042904.1.11",
          "form": "땡길때먹야해요!",
          "original_form": "땡길때먹야해요!",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100042904.1.12",
          "form": "맞아요 ㅠㅠ 넘 늦게 먹었나봐요",
          "original_form": "맞아요 ㅠㅠ 넘 늦게 먹었나봐요",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100042904.1.13",
          "form": "저는 엽떡이 땡기네요!!",
          "original_form": "저는 엽떡이 땡기네요!!",
          "speaker_id": "2",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100042904.1.14",
          "form": "얼른 땡길때 드세요!",
          "original_form": "얼른 땡길때 드세요!",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100042904.1.15",
          "form": "전항상땡겨서 갠춘해요",
          "original_form": "전항상땡겨서 갠춘해요",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100042904.1.16",
          "form": "저녁은 뮤ㅓ드셨어요",
          "original_form": "저녁은 뮤ㅓ드셨어요",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100042904.1.17",
          "form": "대패삼겹먹엇어요!!",
          "original_form": "대패삼겹먹엇어요!!",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100042904.1.18",
          "form": "헝 대패삼겹 넘 맛나겠엉ㅅ",
          "original_form": "헝 대패삼겹 넘 맛나겠엉ㅅ",
