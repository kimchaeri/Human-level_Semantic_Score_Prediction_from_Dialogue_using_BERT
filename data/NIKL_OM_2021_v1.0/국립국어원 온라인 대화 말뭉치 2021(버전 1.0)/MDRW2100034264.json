      "utterance": [
        {
          "id": "MDRW2100034264.1.1",
          "form": "무한도전 옛날꺼하는데 관상편이다 ㅋㅋ",
          "original_form": "무한도전 옛날꺼하는데 관상편이다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034264.1.2",
          "form": "아 재밌었는데",
          "original_form": "아 재밌었는데",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034264.1.3",
          "form": "광대 노홍철.....백정은 길",
          "original_form": "광대 노홍철.....백정은 길",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034264.1.4",
          "form": "아 맞다. 정형돈이 관상이 좋았어",
          "original_form": "아 맞다. 정형돈이 관상이 좋았어",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034264.1.5",
          "form": "길은 투잡 상이란다 낮에 백정 밤에는 기생",
          "original_form": "길은 투잡 상이란다 낮에 백정 밤에는 기생",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034264.1.6",
          "form": "ㅋㅋㅋ  그래 기억남",
          "original_form": "ㅋㅋㅋ  그래 기억남",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100034264.1.7",
          "form": "하하랑 정형돈 중에 왕이랑 망나니가 있다네",
          "original_form": "하하랑 정형돈 중에 왕이랑 망나니가 있다네",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100034264.1.8",
          "form": "정형돈이 왕이야",
          "original_form": "정형돈이 왕이야",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100034264.1.9",
          "form": "박명수는 거상 ㅋㅋ",
          "original_form": "박명수는 거상 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100034264.1.10",
          "form": "딱 맞는 상이네",
          "original_form": "딱 맞는 상이네",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100034264.1.11",
          "form": "난 봤던거 같은데 자세히 기억 안난다",
          "original_form": "난 봤던거 같은데 자세히 기억 안난다",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100034264.1.12",
          "form": "정형돈 코가 코구멍이 안보여서 돈이 안 샌대ㅋ",
          "original_form": "정형돈 코가 코구멍이 안보여서 돈이 안 샌대ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100034264.1.13",
          "form": "아  콧구멍이 보이면 안된다는 건 들어봤다",
          "original_form": "아  콧구멍이 보이면 안된다는 건 들어봤다",
