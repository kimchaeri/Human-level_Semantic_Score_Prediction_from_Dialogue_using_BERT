        },
        {
          "id": "MDRW2100032264.1.2",
          "form": "로제떡볶이 땡기네 요새",
          "original_form": "로제떡볶이 땡기네 요새",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032264.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032264.1.4",
          "form": "로제떡볶이안좋아하지만",
          "original_form": "로제떡볶이안좋아하지만",
          "speaker_id": "2",
          "time": "20210905 15:13"
        },
        {
          "id": "MDRW2100032264.1.5",
          "form": "한번정도는먹을수잇쥐",
          "original_form": "한번정도는먹을수잇쥐",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032264.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아맞다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아맞다",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032264.1.7",
          "form": "별로 안좋아하지",
          "original_form": "별로 안좋아하지",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032264.1.8",
          "form": "근데 걘 일반 로제가 아니여 퓨전 로제여!!!",
          "original_form": "근데 걘 일반 로제가 아니여 퓨전 로제여!!!",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032264.1.9",
          "form": "그 약간 니뽕내뽕 스탈이야",
          "original_form": "그 약간 니뽕내뽕 스탈이야",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032264.1.10",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210905 19:00"
        },
        {
          "id": "MDRW2100032264.1.11",
          "form": "퓨전로제...ㅎㅎㅎ",
          "original_form": "퓨전로제...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:00"
        },
        {
          "id": "MDRW2100032264.1.12",
          "form": "니뽕내뽕스탈은뭐람ㅋㅋㅋㅋ",
          "original_form": "니뽕내뽕스탈은뭐람ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:00"
        },
        {
          "id": "MDRW2100032264.1.13",
          "form": "흠 토미토+크림이 아니야",
          "original_form": "흠 토미토+크림이 아니야",
          "speaker_id": "1",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100032264.1.14",
          "form": "떡볶이소스+크림 느낌ㅋㅋㅋㅋ",
          "original_form": "떡볶이소스+크림 느낌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032264.1.15",
          "form": "아아아아ㅏㅏㅏㅏ",
          "original_form": "아아아아ㅏㅏㅏㅏ",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032264.1.16",
          "form": "나로제는엽떡만먹어봄",
          "original_form": "나로제는엽떡만먹어봄",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032264.1.17",
          "form": "그건 어떤디",
          "original_form": "그건 어떤디",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032264.1.18",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032264.1.19",
          "form": "별로야",
          "original_form": "별로야",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032264.1.20",
          "form": "엽떡이 좀 별로야",
          "original_form": "엽떡이 좀 별로야",
