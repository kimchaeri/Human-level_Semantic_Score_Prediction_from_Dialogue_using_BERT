      "utterance": [
        {
          "id": "MDRW2100036024.1.1",
          "form": "음 당근하세요?ㅎ",
          "original_form": "음 당근하세요?ㅎ",
          "speaker_id": "2",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100036024.1.2",
          "form": "아뇨 예전에 깔았다가 지웠어요",
          "original_form": "아뇨 예전에 깔았다가 지웠어요",
          "speaker_id": "1",
          "time": "20210830 15:36"
        },
        {
          "id": "MDRW2100036024.1.3",
          "form": "아항 저는 아직 깔려있는데 사고싶은게 너무 많아요 ㅋㅋ",
          "original_form": "아항 저는 아직 깔려있는데 사고싶은게 너무 많아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100036024.1.4",
          "form": "동네마다 활성화된게 다른가봐요",
          "original_form": "동네마다 활성화된게 다른가봐요",
          "speaker_id": "1",
          "time": "20210830 18:15"
        },
        {
          "id": "MDRW2100036024.1.5",
          "form": "매물이 많지는 않아요 ㅋㅋ",
          "original_form": "매물이 많지는 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100036024.1.6",
          "form": "그냥제가사고픈게 많은가봐요",
          "original_form": "그냥제가사고픈게 많은가봐요",
          "speaker_id": "2",
          "time": "20210830 18:29"
        },
        {
          "id": "MDRW2100036024.1.7",
          "form": "네 그렇군요",
          "original_form": "네 그렇군요",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036024.1.8",
          "form": "답이 늦어 죄송해요 ㅜㅜ",
          "original_form": "답이 늦어 죄송해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100036024.1.9",
          "form": "괜찮아요ㅎㅎ저는 제가 팔려고 올려놓은 매물 입질이 안오니 속상하네요ㅜ",
          "original_form": "괜찮아요ㅎㅎ저는 제가 팔려고 올려놓은 매물 입질이 안오니 속상하네요ㅜ",
          "speaker_id": "2",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100036024.1.10",
          "form": "아 ㅠㅠㅠ 뭐 올리셨는데요?",
          "original_form": "아 ㅠㅠㅠ 뭐 올리셨는데요?",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036024.1.11",
          "form": "헌혈하고 받은 롯데 영화관람권이랑",
          "original_form": "헌혈하고 받은 롯데 영화관람권이랑",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036024.1.12",
          "form": "스벅 아아요ㅜ",
          "original_form": "스벅 아아요ㅜ",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036024.1.13",
          "form": "그거 기프티스타나 니콘내콘에 올리면",
          "original_form": "그거 기프티스타나 니콘내콘에 올리면",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036024.1.14",
          "form": "당근보다 더 적을 순 있긴 하지만 팔리긴 팔려요",
          "original_form": "당근보다 더 적을 순 있긴 하지만 팔리긴 팔려요",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100036024.1.15",
          "form": "백원이라도 더 받겠다는 제 욕심이 문제였나봐요 ㅋㅋ",
          "original_form": "백원이라도 더 받겠다는 제 욕심이 문제였나봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036024.1.16",
          "form": "당근이 더 많이받아지나봐요 저는 그런거도 몰랐어요",
          "original_form": "당근이 더 많이받아지나봐요 저는 그런거도 몰랐어요",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036024.1.17",
          "form": "아뇨 그냥운? 근데 정 안팔리면 니콘내콘에  팔아야겠어요",
          "original_form": "아뇨 그냥운? 근데 정 안팔리면 니콘내콘에  팔아야겠어요",
          "speaker_id": "2",
          "time": "20210831 11:13"
        }
      ]
