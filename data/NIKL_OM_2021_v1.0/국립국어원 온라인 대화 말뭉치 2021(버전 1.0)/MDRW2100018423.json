        {
          "id": "MDRW2100018423.1.1",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "2",
          "time": "20210827 01:37"
        },
        {
          "id": "MDRW2100018423.1.2",
          "form": "저는 코로나땜애 안가네요",
          "original_form": "저는 코로나땜애 안가네요",
          "speaker_id": "1",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018423.1.3",
          "form": "저는 답답해서 안가게되더라고요 ㅠㅠ",
          "original_form": "저는 답답해서 안가게되더라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018423.1.4",
          "form": "가고싶은 마음은 굴뚝같아요",
          "original_form": "가고싶은 마음은 굴뚝같아요",
          "speaker_id": "1",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018423.1.5",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018423.1.6",
          "form": "재개봉 많이해서요",
          "original_form": "재개봉 많이해서요",
          "speaker_id": "1",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018423.1.7",
          "form": "무슨영화요",
          "original_form": "무슨영화요",
          "speaker_id": "2",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018423.1.8",
          "form": "인생영화도 재개봉했어요",
          "original_form": "인생영화도 재개봉했어요",
          "speaker_id": "1",
