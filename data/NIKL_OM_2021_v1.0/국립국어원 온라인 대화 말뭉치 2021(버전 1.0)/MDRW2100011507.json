      "utterance": [
        {
          "id": "MDRW2100011507.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011507.1.2",
          "form": "예전에는 많이 했는데 요즘은 안해요",
          "original_form": "예전에는 많이 했는데 요즘은 안해요",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011507.1.3",
          "form": "저는 모바일게임정도??",
          "original_form": "저는 모바일게임정도??",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011507.1.4",
          "form": "모바일은 간단한거 퍼즐류요",
          "original_form": "모바일은 간단한거 퍼즐류요",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011507.1.5",
          "form": "캔디크러시도 한동안 열심히 했네요",
          "original_form": "캔디크러시도 한동안 열심히 했네요",
          "speaker_id": "2",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011507.1.6",
          "form": "캔디크러시",
          "original_form": "캔디크러시",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100011507.1.7",
          "form": "저도해봤어요ㅋㅋㅋ",
          "original_form": "저도해봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100011507.1.8",
          "form": "하다보면 시간가는줄 몰라요",
          "original_form": "하다보면 시간가는줄 몰라요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100011507.1.9",
          "form": "요즘도 하시나요?",
          "original_form": "요즘도 하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100011507.1.10",
          "form": "아니요 요즘은 하는 게임이 없네요",
          "original_form": "아니요 요즘은 하는 게임이 없네요",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100011507.1.11",
          "form": "전에는 동물의숲도 열심히 했었어요ㅋㅋ",
          "original_form": "전에는 동물의숲도 열심히 했었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100011507.1.12",
          "form": "동물의숲재밌나요?",
          "original_form": "동물의숲재밌나요?",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100011507.1.13",
          "form": "집만드는 재미가 쏠쏠해요",
          "original_form": "집만드는 재미가 쏠쏠해요",
          "speaker_id": "2",
          "time": "20210825 08:16"
        },
        {
          "id": "MDRW2100011507.1.14",
          "form": "어떤 게임 좋아하세여",
          "original_form": "어떤 게임 좋아하세여",
          "speaker_id": "2",
          "time": "20210825 09:12"
        },
        {
          "id": "MDRW2100011507.1.15",
          "form": "전 아기자기한",
          "original_form": "전 아기자기한",
          "speaker_id": "1",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100011507.1.16",
          "form": "모바일게임?ㅋㅋㅋ",
          "original_form": "모바일게임?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100011507.1.17",
          "form": "오 그런것도 재미있죠",
          "original_form": "오 그런것도 재미있죠",
          "speaker_id": "2",
          "time": "20210825 11:32"
        },
        {
          "id": "MDRW2100011507.1.18",
          "form": "하시는거있나요?",
          "original_form": "하시는거있나요?",
          "speaker_id": "1",
          "time": "20210825 11:34"
        },
        {
          "id": "MDRW2100011507.1.19",
          "form": "전 지금은 없는데 쿠키런 재미있대요",
          "original_form": "전 지금은 없는데 쿠키런 재미있대요",
          "speaker_id": "2",
          "time": "20210825 11:37"
        }
      ]
