      "utterance": [
        {
          "id": "MDRW2100024694.1.1",
          "form": "비가 엄청 내리네요",
          "original_form": "비가 엄청 내리네요",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024694.1.2",
          "form": "네 비가엄청오네요",
          "original_form": "네 비가엄청오네요",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024694.1.3",
          "form": "내일도 온다고하네요ㅠ",
          "original_form": "내일도 온다고하네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024694.1.4",
          "form": "요즘 일기예보가 좀 정확하네요ㅎㅎㅎ 비 안올줄알았는데..",
          "original_form": "요즘 일기예보가 좀 정확하네요ㅎㅎㅎ 비 안올줄알았는데..",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024694.1.5",
          "form": "맞아요 요즘 좀 잘맞는것같아요ㅎㅎ",
          "original_form": "맞아요 요즘 좀 잘맞는것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024694.1.6",
          "form": "옷도 긴팔입었네요 오늘은",
          "original_form": "옷도 긴팔입었네요 오늘은",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024694.1.7",
          "form": "서늘하죠 ㅎㅎ 가을왔나봐요",
          "original_form": "서늘하죠 ㅎㅎ 가을왔나봐요",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024694.1.8",
          "form": "이제 가을이 온거같아요진짜",
          "original_form": "이제 가을이 온거같아요진짜",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024694.1.9",
          "form": "하늘도푸르고ㅎㅎ",
          "original_form": "하늘도푸르고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024694.1.10",
          "form": "ㅎㅎ 그쵸 가을이 길었으면 좋겠어요.. 12개월중11개월..",
          "original_form": "ㅎㅎ 그쵸 가을이 길었으면 좋겠어요.. 12개월중11개월..",
          "speaker_id": "1",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024694.1.11",
          "form": "또가을 한 보름있다가",
          "original_form": "또가을 한 보름있다가",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024694.1.12",
          "form": "추운겨울이 들이닥치겠죠?ㅋㅋㅋ",
          "original_form": "추운겨울이 들이닥치겠죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024694.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 금방요.. 여름겨울뿐인거같아요 계절이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 금방요.. 여름겨울뿐인거같아요 계절이",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024694.1.14",
          "form": "봄가을이 제일좋은데ㅠ",
          "original_form": "봄가을이 제일좋은데ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024694.1.15",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ 부담없는계절!!!",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ 부담없는계절!!!",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024694.1.16",
          "form": "내일도 비가오면",
          "original_form": "내일도 비가오면",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024694.1.17",
          "form": "더서늘해질까요?",
          "original_form": "더서늘해질까요?",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024694.1.18",
          "form": "아마도요... 요즘 옷입기가 매우 애매해요",
          "original_form": "아마도요... 요즘 옷입기가 매우 애매해요",
