      "utterance": [
        {
          "id": "MDRW2100011932.1.1",
          "form": "네이버 카페 어디 자주가시는데 있으세요?",
          "original_form": "네이버 카페 어디 자주가시는데 있으세요?",
          "speaker_id": "1",
          "time": "20210824 17:32"
        },
        {
          "id": "MDRW2100011932.1.2",
          "form": "미주미요",
          "original_form": "미주미요",
          "speaker_id": "2",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011932.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210824 17:36"
        },
        {
          "id": "MDRW2100011932.1.4",
          "form": "미주미는 주식카페인가요?",
          "original_form": "미주미는 주식카페인가요?",
          "speaker_id": "1",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011932.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011932.1.6",
          "form": "저는 월재연 자주 가는거같아요",
          "original_form": "저는 월재연 자주 가는거같아요",
          "speaker_id": "1",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011932.1.7",
          "form": "미국주식카페에요",
          "original_form": "미국주식카페에요",
          "speaker_id": "2",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011932.1.8",
          "form": "월재연이 뭔가요?",
          "original_form": "월재연이 뭔가요?",
          "speaker_id": "2",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011932.1.9",
          "form": "월급쟁이재테크연구?",
          "original_form": "월급쟁이재테크연구?",
          "speaker_id": "1",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011932.1.10",
          "form": "연구소? ㅎ",
          "original_form": "연구소? ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011932.1.11",
          "form": "아 들어봤어요ㅋㅋ",
          "original_form": "아 들어봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011932.1.12",
          "form": "거기서 앱테크 정보 눈팅한적 있는데",
          "original_form": "거기서 앱테크 정보 눈팅한적 있는데",
          "speaker_id": "2",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011932.1.13",
          "form": "맨날 월재연이라고만해서 ㅎ",
          "original_form": "맨날 월재연이라고만해서 ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:28"
        },
        {
          "id": "MDRW2100011932.1.14",
          "form": "연구소인지 연구인지 모르겠네요 ㅎ",
          "original_form": "연구소인지 연구인지 모르겠네요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:29"
        },
        {
          "id": "MDRW2100011932.1.15",
          "form": "미국주식 게시글이 지금 많이 올라오는거아닌가요?",
          "original_form": "미국주식 게시글이 지금 많이 올라오는거아닌가요?",
          "speaker_id": "1",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100011932.1.16",
          "form": "작년 3월부터 많이 올라오네요~",
          "original_form": "작년 3월부터 많이 올라오네요~",
          "speaker_id": "2",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100011932.1.17",
          "form": "가입하신지 오래되셨어요?",
          "original_form": "가입하신지 오래되셨어요?",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100011932.1.18",
          "form": "아니요 2019년부터 가입했어요",
          "original_form": "아니요 2019년부터 가입했어요",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100011932.1.19",
          "form": "오 그래도 꽤되시네요",
          "original_form": "오 그래도 꽤되시네요",
