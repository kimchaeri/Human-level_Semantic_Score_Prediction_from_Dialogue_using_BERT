      "utterance": [
        {
          "id": "MDRW2100013195.1.1",
          "form": "네 종종 봐요",
          "original_form": "네 종종 봐요",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013195.1.2",
          "form": "재밌죠!!",
          "original_form": "재밌죠!!",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013195.1.3",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013195.1.4",
          "form": "저 무서운거 완전 좋아해서",
          "original_form": "저 무서운거 완전 좋아해서",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.5",
          "form": "다 봤어요!!",
          "original_form": "다 봤어요!!",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.6",
          "form": "저는 가끔 생각나면 봐요",
          "original_form": "저는 가끔 생각나면 봐요",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.7",
          "form": "근데 가끔 지어낸 사연 같은거",
          "original_form": "근데 가끔 지어낸 사연 같은거",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.8",
          "form": "많이 나오면",
          "original_form": "많이 나오면",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.9",
          "form": "실망 ㅜ",
          "original_form": "실망 ㅜ",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.10",
          "form": "거의 다 그런 것 같아요",
          "original_form": "거의 다 그런 것 같아요",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.11",
          "form": "네 ㅜㅜ 저도 사연 보냈었는데",
          "original_form": "네 ㅜㅜ 저도 사연 보냈었는데",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.12",
          "form": "전 실화라 별거 없어서",
          "original_form": "전 실화라 별거 없어서",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.13",
          "form": "씹혔어요 ㅠㅠ",
          "original_form": "씹혔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013195.1.14",
          "form": "제보하셨어요??",
          "original_form": "제보하셨어요??",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.15",
          "form": "네 ㅠㅠ 무서웟던 일 보냈는데",
          "original_form": "네 ㅠㅠ 무서웟던 일 보냈는데",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.16",
          "form": "상금 아쉬워요 ㅎㅎㅎ",
          "original_form": "상금 아쉬워요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.17",
          "form": "상금 얼마에요?",
          "original_form": "상금 얼마에요?",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.18",
          "form": "방송만 나와도 44만원이에요!!",
          "original_form": "방송만 나와도 44만원이에요!!",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.19",
          "form": "우와 꽤 크네요",
          "original_form": "우와 꽤 크네요",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.20",
          "form": "네ㅜㅜ 아쉬워요",
          "original_form": "네ㅜㅜ 아쉬워요",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.21",
          "form": "저도 응모해봐야겠어요",
          "original_form": "저도 응모해봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100013195.1.22",
          "form": "오 사연 있으세요?",
          "original_form": "오 사연 있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013195.1.23",
          "form": "아뇨 주변에서 좀 끌어모아서 ㅎㅎ",
          "original_form": "아뇨 주변에서 좀 끌어모아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013195.1.24",
          "form": "됐으면 좋겠네용",
          "original_form": "됐으면 좋겠네용",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013195.1.25",
          "form": "옛날에 토요미스테리도 재밌었는데!!",
          "original_form": "옛날에 토요미스테리도 재밌었는데!!",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013195.1.26",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:26"
        }
      ]
