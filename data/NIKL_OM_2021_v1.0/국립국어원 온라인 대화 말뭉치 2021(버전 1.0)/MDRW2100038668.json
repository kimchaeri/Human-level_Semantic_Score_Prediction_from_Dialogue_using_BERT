      "utterance": [
        {
          "id": "MDRW2100038668.1.1",
          "form": "저는 가을이요",
          "original_form": "저는 가을이요",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100038668.1.2",
          "form": "좋아하는 계절있으세요?",
          "original_form": "좋아하는 계절있으세요?",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100038668.1.3",
          "form": "전 봄이 좋아요",
          "original_form": "전 봄이 좋아요",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100038668.1.4",
          "form": "봄도 좋죠",
          "original_form": "봄도 좋죠",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100038668.1.5",
          "form": "설레임 ㅎㅎ",
          "original_form": "설레임 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100038668.1.6",
          "form": "봄가을은 점점 짧아지는듯해요",
          "original_form": "봄가을은 점점 짧아지는듯해요",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100038668.1.7",
          "form": "따뜻해서 좋아요",
          "original_form": "따뜻해서 좋아요",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038668.1.8",
          "form": "올가을은 트렌치코트 좀 입을수 있으려낭...",
          "original_form": "올가을은 트렌치코트 좀 입을수 있으려낭...",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038668.1.9",
          "form": "가을이 좀 짧죠",
          "original_form": "가을이 좀 짧죠",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100038668.1.10",
          "form": "이러다 금방 겨울올것같아요",
          "original_form": "이러다 금방 겨울올것같아요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100038668.1.11",
          "form": "이젠 해지니 춥네요",
          "original_form": "이젠 해지니 춥네요",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100038668.1.12",
          "form": "추위가 오래가겠죠",
          "original_form": "추위가 오래가겠죠",
          "speaker_id": "1",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100038668.1.13",
          "form": "추위에 강하나요 ?",
          "original_form": "추위에 강하나요 ?",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100038668.1.14",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038668.1.15",
          "form": "추위 더위에 다 약해요",
          "original_form": "추위 더위에 다 약해요",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038668.1.16",
          "form": "둘중하나만 고르라면요 ?",
          "original_form": "둘중하나만 고르라면요 ?",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038668.1.17",
          "form": "아 힘든 질문이네요",
          "original_form": "아 힘든 질문이네요",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038668.1.18",
          "form": "추위?ㅎ",
          "original_form": "추위?ㅎ",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038668.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100038668.1.20",
          "form": "전 추운게 더싫어요",
          "original_form": "전 추운게 더싫어요",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100038668.1.21",
          "form": "ㅎㅎ 둘다 견디기 힘들어요 ㅎ",
          "original_form": "ㅎㅎ 둘다 견디기 힘들어요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 00:20"
        }
      ]
