        {
          "id": "MDRW2100003082.1.1",
          "form": "좋아하는 음식 또 뭐있어?",
          "original_form": "좋아하는 음식 또 뭐있어?",
          "speaker_id": "2",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100003082.1.2",
          "form": "음식은 가리지 않는 편이야",
          "original_form": "음식은 가리지 않는 편이야",
          "speaker_id": "1",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100003082.1.3",
          "form": "name2는?",
          "original_form": "&name2&는?",
          "speaker_id": "1",
          "time": "20210820 18:25"
        },
        {
          "id": "MDRW2100003082.1.4",
          "form": "나도 가리는 거 없이 거의 다 좋아 ㅋㅋ",
          "original_form": "나도 가리는 거 없이 거의 다 좋아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:25"
        },
        {
          "id": "MDRW2100003082.1.5",
          "form": "못 먹는 음식 있어?",
          "original_form": "못 먹는 음식 있어?",
          "speaker_id": "1",
          "time": "20210820 18:25"
        },
        {
          "id": "MDRW2100003082.1.6",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003082.1.7",
          "form": "홍어나 향 센 음식은 좀",
          "original_form": "홍어나 향 센 음식은 좀",
          "speaker_id": "2",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003082.1.8",
          "form": "홍어는 나도 잘 못먹어 ㅎㅎ",
          "original_form": "홍어는 나도 잘 못먹어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003082.1.9",
          "form": "오이도 좀 ㅋㅋㅋ",
          "original_form": "오이도 좀 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003082.1.10",
          "form": "아 오이는 향때문인가?",
          "original_form": "아 오이는 향때문인가?",
          "speaker_id": "1",
          "time": "20210820 18:27"
        },
        {
          "id": "MDRW2100003082.1.11",
          "form": "물냄새? 그런 거",
          "original_form": "물냄새? 그런 거",
          "speaker_id": "2",
          "time": "20210820 18:27"
        },
        {
          "id": "MDRW2100003082.1.12",
          "form": "아 그렇구나",
