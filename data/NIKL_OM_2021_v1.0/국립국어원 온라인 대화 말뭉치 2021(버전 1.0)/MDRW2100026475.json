      "utterance": [
        {
          "id": "MDRW2100026475.1.1",
          "form": "모임 좋아하시나여",
          "original_form": "모임 좋아하시나여",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026475.1.2",
          "form": "전 인도어형 인간이라서요ㅎ",
          "original_form": "전 인도어형 인간이라서요ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026475.1.3",
          "form": "님은 동호회나 모임 있으신가요",
          "original_form": "님은 동호회나 모임 있으신가요",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026475.1.4",
          "form": "저는 고등학교 친구",
          "original_form": "저는 고등학교 친구",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026475.1.5",
          "form": "회사 동기 모임?ㅋㅋ",
          "original_form": "회사 동기 모임?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026475.1.6",
          "form": "아앗 후자는 뭐죠ㅋ",
          "original_form": "아앗 후자는 뭐죠ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026475.1.7",
          "form": "회사 동기 모임 안하시나요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "회사 동기 모임 안하시나요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026475.1.8",
          "form": "우리는 데면데면합니다ㅎ",
          "original_form": "우리는 데면데면합니다ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026475.1.9",
          "form": "이직한 사람 많은 기수라",
          "original_form": "이직한 사람 많은 기수라",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026475.1.10",
          "form": "좀 하다 말았죠",
          "original_form": "좀 하다 말았죠",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026475.1.11",
          "form": "ㅋㅋ오 저희도 숫자 많은데",
          "original_form": "ㅋㅋ오 저희도 숫자 많은데",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026475.1.12",
          "form": "ㅎㅎ 동기가 끈끈한가봐요",
          "original_form": "ㅎㅎ 동기가 끈끈한가봐요",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026475.1.13",
          "form": "어느정도 주기로 모이세요?",
          "original_form": "어느정도 주기로 모이세요?",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026475.1.14",
          "form": "원래는 뭐 번개도 많이 쳤는데",
          "original_form": "원래는 뭐 번개도 많이 쳤는데",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026475.1.15",
          "form": "요새 코로나가. .",
          "original_form": "요새 코로나가. .",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026475.1.16",
          "form": "아...ㅜ",
          "original_form": "아...ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026475.1.17",
          "form": "코로나전에는 자주 모이셨나용",
          "original_form": "코로나전에는 자주 모이셨나용",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026475.1.18",
          "form": "네 일주일에 한번정도는ㅋㅋㅋ",
          "original_form": "네 일주일에 한번정도는ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026475.1.19",
          "form": "아니ㅋㅋ 회사서도 계속볼텐데ㅎㅎ",
          "original_form": "아니ㅋㅋ 회사서도 계속볼텐데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026475.1.20",
          "form": "애인보다 자주보는거 아닌가요ㅎ",
          "original_form": "애인보다 자주보는거 아닌가요ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026475.1.21",
          "form": "ㅋㅋ 아 근무지가 퍼져있어서요 ^^;;",
          "original_form": "ㅋㅋ 아 근무지가 퍼져있어서요 ^^;;",
