      "utterance": [
        {
          "id": "MDRW2100043104.1.1",
          "form": "커피좋아하시나요",
          "original_form": "커피좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043104.1.2",
          "form": "얼죽아 입니다",
          "original_form": "얼죽아 입니다",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043104.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043104.1.4",
          "form": "어우 저도 얼죽아였는데 이제 놓아주려고요",
          "original_form": "어우 저도 얼죽아였는데 이제 놓아주려고요",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043104.1.5",
          "form": "ㅋㅋ그럼 뭘로 갈아타셧나욤",
          "original_form": "ㅋㅋ그럼 뭘로 갈아타셧나욤",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043104.1.6",
          "form": "이제는 뜨거운것도마시고 차도마셔요 ㅋㅋㅋ",
          "original_form": "이제는 뜨거운것도마시고 차도마셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043104.1.7",
          "form": "오늘은 머마시셧나용?",
          "original_form": "오늘은 머마시셧나용?",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043104.1.8",
          "form": "오늘은 아아욬ㅋㅋㅋㅋname1님은요",
          "original_form": "오늘은 아아욬ㅋㅋㅋㅋ&name1&님은요",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043104.1.9",
          "form": "저는 벌써",
          "original_form": "저는 벌써",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043104.1.10",
          "form": "출근하자마자 한잔",
          "original_form": "출근하자마자 한잔",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043104.1.11",
          "form": "점심먹고 한잔",
          "original_form": "점심먹고 한잔",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043104.1.12",
          "form": "아아두잔이욥",
          "original_form": "아아두잔이욥",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043104.1.13",
          "form": "아 못참죠",
          "original_form": "아 못참죠",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043104.1.14",
          "form": "회사카누 뽀려서 마시고있습니다",
          "original_form": "회사카누 뽀려서 마시고있습니다",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043104.1.15",
          "form": "카누좋져ㅋㅋ",
          "original_form": "카누좋져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043104.1.16",
          "form": "과자도같이 드세여",
          "original_form": "과자도같이 드세여",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043104.1.17",
          "form": "흑 과자가없어요",
          "original_form": "흑 과자가없어요",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043104.1.18",
          "form": "항상준비해야하는건데",
          "original_form": "항상준비해야하는건데",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043104.1.19",
          "form": "저는 하루견과 먹고잇어요",
          "original_form": "저는 하루견과 먹고잇어요",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043104.1.20",
          "form": "오 그런거 저도 사다놓을까봐요 ㅜ",
          "original_form": "오 그런거 저도 사다놓을까봐요 ㅜ",
