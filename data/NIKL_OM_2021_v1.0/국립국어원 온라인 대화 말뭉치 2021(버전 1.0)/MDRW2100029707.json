      "utterance": [
        {
          "id": "MDRW2100029707.1.1",
          "form": "저는 오늘 드디어 라떼를 마셨엉ㅅ!",
          "original_form": "저는 오늘 드디어 라떼를 마셨엉ㅅ!",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029707.1.2",
          "form": "점심은 어떤거 드세요?",
          "original_form": "점심은 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029707.1.3",
          "form": "씨유에서 사먹으려구요!!",
          "original_form": "씨유에서 사먹으려구요!!",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029707.1.4",
          "form": "씨유에서 뮤ㅓ 드셨어요?",
          "original_form": "씨유에서 뮤ㅓ 드셨어요?",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029707.1.5",
          "form": "저는 같이 일하는 분이 사주셔서",
          "original_form": "저는 같이 일하는 분이 사주셔서",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029707.1.6",
          "form": "파스타랑 피자 먹었엉ㅅ!!!",
          "original_form": "파스타랑 피자 먹었엉ㅅ!!!",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029707.1.7",
          "form": "헐 맛있겠다",
          "original_form": "헐 맛있겠다",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029707.1.8",
          "form": "전 참치유부초밥이랑 요거트요ㅋㅋ",
          "original_form": "전 참치유부초밥이랑 요거트요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029707.1.9",
          "form": "헐 참치유부초밥",
          "original_form": "헐 참치유부초밥",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029707.1.10",
          "form": "너무 맛있을거같아요",
          "original_form": "너무 맛있을거같아요",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029707.1.11",
          "form": "다 제가 좋아하는 음식들이에요",
          "original_form": "다 제가 좋아하는 음식들이에요",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029707.1.12",
          "form": "전 파스타랑  피자가 더...",
          "original_form": "전 파스타랑  피자가 더...{emoji:♡}",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100029707.1.13",
          "form": "무슨 파스타 드셨나요?",
          "original_form": "무슨 파스타 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100029707.1.14",
          "form": "저 오늘은 명란 오일 파스타 먹었어요!",
          "original_form": "저 오늘은 명란 오일 파스타 먹었어요!",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029707.1.15",
          "form": "어떤 파스타 제일 좋아하세요?",
          "original_form": "어떤 파스타 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029707.1.16",
          "form": "전 매운크림파스타요!!",
          "original_form": "전 매운크림파스타요!!",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029707.1.17",
          "form": "와 매운 크림 파스타!",
          "original_form": "와 매운 크림 파스타!",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029707.1.18",
          "form": "전 약간 매운 크림류는 파스타는 아니지만",
          "original_form": "전 약간 매운 크림류는 파스타는 아니지만",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100029707.1.19",
          "form": "니뽕내뽕아세요?",
          "original_form": "니뽕내뽕아세요?",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100029707.1.20",
          "form": "거기서 먹는거 좋아해요 ㅎㅎ",
          "original_form": "거기서 먹는거 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100029707.1.21",
          "form": "니뽕내뽕!! 짱맛있죠",
          "original_form": "니뽕내뽕!! 짱맛있죠",
          "speaker_id": "1",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100029707.1.22",
          "form": "오늘 니뽕내뽕 가신거에요?",
          "original_form": "오늘 니뽕내뽕 가신거에요?",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100029707.1.23",
          "form": "앗 그건 아님니다!!",
          "original_form": "앗 그건 아님니다!!",
          "speaker_id": "2",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100029707.1.24",
          "form": "하지먼 매운크림 파스타를 좋아하신다구 하니 니뽕내뽕이 생각났엉ㅅ!!",
          "original_form": "하지먼 매운크림 파스타를 좋아하신다구 하니 니뽕내뽕이 생각났엉ㅅ!!",
          "speaker_id": "2",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100029707.1.25",
          "form": "니뽕내뽕...맛없없조합",
          "original_form": "니뽕내뽕...맛없없조합",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100029707.1.26",
          "form": "맞아요! 니뽕내뽕 간지 넘 오래돼서 넘 오랜만에 가고싶네요!",
