          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046010.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046010.1.17",
          "form": "스테이크 해 먹어버릴까 고민돼요 ㅋㅋㅋ",
