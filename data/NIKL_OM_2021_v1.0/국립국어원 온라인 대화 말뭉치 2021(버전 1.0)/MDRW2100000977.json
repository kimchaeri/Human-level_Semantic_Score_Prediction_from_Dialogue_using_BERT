      "utterance": [
        {
          "id": "MDRW2100000977.1.1",
          "form": "이상형이 어떻게 되시나요",
          "original_form": "이상형이 어떻게 되시나요",
          "speaker_id": "1",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000977.1.2",
          "form": "예의바르고 조신한 사람이요~",
          "original_form": "예의바르고 조신한 사람이요~",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000977.1.3",
          "form": "조신한거 매우 중요하죠",
          "original_form": "조신한거 매우 중요하죠",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000977.1.4",
          "form": "결혼 미혼 비혼 중에 선택한다면?",
          "original_form": "결혼 미혼 비혼 중에 선택한다면?",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000977.1.5",
          "form": "음...... 최대한 늦게 늦게 하는 결혼을 택할래요",
          "original_form": "음...... 최대한 늦게 늦게 하는 결혼을 택할래요",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000977.1.6",
          "form": "비혼까지는 아니긴해요",
          "original_form": "비혼까지는 아니긴해요",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000977.1.7",
          "form": "아이는 몇 명 생각하시나요 아들 딸인지도 궁금해요",
          "original_form": "아이는 몇 명 생각하시나요 아들 딸인지도 궁금해요",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000977.1.8",
          "form": "아이는 생각없는 딩크 원하지만 굳이 낳게된다면 딸이요",
          "original_form": "아이는 생각없는 딩크 원하지만 굳이 낳게된다면 딸이요",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000977.1.9",
          "form": "현재 연애는 하시나요?",
          "original_form": "현재 연애는 하시나요?",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100000977.1.10",
          "form": "네 하고있어요",
          "original_form": "네 하고있어요",
          "speaker_id": "2",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100000977.1.11",
          "form": "지금 연애상대와 결혼하고싶은 생각은??",
          "original_form": "지금 연애상대와 결혼하고싶은 생각은??",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100000977.1.12",
          "form": "생각은 있지만 몇년은 걸릴듯해요 ㅎㅎ",
          "original_form": "생각은 있지만 몇년은 걸릴듯해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100000977.1.13",
          "form": "결혼을 결정할 때 가장 중요한게 뭘까요? 조신한데 시부모님이 완전 고지식하면 어떠세요",
          "original_form": "결혼을 결정할 때 가장 중요한게 뭘까요? 조신한데 시부모님이 완전 고지식하면 어떠세요",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100000977.1.14",
          "form": "중간다리 역을 잘 해주면 좋겠어요",
          "original_form": "중간다리 역을 잘 해주면 좋겠어요",
          "speaker_id": "2",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100000977.1.15",
          "form": "사실 사람만 보고 결혼하는게 아니니까요",
          "original_form": "사실 사람만 보고 결혼하는게 아니니까요",
          "speaker_id": "2",
          "time": "20210819 21:30"
        }
