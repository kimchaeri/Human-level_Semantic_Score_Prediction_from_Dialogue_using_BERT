      "utterance": [
        {
          "id": "MDRW2100028324.1.1",
          "form": "엄마 아까 뭐 찾아봤지? 어디 가고 싶다며",
          "original_form": "엄마 아까 뭐 찾아봤지? 어디 가고 싶다며",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028324.1.2",
          "form": "군산에 신시도",
          "original_form": "군산에 신시도",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028324.1.3",
          "form": "머야 그건또 어떻게 알았어ㅋㅋ",
          "original_form": "머야 그건또 어떻게 알았어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028324.1.4",
          "form": "name3이 이모가 갔다왔대",
          "original_form": "&name3&이 이모가 갔다왔대",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028324.1.5",
          "form": "군산까지?",
          "original_form": "군산까지?",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028324.1.6",
          "form": "아 name4이 보고 갔나보다",
          "original_form": "아 &name4&이 보고 갔나보다",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028324.1.7",
          "form": "걔는 안 다니는데가 없어",
          "original_form": "걔는 안 다니는데가 없어",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028324.1.8",
          "form": "신시도 좋대?",
          "original_form": "신시도 좋대?",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028324.1.9",
          "form": "ㅇㅇ사진 보내준 거 보니까 진짜 멋있더라",
          "original_form": "ㅇㅇ사진 보내준 거 보니까 진짜 멋있더라",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028324.1.10",
          "form": "바닷간가? 도면 섬인가?",
          "original_form": "바닷간가? 도면 섬인가?",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028324.1.11",
          "form": "섬인데 섬에 무슨 민박집 하나 있고 아무것도 없대",
          "original_form": "섬인데 섬에 무슨 민박집 하나 있고 아무것도 없대",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028324.1.12",
          "form": "헐 그럼 거기 왜 감",
          "original_form": "헐 그럼 거기 왜 감",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028324.1.13",
          "form": "민박집에서 할 것도 없을거 같은데",
          "original_form": "민박집에서 할 것도 없을거 같은데",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028324.1.14",
          "form": "거기 풍경보러 가는 거지",
          "original_form": "거기 풍경보러 가는 거지",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028324.1.15",
          "form": "먹을 거 요즘은 다 싸다니니까",
          "original_form": "먹을 거 요즘은 다 싸다니니까",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028324.1.16",
          "form": "그래도 밥은 그럼 어디서먹냐..",
          "original_form": "그래도 밥은 그럼 어디서먹냐..",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028324.1.17",
          "form": "우린 안자고 당일치기로 갔다와야지",
          "original_form": "우린 안자고 당일치기로 갔다와야지",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028324.1.18",
          "form": "밥은 군산에서 먹고 사람 별로 없어서 괜찮을거야",
          "original_form": "밥은 군산에서 먹고 사람 별로 없어서 괜찮을거야",
          "speaker_id": "1",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100028324.1.19",
          "form": "엄마 기장에",
          "original_form": "엄마 기장에",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028324.1.20",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028324.1.21",
          "form": "이흥용 제과점 생겼대",
          "original_form": "이흥용 제과점 생겼대",
          "speaker_id": "2",
