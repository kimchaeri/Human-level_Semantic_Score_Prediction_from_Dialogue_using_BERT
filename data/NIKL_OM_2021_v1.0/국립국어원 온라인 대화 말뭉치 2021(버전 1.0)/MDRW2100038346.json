      "utterance": [
        {
          "id": "MDRW2100038346.1.1",
          "form": "말이 어눌해도 자기 할 말은 다 한다",
          "original_form": "말이 어눌해도 자기 할 말은 다 한다",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038346.1.2",
          "form": "우리 투는 이해도가 떨어지거든",
          "original_form": "우리 투는 이해도가 떨어지거든",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038346.1.3",
          "form": "발음도 발음이지만",
          "original_form": "발음도 발음이지만",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038346.1.4",
          "form": "이게 크면 좀 괜찮아 지나??",
          "original_form": "이게 크면 좀 괜찮아 지나??",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038346.1.5",
          "form": "너무 걱정된다 책 내용을 이해 못하는 게",
          "original_form": "너무 걱정된다 책 내용을 이해 못하는 게",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038346.1.6",
          "form": "응응  괜찮아질것같은데",
          "original_form": "응응  괜찮아질것같은데",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038346.1.7",
          "form": "너무 어려운건아니고? 그시기에 적당한내용은 맞나?",
          "original_form": "너무 어려운건아니고? 그시기에 적당한내용은 맞나?",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038346.1.8",
          "form": "그냥 상상하는 거 자체를 못하는 듯",
          "original_form": "그냥 상상하는 거 자체를 못하는 듯",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038346.1.9",
          "form": "창작동화를 이해 못한다",
          "original_form": "창작동화를 이해 못한다",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038346.1.10",
          "form": "내가 어제 무슨 기사를 봤는데",
          "original_form": "내가 어제 무슨 기사를 봤는데",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038346.1.11",
          "form": "시각적인 영상만 많이 본 애들은",
          "original_form": "시각적인 영상만 많이 본 애들은",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.12",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.13",
          "form": "아ㅋㅋㅋ시각적인..영상..",
          "original_form": "아ㅋㅋㅋ시각적인..영상..",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.14",
          "form": "하..찔리네",
          "original_form": "하..찔리네",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.15",
          "form": "후두엽만 쓰다보니까",
          "original_form": "후두엽만 쓰다보니까",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.16",
          "form": "팝콘뇌 라는게 된다대",
          "original_form": "팝콘뇌 라는게 된다대",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.17",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100038346.1.18",
          "form": "팝콘뇌라면 ㅜ",
          "original_form": "팝콘뇌라면 ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.19",
          "form": "그래서 독서라던지 그런 자극을 주지 않으면",
          "original_form": "그래서 독서라던지 그런 자극을 주지 않으면",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.20",
          "form": "응응ㅜ",
          "original_form": "응응ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.21",
          "form": "단어에 의미나",
          "original_form": "단어에 의미나",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.23",
          "form": "맥락 구조 같은 거를 이해하지 못한데",
          "original_form": "맥락 구조 같은 거를 이해하지 못한데",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.24",
          "form": "근데 진짜 딱 우리 투 같은 거야",
          "original_form": "근데 진짜 딱 우리 투 같은 거야",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.25",
          "form": "그러히ㅜ나",
          "original_form": "그러히ㅜ나",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.26",
          "form": "헉 나두",
          "original_form": "헉 나두",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.27",
          "form": "규칙이런 것도 이해를 못해",
          "original_form": "규칙이런 것도 이해를 못해",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.28",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.29",
          "form": "우리 투도 영상 부터 보기 시작해 가지고",
          "original_form": "우리 투도 영상 부터 보기 시작해 가지고",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.30",
          "form": "책을 이해를 못하는 거 같더라고",
          "original_form": "책을 이해를 못하는 거 같더라고",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038346.1.31",
          "form": "맞어맞어 그런건있겠다ㅡ",
          "original_form": "맞어맞어 그런건있겠다ㅡ",
          "speaker_id": "1",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100038346.1.32",
          "form": "어떤 건지 알겠지??",
          "original_form": "어떤 건지 알겠지??",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100038346.1.33",
          "form": "원은 확실히 영상을 거의 안 보여주고 책 위주로",
          "original_form": "원은 확실히 영상을 거의 안 보여주고 책 위주로",
          "speaker_id": "2",
          "time": "20210902 11:17"
        }
      ]
