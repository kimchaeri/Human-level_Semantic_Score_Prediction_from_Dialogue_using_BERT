      "utterance": [
        {
          "id": "MDRW2100020562.1.1",
          "form": "name2님 안녕하세요",
          "original_form": "&name2&님 안녕하세요",
          "speaker_id": "1",
          "time": "20210828 09:51"
        },
        {
          "id": "MDRW2100020562.1.2",
          "form": "혹시 반려동물 키우세요?",
          "original_form": "혹시 반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020562.1.3",
          "form": "안녕하세요\r\n제가 키우는건 아니구 본가에서 키우고있어요",
          "original_form": "안녕하세요\r\n제가 키우는건 아니구 본가에서 키우고있어요",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020562.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020562.1.5",
          "form": "아 전 아니요 ㅎㅎ",
          "original_form": "아 전 아니요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020562.1.6",
          "form": "룸메가 고양이 키웠는데",
          "original_form": "룸메가 고양이 키웠는데",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020562.1.7",
          "form": "나갔어요",
          "original_form": "나갔어요",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020562.1.8",
          "form": "본가에서는 강아지인가요?",
          "original_form": "본가에서는 강아지인가요?",
          "speaker_id": "1",
          "time": "20210828 09:56"
        },
        {
          "id": "MDRW2100020562.1.9",
          "form": "네 강아지키우고있어요!",
          "original_form": "네 강아지키우고있어요!",
          "speaker_id": "2",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020562.1.10",
          "form": "키워보고싶은 반려동물 있으신가요?",
          "original_form": "키워보고싶은 반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020562.1.11",
          "form": "음... 강아지 키우고싶어요",
          "original_form": "음... 강아지 키우고싶어요",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020562.1.12",
          "form": "대형견이요 ㅎㅎ",
          "original_form": "대형견이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 09:59"
        },
        {
          "id": "MDRW2100020562.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020562.1.14",
          "form": "와..대형견 저도 키워보고싶긴한데",
          "original_form": "와..대형견 저도 키워보고싶긴한데",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020562.1.15",
          "form": "힘들거같아요",
          "original_form": "힘들거같아요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020562.1.16",
          "form": "저는 고양이 키워보고싶어요",
          "original_form": "저는 고양이 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210828 10:00"
        },
        {
          "id": "MDRW2100020562.1.17",
          "form": "강아지랑은 또다른 매력이 있는거같더라구요",
          "original_form": "강아지랑은 또다른 매력이 있는거같더라구요",
          "speaker_id": "2",
          "time": "20210828 10:01"
        },
        {
          "id": "MDRW2100020562.1.18",
          "form": "아아 맞아요",
          "original_form": "아아 맞아요",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020562.1.19",
          "form": "행동이 완전 반대예요",
          "original_form": "행동이 완전 반대예요",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020562.1.20",
          "form": "그니까요 좀 시크한매력이 있을거같아요",
          "original_form": "그니까요 좀 시크한매력이 있을거같아요",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020562.1.21",
          "form": "근데 힘든 건 똑같아요 ㅎㅎ",
          "original_form": "근데 힘든 건 똑같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020562.1.22",
          "form": "전 더 쉬울 거라 생각했는데...",
          "original_form": "전 더 쉬울 거라 생각했는데...",
          "speaker_id": "1",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020562.1.23",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210828 10:02"
        },
        {
          "id": "MDRW2100020562.1.24",
          "form": "저도 더 쉬울거같다고 생각했는데",
          "original_form": "저도 더 쉬울거같다고 생각했는데",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.25",
          "form": "네네 ㅎㅎ 높은 데를 잘 올라가니까",
          "original_form": "네네 ㅎㅎ 높은 데를 잘 올라가니까",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.26",
          "form": "고양이는 알아서 잘 지내니까",
          "original_form": "고양이는 알아서 잘 지내니까",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.27",
          "form": "물건이 좀 많이 깨져요",
          "original_form": "물건이 좀 많이 깨져요",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.28",
          "form": "아하! 맞아요",
          "original_form": "아하! 맞아요",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.29",
          "form": "그렇다고 하더라구요",
          "original_form": "그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.30",
          "form": "훈련이 어려워서",
          "original_form": "훈련이 어려워서",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.31",
          "form": "행동 교정이 잘 안돼요",
          "original_form": "행동 교정이 잘 안돼요",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.32",
          "form": "훈련이 가능하긴한가요?",
          "original_form": "훈련이 가능하긴한가요?",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.33",
          "form": "시간은 덜 들어요 확실히!",
          "original_form": "시간은 덜 들어요 확실히!",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020562.1.34",
          "form": "되는 고양이들은 앉아 손도 하더라구요",
          "original_form": "되는 고양이들은 앉아 손도 하더라구요",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020562.1.35",
          "form": "보통은 안된다고 보는 게....",
          "original_form": "보통은 안된다고 보는 게....",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020562.1.36",
          "form": "와~ 신기하네요",
          "original_form": "와~ 신기하네요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020562.1.37",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020562.1.38",
          "form": "고양이 털도 엄청 빠지더라구요",
          "original_form": "고양이 털도 엄청 빠지더라구요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020562.1.39",
          "form": "강아지도 많이 빠지긴하지만요",
          "original_form": "강아지도 많이 빠지긴하지만요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        }
      ]
    }
