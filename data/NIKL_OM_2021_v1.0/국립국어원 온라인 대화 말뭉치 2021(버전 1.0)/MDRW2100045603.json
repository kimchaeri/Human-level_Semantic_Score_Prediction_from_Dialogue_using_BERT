        {
          "id": "MDRW2100045603.1.2",
          "form": "요즘 나 일풍진로 소주에 빠졌어",
          "original_form": "요즘 나 일풍진로 소주에 빠졌어",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045603.1.3",
          "form": "그거 도수 되게 높지않아?",
          "original_form": "그거 도수 되게 높지않아?",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045603.1.4",
          "form": "ㅋㅋ맞아 25도씨야",
          "original_form": "ㅋㅋ맞아 25도씨야",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045603.1.5",
          "form": "25도야",
          "original_form": "25도야",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045603.1.6",
          "form": "알콜 너무 쎈거아니야?",
          "original_form": "알콜 너무 쎈거아니야?",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.7",
          "form": "ㅋㅋ그래서 토닉워터에 섞어마셔",
          "original_form": "ㅋㅋ그래서 토닉워터에 섞어마셔",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.8",
          "form": "헐 소토닉 맛있잖아",
          "original_form": "헐 소토닉 맛있잖아",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.9",
          "form": "화요도 마셔뵐ㅆ니?",
          "original_form": "화요도 마셔뵐ㅆ니?",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.10",
          "form": "안마셔봤니?",
          "original_form": "안마셔봤니?",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.11",
          "form": "화요는 마셔봤지",
          "original_form": "화요는 마셔봤지",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.12",
          "form": "그거랑 비슷한거얌",
          "original_form": "그거랑 비슷한거얌",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045603.1.13",
          "form": "둘중엔 뭐가 더 맛있어.?",
          "original_form": "둘중엔 뭐가 더 맛있어.?",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045603.1.14",
          "form": "화요가 더 맛있긴 해",
          "original_form": "화요가 더 맛있긴 해",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045603.1.15",
          "form": "ㅋㅋㅋ가격은 어때",
          "original_form": "ㅋㅋㅋ가격은 어때",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045603.1.16",
