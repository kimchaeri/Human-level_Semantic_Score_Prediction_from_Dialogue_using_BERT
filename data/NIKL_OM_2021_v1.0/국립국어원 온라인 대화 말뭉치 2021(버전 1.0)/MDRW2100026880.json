      "utterance": [
        {
          "id": "MDRW2100026880.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026880.1.2",
          "form": "운동 하시는 거 있나요?",
          "original_form": "운동 하시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026880.1.3",
          "form": "ㅋㅋㅋ아뇨 ㅠㅠ산챗도 운동으로 쳐주시나요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아뇨 ㅠㅠ산챗도 운동으로 쳐주시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026880.1.4",
          "form": "필라테스...하고싶어하고만 있어요....",
          "original_form": "필라테스...하고싶어하고만 있어요....",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026880.1.5",
          "form": "산책도 운동이죠!",
          "original_form": "산책도 운동이죠!",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026880.1.6",
          "form": "필라테스 전에 해본 적 있는데",
          "original_form": "필라테스 전에 해본 적 있는데",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026880.1.7",
          "form": "기초체력 기르기 좋더라고요~",
          "original_form": "기초체력 기르기 좋더라고요~",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026880.1.8",
          "form": "그쵸ㅠㅠ 또 제가 자세도 안좋고 그래서 .. ㅠㅠ",
          "original_form": "그쵸ㅠㅠ 또 제가 자세도 안좋고 그래서 .. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026880.1.9",
          "form": "미루지 말고 꼭 해보세요!",
          "original_form": "미루지 말고 꼭 해보세요!",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026880.1.10",
          "form": "코로나 시국....",
          "original_form": "코로나 시국....",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026880.1.11",
          "form": "그쵸 ㅠㅠ....",
          "original_form": "그쵸 ㅠㅠ....",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026880.1.12",
          "form": "그래도 일대일이나 이대일로 하는 정도면 괜찮지 않을까하고 고민중이에요ㅠㅠ",
          "original_form": "그래도 일대일이나 이대일로 하는 정도면 괜찮지 않을까하고 고민중이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026880.1.13",
          "form": "그룹필테도 나쁘지 않았어요",
          "original_form": "그룹필테도 나쁘지 않았어요",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026880.1.14",
          "form": "그룹필테 괜찮나요?ㅋㅋㅋㅋㅋ",
          "original_form": "그룹필테 괜찮나요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026880.1.15",
          "form": "그룹필테 좋아요",
          "original_form": "그룹필테 좋아요",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026880.1.16",
          "form": "막 허리디스크 있는 거 아니면",
          "original_form": "막 허리디스크 있는 거 아니면",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026880.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026880.1.18",
          "form": "저번에 친구가 자세 세세하게 봐주는게 좀 어려워서 오히려 자세 안좋아질수도 있다길래 ㅠㅜ",
          "original_form": "저번에 친구가 자세 세세하게 봐주는게 좀 어려워서 오히려 자세 안좋아질수도 있다길래 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026880.1.19",
          "form": "강사분을 잘만나여",
          "original_form": "강사분을 잘만나여",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026880.1.20",
          "form": "야 된다더라구요 ㅠㅠ",
          "original_form": "야 된다더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026880.1.21",
          "form": "흠 맞아요",
          "original_form": "흠 맞아요",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100026880.1.22",
          "form": "자격증 같은 것도 중요해요",
          "original_form": "자격증 같은 것도 중요해요",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100026880.1.23",
          "form": "가격도...따져야 하고요",
          "original_form": "가격도...따져야 하고요",
          "speaker_id": "1",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100026880.1.24",
          "form": "ㅠㅠㅠㅠㅠ그쵸......",
          "original_form": "ㅠㅠㅠㅠㅠ그쵸......",
          "speaker_id": "2",
          "time": "20210902 00:57"
        },
        {
          "id": "MDRW2100026880.1.25",
          "form": "운동은 빠르게 할수록 좋으니까",
          "original_form": "운동은 빠르게 할수록 좋으니까",
          "speaker_id": "1",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026880.1.26",
          "form": "사실 제일 큰 고민이 가격이었어서......ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 제일 큰 고민이 가격이었어서......ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026880.1.27",
          "form": "언능 상담해보세요!",
          "original_form": "언능 상담해보세요!",
          "speaker_id": "1",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026880.1.28",
          "form": "넹넹!! ㅠㅠ",
          "original_form": "넹넹!! ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100026880.1.29",
          "form": "전 3개월 그룹필테에 48만원이었어요",
          "original_form": "전 3개월 그룹필테에 48만원이었어요",
