      "utterance": [
        {
          "id": "MDRW2100017134.1.1",
          "form": "사고 싶은 물건이 있나요?",
          "original_form": "사고 싶은 물건이 있나요?",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017134.1.2",
          "form": "최근에 옷 구매했어요",
          "original_form": "최근에 옷 구매했어요",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017134.1.3",
          "form": "저도 최근에 가을 니트 구매했어요",
          "original_form": "저도 최근에 가을 니트 구매했어요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017134.1.4",
          "form": "어떤 옷 구매하셨나요?",
          "original_form": "어떤 옷 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017134.1.5",
          "form": "저는 트렌치코트 샀어요",
          "original_form": "저는 트렌치코트 샀어요",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017134.1.6",
          "form": "어떤 색 사셨나요?",
          "original_form": "어떤 색 사셨나요?",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017134.1.7",
          "form": "저도 베이지색밖에 없어서 다른 색 구매하고싶어요",
          "original_form": "저도 베이지색밖에 없어서 다른 색 구매하고싶어요",
          "speaker_id": "1",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017134.1.8",
          "form": "저는 인디핑크만 있어서 베이지샀어요",
          "original_form": "저는 인디핑크만 있어서 베이지샀어요",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017134.1.9",
          "form": "또 사고 싶으신 거 있으신가요?",
          "original_form": "또 사고 싶으신 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017134.1.10",
          "form": "글쎄요 맛있는거 쟁이고 싶네요 ㅋㅋㅋ",
          "original_form": "글쎄요 맛있는거 쟁이고 싶네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017134.1.11",
          "form": "달다구리??",
          "original_form": "달다구리??",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017134.1.12",
          "form": "갑자기 노티드도넛이 생각나네요",
          "original_form": "갑자기 노티드도넛이 생각나네요",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017134.1.13",
          "form": "단거 엄청 좋아해요",
          "original_form": "단거 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017134.1.14",
          "form": "노티드 드셔보셨어요?",
          "original_form": "노티드 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017134.1.15",
          "form": "네 맛있더라구요!",
          "original_form": "네 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017134.1.16",
          "form": "드셔보셨나요??",
          "original_form": "드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017134.1.17",
          "form": "유명하더라구여",
          "original_form": "유명하더라구여",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017134.1.18",
          "form": "아니요 ㅠㅠ 저는 랜디스만 한번 먹어봤어요",
          "original_form": "아니요 ㅠㅠ 저는 랜디스만 한번 먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017134.1.19",
          "form": "저는 랜디스를 안먹어봤어요",
          "original_form": "저는 랜디스를 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017134.1.20",
          "form": "엄청 맛있나요?",
          "original_form": "엄청 맛있나요?",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017134.1.21",
          "form": "평일낮에 갔는데도 사람이 너무 많아서 사고픈거 반밖에 못샀어요 ㅠㅠ",
          "original_form": "평일낮에 갔는데도 사람이 너무 많아서 사고픈거 반밖에 못샀어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:39"
        }
