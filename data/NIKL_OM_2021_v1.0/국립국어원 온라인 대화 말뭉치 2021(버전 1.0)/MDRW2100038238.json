      "utterance": [
        {
          "id": "MDRW2100038238.1.1",
          "form": "근데 속성이 전기가 되거나 하는건 아니고",
          "original_form": "근데 속성이 전기가 되거나 하는건 아니고",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038238.1.2",
          "form": "무기가 활에서 한손검으로 바뀌고",
          "original_form": "무기가 활에서 한손검으로 바뀌고",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038238.1.3",
          "form": "일반공격이 원소공격되는 정도?",
          "original_form": "일반공격이 원소공격되는 정도?",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038238.1.4",
          "form": "ㅇㅇ아니 걘 대체",
          "original_form": "ㅇㅇ아니 걘 대체",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038238.1.5",
          "form": "무슨생각으로",
          "original_form": "무슨생각으로",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038238.1.6",
          "form": "사안+마왕화까지",
          "original_form": "사안+마왕화까지",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038238.1.7",
          "form": "쓰는걸까",
          "original_form": "쓰는걸까",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038238.1.8",
          "form": "수명 깎아사네",
          "original_form": "수명 깎아사네",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038238.1.9",
          "form": "근데 필요한게",
          "original_form": "근데 필요한게",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038238.1.10",
          "form": "5성 서폿이야??",
          "original_form": "5성 서폿이야??",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100038238.1.11",
          "form": "유지비 비싸네....",
          "original_form": "유지비 비싸네....",
          "speaker_id": "2",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.12",
          "form": "타탈?",
          "original_form": "타탈?",
          "speaker_id": "1",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.13",
          "form": "라이덴",
          "original_form": "라이덴",
          "speaker_id": "2",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.14",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.15",
          "form": "머 내가 아직",
          "original_form": "머 내가 아직",
          "speaker_id": "2",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.16",
          "form": "5성 물&원소 메인딜러가 필요한듯",
          "original_form": "5성 물&원소 메인딜러가 필요한듯",
          "speaker_id": "1",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.17",
          "form": "그런 조합 맞출 수준은 아니니까",
          "original_form": "그런 조합 맞출 수준은 아니니까",
          "speaker_id": "2",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.18",
          "form": "ㅇㅇ서브딜러에 가깝단 말 나오긴하더라",
          "original_form": "ㅇㅇ서브딜러에 가깝단 말 나오긴하더라",
          "speaker_id": "2",
          "time": "20210902 01:32"
        },
        {
          "id": "MDRW2100038238.1.19",
          "form": "궁쓰고 타탈로 바꿔서",
          "original_form": "궁쓰고 타탈로 바꿔서",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100038238.1.20",
          "form": "원소평타에 전기 들어가면",
          "original_form": "원소평타에 전기 들어가면",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100038238.1.21",
          "form": "세긴 하겠지만",
          "original_form": "세긴 하겠지만",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100038238.1.22",
          "form": "난 그런애 업지...",
          "original_form": "난 그런애 업지...",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100038238.1.23",
          "form": "2.2 복각 가챠 끝나고",
          "original_form": "2.2 복각 가챠 끝나고",
          "speaker_id": "1",
          "time": "20210902 07:50"
        },
        {
          "id": "MDRW2100038238.1.24",
          "form": "2.3에 남캐 대거 등장할거라는 소문이 돌던데",
          "original_form": "2.3에 남캐 대거 등장할거라는 소문이 돌던데",
          "speaker_id": "1",
          "time": "20210902 07:51"
        },
        {
          "id": "MDRW2100038238.1.25",
          "form": "어케 될지는 모르겠음...",
          "original_form": "어케 될지는 모르겠음...",
          "speaker_id": "1",
          "time": "20210902 07:51"
        },
        {
          "id": "MDRW2100038238.1.26",
          "form": "오 벚나무 씨앗 2단계 찍으면",
          "original_form": "오 벚나무 씨앗 2단계 찍으면",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038238.1.27",
          "form": "모양 바뀌네??",
          "original_form": "모양 바뀌네??",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038238.1.28",
          "form": "오 그래??",
          "original_form": "오 그래??",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100038238.1.29",
          "form": "ㅇㅇ 꽃잎이 뾰족하게 펴지는듯?",
          "original_form": "ㅇㅇ 꽃잎이 뾰족하게 펴지는듯?",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038238.1.30",
          "form": "그럼 이제",
          "original_form": "그럼 이제",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100038238.1.31",
          "form": "2단계 결계",
          "original_form": "2단계 결계",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100038238.1.32",
          "form": "통과 가능이야?",
          "original_form": "통과 가능이야?",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100038238.1.33",
          "form": "ㅇㅇ 통과 도ㅑ",
          "original_form": "ㅇㅇ 통과 도ㅑ",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038238.1.34",
          "form": "근데 신규 맵 탐험하느라 아직 안가봄",
          "original_form": "근데 신규 맵 탐험하느라 아직 안가봄",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038238.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038238.1.36",
          "form": "난 아직 멀었어....",
          "original_form": "난 아직 멀었어....",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038238.1.37",
          "form": "지금 13렙인가 14렙인가",
          "original_form": "지금 13렙인가 14렙인가",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038238.1.38",
          "form": "전에는 상자로만 파밍됐는데",
          "original_form": "전에는 상자로만 파밍됐는데",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100038238.1.39",
          "form": "이번에 신규 맵 열려서",
          "original_form": "이번에 신규 맵 열려서",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100038238.1.40",
          "form": "신상 공양하고 비경 열면 금방일듯",
          "original_form": "신상 공양하고 비경 열면 금방일듯",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038238.1.41",
          "form": "아 근데 세이라이 섬 걍 생긴게 너무 살벌하게 생겼어.....",
          "original_form": "아 근데 세이라이 섬 걍 생긴게 너무 살벌하게 생겼어.....",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038238.1.42",
          "form": "나 아직 가지도 않았어 ㅋㅋㅋㅋㅋ",
          "original_form": "나 아직 가지도 않았어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100038238.1.43",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.45",
          "form": "어제 너무 피곤해서",
          "original_form": "어제 너무 피곤해서",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.46",
          "form": "약간 화산처럼",
          "original_form": "약간 화산처럼",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.47",
          "form": "산 가운데가 텅 비어있거든?",
          "original_form": "산 가운데가 텅 비어있거든?",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.48",
          "form": "라이덴이랑 맞대결 하기 전에 있는 미니겜의 문턱을 넘지 못함...",
          "original_form": "라이덴이랑 맞대결 하기 전에 있는 미니겜의 문턱을 넘지 못함...",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.49",
          "form": "근데 그 안에 그",
          "original_form": "근데 그 안에 그",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.50",
          "form": "기괴한",
          "original_form": "기괴한",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.51",
          "form": "벌레처럼 생긴",
          "original_form": "벌레처럼 생긴",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.52",
          "form": "유적가디언이",
          "original_form": "유적가디언이",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100038238.1.53",
