      "utterance": [
        {
          "id": "MDRW2100038905.1.1",
          "form": "정말 배고프네요",
          "original_form": "정말 배고프네요",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038905.1.2",
          "form": "두유라도 하나 까서 드세요",
          "original_form": "두유라도 하나 까서 드세요",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100038905.1.3",
          "form": "너무 배고프면 속스ㅅ리잖아요",
          "original_form": "너무 배고프면 속스ㅅ리잖아요",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038905.1.4",
          "form": "편의점가서 뭐 사올까 고민중이어요ㅋㅋ",
          "original_form": "편의점가서 뭐 사올까 고민중이어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038905.1.5",
          "form": "오늘 삼만보걸었어요",
          "original_form": "오늘 삼만보걸었어요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038905.1.6",
          "form": "우와 운동을 마니하셨네요",
          "original_form": "우와 운동을 마니하셨네요",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100038905.1.7",
          "form": "수고하셨어요",
          "original_form": "수고하셨어요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038905.1.8",
          "form": "오늘은 쉬어야겠어요",
          "original_form": "오늘은 쉬어야겠어요",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100038905.1.9",
          "form": "적당히 걸어야겠어요",
          "original_form": "적당히 걸어야겠어요",
          "speaker_id": "1",
          "time": "20210903 09:12"
        },
        {
          "id": "MDRW2100038905.1.10",
          "form": "안아프고 건강하게 해야죠 ㅋㅋ",
          "original_form": "안아프고 건강하게 해야죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038905.1.11",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038905.1.12",
          "form": "전 점심 뭘 먹을지 고민중ㅋㅋ",
          "original_form": "전 점심 뭘 먹을지 고민중ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038905.1.13",
          "form": "건강하자고하는건데",
          "original_form": "건강하자고하는건데",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038905.1.14",
          "form": "전 오늘도 편의점가요",
          "original_form": "전 오늘도 편의점가요",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038905.1.15",
          "form": "저도 그럴것 같아요 아 힘듭니다",
          "original_form": "저도 그럴것 같아요 아 힘듭니다",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100038905.1.16",
          "form": "오늘 무슨일있으세요???",
          "original_form": "오늘 무슨일있으세요???",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100038905.1.17",
          "form": "ㅠ오늘까진 빡시게 일하고 낼놀려고요.",
          "original_form": "ㅠ오늘까진 빡시게 일하고 낼놀려고요.",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100038905.1.18",
          "form": "ㅜㅠ 직장인이신가요",
          "original_form": "ㅜㅠ 직장인이신가요",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100038905.1.19",
          "form": "넿ㅎ ㅠㅜ암튼 편의점에서 맛있는거 사드세요",
          "original_form": "넿ㅎ ㅠㅜ암튼 편의점에서 맛있는거 사드세요",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100038905.1.20",
          "form": "name2님도요....!",
          "original_form": "&name2&님도요....!",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100038905.1.21",
          "form": "편의점 줄여야지 줄여야지하는데",
          "original_form": "편의점 줄여야지 줄여야지하는데",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100038905.1.22",
          "form": "현실은 또 편의점이네요",
          "original_form": "현실은 또 편의점이네요",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100038905.1.23",
          "form": "그러게요 ㅋㅋ제일 가깝구 편하고 저렴하고하니깐..",
          "original_form": "그러게요 ㅋㅋ제일 가깝구 편하고 저렴하고하니깐..",
