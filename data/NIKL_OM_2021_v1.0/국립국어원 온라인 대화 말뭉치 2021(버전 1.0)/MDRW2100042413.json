      "utterance": [
        {
          "id": "MDRW2100042413.1.1",
          "form": "address만 카페에 그 엄마가 애 얼굴을 올렸구나",
          "original_form": "&address&만 카페에 그 엄마가 애 얼굴을 올렸구나",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.2",
          "form": "뭐 먹는 거 하고",
          "original_form": "뭐 먹는 거 하고",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.3",
          "form": "대답하는 거잖아ㅋㅋㅋㅋㄱ",
          "original_form": "대답하는 거잖아ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.4",
          "form": "적극적으로",
          "original_form": "적극적으로",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.5",
          "form": "합기도 하는데 뭐 띠 딴 거 하고",
          "original_form": "합기도 하는데 뭐 띠 딴 거 하고",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.6",
          "form": "사진 올렸더라고요~",
          "original_form": "사진 올렸더라고요~",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.7",
          "form": "찾아봐야겠다",
          "original_form": "찾아봐야겠다",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.8",
          "form": "그 사람들 다 보는데 얼굴을 왜 올리노",
          "original_form": "그 사람들 다 보는데 얼굴을 왜 올리노",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.9",
          "form": "근데 name3 데리러 갈 때 합기도 가서 맨날 마주치거든요ㅋㅋㅋㅋ",
          "original_form": "근데 &name3& 데리러 갈 때 합기도 가서 맨날 마주치거든요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.10",
          "form": "이해 안 되네",
          "original_form": "이해 안 되네",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.11",
          "form": "내 말이요",
          "original_form": "내 말이요",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.12",
          "form": "어 애기는 그렇다 쳐도",
          "original_form": "어 애기는 그렇다 쳐도",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.13",
          "form": "선생님한테 말도 안하고",
          "original_form": "선생님한테 말도 안하고",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.14",
          "form": "초등학생은 쫌..",
          "original_form": "초등학생은 쫌..",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042413.1.15",
          "form": "늦.엇.나보네",
          "original_form": "늦.엇.나보네",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.16",
          "form": "그리고 name4이 반에 한 명은 또 자각 염리 하는 거 같던데",
          "original_form": "그리고 &name4&이 반에 한 명은 또 자각 염리 하는 거 같던데",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.17",
          "form": "그래 애기 면 몰라도",
          "original_form": "그래 애기 면 몰라도",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.18",
          "form": "자가 경리",
          "original_form": "자가 경리",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.19",
          "form": "진짜??",
          "original_form": "진짜??",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.20",
          "form": "자가 격리",
          "original_form": "자가 격리",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.21",
          "form": "애가 일주일 정도 안왔는데",
          "original_form": "애가 일주일 정도 안왔는데",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.22",
          "form": "학교에서는 왜 아무 말이 없지??",
          "original_form": "학교에서는 왜 아무 말이 없지??",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.23",
          "form": "그 제일 작은 아이",
          "original_form": "그 제일 작은 아이",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.24",
          "form": "물론 그냥 내 느낌이 그래요",
          "original_form": "물론 그냥 내 느낌이 그래요",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.25",
          "form": "하도 학교 안 나와서",
          "original_form": "하도 학교 안 나와서",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042413.1.26",
          "form": "name4가 전화하니까 뭐 아빠가 뭐 아무데도 가면 안 된다 해서",
          "original_form": "&name4&가 전화하니까 뭐 아빠가 뭐 아무데도 가면 안 된다 해서",
