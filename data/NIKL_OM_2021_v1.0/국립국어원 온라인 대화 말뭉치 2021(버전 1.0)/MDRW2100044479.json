      "utterance": [
        {
          "id": "MDRW2100044479.1.1",
          "form": "그래도 비가 오니까 그런 거 같애",
          "original_form": "그래도 비가 오니까 그런 거 같애",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044479.1.2",
          "form": "그래도 더운 것보단 낫네요",
          "original_form": "그래도 더운 것보단 낫네요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044479.1.3",
          "form": "그러게 더우면 잠도 못 자는데",
          "original_form": "그러게 더우면 잠도 못 자는데",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044479.1.4",
          "form": "빗소리도 듣기 좋고",
          "original_form": "빗소리도 듣기 좋고",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.5",
          "form": "빗소리가 잠들기에 좋은 거 같애",
          "original_form": "빗소리가 잠들기에 좋은 거 같애",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.6",
          "form": "맞아요 빗소리 들으면 서자면 기분 좋아",
          "original_form": "맞아요 빗소리 들으면 서자면 기분 좋아",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.7",
          "form": "이제 슬 그치는 거 같은데",
          "original_form": "이제 슬 그치는 거 같은데",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.8",
          "form": "좀 아쉽네",
          "original_form": "좀 아쉽네",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.9",
          "form": "그래도 어제 오늘 많이 내렸지",
          "original_form": "그래도 어제 오늘 많이 내렸지",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.10",
          "form": "덕분에 시원하기도 했고",
          "original_form": "덕분에 시원하기도 했고",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.11",
          "form": "이제 가을비도 지나갔으니 곧 추워지겠네",
          "original_form": "이제 가을비도 지나갔으니 곧 추워지겠네",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.12",
          "form": "겨울에 많이 추우려나",
          "original_form": "겨울에 많이 추우려나",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.13",
          "form": "겨울이니까 춥지 않을까",
          "original_form": "겨울이니까 춥지 않을까",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044479.1.14",
          "form": "곧 패딩 꺼내야겠네요",
          "original_form": "곧 패딩 꺼내야겠네요",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044479.1.15",
          "form": "작년에는 그렇게 안 추웠던 거 같은데",
          "original_form": "작년에는 그렇게 안 추웠던 거 같은데",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044479.1.16",
          "form": "맞아 작년에는 패딩도 많이 안 입었어",
          "original_form": "맞아 작년에는 패딩도 많이 안 입었어",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044479.1.17",
          "form": "올해는 어떨지 모르겠네",
          "original_form": "올해는 어떨지 모르겠네",
          "speaker_id": "2",
          "time": "20210907 22:53"
        }
      ]
