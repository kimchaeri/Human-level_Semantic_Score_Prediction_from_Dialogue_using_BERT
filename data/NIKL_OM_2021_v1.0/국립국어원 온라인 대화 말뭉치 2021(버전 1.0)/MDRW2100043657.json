      "utterance": [
        {
          "id": "MDRW2100043657.1.1",
          "form": "혹시 마카롱도 좋아하세요?",
          "original_form": "혹시 마카롱도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043657.1.2",
          "form": "앗 아뇨!",
          "original_form": "앗 아뇨!",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043657.1.3",
          "form": "마카롱 좋아하세요?",
          "original_form": "마카롱 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043657.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043657.1.5",
          "form": "한때 제 최애 디저트였는데ㅠ",
          "original_form": "한때 제 최애 디저트였는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043657.1.6",
          "form": "너무 달아서 별론건가요",
          "original_form": "너무 달아서 별론건가요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043657.1.7",
          "form": "앗 네. ! 단거 잘 못먹어서요..",
          "original_form": "앗 네. ! 단거 잘 못먹어서요..",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043657.1.8",
          "form": "라고 하기엔.. 크로플도..",
          "original_form": "라고 하기엔.. 크로플도..",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043657.1.9",
          "form": "아이스크림도 장먹지만 뭔가",
          "original_form": "아이스크림도 장먹지만 뭔가",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043657.1.10",
          "form": "마카롱은 손이 잘 안가는거같어요",
          "original_form": "마카롱은 손이 잘 안가는거같어요",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043657.1.11",
          "form": "ㅠㅠ 마카롱 좋아하는 사람도많지만",
          "original_form": "ㅠㅠ 마카롱 좋아하는 사람도많지만",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043657.1.12",
          "form": "왜 좋아하는지 모르는사람도",
          "original_form": "왜 좋아하는지 모르는사람도",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043657.1.13",
          "form": "그만큼 많더라고욬ㅋㅋ",
          "original_form": "그만큼 많더라고욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043657.1.14",
          "form": "제 동생도요ㅋㅋ",
          "original_form": "제 동생도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043657.1.15",
          "form": "앗 이런 저도 언니는 마카롱 되게 좋아해요",
          "original_form": "앗 이런 저도 언니는 마카롱 되게 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043657.1.16",
          "form": "신기하네요!!",
          "original_form": "신기하네요!!",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043657.1.17",
          "form": "심각하게 좋아할땐",
          "original_form": "심각하게 좋아할땐",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043657.1.18",
          "form": "밥대신 먹기도하고",
          "original_form": "밥대신 먹기도하고",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043657.1.19",
          "form": "살이많이쪄버렸었답니다",
          "original_form": "살이많이쪄버렸었답니다",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043657.1.20",
          "form": "앟 이런..",
          "original_form": "앟 이런..",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043657.1.21",
          "form": "맞앙ㅅ 저도 아이스크림 엄청 좋아해서 많이 먹다보니..",
          "original_form": "맞앙ㅅ 저도 아이스크림 엄청 좋아해서 많이 먹다보니..",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043657.1.22",
          "form": "그럼 배라도 좋아하세요?",
          "original_form": "그럼 배라도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043657.1.23",
          "form": "당연한거 아니겠습니가..",
          "original_form": "당연한거 아니겠습니가..",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043657.1.24",
          "form": "저 하프갤런으로 쌓아두고 먹어요",
          "original_form": "저 하프갤런으로 쌓아두고 먹어요",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043657.1.25",
          "form": "wow",
          "original_form": "wow",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043657.1.26",
          "form": "최애맛은 어떤건가요",
          "original_form": "최애맛은 어떤건가요",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043657.1.27",
          "form": "전..",
          "original_form": "전..",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043657.1.28",
          "form": "역시 뉴욕치주케이크..?",
          "original_form": "역시 뉴욕치주케이크..?",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043657.1.29",
          "form": "아님 레인보우샤베트..",
          "original_form": "아님 레인보우샤베트..",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043657.1.30",
          "form": "입니다!@",
          "original_form": "입니다!@",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043657.1.31",
          "form": "헉 저랑 반대네요",
          "original_form": "헉 저랑 반대네요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043657.1.32",
