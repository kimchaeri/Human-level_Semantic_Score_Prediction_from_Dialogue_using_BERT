      "utterance": [
        {
          "id": "MDRW2100024118.1.1",
          "form": "나 신도 야근을 하나요 엔딩에 가까워져가",
          "original_form": "나 신도 야근을 하나요 엔딩에 가까워져가",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024118.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024118.1.3",
          "form": "뭔가 좀",
          "original_form": "뭔가 좀",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024118.1.4",
          "form": "진행된건 있어?",
          "original_form": "진행된건 있어?",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024118.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024118.1.6",
          "form": "지금 5-12장 플레이중인데",
          "original_form": "지금 5-12장 플레이중인데",
          "speaker_id": "2",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024118.1.7",
          "form": "스포 괜찮으니까",
          "original_form": "스포 괜찮으니까",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024118.1.8",
          "form": "억케되는지 좀...",
          "original_form": "억케되는지 좀...",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100024118.1.9",
          "form": "ㅋㅋㅋㅋㅋ근데",
          "original_form": "ㅋㅋㅋㅋㅋ근데",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.10",
          "form": "아직 2챕이면",
          "original_form": "아직 2챕이면",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.11",
          "form": "등장 안 한 인물이 너무 많아서",
          "original_form": "등장 안 한 인물이 너무 많아서",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.12",
          "form": "너무까지는 아닌가?",
          "original_form": "너무까지는 아닌가?",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.13",
          "form": "글쿤........",
          "original_form": "글쿤........",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.14",
          "form": "아무튼 좀 많은 배경설명이 필요함",
          "original_form": "아무튼 좀 많은 배경설명이 필요함",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.15",
          "form": "ㅇㅋ...",
          "original_form": "ㅇㅋ...",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100024118.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 대충 얘기해줄까?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 대충 얘기해줄까?",
          "speaker_id": "2",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024118.1.17",
          "form": "아냐 괜찮아",
          "original_form": "아냐 괜찮아",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024118.1.18",
          "form": "사실 그렇게까지 궁금하진 않음",
          "original_form": "사실 그렇게까지 궁금하진 않음",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024118.1.19",
          "form": "원신 그거",
          "original_form": "원신 그거",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024118.1.20",
          "form": "월드렙업 비경",
          "original_form": "월드렙업 비경",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024118.1.21",
          "form": "물이랑 바람 원소 필요하다는데",
          "original_form": "물이랑 바람 원소 필요하다는데",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100024118.1.22",
          "form": "걍 쌩가고 들가도 돼?",
          "original_form": "걍 쌩가고 들가도 돼?",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.24",
          "form": "이거 그 전 렙업비경이랑",
          "original_form": "이거 그 전 렙업비경이랑",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.25",
          "form": "구조 완전히 똑같아",
          "original_form": "구조 완전히 똑같아",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.26",
          "form": "ㅇㅎ그럼 추천원소는 왜 있는거임",
          "original_form": "ㅇㅎ그럼 추천원소는 왜 있는거임",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.27",
          "form": "ㅇㅏ 그럼 렙 80짜리 몹들 나오나",
          "original_form": "ㅇㅏ 그럼 렙 80짜리 몹들 나오나",
          "speaker_id": "1",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.28",
          "form": "80렙인지는 기억이 안나는데",
          "original_form": "80렙인지는 기억이 안나는데",
          "speaker_id": "2",
          "time": "20210830 22:56"
        },
        {
          "id": "MDRW2100024118.1.29",
          "form": "겁나 빣세네 진짜",
          "original_form": "겁나 빣세네 진짜",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.30",
          "form": "처음에 엄청 긴장하면서 던전 들어갔다가",
          "original_form": "처음에 엄청 긴장하면서 던전 들어갔다가",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.31",
          "form": "? 다 아는 몹들이구만",
          "original_form": "? 다 아는 몹들이구만",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.32",
          "form": "75렙에 72렙 몹들 나왓어",
          "original_form": "75렙에 72렙 몹들 나왓어",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.33",
          "form": "상태 돼서 좀 편하게 함 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "상태 돼서 좀 편하게 함 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.34",
          "form": "월드렙 비경이?",
          "original_form": "월드렙 비경이?",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.36",
          "form": "아닌가...? 여튼",
          "original_form": "아닌가...? 여튼",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.37",
          "form": "필드 잡몹들이랑",
          "original_form": "필드 잡몹들이랑",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.38",
          "form": "종류는 같은데",
          "original_form": "종류는 같은데",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024118.1.39",
          "form": "렙이 더 높았었음...",
          "original_form": "렙이 더 높았었음...",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024118.1.40",
          "form": "밥 잘 챙겨가야겟다",
          "original_form": "밥 잘 챙겨가야겟다",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024118.1.41",
          "form": "난 오히려",
          "original_form": "난 오히려",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024118.1.42",
          "form": "월드렙 5?맞나? 올릴 때보다",
          "original_form": "월드렙 5?맞나? 올릴 때보다",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024118.1.43",
          "form": "훨씬 수월하게 깼음",
          "original_form": "훨씬 수월하게 깼음",
          "speaker_id": "2",
          "time": "20210830 22:58"
        }
      ]
    }
