      "utterance": [
        {
          "id": "MDRW2100043733.1.1",
          "form": "ㅋㅋ맞아요 최근 머사셨어요?",
          "original_form": "ㅋㅋ맞아요 최근 머사셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043733.1.2",
          "form": "방금 트레이닝세트로 결제하고왔습니다",
          "original_form": "방금 트레이닝세트로 결제하고왔습니다",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043733.1.3",
          "form": "수영복도 샀어요!",
          "original_form": "수영복도 샀어요!",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043733.1.4",
          "form": "오 운동 다니시나여??ㅋㅋㅋ",
          "original_form": "오 운동 다니시나여??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043733.1.5",
          "form": "운동도 다니지만 평소에 트레이닝복을 좋아해서요 ㅎㅎ",
          "original_form": "운동도 다니지만 평소에 트레이닝복을 좋아해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043733.1.6",
          "form": "오오 그러시구나 어떤거 사셨나요??ㅋㅋ",
          "original_form": "오오 그러시구나 어떤거 사셨나요??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043733.1.7",
          "form": "히프나틱의 역대급세트요!",
          "original_form": "히프나틱의 역대급세트요!",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043733.1.8",
          "form": "아시나용",
          "original_form": "아시나용",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043733.1.9",
          "form": "아뇨 첨들어욬ㅋㅋ 유명템인가여?",
          "original_form": "아뇨 첨들어욬ㅋㅋ 유명템인가여?",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043733.1.10",
          "form": "골반이 별로없어도 주머니로 있어보이는 효과가생겨료 ㅋㅋ",
          "original_form": "골반이 별로없어도 주머니로 있어보이는 효과가생겨료 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043733.1.11",
          "form": "오좋네요 마침 제가 없는데 ^^..",
          "original_form": "오좋네요 마침 제가 없는데 ^^..",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043733.1.12",
          "form": "한번 사봐야겠어옄ㅋㅋ",
          "original_form": "한번 사봐야겠어옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043733.1.13",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043733.1.14",
          "form": "보셨나요",
          "original_form": "보셨나요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043733.1.15",
          "form": "네넼ㅋㅋㅋ 검색해봤어옄ㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋ 검색해봤어옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043733.1.16",
          "form": "저는 요즘 니트가 사고싶네여ㅋㅋ",
          "original_form": "저는 요즘 니트가 사고싶네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043733.1.17",
          "form": "어때용?",
          "original_form": "어때용?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043733.1.18",
          "form": "오 어떤니트좋아하세요",
          "original_form": "오 어떤니트좋아하세요",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043733.1.19",
          "form": "저는 피셔맨 사고파요 ㅋㅋ",
          "original_form": "저는 피셔맨 사고파요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043733.1.20",
          "form": "근데 입으려면 초겨울은 되어야할거같아여 ㅋㅋ",
