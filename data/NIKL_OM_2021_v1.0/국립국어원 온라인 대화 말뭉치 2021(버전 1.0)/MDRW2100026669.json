      "utterance": [
        {
          "id": "MDRW2100026669.1.1",
          "form": "종교 가지는거 생각해  보셨나요",
          "original_form": "종교 가지는거 생각해  보셨나요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026669.1.2",
          "form": "저는 어릴 때 교회 가서 스페셜 떡볶이를 얻어 먹었습니다",
          "original_form": "저는 어릴 때 교회 가서 스페셜 떡볶이를 얻어 먹었습니다",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026669.1.3",
          "form": "교회 아저씨가 스페셜 떡볶이를 사줬기 때문에 교회를 갔습니다",
          "original_form": "교회 아저씨가 스페셜 떡볶이를 사줬기 때문에 교회를 갔습니다",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026669.1.4",
          "form": "가끔 돈까스 도 먹었어요",
          "original_form": "가끔 돈까스 도 먹었어요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.5",
          "form": "ㅎㅎ 저도 치킨을 줬다면 개신교가 됐을 거예요",
          "original_form": "ㅎㅎ 저도 치킨을 줬다면 개신교가 됐을 거예요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.6",
          "form": "무슨 치킨을 좋아하시나요",
          "original_form": "무슨 치킨을 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.7",
          "form": "아니ㅜㅜ",
          "original_form": "아니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.8",
          "form": "손 살치킨 인가요",
          "original_form": "손 살치킨 인가요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.9",
          "form": "님은 그래서 개 신 교 가 됐나요",
          "original_form": "님은 그래서 개 신 교 가 됐나요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.10",
          "form": "아니요 먹튀 했습니다",
          "original_form": "아니요 먹튀 했습니다",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026669.1.11",
          "form": "종교를 가지는 것은 좋은 일이라고 생각합니다",
          "original_form": "종교를 가지는 것은 좋은 일이라고 생각합니다",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026669.1.12",
          "form": "그러나 저는 무교입니다",
          "original_form": "그러나 저는 무교입니다",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026669.1.13",
          "form": "ㅋㅋㅋ 경찰 아저씨! 여기 범죄자 있어요!",
          "original_form": "ㅋㅋㅋ 경찰 아저씨! 여기 범죄자 있어요!",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026669.1.14",
          "form": "저는 세례명이 있습니다",
          "original_form": "저는 세례명이 있습니다",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026669.1.15",
          "form": "그러나 저는 무교입니다",
          "original_form": "그러나 저는 무교입니다",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026669.1.16",
          "form": "고기를 안 사줬기 때문이쥬ㅎ",
          "original_form": "고기를 안 사줬기 때문이쥬ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026669.1.17",
          "form": "오세례명이 뭘까요",
          "original_form": "오세례명이 뭘까요",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.18",
          "form": "루시아요! 유아세례라ㅎ",
          "original_form": "루시아요! 유아세례라ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.19",
          "form": "와우 예쁜 이름입니다",
          "original_form": "와우 예쁜 이름입니다",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.20",
          "form": "그래서 치킨은 무슨 치킨?",
          "original_form": "그래서 치킨은 무슨 치킨?",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.21",
          "form": "쏜살치킨은 첫도전이구",
          "original_form": "쏜살치킨은 첫도전이구",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.22",
          "form": "감사합니다ㅎ",
          "original_form": "감사합니다ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.23",
          "form": "그건..",
          "original_form": "그건..",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026669.1.24",
          "form": "투비컨티뉴!",
          "original_form": "투비컨티뉴!",
          "speaker_id": "1",
          "time": "20210901 23:34"
        }
      ]
    }
