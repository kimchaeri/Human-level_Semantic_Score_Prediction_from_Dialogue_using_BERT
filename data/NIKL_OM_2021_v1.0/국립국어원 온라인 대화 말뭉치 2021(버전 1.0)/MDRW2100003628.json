        },
        {
          "id": "MDRW2100003628.1.2",
          "form": "축구얘기 맨날하던데",
          "original_form": "축구얘기 맨날하던데",
          "speaker_id": "1",
          "time": "20210821 09:13"
        },
        {
          "id": "MDRW2100003628.1.3",
          "form": "진짜 축구 자주해요?",
          "original_form": "진짜 축구 자주해요?",
          "speaker_id": "1",
          "time": "20210821 09:13"
        },
        {
          "id": "MDRW2100003628.1.4",
          "form": "족구는요?",
          "original_form": "족구는요?",
          "speaker_id": "2",
          "time": "20210821 09:13"
        },
        {
          "id": "MDRW2100003628.1.5",
          "form": "족구에요?",
          "original_form": "족구에요?",
          "speaker_id": "1",
          "time": "20210821 09:13"
        },
        {
          "id": "MDRW2100003628.1.6",
          "form": "전 축구보다 족구 엄청해서....",
          "original_form": "전 축구보다 족구 엄청해서....",
