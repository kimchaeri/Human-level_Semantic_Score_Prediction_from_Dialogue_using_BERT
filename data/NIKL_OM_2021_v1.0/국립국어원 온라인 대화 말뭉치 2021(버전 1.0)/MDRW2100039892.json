      "utterance": [
        {
          "id": "MDRW2100039892.1.1",
          "form": "운동은 하시나요?",
          "original_form": "운동은 하시나요?",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039892.1.2",
          "form": "네네 산책과 댄스합니다",
          "original_form": "네네 산책과 댄스합니다",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039892.1.3",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039892.1.4",
          "form": "댄스요??",
          "original_form": "댄스요??",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039892.1.5",
          "form": "무슨 댄스요??",
          "original_form": "무슨 댄스요??",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039892.1.6",
          "form": "저는 헬스요",
          "original_form": "저는 헬스요",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039892.1.7",
          "form": "아 전 아이돌 댄스 합니다",
          "original_form": "아 전 아이돌 댄스 합니다",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039892.1.8",
          "form": "레슨으로 배워요",
          "original_form": "레슨으로 배워요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039892.1.9",
          "form": "우와.. 아이돌 준비하시거나 그런건 아니죠?",
          "original_form": "우와.. 아이돌 준비하시거나 그런건 아니죠?",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039892.1.10",
          "form": "악 30살이에요 미련남아서 배워봅니다",
          "original_form": "악 30살이에요 미련남아서 배워봅니다",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039892.1.11",
          "form": "우와 나이대가 비슷한가봐요",
          "original_form": "우와 나이대가 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210904 08:30"
        },
        {
          "id": "MDRW2100039892.1.12",
          "form": "멋있어요",
          "original_form": "멋있어요",
          "speaker_id": "1",
          "time": "20210904 08:30"
        },
        {
          "id": "MDRW2100039892.1.13",
          "form": "오 그런가요 ㅋㅋㅋ",
          "original_form": "오 그런가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100039892.1.14",
          "form": "새롭게 하시는 운동 있으신가요",
          "original_form": "새롭게 하시는 운동 있으신가요",
          "speaker_id": "2",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100039892.1.15",
          "form": "저는 그냥 헬스해요..기구",
          "original_form": "저는 그냥 헬스해요..기구",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100039892.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100039892.1.17",
          "form": "저도 헬스 해여하는데",
          "original_form": "저도 헬스 해여하는데",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100039892.1.18",
          "form": "근육이 너무 없어서",
          "original_form": "근육이 너무 없어서",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100039892.1.19",
          "form": "자도 근육 없어요ㅠㅠㅠㅠ",
          "original_form": "자도 근육 없어요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100039892.1.20",
          "form": "해보면 좋긴하더라구요",
          "original_form": "해보면 좋긴하더라구요",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100039892.1.21",
          "form": "구쵸 근육이 많아야해요",
          "original_form": "구쵸 근육이 많아야해요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100039892.1.22",
          "form": "네 마자요 헬스하면 근육이 좀 생기는 것 같아요ㅎㅎ",
          "original_form": "네 마자요 헬스하면 근육이 좀 생기는 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100039892.1.23",
          "form": "맞아요 전 유산소만 해서 큰일이네요",
          "original_form": "맞아요 전 유산소만 해서 큰일이네요",
          "speaker_id": "2",
          "time": "20210905 11:41"
        }
      ]
