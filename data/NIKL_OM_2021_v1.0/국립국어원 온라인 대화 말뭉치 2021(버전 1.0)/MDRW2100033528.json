      "utterance": [
        {
          "id": "MDRW2100033528.1.1",
          "form": "점심은 뭐드실예정인가요ㅎ",
          "original_form": "점심은 뭐드실예정인가요ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100033528.1.2",
          "form": "김밥헤븐 가려구요. 혼밥러라서요",
          "original_form": "김밥헤븐 가려구요. 혼밥러라서요",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100033528.1.3",
          "form": "점심 뭐 드세요?",
          "original_form": "점심 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100033528.1.4",
          "form": "아 직장동료랑  같이안드세요?",
          "original_form": "아 직장동료랑  같이안드세요?",
          "speaker_id": "1",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100033528.1.5",
          "form": "전 가면서정하려구요",
          "original_form": "전 가면서정하려구요",
          "speaker_id": "1",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100033528.1.6",
          "form": "직원이 저 혼자라서요 ㅎ\r\n사장님은 맨날 나가계셔서",
          "original_form": "직원이 저 혼자라서요 ㅎ\r\n사장님은 맨날 나가계셔서",
          "speaker_id": "2",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100033528.1.7",
          "form": "아 두분이신거예요?ㅎ",
          "original_form": "아 두분이신거예요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100033528.1.8",
          "form": "맛난거드시지 왜 맨날 김천으로~",
          "original_form": "맛난거드시지 왜 맨날 김천으로~",
          "speaker_id": "1",
          "time": "20210824 11:40"
        },
        {
          "id": "MDRW2100033528.1.9",
          "form": "혼밥이라 좀 그래요. 햄버거나 천국이 맘이 편해요",
          "original_form": "혼밥이라 좀 그래요. 햄버거나 천국이 맘이 편해요",
          "speaker_id": "2",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100033528.1.10",
          "form": "사실 전 혼밥을 못해서 아마 맨날포장해서 먹을듯요ㅎㅎ",
          "original_form": "사실 전 혼밥을 못해서 아마 맨날포장해서 먹을듯요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100033528.1.11",
          "form": "굶거나",
          "original_form": "굶거나",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100033528.1.12",
          "form": "굶는건 안돼요. 뭐라도 먹어야죠",
          "original_form": "굶는건 안돼요. 뭐라도 먹어야죠",
          "speaker_id": "2",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100033528.1.13",
          "form": "전 이사님이 밥멤버라 ㅎ",
          "original_form": "전 이사님이 밥멤버라 ㅎ",
          "speaker_id": "1",
          "time": "20210824 11:44"
        },
        {
          "id": "MDRW2100033528.1.14",
          "form": "밥먹으러 가고있어요",
          "original_form": "밥먹으러 가고있어요",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033528.1.15",
          "form": "저도 밥멤 있음 좋을거 같아요.\r\n맛난거 드시고 오세요!!",
          "original_form": "저도 밥멤 있음 좋을거 같아요.\r\n맛난거 드시고 오세요!!",
          "speaker_id": "2",
          "time": "20210824 11:46"
        },
        {
          "id": "MDRW2100033528.1.16",
          "form": "국물땡겨서 바지락칼국수를 먹을까싶어요",
          "original_form": "국물땡겨서 바지락칼국수를 먹을까싶어요",
          "speaker_id": "1",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100033528.1.17",
          "form": "바지락 칼국수 맛나죠~~",
          "original_form": "바지락 칼국수 맛나죠~~",
          "speaker_id": "2",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100033528.1.18",
          "form": "name2님도 맛난걸로 드세요^^",
          "original_form": "&name2&님도 맛난걸로 드세요^^",
          "speaker_id": "1",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100033528.1.19",
          "form": "쫄면 먹고 삼실 가려구요.\r\n무ㅓ 드셨어요?",
          "original_form": "쫄면 먹고 삼실 가려구요.\r\n무ㅓ 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100033528.1.20",
          "form": "오ㅎ 쫄면 맛나죠ㅎ",
          "original_form": "오ㅎ 쫄면 맛나죠ㅎ",
