      "utterance": [
        {
          "id": "MDRW2100024485.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100024485.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100024485.1.3",
          "form": "요즘 건강을 위해 하고있는게 있나요??",
          "original_form": "요즘 건강을 위해 하고있는게 있나요??",
          "speaker_id": "2",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100024485.1.4",
          "form": "홈트 간단하게 하고 있어요",
          "original_form": "홈트 간단하게 하고 있어요",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024485.1.5",
          "form": "저도 땅끄부부나 여리나핏 많이 따라했었는데 요즘은 저스트댄스가 하고싶더라구요!",
          "original_form": "저도 땅끄부부나 여리나핏 많이 따라했었는데 요즘은 저스트댄스가 하고싶더라구요!",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100024485.1.6",
          "form": "저스트댄스 햐보셨나요?",
          "original_form": "저스트댄스 햐보셨나요?",
          "speaker_id": "2",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100024485.1.7",
          "form": "해봤어요",
          "original_form": "해봤어요",
          "speaker_id": "1",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100024485.1.8",
          "form": "진짜 힘들고 나중에 몸 엄청 쑤셔요",
          "original_form": "진짜 힘들고 나중에 몸 엄청 쑤셔요",
          "speaker_id": "1",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024485.1.9",
          "form": "저도 그렇게 운동되는 느낌이 좋은데",
          "original_form": "저도 그렇게 운동되는 느낌이 좋은데",
          "speaker_id": "2",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024485.1.10",
          "form": "제가 과연 그때까지 끈기있게 할지 모르겠네요... ㅋㅋㅋㅋㅋ",
          "original_form": "제가 과연 그때까지 끈기있게 할지 모르겠네요... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024485.1.11",
          "form": "저스트댄스에서 추천하시는 곡 있나요??",
          "original_form": "저스트댄스에서 추천하시는 곡 있나요??",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024485.1.12",
          "form": "저스트댄스하면 역시 라스푸틴이죠",
          "original_form": "저스트댄스하면 역시 라스푸틴이죠",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024485.1.13",
          "form": "라스푸틴 영상 본적 있으세요?",
          "original_form": "라스푸틴 영상 본적 있으세요?",
          "speaker_id": "1",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024485.1.14",
          "form": "네! 라스푸틴은 유명해서 저도 해봤어요 ㅋㅋㅋㅋ",
          "original_form": "네! 라스푸틴은 유명해서 저도 해봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024485.1.15",
          "form": "물론 똑같이는 못하지만...",
          "original_form": "물론 똑같이는 못하지만...",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024485.1.16",
          "form": "마지막에 날아다니는 거 정말 못하겠지 않나요?",
          "original_form": "마지막에 날아다니는 거 정말 못하겠지 않나요?",
          "speaker_id": "2",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024485.1.17",
          "form": "맞아요ㅋㅋㅋ 저 그거 하다가 몇번은 엉덩방아 찧었어요",
          "original_form": "맞아요ㅋㅋㅋ 저 그거 하다가 몇번은 엉덩방아 찧었어요",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024485.1.18",
          "form": "그럼에도 불구하고 하게되는 마성의 춤ㅋㅋ",
          "original_form": "그럼에도 불구하고 하게되는 마성의 춤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024485.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 열심히 하시는군요...!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 열심히 하시는군요...!",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024485.1.20",
          "form": "혹시 닌텐도가 있으신건가요?",
          "original_form": "혹시 닌텐도가 있으신건가요?",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024485.1.21",
          "form": "아니요 예전에 친척한테 빌려서 했어요",
          "original_form": "아니요 예전에 친척한테 빌려서 했어요",
          "speaker_id": "1",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100024485.1.22",
          "form": "아 그러시군요 !",
          "original_form": "아 그러시군요 !",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100024485.1.23",
          "form": "링피트도 해보고싶었는데 혹시 해보셨나 해서 여쭤봤어요 ㅋㅋㅋㅋ",
          "original_form": "링피트도 해보고싶었는데 혹시 해보셨나 해서 여쭤봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024485.1.24",
          "form": "저는 저스트댄스 어플로 결제하려구요!!",
          "original_form": "저는 저스트댄스 어플로 결제하려구요!!",
          "speaker_id": "2",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024485.1.25",
          "form": "링피트는 못해봤고 예전에 나왔던 위핏은 해봤어요",
          "original_form": "링피트는 못해봤고 예전에 나왔던 위핏은 해봤어요",
          "speaker_id": "1",
          "time": "20210831 11:44"
        },
        {
          "id": "MDRW2100024485.1.26",
          "form": "어플로 결제하면 기기 없어도 되나요?",
          "original_form": "어플로 결제하면 기기 없어도 되나요?",
          "speaker_id": "1",
          "time": "20210831 11:44"
        },
        {
          "id": "MDRW2100024485.1.27",
          "form": "아하 위핏도 비슷한건가보네요!",
          "original_form": "아하 위핏도 비슷한건가보네요!",
          "speaker_id": "2",
          "time": "20210831 11:44"
        },
        {
          "id": "MDRW2100024485.1.28",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "2",
          "time": "20210831 11:45"
        },
        {
          "id": "MDRW2100024485.1.29",
          "form": "네! 노트북이나 탭으로 화면보고, 핸드폰 들고 춤추면 돼요!!",
          "original_form": "네! 노트북이나 탭으로 화면보고, 핸드폰 들고 춤추면 돼요!!",
          "speaker_id": "2",
          "time": "20210831 11:45"
        },
        {
          "id": "MDRW2100024485.1.30",
          "form": "와 저도 어플 결제해야겠네요",
          "original_form": "와 저도 어플 결제해야겠네요",
          "speaker_id": "1",
          "time": "20210831 11:46"
        }
      ]
