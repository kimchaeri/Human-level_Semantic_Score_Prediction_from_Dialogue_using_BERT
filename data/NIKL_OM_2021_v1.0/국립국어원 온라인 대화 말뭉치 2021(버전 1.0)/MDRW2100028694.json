        },
        {
          "id": "MDRW2100028694.1.2",
          "form": "재택근무예요",
          "original_form": "재택근무예요",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028694.1.3",
          "form": "항상..",
          "original_form": "항상..",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028694.1.4",
          "form": "유머감각이 있으시네요",
          "original_form": "유머감각이 있으시네요",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028694.1.5",
          "form": "저는 요즘 재택 근무만 하니까",
          "original_form": "저는 요즘 재택 근무만 하니까",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028694.1.6",
          "form": "너무 무료해요",
          "original_form": "너무 무료해요",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028694.1.7",
          "form": "다들 재택근무가 좋다고들 하더군요",
          "original_form": "다들 재택근무가 좋다고들 하더군요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028694.1.8",
          "form": "그렇기도 하시겠네요",
          "original_form": "그렇기도 하시겠네요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028694.1.9",
          "form": "아니예요 오래하면 대면이 더 좋아요",
          "original_form": "아니예요 오래하면 대면이 더 좋아요",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028694.1.10",
          "form": "체육은 대면수업이 더 좋을것같아요",
          "original_form": "체육은 대면수업이 더 좋을것같아요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028694.1.11",
          "form": "맞아요 이론 수업 밖에 안해서",
          "original_form": "맞아요 이론 수업 밖에 안해서",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028694.1.12",
          "form": "저뿐만아니라",
          "original_form": "저뿐만아니라",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028694.1.13",
          "form": "학생들도 먾이 지루해요",
          "original_form": "학생들도 먾이 지루해요",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028694.1.14",
          "form": "체육은 뛰면서 하는게 좋은대..아쉽네요",
          "original_form": "체육은 뛰면서 하는게 좋은대..아쉽네요",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028694.1.15",
          "form": "맞아요. 다음주에 끝나니까",
          "original_form": "맞아요. 다음주에 끝나니까",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028694.1.16",
          "form": "그래도 괜찮아요",
          "original_form": "그래도 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028694.1.17",
          "form": "재택근무하시는분 많은것같아요",
          "original_form": "재택근무하시는분 많은것같아요",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028694.1.18",
          "form": "맞아요 선생님들도 다 하시고",
          "original_form": "맞아요 선생님들도 다 하시고",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028694.1.19",
          "form": "다음주부터는 전면등교니 재택근무안하시겠네요",
          "original_form": "다음주부터는 전면등교니 재택근무안하시겠네요",
          "speaker_id": "2",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028694.1.20",
          "form": "회사원도 하시고",
          "original_form": "회사원도 하시고",
          "speaker_id": "1",
          "time": "20210902 21:25"
        },
        {
          "id": "MDRW2100028694.1.21",
          "form": "주부도 하시고^^",
          "original_form": "주부도 하시고^^",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028694.1.22",
          "form": "그런데 언젠가는 재택근무가 그리운 날이 올 것 같아요",
          "original_form": "그런데 언젠가는 재택근무가 그리운 날이 올 것 같아요",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028694.1.23",
          "form": "코로나 없어지면 재택근무 없어지겠네요",
          "original_form": "코로나 없어지면 재택근무 없어지겠네요",
          "speaker_id": "2",
          "time": "20210902 21:26"
