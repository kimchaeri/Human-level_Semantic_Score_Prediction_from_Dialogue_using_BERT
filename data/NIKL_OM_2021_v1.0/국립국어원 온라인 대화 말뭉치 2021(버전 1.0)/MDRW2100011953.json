      "utterance": [
        {
          "id": "MDRW2100011953.1.1",
          "form": "내 얼굴 악건성이라서",
          "original_form": "내 얼굴 악건성이라서",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011953.1.2",
          "form": "매우 건조하다",
          "original_form": "매우 건조하다",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.3",
          "form": "하긴 언니는 피부가 좋으니깐ㅋ",
          "original_form": "하긴 언니는 피부가 좋으니깐ㅋ",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.4",
          "form": "여름에도 크림까지 발라야되서",
          "original_form": "여름에도 크림까지 발라야되서",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.5",
          "form": "그래 입술도 건조해서 매일바르잖아",
          "original_form": "그래 입술도 건조해서 매일바르잖아",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.6",
          "form": "수분크림 바르니까 건조하고",
          "original_form": "수분크림 바르니까 건조하고",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.8",
          "form": "어어 그렇지",
          "original_form": "어어 그렇지",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.9",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.10",
          "form": "수분크림은 건조하지ㅜ",
          "original_form": "수분크림은 건조하지ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.11",
          "form": "오일발랐는데 여름되니 좀 끈적여서",
          "original_form": "오일발랐는데 여름되니 좀 끈적여서",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.12",
          "form": "나두 요즘 피부가 막 뒤집어져서ㅜㅜ아고",
          "original_form": "나두 요즘 피부가 막 뒤집어져서ㅜㅜ아고",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.13",
          "form": "한번 사봤는데",
          "original_form": "한번 사봤는데",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.14",
          "form": "진짜 마스크때문에 더한듯",
          "original_form": "진짜 마스크때문에 더한듯",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.15",
          "form": "그래 입술옆에 뭐 낫드만",
          "original_form": "그래 입술옆에 뭐 낫드만",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011953.1.16",
          "form": "글타 나도 마스크때문에 자잘한거 많이나서",
          "original_form": "글타 나도 마스크때문에 자잘한거 많이나서",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.17",
          "form": "응 안맞다ㅜㅋㄱㅋㅋㅋㅋ",
          "original_form": "응 안맞다ㅜㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.18",
          "form": "맨날 면봉으로 짜고 있음",
          "original_form": "맨날 면봉으로 짜고 있음",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.19",
          "form": "내말이 ㅜ",
          "original_form": "내말이 ㅜ",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.20",
          "form": "그래두 언니는 좋아보인다ㅋㅋ 내 피부에 맞는 화장품을 써야되는데",
          "original_form": "그래두 언니는 좋아보인다ㅋㅋ 내 피부에 맞는 화장품을 써야되는데",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.21",
          "form": "선물받은게 안맞나봐",
          "original_form": "선물받은게 안맞나봐",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.22",
          "form": "더 뒤집어지네",
          "original_form": "더 뒤집어지네",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.23",
          "form": "아하 그렇구막",
          "original_form": "아하 그렇구막",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011953.1.24",
          "form": "응응 장난아니다.",
          "original_form": "응응 장난아니다.",
          "speaker_id": "1",
          "time": "20210824 17:48"
        }
      ]
