      "utterance": [
        {
          "id": "MDRW2100015499.1.1",
          "form": "여행가면 주로 무슨 기념품을사시나요",
          "original_form": "여행가면 주로 무슨 기념품을사시나요",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015499.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015499.1.3",
          "form": "특산물정도????",
          "original_form": "특산물정도????",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015499.1.4",
          "form": "특산물! 기억에남으시는것있나요?",
          "original_form": "특산물! 기억에남으시는것있나요?",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015499.1.5",
          "form": "펑리수랑",
          "original_form": "펑리수랑",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015499.1.6",
          "form": "그거뭐죠과자? 이름이 기억이안나네",
          "original_form": "그거뭐죠과자? 이름이 기억이안나네",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015499.1.7",
          "form": "그거사왓어요ㅋㅋㅋ",
          "original_form": "그거사왓어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015499.1.8",
          "form": "특산물이라기보단 맛난거네요ㅋㅋㅋ",
          "original_form": "특산물이라기보단 맛난거네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015499.1.9",
          "form": "우와! 대만이 먹거리 천국이라던데",
          "original_form": "우와! 대만이 먹거리 천국이라던데",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015499.1.10",
          "form": "ㅋㅋㅋ 한국인입맛에",
          "original_form": "ㅋㅋㅋ 한국인입맛에",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015499.1.11",
          "form": "아주아주",
          "original_form": "아주아주",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015499.1.12",
          "form": "딱이랍니다",
          "original_form": "딱이랍니다",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015499.1.13",
          "form": "대만 카스테라도 드셨나요?",
          "original_form": "대만 카스테라도 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015499.1.14",
          "form": "네네 먹어봤죠",
          "original_form": "네네 먹어봤죠",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100015499.1.15",
          "form": "우육면도 먹고왔답니다ㅋㅋㅋ",
          "original_form": "우육면도 먹고왔답니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100015499.1.16",
          "form": "우와 ㅎㅎ부러워요  저도 코로나 풀리면 가보고 싶어요",
          "original_form": "우와 ㅎㅎ부러워요  저도 코로나 풀리면 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100015499.1.17",
          "form": "저도 가고싶네요ㅠ",
          "original_form": "저도 가고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100015499.1.18",
          "form": "제주도도요",
          "original_form": "제주도도요",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100015499.1.19",
          "form": "제주도는 정말 많이 가더라구요 요즘",
          "original_form": "제주도는 정말 많이 가더라구요 요즘",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100015499.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100015499.1.21",
          "form": "제주도가 진짜좋긴하죠",
          "original_form": "제주도가 진짜좋긴하죠",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100015499.1.22",
          "form": "제주도는 바다 색이 참 예쁜 것 같아요",
          "original_form": "제주도는 바다 색이 참 예쁜 것 같아요",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100015499.1.23",
          "form": "그리고 맛있는 음식이",
          "original_form": "그리고 맛있는 음식이",
          "speaker_id": "2",
          "time": "20210826 13:52"
        }
