      "utterance": [
        {
          "id": "MDRW2100033621.1.1",
          "form": "세탁기종류는 엘지아닐까요~~",
          "original_form": "세탁기종류는 엘지아닐까요~~",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100033621.1.2",
          "form": "ㅎ근데 세탁기 삼성써요~",
          "original_form": "ㅎ근데 세탁기 삼성써요~",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100033621.1.3",
          "form": "가전은 엘지인가요ㅎㅎ",
          "original_form": "가전은 엘지인가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100033621.1.4",
          "form": "회사동생 신혼가전으로 오브제로 쫙 뽑아더라구요",
          "original_form": "회사동생 신혼가전으로 오브제로 쫙 뽑아더라구요",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100033621.1.5",
          "form": "오~~~ 좋겠어요",
          "original_form": "오~~~ 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100033621.1.6",
          "form": "고장이나야 바꿀텐데 다 한번씩 바꾼거라 몇년은 더 쓸듯해요",
          "original_form": "고장이나야 바꿀텐데 다 한번씩 바꾼거라 몇년은 더 쓸듯해요",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100033621.1.7",
          "form": "남친이  좋은회사라 직원할인도되고 ㅎ",
          "original_form": "남친이  좋은회사라 직원할인도되고 ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100033621.1.8",
          "form": "2천만원 넘게산거같은데",
          "original_form": "2천만원 넘게산거같은데",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100033621.1.9",
          "form": "할인이 많이됐더라구요",
          "original_form": "할인이 많이됐더라구요",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100033621.1.10",
          "form": "두고쓸꺼니 이쁘고 맘에드는거 사야겠더라구요",
          "original_form": "두고쓸꺼니 이쁘고 맘에드는거 사야겠더라구요",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100033621.1.11",
          "form": "결혼은 내년5월인데 ㅋ 남자가먼저들어가살아서",
          "original_form": "결혼은 내년5월인데 ㅋ 남자가먼저들어가살아서",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100033621.1.12",
          "form": "넘일찍산거아닌가싶기도",
          "original_form": "넘일찍산거아닌가싶기도",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100033621.1.13",
          "form": "ㅋㅋ 혼자 살아요?",
          "original_form": "ㅋㅋ 혼자 살아요?",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033621.1.14",
          "form": "이거 붙여썼더니 나쁜말이래요",
          "original_form": "이거 붙여썼더니 나쁜말이래요",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033621.1.15",
          "form": "암튼 그거 위험한데~~~",
          "original_form": "암튼 그거 위험한데~~~",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033621.1.16",
          "form": "네 먼저 들어가사는데",
          "original_form": "네 먼저 들어가사는데",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033621.1.17",
          "form": "아직 가구는안사고 티비랑",
          "original_form": "아직 가구는안사고 티비랑",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033621.1.18",
          "form": "우선 가전만 산듯요",
          "original_form": "우선 가전만 산듯요",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100033621.1.19",
          "form": "제친구가 그렇게했다가 남자 바람나서 파혼했더라구요",
          "original_form": "제친구가 그렇게했다가 남자 바람나서 파혼했더라구요",
          "speaker_id": "2",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100033621.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100033621.1.21",
          "form": "가전가구도 못빼왔데요",
          "original_form": "가전가구도 못빼왔데요",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100033621.1.22",
          "form": "저런 ㅡㅡ",
          "original_form": "저런 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100033621.1.23",
          "form": "못가져가게했다고그랬나 치사스럽다고 두고왔다고 그런건지...",
          "original_form": "못가져가게했다고그랬나 치사스럽다고 두고왔다고 그런건지...",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100033621.1.24",
          "form": "역시 식들어갈때까진 아무도모르는건가봐요",
          "original_form": "역시 식들어갈때까진 아무도모르는건가봐요",
          "speaker_id": "1",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100033621.1.25",
          "form": "그럼요.  들어갔다가도 돌아나올판인데요",
          "original_form": "그럼요.  들어갔다가도 돌아나올판인데요",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100033621.1.26",
          "form": "그러게요 신혼살림은 직전에 사는걸로ㅎㅎ",
          "original_form": "그러게요 신혼살림은 직전에 사는걸로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100033621.1.27",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100033621.1.28",
          "form": "전 냉장고 얼음냉장고 사려고 계약했는데 사는집이 지하수라고ㅠ",
          "original_form": "전 냉장고 얼음냉장고 사려고 계약했는데 사는집이 지하수라고ㅠ",
          "speaker_id": "2",
          "time": "20210824 17:55"
        }
      ]
