      "utterance": [
        {
          "id": "MDRW2100006485.1.1",
          "form": "콜라 좋아하세요?",
          "original_form": "콜라 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006485.1.2",
          "form": "네 완전좋아해요",
          "original_form": "네 완전좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006485.1.3",
          "form": "오오 치킨이랑 먹으면 너무 좋아요 ㅠㅠ",
          "original_form": "오오 치킨이랑 먹으면 너무 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006485.1.4",
          "form": "맞아요 전 치맥보다 치킨에콜라에요",
          "original_form": "맞아요 전 치맥보다 치킨에콜라에요",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006485.1.5",
          "form": "맞아요 !! 저도요 ㅋㅋ",
          "original_form": "맞아요 !! 저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006485.1.6",
          "form": "술을 못마셔서 ㅎ",
          "original_form": "술을 못마셔서 ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006485.1.7",
          "form": "저도 알쓰에요ㅠㅜ",
          "original_form": "저도 알쓰에요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006485.1.8",
          "form": "오오 저랑 같네요!!",
          "original_form": "오오 저랑 같네요!!",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006485.1.9",
          "form": "술 잘마시는사람 부러워요 ㅋㅋ",
          "original_form": "술 잘마시는사람 부러워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006485.1.10",
          "form": "맞아요ㅠㅜ",
          "original_form": "맞아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100006485.1.11",
          "form": "저 완전 못 마셔요",
          "original_form": "저 완전 못 마셔요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006485.1.12",
          "form": "그래도 술안주는 잘먹어요 ㅋㅋ",
          "original_form": "그래도 술안주는 잘먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100006485.1.13",
          "form": "ㅋㅋㅋㅋ저두에요",
          "original_form": "ㅋㅋㅋㅋ저두에요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100006485.1.14",
          "form": "술안주 국물이 좋아요 저는 ㅋㅋ",
          "original_form": "술안주 국물이 좋아요 저는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100006485.1.15",
          "form": "저두요 부대찌개나 국물 떡볶이...",
          "original_form": "저두요 부대찌개나 국물 떡볶이...",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100006485.1.16",
          "form": "우아 국물떡볶이 ㅠㅠㅠ",
          "original_form": "우아 국물떡볶이 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100006485.1.17",
          "form": "조개탕도 너무 좋아요 ㅋㅋ",
          "original_form": "조개탕도 너무 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100006485.1.18",
          "form": "아 조개탕 좋죠 역전할매",
          "original_form": "아 조개탕 좋죠 역전할매",
