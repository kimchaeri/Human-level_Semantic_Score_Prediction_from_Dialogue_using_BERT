      "utterance": [
        {
          "id": "MDRW2100038219.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100038219.1.2",
          "form": "네 안냐세여",
          "original_form": "네 안냐세여",
          "speaker_id": "2",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038219.1.3",
          "form": "혹시야식드셧나요?",
          "original_form": "혹시야식드셧나요?",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038219.1.4",
          "form": "먹고싶네여",
          "original_form": "먹고싶네여",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038219.1.5",
          "form": "라면같은거",
          "original_form": "라면같은거",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038219.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 01:15"
        },
        {
          "id": "MDRW2100038219.1.7",
          "form": "저는 야밤에 주먹밥만들어먹엇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 야밤에 주먹밥만들어먹엇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038219.1.8",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038219.1.9",
          "form": "안에 뭐뭐 넣었어요 조리법 알려주세여",
          "original_form": "안에 뭐뭐 넣었어요 조리법 알려주세여",
          "speaker_id": "2",
          "time": "20210902 01:23"
        },
        {
          "id": "MDRW2100038219.1.10",
          "form": "저는 참치마요주먹밥 젤 좋아해서 참치마요랑 다진양파에다가 단무지잘게썰고 김자반이랑 비벼서 뭉쳐서먹었어요~",
          "original_form": "저는 참치마요주먹밥 젤 좋아해서 참치마요랑 다진양파에다가 단무지잘게썰고 김자반이랑 비벼서 뭉쳐서먹었어요~",
          "speaker_id": "1",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038219.1.11",
          "form": "오 굿굿",
          "original_form": "오 굿굿",
          "speaker_id": "2",
          "time": "20210902 01:25"
        },
        {
          "id": "MDRW2100038219.1.12",
          "form": "저는 최근에 마라탕 배달시켜먹었다가",
          "original_form": "저는 최근에 마라탕 배달시켜먹었다가",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038219.1.13",
          "form": "맛없는데 걸려서 혼났어요",
          "original_form": "맛없는데 걸려서 혼났어요",
          "speaker_id": "2",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100038219.1.14",
          "form": "오이런.. 마라탕 점바점 아주심하죠 ㅠㅠ",
          "original_form": "오이런.. 마라탕 점바점 아주심하죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038219.1.15",
          "form": "1인분인에 양은 또 많아서",
          "original_form": "1인분인에 양은 또 많아서",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100038219.1.16",
          "form": "라면스프로 겨우 맛을 살려냈죠 ㅎㅎ",
          "original_form": "라면스프로 겨우 맛을 살려냈죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100038219.1.17",
          "form": "헕ㅋㅋㅋ그정도였나여 ㅜㅜ",
          "original_form": "헕ㅋㅋㅋ그정도였나여 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100038219.1.18",
          "form": "네 힘든 메뉴였죠",
          "original_form": "네 힘든 메뉴였죠",
          "speaker_id": "2",
          "time": "20210902 14:05"
        },
        {
          "id": "MDRW2100038219.1.19",
          "form": "담부턴 배달시킬때 평점이랑 꼼꼼히 봐야겠어요",
          "original_form": "담부턴 배달시킬때 평점이랑 꼼꼼히 봐야겠어요",
