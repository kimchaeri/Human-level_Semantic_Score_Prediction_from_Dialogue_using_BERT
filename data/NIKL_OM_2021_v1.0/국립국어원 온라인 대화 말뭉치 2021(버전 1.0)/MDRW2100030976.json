      "utterance": [
        {
          "id": "MDRW2100030976.1.1",
          "form": "닭고기 좋아하세요?",
          "original_form": "닭고기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 15:01"
        },
        {
          "id": "MDRW2100030976.1.2",
          "form": "저 닭 진짜 좋아합니다",
          "original_form": "저 닭 진짜 좋아합니다",
          "speaker_id": "1",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100030976.1.3",
          "form": "그쵸 치킨도 맛있고 삼계탕도 맛있고",
          "original_form": "그쵸 치킨도 맛있고 삼계탕도 맛있고",
          "speaker_id": "2",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100030976.1.4",
          "form": "찜닭도",
          "original_form": "찜닭도",
          "speaker_id": "2",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100030976.1.5",
          "form": "닭이 참 좋은 식재료에요",
          "original_form": "닭이 참 좋은 식재료에요",
          "speaker_id": "2",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100030976.1.6",
          "form": "그쵸 저 어제 찜닭 먹었었는데",
          "original_form": "그쵸 저 어제 찜닭 먹었었는데",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100030976.1.7",
          "form": "오늘 뭐 드셨나요?",
          "original_form": "오늘 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100030976.1.8",
          "form": "오늘은 그냥 과일스무디 먹었어요",
          "original_form": "오늘은 그냥 과일스무디 먹었어요",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100030976.1.9",
          "form": "블루베리 토마토",
          "original_form": "블루베리 토마토",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100030976.1.10",
          "form": "저녁은 맛잇게 드셨나요",
          "original_form": "저녁은 맛잇게 드셨나요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100030976.1.11",
          "form": "저 아직 안 먹었는데",
          "original_form": "저 아직 안 먹었는데",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100030976.1.12",
          "form": "이따 불고기 먹으려구요",
          "original_form": "이따 불고기 먹으려구요",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100030976.1.13",
          "form": "불고기 좋아하시나요",
          "original_form": "불고기 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100030976.1.14",
          "form": "네 넘 맛나죠",
          "original_form": "네 넘 맛나죠",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100030976.1.15",
          "form": "전 당면넣어먹는것도 좋아해요",
          "original_form": "전 당면넣어먹는것도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100030976.1.16",
          "form": "우와 당면은 한 번도 안 넣어봤는데",
          "original_form": "우와 당면은 한 번도 안 넣어봤는데",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100030976.1.17",
          "form": "나중에 넣어봐야겠네요",
          "original_form": "나중에 넣어봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100030976.1.18",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100030976.1.19",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100030976.1.20",
          "form": "코다리찜",
          "original_form": "코다리찜",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100030976.1.21",
          "form": "오오 코다리찜",
          "original_form": "오오 코다리찜",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100030976.1.22",
          "form": "코다리 좋아하시나요",
          "original_form": "코다리 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100030976.1.23",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100030976.1.24",
          "form": "풀치도 좋고",
          "original_form": "풀치도 좋고",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100030976.1.25",
          "form": "오 풀치는 처음 들어봐서",
          "original_form": "오 풀치는 처음 들어봐서",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100030976.1.26",
          "form": "검색해봤는데",
          "original_form": "검색해봤는데",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100030976.1.27",
          "form": "어떤 식감인가요? 고등어류?",
          "original_form": "어떤 식감인가요? 고등어류?",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100030976.1.28",
          "form": "아기 갈치를 풀치라고 하더라구요",
          "original_form": "아기 갈치를 풀치라고 하더라구요",
          "speaker_id": "2",
          "time": "20210908 06:30"
        }
      ]
    }
