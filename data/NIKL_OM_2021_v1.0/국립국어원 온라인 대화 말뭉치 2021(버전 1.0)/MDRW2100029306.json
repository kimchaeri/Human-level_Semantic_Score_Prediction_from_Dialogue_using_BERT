        {
          "id": "MDRW2100029306.1.1",
          "form": "짬뽕 . 짜장면",
          "original_form": "짬뽕 . 짜장면",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029306.1.2",
          "form": "저는 짜장ㅋㅋ",
          "original_form": "저는 짜장ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029306.1.3",
          "form": "저도요",
          "original_form": "저도요{emoji:✋}{emoji:✋}{emoji:✋}",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029306.1.4",
          "form": "쟁반짜장도 좋아요",
          "original_form": "쟁반짜장도 좋아요",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.5",
          "form": "짬뽕은 국물이 좋은데 짜장은 면도 좋아요",
          "original_form": "짬뽕은 국물이 좋은데 짜장은 면도 좋아요",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.6",
          "form": "쟁반짜장 맛있죠",
          "original_form": "쟁반짜장 맛있죠",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.7",
          "form": "저는 간짜장파",
          "original_form": "저는 간짜장파",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.8",
          "form": "곱창드세요??",
          "original_form": "곱창드세요??",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.9",
          "form": "아간짜장",
          "original_form": "아간짜장",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.10",
          "form": "양파듬뿍!",
          "original_form": "양파듬뿍!",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029306.1.11",
          "form": "간짜장 양파는먹어요,,,",
          "original_form": "간짜장 양파는먹어요,,,",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.12",
          "form": "곱창은 먹긴 한데",
          "original_form": "곱창은 먹긴 한데",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.13",
          "form": "좋아하지는 않아요",
          "original_form": "좋아하지는 않아요",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.14",
          "form": "무슨 맛인지 사실 모르겠어욬ㅋㅋ",
          "original_form": "무슨 맛인지 사실 모르겠어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.15",
          "form": "헉..아쉬운마음",
          "original_form": "헉..아쉬운마음",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.16",
          "form": "그럼 칼국수. 수제비",
          "original_form": "그럼 칼국수. 수제비",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.17",
          "form": "제가 맛있는 집을 못가서 그럴수도 있어요 ㅋㅋㅋ",
          "original_form": "제가 맛있는 집을 못가서 그럴수도 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.18",
          "form": "전 수제비",
          "original_form": "전 수제비",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029306.1.19",
          "form": "사실 칼국수랑 수제비는 들깨면 무조건 좋아요",
          "original_form": "사실 칼국수랑 수제비는 들깨면 무조건 좋아요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029306.1.20",
          "form": "아 들깨 걸쭉하니 맛있죠ㅠ",
          "original_form": "아 들깨 걸쭉하니 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029306.1.21",
          "form": "들깨 너무 맛있어요",
          "original_form": "들깨 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029306.1.22",
          "form": "삼계탕에도 들깨 맛있더라고요",
          "original_form": "삼계탕에도 들깨 맛있더라고요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029306.1.23",
          "form": "삼계탕들깨는 안먹어봤어요",
          "original_form": "삼계탕들깨는 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029306.1.24",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029306.1.25",
          "form": "진짜 맛있어요",
          "original_form": "진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029306.1.26",
          "form": "넣은사람 천재같아요",
          "original_form": "넣은사람 천재같아요",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029306.1.27",
          "form": "먹으면서 느껴지는 몸보신",
          "original_form": "먹으면서 느껴지는 몸보신",
          "speaker_id": "1",
          "time": "20210903 01:20"
