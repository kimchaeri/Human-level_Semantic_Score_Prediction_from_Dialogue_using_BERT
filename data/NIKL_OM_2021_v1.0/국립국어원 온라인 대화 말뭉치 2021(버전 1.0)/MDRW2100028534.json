      "utterance": [
        {
          "id": "MDRW2100028534.1.1",
          "form": "일 힘들지 않았어?",
          "original_form": "일 힘들지 않았어?",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028534.1.2",
          "form": "힘들지",
          "original_form": "힘들지",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028534.1.3",
          "form": "일은 다 힘들어",
          "original_form": "일은 다 힘들어",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028534.1.4",
          "form": "무슨일을 하든",
          "original_form": "무슨일을 하든",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028534.1.5",
          "form": "너는 힘들어?",
          "original_form": "너는 힘들어?",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028534.1.6",
          "form": "일 많아서 힘들지",
          "original_form": "일 많아서 힘들지",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028534.1.7",
          "form": "요새?",
          "original_form": "요새?",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028534.1.8",
          "form": "응 쿠키 많이 만들어서",
          "original_form": "응 쿠키 많이 만들어서",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028534.1.9",
          "form": "몇개?",
          "original_form": "몇개?",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028534.1.10",
          "form": "종류별 전부 다",
          "original_form": "종류별 전부 다",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028534.1.11",
          "form": "몇개?",
          "original_form": "몇개?",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028534.1.12",
          "form": "수 백개씩",
          "original_form": "수 백개씩",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028534.1.13",
          "form": "어디에서 주문했어?",
          "original_form": "어디에서 주문했어?",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028534.1.14",
          "form": "여기저기서 ㅋㅋ",
          "original_form": "여기저기서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028534.1.15",
          "form": "쿠키종류 뭐있어?",
          "original_form": "쿠키종류 뭐있어?",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028534.1.16",
          "form": "초코칩 오트밀 오렌지 무화과 녹차 등등",
          "original_form": "초코칩 오트밀 오렌지 무화과 녹차 등등",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028534.1.17",
          "form": "뭐가 젤 많이 팔려?",
          "original_form": "뭐가 젤 많이 팔려?",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028534.1.18",
          "form": "초코칩",
          "original_form": "초코칩",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028534.1.19",
          "form": "빵이랑 제과 전부 다 포함해서 뭐가 만들기 제일 어려워?",
          "original_form": "빵이랑 제과 전부 다 포함해서 뭐가 만들기 제일 어려워?",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028534.1.20",
          "form": "다 어려워",
          "original_form": "다 어려워",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028534.1.21",
          "form": "제일 만들기 어려운게 뭔데?",
          "original_form": "제일 만들기 어려운게 뭔데?",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028534.1.22",
          "form": "케이크같아",
          "original_form": "케이크같아",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028534.1.23",
          "form": "무슨 케이크 만들수 있어?",
          "original_form": "무슨 케이크 만들수 있어?",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028534.1.24",
          "form": "뭐든 만들 수 있긴한데 잘 안 만들어",
