        {
          "id": "MDRW2100008152.1.1",
          "form": "요즘 날씨가 너무 오락가락하네요",
          "original_form": "요즘 날씨가 너무 오락가락하네요",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008152.1.2",
          "form": "비온다",
          "original_form": "비온다",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008152.1.3",
          "form": "장마다",
          "original_form": "장마다",
          "speaker_id": "2",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008152.1.4",
          "form": "비가 내렸다 그쳤다",
          "original_form": "비가 내렸다 그쳤다",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008152.1.5",
          "form": "요즘 날씨 때문에 나갈 때 우산 필수인 듯",
          "original_form": "요즘 날씨 때문에 나갈 때 우산 필수인 듯",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008152.1.6",
          "form": "천둥번개 우르릉",
          "original_form": "천둥번개 우르릉",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.7",
          "form": "그래서 단우산필수",
          "original_form": "그래서 단우산필수",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.8",
          "form": "장우산은 불편",
          "original_form": "장우산은 불편",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.9",
          "form": "양산도 필수",
          "original_form": "양산도 필수",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.10",
          "form": "비와서 습도가 높아져서 끈적함",
          "original_form": "비와서 습도가 높아져서 끈적함",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.11",
          "form": "이제 처서임",
          "original_form": "이제 처서임",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.12",
          "form": "가을이온다",
          "original_form": "가을이온다",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.13",
          "form": "원터이즈커밍",
          "original_form": "원터이즈커밍",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.14",
          "form": "처서 지나면 좀 서늘해지겠죠",
          "original_form": "처서 지나면 좀 서늘해지겠죠",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.16",
          "form": "가을옷사야지",
          "original_form": "가을옷사야지",
          "speaker_id": "2",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008152.1.17",
          "form": "그럼 가을옷을 입어야 하고",
          "original_form": "그럼 가을옷을 입어야 하고",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008152.1.18",
          "form": "옷을사야지",
          "original_form": "옷을사야지",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008152.1.19",
          "form": "그렇지 근데 요즘 날씨 변화 너무 심각하다고 생각하지 않나요",
          "original_form": "그렇지 근데 요즘 날씨 변화 너무 심각하다고 생각하지 않나요",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008152.1.20",
          "form": "기후변화때문이지",
          "original_form": "기후변화때문이지",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008152.1.21",
          "form": "곧멸망각",
          "original_form": "곧멸망각",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008152.1.22",
          "form": "점점 날씨를 예측하기가 힘들어질 듯",
          "original_form": "점점 날씨를 예측하기가 힘들어질 듯",
