      "utterance": [
        {
          "id": "MDRW2100008891.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008891.1.2",
          "form": "아뇨 없어요!!",
          "original_form": "아뇨 없어요!!",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008891.1.3",
          "form": "님은용??",
          "original_form": "님은용??",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008891.1.4",
          "form": "저도용 ㅎㅎ",
          "original_form": "저도용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008891.1.5",
          "form": "종교 어떻게생각해요",
          "original_form": "종교 어떻게생각해요",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008891.1.6",
          "form": "음 간단하게 생각하는건 좋은것같은데",
          "original_form": "음 간단하게 생각하는건 좋은것같은데",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008891.1.7",
          "form": "너무 맹신하는건 안좋은것같아요",
          "original_form": "너무 맹신하는건 안좋은것같아요",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008891.1.8",
          "form": "저도 같은생각 ㅎㅎ",
          "original_form": "저도 같은생각 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008891.1.9",
          "form": "요즘같은 시국엔 모임좀 안햇으면 ..",
          "original_form": "요즘같은 시국엔 모임좀 안햇으면 ..",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008891.1.10",
          "form": "인정합니다ㅠㅠ 너무 위험해요",
          "original_form": "인정합니다ㅠㅠ 너무 위험해요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008891.1.11",
          "form": "주변에 사이비는 없으세용?",
          "original_form": "주변에 사이비는 없으세용?",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008891.1.12",
          "form": "저는 잘 모르지만 어딘가에는 있지않을까요 ㅋㅋㅋ",
          "original_form": "저는 잘 모르지만 어딘가에는 있지않을까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008891.1.13",
          "form": "여긴 경기도인데 거의모든사람이 기독교인것같아요",
          "original_form": "여긴 경기도인데 거의모든사람이 기독교인것같아요",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008891.1.14",
          "form": "오 그렇군용 전 주변에 종교 없는사람이 대부분이라서요 ㅋㅋ",
          "original_form": "오 그렇군용 전 주변에 종교 없는사람이 대부분이라서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008891.1.15",
          "form": "무교가 젤 좋은거같아요 ㅋㅋ",
          "original_form": "무교가 젤 좋은거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008891.1.16",
          "form": "비빔밥먹으러 절 가보셨나뇨",
          "original_form": "비빔밥먹으러 절 가보셨나뇨",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008891.1.17",
          "form": "아뇨 안가봣어요!!",
          "original_form": "아뇨 안가봣어요!!",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008891.1.18",
          "form": "기회되면 가보세용 절비빔밥 정말 맛나용 ㅜ_ㅜ 웬만한 음식점은 비교도안된다는..",
          "original_form": "기회되면 가보세용 절비빔밥 정말 맛나용 ㅜ_ㅜ 웬만한 음식점은 비교도안된다는..",
