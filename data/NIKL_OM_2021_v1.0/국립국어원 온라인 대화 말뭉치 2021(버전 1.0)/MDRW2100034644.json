      "utterance": [
        {
          "id": "MDRW2100034644.1.1",
          "form": "이젠 비공개잖아요",
          "original_form": "이젠 비공개잖아요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100034644.1.2",
          "form": "어디서 확진된건지도 비공개인가요?",
          "original_form": "어디서 확진된건지도 비공개인가요?",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034644.1.3",
          "form": "그거까진 요새 잘 안봐서 모르겠는데",
          "original_form": "그거까진 요새 잘 안봐서 모르겠는데",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034644.1.4",
          "form": "전처럼 경로를 디테일하게 제공하진 않더라구요",
          "original_form": "전처럼 경로를 디테일하게 제공하진 않더라구요",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034644.1.5",
          "form": "마자요 경로는 안나오더라구요",
          "original_form": "마자요 경로는 안나오더라구요",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034644.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034644.1.7",
          "form": "그래도 어디서 확진된건지는 나오나보더라구요",
          "original_form": "그래도 어디서 확진된건지는 나오나보더라구요",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034644.1.8",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034644.1.9",
          "form": "업주 입장에서는 꺼려하더라구요 ..",
          "original_form": "업주 입장에서는 꺼려하더라구요 ..",
          "speaker_id": "1",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034644.1.10",
          "form": "아그쵸ㅠ 그런거 나오면 장사 더 안되니까요",
          "original_form": "아그쵸ㅠ 그런거 나오면 장사 더 안되니까요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034644.1.11",
          "form": "저도 확인해본지 좀 돼서\n제가 확인했을때는 그렇게나오더라구요",
          "original_form": "저도 확인해본지 좀 돼서\n제가 확인했을때는 그렇게나오더라구요",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034644.1.12",
          "form": "너무 길어지니까",
          "original_form": "너무 길어지니까",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034644.1.13",
          "form": "다들 확인도 덜할거에요 ㅋㅋㅋㅋ",
          "original_form": "다들 확인도 덜할거에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034644.1.14",
          "form": "아 마자요 그렇긴하네요ㅋㅋ",
          "original_form": "아 마자요 그렇긴하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034644.1.15",
          "form": "길어지니 뭔가 더 루즈해지는...ㅜㅜ",
          "original_form": "길어지니 뭔가 더 루즈해지는...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034644.1.16",
          "form": "마자요 경각심이 덜해지죠ㅠ",
          "original_form": "마자요 경각심이 덜해지죠ㅠ",
          "speaker_id": "2",
