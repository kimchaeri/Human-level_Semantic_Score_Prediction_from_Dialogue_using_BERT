      "utterance": [
        {
          "id": "MDRW2100008455.1.1",
          "form": "거기나오는 의사중 누가 가장 좋으세요?",
          "original_form": "거기나오는 의사중 누가 가장 좋으세요?",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008455.1.2",
          "form": "안정원요",
          "original_form": "안정원요",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.3",
          "form": "따뜻해요",
          "original_form": "따뜻해요",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.4",
          "form": "아...안정원 스님 ㅎ",
          "original_form": "아...안정원 스님 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.5",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.6",
          "form": "누구 좋으세요?",
          "original_form": "누구 좋으세요?",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.7",
          "form": "저는 조정석 이요 ㅎㅎ",
          "original_form": "저는 조정석 이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.8",
          "form": "제가 응답때부터 좋아하긴 했네요 ㅋㅋ",
          "original_form": "제가 응답때부터 좋아하긴 했네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008455.1.9",
          "form": "아 조정석 좋죠",
          "original_form": "아 조정석 좋죠",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.10",
          "form": "언니들이랑 이야기하다 나는 조정석이 젤좋다고하니",
          "original_form": "언니들이랑 이야기하다 나는 조정석이 젤좋다고하니",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.11",
          "form": "전미도랑 어때요",
          "original_form": "전미도랑 어때요",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.12",
          "form": "저번에 거절 하던대",
          "original_form": "저번에 거절 하던대",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.13",
          "form": "그런의사 없다고 정신차리레요 ㅋ",
          "original_form": "그런의사 없다고 정신차리레요 ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.14",
          "form": "ㅋㅋ그렇죠",
          "original_form": "ㅋㅋ그렇죠",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.15",
          "form": "맞아요 거절했는데",
          "original_form": "맞아요 거절했는데",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.16",
          "form": "오늘꺼보니 전미도도 학생시절 조정석한테 맘이 있었던거같아요",
          "original_form": "오늘꺼보니 전미도도 학생시절 조정석한테 맘이 있었던거같아요",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008455.1.17",
          "form": "네 있었어요",
          "original_form": "네 있었어요",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.18",
          "form": "그게 조정석하고 학창시절돼야 하는데 친구가 껴서",
          "original_form": "그게 조정석하고 학창시절돼야 하는데 친구가 껴서",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.19",
          "form": "참 타이밍 중요해요",
          "original_form": "참 타이밍 중요해요",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.20",
          "form": "저는 전미도랑 조정석 커플보다",
          "original_form": "저는 전미도랑 조정석 커플보다",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.22",
          "form": "산부인과 의사둘이가 잘됐으면 좋겠어요",
          "original_form": "산부인과 의사둘이가 잘됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008455.1.24",
          "form": "은근 잘맞나료",
          "original_form": "은근 잘맞나료",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008455.1.25",
          "form": "여자의사 갑자기 이름이 생각이 안나네요",
          "original_form": "여자의사 갑자기 이름이 생각이 안나네요",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008455.1.26",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008455.1.27",
          "form": "저도 귀엽던데",
          "original_form": "저도 귀엽던데",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008455.1.28",
          "form": "추민하 쌤이네요",
          "original_form": "추민하 쌤이네요",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008455.1.29",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008455.1.30",
          "form": "양석현쌤도 아픔이 있으신데 밝고 귀여운 추민하쌤으로 치유되셨으면 좋겠더라구요",
          "original_form": "양석현쌤도 아픔이 있으신데 밝고 귀여운 추민하쌤으로 치유되셨으면 좋겠더라구요",
          "speaker_id": "1",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008455.1.31",
          "form": "네 그럴것 같아요",
          "original_form": "네 그럴것 같아요",
          "speaker_id": "2",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008455.1.32",
          "form": "도움이 많이 될것 같아요",
          "original_form": "도움이 많이 될것 같아요",
          "speaker_id": "2",
          "time": "20210822 16:02"
        }
      ]
    }
