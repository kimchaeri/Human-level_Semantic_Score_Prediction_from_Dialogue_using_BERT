      "utterance": [
        {
          "id": "MDRW2100009648.1.1",
          "form": "가고 싶은 여행지 어디야?",
          "original_form": "가고 싶은 여행지 어디야?",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009648.1.2",
          "form": "유럽 가고 싶어",
          "original_form": "유럽 가고 싶어",
          "speaker_id": "2",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009648.1.3",
          "form": "너는 어디 가고싶어?",
          "original_form": "너는 어디 가고싶어?",
          "speaker_id": "2",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009648.1.4",
          "form": "나는 유럽 한번도 안가서",
          "original_form": "나는 유럽 한번도 안가서",
          "speaker_id": "2",
          "time": "20210823 02:20"
        },
        {
          "id": "MDRW2100009648.1.5",
          "form": "오 나는 어학연수하러 아일랜드 가보고 싶어",
          "original_form": "오 나는 어학연수하러 아일랜드 가보고 싶어",
          "speaker_id": "1",
          "time": "20210823 02:20"
        },
        {
          "id": "MDRW2100009648.1.6",
          "form": "관광가고 싶은 곳은 스위스!",
          "original_form": "관광가고 싶은 곳은 스위스!",
          "speaker_id": "1",
          "time": "20210823 02:20"
        },
        {
          "id": "MDRW2100009648.1.7",
          "form": "아일랜드 멋지다",
          "original_form": "아일랜드 멋지다",
          "speaker_id": "2",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009648.1.8",
          "form": "왜 아일랜드야?",
          "original_form": "왜 아일랜드야?",
          "speaker_id": "2",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009648.1.9",
          "form": "아일랜드가 한국인이 많이 없다고 하더라고 ㅎㅎ 그래서",
          "original_form": "아일랜드가 한국인이 많이 없다고 하더라고 ㅎㅎ 그래서",
          "speaker_id": "1",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009648.1.10",
          "form": "거기가면 영어 많이 쓸것 같아서 알아봤어!",
          "original_form": "거기가면 영어 많이 쓸것 같아서 알아봤어!",
          "speaker_id": "1",
          "time": "20210823 02:23"
        },
        {
          "id": "MDRW2100009648.1.11",
          "form": "한국인이 없어야 좋지",
          "original_form": "한국인이 없어야 좋지",
          "speaker_id": "2",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009648.1.12",
          "form": "name2님은 가고 싶은 곳 있어요?",
          "original_form": "&name2&님은 가고 싶은 곳 있어요?",
          "speaker_id": "1",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009648.1.13",
          "form": "나는 영국 가고 싶어",
          "original_form": "나는 영국 가고 싶어",
          "speaker_id": "2",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009648.1.14",
          "form": "오 이유 있어?? 영국 물가 비싸던데",
          "original_form": "오 이유 있어?? 영국 물가 비싸던데",
          "speaker_id": "1",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009648.1.15",
          "form": "그냥 로망까지는 없는데",
          "original_form": "그냥 로망까지는 없는데",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009648.1.16",
          "form": "한번 가보고싶어",
          "original_form": "한번 가보고싶어",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009648.1.17",
          "form": "그렇구나 나도 영국 궁금하긴 해",
          "original_form": "그렇구나 나도 영국 궁금하긴 해",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009648.1.18",
          "form": "날씨가 안좋다는데",
          "original_form": "날씨가 안좋다는데",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009648.1.19",
          "form": "날씨가 좋으면 다들 햇빛 쬐러 나간대",
          "original_form": "날씨가 좋으면 다들 햇빛 쬐러 나간대",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009648.1.20",
          "form": "그렇구나 피쉬앤 칩스도 생각난다",
          "original_form": "그렇구나 피쉬앤 칩스도 생각난다",
          "speaker_id": "1",
          "time": "20210823 02:32"
        }
      ]
