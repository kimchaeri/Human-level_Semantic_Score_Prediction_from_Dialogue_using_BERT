        },
        {
          "id": "MDRW2100020945.1.2",
          "form": "양념갈비랑 빵이요!!ㅋㅋㅋ",
          "original_form": "양념갈비랑 빵이요!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020945.1.3",
          "form": "아 다 너무 맛있죠",
          "original_form": "아 다 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020945.1.4",
          "form": "좋아하세요??",
          "original_form": "좋아하세요??",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020945.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020945.1.6",
          "form": "빵은 어느 브랜드 좋아하세요?",
          "original_form": "빵은 어느 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020945.1.7",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020945.1.8",
          "form": "뚜레쥬르가 나은거같아요!",
          "original_form": "뚜레쥬르가 나은거같아요!",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.9",
          "form": "사실 요새는 개인빵집이 좋더라구요",
          "original_form": "사실 요새는 개인빵집이 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.11",
          "form": "체인점은 너무 비싸요",
          "original_form": "체인점은 너무 비싸요",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.12",
          "form": "근데 요새는 또",
          "original_form": "근데 요새는 또",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.13",
          "form": "개인 빵집도 고가더라구요",
          "original_form": "개인 빵집도 고가더라구요",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.14",
          "form": "빵값이 너무 비싸요 ㅠ",
          "original_form": "빵값이 너무 비싸요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020945.1.15",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020945.1.16",
          "form": "다 체인점가격 따라가더라구요",
          "original_form": "다 체인점가격 따라가더라구요",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020945.1.17",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020945.1.18",
          "form": "어디거 좋아하세요?",
          "original_form": "어디거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020945.1.19",
          "form": "저는 파바가 좋긴한데",
          "original_form": "저는 파바가 좋긴한데",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020945.1.20",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020945.1.21",
          "form": "비싸서 잘 못사다먹어요",
          "original_form": "비싸서 잘 못사다먹어요",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020945.1.22",
          "form": "요새 요기요 할인 자주하던데",
          "original_form": "요새 요기요 할인 자주하던데",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100020945.1.23",
          "form": "만원어치사면 반값이에요!!",
          "original_form": "만원어치사면 반값이에요!!",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
