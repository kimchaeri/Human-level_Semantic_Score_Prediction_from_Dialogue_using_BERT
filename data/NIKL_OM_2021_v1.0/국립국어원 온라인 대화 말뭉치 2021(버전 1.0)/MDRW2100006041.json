      "utterance": [
        {
          "id": "MDRW2100006041.1.1",
          "form": "8시시작해요?",
          "original_form": "8시시작해요?",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100006041.1.2",
          "form": "넵..",
          "original_form": "넵..",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100006041.1.3",
          "form": "흥미니안나와",
          "original_form": "흥미니안나와",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100006041.1.4",
          "form": "이거 수원월컵에 열려...직관가고싶다..",
          "original_form": "이거 수원월컵에 열려...직관가고싶다..",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100006041.1.5",
          "form": "아예 안하나? 오프는?",
          "original_form": "아예 안하나? 오프는?",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006041.1.6",
          "form": "ㅇㅇ 안해",
          "original_form": "ㅇㅇ 안해",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006041.1.7",
          "form": "4단계 무관중",
          "original_form": "4단계 무관중",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006041.1.8",
          "form": "선수들도 힘 안나겠다",
          "original_form": "선수들도 힘 안나겠다",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100006041.1.9",
          "form": "ㅇㅇ 진짜야..홈인데 응원받고해야 ㅠㅠ",
          "original_form": "ㅇㅇ 진짜야..홈인데 응원받고해야 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006041.1.10",
          "form": "맞아 집중도 안될테고 ㅜㅠ",
          "original_form": "맞아 집중도 안될테고 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006041.1.11",
          "form": "그니까 티켓값있는데 왜 받지를 못하니..ㅠㅠ",
          "original_form": "그니까 티켓값있는데 왜 받지를 못하니..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006041.1.12",
          "form": "근데 레바논이니까... ㅎ...이겨야...못잡으면 큰일",
          "original_form": "근데 레바논이니까... ㅎ...이겨야...못잡으면 큰일",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100006041.1.13",
          "form": "겁나웃곀ㅋㅋㅋㅋ 수원사루때 한번 가볼걸",
          "original_form": "겁나웃곀ㅋㅋㅋㅋ 수원사루때 한번 가볼걸",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006041.1.14",
          "form": "존잼인데 아쉽다.. 가면재미써",
          "original_form": "존잼인데 아쉽다.. 가면재미써",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006041.1.15",
          "form": "야구도 봐볼걸했어",
          "original_form": "야구도 봐볼걸했어",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006041.1.16",
          "form": "음식도 팔고 맥주도 있오",
          "original_form": "음식도 팔고 맥주도 있오",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006041.1.17",
          "form": "야구는 나도 안가봄 ㅋㅋㅋ",
          "original_form": "야구는 나도 안가봄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006041.1.18",
          "form": "케이티 지금은 잘한댔나..",
          "original_form": "케이티 지금은 잘한댔나..",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100006041.1.19",
          "form": "그렇ㄱㅔ잼다던데",
          "original_form": "그렇ㄱㅔ잼다던데",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100006041.1.20",
          "form": "암턴 축두 흥미니다음 민재 조아 잘해... 수비수..",
          "original_form": "암턴 축두 흥미니다음 민재 조아 잘해... 수비수..",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100006041.1.21",
          "form": "걔도나와?",
          "original_form": "걔도나와?",
          "speaker_id": "1",
