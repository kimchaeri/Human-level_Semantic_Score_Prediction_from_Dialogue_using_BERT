      "utterance": [
        {
          "id": "MDRW2100039787.1.1",
          "form": "올해 여행 다녀오셨나요?",
          "original_form": "올해 여행 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039787.1.2",
          "form": "저는 코로나때문에 못갔어요ㅠㅠ",
          "original_form": "저는 코로나때문에 못갔어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.3",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.4",
          "form": "ㅠㅠ제가 죄송하네요 괜히..",
          "original_form": "ㅠㅠ제가 죄송하네요 괜히..",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.5",
          "form": "저는 조심해서ㅠ다녀왔어요 ㅠ",
          "original_form": "저는 조심해서ㅠ다녀왔어요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.6",
          "form": "앗 어디다녀오셨어요?",
          "original_form": "앗 어디다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.7",
          "form": "어디 가장 가고싶으세요?",
          "original_form": "어디 가장 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.8",
          "form": "아뇨 저도 가려다 무산된거라ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 저도 가려다 무산된거라ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039787.1.9",
          "form": "저는 바다 가고싶네요...",
          "original_form": "저는 바다 가고싶네요...",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039787.1.10",
          "form": "아 저는 고성 다녀왔어요 강원도 통일전망대쪽",
          "original_form": "아 저는 고성 다녀왔어요 강원도 통일전망대쪽",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039787.1.11",
          "form": "오 대박 멀리 다녀오셨다 북한 보고오셨나요?",
          "original_form": "오 대박 멀리 다녀오셨다 북한 보고오셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039787.1.12",
          "form": "바다,, 물멍하세요 힐링..",
          "original_form": "바다,, 물멍하세요 힐링..",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039787.1.13",
          "form": "오오 네",
          "original_form": "오오 네",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039787.1.14",
          "form": "통일전망대에서 5분만 걸어가면 북한이더라구요",
          "original_form": "통일전망대에서 5분만 걸어가면 북한이더라구요",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039787.1.15",
          "form": "너무 기분이 이상했어요 ㅎㅎ",
          "original_form": "너무 기분이 이상했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039787.1.16",
          "form": "우와ㅋㅋㅋㅋㅋ저도 거기 가보고 싶었는데",
          "original_form": "우와ㅋㅋㅋㅋㅋ저도 거기 가보고 싶었는데",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039787.1.17",
          "form": "진짜 신기하네요",
          "original_form": "진짜 신기하네요",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039787.1.18",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039787.1.19",
          "form": "꼭 가보세요 진짜 묘해요 ㅋㅋㅋ",
          "original_form": "꼭 가보세요 진짜 묘해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039787.1.20",
          "form": "가기 전에 초소 같은 곳에서 신분확인도 하더라구요",
          "original_form": "가기 전에 초소 같은 곳에서 신분확인도 하더라구요",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039787.1.21",
          "form": "우와 신기하네요",
          "original_form": "우와 신기하네요",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039787.1.22",
          "form": "그쵸 탈북할까봐 그렇다고 하더라구요.. ㅎ",
          "original_form": "그쵸 탈북할까봐 그렇다고 하더라구요.. ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039787.1.23",
          "form": "남해,동해,서해 중 어디가고 싶으세요?",
          "original_form": "남해,동해,서해 중 어디가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039787.1.24",
          "form": "아ㅋㅋㅋㅋㅋ탈북이라니 상상도 못했네요",
          "original_form": "아ㅋㅋㅋㅋㅋ탈북이라니 상상도 못했네요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039787.1.25",
          "form": "저는 부산 자주가는데요 남해도 가보고 싶어요",
          "original_form": "저는 부산 자주가는데요 남해도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039787.1.26",
          "form": "요즘 제주도 많이들 가시더라구요",
          "original_form": "요즘 제주도 많이들 가시더라구요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039787.1.27",
          "form": "오 부산 좋죠",
          "original_form": "오 부산 좋죠",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039787.1.28",
          "form": "맞아요 제주도도 코로나 단계 격상 했다고 뉴스 봤어요",
          "original_form": "맞아요 제주도도 코로나 단계 격상 했다고 뉴스 봤어요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039787.1.29",
          "form": "해운대 바다 보고싶네요..ㅎㅎ",
          "original_form": "해운대 바다 보고싶네요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039787.1.30",
          "form": "자갈치 시장도 가고",
          "original_form": "자갈치 시장도 가고",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039787.1.31",
          "form": "저는 광안대교 보고 싶어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 광안대교 보고 싶어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039787.1.32",
          "form": "그걸 보면 아 올해도 부산 왔군~ 싶더라구요",
          "original_form": "그걸 보면 아 올해도 부산 왔군~ 싶더라구요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039787.1.33",
          "form": "오오 광안대교 야경 멋지지 않나요",
          "original_form": "오오 광안대교 야경 멋지지 않나요",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039787.1.34",
          "form": "부산 매년 가시나봐요",
          "original_form": "부산 매년 가시나봐요",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039787.1.35",
          "form": "좋겠다..ㅎㅎ",
          "original_form": "좋겠다..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039787.1.36",
          "form": "스무살 넘어서는 거의 매년? 자주 갔던 거 같아요 작년부터 못 가고...",
          "original_form": "스무살 넘어서는 거의 매년? 자주 갔던 거 같아요 작년부터 못 가고...",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039787.1.37",
          "form": "근데 맨날 1박 2일이어서ㅋㅋㅋㅋㅋㅋㅋㅋ아직도 못 가본 곳도 많아요",
          "original_form": "근데 맨날 1박 2일이어서ㅋㅋㅋㅋㅋㅋㅋㅋ아직도 못 가본 곳도 많아요",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039787.1.38",
          "form": "어머 진짜 부산 좋아하시나봐요",
          "original_form": "어머 진짜 부산 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039787.1.39",
          "form": "송도쪽? 가보셨어요?",
          "original_form": "송도쪽? 가보셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039787.1.40",
          "form": "아니요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039787.1.41",
          "form": "좋아한다기보다 친구들에 휩쓸려서 자주 갔던 거 같아요",
          "original_form": "좋아한다기보다 친구들에 휩쓸려서 자주 갔던 거 같아요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039787.1.42",
          "form": "송도가 좋나요??",
          "original_form": "송도가 좋나요??",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039787.1.43",
          "form": "좋다고 이야기 들었어요 ㅎㅎㅋㅋ",
          "original_form": "좋다고 이야기 들었어요 ㅎㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039787.1.44",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039787.1.45",
          "form": "그래도 뭔가 애정이 있을 거 같아요 매년 가니까",
          "original_form": "그래도 뭔가 애정이 있을 거 같아요 매년 가니까",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039787.1.46",
          "form": "맞아요 이제 지하철이 좀 익숙해요...",
          "original_form": "맞아요 이제 지하철이 좀 익숙해요...",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039787.1.47",
          "form": "전 지하철 잘 안타는 지역에 살거든요ㅋㅋㅋㅋㅋ",
          "original_form": "전 지하철 잘 안타는 지역에 살거든요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039787.1.48",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039787.1.49",
          "form": "어느 쪽에 사세요?",
          "original_form": "어느 쪽에 사세요?",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039787.1.50",
          "form": "저....광주요",
          "original_form": "저....광주요",
          "speaker_id": "2",
          "time": "20210903 21:42"
        }
      ]
