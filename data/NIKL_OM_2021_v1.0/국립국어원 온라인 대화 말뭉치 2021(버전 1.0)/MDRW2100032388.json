        {
          "id": "MDRW2100032388.1.1",
          "form": "음료수뭐좋아하세요?",
          "original_form": "음료수뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100032388.1.2",
          "form": "패션후르츠요",
          "original_form": "패션후르츠요",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032388.1.3",
          "form": "열대과일요",
          "original_form": "열대과일요",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032388.1.4",
          "form": "그게뭐예요?",
          "original_form": "그게뭐예요?",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032388.1.5",
          "form": "열대과일을청으로 만들어서",
          "original_form": "열대과일을청으로 만들어서",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032388.1.6",
          "form": "아..맛있겠네요",
          "original_form": "아..맛있겠네요",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032388.1.7",
          "form": "물에타서드세요",
          "original_form": "물에타서드세요",
          "speaker_id": "1",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032388.1.8",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032388.1.9",
          "form": "탄산에타면더맛있어요",
          "original_form": "탄산에타면더맛있어요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032388.1.10",
          "form": "저도먹어봐야겠네요",
          "original_form": "저도먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032388.1.11",
          "form": "드셔보시면 좋아한것같ㅇ네요",
          "original_form": "드셔보시면 좋아한것같ㅇ네요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032388.1.12",
          "form": "네.맛있게드세요",
          "original_form": "네.맛있게드세요",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032388.1.13",
          "form": "네..꼭 챙겨먹어보도록할께요",
          "original_form": "네..꼭 챙겨먹어보도록할께요",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032388.1.14",
          "form": "저는 커피좋아해오ㅡ",
          "original_form": "저는 커피좋아해오ㅡ",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032388.1.15",
          "form": "저는 좀싫어해요.커피는",
          "original_form": "저는 좀싫어해요.커피는",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032388.1.16",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032388.1.17",
          "form": "쓴맛때문에",
          "original_form": "쓴맛때문에",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032388.1.18",
          "form": "아..써서 싫어하시는구나",
          "original_form": "아..써서 싫어하시는구나",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032388.1.19",
          "form": "네..단맛좋아해요",
          "original_form": "네..단맛좋아해요",
