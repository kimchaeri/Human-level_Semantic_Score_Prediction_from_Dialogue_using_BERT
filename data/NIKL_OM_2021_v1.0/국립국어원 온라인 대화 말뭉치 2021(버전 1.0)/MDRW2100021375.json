      "utterance": [
        {
          "id": "MDRW2100021375.1.1",
          "form": "휴 캡쳐 모두 완료했어요",
          "original_form": "휴 캡쳐 모두 완료했어요",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100021375.1.2",
          "form": "그리고 계속 추천드릴께요!",
          "original_form": "그리고 계속 추천드릴께요!",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100021375.1.3",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100021375.1.4",
          "form": "다 끝나시면 저도 여행지 가봤던 곳들중에 좋은 곳이나 서울 맛집 알려드릴게요",
          "original_form": "다 끝나시면 저도 여행지 가봤던 곳들중에 좋은 곳이나 서울 맛집 알려드릴게요",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100021375.1.5",
          "form": "남포동에서 한 삼십분정도 버스타고 가시면",
          "original_form": "남포동에서 한 삼십분정도 버스타고 가시면",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100021375.1.6",
          "form": "헉너무좋죠",
          "original_form": "헉너무좋죠",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100021375.1.7",
          "form": "태종대라고 바다랑 산을 동시에 즐길수있는곳이 있어요",
          "original_form": "태종대라고 바다랑 산을 동시에 즐길수있는곳이 있어요",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100021375.1.8",
          "form": "수국이 많이펴서 수국필때가시면 이뻐요",
          "original_form": "수국이 많이펴서 수국필때가시면 이뻐요",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100021375.1.9",
          "form": "걷기싫으시면 열차있는데 그거타시고 한바퀴 구경하세요",
          "original_form": "걷기싫으시면 열차있는데 그거타시고 한바퀴 구경하세요",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100021375.1.10",
          "form": "오 저 태종대 가봤어요!",
          "original_form": "오 저 태종대 가봤어요!",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100021375.1.11",
          "form": "가보셨구나!",
          "original_form": "가보셨구나!",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100021375.1.12",
          "form": "수국은 못봤네요ㅠㅠ",
          "original_form": "수국은 못봤네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100021375.1.13",
          "form": "아쉽네요ㅠㅠ",
          "original_form": "아쉽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100021375.1.14",
          "form": "담에 또 가봐야겠어요",
          "original_form": "담에 또 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100021375.1.15",
          "form": "밥먹고와서 마저 추천드릴께요!",
          "original_form": "밥먹고와서 마저 추천드릴께요!",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100021375.1.16",
          "form": "맛점하세요!!",
          "original_form": "맛점하세요!!",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100021375.1.17",
          "form": "맛저..",
          "original_form": "맛저..",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100021375.1.18",
          "form": "왔어요",
          "original_form": "왔어요",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021375.1.19",
          "form": "태종대에서 이십분정도 버스타고 나오면 흰여울길을 갈수있어요",
          "original_form": "태종대에서 이십분정도 버스타고 나오면 흰여울길을 갈수있어요",
          "speaker_id": "1",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021375.1.20",
          "form": "거기 카페 다이쁘긴한데 저는 에테르 추천드려요",
          "original_form": "거기 카페 다이쁘긴한데 저는 에테르 추천드려요",
          "speaker_id": "1",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100021375.1.21",
          "form": "ㅋㅋㅋㅋ 아 ㅋㅋㅋㅋ 너무 귀여우세요...",
          "original_form": "ㅋㅋㅋㅋ 아 ㅋㅋㅋㅋ 너무 귀여우세요...",
          "speaker_id": "2",
          "time": "20210828 18:54"
        },
        {
          "id": "MDRW2100021375.1.22",
          "form": "저요?",
          "original_form": "저요?",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021375.1.23",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021375.1.24",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021375.1.25",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021375.1.26",
          "form": "식사끝나시자마자 달려와서 알려주시는게 넘 감사하고 귀여우세요ㅠㅋㅋㅋㅋ",
          "original_form": "식사끝나시자마자 달려와서 알려주시는게 넘 감사하고 귀여우세요ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021375.1.27",
          "form": "고백 그만하겠습니다,,",
          "original_form": "고백 그만하겠습니다,,",
          "speaker_id": "2",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021375.1.28",
          "form": "앜ㅋㅋㅋㅋ호다닥 왔어요",
          "original_form": "앜ㅋㅋㅋㅋ호다닥 왔어요",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021375.1.29",
          "form": "서울맛집 알고싶어서요ㅋㅋㄱㄲ",
          "original_form": "서울맛집 알고싶어서요ㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021375.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 얼마든지요 ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 얼마든지요 ",
