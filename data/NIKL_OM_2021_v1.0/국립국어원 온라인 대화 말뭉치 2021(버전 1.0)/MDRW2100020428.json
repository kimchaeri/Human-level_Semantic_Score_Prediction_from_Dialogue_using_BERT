      "utterance": [
        {
          "id": "MDRW2100020428.1.1",
          "form": "넹넹 구독해놓고 있어요",
          "original_form": "넹넹 구독해놓고 있어요",
          "speaker_id": "2",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.2",
          "form": "킹덤 완전 재밌게 봤어용",
          "original_form": "킹덤 완전 재밌게 봤어용",
          "speaker_id": "1",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.3",
          "form": "근데 현생이 바쁘니 잘 못보고 있어요 ㅋㅋㅋ",
          "original_form": "근데 현생이 바쁘니 잘 못보고 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.4",
          "form": "맞아요 킹덤은 진짜",
          "original_form": "맞아요 킹덤은 진짜",
          "speaker_id": "2",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.5",
          "form": "ㅠㅠ 그렇죠...",
          "original_form": "ㅠㅠ 그렇죠...",
          "speaker_id": "1",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.6",
          "form": "최근에 아신전까지 다 봤어요",
          "original_form": "최근에 아신전까지 다 봤어요",
          "speaker_id": "1",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.7",
          "form": "너무 잘만들었어요",
          "original_form": "너무 잘만들었어요",
          "speaker_id": "2",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.8",
          "form": "저두 아신전 봤어요",
          "original_form": "저두 아신전 봤어요",
          "speaker_id": "2",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.9",
          "form": "조금 잔인하긴한데 스토리가 어찌이렇게 탄탄한지",
          "original_form": "조금 잔인하긴한데 스토리가 어찌이렇게 탄탄한지",
          "speaker_id": "1",
          "time": "20210828 05:23"
        },
        {
          "id": "MDRW2100020428.1.10",
          "form": "전지현의 흑화!!",
          "original_form": "전지현의 흑화!!",
          "speaker_id": "2",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.11",
          "form": "좀비물 이런거 좋아해서ㅋㅋㅋ",
          "original_form": "좀비물 이런거 좋아해서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.12",
          "form": "오징어게임 개봉한다고 하던데",
          "original_form": "오징어게임 개봉한다고 하던데",
          "speaker_id": "1",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.13",
          "form": "특히 우리나라 좀비물 너무 스릴있어여",
          "original_form": "특히 우리나라 좀비물 너무 스릴있어여",
          "speaker_id": "2",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.14",
          "form": "요즘 그거 기다리고 있어요",
          "original_form": "요즘 그거 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.15",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.16",
          "form": "연기자들이 연기를 너무 잘해요",
          "original_form": "연기자들이 연기를 너무 잘해요",
          "speaker_id": "1",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.17",
          "form": "오늘 공개되지 않았어요?",
          "original_form": "오늘 공개되지 않았어요?",
          "speaker_id": "2",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.18",
          "form": "아 진짜요????",
          "original_form": "아 진짜요????",
          "speaker_id": "1",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.19",
          "form": "아직 아닌가",
          "original_form": "아직 아닌가",
          "speaker_id": "2",
          "time": "20210828 05:24"
        },
        {
          "id": "MDRW2100020428.1.20",
          "form": "9월17일 개봉이라고 하긴 하네용",
          "original_form": "9월17일 개봉이라고 하긴 하네용",
