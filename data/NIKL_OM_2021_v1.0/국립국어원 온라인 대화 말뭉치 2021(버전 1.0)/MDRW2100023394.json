      "utterance": [
        {
          "id": "MDRW2100023394.1.1",
          "form": "오잉? 미국에서",
          "original_form": "오잉? 미국에서",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023394.1.2",
          "form": "맥도날드 아아에다 소프트콘 퐁당해서 먹는건데",
          "original_form": "맥도날드 아아에다 소프트콘 퐁당해서 먹는건데",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023394.1.3",
          "form": "한국음식을 해먹어요?",
          "original_form": "한국음식을 해먹어요?",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023394.1.4",
          "form": "이걸 한국식 커피라고 부르더라구요 미국에서",
          "original_form": "이걸 한국식 커피라고 부르더라구요 미국에서",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023394.1.5",
          "form": "아포가토를",
          "original_form": "아포가토를",
          "speaker_id": "2",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100023394.1.6",
          "form": "왜 이게 한국식인거죵..",
          "original_form": "왜 이게 한국식인거죵..",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.7",
          "form": "혼란스러운데",
          "original_form": "혼란스러운데",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.8",
          "form": "맞아용ㅋㅋㅋㅋ 그냥 아포가토같긴한데!",
          "original_form": "맞아용ㅋㅋㅋㅋ 그냥 아포가토같긴한데!",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.9",
          "form": "근데 이거 말구",
          "original_form": "근데 이거 말구",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.10",
          "form": "맥날 라떼에다 콘 퐁당해서 먹으면",
          "original_form": "맥날 라떼에다 콘 퐁당해서 먹으면",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.11",
          "form": "폴바셋 저렴이버전인데 맛있어요!",
          "original_form": "폴바셋 저렴이버전인데 맛있어요!",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.12",
          "form": "헐 그런게 있군요 전혀 몰랐어요 이렇게 늙는건가..ㅠㅠ",
          "original_form": "헐 그런게 있군요 전혀 몰랐어요 이렇게 늙는건가..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023394.1.13",
          "form": "아니에욬ㅋㅋㅋ 저도 요즘 젊은 사람들 겨우겨우 따라가요....",
          "original_form": "아니에욬ㅋㅋㅋ 저도 요즘 젊은 사람들 겨우겨우 따라가요....",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023394.1.14",
          "form": "sns를 안해가지구.. 먹는방법? 먹는건 다 유튜브로 보는 편인데",
          "original_form": "sns를 안해가지구.. 먹는방법? 먹는건 다 유튜브로 보는 편인데",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023394.1.15",
          "form": "유튜브 요즘 모가 제일 핫해용?",
          "original_form": "유튜브 요즘 모가 제일 핫해용?",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023394.1.16",
          "form": "저는 먹방은 안봐서",
          "original_form": "저는 먹방은 안봐서",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023394.1.17",
          "form": "요즘 유행하는 음식이 궁금해용",
          "original_form": "요즘 유행하는 음식이 궁금해용",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023394.1.18",
          "form": "아 저도 먹방은 안 보는데 숏클립? 보면",
          "original_form": "아 저도 먹방은 안 보는데 숏클립? 보면",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023394.1.19",
          "form": "다들 와플기계로",
          "original_form": "다들 와플기계로",
          "speaker_id": "2",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.20",
          "form": "모든음식을 눌러먹더라구요",
          "original_form": "모든음식을 눌러먹더라구요",
          "speaker_id": "2",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.21",
          "form": "아아아 와플 기계로 별걸다 구워먹더라구욬ㅋㅋㅋ",
          "original_form": "아아아 와플 기계로 별걸다 구워먹더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.22",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.23",
          "form": "이거야말루 한국식이 아닐른지ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이거야말루 한국식이 아닐른지ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.24",
          "form": "그러게요ㅋㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.25",
          "form": "와플기계에다 김치부침개",
          "original_form": "와플기계에다 김치부침개",
          "speaker_id": "1",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.26",
          "form": "넣어 먹는거 봤는데",
          "original_form": "넣어 먹는거 봤는데",
          "speaker_id": "1",
          "time": "20210830 01:26"
        },
        {
          "id": "MDRW2100023394.1.27",
