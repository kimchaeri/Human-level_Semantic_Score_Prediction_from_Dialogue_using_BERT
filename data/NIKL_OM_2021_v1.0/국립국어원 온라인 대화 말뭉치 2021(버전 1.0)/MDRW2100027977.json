      "utterance": [
        {
          "id": "MDRW2100027977.1.1",
          "form": "생각을 해보면 나는 왕따를 당햇던 거 같아 ",
          "original_form": "생각을 해보면 나는 왕따를 당햇던 거 같아 ",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100027977.1.2",
          "form": "머? 충격고백인데",
          "original_form": "머? 충격고백인데",
          "speaker_id": "2",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100027977.1.3",
          "form": "은따였던 거 같은데 중2때 나 안 좋아했던 애가",
          "original_form": "은따였던 거 같은데 중2때 나 안 좋아했던 애가",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100027977.1.4",
          "form": "중3때 분위기 주름잡는 애가 됐는데",
          "original_form": "중3때 분위기 주름잡는 애가 됐는데",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100027977.1.5",
          "form": "중3때 담임이 애들한테 인기가 없었는데",
          "original_form": "중3때 담임이 애들한테 인기가 없었는데",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100027977.1.6",
          "form": "난 그분에게 총애를 받앗어",
          "original_form": "난 그분에게 총애를 받앗어",
          "speaker_id": "1",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100027977.1.7",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.8",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.9",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.10",
          "form": "상황이 안좋게돌아가요",
          "original_form": "상황이 안좋게돌아가요",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.11",
          "form": "역할분담 결정하는데",
          "original_form": "역할분담 결정하는데",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.12",
          "form": "아무도 손 안들다가 칠판닦는 걸 내가하겠다고",
          "original_form": "아무도 손 안들다가 칠판닦는 걸 내가하겠다고",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.13",
          "form": "손을 들었기 때문임",
          "original_form": "손을 들었기 때문임",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅋ ㅋ ㅋㅋ  ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅋ ㅋ ㅋㅋ  ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.15",
          "form": "그게 총애의 이유라니",
          "original_form": "그게 총애의 이유라니",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.17",
          "form": "그래성?",
          "original_form": "그래성?",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100027977.1.18",
          "form": "애들이 다 비융신이라",
          "original_form": "애들이 다 비융신이라",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100027977.1.19",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100027977.1.20",
          "form": "총애받는다고 뭐 할게있나",
          "original_form": "총애받는다고 뭐 할게있나",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100027977.1.21",
          "form": "차별하신 것두 아니고",
          "original_form": "차별하신 것두 아니고",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100027977.1.22",
          "form": "편애도 아니고 그냥 예뻐해주신 것뿐이라",
          "original_form": "편애도 아니고 그냥 예뻐해주신 것뿐이라",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100027977.1.23",
          "form": "근데 나 저때",
          "original_form": "근데 나 저때",
          "speaker_id": "1",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100027977.1.24",
          "form": "입시해서",
          "original_form": "입시해서",
          "speaker_id": "1",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100027977.1.25",
          "form": "애들",
          "original_form": "애들",
          "speaker_id": "1",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100027977.1.26",
          "form": "노관심",
          "original_form": "노관심",
          "speaker_id": "1",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100027977.1.27",
          "form": "이었다",
          "original_form": "이었다",
          "speaker_id": "1",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100027977.1.28",
          "form": "본인이 못느꼈을정도면",
          "original_form": "본인이 못느꼈을정도면",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100027977.1.29",
          "form": "딴애들만 바보네",
          "original_form": "딴애들만 바보네",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100027977.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100027977.1.31",
          "form": "아니 느끼긴했는데",
          "original_form": "아니 느끼긴했는데",
          "speaker_id": "1",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.32",
          "form": "관심이 없었지",
          "original_form": "관심이 없었지",
          "speaker_id": "1",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.33",
          "form": "바빠죽겟는데",
          "original_form": "바빠죽겟는데",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.34",
          "form": "친구들이 모 그 반애만 잇나ㅋㅋㅋㅋㅋㅋ",
          "original_form": "친구들이 모 그 반애만 잇나ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.36",
          "form": "나는 되게 별 생각없이 학교다닌듯...",
          "original_form": "나는 되게 별 생각없이 학교다닌듯...",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.37",
          "form": "부우웅",
          "original_form": "부우웅",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100027977.1.38",
          "form": "뭐 생각하면서 다니는 사람이 있나",
          "original_form": "뭐 생각하면서 다니는 사람이 있나",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100027977.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100027977.1.40",
          "form": "에이에엥 다시바꿔죠",
