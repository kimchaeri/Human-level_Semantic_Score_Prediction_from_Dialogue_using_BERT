      "utterance": [
        {
          "id": "MDRW2100039006.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100039006.1.2",
          "form": "피자 살 때 어떤 메뉴 선호하세여?",
          "original_form": "피자 살 때 어떤 메뉴 선호하세여?",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039006.1.3",
          "form": "포테이토 콤비네이션 불고기 주로 주문하는데",
          "original_form": "포테이토 콤비네이션 불고기 주로 주문하는데",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039006.1.4",
          "form": "포테이토 가장 좋아해요",
          "original_form": "포테이토 가장 좋아해요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039006.1.5",
          "form": "오ㅎㅎ 저도 포테이토 좋아해요ㅎㅎ",
          "original_form": "오ㅎㅎ 저도 포테이토 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100039006.1.6",
          "form": "고소하고 맛나죠",
          "original_form": "고소하고 맛나죠",
          "speaker_id": "1",
          "time": "20210903 01:22"
        },
        {
          "id": "MDRW2100039006.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100039006.1.8",
          "form": "저는 포테이토가 좋은데 동생은 불고기를 선호해서 경쟁이에요",
          "original_form": "저는 포테이토가 좋은데 동생은 불고기를 선호해서 경쟁이에요",
          "speaker_id": "2",
          "time": "20210903 01:23"
        },
        {
          "id": "MDRW2100039006.1.9",
          "form": "ㅋㅋㅋ저도 불고기보단 포테이토가 더 좋아요ㅋㅋ",
          "original_form": "ㅋㅋㅋ저도 불고기보단 포테이토가 더 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100039006.1.10",
          "form": "치열하겠어요",
          "original_form": "치열하겠어요",
          "speaker_id": "1",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100039006.1.11",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100039006.1.12",
          "form": "막내와 치열한 경쟁을 합니다",
          "original_form": "막내와 치열한 경쟁을 합니다",
          "speaker_id": "2",
          "time": "20210903 01:24"
        },
        {
          "id": "MDRW2100039006.1.13",
          "form": "ㅋㅋㅋㅋㅋ가족은 다 그런가봐요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ가족은 다 그런가봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:25"
        },
        {
          "id": "MDRW2100039006.1.14",
          "form": "어릴때는 남동생과 경쟁을 했죠",
          "original_form": "어릴때는 남동생과 경쟁을 했죠",
          "speaker_id": "2",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100039006.1.15",
          "form": "피자냐 치킨이냐",
          "original_form": "피자냐 치킨이냐",
          "speaker_id": "2",
          "time": "20210903 01:28"
        },
        {
          "id": "MDRW2100039006.1.16",
          "form": "ㅋㅋㅋㅋ name2님은 보통 무슨파세요?",
          "original_form": "ㅋㅋㅋㅋ &name2&님은 보통 무슨파세요?",
          "speaker_id": "1",
          "time": "20210903 01:29"
        },
        {
          "id": "MDRW2100039006.1.17",
          "form": "전 어릴때 피자파였어요",
          "original_form": "전 어릴때 피자파였어요",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100039006.1.18",
          "form": "지금은 달라졌나요?",
          "original_form": "지금은 달라졌나요?",
          "speaker_id": "1",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100039006.1.19",
          "form": "지금은 치킨 피자 둘 다 선호해요",
          "original_form": "지금은 치킨 피자 둘 다 선호해요",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100039006.1.20",
          "form": "어릴 때 부모님이 하나로 통일하라 하셨지만\r\n매번 치열하게 하나는 치킨 하나는 피자를 외쳤어요",
