      "utterance": [
        {
          "id": "MDRW2100016906.1.1",
          "form": "안녕하세요 지금 대화 가능하신가요?",
          "original_form": "안녕하세요 지금 대화 가능하신가요?",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016906.1.2",
          "form": "네 가능해요! 안녕하세요~",
          "original_form": "네 가능해요! 안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016906.1.3",
          "form": "점심은 뭐 챙겨드셨어요?",
          "original_form": "점심은 뭐 챙겨드셨어요?",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016906.1.4",
          "form": "전 우육면이요. name2님은요?",
          "original_form": "전 우육면이요. &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016906.1.5",
          "form": "저는 롯데리아에 치킨버거랑 새우버거 4200원 이벤트하길래 ㅎㅎ 그거 사먹었어요",
          "original_form": "저는 롯데리아에 치킨버거랑 새우버거 4200원 이벤트하길래 ㅎㅎ 그거 사먹었어요",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016906.1.6",
          "form": "아 전 롯데리아서 드셨구나 저도 가끔 가요",
          "original_form": "아 전 롯데리아서 드셨구나 저도 가끔 가요",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016906.1.7",
          "form": "버거 두개만 드셨나요? 사이드는요?",
          "original_form": "버거 두개만 드셨나요? 사이드는요?",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016906.1.8",
          "form": "그냥 버거만 먹었어요 ㅎㅎ",
          "original_form": "그냥 버거만 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016906.1.9",
          "form": "가격이 은근 비싸서 저도 이벤트할댜만 가끔 먹어요",
          "original_form": "가격이 은근 비싸서 저도 이벤트할댜만 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016906.1.10",
          "form": "아항~ 저도 자주는 안먹어요.",
          "original_form": "아항~ 저도 자주는 안먹어요.",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016906.1.11",
          "form": "name1님은 어떤음식 좋아하세요?",
          "original_form": "&name1&님은 어떤음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016906.1.12",
          "form": "전..대부분 다 좋아해요",
          "original_form": "전..대부분 다 좋아해요",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016906.1.13",
          "form": "오 안가리고 다 잘드시나보네요",
          "original_form": "오 안가리고 다 잘드시나보네요",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016906.1.14",
          "form": "전 편식도 심하고 초딩입맛이라ㅋㅋㅋㅋ",
          "original_form": "전 편식도 심하고 초딩입맛이라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016906.1.15",
          "form": "아 어떤걸 안드세요?",
          "original_form": "아 어떤걸 안드세요?",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016906.1.16",
          "form": "음 물컹물컹한 식감을 싫어해서",
          "original_form": "음 물컹물컹한 식감을 싫어해서",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016906.1.17",
          "form": "해산물이랑 가지 버섯 비계 이런거 안먹어요",
          "original_form": "해산물이랑 가지 버섯 비계 이런거 안먹어요",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016906.1.18",
          "form": "아 오이두요ㅋㅋㅋ오이는 그냥..맛이 없어요 ㅠ",
          "original_form": "아 오이두요ㅋㅋㅋ오이는 그냥..맛이 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016906.1.19",
          "form": "아! 전형적인데요 근데 오이안티는 마나요 자신감을 가지세요",
          "original_form": "아! 전형적인데요 근데 오이안티는 마나요 자신감을 가지세요",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016906.1.20",
          "form": "가지안티도 메이저에요",
          "original_form": "가지안티도 메이저에요",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016906.1.21",
          "form": "당근은 드세요?",
          "original_form": "당근은 드세요?",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016906.1.22",
          "form": "네 다른 야채들은 잘먹어요 ㅎㅎ",
          "original_form": "네 다른 야채들은 잘먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016906.1.23",
          "form": "그럼 왕편식은 아니세요 자신감을 가지세요 ㅎㅎㅎ",
          "original_form": "그럼 왕편식은 아니세요 자신감을 가지세요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016906.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 감사해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 감사해요",
