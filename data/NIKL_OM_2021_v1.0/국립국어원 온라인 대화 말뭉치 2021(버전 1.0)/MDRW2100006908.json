      "utterance": [
        {
          "id": "MDRW2100006908.1.1",
          "form": "저녁 뭐드셨나요?",
          "original_form": "저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006908.1.2",
          "form": "저는 김치전이요!!! *-* 마쉿는 김치전",
          "original_form": "저는 김치전이요!!! *-* 마쉿는 김치전",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006908.1.3",
          "form": "비오는데 김치전이라 너무 맛있었겠어요..",
          "original_form": "비오는데 김치전이라 너무 맛있었겠어요..",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006908.1.4",
          "form": "바삭한 김치전은 최고죳....",
          "original_form": "바삭한 김치전은 최고죳....",
          "speaker_id": "1",
          "time": "20210822 07:20"
        },
        {
          "id": "MDRW2100006908.1.5",
          "form": "쿼카님은 아침 챙겨드시나유",
          "original_form": "쿼카님은 아침 챙겨드시나유",
          "speaker_id": "1",
          "time": "20210822 07:20"
        },
        {
          "id": "MDRW2100006908.1.6",
          "form": "아침 저는 늦게 일어나서 못 챙겨먹었네요..",
          "original_form": "아침 저는 늦게 일어나서 못 챙겨먹었네요..",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100006908.1.7",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100006908.1.8",
          "form": "아이구... 전 주먹밥 먹었습니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이구... 전 주먹밥 먹었습니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100006908.1.9",
          "form": "전 공부를 하다보니까 아침 꼭 필요하더라구요",
          "original_form": "전 공부를 하다보니까 아침 꼭 필요하더라구요",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100006908.1.10",
          "form": "주먹밥 맛있겠네요",
          "original_form": "주먹밥 맛있겠네요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100006908.1.11",
          "form": "공부하면 금방 배고프죠ㅠㅠ",
          "original_form": "공부하면 금방 배고프죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100006908.1.12",
          "form": "맞아요ㅜㅜ 진짜 밥심 한국인....",
          "original_form": "맞아요ㅜㅜ 진짜 밥심 한국인....",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100006908.1.13",
          "form": "쿼카님도 점심 드셔야죠+",
          "original_form": "쿼카님도 점심 드셔야죠+",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100006908.1.14",
          "form": "저는 저녁에 떡볶이 먹기로해서 점심은 다이어트볶음밥 먹고있어요..!",
          "original_form": "저는 저녁에 떡볶이 먹기로해서 점심은 다이어트볶음밥 먹고있어요..!",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100006908.1.15",
          "form": "복숭아님은 점심 뭐드셨나요?",
          "original_form": "복숭아님은 점심 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100006908.1.16",
          "form": "앗 맛있겠다",
          "original_form": "앗 맛있겠다",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100006908.1.17",
          "form": "전 점심 아직이에요!",
          "original_form": "전 점심 아직이에요!",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100006908.1.18",
          "form": "이따가 세시쯤 먹고 저녁을 안먹으려구요 히히",
          "original_form": "이따가 세시쯤 먹고 저녁을 안먹으려구요 히히",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100006908.1.19",
          "form": "어떤거 드실 생각이세요?",
          "original_form": "어떤거 드실 생각이세요?",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100006908.1.20",
          "form": "생각해두신 메뉴있나용",
          "original_form": "생각해두신 메뉴있나용",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100006908.1.21",
          "form": "사실 쫌 고민중인데 국밥 먹을거같아유",
          "original_form": "사실 쫌 고민중인데 국밥 먹을거같아유",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100006908.1.22",
          "form": "든든한 국밥...+",
          "original_form": "든든한 국밥...+",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100006908.1.23",
          "form": "너무 맛있겠어요ㅠㅠ",
          "original_form": "너무 맛있겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100006908.1.24",
          "form": "오늘 날씨 좀 선선해서 국밥먹으면 딱일 것 같아요",
          "original_form": "오늘 날씨 좀 선선해서 국밥먹으면 딱일 것 같아요",
          "speaker_id": "2",
          "time": "20210822 13:42"
        },
        {
