      "utterance": [
        {
          "id": "MDRW2100010247.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100010247.1.2",
          "form": "밖에 비가오네요ㅋㅋㅋ",
          "original_form": "밖에 비가오네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010247.1.3",
          "form": "거기도 비가오나요?",
          "original_form": "거기도 비가오나요?",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010247.1.4",
          "form": "어제? 부터 계속 오네요",
          "original_form": "어제? 부터 계속 오네요",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010247.1.5",
          "form": "중간중간 안 오긴 했었는데 꾸준히와요",
          "original_form": "중간중간 안 오긴 했었는데 꾸준히와요",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010247.1.6",
          "form": "요새 날씨가 동남아 같네요",
          "original_form": "요새 날씨가 동남아 같네요",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100010247.1.7",
          "form": "그러게요ㅋㅋㅋ 여름이 다가려나봐요",
          "original_form": "그러게요ㅋㅋㅋ 여름이 다가려나봐요",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010247.1.8",
          "form": "날이 선선해지니 가을옷 사고싶네요",
          "original_form": "날이 선선해지니 가을옷 사고싶네요",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010247.1.9",
          "form": "옷 많이 사세요?? 전 코로나 터지고 옷 거의 안 사게 되네요",
          "original_form": "옷 많이 사세요?? 전 코로나 터지고 옷 거의 안 사게 되네요",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010247.1.10",
          "form": "이번에 여름옷 두세개 샀나ㅋㅋ",
          "original_form": "이번에 여름옷 두세개 샀나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010247.1.11",
          "form": "저도 거의 안사는거같아요ㅋㅋㅋ \r\n여름극초반에만 샀었고",
          "original_form": "저도 거의 안사는거같아요ㅋㅋㅋ \r\n여름극초반에만 샀었고",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010247.1.12",
          "form": "근데 가을옷 자켓이 사고싶네요",
          "original_form": "근데 가을옷 자켓이 사고싶네요",
          "speaker_id": "2",
          "time": "20210823 16:09"
        },
        {
          "id": "MDRW2100010247.1.13",
          "form": "가을옷은 한달? 잘 입어야 한달반이면 끝이라 뭔가 아깝더라구요",
          "original_form": "가을옷은 한달? 잘 입어야 한달반이면 끝이라 뭔가 아깝더라구요",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010247.1.14",
          "form": "옛날에 왁스자켓 살까말까 엄청 고민했었는데 결국 안 샀어요ㅋ",
          "original_form": "옛날에 왁스자켓 살까말까 엄청 고민했었는데 결국 안 샀어요ㅋ",
          "speaker_id": "1",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100010247.1.15",
          "form": "뭔가근데 가을옷이 이쁘지않나요ㅋㅋㅋ",
          "original_form": "뭔가근데 가을옷이 이쁘지않나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010247.1.16",
          "form": "왁스자켓은 뭔가요?",
          "original_form": "왁스자켓은 뭔가요?",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010247.1.17",
          "form": "음.. 자켓에 왁스(기름) 발려있는건데",
          "original_form": "음.. 자켓에 왁스(기름) 발려있는건데",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100010247.1.18",
          "form": "이쁘긴한데요 이게 관리도 어렵고 어디 묻으면 골아프고 그래요ㅋㅋ",
          "original_form": "이쁘긴한데요 이게 관리도 어렵고 어디 묻으면 골아프고 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100010247.1.19",
          "form": "ㅋㅋㅋ 신기하네요ㅋㅋ",
          "original_form": "ㅋㅋㅋ 신기하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010247.1.20",
          "form": "역시 이쁜옷은",
          "original_form": "역시 이쁜옷은",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010247.1.21",
          "form": "관리가 어렵죠",
          "original_form": "관리가 어렵죠",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010247.1.22",
          "form": "바버 왁스자켓 검색해보세요 은근 많이들 입고다녀요ㅋㅋ",
          "original_form": "바버 왁스자켓 검색해보세요 은근 많이들 입고다녀요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010247.1.23",
          "form": "왁스 없는 것도 있고",
          "original_form": "왁스 없는 것도 있고",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010247.1.24",
          "form": "지금 같은 날씨에 딱 좋겠네요",
          "original_form": "지금 같은 날씨에 딱 좋겠네요",
          "speaker_id": "1",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100010247.1.25",
          "form": "아 저런거군요ㅋㅋㅋ",
          "original_form": "아 저런거군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:17"
        }
      ]
    }
  ]
