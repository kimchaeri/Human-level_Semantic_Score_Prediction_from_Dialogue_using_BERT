      "utterance": [
        {
          "id": "MDRW2100032033.1.1",
          "form": "꾸준하네 멋있어",
          "original_form": "꾸준하네 멋있어",
          "speaker_id": "2",
          "time": "20210905 11:14"
        },
        {
          "id": "MDRW2100032033.1.2",
          "form": "더워",
          "original_form": "더워",
          "speaker_id": "1",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032033.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근ㄷㅔ 다왓어 ㅇㅣ제",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근ㄷㅔ 다왓어 ㅇㅣ제",
          "speaker_id": "1",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032033.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ어디 찍으려고??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ어디 찍으려고??",
          "speaker_id": "2",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032033.1.5",
          "form": "시장?",
          "original_form": "시장?",
          "speaker_id": "2",
          "time": "20210905 11:21"
        },
        {
          "id": "MDRW2100032033.1.6",
          "form": "아니 카페",
          "original_form": "아니 카페",
          "speaker_id": "1",
          "time": "20210905 11:22"
        },
        {
          "id": "MDRW2100032033.1.7",
          "form": "카페 좋지",
          "original_form": "카페 좋지",
          "speaker_id": "2",
          "time": "20210905 11:26"
        },
        {
          "id": "MDRW2100032033.1.8",
          "form": "시원한 곳 가네",
          "original_form": "시원한 곳 가네",
          "speaker_id": "2",
          "time": "20210905 11:26"
        },
        {
          "id": "MDRW2100032033.1.9",
          "form": "도착했으",
          "original_form": "도착했으",
          "speaker_id": "1",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100032033.1.10",
          "form": "ㅋㅋㅋㅋㅋ운동하면 카페인이 최고지",
          "original_form": "ㅋㅋㅋㅋㅋ운동하면 카페인이 최고지",
          "speaker_id": "2",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100032033.1.11",
          "form": "단백질 아니고??",
          "original_form": "단백질 아니고??",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100032033.1.12",
          "form": "그것도 맞고 나 쉐이크 먹었었는데",
          "original_form": "그것도 맞고 나 쉐이크 먹었었는데",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100032033.1.13",
          "form": "어우 난 그거 싫어 맛없오",
          "original_form": "어우 난 그거 싫어 맛없오",
          "speaker_id": "1",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100032033.1.14",
          "form": "엥 너도 먹었어??왜?",
          "original_form": "엥 너도 먹었어??왜?",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100032033.1.15",
          "form": "옛날에 먹어는 봤지 계속 먹고 그런건 아냐",
          "original_form": "옛날에 먹어는 봤지 계속 먹고 그런건 아냐",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100032033.1.16",
          "form": "계속먹은건 디톡스",
          "original_form": "계속먹은건 디톡스",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100032033.1.17",
          "form": "그나마 걘먹을만 해서",
          "original_form": "그나마 걘먹을만 해서",
          "speaker_id": "1",
          "time": "20210905 11:50"
        },
        {
          "id": "MDRW2100032033.1.18",
          "form": "아하 다이어트용으로??",
          "original_form": "아하 다이어트용으로??",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032033.1.19",
          "form": "으응",
          "original_form": "으응",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032033.1.20",
          "form": "건강! 카페인 진짜 적당히 먹어야 해",
          "original_form": "건강! 카페인 진짜 적당히 먹어야 해",
