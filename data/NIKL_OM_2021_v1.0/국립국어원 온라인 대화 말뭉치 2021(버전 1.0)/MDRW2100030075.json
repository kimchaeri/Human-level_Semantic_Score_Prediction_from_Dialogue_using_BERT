      "utterance": [
        {
          "id": "MDRW2100030075.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100030075.1.2",
          "form": "잘안봤어요~",
          "original_form": "잘안봤어요~",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100030075.1.3",
          "form": "스포츠 보시는거 있나요?",
          "original_form": "스포츠 보시는거 있나요?",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030075.1.4",
          "form": "구냥 예능 농구나 축구 한번씩봐요 ㅎ",
          "original_form": "구냥 예능 농구나 축구 한번씩봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030075.1.5",
          "form": "그럼 골때리는 그거 보시나요?",
          "original_form": "그럼 골때리는 그거 보시나요?",
          "speaker_id": "1",
          "time": "20210904 09:43"
        },
        {
          "id": "MDRW2100030075.1.6",
          "form": "틀어져있으면 봐요 ㅋㅋ챙겨보는건아니지만",
          "original_form": "틀어져있으면 봐요 ㅋㅋ챙겨보는건아니지만",
          "speaker_id": "2",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030075.1.7",
          "form": "전 올림픽 기간에 엄청 봤어요 이것저것",
          "original_form": "전 올림픽 기간에 엄청 봤어요 이것저것",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100030075.1.8",
          "form": "전 사실 잘안봣어요ㅠㅠ기사로만 접햇네요",
          "original_form": "전 사실 잘안봣어요ㅠㅠ기사로만 접햇네요",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100030075.1.9",
          "form": "그럼 스포츠를 직접 하는건 즐기시나요?",
          "original_form": "그럼 스포츠를 직접 하는건 즐기시나요?",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100030075.1.10",
          "form": "아뇨ㅜㅜ",
          "original_form": "아뇨ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100030075.1.11",
          "form": "좋아하세요??",
          "original_form": "좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100030075.1.12",
          "form": "네~ 저 이번 올림픽 보고 펜싱을 해보고 싶어졌어요",
          "original_form": "네~ 저 이번 올림픽 보고 펜싱을 해보고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100030075.1.13",
          "form": "오~ 멋쩌보여요",
          "original_form": "오~ 멋쩌보여요",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100030075.1.14",
          "form": "예능에서 칼보여줄때보니 꼬ㅐ 단단하고 날카롭더라구요",
          "original_form": "예능에서 칼보여줄때보니 꼬ㅐ 단단하고 날카롭더라구요",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100030075.1.15",
          "form": "방금 출장가다 봤는데 우리집 근처에 펜싱 클럽이 있더라고요",
          "original_form": "방금 출장가다 봤는데 우리집 근처에 펜싱 클럽이 있더라고요",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100030075.1.16",
          "form": "정말요~~ 운명일수도!",
          "original_form": "정말요~~ 운명일수도!",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100030075.1.17",
          "form": "전펜싱 클럽을 본적이없네요!",
          "original_form": "전펜싱 클럽을 본적이없네요!",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100030075.1.18",
          "form": "이 참에 배워보고 싶어요^-^ 챙챙챙 ㅋㅋ",
          "original_form": "이 참에 배워보고 싶어요^-^ 챙챙챙 ㅋㅋ",
