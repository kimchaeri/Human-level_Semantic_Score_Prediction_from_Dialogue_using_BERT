      "utterance": [
        {
          "id": "MDRW2100040379.1.1",
          "form": "고민중이에요",
          "original_form": "고민중이에요",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100040379.1.2",
          "form": "저보다는 우선 엄마 먼저 사드리고 싶어서요 ㅎㅎ",
          "original_form": "저보다는 우선 엄마 먼저 사드리고 싶어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100040379.1.3",
          "form": "그리고 지플립도 좋지만 폴드가 더 탐이 나기도 하구요",
          "original_form": "그리고 지플립도 좋지만 폴드가 더 탐이 나기도 하구요",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100040379.1.4",
          "form": "효녀시군요",
          "original_form": "효녀시군요",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100040379.1.5",
          "form": "폴드 탐나요",
          "original_form": "폴드 탐나요",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040379.1.6",
          "form": "지플립은 이쁘고ㅋㅋ",
          "original_form": "지플립은 이쁘고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040379.1.7",
          "form": "그쵸 이쁜거 생각하면 지플립인데",
          "original_form": "그쵸 이쁜거 생각하면 지플립인데",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040379.1.8",
          "form": "폴드가 넓고 결정적으로 S펜이 있는게 너무 좋아보이더라구요",
          "original_form": "폴드가 넓고 결정적으로 S펜이 있는게 너무 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040379.1.9",
          "form": "ㅋㅋㅋㅋ 그죠 활용성은",
          "original_form": "ㅋㅋㅋㅋ 그죠 활용성은",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040379.1.10",
          "form": "근데 무거울듯..",
          "original_form": "근데 무거울듯..",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040379.1.11",
          "form": "그게 제일 아쉬운 점인것 같아요",
          "original_form": "그게 제일 아쉬운 점인것 같아요",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040379.1.12",
          "form": "접히는데 두께가 얇아졌으면 더 좋았을 건데",
          "original_form": "접히는데 두께가 얇아졌으면 더 좋았을 건데",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040379.1.13",
          "form": "안 얇아져서요.. ㅠ",
          "original_form": "안 얇아져서요.. ㅠ",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040379.1.14",
          "form": "한계가 잇나바요",
          "original_form": "한계가 잇나바요",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040379.1.15",
          "form": "아직은 기술력에 한계를 보이는 것 같아서",
          "original_form": "아직은 기술력에 한계를 보이는 것 같아서",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040379.1.16",
          "form": "다음번에 나오는 걸 살까 싶기도해요",
          "original_form": "다음번에 나오는 걸 살까 싶기도해요",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040379.1.17",
          "form": "ㅋㅋㅋㅋ글게요 ..담꺼 ..",
          "original_form": "ㅋㅋㅋㅋ글게요 ..담꺼 ..",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100040379.1.18",
          "form": "지플립 케이스가 넘 이쁨",
          "original_form": "지플립 케이스가 넘 이쁨",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100040379.1.19",
          "form": "케이스도 예쁘고",
          "original_form": "케이스도 예쁘고",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100040379.1.20",
          "form": "gif로 설정가능한 외부 액정도 좋은것 같아요",
          "original_form": "gif로 설정가능한 외부 액정도 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100040379.1.21",
          "form": "ㄹㅇㄹㅇ... 뽐뿌오는사진 넘 많....",
          "original_form": "ㄹㅇㄹㅇ... 뽐뿌오는사진 넘 많....",
