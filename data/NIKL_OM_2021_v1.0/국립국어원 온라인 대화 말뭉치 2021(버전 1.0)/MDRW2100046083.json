      "utterance": [
        {
          "id": "MDRW2100046083.1.1",
          "form": "근데 전 사기만 사고 정리는 왤케 못하는지 모르겠어요",
          "original_form": "근데 전 사기만 사고 정리는 왤케 못하는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046083.1.2",
          "form": "그래도 이 주제가 제일 광범위한듯...ㅋㅋ",
          "original_form": "그래도 이 주제가 제일 광범위한듯...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046083.1.3",
          "form": "전 정리병있어서",
          "original_form": "전 정리병있어서",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046083.1.4",
          "form": "정리는 잘하는편이에요",
          "original_form": "정리는 잘하는편이에요",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.5",
          "form": "오오 그게 좋은 거죠ㅠㅠㅠ",
          "original_form": "오오 그게 좋은 거죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.6",
          "form": "전 뭔가 무질서해요...",
          "original_form": "전 뭔가 무질서해요...",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.7",
          "form": "혹시넷플하시나요?",
          "original_form": "혹시넷플하시나요?",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.8",
          "form": "그래도 정리하려고 칸막이 사긴 했는데 휴",
          "original_form": "그래도 정리하려고 칸막이 사긴 했는데 휴",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.9",
          "form": "넷플 지금은 끊었어요ㅜㅜ",
          "original_form": "넷플 지금은 끊었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.10",
          "form": "글쿤용",
          "original_form": "글쿤용",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.11",
          "form": "거기보면 안도 어쩌고 하는 일본여자분나와서",
          "original_form": "거기보면 안도 어쩌고 하는 일본여자분나와서",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.12",
          "form": "그것도 고르는 데 한참 시간 쏟고ㅋㅋㅋ",
          "original_form": "그것도 고르는 데 한참 시간 쏟고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.13",
          "form": "정리도와주는데",
          "original_form": "정리도와주는데",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046083.1.14",
          "form": "나름볼만한..ㅋㅋ",
          "original_form": "나름볼만한..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.15",
          "form": "오 좋은데요",
          "original_form": "오 좋은데요",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.16",
          "form": "저 좀 도와줬음 좋겠는ㅋㅋㅋㅋㅋ",
          "original_form": "저 좀 도와줬음 좋겠는ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.17",
          "form": "신박한정리인가",
          "original_form": "신박한정리인가",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.18",
          "form": "아 그거 알아요",
          "original_form": "아 그거 알아요",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.19",
          "form": "한국프로그램이 그거 따라핱거라구하더라고용",
          "original_form": "한국프로그램이 그거 따라핱거라구하더라고용",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.20",
          "form": "그거보면",
          "original_form": "그거보면",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.21",
          "form": "오오 원조가 있는지 몰랐네요",
          "original_form": "오오 원조가 있는지 몰랐네요",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.22",
          "form": "저희같은",
          "original_form": "저희같은",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.23",
          "form": "쟁임병걸린사람들 ㅋㅋㅋ",
          "original_form": "쟁임병걸린사람들 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.24",
          "form": "핫딜러가 나오나욬ㅋㅋㅋㅋ",
          "original_form": "핫딜러가 나오나욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046083.1.25",
          "form": "나오는것같던데",
          "original_form": "나오는것같던데",
          "speaker_id": "1",
          "time": "20210908 10:13"
