      "utterance": [
        {
          "id": "MDRW2100011579.1.1",
          "form": "저 뮤지컬 예매했어요!!!",
          "original_form": "저 뮤지컬 예매했어요!!!",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011579.1.2",
          "form": "와 어떤거요??",
          "original_form": "와 어떤거요??",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011579.1.3",
          "form": "광화문연가",
          "original_form": "광화문연가",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011579.1.4",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.5",
          "form": "하하.. 아니요",
          "original_form": "하하.. 아니요",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.6",
          "form": "어떤 내용이에요?",
          "original_form": "어떤 내용이에요?",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.7",
          "form": "저도 내용은 아지구잘 몰러요",
          "original_form": "저도 내용은 아지구잘 몰러요",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.8",
          "form": "아직 잘 몰라요",
          "original_form": "아직 잘 몰라요",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.9",
          "form": "원래 내용 안보고 가거든요",
          "original_form": "원래 내용 안보고 가거든요",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.10",
          "form": "그걸 선택하신 이유가 있나요?",
          "original_form": "그걸 선택하신 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.11",
          "form": "일단 엄청 많이 할인해줘서",
          "original_form": "일단 엄청 많이 할인해줘서",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011579.1.12",
          "form": "9500원에 예매가능해서요 ㅋㅋ",
          "original_form": "9500원에 예매가능해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011579.1.13",
          "form": "와 정말 싸네요",
          "original_form": "와 정말 싸네요",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011579.1.14",
          "form": "네 그래서 일단 가도 손해는 없을 것 같아요",
          "original_form": "네 그래서 일단 가도 손해는 없을 것 같아요",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011579.1.15",
          "form": "누구 나오나요?",
          "original_form": "누구 나오나요?",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011579.1.16",
          "form": "윤도현 강필석 엄기준",
          "original_form": "윤도현 강필석 엄기준",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011579.1.17",
          "form": "차지연 김호영 나와요!",
          "original_form": "차지연 김호영 나와요!",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011579.1.18",
          "form": "엄기준 기대되네요",
          "original_form": "엄기준 기대되네요",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011579.1.19",
          "form": "네 근데 저는 저번에 엄기준 삼총사에서 봤을 땐",
          "original_form": "네 근데 저는 저번에 엄기준 삼총사에서 봤을 땐",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011579.1.20",
          "form": "좀 별로였어서",
          "original_form": "좀 별로였어서",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011579.1.21",
          "form": "강필석으로 보려구요",
          "original_form": "강필석으로 보려구요",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011579.1.22",
          "form": "왜 별로였어요?",
          "original_form": "왜 별로였어요?",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011579.1.23",
          "form": "노래가 다른 분들보단 좀 약하더라구요",
          "original_form": "노래가 다른 분들보단 좀 약하더라구요",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011579.1.24",
          "form": "아 그렇군요..",
          "original_form": "아 그렇군요..",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011579.1.25",
          "form": "내일 광화문연가 보러가려면 일찍 자야하는데 ㅜㅜ",
