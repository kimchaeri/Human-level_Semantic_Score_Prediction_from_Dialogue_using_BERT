      "utterance": [
        {
          "id": "MDRW2100038398.1.1",
          "form": "요리하다보면 야채손질이 진짜 귀찮거든요",
          "original_form": "요리하다보면 야채손질이 진짜 귀찮거든요",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038398.1.2",
          "form": "하긴 맞아요 ㅋㄱㄲ",
          "original_form": "하긴 맞아요 ㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038398.1.3",
          "form": "사람들도 손질된거 사잖아요ㅋㅋㅋ",
          "original_form": "사람들도 손질된거 사잖아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100038398.1.4",
          "form": "마트에서 손질 야채 사면 비닐쓰레기가 너무 많아요",
          "original_form": "마트에서 손질 야채 사면 비닐쓰레기가 너무 많아요",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100038398.1.5",
          "form": "그렇다고 시장에서 대용량을 사면",
          "original_form": "그렇다고 시장에서 대용량을 사면",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100038398.1.6",
          "form": "버리는게 반이고ㅠ",
          "original_form": "버리는게 반이고ㅠ",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100038398.1.7",
          "form": "아 맞아요 ㅜㅜ",
          "original_form": "아 맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038398.1.8",
          "form": "그래도 전 버리는게 나아서",
          "original_form": "그래도 전 버리는게 나아서",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038398.1.9",
          "form": "적당한 크기로 사요 ㅋㄱㄲ",
          "original_form": "적당한 크기로 사요 ㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038398.1.10",
          "form": "양배추가 그래도 싸고 오래가고 활용도도 높아서 자주 쓰는데",
          "original_form": "양배추가 그래도 싸고 오래가고 활용도도 높아서 자주 쓰는데",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038398.1.11",
          "form": "손질이 힘들어요",
          "original_form": "손질이 힘들어요",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100038398.1.12",
          "form": "아 맞아요 ㅎㅎ 저는 양상추를 더 좋아하는데",
          "original_form": "아 맞아요 ㅎㅎ 저는 양상추를 더 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038398.1.13",
          "form": "양상추는 금방 상해서",
          "original_form": "양상추는 금방 상해서",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038398.1.14",
          "form": "양배추 사서 쪄먹거나 해요 !",
          "original_form": "양배추 사서 쪄먹거나 해요 !",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038398.1.15",
          "form": "맞아요, 걔도 손질이 쉽진않고",
          "original_form": "맞아요, 걔도 손질이 쉽진않고",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038398.1.16",
          "form": "양상추는 서브웨이갈때나 많이 넣어달라고 하는편 ㅇㅅ",
          "original_form": "양상추는 서브웨이갈때나 많이 넣어달라고 하는편 ㅇㅅ",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038398.1.17",
          "form": "마자요...단단 그자체",
          "original_form": "마자요...단단 그자체",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100038398.1.18",
          "form": "아ㅈ서브웨이 좋아하세요?",
          "original_form": "아ㅈ서브웨이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038398.1.19",
          "form": "종종 가요",
          "original_form": "종종 가요",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038398.1.20",
          "form": "신선한 야채가 필요하다 싶을때",
          "original_form": "신선한 야채가 필요하다 싶을때",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038398.1.21",
          "form": "아 ㅋㅋㅋ 맞아요 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038398.1.22",
          "form": "전 올리브싫어해서",
          "original_form": "전 올리브싫어해서",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038398.1.23",
          "form": "올리브는 빼욯ㅎ",
          "original_form": "올리브는 빼욯ㅎ",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100038398.1.24",
          "form": "아, 전 올리브랑 피클빼요",
          "original_form": "아, 전 올리브랑 피클빼요",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038398.1.25",
          "form": "오이는 넣지만요",
          "original_form": "오이는 넣지만요",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038398.1.26",
          "form": "저두여 ㅋㅋㅋ 오이 좋아해서 ㅋㅋㅋ",
          "original_form": "저두여 ㅋㅋㅋ 오이 좋아해서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038398.1.27",
          "form": "오이 맛있죠",
          "original_form": "오이 맛있죠",
          "speaker_id": "2",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100038398.1.28",
          "form": "가끔 간식으로 생오이 으적으적 씹어먹기도해요",
          "original_form": "가끔 간식으로 생오이 으적으적 씹어먹기도해요",
          "speaker_id": "2",
