      "utterance": [
        {
          "id": "MDRW2100004190.1.1",
          "form": "여행 가는 거 좋아하세요????",
          "original_form": "여행 가는 거 좋아하세요????",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004190.1.2",
          "form": "네에 name2님 일본가셨을때 어디어디가셨어요?",
          "original_form": "네에 &name2&님 일본가셨을때 어디어디가셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004190.1.3",
          "form": "저는 오사카만 가봤어요",
          "original_form": "저는 오사카만 가봤어요",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004190.1.4",
          "form": "첫 해외여행이라 너무 설렜어요",
          "original_form": "첫 해외여행이라 너무 설렜어요",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004190.1.5",
          "form": "첫여행진짜 비행기탈때부터 설레요 ㅎㅎ",
          "original_form": "첫여행진짜 비행기탈때부터 설레요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004190.1.6",
          "form": "뭐뭐드시고오셨어요",
          "original_form": "뭐뭐드시고오셨어요",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004190.1.7",
          "form": "저 오꼬나미야끼랑 타코야끼랑 등",
          "original_form": "저 오꼬나미야끼랑 타코야끼랑 등",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004190.1.8",
          "form": "막 엄청 먹었어요",
          "original_form": "막 엄청 먹었어요",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004190.1.9",
          "form": "저도 일본간다면 그두개먹어보고싶어요 단짠단짠음식을좋아해서",
          "original_form": "저도 일본간다면 그두개먹어보고싶어요 단짠단짠음식을좋아해서",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004190.1.10",
          "form": "타코야끼 문어 쪼꼬만거아니고 큰거넣어주나여",
          "original_form": "타코야끼 문어 쪼꼬만거아니고 큰거넣어주나여",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004190.1.11",
          "form": "네 그냥 한국이렁 비슷했던 거 같기도 해요 하하",
          "original_form": "네 그냥 한국이렁 비슷했던 거 같기도 해요 하하",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004190.1.12",
          "form": "ㅎㅎ아무래도 근처나라고 한국에도 일식워낙에 많이팔아서 그른가봐여",
          "original_form": "ㅎㅎ아무래도 근처나라고 한국에도 일식워낙에 많이팔아서 그른가봐여",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004190.1.13",
          "form": "맞아요 ㅎ 다들 비슷해서",
          "original_form": "맞아요 ㅎ 다들 비슷해서",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004190.1.14",
          "form": "국내여행은 어디가 좋았어요?",
          "original_form": "국내여행은 어디가 좋았어요?",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004190.1.15",
          "form": "국내는 그냥 단순한데 서울이요 ㅎ",
          "original_form": "국내는 그냥 단순한데 서울이요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004190.1.16",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004190.1.17",
          "form": "서울좋져 서울은 각 동네마다 볼거리가많아서 좋았던것같아요 한강도 넘좋았구여",
          "original_form": "서울좋져 서울은 각 동네마다 볼거리가많아서 좋았던것같아요 한강도 넘좋았구여",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004190.1.18",
          "form": "전 포항이요 포항바다가 너무이뻤어요ㅜ 제주도도좋았는데",
          "original_form": "전 포항이요 포항바다가 너무이뻤어요ㅜ 제주도도좋았는데",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004190.1.19",
          "form": "코로나얼른끝났으면..",
          "original_form": "코로나얼른끝났으면..",
          "speaker_id": "1",
          "time": "20210821 18:22"
        },
        {
          "id": "MDRW2100004190.1.20",
          "form": "우와 포항도 좋아보이네요",
          "original_form": "우와 포항도 좋아보이네요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004190.1.21",
          "form": "급 궁금해요",
          "original_form": "급 궁금해요",
          "speaker_id": "2",
