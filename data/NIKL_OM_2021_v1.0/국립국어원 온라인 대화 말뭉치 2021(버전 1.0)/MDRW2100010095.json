      "utterance": [
        {
          "id": "MDRW2100010095.1.1",
          "form": "하고 싶은 일이나 직업 있어?",
          "original_form": "하고 싶은 일이나 직업 있어?",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010095.1.2",
          "form": "나는 정해져 있지 시험공부하잖아",
          "original_form": "나는 정해져 있지 시험공부하잖아",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010095.1.3",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "1",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010095.1.4",
          "form": "뭐 하고싶은 거 있어?",
          "original_form": "뭐 하고싶은 거 있어?",
          "speaker_id": "2",
          "time": "20210823 14:17"
        },
        {
          "id": "MDRW2100010095.1.5",
          "form": "음 병원쪽?",
          "original_form": "음 병원쪽?",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010095.1.6",
          "form": "일단 좀 폭넓게 생각해보려고?",
          "original_form": "일단 좀 폭넓게 생각해보려고?",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010095.1.7",
          "form": "응 계속 고민중",
          "original_form": "응 계속 고민중",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010095.1.8",
          "form": "고민이 많이 되겠네",
          "original_form": "고민이 많이 되겠네",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010095.1.9",
          "form": "무슨 공부하는 거야?",
          "original_form": "무슨 공부하는 거야?",
          "speaker_id": "1",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010095.1.10",
          "form": "알잖아",
          "original_form": "알잖아",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010095.1.11",
          "form": "무슨 공부하고 있어?",
          "original_form": "무슨 공부하고 있어?",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010095.1.12",
          "form": "고민중",
          "original_form": "고민중",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100010095.1.13",
          "form": "도움 필요하면 얘기해줘",
