      "utterance": [
        {
          "id": "MDRW2100015967.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210826 04:11"
        },
        {
          "id": "MDRW2100015967.1.2",
          "form": "온라인 커뮤니티 하시나요?",
          "original_form": "온라인 커뮤니티 하시나요?",
          "speaker_id": "1",
          "time": "20210826 04:11"
        },
        {
          "id": "MDRW2100015967.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 04:12"
        },
        {
          "id": "MDRW2100015967.1.4",
          "form": "넹 sns랑 카페 같은 것도 커뮤니티 활동이죠??",
          "original_form": "넹 sns랑 카페 같은 것도 커뮤니티 활동이죠??",
          "speaker_id": "2",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015967.1.5",
          "form": "그쵸그쵸!",
          "original_form": "그쵸그쵸!",
          "speaker_id": "1",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015967.1.6",
          "form": "블로그도 커뮤니티라고 하더라구요",
          "original_form": "블로그도 커뮤니티라고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 04:13"
        },
        {
          "id": "MDRW2100015967.1.7",
          "form": "오호! 뭔가 정보 찾으려고 검색하면 왠지 커뮤니티들이 많이 나오더라구여!",
          "original_form": "오호! 뭔가 정보 찾으려고 검색하면 왠지 커뮤니티들이 많이 나오더라구여!",
          "speaker_id": "2",
          "time": "20210826 04:14"
        },
        {
          "id": "MDRW2100015967.1.8",
          "form": "블로그 하시나영~??",
          "original_form": "블로그 하시나영~??",
          "speaker_id": "2",
          "time": "20210826 04:15"
        },
        {
          "id": "MDRW2100015967.1.9",
          "form": "그러더라구용ㅎㅎ 블로그 했는데",
          "original_form": "그러더라구용ㅎㅎ 블로그 했는데",
          "speaker_id": "1",
          "time": "20210826 04:15"
        },
        {
          "id": "MDRW2100015967.1.10",
          "form": "누가 자꾸 제 블로그에",
          "original_form": "누가 자꾸 제 블로그에",
          "speaker_id": "1",
          "time": "20210826 04:15"
        },
        {
          "id": "MDRW2100015967.1.11",
          "form": "주기적으로 오는게 무서워서ㅠㅠㅋㅋ",
          "original_form": "주기적으로 오는게 무서워서ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:15"
        },
        {
          "id": "MDRW2100015967.1.12",
          "form": "접었어요",
          "original_form": "접었어요",
          "speaker_id": "1",
          "time": "20210826 04:15"
        },
        {
          "id": "MDRW2100015967.1.13",
          "form": "오 관심이 쏟아지면 부담스러울 수 있죠!",
          "original_form": "오 관심이 쏟아지면 부담스러울 수 있죠!",
          "speaker_id": "2",
          "time": "20210826 04:16"
        },
        {
          "id": "MDRW2100015967.1.14",
          "form": "전 블로그 꾸미는 게 은근 어렵더라구여 ㅜㅜ",
          "original_form": "전 블로그 꾸미는 게 은근 어렵더라구여 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 04:16"
        },
        {
          "id": "MDRW2100015967.1.15",
          "form": "네네ㅋㅋ 혼잣말 써놓는 블로그였는데ㅠ",
          "original_form": "네네ㅋㅋ 혼잣말 써놓는 블로그였는데ㅠ",
          "speaker_id": "1",
          "time": "20210826 04:16"
        },
        {
          "id": "MDRW2100015967.1.16",
          "form": "레이아웃도 다양하고",
          "original_form": "레이아웃도 다양하고",
          "speaker_id": "1",
          "time": "20210826 04:16"
        },
        {
          "id": "MDRW2100015967.1.17",
          "form": "꾸미기 어려워용",
          "original_form": "꾸미기 어려워용",
          "speaker_id": "1",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015967.1.18",
          "form": "약간 다꾸 같은 느낌이에요",
          "original_form": "약간 다꾸 같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015967.1.19",
          "form": "잘 꾸미시는 분들 보면 신기하고",
          "original_form": "잘 꾸미시는 분들 보면 신기하고",
          "speaker_id": "2",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015967.1.20",
          "form": "무슨 느낌인지 알것같아요ㅋㅋ",
          "original_form": "무슨 느낌인지 알것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015967.1.21",
          "form": "그리고 블로그도 운영하려면 엄청 부지런 해야 되더라구요",
          "original_form": "그리고 블로그도 운영하려면 엄청 부지런 해야 되더라구요",
          "speaker_id": "2",
          "time": "20210826 04:17"
        },
        {
          "id": "MDRW2100015967.1.22",
          "form": "업로드 주기 길어지면",
          "original_form": "업로드 주기 길어지면",
          "speaker_id": "1",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015967.1.23",
          "form": "막 순위에서 내려가고",
          "original_form": "막 순위에서 내려가고",
          "speaker_id": "1",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015967.1.24",
          "form": "그러더라구요",
          "original_form": "그러더라구요",
          "speaker_id": "1",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015967.1.25",
          "form": "꾸준히 하시는 분들 보면 오오 하고 대단하다 싶어요",
          "original_form": "꾸준히 하시는 분들 보면 오오 하고 대단하다 싶어요",
          "speaker_id": "2",
          "time": "20210826 04:18"
        },
        {
          "id": "MDRW2100015967.1.26",
          "form": "맞아요 이건 끈기인것같아요",
          "original_form": "맞아요 이건 끈기인것같아요",
          "speaker_id": "1",
          "time": "20210826 04:19"
        }
      ]
    }
  ]
