      "utterance": [
        {
          "id": "MDRW2100021202.1.1",
          "form": "나 지금 배고픈데 뭐 먹을까?",
          "original_form": "나 지금 배고픈데 뭐 먹을까?",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021202.1.2",
          "form": "나 점심때 갈치조림 먹었는데 맛있었어",
          "original_form": "나 점심때 갈치조림 먹었는데 맛있었어",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021202.1.3",
          "form": "아 갈치조림 맛있겠다!",
          "original_form": "아 갈치조림 맛있겠다!",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021202.1.4",
          "form": "응 맛있더라 어릴땐 안좋아했는데..",
          "original_form": "응 맛있더라 어릴땐 안좋아했는데..",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021202.1.5",
          "form": "나는 어려서부터 생선 되게 좋아했어",
          "original_form": "나는 어려서부터 생선 되게 좋아했어",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021202.1.6",
          "form": "아 정말? 맛잘알 어린이였네",
          "original_form": "아 정말? 맛잘알 어린이였네",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021202.1.7",
          "form": "그치 어려서부터 맛잘알이었지!",
          "original_form": "그치 어려서부터 맛잘알이었지!",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021202.1.8",
          "form": "응 난 어릴땐 생선 맛을 모르겠었어 그냥 맹맛인줄",
          "original_form": "응 난 어릴땐 생선 맛을 모르겠었어 그냥 맹맛인줄",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021202.1.9",
          "form": "나는 할머니가 임연수구이를 엄청 맛있게 해주셨었어!",
          "original_form": "나는 할머니가 임연수구이를 엄청 맛있게 해주셨었어!",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021202.1.10",
          "form": "헉 그거 안먹어봤는데 흰살생선이야?",
          "original_form": "헉 그거 안먹어봤는데 흰살생선이야?",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021202.1.11",
          "form": "응응! 되게 고소하고 맛있어! 두부찌개에 임연수구이, 계란후라이 이렇게 하면 밥 세공기 뚝딱",
          "original_form": "응응! 되게 고소하고 맛있어! 두부찌개에 임연수구이, 계란후라이 이렇게 하면 밥 세공기 뚝딱",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021202.1.12",
          "form": "아 듣기만해도 맛있다.. 밥도둑",
          "original_form": "아 듣기만해도 맛있다.. 밥도둑",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021202.1.13",
          "form": "밥도둑 하니까 생각난건데 얼마전에 게살장 사봤어",
          "original_form": "밥도둑 하니까 생각난건데 얼마전에 게살장 사봤어",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021202.1.14",
          "form": "오 맛있겠다. 먹어봤어?",
          "original_form": "오 맛있겠다. 먹어봤어?",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021202.1.15",
          "form": "응 먹어봤는데 편하긴 한데",
          "original_form": "응 먹어봤는데 편하긴 한데",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021202.1.16",
          "form": "안좋은점이 있는거야?",
          "original_form": "안좋은점이 있는거야?",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021202.1.17",
          "form": "그집건 양념이 별로였어ㅠ 다른데서도 한번 시켜먹어보려고",
          "original_form": "그집건 양념이 별로였어ㅠ 다른데서도 한번 시켜먹어보려고",
          "speaker_id": "1",
          "time": "20210828 16:52"
        }
      ]
