      "utterance": [
        {
          "id": "MDRW2100000683.1.1",
          "form": "혹시 키우시는 동물 있으신가요?",
          "original_form": "혹시 키우시는 동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000683.1.2",
          "form": "저는 키우는 동물없어요ㅜ",
          "original_form": "저는 키우는 동물없어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000683.1.3",
          "form": "앗 저는 강아지 키우고 있어요",
          "original_form": "앗 저는 강아지 키우고 있어요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000683.1.4",
          "form": "키우는 동물 있으세요?",
          "original_form": "키우는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000683.1.5",
          "form": "만약 키운다면 어떤 동물을 키우고 싶으세요??",
          "original_form": "만약 키운다면 어떤 동물을 키우고 싶으세요??",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000683.1.6",
          "form": "고양이요!",
          "original_form": "고양이요!",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000683.1.7",
          "form": "강아지 어떤 종인가요?",
          "original_form": "강아지 어떤 종인가요?",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000683.1.8",
          "form": "푸들키우고 있어요!",
          "original_form": "푸들키우고 있어요!",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000683.1.9",
          "form": "저희 사무실에 고양이 키우시는 분 있는데",
          "original_form": "저희 사무실에 고양이 키우시는 분 있는데",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000683.1.10",
          "form": "가끔 데려오셔서 봤는데 귀엽더라구요",
          "original_form": "가끔 데려오셔서 봤는데 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000683.1.11",
          "form": "푸들! 제 사촌동생도 푸들 키우는데 귀엽죠 푸들ㅜ",
          "original_form": "푸들! 제 사촌동생도 푸들 키우는데 귀엽죠 푸들ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000683.1.12",
          "form": "고양이는 개냥이일수록 귀여워요",
          "original_form": "고양이는 개냥이일수록 귀여워요",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000683.1.13",
          "form": "그 고양이가 마침 개냥이더라구요ㅠㅠ",
          "original_form": "그 고양이가 마침 개냥이더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000683.1.14",
          "form": "너무귀엽ㅜ",
          "original_form": "너무귀엽ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000683.1.15",
          "form": "막 다가와서 부비거리지않나요?!",
          "original_form": "막 다가와서 부비거리지않나요?!",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000683.1.16",
          "form": "그걸보다 장난감 던져주면 혼자 잘놀고",
          "original_form": "그걸보다 장난감 던져주면 혼자 잘놀고",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000683.1.17",
          "form": "가끔 자기한테 무관심한사람한테",
          "original_form": "가끔 자기한테 무관심한사람한테",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000683.1.18",
          "form": "비비러오고 그래요",
          "original_form": "비비러오고 그래요",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000683.1.19",
          "form": "ㅋㅋㅋ애교쟁이네요",
          "original_form": "ㅋㅋㅋ애교쟁이네요",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000683.1.20",
          "form": "고양이도 키워보고싶네용",
          "original_form": "고양이도 키워보고싶네용",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000683.1.21",
          "form": "관심 있으시면 고양이 카페에도 한 번 가보세용",
          "original_form": "관심 있으시면 고양이 카페에도 한 번 가보세용",
          "speaker_id": "2",
          "time": "20210819 17:55"
        }
      ]
