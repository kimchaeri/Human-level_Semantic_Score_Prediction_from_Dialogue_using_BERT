      "utterance": [
        {
          "id": "MDRW2100000742.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100000742.1.2",
          "form": "안녕하세요~ 코로나때문에 모임갖기가 쉽지않죠ㅠ",
          "original_form": "안녕하세요~ 코로나때문에 모임갖기가 쉽지않죠ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000742.1.3",
          "form": "맞아요 집에만 있어요 ㅋㅋ그래서",
          "original_form": "맞아요 집에만 있어요 ㅋㅋ그래서",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000742.1.4",
          "form": "원래 모임 좋아하시나요?",
          "original_form": "원래 모임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000742.1.5",
          "form": "낯선 모임은 좀 꺼려하는 편이에요 내성적이라 ㅎㅎ",
          "original_form": "낯선 모임은 좀 꺼려하는 편이에요 내성적이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000742.1.6",
          "form": "맞아요 알던사람 만나는게 최고죠",
          "original_form": "맞아요 알던사람 만나는게 최고죠",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000742.1.7",
          "form": "마음도 편하구",
          "original_form": "마음도 편하구",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000742.1.8",
          "form": "네 맘 맞는 사람하고 대화할때가 제일 편하고 좋은거같아요",
          "original_form": "네 맘 맞는 사람하고 대화할때가 제일 편하고 좋은거같아요",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000742.1.9",
          "form": "저두 사람 만나는거 좋아하는데 예전만큼 못만나서 아쉬워요",
          "original_form": "저두 사람 만나는거 좋아하는데 예전만큼 못만나서 아쉬워요",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000742.1.10",
          "form": "흠 이렇게 길어질지 몰랏네요.. 저는",
          "original_form": "흠 이렇게 길어질지 몰랏네요.. 저는",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000742.1.11",
          "form": "저두요...",
          "original_form": "저두요...",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000742.1.12",
          "form": "이제 좀 만나고 싶은데 좀 그런거 만들기도 어렵네요 ㅋㅋ 뭔가",
          "original_form": "이제 좀 만나고 싶은데 좀 그런거 만들기도 어렵네요 ㅋㅋ 뭔가",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000742.1.13",
          "form": "그쵸?!",
          "original_form": "그쵸?!",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000742.1.14",
          "form": "먼저 말꺼내기가 쉽지도 않구",
          "original_form": "먼저 말꺼내기가 쉽지도 않구",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000742.1.15",
          "form": "회식도 많이 사라지고 있는것같아요",
          "original_form": "회식도 많이 사라지고 있는것같아요",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000742.1.16",
          "form": "ㅎㅎ 내년에도 이럴거같은데 .. ㅎ",
          "original_form": "ㅎㅎ 내년에도 이럴거같은데 .. ㅎ",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100000742.1.17",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000742.1.18",
          "form": ".....또르륵?",
          "original_form": ".....또르륵?",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000742.1.19",
          "form": "오히려 마음은 편해요....!ㅋㅋㅋ",
          "original_form": "오히려 마음은 편해요....!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000742.1.20",
          "form": "ㅋㅋ 저는 취준생인데 근데 만남보다 자기계발에 더 시간 투입할수잇는점은 좋은거같아요.. ㅋ",
          "original_form": "ㅋㅋ 저는 취준생인데 근데 만남보다 자기계발에 더 시간 투입할수잇는점은 좋은거같아요.. ㅋ",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000742.1.21",
          "form": "그쵸?!ㅎㅎ",
          "original_form": "그쵸?!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000742.1.22",
          "form": "자기한테 투자하는게 최고인것같아요",
          "original_form": "자기한테 투자하는게 최고인것같아요",
