      "utterance": [
        {
          "id": "MDRW2100034969.1.1",
          "form": "아 맛있죠 크랜베리두",
          "original_form": "아 맛있죠 크랜베리두",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034969.1.2",
          "form": "저는 아몬드도 곁들인거 좋아해요",
          "original_form": "저는 아몬드도 곁들인거 좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034969.1.3",
          "form": "아몬드ㅠㅠ",
          "original_form": "아몬드ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034969.1.4",
          "form": "혹시 베이글에",
          "original_form": "혹시 베이글에",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034969.1.5",
          "form": "요거트 발라서 드셔보셨나요?",
          "original_form": "요거트 발라서 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034969.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034969.1.7",
          "form": "아니요 크림치즈는 해봤는데",
          "original_form": "아니요 크림치즈는 해봤는데",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034969.1.8",
          "form": "요거트는 맛있나요?",
          "original_form": "요거트는 맛있나요?",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034969.1.9",
          "form": "네네! 요거트가 달달하고 시원해서",
          "original_form": "네네! 요거트가 달달하고 시원해서",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034969.1.10",
          "form": "같이먹으면 환상이에요ㅋㅋㅋㅋ",
          "original_form": "같이먹으면 환상이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034969.1.11",
          "form": "베이글이랑 궁합이 잘맞아요",
          "original_form": "베이글이랑 궁합이 잘맞아요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034969.1.12",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034969.1.13",
          "form": "또 name1님덕에 먹어볼게생겼군요",
          "original_form": "또 &name1&님덕에 먹어볼게생겼군요",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034969.1.14",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034969.1.15",
          "form": "그릭복숭아라고",
          "original_form": "그릭복숭아라고",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034969.1.16",
          "form": "나혼자산다에 키가 나와서 했던 요린데 핫하더라구요",
          "original_form": "나혼자산다에 키가 나와서 했던 요린데 핫하더라구요",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034969.1.17",
          "form": "저희동네 베이글 잘하는집이 있어서 함 해봐야겠어요",
          "original_form": "저희동네 베이글 잘하는집이 있어서 함 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034969.1.18",
          "form": "오 키가참 잘먹어요",
          "original_form": "오 키가참 잘먹어요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034969.1.19",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034969.1.20",
          "form": "복숭아에",
          "original_form": "복숭아에",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034969.1.21",
          "form": "홈을 파서",
          "original_form": "홈을 파서",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034969.1.22",
          "form": "그 안에 그릭요거트를 집어넣고",
          "original_form": "그 안에 그릭요거트를 집어넣고",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034969.1.23",
          "form": "먹는건데",
          "original_form": "먹는건데",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034969.1.24",
          "form": "엄머",
          "original_form": "엄머",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034969.1.25",
          "form": "요즘 sns에 이 음식 계속 올라오더라구요",
          "original_form": "요즘 sns에 이 음식 계속 올라오더라구요",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034969.1.26",
          "form": "맛없을수가 없다",
          "original_form": "맛없을수가 없다",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034969.1.27",
          "form": "복숭아 끝물인데 해보고싶네요 그럼 씨를 파내야겠당",
          "original_form": "복숭아 끝물인데 해보고싶네요 그럼 씨를 파내야겠당",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034969.1.28",
          "form": "넹넹 추천드려요ㅋㅋ",
          "original_form": "넹넹 추천드려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034969.1.29",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.30",
          "form": "맥도날드에서 파는 라떼에",
          "original_form": "맥도날드에서 파는 라떼에",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.31",
          "form": "오 감사함니다",
          "original_form": "오 감사함니다",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.32",
          "form": "맥도날드 콘 아이스크림 있죠",
          "original_form": "맥도날드 콘 아이스크림 있죠",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.33",
          "form": "그거 퐁당 담궈먹으면",
          "original_form": "그거 퐁당 담궈먹으면",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.34",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.35",
          "form": "폴바셋 느낌난대요",
          "original_form": "폴바셋 느낌난대요",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.36",
          "form": "맥도날드 커피는 맛없기로 소문났잖아요",
          "original_form": "맥도날드 커피는 맛없기로 소문났잖아요",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.37",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "2",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.38",
          "form": "맛없기롴ㅋㅋㅋㅋ",
          "original_form": "맛없기롴ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034969.1.39",
          "form": "최근에 그래도 나아졌다고 하더라구요",
          "original_form": "최근에 그래도 나아졌다고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 23:14"
        }
      ]
