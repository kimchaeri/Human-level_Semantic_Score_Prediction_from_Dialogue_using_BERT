      "utterance": [
        {
          "id": "MDRW2100004168.1.1",
          "form": "지금 하고 있는 동호회나 봉사 활동이 있어?",
          "original_form": "지금 하고 있는 동호회나 봉사 활동이 있어?",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004168.1.2",
          "form": "아니 없어~",
          "original_form": "아니 없어~",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004168.1.3",
          "form": "동호회면 스터디도 포함인가?",
          "original_form": "동호회면 스터디도 포함인가?",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004168.1.4",
          "form": "그것도 포함되지 않을까?",
          "original_form": "그것도 포함되지 않을까?",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004168.1.5",
          "form": "무슨 스터디를 하고 있는데?",
          "original_form": "무슨 스터디를 하고 있는데?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004168.1.6",
          "form": "그럼 친구들이랑 주식스터디 중이야!",
          "original_form": "그럼 친구들이랑 주식스터디 중이야!",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004168.1.7",
          "form": "너는 뭐 하는거 잇어?",
          "original_form": "너는 뭐 하는거 잇어?",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004168.1.8",
          "form": "아니 근데 해보고는 싶어!",
          "original_form": "아니 근데 해보고는 싶어!",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004168.1.9",
          "form": "오 어떤거 해보고싶어?",
          "original_form": "오 어떤거 해보고싶어?",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004168.1.10",
          "form": "주식이나 악기 배우는 모임!",
          "original_form": "주식이나 악기 배우는 모임!",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004168.1.11",
          "form": "네가 하는 스터디는 몇명이서 하고 있어?",
          "original_form": "네가 하는 스터디는 몇명이서 하고 있어?",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004168.1.12",
          "form": "오 나도 악기 배우고 싶어",
          "original_form": "오 나도 악기 배우고 싶어",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004168.1.13",
          "form": "우리 5명!",
          "original_form": "우리 5명!",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004168.1.14",
          "form": "소소하지",
          "original_form": "소소하지",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004168.1.15",
          "form": "악기 스터디하면 무슨 악기 배우고 싶어?",
          "original_form": "악기 스터디하면 무슨 악기 배우고 싶어?",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004168.1.16",
          "form": "음...바이올린이나 플룻?",
          "original_form": "음...바이올린이나 플룻?",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004168.1.17",
          "form": "와 바이올린 멋잇지",
          "original_form": "와 바이올린 멋잇지",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004168.1.18",
          "form": "나는 기타배우고 싶어",
          "original_form": "나는 기타배우고 싶어",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004168.1.19",
          "form": "오 기타도 멋있어!",
          "original_form": "오 기타도 멋있어!",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004168.1.20",
          "form": "기타 배우면 치고 싶은 곡이 있어?",
          "original_form": "기타 배우면 치고 싶은 곡이 있어?",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004168.1.21",
          "form": "전에 누가 호텔캘리포니아 치는 영상보고 반해가지고ㅋㅋ",
          "original_form": "전에 누가 호텔캘리포니아 치는 영상보고 반해가지고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004168.1.22",
          "form": "합주도 해보고싶고",
          "original_form": "합주도 해보고싶고",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004168.1.23",
          "form": "나 방금 호텔캘리포니아 기타버전 검색해보고 왔어!",
          "original_form": "나 방금 호텔캘리포니아 기타버전 검색해보고 왔어!",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004168.1.24",
          "form": "멋있는 곡이네",
          "original_form": "멋있는 곡이네",
          "speaker_id": "1",
