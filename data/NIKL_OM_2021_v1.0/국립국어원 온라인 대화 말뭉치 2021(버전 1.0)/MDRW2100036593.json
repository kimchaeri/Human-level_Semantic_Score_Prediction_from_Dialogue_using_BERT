      "utterance": [
        {
          "id": "MDRW2100036593.1.1",
          "form": "메이크업에 제일 중요한게 피부표현",
          "original_form": "메이크업에 제일 중요한게 피부표현",
          "speaker_id": "1",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100036593.1.2",
          "form": "그래 기초가 중요하지",
          "original_form": "그래 기초가 중요하지",
          "speaker_id": "2",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100036593.1.3",
          "form": "응 피부표현이 잘 되어있어야 그위에 화장이 살아나거든",
          "original_form": "응 피부표현이 잘 되어있어야 그위에 화장이 살아나거든",
          "speaker_id": "1",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100036593.1.4",
          "form": "근데 피부표현이 쉽지않다",
          "original_form": "근데 피부표현이 쉽지않다",
          "speaker_id": "2",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100036593.1.5",
          "form": "엉 잘먹지 않고 들떠서",
          "original_form": "엉 잘먹지 않고 들떠서",
          "speaker_id": "1",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100036593.1.6",
          "form": "그래 착 밀착이 되어야하는데",
          "original_form": "그래 착 밀착이 되어야하는데",
          "speaker_id": "2",
          "time": "20210831 01:27"
        },
        {
          "id": "MDRW2100036593.1.7",
          "form": "그러게 테크닉이 필요없을거 같은데 어려움",
          "original_form": "그러게 테크닉이 필요없을거 같은데 어려움",
          "speaker_id": "1",
          "time": "20210831 01:27"
        },
        {
          "id": "MDRW2100036593.1.8",
          "form": "각질때문에 그러나",
          "original_form": "각질때문에 그러나",
          "speaker_id": "2",
          "time": "20210831 01:28"
        },
        {
          "id": "MDRW2100036593.1.9",
          "form": "아닌듯한데 각질제거 했는데도 막 들뜸",
          "original_form": "아닌듯한데 각질제거 했는데도 막 들뜸",
          "speaker_id": "1",
          "time": "20210831 01:28"
        },
        {
          "id": "MDRW2100036593.1.10",
          "form": "그럼뭐지",
          "original_form": "그럼뭐지",
          "speaker_id": "2",
          "time": "20210831 01:28"
        },
        {
          "id": "MDRW2100036593.1.11",
          "form": "기초제품을 뭘 어떻게 바르는지도 중요할듯",
          "original_form": "기초제품을 뭘 어떻게 바르는지도 중요할듯",
          "speaker_id": "1",
          "time": "20210831 01:29"
        },
        {
          "id": "MDRW2100036593.1.12",
          "form": "그런강",
          "original_form": "그런강",
          "speaker_id": "2",
          "time": "20210831 01:29"
        },
        {
          "id": "MDRW2100036593.1.13",
          "form": "엉 너무 유분기잇는 기초를 발라도 화장이 들뜸",
          "original_form": "엉 너무 유분기잇는 기초를 발라도 화장이 들뜸",
          "speaker_id": "1",
          "time": "20210831 01:29"
        },
        {
          "id": "MDRW2100036593.1.14",
          "form": "선크림도",
          "original_form": "선크림도",
          "speaker_id": "2",
          "time": "20210831 01:29"
        },
        {
          "id": "MDRW2100036593.1.15",
          "form": "맞아 선크림에 백테현상은 정말 ㅜ.ㅜ",
