          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100005222.1.7",
          "form": "돈 쓰는 걸 좋아해요",
          "original_form": "돈 쓰는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100005222.1.8",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100005222.1.9",
          "form": "근데 많이 쓰지는 않아요",
          "original_form": "근데 많이 쓰지는 않아요",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
