      "utterance": [
        {
          "id": "MDRW2100017809.1.1",
          "form": "영화는 어떤 장르 좋아해유?",
          "original_form": "영화는 어떤 장르 좋아해유?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017809.1.2",
          "form": "영화덕후라 다 좋아하는데",
          "original_form": "영화덕후라 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017809.1.3",
          "form": "봉감동님이 장르라고 할께요",
          "original_form": "봉감동님이 장르라고 할께요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017809.1.4",
          "form": "오.. 천재이죠... \r\n다 극장가서 보셧나요? 전 살인의 추억 정도인듯..",
          "original_form": "오.. 천재이죠... \r\n다 극장가서 보셧나요? 전 살인의 추억 정도인듯..",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017809.1.5",
          "form": "다 극장서보고",
          "original_form": "다 극장서보고",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017809.1.6",
          "form": "집에서 다시보고또보고 ㅎㅎ",
          "original_form": "집에서 다시보고또보고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017809.1.7",
          "form": "좋아하는영화는 열번도넘게봐요",
          "original_form": "좋아하는영화는 열번도넘게봐요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017809.1.8",
          "form": "복수는나의것 인가?",
          "original_form": "복수는나의것 인가?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017809.1.9",
          "form": "첫작품이요",
          "original_form": "첫작품이요",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017809.1.10",
          "form": "아아.. 이건 박찬욱이다..",
          "original_form": "아아.. 이건 박찬욱이다..",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017809.1.11",
          "form": "크...",
          "original_form": "크...",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017809.1.12",
          "form": "살인이 첫작?",
          "original_form": "살인이 첫작?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017809.1.13",
          "form": "박찬욱감독님 영화도 좋아해요",
          "original_form": "박찬욱감독님 영화도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017809.1.14",
          "form": "요즘 박찬욱 작품이 안나오네염?",
          "original_form": "요즘 박찬욱 작품이 안나오네염?",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017809.1.15",
          "form": "영화계가 타격 직빵이라",
          "original_form": "영화계가 타격 직빵이라",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017809.1.16",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017809.1.17",
          "form": "준비하고 계시는중일듯",
          "original_form": "준비하고 계시는중일듯",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017809.1.18",
          "form": "마지막으로 극장에서ㅜ본게 모여유?",
          "original_form": "마지막으로 극장에서ㅜ본게 모여유?",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017809.1.19",
          "form": "모가디슈임다",
          "original_form": "모가디슈임다",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017809.1.20",
          "form": "주말엔 프리가이 볼거에여",
          "original_form": "주말엔 프리가이 볼거에여",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017809.1.21",
          "form": "옹? 최근이네여?",
          "original_form": "옹? 최근이네여?",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017809.1.22",
          "form": "극장 널럴하징요?",
          "original_form": "극장 널럴하징요?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017809.1.23",
          "form": "네ㅋㅋ그렇더라구요ㅜ",
          "original_form": "네ㅋㅋ그렇더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        }
      ]
