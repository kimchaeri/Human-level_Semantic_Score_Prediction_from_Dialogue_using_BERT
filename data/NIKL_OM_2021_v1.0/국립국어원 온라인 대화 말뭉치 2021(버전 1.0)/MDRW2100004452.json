      "utterance": [
        {
          "id": "MDRW2100004452.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004452.1.2",
          "form": "요즘 즐겨보시는 책이 있나요?",
          "original_form": "요즘 즐겨보시는 책이 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004452.1.3",
          "form": "최근 여명기를 사서 봤어요",
          "original_form": "최근 여명기를 사서 봤어요",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004452.1.4",
          "form": "텀블벅 후원으로 출판된 책인데",
          "original_form": "텀블벅 후원으로 출판된 책인데",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004452.1.5",
          "form": "여성작가 모음집같은 거예요",
          "original_form": "여성작가 모음집같은 거예요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004452.1.6",
          "form": "어릴 땐 공립도서관에 가서",
          "original_form": "어릴 땐 공립도서관에 가서",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004452.1.7",
          "form": "명탐정 땡땡을 자주 봤어요",
          "original_form": "명탐정 땡땡을 자주 봤어요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004452.1.8",
          "form": "숨은그림찾기가 포함된 코믹북인데",
          "original_form": "숨은그림찾기가 포함된 코믹북인데",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004452.1.9",
          "form": "독서에 거부감을 느끼는 어린이들에게 추천해주고 싶어요",
          "original_form": "독서에 거부감을 느끼는 어린이들에게 추천해주고 싶어요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004452.1.10",
          "form": "우와 저도 한번보고싶네요 나중에 책사서 읽어봐야겠어요",
          "original_form": "우와 저도 한번보고싶네요 나중에 책사서 읽어봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004452.1.11",
          "form": "추리소설 재밌죠 평소 즐겨보시나요?",
          "original_form": "추리소설 재밌죠 평소 즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004452.1.12",
          "form": "네 학생 때 넬레 노이하우스 추리소설을 좋아했어요",
          "original_form": "네 학생 때 넬레 노이하우스 추리소설을 좋아했어요",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004452.1.13",
          "form": "와 저는 추리소설 별로 안보는 편이에요",
          "original_form": "와 저는 추리소설 별로 안보는 편이에요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004452.1.14",
          "form": "요즘 재테크 관련 도서에 관심이 있어요",
          "original_form": "요즘 재테크 관련 도서에 관심이 있어요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004452.1.15",
          "form": "추천하는 재테크 관련 도서가 있나요?",
          "original_form": "추천하는 재테크 관련 도서가 있나요?",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004452.1.16",
          "form": "네 아기곰의 재테크 불변의 법칙이라는책을 추천드릴게요",
          "original_form": "네 아기곰의 재테크 불변의 법칙이라는책을 추천드릴게요",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004452.1.17",
          "form": "그밖에 제가 최근 본 책은 그리스로마신화에요",
          "original_form": "그밖에 제가 최근 본 책은 그리스로마신화에요",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004452.1.18",
          "form": "어릴 때 그리스로마신화 책을 모았었어요.",
          "original_form": "어릴 때 그리스로마신화 책을 모았었어요.",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004452.1.19",
          "form": "비단털람쥐님도 수집하는 도서가 있나요?",
          "original_form": "비단털람쥐님도 수집하는 도서가 있나요?",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004452.1.20",
          "form": "맞아요 어릴때 그리스 로마신화 재밌게 읽었었어요",
          "original_form": "맞아요 어릴때 그리스 로마신화 재밌게 읽었었어요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004452.1.21",
          "form": "그안의 신비로운 이야기들이 너무 재밌더라고요",
          "original_form": "그안의 신비로운 이야기들이 너무 재밌더라고요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004452.1.22",
          "form": "지금은 수집하는 도서가 딱히 없어요",
          "original_form": "지금은 수집하는 도서가 딱히 없어요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004452.1.23",
          "form": "크세니아반도님은 있나요?",
          "original_form": "크세니아반도님은 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004452.1.24",
          "form": "학생때는 많이 했었는데 지금은 e북만 사고 있어요",
          "original_form": "학생때는 많이 했었는데 지금은 e북만 사고 있어요",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004452.1.25",
          "form": "책을 와다다 많이 사고 보니까 짐이더라구요",
          "original_form": "책을 와다다 많이 사고 보니까 짐이더라구요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004452.1.26",
          "form": "맞네요 종이책은 이사하거나 정리할때 무거워서 힘들겠어요",
          "original_form": "맞네요 종이책은 이사하거나 정리할때 무거워서 힘들겠어요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004452.1.27",
          "form": "e북은 편리해서 좋겠어요",
          "original_form": "e북은 편리해서 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004452.1.28",
          "form": "e북은 가격도 저렴해서 자주 보게 되네요",
          "original_form": "e북은 가격도 저렴해서 자주 보게 되네요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004452.1.29",
          "form": "저도 e북으로 갈아타야겠어요 유익한 대화 즐거웠어요",
          "original_form": "저도 e북으로 갈아타야겠어요 유익한 대화 즐거웠어요",
