      "utterance": [
        {
          "id": "MDRW2100032970.1.1",
          "form": "최근에 구입한 제품이 무엇인가요",
          "original_form": "최근에 구입한 제품이 무엇인가요",
          "speaker_id": "1",
          "time": "20210906 11:13"
        },
        {
          "id": "MDRW2100032970.1.2",
          "form": "화장품샀어요",
          "original_form": "화장품샀어요",
          "speaker_id": "2",
          "time": "20210906 11:13"
        },
        {
          "id": "MDRW2100032970.1.3",
          "form": "어떤 화장품이요?",
          "original_form": "어떤 화장품이요?",
          "speaker_id": "1",
          "time": "20210906 11:13"
        },
        {
          "id": "MDRW2100032970.1.4",
          "form": "체험단 이벤트를 해서 배송비 내고받았어요",
          "original_form": "체험단 이벤트를 해서 배송비 내고받았어요",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100032970.1.5",
          "form": "저렴하게 신상을 써볼 수 있겠네요",
          "original_form": "저렴하게 신상을 써볼 수 있겠네요",
          "speaker_id": "1",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100032970.1.6",
          "form": "그렇죠 가을을 겨냥한 보습로션이에요",
          "original_form": "그렇죠 가을을 겨냥한 보습로션이에요",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100032970.1.7",
          "form": "써보니까 좋던가요?",
          "original_form": "써보니까 좋던가요?",
          "speaker_id": "1",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100032970.1.8",
          "form": "30미리라 금방 쓸거같은데",
          "original_form": "30미리라 금방 쓸거같은데",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100032970.1.9",
          "form": "소량만 발라도 촉촉하요",
          "original_form": "소량만 발라도 촉촉하요",
          "speaker_id": "2",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100032970.1.10",
          "form": "체험단 이벤트로 저도 주방세제 받은적있어요",
          "original_form": "체험단 이벤트로 저도 주방세제 받은적있어요",
          "speaker_id": "1",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100032970.1.11",
          "form": "그런거 좋지요",
          "original_form": "그런거 좋지요",
          "speaker_id": "2",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100032970.1.12",
          "form": "체험단은 일종의 광고이니까 좋은 제품으로 승부를 걸어야 겠어요",
          "original_form": "체험단은 일종의 광고이니까 좋은 제품으로 승부를 걸어야 겠어요",
          "speaker_id": "1",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100032970.1.13",
          "form": "매출 증대를 위한거니 더 좋게 나와야죠",
          "original_form": "매출 증대를 위한거니 더 좋게 나와야죠",
          "speaker_id": "2",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100032970.1.14",
          "form": "체험단으로 콜라겐이나 음식도 많이 줘요",
          "original_form": "체험단으로 콜라겐이나 음식도 많이 줘요",
