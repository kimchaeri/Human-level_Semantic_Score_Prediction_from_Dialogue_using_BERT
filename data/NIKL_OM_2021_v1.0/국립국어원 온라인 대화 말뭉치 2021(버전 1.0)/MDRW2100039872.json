      "utterance": [
        {
          "id": "MDRW2100039872.1.1",
          "form": "요즘 건강 잘 챙기시나용?",
          "original_form": "요즘 건강 잘 챙기시나용?",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039872.1.2",
          "form": "음 네! 건강을 위해서 운동 중이에요",
          "original_form": "음 네! 건강을 위해서 운동 중이에요",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100039872.1.3",
          "form": "오 무슨 운동 하세요?",
          "original_form": "오 무슨 운동 하세요?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100039872.1.4",
          "form": "홈트 합니다! 조깅이랑요",
          "original_form": "홈트 합니다! 조깅이랑요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100039872.1.5",
          "form": "헉 홈트,,,",
          "original_form": "헉 홈트,,,",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100039872.1.6",
          "form": "전 홈트 작심사일하고 실패하는데",
          "original_form": "전 홈트 작심사일하고 실패하는데",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100039872.1.7",
          "form": "대단하세요...!",
          "original_form": "대단하세요...!",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100039872.1.8",
          "form": "오오...ㅋㅋㅋㅋㅋ 저도 홈트 영상 계속 보다가",
          "original_form": "오오...ㅋㅋㅋㅋㅋ 저도 홈트 영상 계속 보다가",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100039872.1.9",
          "form": "귀찮아져서 제가 하고 싶은 거만 해요",
          "original_form": "귀찮아져서 제가 하고 싶은 거만 해요",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100039872.1.10",
          "form": "ㅋㅋㅋㅋㅋ주로 어떤거하세요?",
          "original_form": "ㅋㅋㅋㅋㅋ주로 어떤거하세요?",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100039872.1.11",
          "form": "스쿼트랑 플랭크... 주로 뱃살 조지기 하고 있어요",
          "original_form": "스쿼트랑 플랭크... 주로 뱃살 조지기 하고 있어요",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100039872.1.12",
          "form": "ㅋㅋㅋㅋ하 제일 힘든거 하시네요ㅋㄱㄲ",
          "original_form": "ㅋㅋㅋㅋ하 제일 힘든거 하시네요ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100039872.1.13",
          "form": "스쿼트하면 진짜 허벅지 터질것같더라구요",
          "original_form": "스쿼트하면 진짜 허벅지 터질것같더라구요",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100039872.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ아 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 맞아요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100039872.1.15",
          "form": "근데 저는 계속 하다보니까 좀 단련이 됐는지 괜찮아요",
          "original_form": "근데 저는 계속 하다보니까 좀 단련이 됐는지 괜찮아요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100039872.1.16",
          "form": "제일 힘든 건 런지... 런지 진짜 힘들어요",
          "original_form": "제일 힘든 건 런지... 런지 진짜 힘들어요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100039872.1.17",
          "form": "ㅋㄱㅋㅋㄱㅋㅋㄱㅋ진짜 그 3개가 제일 힘들어요...",
          "original_form": "ㅋㄱㅋㅋㄱㅋㅋㄱㅋ진짜 그 3개가 제일 힘들어요...",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100039872.1.18",
          "form": "그래도 점점 갯수나 시간이 늘어나더라구요",
          "original_form": "그래도 점점 갯수나 시간이 늘어나더라구요",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100039872.1.19",
          "form": "그때 조금 뿌듯해요ㅋㄱㅋ",
          "original_form": "그때 조금 뿌듯해요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100039872.1.20",
          "form": "맞아요...ㅋㅋㅋㅋㅋ 근데 살 빠지는 건 그 3개가 제일 좋아서...ㅎ",
          "original_form": "맞아요...ㅋㅋㅋㅋㅋ 근데 살 빠지는 건 그 3개가 제일 좋아서...ㅎ",
