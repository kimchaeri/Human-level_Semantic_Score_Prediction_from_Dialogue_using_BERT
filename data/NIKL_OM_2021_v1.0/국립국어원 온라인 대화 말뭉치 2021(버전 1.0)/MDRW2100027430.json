      "utterance": [
        {
          "id": "MDRW2100027430.1.1",
          "form": "대신 메타 변화를 자주 시켜요",
          "original_form": "대신 메타 변화를 자주 시켜요",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027430.1.2",
          "form": "밸런스패치도 꾸준히 하고 있구요",
          "original_form": "밸런스패치도 꾸준히 하고 있구요",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027430.1.3",
          "form": "오... 역시 그래야 잘팔리겠네요",
          "original_form": "오... 역시 그래야 잘팔리겠네요",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027430.1.4",
          "form": "저는 컨트롤이 딸려서 ㅠㅠ 그런 겜은 잘 못하긴 하지만요",
          "original_form": "저는 컨트롤이 딸려서 ㅠㅠ 그런 겜은 잘 못하긴 하지만요",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027430.1.5",
          "form": "저도 컨트롤 엄청 딸려요",
          "original_form": "저도 컨트롤 엄청 딸려요",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027430.1.6",
          "form": "매일 버스타고 다닙니다ㅋㅋ",
          "original_form": "매일 버스타고 다닙니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027430.1.7",
          "form": "저는 AOS 못해서 사이퍼즈 이런것도 못해요 ㅋㅋㅋ",
          "original_form": "저는 AOS 못해서 사이퍼즈 이런것도 못해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027430.1.8",
          "form": "친구들 하는거만 맨날 보고",
          "original_form": "친구들 하는거만 맨날 보고",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027430.1.9",
          "form": "사이퍼즈는 못해봤어요",
          "original_form": "사이퍼즈는 못해봤어요",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027430.1.10",
          "form": "주변에 하는 친구가 없더라구요",
          "original_form": "주변에 하는 친구가 없더라구요",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027430.1.11",
          "form": "아 그럼 어쩔수 없네요 ㅋㅋㅋ 저도 보기만 해요!! 그쪽은 요즘 패치가 좀 이상하다고 그렇긴 하는데",
          "original_form": "아 그럼 어쩔수 없네요 ㅋㅋㅋ 저도 보기만 해요!! 그쪽은 요즘 패치가 좀 이상하다고 그렇긴 하는데",
          "speaker_id": "2",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027430.1.12",
          "form": "어떻게 이상하면 그런 말이 나올까요...ㄷㄷ",
          "original_form": "어떻게 이상하면 그런 말이 나올까요...ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027430.1.13",
          "form": "좀 밸런스적으로 안좋은 패치를 했다가 되돌렸다고 듣긴 했어요",
          "original_form": "좀 밸런스적으로 안좋은 패치를 했다가 되돌렸다고 듣긴 했어요",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027430.1.14",
          "form": "게임은 디렉터가 잘해야하나봐요 역시 ㅋㅋㅋ",
          "original_form": "게임은 디렉터가 잘해야하나봐요 역시 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027430.1.15",
          "form": "잘하는 디렉터 어디 없을까요",
          "original_form": "잘하는 디렉터 어디 없을까요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027430.1.16",
          "form": "본적이 없는거 같아요",
          "original_form": "본적이 없는거 같아요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027430.1.17",
          "form": "그러게요 ㅠㅠ 로스트아크는 그래도 호평이던데",
          "original_form": "그러게요 ㅠㅠ 로스트아크는 그래도 호평이던데",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100027430.1.18",
          "form": "전 스마게(개발사)쪽은 잘 안해서 ㅋㅋㅋㅋ 과금 유도 심해서...",
          "original_form": "전 스마게(개발사)쪽은 잘 안해서 ㅋㅋㅋㅋ 과금 유도 심해서...",
          "speaker_id": "2",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100027430.1.19",
          "form": "과금 유도 심한 게임이 꺼려지긴 하죠...",
          "original_form": "과금 유도 심한 게임이 꺼려지긴 하죠...",
