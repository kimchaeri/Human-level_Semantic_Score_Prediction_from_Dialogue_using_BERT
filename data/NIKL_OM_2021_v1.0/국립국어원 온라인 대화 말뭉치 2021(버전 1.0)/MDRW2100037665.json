      "utterance": [
        {
          "id": "MDRW2100037665.1.1",
          "form": "저녁에는 카레 해서 먹기로 했어요",
          "original_form": "저녁에는 카레 해서 먹기로 했어요",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100037665.1.2",
          "form": "양파만 들들 볶는중입니다",
          "original_form": "양파만 들들 볶는중입니다",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100037665.1.3",
          "form": "카라멜라이즈드 한 양파 진짜 맛있는거같아요",
          "original_form": "카라멜라이즈드 한 양파 진짜 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100037665.1.4",
          "form": "맞아요 그거 하고 있었어요ㅋㅋ",
          "original_form": "맞아요 그거 하고 있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037665.1.5",
          "form": "근데 엄청오래걸려서 힘들지않나요 ㅋㅋㅋ",
          "original_form": "근데 엄청오래걸려서 힘들지않나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:42"
        },
        {
          "id": "MDRW2100037665.1.6",
          "form": "맞아요... 날씨가 시원해서 버티고 했습니다",
          "original_form": "맞아요... 날씨가 시원해서 버티고 했습니다",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037665.1.7",
          "form": "그래서 여름에는 못먹었어요",
          "original_form": "그래서 여름에는 못먹었어요",
          "speaker_id": "2",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037665.1.8",
          "form": "아 맞아요 여름엔 진짜 불 오래 못쓰겟어요",
          "original_form": "아 맞아요 여름엔 진짜 불 오래 못쓰겟어요",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037665.1.9",
          "form": "일부러 전자레인지에 해먹을수잇는 요리하고..",
          "original_form": "일부러 전자레인지에 해먹을수잇는 요리하고..",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100037665.1.10",
          "form": "아니면 완전조리로 포장하거나 레토르트로요",
          "original_form": "아니면 완전조리로 포장하거나 레토르트로요",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100037665.1.11",
          "form": "아 맞아요 저 순대국 포장해서 렌지에 돌려먹고 그랫어요 ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 저 순대국 포장해서 렌지에 돌려먹고 그랫어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100037665.1.12",
          "form": "ㅋㅋㅋㅋ 순대국은 진리죠",
          "original_form": "ㅋㅋㅋㅋ 순대국은 진리죠",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100037665.1.13",
          "form": "카레랑 구운달걀 같이먹으면 맛잇다더넫 드셔보셧나요",
          "original_form": "카레랑 구운달걀 같이먹으면 맛잇다더넫 드셔보셧나요",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037665.1.14",
          "form": "이미 먹어봤죠ㅋㅋ",
          "original_form": "이미 먹어봤죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037665.1.15",
          "form": "마늘 튀겨서 넣어도 맛있어요",
          "original_form": "마늘 튀겨서 넣어도 맛있어요",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037665.1.16",
          "form": "오 맞아요 ㅜㅜ 마늘 후레이크",
          "original_form": "오 맞아요 ㅜㅜ 마늘 후레이크",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037665.1.17",
          "form": "카레가 은근 여기저기 어울리는거같아요",
          "original_form": "카레가 은근 여기저기 어울리는거같아요",
