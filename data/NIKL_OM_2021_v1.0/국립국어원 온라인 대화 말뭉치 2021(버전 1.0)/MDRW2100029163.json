        },
        {
          "id": "MDRW2100029163.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029163.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029163.1.4",
          "form": "네 오늘 부대찌개 먹었어요ㅎ",
          "original_form": "네 오늘 부대찌개 먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029163.1.5",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029163.1.6",
          "form": "저는 그냥 볶음밥 먹었어요",
          "original_form": "저는 그냥 볶음밥 먹었어요",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029163.1.7",
          "form": "부대찌개 맛있겠당...",
          "original_form": "부대찌개 맛있겠당...",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029163.1.8",
          "form": "급 끌리네여",
          "original_form": "급 끌리네여",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029163.1.9",
          "form": "헐 볶음밥도 좋은데요?",
          "original_form": "헐 볶음밥도 좋은데요?",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029163.1.10",
          "form": "부대찌개에 어떤 사리 좋아하시나요?",
          "original_form": "부대찌개에 어떤 사리 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029163.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 라면사리 완전 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 라면사리 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100029163.1.12",
          "form": "무조건 추가해야한다고 생각합니다...",
          "original_form": "무조건 추가해야한다고 생각합니다...",
          "speaker_id": "2",
          "time": "20210903 18:13"
        },
        {
          "id": "MDRW2100029163.1.13",
          "form": "헐 저두여",
          "original_form": "헐 저두여",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029163.1.14",
          "form": "그래서 부대찌개 가끔 찾아보면 라면사리 무한인곳이 있더라구요",
          "original_form": "그래서 부대찌개 가끔 찾아보면 라면사리 무한인곳이 있더라구요",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029163.1.15",
          "form": "단골됩니댜....라면 소중해요",
          "original_form": "단골됩니댜....라면 소중해요",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029163.1.16",
          "form": "헐 대박 무한이에요? 짱이당",
          "original_form": "헐 대박 무한이에요? 짱이당",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029163.1.17",
          "form": "마자요 라면 너무 소중ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 라면 너무 소중ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029163.1.18",
          "form": "뜬금 없는데 무슨 라면 좋아하세용?",
          "original_form": "뜬금 없는데 무슨 라면 좋아하세용?",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100029163.1.19",
          "form": "저 원래는 너구리 좋아했는데 요즘엔 좀 다양하게 먹고 있어요ㅋㅋ",
          "original_form": "저 원래는 너구리 좋아했는데 요즘엔 좀 다양하게 먹고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100029163.1.20",
          "form": "삼양라면 이번에 오랜만에 먹어봤는데 맛있더라구요...",
          "original_form": "삼양라면 이번에 오랜만에 먹어봤는데 맛있더라구요...",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100029163.1.21",
          "form": "너구리 맛있죠!! 헉 저 삼양 좋아해요",
          "original_form": "너구리 맛있죠!! 헉 저 삼양 좋아해요",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100029163.1.22",
          "form": "저 너무 매운 음식은 잘 못먹어서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 너무 매운 음식은 잘 못먹어서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100029163.1.23",
          "form": "삼양이나 진라면 매운맛이 최애입니당ㅋㅋㅋㅋ",
          "original_form": "삼양이나 진라면 매운맛이 최애입니당ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.24",
          "form": "헐 저도 맵찔이에요ㅠㅠㅋㅋ",
          "original_form": "헐 저도 맵찔이에요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.25",
          "form": "아 그리구 참깨라면도 좋아요!!",
          "original_form": "아 그리구 참깨라면도 좋아요!!",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.26",
          "form": "헉ㅋㅋㅋㅋㅋ대박",
          "original_form": "헉ㅋㅋㅋㅋㅋ대박",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.27",
          "form": "앗 진라면 매운맛도 맛있어요!ㅋㅋ",
          "original_form": "앗 진라면 매운맛도 맛있어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.28",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.29",
          "form": "순한맛도 좋지만 매운맛도 별로 안 매워서",
          "original_form": "순한맛도 좋지만 매운맛도 별로 안 매워서",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.30",
          "form": "좋아요!!",
          "original_form": "좋아요!!",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.31",
          "form": "김치 넣어먹는거 좋아합니다...",
          "original_form": "김치 넣어먹는거 좋아합니다...",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.32",
          "form": "한국 라면 다 너무 매워요",
          "original_form": "한국 라면 다 너무 매워요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.33",
          "form": "맞아여 진라면은 괜춘해여",
          "original_form": "맞아여 진라면은 괜춘해여",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029163.1.34",
          "form": "저 열라면 먹다가 정말...",
          "original_form": "저 열라면 먹다가 정말...",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.35",
          "form": "아ㅠㅠㅠㅠ열라면",
          "original_form": "아ㅠㅠㅠㅠ열라면",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.36",
          "form": "틈새는 시도도 하지 마십셔 먹다가....",
          "original_form": "틈새는 시도도 하지 마십셔 먹다가....",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.37",
          "form": "그 순두부열라면 아세요?",
          "original_form": "그 순두부열라면 아세요?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.38",
          "form": "저 그거때매 첨 먹어봤는데",
          "original_form": "저 그거때매 첨 먹어봤는데",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.39",
          "form": "앗 그거 알아여!",
          "original_form": "앗 그거 알아여!",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.40",
          "form": "그거 맛나다고 하더라구요",
          "original_form": "그거 맛나다고 하더라구요",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.41",
          "form": "순두부 넣어두 맵더라구욬ㅋㅋㅋㅋ",
          "original_form": "순두부 넣어두 맵더라구욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.42",
          "form": "궁금한데 시도는 아직 안해본ㅠㅠㅋㅋ",
          "original_form": "궁금한데 시도는 아직 안해본ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.43",
          "form": "근데 맛있긴 했어요",
          "original_form": "근데 맛있긴 했어요",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.44",
          "form": "헐 드셔보셨군요",
          "original_form": "헐 드셔보셨군요",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.45",
          "form": "틈새 저 시도도 못해봤어요ㅋ큐큐ㅠㅠ",
          "original_form": "틈새 저 시도도 못해봤어요ㅋ큐큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.46",
          "form": "네넹 맛있어요!!!",
          "original_form": "네넹 맛있어요!!!",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.47",
          "form": "근데 매워용...ㅋ큐큐ㅠㅠ",
          "original_form": "근데 매워용...ㅋ큐큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.48",
          "form": "진짜 순두부찌개 맛 나요?",
          "original_form": "진짜 순두부찌개 맛 나요?",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100029163.1.49",
          "form": "네네네!!! 진짜신기해요",
          "original_form": "네네네!!! 진짜신기해요",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029163.1.50",
          "form": "칼칼한 순두부찌개에 라면 사리 넣은 느낌!!",
          "original_form": "칼칼한 순두부찌개에 라면 사리 넣은 느낌!!",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029163.1.51",
          "form": "오 신기해...한번 먹어봅니댜!",
          "original_form": "오 신기해...한번 먹어봅니댜!",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029163.1.52",
          "form": "와 아니 어찌 그리 맛이 변하죠ㅋㅋ 발견한 사람 넘나 신기한ㅋㅋ",
          "original_form": "와 아니 어찌 그리 맛이 변하죠ㅋㅋ 발견한 사람 넘나 신기한ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029163.1.53",
          "form": "ㅋㅋㅋㅋ그니까요 완전 천재만재셔요ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그니까요 완전 천재만재셔요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029163.1.54",
          "form": "딴 라면은 전혀 그런 느낌 없다던데...",
          "original_form": "딴 라면은 전혀 그런 느낌 없다던데...",
