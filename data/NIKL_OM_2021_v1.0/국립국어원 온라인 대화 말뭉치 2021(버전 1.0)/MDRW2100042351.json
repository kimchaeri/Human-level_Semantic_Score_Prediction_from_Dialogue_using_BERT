      "utterance": [
        {
          "id": "MDRW2100042351.1.1",
          "form": "학교 거는 밍밍하고 느끼하니까",
          "original_form": "학교 거는 밍밍하고 느끼하니까",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.2",
          "form": "언제 집에 들어올지 모르니",
          "original_form": "언제 집에 들어올지 모르니",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.3",
          "form": "먹기 싫다 하더라고요",
          "original_form": "먹기 싫다 하더라고요",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.4",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.5",
          "form": "아 그렇구만",
          "original_form": "아 그렇구만",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.6",
          "form": "우리 name3은 내가 잔소리할까봐",
          "original_form": "우리 &name3&은 내가 잔소리할까봐",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.7",
          "form": "아침은 밥으로",
          "original_form": "아침은 밥으로",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.8",
          "form": "그냥 맛있다고 하고",
          "original_form": "그냥 맛있다고 하고",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.9",
          "form": "근데 시간이 너무 많이 걸림",
          "original_form": "근데 시간이 너무 많이 걸림",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.10",
          "form": "그래 아침은 밥으로",
          "original_form": "그래 아침은 밥으로",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.11",
          "form": "그냥 국에 말아준다 나는",
          "original_form": "그냥 국에 말아준다 나는",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.12",
          "form": "다른 반찬 없이",
          "original_form": "다른 반찬 없이",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.13",
          "form": "저녁에 먹은 국 그대로",
          "original_form": "저녁에 먹은 국 그대로",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.14",
          "form": "언니를 너무  잘아는 거지",
          "original_form": "언니를 너무  잘아는 거지",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042351.1.15",
          "form": "애들은 참 똑똑해ㅋㅋㅋㅋ",
          "original_form": "애들은 참 똑똑해ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.16",
          "form": "다른 거에 좀 똑똑했으면 좋겠는데",
          "original_form": "다른 거에 좀 똑똑했으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.17",
          "form": "벌써 엄마를 속이는 거에 똑똑하다니",
          "original_form": "벌써 엄마를 속이는 거에 똑똑하다니",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.18",
          "form": "아아.....",
          "original_form": "아아.....",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.19",
          "form": "지금부터 언니 서서히 연습을 해야 될 듯 요ㅋㅋㅋ",
          "original_form": "지금부터 언니 서서히 연습을 해야 될 듯 요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.20",
          "form": "우리 name3도 슬슬 거짓말 이 늘어납니다ㅋㅋㅋ",
          "original_form": "우리 &name3&도 슬슬 거짓말 이 늘어납니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.21",
          "form": "그래 벌써 속고 있다",
          "original_form": "그래 벌써 속고 있다",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.22",
          "form": "맞제 이놈들이",
          "original_form": "맞제 이놈들이",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.23",
          "form": "근데 너무나도 감쪽같다는 거ㅋㅋㅋ",
          "original_form": "근데 너무나도 감쪽같다는 거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.24",
          "form": "거짓말을 이제 슬슬 시작하지",
          "original_form": "거짓말을 이제 슬슬 시작하지",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042351.1.25",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042351.1.26",
