      "utterance": [
        {
          "id": "MDRW2100040082.1.1",
          "form": "맞네, 새로운 노래 듣는 재미도 있고.",
          "original_form": "맞네, 새로운 노래 듣는 재미도 있고.",
          "speaker_id": "2",
          "time": "20210904 07:19"
        },
        {
          "id": "MDRW2100040082.1.2",
          "form": "간식게임도 너무 재밋음",
          "original_form": "간식게임도 너무 재밋음",
          "speaker_id": "1",
          "time": "20210904 07:19"
        },
        {
          "id": "MDRW2100040082.1.3",
          "form": "키가 춤 추는 거 짱",
          "original_form": "키가 춤 추는 거 짱",
          "speaker_id": "1",
          "time": "20210904 07:19"
        },
        {
          "id": "MDRW2100040082.1.4",
          "form": "그래 키는 어떻게 모든 안무를 다 외우는지. 대단해",
          "original_form": "그래 키는 어떻게 모든 안무를 다 외우는지. 대단해",
          "speaker_id": "2",
          "time": "20210904 07:20"
        },
        {
          "id": "MDRW2100040082.1.5",
          "form": "ㅋㅋㅋㅋ받쓰도 잘해",
          "original_form": "ㅋㅋㅋㅋ받쓰도 잘해",
          "speaker_id": "1",
          "time": "20210904 07:21"
        },
        {
          "id": "MDRW2100040082.1.6",
          "form": "또 공동원샷 받았네",
          "original_form": "또 공동원샷 받았네",
          "speaker_id": "1",
          "time": "20210904 07:21"
        },
        {
          "id": "MDRW2100040082.1.7",
          "form": "키가 금방 문세윤 따라 잡을 것 같아",
          "original_form": "키가 금방 문세윤 따라 잡을 것 같아",
          "speaker_id": "2",
          "time": "20210904 07:21"
        },
        {
          "id": "MDRW2100040082.1.8",
          "form": "ㅋㅋㅋ난 키-문세윤보다 태연이 신동엽하고 김동현을 얼마나 빨리 따라잡을지가 궁금해",
          "original_form": "ㅋㅋㅋ난 키-문세윤보다 태연이 신동엽하고 김동현을 얼마나 빨리 따라잡을지가 궁금해",
          "speaker_id": "1",
          "time": "20210904 07:22"
        },
        {
          "id": "MDRW2100040082.1.9",
          "form": "태연이 잘하니까 금방 이길 수 있을 것 같아",
          "original_form": "태연이 잘하니까 금방 이길 수 있을 것 같아",
          "speaker_id": "2",
          "time": "20210904 07:22"
        },
        {
          "id": "MDRW2100040082.1.10",
          "form": "그치?ㅋㅋㅋㅋ",
          "original_form": "그치?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 07:22"
        },
        {
          "id": "MDRW2100040082.1.11",
          "form": "놀토 보면서 음식이 너무 맛있어보여서 힘들다..",
          "original_form": "놀토 보면서 음식이 너무 맛있어보여서 힘들다..",
          "speaker_id": "1",
          "time": "20210904 07:23"
        },
        {
          "id": "MDRW2100040082.1.12",
          "form": "제작진이 시장 선택도 잘하는 듯",
          "original_form": "제작진이 시장 선택도 잘하는 듯",
          "speaker_id": "1",
          "time": "20210904 07:23"
        },
        {
          "id": "MDRW2100040082.1.13",
          "form": "이제 간식게임 라운드다",
          "original_form": "이제 간식게임 라운드다",
          "speaker_id": "1",
          "time": "20210904 07:25"
        },
        {
          "id": "MDRW2100040082.1.14",
          "form": "간식게임에서 신동엽이 춤 열심히 추는 것도 참 웃겨",
          "original_form": "간식게임에서 신동엽이 춤 열심히 추는 것도 참 웃겨",
          "speaker_id": "2",
          "time": "20210904 07:26"
        },
        {
          "id": "MDRW2100040082.1.15",
          "form": "맞어ㅋㅋㅋ 안 할 것 같은데 은근 열심히 함ㅋㅋㅋ",
          "original_form": "맞어ㅋㅋㅋ 안 할 것 같은데 은근 열심히 함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 07:26"
        },
        {
          "id": "MDRW2100040082.1.16",
          "form": "한해도 요즘 너무 웃기다",
          "original_form": "한해도 요즘 너무 웃기다",
          "speaker_id": "1",
          "time": "20210904 07:27"
        },
        {
          "id": "MDRW2100040082.1.17",
          "form": "한해는 키랑 티격태격할 때 웃겨",
          "original_form": "한해는 키랑 티격태격할 때 웃겨",
          "speaker_id": "2",
          "time": "20210904 07:27"
        },
        {
          "id": "MDRW2100040082.1.18",
          "form": "ㅋㅋㅋㅋㅋ 둘이 케미가 좋아서 자꾸 붙여 놓는 듯",
          "original_form": "ㅋㅋㅋㅋㅋ 둘이 케미가 좋아서 자꾸 붙여 놓는 듯",
          "speaker_id": "1",
          "time": "20210904 07:28"
        },
        {
          "id": "MDRW2100040082.1.19",
          "form": "둘은 같이 음원 안 내나",
          "original_form": "둘은 같이 음원 안 내나",
          "speaker_id": "2",
          "time": "20210904 07:28"
        },
        {
          "id": "MDRW2100040082.1.20",
          "form": "잘 어울릴 것 같은데.",
          "original_form": "잘 어울릴 것 같은데.",
          "speaker_id": "2",
          "time": "20210904 07:28"
        },
        {
          "id": "MDRW2100040082.1.21",
          "form": "예전에 같이 음원 낸 적 있어 cold 라고..",
          "original_form": "예전에 같이 음원 낸 적 있어 cold 라고..",
          "speaker_id": "1",
          "time": "20210904 07:29"
        },
        {
          "id": "MDRW2100040082.1.22",
          "form": "그 노래도 좋음",
          "original_form": "그 노래도 좋음",
          "speaker_id": "1",
          "time": "20210904 07:29"
        }
      ]
