      "utterance": [
        {
          "id": "MDRW2100030355.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030355.1.2",
          "form": "좋아하는 편이에요",
          "original_form": "좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030355.1.3",
          "form": "좋아하는 작가 있으세요?",
          "original_form": "좋아하는 작가 있으세요?",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030355.1.4",
          "form": "딱히 작가를 따지는 타입은 아니지만...",
          "original_form": "딱히 작가를 따지는 타입은 아니지만...",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030355.1.5",
          "form": "음... 정유정 작가님이랑 구병모 작가님정도?",
          "original_form": "음... 정유정 작가님이랑 구병모 작가님정도?",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030355.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030355.1.7",
          "form": "저도 정유정 작가님 책 재밌게 읽었어요",
          "original_form": "저도 정유정 작가님 책 재밌게 읽었어요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030355.1.8",
          "form": "구병모 작가님 책은 아직 못 읽어봤네요",
          "original_form": "구병모 작가님 책은 아직 못 읽어봤네요",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030355.1.9",
          "form": "파과랑 위저드베이커리가 유명해요",
          "original_form": "파과랑 위저드베이커리가 유명해요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030355.1.10",
          "form": "읽어보고 싶네요 지금 읽는 책 다보면 봐야겠어요",
          "original_form": "읽어보고 싶네요 지금 읽는 책 다보면 봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030355.1.11",
          "form": "7년의 밤 보셨어요?",
          "original_form": "7년의 밤 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030355.1.12",
          "form": "아... 그게 제 입문작이었죠",
          "original_form": "아... 그게 제 입문작이었죠",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100030355.1.13",
          "form": "저두요ㅎ 그거 읽고 악몽꾸고 그랬어요ㅜㅜ",
          "original_form": "저두요ㅎ 그거 읽고 악몽꾸고 그랬어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100030355.1.14",
          "form": "초반에 그 우편물 얘기 나올때부터 질리더라구요",
          "original_form": "초반에 그 우편물 얘기 나올때부터 질리더라구요",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030355.1.15",
          "form": "담담한 서술로도 숨길수 없는 지독한 악의가... ㄷㄷ",
          "original_form": "담담한 서술로도 숨길수 없는 지독한 악의가... ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030355.1.16",
          "form": "네 정말... 왠만한 공포영화보다 무서웠어요.",
          "original_form": "네 정말... 왠만한 공포영화보다 무서웠어요.",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030355.1.17",
          "form": "영화도 보셨어요?",
          "original_form": "영화도 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030355.1.18",
          "form": "영화는 아뇨...",
          "original_form": "영화는 아뇨...",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030355.1.19",
          "form": "볼까 할때 혹평이 쏟아져서...",
          "original_form": "볼까 할때 혹평이 쏟아져서...",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100030355.1.20",
          "form": "네 정말 별로였어요 ㅜㅜ",
          "original_form": "네 정말 별로였어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:17"
        }
