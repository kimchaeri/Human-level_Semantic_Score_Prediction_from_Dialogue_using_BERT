      "utterance": [
        {
          "id": "MDRW2100002741.1.1",
          "form": "요즘 날씨 좋지",
          "original_form": "요즘 날씨 좋지",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002741.1.2",
          "form": "전보다 시원해지긴 했는데 난 아직 더워",
          "original_form": "전보다 시원해지긴 했는데 난 아직 더워",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002741.1.3",
          "form": "어제 잠깐 소나기 와서 그런지 시원한 것 같은데.",
          "original_form": "어제 잠깐 소나기 와서 그런지 시원한 것 같은데.",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002741.1.4",
          "form": "이제 곧 가을이긴 하지",
          "original_form": "이제 곧 가을이긴 하지",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002741.1.5",
          "form": "오늘도 에어컨 틀었어?",
          "original_form": "오늘도 에어컨 틀었어?",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002741.1.6",
          "form": "아니~ 선풍기만 틀어놓고 있어",
          "original_form": "아니~ 선풍기만 틀어놓고 있어",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002741.1.7",
          "form": "엄마랑 아빠는 저번주부터 에어컨 안 틀었어.",
          "original_form": "엄마랑 아빠는 저번주부터 에어컨 안 틀었어.",
          "speaker_id": "1",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002741.1.8",
          "form": "ㅋㅋㅋ아빠가 덥다고 하지 않아?",
          "original_form": "ㅋㅋㅋ아빠가 덥다고 하지 않아?",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002741.1.9",
          "form": "너네 아빠는 여름에는 돕다고 난리. 겨울에는 춥다고 난리야.",
          "original_form": "너네 아빠는 여름에는 돕다고 난리. 겨울에는 춥다고 난리야.",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002741.1.10",
          "form": "집은 청문 열어 놓으면 시원해.",
          "original_form": "집은 청문 열어 놓으면 시원해.",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002741.1.11",
          "form": "아 거기는 창문이 크니까 그렇겠구나",
          "original_form": "아 거기는 창문이 크니까 그렇겠구나",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002741.1.12",
          "form": "응 오늘 미세먼지 수치도 좋아서 열어놨어",
          "original_form": "응 오늘 미세먼지 수치도 좋아서 열어놨어",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002741.1.13",
          "form": "오늘 밤에도 비온대",
          "original_form": "오늘 밤에도 비온대",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002741.1.14",
          "form": "밤에는 창문 닫아놔",
          "original_form": "밤에는 창문 닫아놔",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002741.1.15",
          "form": "내일까지 계속 비 온다던데.",
          "original_form": "내일까지 계속 비 온다던데.",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002741.1.16",
          "form": "집 올 때 힘들어서 어떡해.",
          "original_form": "집 올 때 힘들어서 어떡해.",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002741.1.17",
          "form": "그렇게 많이는 안 올걸??",
          "original_form": "그렇게 많이는 안 올걸??",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002741.1.18",
          "form": "요즘 비도 산성비인가?",
          "original_form": "요즘 비도 산성비인가?",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002741.1.19",
          "form": "그렇지. 비 맞으면 안 좋으니까 조심해.",
          "original_form": "그렇지. 비 맞으면 안 좋으니까 조심해.",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002741.1.20",
          "form": "그래도 이번 여름은 비 많이 안 오는 것 같아",
          "original_form": "그래도 이번 여름은 비 많이 안 오는 것 같아",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002741.1.21",
          "form": "태풍 소식도 딱히 없지 않아?",
          "original_form": "태풍 소식도 딱히 없지 않아?",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002741.1.22",
          "form": "올해는 그러네.",
          "original_form": "올해는 그러네.",
          "speaker_id": "1",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002741.1.23",
          "form": "아무탈 없이 가을로 지나가야 될 텐데..",
          "original_form": "아무탈 없이 가을로 지나가야 될 텐데..",
          "speaker_id": "1",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002741.1.24",
          "form": "그니까... 난 여름이 제일 싫어",
          "original_form": "그니까... 난 여름이 제일 싫어",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002741.1.25",
          "form": "요즘 지구온난화 때문에 햇빛이 세서 그런가 햇빛 알러지 생겼어",
          "original_form": "요즘 지구온난화 때문에 햇빛이 세서 그런가 햇빛 알러지 생겼어",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002741.1.26",
          "form": "팔에 두드러기 올라와ㅠㅠ",
          "original_form": "팔에 두드러기 올라와ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002741.1.27",
          "form": "양산 꼭 쓰고 다녀. 햇빛 맞는 거 피부에 엄청 안 좋아.",
          "original_form": "양산 꼭 쓰고 다녀. 햇빛 맞는 거 피부에 엄청 안 좋아.",
          "speaker_id": "1",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002741.1.28",
          "form": "더워도 긴팔 챙겨다니고.",
          "original_form": "더워도 긴팔 챙겨다니고.",
          "speaker_id": "1",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002741.1.29",
          "form": "응 안 그래도 얇은 린넨 셔츠 입고 다녀~",
          "original_form": "응 안 그래도 얇은 린넨 셔츠 입고 다녀~",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002741.1.30",
          "form": "날씨가 아주 제멋대로야",
          "original_form": "날씨가 아주 제멋대로야",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002741.1.31",
          "form": "올해는 많이 안 더운데 지구온난화는 더 심해졌대",
          "original_form": "올해는 많이 안 더운데 지구온난화는 더 심해졌대",
          "speaker_id": "2",
          "time": "20210820 15:27"
        },
        {
          "id": "MDRW2100002741.1.32",
          "form": "그러게. 큰 일이야.",
          "original_form": "그러게. 큰 일이야.",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002741.1.33",
          "form": "이번 여름이 별로 덥지 않아서 겨울은 또 어떨지 모르겠네.",
          "original_form": "이번 여름이 별로 덥지 않아서 겨울은 또 어떨지 모르겠네.",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002741.1.34",
          "form": "작년엔 별로 안 추웠잖아.",
          "original_form": "작년엔 별로 안 추웠잖아.",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002741.1.35",
          "form": "겨울엔 옷을 많이 껴입으니까 무거워지는 느낌이 싫어",
          "original_form": "겨울엔 옷을 많이 껴입으니까 무거워지는 느낌이 싫어",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002741.1.36",
          "form": "그것만 빼면 겨울이 참 좋은뎋ㅎ",
          "original_form": "그것만 빼면 겨울이 참 좋은뎋ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002741.1.37",
          "form": "엄마는 가을이 제일 좋아. 날씨도 선선하고 코스모스도 피고.",
          "original_form": "엄마는 가을이 제일 좋아. 날씨도 선선하고 코스모스도 피고.",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002741.1.38",
          "form": "난 그래도 겨울이 제일 좋다~",
          "original_form": "난 그래도 겨울이 제일 좋다~",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002741.1.39",
          "form": "겨울이 왜 좋은데?",
          "original_form": "겨울이 왜 좋은데?",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002741.1.40",
          "form": "생일이기도 하고ㅋㅋㅋ 겨울에 목도리하고 있으면 포근한 느낌이 넘 좋아",
          "original_form": "생일이기도 하고ㅋㅋㅋ 겨울에 목도리하고 있으면 포근한 느낌이 넘 좋아",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002741.1.41",
          "form": "요즘 겨울도 점점 짧아지는 느낌이야",
          "original_form": "요즘 겨울도 점점 짧아지는 느낌이야",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002741.1.42",
          "form": "그치? 엄청 춥다가 짧게 끝나는 느낌이야. 기간은 짧아졌는데 더 추워진 느낌이랄까...",
          "original_form": "그치? 엄청 춥다가 짧게 끝나는 느낌이야. 기간은 짧아졌는데 더 추워진 느낌이랄까...",
          "speaker_id": "2",
          "time": "20210820 15:36"
        }
      ]
    }
  ]
