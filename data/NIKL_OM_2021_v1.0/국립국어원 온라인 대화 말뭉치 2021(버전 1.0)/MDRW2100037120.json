      "utterance": [
        {
          "id": "MDRW2100037120.1.1",
          "form": "맞아요.. 비싸여...",
          "original_form": "맞아요.. 비싸여...",
          "speaker_id": "1",
          "time": "20210831 20:04"
        },
        {
          "id": "MDRW2100037120.1.2",
          "form": "진짜 샐러드전문점가면 거의 만원초반이더라구요",
          "original_form": "진짜 샐러드전문점가면 거의 만원초반이더라구요",
          "speaker_id": "2",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100037120.1.3",
          "form": "양 정말 쪼그만하구",
          "original_form": "양 정말 쪼그만하구",
          "speaker_id": "2",
          "time": "20210831 20:05"
        },
        {
          "id": "MDRW2100037120.1.4",
          "form": "맞아요 샐러드 너무 비싸오ㅠ",
          "original_form": "맞아요 샐러드 너무 비싸오ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100037120.1.5",
          "form": "샌드위치는 좋아하세요?",
          "original_form": "샌드위치는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100037120.1.6",
          "form": "녱!",
          "original_form": "녱!",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100037120.1.7",
          "form": "ㅋㅋㅋㅋ저두좋아해요 써브웨이 자주가요!",
          "original_form": "ㅋㅋㅋㅋ저두좋아해요 써브웨이 자주가요!",
          "speaker_id": "2",
          "time": "20210831 20:10"
        },
        {
          "id": "MDRW2100037120.1.8",
          "form": "저두 배달 자주 시켜용!!",
          "original_form": "저두 배달 자주 시켜용!!",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100037120.1.9",
          "form": "이것저것 넣음 가격이.. 깜짝깜짝 놀라요",
          "original_form": "이것저것 넣음 가격이.. 깜짝깜짝 놀라요",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100037120.1.10",
          "form": "맞아요 ㅠㅠ 은근 싼가격은 아닌거같아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 은근 싼가격은 아닌거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100037120.1.11",
          "form": "그래도 저는 제취향이라 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "그래도 저는 제취향이라 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:12"
        },
        {
          "id": "MDRW2100037120.1.12",
          "form": "저두용ㅋㅋㅋㅋㅋ",
          "original_form": "저두용ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:15"
        },
        {
          "id": "MDRW2100037120.1.13",
          "form": "맛난거 먹으며 지내야죵",
          "original_form": "맛난거 먹으며 지내야죵",
          "speaker_id": "1",
          "time": "20210831 20:15"
        },
        {
          "id": "MDRW2100037120.1.14",
          "form": "ㅋㅋㅋㅋㅋ샌드위치 무슨메뉴 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋ샌드위치 무슨메뉴 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:17"
        },
        {
          "id": "MDRW2100037120.1.15",
          "form": "저는 고기요!!",
          "original_form": "저는 고기요!!",
          "speaker_id": "1",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100037120.1.16",
          "form": "계란마요 추가합니닷",
          "original_form": "계란마요 추가합니닷",
          "speaker_id": "1",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037120.1.17",
          "form": "저는 아보카도 추가 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 아보카도 추가 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037120.1.18",
          "form": "소스 뭐 좋아하세요?",
          "original_form": "소스 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 20:20"
        },
        {
          "id": "MDRW2100037120.1.19",
          "form": "저는 기본으로..ㅋㅋㅋ 먹습니닷",
          "original_form": "저는 기본으로..ㅋㅋㅋ 먹습니닷",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100037120.1.20",
          "form": "새로운거 먹고싶을때는 인터넷에서 소스 조합 찾아서 먹어용!",
          "original_form": "새로운거 먹고싶을때는 인터넷에서 소스 조합 찾아서 먹어용!",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037120.1.21",
          "form": "그렇군요 ㅋㅋㅋㅋ",
          "original_form": "그렇군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100037120.1.22",
          "form": "저는 랜치 스위트어니언 스위트칠리 고정이에요 ㅋㅋㅋㅋ",
          "original_form": "저는 랜치 스위트어니언 스위트칠리 고정이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037120.1.23",
          "form": "넹ㅋㅋ 소스 어떤거 좋아하세요?",
          "original_form": "넹ㅋㅋ 소스 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037120.1.24",
          "form": "오옹ㅋㅋㅋ 그거 좋아하시눈군요",
          "original_form": "오옹ㅋㅋㅋ 그거 좋아하시눈군요",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037120.1.25",
          "form": "다이어트할땐 올리브오일이랑 후추먹어요 ㅋㅋㅋㅋ",
          "original_form": "다이어트할땐 올리브오일이랑 후추먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100037120.1.26",
          "form": "오우ㅋㅋㅋ 그 조합은 저희 엄마 픽이에요!",
          "original_form": "오우ㅋㅋㅋ 그 조합은 저희 엄마 픽이에요!",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037120.1.27",
          "form": "전에 조합에서 찾았는데 딱 입맛이라시더라구요",
          "original_form": "전에 조합에서 찾았는데 딱 입맛이라시더라구요",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100037120.1.28",
          "form": "오 그렇군요 하긴 다른 소스는 좀 달아서 자극적이긴해요 ㅋㅋㅋㅋ",
          "original_form": "오 그렇군요 하긴 다른 소스는 좀 달아서 자극적이긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100037120.1.29",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100037120.1.30",
          "form": "야채도 많고 그래서 좋아하시더라구용",
          "original_form": "야채도 많고 그래서 좋아하시더라구용",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100037120.1.31",
          "form": "맞아요 저도 최대한 소스 적게 먹으려구해요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 저도 최대한 소스 적게 먹으려구해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:33"
        },
        {
          "id": "MDRW2100037120.1.32",
          "form": "양심상 절임류는 빼서먹어요 ㅋㅋㅋ",
