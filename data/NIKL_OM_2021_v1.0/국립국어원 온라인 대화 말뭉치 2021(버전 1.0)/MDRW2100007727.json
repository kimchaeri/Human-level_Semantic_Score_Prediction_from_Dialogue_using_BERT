      "utterance": [
        {
          "id": "MDRW2100007727.1.1",
          "form": "운동 해?",
          "original_form": "운동 해?",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007727.1.2",
          "form": "음 조금ㅋㅋㅋ",
          "original_form": "음 조금ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007727.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007727.1.4",
          "form": "나야 좋아하지",
          "original_form": "나야 좋아하지",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007727.1.5",
          "form": "헬스장 가고싶은데 못 가고있어",
          "original_form": "헬스장 가고싶은데 못 가고있어",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007727.1.6",
          "form": "아 ㅠ 시국이그래서?",
          "original_form": "아 ㅠ 시국이그래서?",
          "speaker_id": "1",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100007727.1.7",
          "form": "응응 헬스장 못 간지 1년이야...",
          "original_form": "응응 헬스장 못 간지 1년이야...",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007727.1.8",
          "form": "무슨 운동 해?",
          "original_form": "무슨 운동 해?",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007727.1.9",
          "form": "시국이라서 숨쉬기 운동 열심히해",
          "original_form": "시국이라서 숨쉬기 운동 열심히해",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007727.1.10",
          "form": "좋은 운동이지",
          "original_form": "좋은 운동이지",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100007727.1.11",
          "form": "요즘엔 그래 그거 잘하는게 어디야 ㅋㅋㅋ",
          "original_form": "요즘엔 그래 그거 잘하는게 어디야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100007727.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 진짜 나이들어 가는 느낌! 운동 힘들어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 진짜 나이들어 가는 느낌! 운동 힘들어",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100007727.1.13",
          "form": "그래도 하는게...좋더라 난!!",
          "original_form": "그래도 하는게...좋더라 난!!",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100007727.1.14",
          "form": "하다보면 중독돼 ㅋㅋㅋ",
          "original_form": "하다보면 중독돼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100007727.1.15",
          "form": "오 김종국 구독했어?",
          "original_form": "오 김종국 구독했어?",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100007727.1.16",
          "form": "아근데 혓바늘 또낫네 ",
          "original_form": "아근데 혓바늘 또낫네 ",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007727.1.17",
          "form": "김종국 구독했지",
          "original_form": "김종국 구독했지",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100007727.1.18",
          "form": "혓바늘? 나랑 똑같네 ㅋㅋㅋ 알보칠 발라",
          "original_form": "혓바늘? 나랑 똑같네 ㅋㅋㅋ 알보칠 발라",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007727.1.19",
          "form": "아프잖아,,,",
          "original_form": "아프잖아,,,",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007727.1.20",
          "form": "아니 왜생겼지",
          "original_form": "아니 왜생겼지",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007727.1.21",
          "form": "건강하고 싶다 혓바늘 없이",
          "original_form": "건강하고 싶다 혓바늘 없이",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007727.1.22",
          "form": "뭐 자소설쓰면 신경쓸것도 많고 하지",
          "original_form": "뭐 자소설쓰면 신경쓸것도 많고 하지",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007727.1.23",
          "form": "그래서이지 않으려나?",
          "original_form": "그래서이지 않으려나?",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007727.1.24",
          "form": "마자 스트레스성,, 건강은 스트레스 안받는게 짱인듯",
          "original_form": "마자 스트레스성,, 건강은 스트레스 안받는게 짱인듯",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100007727.1.25",
          "form": "걷는중 운동하고 있어",
          "original_form": "걷는중 운동하고 있어",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100007727.1.26",
          "form": "살찐것 같구 너무 운동 안한것같아서ㅋㅋㅋ",
          "original_form": "살찐것 같구 너무 운동 안한것같아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:02"
        }
      ]
