      "utterance": [
        {
          "id": "MDRW2100008312.1.1",
          "form": "하루 한잔정돞마셔요",
          "original_form": "하루 한잔정돞마셔요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008312.1.2",
          "form": "아이가 커피를 내려줘요 ㅎㅎ",
          "original_form": "아이가 커피를 내려줘요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008312.1.3",
          "form": "카페는 음료 먹으러 가요 ㅋㅋ",
          "original_form": "카페는 음료 먹으러 가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008312.1.4",
          "form": "오늘 어디 안가셨나봐요?",
          "original_form": "오늘 어디 안가셨나봐요?",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008312.1.5",
          "form": "저도 집콕 입니다 ㅎㅎ",
          "original_form": "저도 집콕 입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008312.1.6",
          "form": "저희 애들은 자기 입으로 들어가기 바쁜데...ㅎㅎ",
          "original_form": "저희 애들은 자기 입으로 들어가기 바쁜데...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008312.1.7",
          "form": "돌아서니 밥차리고 정리하고 또 밥차리고 설거지하고 하다보니 벌써 이시간이 되었네요 ㅠㅠ",
          "original_form": "돌아서니 밥차리고 정리하고 또 밥차리고 설거지하고 하다보니 벌써 이시간이 되었네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008312.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008312.1.9",
          "form": "계속 뭔가 하고 있네요",
          "original_form": "계속 뭔가 하고 있네요",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008312.1.10",
          "form": "먹고 돌아서면 또 밥하고",
          "original_form": "먹고 돌아서면 또 밥하고",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008312.1.11",
          "form": "그나마 오후시간이라 조금 여유가 있네요 ㅎㅎㅎ",
          "original_form": "그나마 오후시간이라 조금 여유가 있네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008312.1.12",
          "form": "네 오전에 분주해여",
          "original_form": "네 오전에 분주해여",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008312.1.13",
          "form": "그쵸  ㅎㅎ 근데 어제 늦게 주무신거 같은데 일찍 일어나셨나봐요",
          "original_form": "그쵸  ㅎㅎ 근데 어제 늦게 주무신거 같은데 일찍 일어나셨나봐요",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008312.1.14",
          "form": "저 불면증때문에",
          "original_form": "저 불면증때문에",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008312.1.15",
          "form": "6시반이면 깨요",
          "original_form": "6시반이면 깨요",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008312.1.16",
          "form": "애아빠가 일찍가서 그시간 자동요",
          "original_form": "애아빠가 일찍가서 그시간 자동요",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008312.1.17",
          "form": "아 그러시구나... 저도 어제 3시쯤 잤는데 7시에 깼네요..",
          "original_form": "아 그러시구나... 저도 어제 3시쯤 잤는데 7시에 깼네요..",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.19",
          "form": "평소엔 1시쯤 자고 6시.. ㅎㅎ",
          "original_form": "평소엔 1시쯤 자고 6시.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.20",
          "form": "똑같네요",
          "original_form": "똑같네요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.21",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.22",
          "form": "중간에 자럐요",
          "original_form": "중간에 자럐요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.23",
          "form": "오늘 낮잠 좀 자야하는데 아직 덜 피곤한지 ㅋㅋㅋㅋ 잠이 안오네요",
          "original_form": "오늘 낮잠 좀 자야하는데 아직 덜 피곤한지 ㅋㅋㅋㅋ 잠이 안오네요",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.24",
          "form": "잘깨요",
          "original_form": "잘깨요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008312.1.25",
          "form": "전 낮잠이 없어서요",
          "original_form": "전 낮잠이 없어서요",
          "speaker_id": "2",
