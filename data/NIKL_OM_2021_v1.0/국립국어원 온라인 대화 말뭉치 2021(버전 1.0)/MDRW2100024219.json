      "utterance": [
        {
          "id": "MDRW2100024219.1.1",
          "form": "빵빠레 맛이에요?",
          "original_form": "빵빠레 맛이에요?",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024219.1.2",
          "form": "엄청~달아요",
          "original_form": "엄청~달아요",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024219.1.3",
          "form": "색상이 진짜 카스테라크림색ㅋㅋ",
          "original_form": "색상이 진짜 카스테라크림색ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024219.1.4",
          "form": "처음에 일반빵빠레인줄알고 샀는데ㅋㅋ",
          "original_form": "처음에 일반빵빠레인줄알고 샀는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024219.1.5",
          "form": "색이 예쁘겠는데요?ㅋㅋㅋ",
          "original_form": "색이 예쁘겠는데요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024219.1.6",
          "form": "한입 먹고 놀랐어요ㅋㅋ",
          "original_form": "한입 먹고 놀랐어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024219.1.7",
          "form": "올해는 빵빠레를 안먹고 지나갔어요",
          "original_form": "올해는 빵빠레를 안먹고 지나갔어요",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024219.1.8",
          "form": "이런 해도 있네",
          "original_form": "이런 해도 있네",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024219.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024219.1.10",
          "form": "원래 바닐라 초코 잘 사먹거든요 빵빠레를 잊고 살다니",
          "original_form": "원래 바닐라 초코 잘 사먹거든요 빵빠레를 잊고 살다니",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024219.1.11",
          "form": "덕분에 새로운 빵빠레 정보도 알아가고 좋네여 ㅋㅋㅋ",
          "original_form": "덕분에 새로운 빵빠레 정보도 알아가고 좋네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.12",
          "form": "ㅋㅋㅋ초코빵빠레도 좋고ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ초코빵빠레도 좋고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.13",
          "form": "바닐라빵빠레도좋죠",
          "original_form": "바닐라빵빠레도좋죠",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.14",
          "form": "저 빙그레 아이스크림 좋아해요",
          "original_form": "저 빙그레 아이스크림 좋아해요",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.15",
          "form": "카스테라보다 바닐라나 초코를 더 추천해요",
          "original_form": "카스테라보다 바닐라나 초코를 더 추천해요",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.16",
          "form": "ㅋㅋㅋ 역시 기본이 맛있나봐요",
          "original_form": "ㅋㅋㅋ 역시 기본이 맛있나봐요",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.17",
          "form": "역시 스테디셀러는 다르죠ㅋㅋㅋ",
          "original_form": "역시 스테디셀러는 다르죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.18",
          "form": "지금 먹고 싶네요 ㅋㅋㅋㅋ",
          "original_form": "지금 먹고 싶네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024219.1.19",
          "form": "말이 나오니까 먹고 싶음ㅋㅋ",
          "original_form": "말이 나오니까 먹고 싶음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024219.1.20",
          "form": "ㅋㅋㅋ저는 내일 먹을 예정이예요",
          "original_form": "ㅋㅋㅋ저는 내일 먹을 예정이예요",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024219.1.21",
          "form": "이야기하니 먹고싶네요ㅋㅋ바닐라로 먹어야지ㅋㅋ",
          "original_form": "이야기하니 먹고싶네요ㅋㅋ바닐라로 먹어야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024219.1.22",
          "form": "올해는 저희동네 마트에서 못봤던거 같기도 해요",
          "original_form": "올해는 저희동네 마트에서 못봤던거 같기도 해요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024219.1.23",
          "form": "안사먹은걸 보니 ㅋㅋ",
          "original_form": "안사먹은걸 보니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024219.1.24",
          "form": "식음료가 아무래도 만만해서 ㅋㅋ",
          "original_form": "식음료가 아무래도 만만해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024219.1.25",
          "form": "몇개가 목표이신가요? 저는 베스킨이 목표였었어요...",
          "original_form": "몇개가 목표이신가요? 저는 베스킨이 목표였었어요...",
          "speaker_id": "1",
          "time": "20210830 23:26"
        }
      ]
    }
