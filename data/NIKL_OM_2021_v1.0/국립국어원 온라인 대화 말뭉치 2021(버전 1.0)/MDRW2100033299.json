        {
          "id": "MDRW2100033299.1.1",
          "form": "책좋아하세요?",
          "original_form": "책좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033299.1.2",
          "form": "네 좋아해요 ㅎㅎ",
          "original_form": "네 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033299.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033299.1.4",
          "form": "ㅎㅎ저는소설류나여행책이요ㅎㅎ",
          "original_form": "ㅎㅎ저는소설류나여행책이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033299.1.5",
          "form": "오 전 소설이랑 실용서요",
          "original_form": "오 전 소설이랑 실용서요",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033299.1.6",
          "form": "실용서라면...?",
          "original_form": "실용서라면...?",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033299.1.7",
          "form": "건강이나 정리 등 정보가 있는 책이요",
          "original_form": "건강이나 정리 등 정보가 있는 책이요",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033299.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100033299.1.9",
          "form": "최근얻은유용한정보가있나요",
          "original_form": "최근얻은유용한정보가있나요",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100033299.1.10",
          "form": "네 코 훌쩍이지 말 것! 귀에 안 좋데요",
          "original_form": "네 코 훌쩍이지 말 것! 귀에 안 좋데요",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100033299.1.11",
          "form": "아맞아요ㅎㅎ넘어가죠",
          "original_form": "아맞아요ㅎㅎ넘어가죠",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033299.1.12",
          "form": "읽고 있는 책 있나요?",
          "original_form": "읽고 있는 책 있나요?",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033299.1.13",
          "form": "꿀팁이네요",
          "original_form": "꿀팁이네요",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033299.1.14",
          "form": "음 종양에관한걸읽었어요",
          "original_form": "음 종양에관한걸읽었어요",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033299.1.15",
          "form": "종양이요? 흔한 주제는 아니네요",
          "original_form": "종양이요? 흔한 주제는 아니네요",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100033299.1.16",
          "form": "앗 그런가요ㅎㅎ",
