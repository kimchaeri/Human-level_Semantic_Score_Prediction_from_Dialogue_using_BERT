      "utterance": [
        {
          "id": "MDRW2100020038.1.1",
          "form": "저 학교 이제 안나가여",
          "original_form": "저 학교 이제 안나가여",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020038.1.2",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020038.1.3",
          "form": "자퇴했어요?",
          "original_form": "자퇴했어요?",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020038.1.4",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100020038.1.5",
          "form": "실습나가여",
          "original_form": "실습나가여",
          "speaker_id": "2",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100020038.1.6",
          "form": "아 그러면 학교수업은 없는거야?",
          "original_form": "아 그러면 학교수업은 없는거야?",
          "speaker_id": "1",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020038.1.7",
          "form": "학교수업대신 하는거라",
          "original_form": "학교수업대신 하는거라",
          "speaker_id": "2",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020038.1.8",
          "form": "패슺니ㅣ로 진행된답니당",
          "original_form": "패슺니ㅣ로 진행된답니당",
          "speaker_id": "2",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020038.1.9",
          "form": "시험,  과제 이런거 없어여",
          "original_form": "시험,  과제 이런거 없어여",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020038.1.10",
          "form": "와 부러워",
          "original_form": "와 부러워",
          "speaker_id": "1",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020038.1.11",
          "form": "나는 2주 수업, 2주 실습 번갈아 가면서 한다",
          "original_form": "나는 2주 수업, 2주 실습 번갈아 가면서 한다",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020038.1.12",
          "form": "수업은 2배수",
          "original_form": "수업은 2배수",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020038.1.13",
          "form": "중간고사 없고 기말만",
          "original_form": "중간고사 없고 기말만",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020038.1.14",
          "form": "진짜 별로다,,,",
          "original_form": "진짜 별로다,,,",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100020038.1.15",
          "form": "수업 안들어서 좋긴 한데 스트레스 후",
          "original_form": "수업 안들어서 좋긴 한데 스트레스 후",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100020038.1.16",
          "form": "그래도 과제 없으니까 좀 괜찮지 않아?",
          "original_form": "그래도 과제 없으니까 좀 괜찮지 않아?",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100020038.1.17",
          "form": "현장대신 과제하고싶어",
          "original_form": "현장대신 과제하고싶어",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100020038.1.18",
          "form": "나는 발표만 아니면 수업, 과제 다 좋아",
          "original_form": "나는 발표만 아니면 수업, 과제 다 좋아",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100020038.1.19",
          "form": "발표 너무 싫어..",
          "original_form": "발표 너무 싫어..",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100020038.1.20",
          "form": "우리는 발표를 잘 안해",
          "original_form": "우리는 발표를 잘 안해",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100020038.1.21",
          "form": "하지만 학년이 올라갈수록 많이 하더군,,,",
          "original_form": "하지만 학년이 올라갈수록 많이 하더군,,,",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100020038.1.22",
          "form": "우리는 한 과목 당 발표 한 번씩은 꼭 해",
          "original_form": "우리는 한 과목 당 발표 한 번씩은 꼭 해",
          "speaker_id": "1",
          "time": "20210829 00:01"
        }
      ]
