      "utterance": [
        {
          "id": "MDRW2100025590.1.1",
          "form": "최근 영화 관람 뭐 하셨나요?",
          "original_form": "최근 영화 관람 뭐 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:00"
        },
        {
          "id": "MDRW2100025590.1.2",
          "form": "싱크홀이요",
          "original_form": "싱크홀이요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025590.1.3",
          "form": "싱크홀 괜찮더라구요 재법 웃김",
          "original_form": "싱크홀 괜찮더라구요 재법 웃김",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025590.1.4",
          "form": "제법 ㅋㅋ",
          "original_form": "제법 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100025590.1.5",
          "form": "그러게요 웃긴부분꽤있었어요",
          "original_form": "그러게요 웃긴부분꽤있었어요",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025590.1.6",
          "form": "보고싶은 영화있으세요?",
          "original_form": "보고싶은 영화있으세요?",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025590.1.7",
          "form": "최근 못본거 중에 올드 보고 싶어요",
          "original_form": "최근 못본거 중에 올드 보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025590.1.8",
          "form": "근데 관이 별로 없더라구요",
          "original_form": "근데 관이 별로 없더라구요",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025590.1.9",
          "form": "맞아요 인기있어보였는데 금방 막을 내리네요",
          "original_form": "맞아요 인기있어보였는데 금방 막을 내리네요",
          "speaker_id": "2",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100025590.1.10",
          "form": "케이블로 뜨면 봐야할까봐요",
          "original_form": "케이블로 뜨면 봐야할까봐요",
          "speaker_id": "1",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025590.1.11",
          "form": "그래야겠네요",
          "original_form": "그래야겠네요",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025590.1.12",
          "form": "콘서트도 가고싶어요",
          "original_form": "콘서트도 가고싶어요",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025590.1.13",
          "form": "콘서트좋아하세요?",
          "original_form": "콘서트좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025590.1.14",
          "form": "가수 노래 듣는 재미가 있는데  ㅜㅜ",
          "original_form": "가수 노래 듣는 재미가 있는데  ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025590.1.15",
          "form": "요즘 잘 못가니 아쉬워요",
          "original_form": "요즘 잘 못가니 아쉬워요",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025590.1.16",
          "form": "부산 국제 영화제도 곧 하네요",
          "original_form": "부산 국제 영화제도 곧 하네요",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025590.1.17",
          "form": "부국제 좋죠 코로나가 이렇게 오래 갈지 몰랐어요",
          "original_form": "부국제 좋죠 코로나가 이렇게 오래 갈지 몰랐어요",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025590.1.18",
          "form": "그래도 작년보다는 크게 한다고 하더라구요",
          "original_form": "그래도 작년보다는 크게 한다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 14:19"
        },
        {
          "id": "MDRW2100025590.1.19",
          "form": "기대가 되네요",
          "original_form": "기대가 되네요",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025590.1.20",
          "form": "작년에는 못간거같은데",
          "original_form": "작년에는 못간거같은데",
          "speaker_id": "1",
          "time": "20210901 14:20"
        }
