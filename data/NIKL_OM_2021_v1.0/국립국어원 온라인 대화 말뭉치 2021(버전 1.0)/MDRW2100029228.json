      "utterance": [
        {
          "id": "MDRW2100029228.1.1",
          "form": "태클걸고했음",
          "original_form": "태클걸고했음",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.2",
          "form": "친한 친구였네요",
          "original_form": "친한 친구였네요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.3",
          "form": "난 과생활은 안하고",
          "original_form": "난 과생활은 안하고",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.4",
          "form": "과 생활 안 하고 게임 하셨죠",
          "original_form": "과 생활 안 하고 게임 하셨죠",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.5",
          "form": "동아리에 완전 올인했었음",
          "original_form": "동아리에 완전 올인했었음",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.6",
          "form": "게임 동아리 아닌가요",
          "original_form": "게임 동아리 아닌가요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.7",
          "form": "타이이요",
          "original_form": "타이이요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.8",
          "form": "타임이요",
          "original_form": "타임이요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.9",
          "form": "영어동아리였음",
          "original_form": "영어동아리였음",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.10",
          "form": "타임 아 아까 해서",
          "original_form": "타임 아 아까 해서",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.11",
          "form": "멋있는데요",
          "original_form": "멋있는데요",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029228.1.12",
          "form": "맨날 발표듣고 발표해야해서",
          "original_form": "맨날 발표듣고 발표해야해서",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029228.1.13",
          "form": "그때 영어공부 엄청해서",
          "original_form": "그때 영어공부 엄청해서",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029228.1.14",
          "form": "훌륭한 동아리네요",
          "original_form": "훌륭한 동아리네요",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029228.1.15",
          "form": "많이 늘었음",
          "original_form": "많이 늘었음",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029228.1.16",
          "form": "affiliation 타임이랑",
          "original_form": "&affiliation& 타임이랑",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029228.1.17",
          "form": "저도 대학교로 다시 돌아가면 동호회 쫌 하고 싶은데",
          "original_form": "저도 대학교로 다시 돌아가면 동호회 쫌 하고 싶은데",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029228.1.18",
          "form": "교환발표도 하고",
          "original_form": "교환발표도 하고",
          "speaker_id": "2",
          "time": "20210903 00:29"
        },
        {
