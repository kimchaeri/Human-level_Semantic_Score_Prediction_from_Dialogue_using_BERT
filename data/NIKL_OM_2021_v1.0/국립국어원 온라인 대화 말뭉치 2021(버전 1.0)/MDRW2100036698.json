      "utterance": [
        {
          "id": "MDRW2100036698.1.1",
          "form": "점심 메뉴는 땡기는거 드시러가나요?",
          "original_form": "점심 메뉴는 땡기는거 드시러가나요?",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100036698.1.2",
          "form": "비도 오고 하니 쌀국수를 먹을까 해요ㅎ",
          "original_form": "비도 오고 하니 쌀국수를 먹을까 해요ㅎ",
          "speaker_id": "1",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036698.1.3",
          "form": "님은 뭐 드실건가요",
          "original_form": "님은 뭐 드실건가요",
          "speaker_id": "1",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036698.1.4",
          "form": "와~~~ 저는 음.. 고민중이에여ㅠㅠ",
          "original_form": "와~~~ 저는 음.. 고민중이에여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036698.1.5",
          "form": "남은짜장소스에 후라이. 아니면 육개장사발면?ㅋㅋ",
          "original_form": "남은짜장소스에 후라이. 아니면 육개장사발면?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036698.1.6",
          "form": "둘중",
          "original_form": "둘중",
          "speaker_id": "2",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036698.1.7",
          "form": "육개장은 김치인가요 기본인가요",
          "original_form": "육개장은 김치인가요 기본인가요",
          "speaker_id": "1",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036698.1.8",
          "form": "기본ㅋㅋ작은사발면",
          "original_form": "기본ㅋㅋ작은사발면",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036698.1.9",
          "form": "어...날씨로는 후자인데... 제 취향은 전자요!",
          "original_form": "어...날씨로는 후자인데... 제 취향은 전자요!",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036698.1.10",
          "form": "제취향은 후자고 날씨도 후자인데",
          "original_form": "제취향은 후자고 날씨도 후자인데",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036698.1.11",
          "form": "아침에 밀가루먹었으니ㅠ",
          "original_form": "아침에 밀가루먹었으니ㅠ",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036698.1.12",
          "form": "근데 꼭 둘다 택일해야 할까요",
          "original_form": "근데 꼭 둘다 택일해야 할까요",
          "speaker_id": "1",
          "time": "20210831 09:47"
        },
        {
          "id": "MDRW2100036698.1.13",
          "form": "우리는 두 토끼라는 좋은 말을 알고 있죠ㅎ",
          "original_form": "우리는 두 토끼라는 좋은 말을 알고 있죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 09:50"
        },
        {
          "id": "MDRW2100036698.1.14",
          "form": "ㅋㅋㅋ아미쳐 용량이..ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아미쳐 용량이..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036698.1.15",
          "form": "되지만ㅠㅠ 제살은 어쩌죠",
          "original_form": "되지만ㅠㅠ 제살은 어쩌죠",
          "speaker_id": "2",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036698.1.16",
          "form": "그건...내일의 name2님이 어떻게든 해주겠죠!",
          "original_form": "그건...내일의 &name2&님이 어떻게든 해주겠죠!",
          "speaker_id": "1",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036698.1.17",
          "form": "축척됩니다",
          "original_form": "축척됩니다",
          "speaker_id": "2",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036698.1.18",
          "form": "나가진않아여",
          "original_form": "나가진않아여",
          "speaker_id": "2",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036698.1.19",
          "form": "아니!사람이 왜 그렇게 야박해요",
          "original_form": "아니!사람이 왜 그렇게 야박해요",
          "speaker_id": "1",
          "time": "20210831 09:56"
        },
        {
          "id": "MDRW2100036698.1.20",
          "form": "육개장이 서운해하는 거 안 보여요?",
          "original_form": "육개장이 서운해하는 거 안 보여요?",
          "speaker_id": "1",
          "time": "20210831 09:56"
        },
        {
          "id": "MDRW2100036698.1.21",
          "form": "지금은그런데ㅋㅋ또 그시간에먹고있을거에요",
          "original_form": "지금은그런데ㅋㅋ또 그시간에먹고있을거에요",
