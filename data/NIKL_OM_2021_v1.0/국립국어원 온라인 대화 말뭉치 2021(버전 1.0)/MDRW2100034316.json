      "utterance": [
        {
          "id": "MDRW2100034316.1.1",
          "form": "그럴수있죠\n사람마다 다르니까요",
          "original_form": "그럴수있죠\n사람마다 다르니까요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034316.1.2",
          "form": "맞아요ㅠ흑",
          "original_form": "맞아요ㅠ흑",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034316.1.3",
          "form": "선크림 꾸준히 바르세요?",
          "original_form": "선크림 꾸준히 바르세요?",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034316.1.4",
          "form": "바르려고 노력하는데\n귀찮기도하고",
          "original_form": "바르려고 노력하는데\n귀찮기도하고",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034316.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034316.1.6",
          "form": "선크림바르면 눈따갑고해서",
          "original_form": "선크림바르면 눈따갑고해서",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034316.1.7",
          "form": "잘 안바르게 되더라구요ㅠ",
          "original_form": "잘 안바르게 되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.8",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.9",
          "form": "그렇단 분들 계시더라구요",
          "original_form": "그렇단 분들 계시더라구요",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.10",
          "form": "안따가운 선크림도 있다던데",
          "original_form": "안따가운 선크림도 있다던데",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.11",
          "form": "아그래요?",
          "original_form": "아그래요?",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.12",
          "form": "저는  아직 못찾아서ㅠ",
          "original_form": "저는  아직 못찾아서ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.13",
          "form": "찾으면 같이 공유해요 ㅋㅋㅋ",
          "original_form": "찾으면 같이 공유해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.14",
          "form": "네 있다그러더라구요",
          "original_form": "네 있다그러더라구요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034316.1.15",
          "form": "그래요ㅋㅋㅋㄱ",
          "original_form": "그래요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034316.1.16",
          "form": "얼른 찾고싶네요 ㅋㅋㅋ",
          "original_form": "얼른 찾고싶네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034316.1.17",
          "form": "화장하실때도 크림바르세요?",
          "original_form": "화장하실때도 크림바르세요?",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034316.1.18",
          "form": "피부과의사들은 선크림 필수라하더라구요",
          "original_form": "피부과의사들은 선크림 필수라하더라구요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034316.1.19",
          "form": "네 화장할때도 기초 똑같이 해요",
          "original_form": "네 화장할때도 기초 똑같이 해요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034316.1.20",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034316.1.21",
          "form": "화장할땐 양조절만 해요",
          "original_form": "화장할땐 양조절만 해요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        }
      ]
