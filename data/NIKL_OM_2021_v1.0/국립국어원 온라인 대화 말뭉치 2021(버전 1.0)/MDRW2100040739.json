      "utterance": [
        {
          "id": "MDRW2100040739.1.1",
          "form": "그런데서해도 안 팔리는데 마을기업에서 팔면 참 많이 빠르겠네요",
          "original_form": "그런데서해도 안 팔리는데 마을기업에서 팔면 참 많이 빠르겠네요",
          "speaker_id": "1",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100040739.1.2",
          "form": "팔리겠네요",
          "original_form": "팔리겠네요",
          "speaker_id": "1",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100040739.1.3",
          "form": "ㅋㅋㅋ 그러니까여",
          "original_form": "ㅋㅋㅋ 그러니까여",
          "speaker_id": "2",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100040739.1.4",
          "form": "연예인들이 팔아도 안사는 마당에",
          "original_form": "연예인들이 팔아도 안사는 마당에",
          "speaker_id": "2",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100040739.1.5",
          "form": "실적때문에 사야되는 사람이나 사겠죠?ㅋㅋㅋㅋ",
          "original_form": "실적때문에 사야되는 사람이나 사겠죠?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100040739.1.6",
          "form": "아 저는 그걸 페이스북 광고에 떠 가지고가 보고 알게 됐어요",
          "original_form": "아 저는 그걸 페이스북 광고에 떠 가지고가 보고 알게 됐어요",
          "speaker_id": "1",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100040739.1.7",
          "form": "페이스북 하시는구나!",
          "original_form": "페이스북 하시는구나!",
          "speaker_id": "2",
          "time": "20210905 01:26"
        },
        {
          "id": "MDRW2100040739.1.8",
          "form": "네 거기서 봤어요",
          "original_form": "네 거기서 봤어요",
          "speaker_id": "1",
          "time": "20210905 01:27"
        },
        {
          "id": "MDRW2100040739.1.9",
          "form": "호응 없으면 선물 준다면서 댓글 달라고 하더라고요",
          "original_form": "호응 없으면 선물 준다면서 댓글 달라고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040739.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040739.1.11",
          "form": "뭔 야바위 짓인지 모르겠어요",
          "original_form": "뭔 야바위 짓인지 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040739.1.12",
          "form": "진짜 그런걸로 돈 버는 사람도 있고",
          "original_form": "진짜 그런걸로 돈 버는 사람도 있고",
          "speaker_id": "2",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040739.1.13",
          "form": "참 알다가도 모르겠어요",
          "original_form": "참 알다가도 모르겠어요",
          "speaker_id": "2",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100040739.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040739.1.15",
          "form": "좀 약삭빠르다고 해야되나",
          "original_form": "좀 약삭빠르다고 해야되나",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040739.1.16",
          "form": "저도 욕했는데 재주 만 있으면 하시라고 하고 싶어요",
          "original_form": "저도 욕했는데 재주 만 있으면 하시라고 하고 싶어요",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040739.1.17",
          "form": "그런 사람들이 돈버는거 같아서",
          "original_form": "그런 사람들이 돈버는거 같아서",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040739.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040739.1.19",
          "form": "그런 재주가 있었다면",
          "original_form": "그런 재주가 있었다면",
          "speaker_id": "2",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100040739.1.20",
          "form": "진짜 그런 사람도 자기가 되게  대단한 사업 하는 줄 알더라구요",
          "original_form": "진짜 그런 사람도 자기가 되게  대단한 사업 하는 줄 알더라구요",
