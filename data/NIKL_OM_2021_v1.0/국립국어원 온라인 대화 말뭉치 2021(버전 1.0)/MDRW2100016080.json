      "utterance": [
        {
          "id": "MDRW2100016080.1.1",
          "form": "오늘은 비소식이 맞을라나",
          "original_form": "오늘은 비소식이 맞을라나",
          "speaker_id": "2",
          "time": "20210826 09:16"
        },
        {
          "id": "MDRW2100016080.1.2",
          "form": "어제처럼 갑자기 해떴으면 좋겠다",
          "original_form": "어제처럼 갑자기 해떴으면 좋겠다",
          "speaker_id": "1",
          "time": "20210826 09:16"
        },
        {
          "id": "MDRW2100016080.1.3",
          "form": "습도가 너무 높아",
          "original_form": "습도가 너무 높아",
          "speaker_id": "2",
          "time": "20210826 09:16"
        },
        {
          "id": "MDRW2100016080.1.4",
          "form": "집안 습도는 벌써 80프로야 ㅋㅋ",
          "original_form": "집안 습도는 벌써 80프로야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:16"
        },
        {
          "id": "MDRW2100016080.1.5",
          "form": "바닷가에 놀러 온 기분이겠네",
          "original_form": "바닷가에 놀러 온 기분이겠네",
          "speaker_id": "2",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100016080.1.6",
          "form": "ㅇㅇ 진짜 바닷가처럼 끈적거리네 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 진짜 바닷가처럼 끈적거리네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100016080.1.7",
          "form": "ㅎㅎㅎ 바닷가 근처에 사는 사람들은 매일 습한데 어떻게 사나 몰라",
          "original_form": "ㅎㅎㅎ 바닷가 근처에 사는 사람들은 매일 습한데 어떻게 사나 몰라",
          "speaker_id": "2",
          "time": "20210826 09:17"
        },
        {
          "id": "MDRW2100016080.1.8",
          "form": "빨래 같은 거 잘 안말라서 제습기 필수일듯",
          "original_form": "빨래 같은 거 잘 안말라서 제습기 필수일듯",
          "speaker_id": "1",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016080.1.9",
          "form": "ㅇㅇ 에어컨도 제습 모드로 많이 돌릴듯",
          "original_form": "ㅇㅇ 에어컨도 제습 모드로 많이 돌릴듯",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016080.1.10",
          "form": "가을 장마 빨리 지나가면 좋겠다",
          "original_form": "가을 장마 빨리 지나가면 좋겠다",
          "speaker_id": "1",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016080.1.11",
          "form": "다음주까지 갈 것 같던데",
          "original_form": "다음주까지 갈 것 같던데",
          "speaker_id": "2",
          "time": "20210826 09:18"
        },
        {
          "id": "MDRW2100016080.1.12",
          "form": "최소한 비라도 안왔으면",
          "original_form": "최소한 비라도 안왔으면",
          "speaker_id": "1",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016080.1.13",
          "form": "ㅇㅇ 그래야 밖에 산책이라도 하니깐",
          "original_form": "ㅇㅇ 그래야 밖에 산책이라도 하니깐",
          "speaker_id": "2",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016080.1.14",
          "form": "오 오전의 내 바람대로 해가 뜨고 있어",
          "original_form": "오 오전의 내 바람대로 해가 뜨고 있어",
