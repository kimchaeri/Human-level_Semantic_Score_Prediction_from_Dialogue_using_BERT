      "utterance": [
        {
          "id": "MDRW2100047566.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100047566.1.2",
          "form": "네 저녁 먹었어요",
          "original_form": "네 저녁 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100047566.1.3",
          "form": "오랜만에 보쌈 든든히 먹었습니다~",
          "original_form": "오랜만에 보쌈 든든히 먹었습니다~",
          "speaker_id": "2",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100047566.1.4",
          "form": "아 그러셨지....제가 헷갈렸네요",
          "original_form": "아 그러셨지....제가 헷갈렸네요",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100047566.1.5",
          "form": "보쌈 족발중에 뭐좋아하세요?",
          "original_form": "보쌈 족발중에 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100047566.1.6",
          "form": "ㅎㅎ둘 다 좋은데",
          "original_form": "ㅎㅎ둘 다 좋은데",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047566.1.7",
          "form": "보쌈은 집에서도 종종 해먹어서",
          "original_form": "보쌈은 집에서도 종종 해먹어서",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047566.1.8",
          "form": "배달시킬 땐 족발이요",
          "original_form": "배달시킬 땐 족발이요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100047566.1.9",
          "form": "헉 집에서도 해드세요?",
          "original_form": "헉 집에서도 해드세요?",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047566.1.10",
          "form": "조리법 물어봐도 될까요?",
          "original_form": "조리법 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100047566.1.11",
          "form": "제가 만들지는 않아서ㅎㅎ",
          "original_form": "제가 만들지는 않아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047566.1.12",
          "form": "보쌈은 맛있는 돼지고기 부위를 사서",
          "original_form": "보쌈은 맛있는 돼지고기 부위를 사서",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100047566.1.13",
          "form": "된장이랑 이것저것 넣고 끓인다는데",
          "original_form": "된장이랑 이것저것 넣고 끓인다는데",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100047566.1.14",
          "form": "반찬은 어떻게 해드세요?",
          "original_form": "반찬은 어떻게 해드세요?",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047566.1.15",
          "form": "부모님이 주시기도 하는데",
          "original_form": "부모님이 주시기도 하는데",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047566.1.16",
          "form": "보쌈은 반찬도 중요하다고 생각해서요",
          "original_form": "보쌈은 반찬도 중요하다고 생각해서요",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100047566.1.17",
          "form": "아 김치랑 무말랭이, 쌈채소  이런 것들 곁들여먹어요",
          "original_form": "아 김치랑 무말랭이, 쌈채소  이런 것들 곁들여먹어요",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047566.1.18",
          "form": "가끔 장아찌류 있음 같이 먹고",
          "original_form": "가끔 장아찌류 있음 같이 먹고",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100047566.1.19",
          "form": "부모님이 주시기도 한다시는걸 보면 자취하세요?",
          "original_form": "부모님이 주시기도 한다시는걸 보면 자취하세요?",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047566.1.20",
          "form": "아 결혼해서 따로 살아요ㅎㅎ",
          "original_form": "아 결혼해서 따로 살아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100047566.1.21",
          "form": "결혼하셨구나!",
          "original_form": "결혼하셨구나!",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047566.1.22",
          "form": "요리는 파트너분이 담당하시나요?",
          "original_form": "요리는 파트너분이 담당하시나요?",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100047566.1.23",
          "form": "아 네네~",
          "original_form": "아 네네~",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100047566.1.24",
          "form": "저보단 요리에 소질이 있어서ㅎㅎ",
          "original_form": "저보단 요리에 소질이 있어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:35"
        }
      ]
    }
  ]
