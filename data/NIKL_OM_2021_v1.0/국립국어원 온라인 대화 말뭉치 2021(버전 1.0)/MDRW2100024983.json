      "utterance": [
        {
          "id": "MDRW2100024983.1.1",
          "form": "지난번에 mbc 근처에 있는 카페 갔는데",
          "original_form": "지난번에 mbc 근처에 있는 카페 갔는데",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024983.1.2",
          "form": "스콘이 맛있더라",
          "original_form": "스콘이 맛있더라",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024983.1.3",
          "form": "요즘엔 발굴한 빵맛집카페 없니",
          "original_form": "요즘엔 발굴한 빵맛집카페 없니",
          "speaker_id": "2",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024983.1.4",
          "form": "어머나 스콘 나도먹으러갈래",
          "original_form": "어머나 스콘 나도먹으러갈래",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024983.1.5",
          "form": "요즘 잘 밖에 안나가 ㅠ",
          "original_form": "요즘 잘 밖에 안나가 ㅠ",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100024983.1.6",
          "form": "아..하긴 ㅠ 코시국...",
          "original_form": "아..하긴 ㅠ 코시국...",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024983.1.7",
          "form": "무슴맛 스콘먹엇어?",
          "original_form": "무슴맛 스콘먹엇어?",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024983.1.8",
          "form": "그런 건 없었는데 스콘에서 살짝 짭짤한 맛이 나더라",
          "original_form": "그런 건 없었는데 스콘에서 살짝 짭짤한 맛이 나더라",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.9",
          "form": "단짠단짠이구나",
          "original_form": "단짠단짠이구나",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.10",
          "form": "쨈이랑 버러랑 먹엇니?",
          "original_form": "쨈이랑 버러랑 먹엇니?",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.11",
          "form": "소스? 같은게 4개정도 잇었는데 토마토잼에 치즈같은거 섞어서",
          "original_form": "소스? 같은게 4개정도 잇었는데 토마토잼에 치즈같은거 섞어서",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.12",
          "form": "맛있었음...",
          "original_form": "맛있었음...",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.13",
          "form": "와 토마토잼",
          "original_form": "와 토마토잼",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.14",
          "form": "레몬잼 같은 것도 있고",
          "original_form": "레몬잼 같은 것도 있고",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.15",
          "form": "맛잇겟다..",
          "original_form": "맛잇겟다..",
          "speaker_id": "1",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024983.1.16",
          "form": "어엉 담에 우리도 그런 곳으루...",
          "original_form": "어엉 담에 우리도 그런 곳으루...",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024983.1.17",
          "form": "음료는 머먹엇어",
          "original_form": "음료는 머먹엇어",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024983.1.18",
          "form": "당연 아아...",
          "original_form": "당연 아아...",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024983.1.19",
          "form": "가즈아 핫한 카페 한번 찾아바야것어",
          "original_form": "가즈아 핫한 카페 한번 찾아바야것어",
          "speaker_id": "1",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024983.1.20",
          "form": "빵이 풍족한 곳으러 ㅋㅋ",
          "original_form": "빵이 풍족한 곳으러 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024983.1.21",
          "form": "마자 디저트 맛잇어야지",
          "original_form": "마자 디저트 맛잇어야지",
          "speaker_id": "1",
          "time": "20210831 21:50"
        }
