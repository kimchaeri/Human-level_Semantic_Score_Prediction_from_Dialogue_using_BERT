      "utterance": [
        {
          "id": "MDRW2100007005.1.1",
          "form": "새롭게 이야기를 ㅋㅋ",
          "original_form": "새롭게 이야기를 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007005.1.2",
          "form": "커피 좋아하시나요",
          "original_form": "커피 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100007005.1.3",
          "form": "전 카페인에 좀 약해서",
          "original_form": "전 카페인에 좀 약해서",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007005.1.4",
          "form": "즐기지는 않는 편입니다 ㅎㅎ",
          "original_form": "즐기지는 않는 편입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007005.1.5",
          "form": "아 그러면 카페 가시면 주로 어떤거 드세요?",
          "original_form": "아 그러면 카페 가시면 주로 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007005.1.6",
          "form": "에이드랑 차 같은거요",
          "original_form": "에이드랑 차 같은거요",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007005.1.7",
          "form": "그러시군요 카페는 자주 가시나요?",
          "original_form": "그러시군요 카페는 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007005.1.8",
          "form": "친구 만났을때만 가요~",
          "original_form": "친구 만났을때만 가요~",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100007005.1.9",
          "form": "저는 카페 가는 거 좋아하는 편이었는데 코로나로 거의 손에 꼽게 가본 것 같아요 ㅜ",
          "original_form": "저는 카페 가는 거 좋아하는 편이었는데 코로나로 거의 손에 꼽게 가본 것 같아요 ㅜ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100007005.1.10",
          "form": "아이구 ㅠㅠ 코로나로 진짜 못하는게 많아졌죠",
          "original_form": "아이구 ㅠㅠ 코로나로 진짜 못하는게 많아졌죠",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007005.1.11",
          "form": "커피도 좋아하고 카페라는 공간 자체도 좋아해서 많이 아쉬워요",
          "original_form": "커피도 좋아하고 카페라는 공간 자체도 좋아해서 많이 아쉬워요",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007005.1.12",
          "form": "가서 앉아있으면 좋긴하죠 ㅎㅎ.. 맛난거 먹고 멍때리고",
          "original_form": "가서 앉아있으면 좋긴하죠 ㅎㅎ.. 맛난거 먹고 멍때리고",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007005.1.13",
          "form": "맞아요 별로 하는건 없는데 ㅋㅋ",
          "original_form": "맞아요 별로 하는건 없는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007005.1.14",
          "form": "돈으로 행복을 사는 ㅎㅎ",
          "original_form": "돈으로 행복을 사는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007005.1.15",
          "form": "커피를 한동안 끊었는데 내일부터 다시 마시려구요 ㅋㅋ 카페는 못 가지만",
          "original_form": "커피를 한동안 끊었는데 내일부터 다시 마시려구요 ㅋㅋ 카페는 못 가지만",
          "speaker_id": "1",
