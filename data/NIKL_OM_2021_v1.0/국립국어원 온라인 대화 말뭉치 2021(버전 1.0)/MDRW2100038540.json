      "utterance": [
        {
          "id": "MDRW2100038540.1.1",
          "form": "저도 보고 헉한게 한두개가아녀요",
          "original_form": "저도 보고 헉한게 한두개가아녀요",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100038540.1.2",
          "form": "그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.3",
          "form": "인스타에 들어가면 다들",
          "original_form": "인스타에 들어가면 다들",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.4",
          "form": "그냥.... 맥심 찍고 싶어서 안달난 것처럼 보여요",
          "original_form": "그냥.... 맥심 찍고 싶어서 안달난 것처럼 보여요",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.5",
          "form": "헐벗고있잖아요",
          "original_form": "헐벗고있잖아요",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.6",
          "form": "누구 좋으라고 그러는건지?",
          "original_form": "누구 좋으라고 그러는건지?",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.7",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.8",
          "form": "그니까여 보는사람도 민망한..",
          "original_form": "그니까여 보는사람도 민망한..",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.9",
          "form": "솔직히 보는 남자들만 좋은 일 하는거죠....",
          "original_form": "솔직히 보는 남자들만 좋은 일 하는거죠....",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038540.1.10",
          "form": "여자들 사이에서 더 유행인게 현타 와요",
          "original_form": "여자들 사이에서 더 유행인게 현타 와요",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.11",
          "form": "내가 아는사람의 빤스차림을..",
          "original_form": "내가 아는사람의 빤스차림을..",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.12",
          "form": "ㅋㅋㅋㅋㅋ 그니까요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그니까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.13",
          "form": "지하철 타면",
          "original_form": "지하철 타면",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.14",
          "form": "씁쓸합니다..",
          "original_form": "씁쓸합니다..",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.15",
          "form": "남자들 다 배 나오고",
          "original_form": "남자들 다 배 나오고",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.16",
          "form": "마자요 정작 뺄사람안안빼구여 ㅋㅋㅋㅋ",
          "original_form": "마자요 정작 뺄사람안안빼구여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.17",
          "form": "코로나여서 마스크 쓰고있는데도 턱선 무너진거 다 보이는데",
          "original_form": "코로나여서 마스크 쓰고있는데도 턱선 무너진거 다 보이는데",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.18",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.19",
          "form": "여자들만 예뻐보이려고 애쓰는 게 현타 와요",
          "original_form": "여자들만 예뻐보이려고 애쓰는 게 현타 와요",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038540.1.20",
          "form": "네 그렇게까지해서..건강다상하면서ㅜ",
          "original_form": "네 그렇게까지해서..건강다상하면서ㅜ",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100038540.1.21",
          "form": "맞아요 돈도 많이 들더라구요",
          "original_form": "맞아요 돈도 많이 들더라구요",
          "speaker_id": "1",
