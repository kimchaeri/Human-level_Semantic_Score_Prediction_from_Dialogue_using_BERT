      "utterance": [
        {
          "id": "MDRW2100006854.1.1",
          "form": "선물한 물건중에서는요?",
          "original_form": "선물한 물건중에서는요?",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006854.1.2",
          "form": "걱정인형이요",
          "original_form": "걱정인형이요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006854.1.3",
          "form": "어디서 사셨어요?",
          "original_form": "어디서 사셨어요?",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006854.1.4",
          "form": "인터넷으로 시켰어요",
          "original_form": "인터넷으로 시켰어요",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006854.1.5",
          "form": "아 그렇게 파는군요\n중고거래는 해보셨나요",
          "original_form": "아 그렇게 파는군요\n중고거래는 해보셨나요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006854.1.6",
          "form": "네 가끔 거래해요",
          "original_form": "네 가끔 거래해요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006854.1.7",
          "form": "어디서 하시나요",
          "original_form": "어디서 하시나요",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006854.1.8",
          "form": "중고나라요",
          "original_form": "중고나라요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006854.1.9",
          "form": "거래 잘 되던가요",
          "original_form": "거래 잘 되던가요",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006854.1.10",
          "form": "네 싸게 내놓으면 금방 나가요",
          "original_form": "네 싸게 내놓으면 금방 나가요",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006854.1.11",
          "form": "그렇군요 저는 당근 애용해요",
          "original_form": "그렇군요 저는 당근 애용해요",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100006854.1.12",
          "form": "요즘 당근이 핫하더라고요",
          "original_form": "요즘 당근이 핫하더라고요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006854.1.13",
