        },
        {
          "id": "MDRW2100014219.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014219.1.3",
          "form": "비가 자주오는거같아요",
          "original_form": "비가 자주오는거같아요",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014219.1.4",
          "form": "넵 그러게용",
          "original_form": "넵 그러게용",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014219.1.5",
          "form": "여기도 지금 비가 엄청내려서",
          "original_form": "여기도 지금 비가 엄청내려서",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014219.1.6",
          "form": "우산 완전 필수에영",
          "original_form": "우산 완전 필수에영",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014219.1.7",
          "form": "맞아요 근데  이런 날씨는",
          "original_form": "맞아요 근데  이런 날씨는",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014219.1.8",
          "form": "우산도 소용이 없더라구요",
          "original_form": "우산도 소용이 없더라구요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014219.1.9",
          "form": "마자요 흑흑..",
          "original_form": "마자요 흑흑..",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014219.1.10",
          "form": "비에  젖어 축축해지더라구요",
          "original_form": "비에  젖어 축축해지더라구요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014219.1.11",
          "form": "사실 오늘 밖에 나왔는데 차가 지나가서 빗물로 다 젖었답니다^^;;;",
          "original_form": "사실 오늘 밖에 나왔는데 차가 지나가서 빗물로 다 젖었답니다^^;;;",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.12",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.13",
          "form": "너무 화나셨겠다",
          "original_form": "너무 화나셨겠다",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.14",
          "form": "괜찮으세요?",
          "original_form": "괜찮으세요?",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.16",
          "form": "뭐 그런 차가 다 있대요",
          "original_form": "뭐 그런 차가 다 있대요",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.17",
          "form": "어쩔 슈 없죠ㅠ",
          "original_form": "어쩔 슈 없죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.18",
          "form": "고생이 많으셨네요ㅠ",
          "original_form": "고생이 많으셨네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014219.1.19",
          "form": "좋아하는 계절있나요",
          "original_form": "좋아하는 계절있나요",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014219.1.20",
          "form": "name2은 오늘 비오는데 뭐하셨어여",
          "original_form": "&name2&은 오늘 비오는데 뭐하셨어여",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014219.1.21",
          "form": "아 저는 겨울이요",
          "original_form": "아 저는 겨울이요",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014219.1.22",
          "form": "더운거 안좋아하시는구나",
          "original_form": "더운거 안좋아하시는구나",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014219.1.23",
          "form": "저도그래요",
          "original_form": "저도그래요",
