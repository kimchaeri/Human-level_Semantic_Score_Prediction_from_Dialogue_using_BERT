      "utterance": [
        {
          "id": "MDRW2100034828.1.1",
          "form": "그알 같은것도 보세요?",
          "original_form": "그알 같은것도 보세요?",
          "speaker_id": "2",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100034828.1.2",
          "form": "보고싶은 사건 위주로 가끔 챙겨봐요!",
          "original_form": "보고싶은 사건 위주로 가끔 챙겨봐요!",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100034828.1.3",
          "form": "저도 종종 보는데",
          "original_form": "저도 종종 보는데",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034828.1.4",
          "form": "엄청 무서운 사건 많지않나요 ㅠㅜ",
          "original_form": "엄청 무서운 사건 많지않나요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100034828.1.5",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034828.1.6",
          "form": "해결 안 된 사건도 많고...",
          "original_form": "해결 안 된 사건도 많고...",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034828.1.7",
          "form": "그 기동슈퍼 사건 아세요?",
          "original_form": "그 기동슈퍼 사건 아세요?",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034828.1.8",
          "form": "헉 아뇨! 그건 안 본 거 같아요",
          "original_form": "헉 아뇨! 그건 안 본 거 같아요",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034828.1.9",
          "form": "그거 한번 보세요 .. 좀 무섭긴한데",
          "original_form": "그거 한번 보세요 .. 좀 무섭긴한데",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100034828.1.10",
          "form": "재밌어요!",
          "original_form": "재밌어요!",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100034828.1.11",
          "form": "기동 슈퍼는 그알인 거죠?",
          "original_form": "기동 슈퍼는 그알인 거죠?",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100034828.1.12",
          "form": "네네 ! 그알이에요",
          "original_form": "네네 ! 그알이에요",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034828.1.13",
          "form": "유튭에 정리해논거 있더라구요",
          "original_form": "유튭에 정리해논거 있더라구요",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034828.1.14",
          "form": "유튜브에 있군요! 나중에 챙겨볼게요!",
          "original_form": "유튜브에 있군요! 나중에 챙겨볼게요!",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100034828.1.15",
          "form": "무서운거 잘 잘보시나요?",
          "original_form": "무서운거 잘 잘보시나요?",
          "speaker_id": "2",
          "time": "20210828 20:31"
        },
        {
          "id": "MDRW2100034828.1.16",
          "form": "좋아하진 않지만 그래도 잘 보는 편인 거 같아요",
          "original_form": "좋아하진 않지만 그래도 잘 보는 편인 거 같아요",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034828.1.17",
          "form": "저도 무서워하면서 봐요 ㅋㅋㅋ",
          "original_form": "저도 무서워하면서 봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:36"
        }
      ]
