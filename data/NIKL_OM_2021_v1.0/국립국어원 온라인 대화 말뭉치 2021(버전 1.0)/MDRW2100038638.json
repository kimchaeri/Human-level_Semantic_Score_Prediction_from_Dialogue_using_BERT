        },
        {
          "id": "MDRW2100038638.1.2",
          "form": "무슨색 갖고싶으세요?",
          "original_form": "무슨색 갖고싶으세요?",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038638.1.3",
          "form": "색은 그냥 화이트나 블랙이요",
          "original_form": "색은 그냥 화이트나 블랙이요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038638.1.4",
          "form": "근데 콜라보한 케이스들 왜케이뻐요",
          "original_form": "근데 콜라보한 케이스들 왜케이뻐요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038638.1.5",
          "form": "그거보고 혹했어요",
          "original_form": "그거보고 혹했어요",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100038638.1.6",
          "form": "ㅋㅋㅋ맞아요 콜라보제품들이 다 귀여워요ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ맞아요 콜라보제품들이 다 귀여워요ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100038638.1.7",
          "form": "저도 화이트가 예뻐보여요 ㅋㅋㅋㅋ 글구 외부액정ㅋㅁㅋ",
          "original_form": "저도 화이트가 예뻐보여요 ㅋㅋㅋㅋ 글구 외부액정ㅋㅁㅋ",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100038638.1.8",
          "form": "스티커랑 핑거톡 이런거랑",
          "original_form": "스티커랑 핑거톡 이런거랑",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038638.1.9",
          "form": "이갈고 냈나봐요 삼성",
          "original_form": "이갈고 냈나봐요 삼성",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038638.1.10",
          "form": "근데 랜덤박스 선물은 너무 ㅋㅋㅋ허접한거 주는게 웃겻어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 랜덤박스 선물은 너무 ㅋㅋㅋ허접한거 주는게 웃겻어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100038638.1.11",
          "form": "헐 그건모에요? 랜박??",
          "original_form": "헐 그건모에요? 랜박??",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038638.1.12",
          "form": "그 사전예약 사은품으로 랜덤박스를 줬대여",
          "original_form": "그 사전예약 사은품으로 랜덤박스를 줬대여",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100038638.1.13",
          "form": "근데 완전 거지같은거만 들어있었나봐요 ㅋㅋㅋㅋ",
          "original_form": "근데 완전 거지같은거만 들어있었나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100038638.1.14",
          "form": "으이구 삼성 돈도많으면서... 인심 좀 더 쓰지!!",
          "original_form": "으이구 삼성 돈도많으면서... 인심 좀 더 쓰지!!",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038638.1.15",
          "form": "ㅋㅋㅋㅋ근데 저는 플립 사도 안접을 것같아요",
          "original_form": "ㅋㅋㅋㅋ근데 저는 플립 사도 안접을 것같아요",
