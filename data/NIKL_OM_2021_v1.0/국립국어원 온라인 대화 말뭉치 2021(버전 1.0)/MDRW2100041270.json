        {
          "id": "MDRW2100041270.1.3",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041270.1.4",
          "form": "그런 관광이 좀 싫어서",
          "original_form": "그런 관광이 좀 싫어서",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041270.1.5",
          "form": "자유여행이 좋더라",
          "original_form": "자유여행이 좋더라",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041270.1.6",
          "form": "우리둘이 해외 자유여행가려면",
          "original_form": "우리둘이 해외 자유여행가려면",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041270.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041270.1.8",
          "form": "진짜 나 운동 ㄱ ㅐ열심히해야겟다",
          "original_form": "진짜 나 운동 ㄱ ㅐ열심히해야겟다",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.10",
          "form": "우리 약감",
          "original_form": "우리 약감",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.11",
          "form": "성인남자가 시비걸어도",
          "original_form": "성인남자가 시비걸어도",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.12",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.13",
          "form": "한명정도는 족칠수잇게",
          "original_form": "한명정도는 족칠수잇게",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.14",
          "form": "2일에 한번씩 관광지 보고",
          "original_form": "2일에 한번씩 관광지 보고",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅈ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅈ",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.16",
          "form": "맛집 돌아다니다 숙소와서 놀고",
          "original_form": "맛집 돌아다니다 숙소와서 놀고",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041270.1.17",
          "form": "남은 시간은 호텔 주변 어슬렁 거릴듯ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "남은 시간은 호텔 주변 어슬렁 거릴듯ㅋㅋㅋㅋㅋㅋㅋ",
