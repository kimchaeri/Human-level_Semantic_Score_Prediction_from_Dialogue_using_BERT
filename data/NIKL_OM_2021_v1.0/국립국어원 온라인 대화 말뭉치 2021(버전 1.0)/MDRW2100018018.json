      "utterance": [
        {
          "id": "MDRW2100018018.1.1",
          "form": "요즘 농사는 잘 되어가시나요?",
          "original_form": "요즘 농사는 잘 되어가시나요?",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100018018.1.2",
          "form": "이제 여름이 끝나가서 농사도 끝물이지",
          "original_form": "이제 여름이 끝나가서 농사도 끝물이지",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100018018.1.3",
          "form": "깨만 털고 김장재료만 수확하면 끝이야",
          "original_form": "깨만 털고 김장재료만 수확하면 끝이야",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018018.1.4",
          "form": "가장 핵심이 남았잖아요 깨 베는게 가장 힘들텐데요",
          "original_form": "가장 핵심이 남았잖아요 깨 베는게 가장 힘들텐데요",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018018.1.5",
          "form": "그래도 직장에서 일하는것 보다는 덜 힘들지",
          "original_form": "그래도 직장에서 일하는것 보다는 덜 힘들지",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018018.1.6",
          "form": "이건 내가 좋아서 하는 일이니까",
          "original_form": "이건 내가 좋아서 하는 일이니까",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018018.1.7",
          "form": "좋아서 하는 일이라고 하기에는 너무 힘든일이라 생각해요",
          "original_form": "좋아서 하는 일이라고 하기에는 너무 힘든일이라 생각해요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018018.1.8",
          "form": "어느 직장에서 일해도 이것만큼 일힘든거 없는데",
          "original_form": "어느 직장에서 일해도 이것만큼 일힘든거 없는데",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018018.1.9",
          "form": "이거는 즐거워",
          "original_form": "이거는 즐거워",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018018.1.10",
          "form": "옥수수대도 자르려면 힘드실거 같은데요",
          "original_form": "옥수수대도 자르려면 힘드실거 같은데요",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018018.1.11",
          "form": "그게 좀 딱딱하긴 해",
          "original_form": "그게 좀 딱딱하긴 해",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018018.1.12",
          "form": "갯수도 많아서요 힘들거 같아요",
          "original_form": "갯수도 많아서요 힘들거 같아요",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018018.1.13",
          "form": "아무래도 조금 그렇지",
          "original_form": "아무래도 조금 그렇지",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018018.1.14",
          "form": "깨보다는 나을거야",
          "original_form": "깨보다는 나을거야",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018018.1.15",
          "form": "작년에 몇시간 걸렸죠?",
          "original_form": "작년에 몇시간 걸렸죠?",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018018.1.16",
          "form": "아마 베는데만 삼일 걸렸을거야",
          "original_form": "아마 베는데만 삼일 걸렸을거야",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100018018.1.17",
          "form": "역시 너무 힘든 일 같아요 취미생활이라기엔 노동이에요",
          "original_form": "역시 너무 힘든 일 같아요 취미생활이라기엔 노동이에요",
          "speaker_id": "2",
          "time": "20210826 22:17"
        }
      ]
