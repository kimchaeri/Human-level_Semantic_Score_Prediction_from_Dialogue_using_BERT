      "utterance": [
        {
          "id": "MDRW2100032859.1.1",
          "form": "나...",
          "original_form": "나...",
          "speaker_id": "1",
          "time": "20210906 02:21"
        },
        {
          "id": "MDRW2100032859.1.2",
          "form": "코트입고싶어....",
          "original_form": "코트입고싶어....",
          "speaker_id": "1",
          "time": "20210906 02:21"
        },
        {
          "id": "MDRW2100032859.1.3",
          "form": "입을 수 있을까",
          "original_form": "입을 수 있을까",
          "speaker_id": "1",
          "time": "20210906 02:21"
        },
        {
          "id": "MDRW2100032859.1.4",
          "form": "ㅇㅇ 입으면되지.",
          "original_form": "ㅇㅇ 입으면되지.",
          "speaker_id": "2",
          "time": "20210906 02:22"
        },
        {
          "id": "MDRW2100032859.1.5",
          "form": "지금 아침이랑 낮에 꽤 쌀쌀하던데 뭐",
          "original_form": "지금 아침이랑 낮에 꽤 쌀쌀하던데 뭐",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100032859.1.6",
          "form": "아까 6시쯤 잠깐 마트다녀오는데 바람불어서 그런지 이제 반팔은 못입겠더라고",
          "original_form": "아까 6시쯤 잠깐 마트다녀오는데 바람불어서 그런지 이제 반팔은 못입겠더라고",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100032859.1.7",
          "form": "아 구래?",
          "original_form": "아 구래?",
          "speaker_id": "1",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032859.1.8",
          "form": "긴팔은 좀 더울 것같고 반팔에 코트 어때",
          "original_form": "긴팔은 좀 더울 것같고 반팔에 코트 어때",
          "speaker_id": "1",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032859.1.9",
          "form": "ㅋㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032859.1.10",
          "form": "반바지 입으면 패고같겠지ㅠ",
          "original_form": "반바지 입으면 패고같겠지ㅠ",
          "speaker_id": "1",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032859.1.11",
          "form": "아직 반바지 입는 사람 있으니까 이쁜거 있으면 괜찮지 않을까??",
          "original_form": "아직 반바지 입는 사람 있으니까 이쁜거 있으면 괜찮지 않을까??",
          "speaker_id": "2",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032859.1.12",
          "form": "그래도 아직 낮에는 해도 찌고 더우니까 괜찮을 것 같은데",
          "original_form": "그래도 아직 낮에는 해도 찌고 더우니까 괜찮을 것 같은데",
          "speaker_id": "2",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032859.1.13",
          "form": "헤헤 믿고 한번 도전해볼게",
          "original_form": "헤헤 믿고 한번 도전해볼게",
          "speaker_id": "1",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032859.1.14",
          "form": "출근할 때 입어야겠다",
          "original_form": "출근할 때 입어야겠다",
          "speaker_id": "1",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032859.1.15",
          "form": "ㅇㅇ. 지금 구글에도 검색해보니까 이상한 것도 있지만 괜찮은 것도 있어.",
          "original_form": "ㅇㅇ. 지금 구글에도 검색해보니까 이상한 것도 있지만 괜찮은 것도 있어.",
          "speaker_id": "2",
          "time": "20210906 02:26"
        },
        {
          "id": "MDRW2100032859.1.16",
          "form": "추운 날을 위해 6달을 기다렸다....",
          "original_form": "추운 날을 위해 6달을 기다렸다....",
          "speaker_id": "1",
          "time": "20210906 02:26"
        },
        {
          "id": "MDRW2100032859.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ이상한 축이면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이상한 축이면",
          "speaker_id": "1",
          "time": "20210906 02:26"
        },
        {
          "id": "MDRW2100032859.1.18",
          "form": "나 부끄러워서 어떡해",
          "original_form": "나 부끄러워서 어떡해",
          "speaker_id": "1",
          "time": "20210906 02:26"
        },
        {
          "id": "MDRW2100032859.1.19",
          "form": "걍 반팔 긴바지 입으까",
          "original_form": "걍 반팔 긴바지 입으까",
          "speaker_id": "1",
          "time": "20210906 02:27"
        },
        {
          "id": "MDRW2100032859.1.20",
          "form": "괜찮을거야 내가본건 잠옷처럼 보이는 거에 입은거라섴ㅋㅋㅋㅋ",
          "original_form": "괜찮을거야 내가본건 잠옷처럼 보이는 거에 입은거라섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 02:27"
        },
        {
          "id": "MDRW2100032859.1.21",
          "form": "근데 추운날씨 기다린거면 코트 되게 좋아하나보다",
          "original_form": "근데 추운날씨 기다린거면 코트 되게 좋아하나보다",
          "speaker_id": "2",
          "time": "20210906 02:27"
        },
        {
          "id": "MDRW2100032859.1.22",
          "form": "그럼 계절 중에서는 뭐가 좋아? 가을? 겨울? 봄?",
          "original_form": "그럼 계절 중에서는 뭐가 좋아? 가을? 겨울? 봄?",
          "speaker_id": "2",
          "time": "20210906 02:29"
        },
        {
          "id": "MDRW2100032859.1.23",
          "form": "아니 잠옷에 코트ㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋ",
          "original_form": "아니 잠옷에 코트ㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 02:29"
        },
        {
          "id": "MDRW2100032859.1.24",
          "form": "차라리 추리닝을 걸치지",
          "original_form": "차라리 추리닝을 걸치지",
          "speaker_id": "1",
          "time": "20210906 02:29"
        },
        {
          "id": "MDRW2100032859.1.25",
          "form": "코트가 봄 다 지나서 배송와갖구ㅠㅋㅋㅋ 이제야 꺼낸다",
          "original_form": "코트가 봄 다 지나서 배송와갖구ㅠㅋㅋㅋ 이제야 꺼낸다",
          "speaker_id": "1",
          "time": "20210906 02:29"
        },
        {
          "id": "MDRW2100032859.1.26",
          "form": "난 가을 ㅎ 내 생일 있어서",
          "original_form": "난 가을 ㅎ 내 생일 있어서",
          "speaker_id": "1",
          "time": "20210906 02:29"
        },
        {
          "id": "MDRW2100032859.1.27",
          "form": "가을생이면 주변에서 좋다는 소리 많이 들었겠네",
          "original_form": "가을생이면 주변에서 좋다는 소리 많이 들었겠네",
          "speaker_id": "2",
          "time": "20210906 02:31"
        },
        {
          "id": "MDRW2100032859.1.28",
          "form": "오잉 왜?",
          "original_form": "오잉 왜?",
          "speaker_id": "1",
          "time": "20210906 02:31"
        },
        {
          "id": "MDRW2100032859.1.29",
          "form": "생일이 가을이면 좋은가..?",
          "original_form": "생일이 가을이면 좋은가..?",
          "speaker_id": "1",
          "time": "20210906 02:32"
        },
        {
          "id": "MDRW2100032859.1.30",
          "form": "나도 잘 모르겠는데 날도 선선하고 지금이야 이렇지만 덥지도 춥지도 않아서 그런거 같애",
          "original_form": "나도 잘 모르겠는데 날도 선선하고 지금이야 이렇지만 덥지도 춥지도 않아서 그런거 같애",
          "speaker_id": "2",
          "time": "20210906 02:33"
        },
        {
          "id": "MDRW2100032859.1.31",
          "form": "그리고 한국사람들 그 사주팔자때문에도 있는거 같고",
          "original_form": "그리고 한국사람들 그 사주팔자때문에도 있는거 같고",
          "speaker_id": "2",
          "time": "20210906 02:33"
        },
        {
          "id": "MDRW2100032859.1.32",
          "form": "어 맞다 나 사주 좋댔어ㅋㅋㅋㅋ 이게 관련이 있구나...신기하다",
          "original_form": "어 맞다 나 사주 좋댔어ㅋㅋㅋㅋ 이게 관련이 있구나...신기하다",
          "speaker_id": "1",
          "time": "20210906 02:34"
        },
        {
          "id": "MDRW2100032859.1.33",
          "form": "ㅎㅎ 마쟈 생일파티할 때 어디 놀러가기 좋긴 하쥐",
          "original_form": "ㅎㅎ 마쟈 생일파티할 때 어디 놀러가기 좋긴 하쥐",
          "speaker_id": "1",
          "time": "20210906 02:34"
        }
      ]
    }
