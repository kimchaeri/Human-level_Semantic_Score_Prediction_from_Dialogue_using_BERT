        },
        {
          "id": "MDRW2100014298.1.2",
          "form": "계속 요거트 딸기만 먹을 거야",
          "original_form": "계속 요거트 딸기만 먹을 거야",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014298.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014298.1.4",
          "form": "아니 그건 아닌데",
          "original_form": "아니 그건 아닌데",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014298.1.5",
          "form": "그럼 다른 음료를 먹게 된다면 뭐 먹을 거야",
          "original_form": "그럼 다른 음료를 먹게 된다면 뭐 먹을 거야",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014298.1.6",
          "form": "키위?",
          "original_form": "키위?",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014298.1.7",
          "form": "아 맞다 키위 좋아하지",
          "original_form": "아 맞다 키위 좋아하지",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014298.1.8",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014298.1.9",
          "form": "뭘 좋아해?",
          "original_form": "뭘 좋아해?",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014298.1.10",
          "form": "나는 수박 복숭아 이런 거 좋아해",
          "original_form": "나는 수박 복숭아 이런 거 좋아해",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014298.1.11",
          "form": "복숭아도 맛있지",
          "original_form": "복숭아도 맛있지",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014298.1.12",
          "form": "체리도 좋아하는데 저번에 체리 주스는 너무 맛없었어",
          "original_form": "체리도 좋아하는데 저번에 체리 주스는 너무 맛없었어",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014298.1.13",
          "form": "다 잘먹는 사람도 맛 없다는건 ㅋㅋ",
          "original_form": "다 잘먹는 사람도 맛 없다는건 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014298.1.14",
          "form": "진짜 별로라는 거지",
          "original_form": "진짜 별로라는 거지",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014298.1.15",
          "form": "ㅋㅋ 그럴 수 있지",
          "original_form": "ㅋㅋ 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014298.1.16",
          "form": "미숫가루는 별로 안 좋아해?",
          "original_form": "미숫가루는 별로 안 좋아해?",
