      "utterance": [
        {
          "id": "MDRW2100023671.1.1",
          "form": "이게 디폴트가 된 상황이니까",
          "original_form": "이게 디폴트가 된 상황이니까",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.2",
          "form": "오히려비혼권장프로그램이라니까",
          "original_form": "오히려비혼권장프로그램이라니까",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.4",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.5",
          "form": "원래시대가",
          "original_form": "원래시대가",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.6",
          "form": "흐를숟록",
          "original_form": "흐를숟록",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.7",
          "form": "반반육아이런게",
          "original_form": "반반육아이런게",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.8",
          "form": "정착되ㅑ야하는데",
          "original_form": "정착되ㅑ야하는데",
          "speaker_id": "1",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100023671.1.10",
          "form": "오히러ㅡㅕ퇴화함",
          "original_form": "오히러ㅡㅕ퇴화함",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023671.1.11",
          "form": "여자가 출산했으면",
          "original_form": "여자가 출산했으면",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023671.1.12",
          "form": "그 이후로는 남성이 보는 게 당연한거 아니냐",
          "original_form": "그 이후로는 남성이 보는 게 당연한거 아니냐",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023671.1.13",
          "form": "그러니가",
          "original_form": "그러니가",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023671.1.14",
          "form": "그거뭐지",
          "original_form": "그거뭐지",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023671.1.15",
          "form": "근데 슈퍼맨이 돌아왔다 보면 지금에서야 하니 힘들다 뻘뻘이라고 있고",
          "original_form": "근데 슈퍼맨이 돌아왔다 보면 지금에서야 하니 힘들다 뻘뻘이라고 있고",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.16",
          "form": "요즘에는 또 같이 나오더라 부부가",
          "original_form": "요즘에는 또 같이 나오더라 부부가",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.17",
          "form": "뭐여 그게",
          "original_form": "뭐여 그게",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.18",
          "form": "너무싫어",
          "original_form": "너무싫어",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.19",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.20",
          "form": "그거독립어쩌고예능인데",
          "original_form": "그거독립어쩌고예능인데",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.21",
          "form": "여자연예인들은요리하고",
          "original_form": "여자연예인들은요리하고",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.22",
          "form": "아 독립타운",
          "original_form": "아 독립타운",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.23",
          "form": "알아서",
          "original_form": "알아서",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.24",
          "form": "재밌게서는데",
          "original_form": "재밌게서는데",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.25",
          "form": "기혼남자들은",
          "original_form": "기혼남자들은",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.26",
          "form": "아무것도혼자할줄모르잖아",
          "original_form": "아무것도혼자할줄모르잖아",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.27",
          "form": "ㅇㅈ 얼마나 집안 일을 안했으면 진짜",
          "original_form": "ㅇㅈ 얼마나 집안 일을 안했으면 진짜",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.28",
          "form": "그리고요리조금이라도하면",
          "original_form": "그리고요리조금이라도하면",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.29",
          "form": "와대단하다고",
          "original_form": "와대단하다고",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.30",
          "form": "밥솥 여는 법도 모름",
          "original_form": "밥솥 여는 법도 모름",
          "speaker_id": "2",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.31",
          "form": "칭찬해주고",
          "original_form": "칭찬해주고",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.32",
          "form": "그니가",
          "original_form": "그니가",
          "speaker_id": "1",
          "time": "20210830 14:17"
        },
        {
          "id": "MDRW2100023671.1.33",
          "form": "진짜 어휴",
          "original_form": "진짜 어휴",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.34",
          "form": "요즘초딩도안그런다고",
          "original_form": "요즘초딩도안그런다고",
          "speaker_id": "1",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.35",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.36",
          "form": "이런예능들이",
          "original_form": "이런예능들이",
          "speaker_id": "1",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.37",
          "form": "오히려",
          "original_form": "오히려",
          "speaker_id": "1",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.38",
          "form": "더",
          "original_form": "더",
          "speaker_id": "1",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.39",
          "form": "당연하게생각하게만단다니까",
          "original_form": "당연하게생각하게만단다니까",
          "speaker_id": "1",
          "time": "20210830 14:18"
        },
        {
          "id": "MDRW2100023671.1.40",
          "form": "남자가집안일안하는걸",
          "original_form": "남자가집안일안하는걸",
          "speaker_id": "1",
