      "utterance": [
        {
          "id": "MDRW2100027456.1.1",
          "form": "점심드셨어유~?",
          "original_form": "점심드셨어유~?",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027456.1.2",
          "form": "아뇽ㅠ name3이 델고\r\n어디가야해서 못먹었음",
          "original_form": "아뇽ㅠ &name3&이 델고\r\n어디가야해서 못먹었음",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027456.1.3",
          "form": "이제 퇴근하고 가면 먹을거에용?",
          "original_form": "이제 퇴근하고 가면 먹을거에용?",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027456.1.4",
          "form": "홀 어디가는뎅 ㅜㅜ",
          "original_form": "홀 어디가는뎅 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027456.1.5",
          "form": "퇴근하곶",
          "original_form": "퇴근하곶",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027456.1.6",
          "form": "장좀봐야해서 ㅠㅠ",
          "original_form": "장좀봐야해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027456.1.7",
          "form": "장보고",
          "original_form": "장보고",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027456.1.8",
          "form": "밥먹어야할듯 ㅜㅠ",
          "original_form": "밥먹어야할듯 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027456.1.9",
          "form": "ㅋㅋ장 뭐 보려고?",
          "original_form": "ㅋㅋ장 뭐 보려고?",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027456.1.10",
          "form": "삼겹살이 먹고싶어쟜음",
          "original_form": "삼겹살이 먹고싶어쟜음",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027456.1.11",
          "form": "ㅎ.ㅎ",
          "original_form": "ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027456.1.12",
          "form": "오 맛있겠당 ㅜㅜ",
          "original_form": "오 맛있겠당 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027456.1.13",
          "form": "나는 오늘 저녁 피자임 ㅋㅋ",
          "original_form": "나는 오늘 저녁 피자임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027456.1.14",
          "form": "어디피자얌??",
          "original_form": "어디피자얌??",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027456.1.15",
          "form": "나도 피자 어랜만에 먹고싶넹 ㅠㅜ",
          "original_form": "나도 피자 어랜만에 먹고싶넹 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027456.1.16",
          "form": "알볼로 먹습니다 ㅋㅋ",
          "original_form": "알볼로 먹습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027456.1.17",
          "form": "알볼로",
          "original_form": "알볼로",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027456.1.18",
          "form": "거기 그...",
          "original_form": "거기 그...",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027456.1.19",
          "form": "뭐였제",
          "original_form": "뭐였제",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027456.1.20",
          "form": "한가지종류밖에안먹어봄",
          "original_form": "한가지종류밖에안먹어봄",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027456.1.21",
          "form": "뭐염? ㅋㅋ",
          "original_form": "뭐염? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027456.1.22",
          "form": "여기 피자이름 넘 웃김",
          "original_form": "여기 피자이름 넘 웃김",
          "speaker_id": "1",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027456.1.23",
          "form": "고르곤??ㅋㅋㅋ",
          "original_form": "고르곤??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100027456.1.24",
          "form": "피자이름?",
          "original_form": "피자이름?",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100027456.1.25",
          "form": "네? 알볼로에\r\n그런게 있던가요?ㅋㅋ",
          "original_form": "네? 알볼로에\r\n그런게 있던가요?ㅋㅋ",
