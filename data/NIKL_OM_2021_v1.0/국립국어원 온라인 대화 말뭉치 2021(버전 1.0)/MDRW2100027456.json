          "original_form": "날개피자",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100027456.1.34",
          "form": "이런식인데ㅋㅋㅋㅋㅋ",
          "original_form": "이런식인데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100027456.1.35",
          "form": "엥????ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엥????ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100027456.1.36",
          "form": "내가생각한곳은",
          "original_form": "내가생각한곳은",
          "speaker_id": "2",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100027456.1.37",
          "form": "다른곳이았나..?",
          "original_form": "다른곳이았나..?",
          "speaker_id": "2",
          "time": "20210902 20:32"
        },
        {
