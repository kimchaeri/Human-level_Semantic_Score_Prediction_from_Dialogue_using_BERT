      "utterance": [
        {
          "id": "MDRW2100033597.1.1",
          "form": "군것질 좋아하시나요",
          "original_form": "군것질 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 16:37"
        },
        {
          "id": "MDRW2100033597.1.2",
          "form": "네 좋아해요~",
          "original_form": "네 좋아해요~",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100033597.1.3",
          "form": "달달한 것보다는 담백한 걸로~",
          "original_form": "달달한 것보다는 담백한 걸로~",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100033597.1.4",
          "form": "군것질 자주 하시나요??",
          "original_form": "군것질 자주 하시나요??",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100033597.1.5",
          "form": "네 저는 자주하는 편이에요",
          "original_form": "네 저는 자주하는 편이에요",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100033597.1.6",
          "form": "어떤거 자주드시나요",
          "original_form": "어떤거 자주드시나요",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100033597.1.7",
          "form": "저는 견과류나 건과일 같은 거 먹어요~~",
          "original_form": "저는 견과류나 건과일 같은 거 먹어요~~",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033597.1.8",
          "form": "너무 좋네요",
          "original_form": "너무 좋네요",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033597.1.9",
          "form": "가끔 초콜릿만 왕창 먹을 때가 있긴 하지만..ㅎㅎ",
          "original_form": "가끔 초콜릿만 왕창 먹을 때가 있긴 하지만..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033597.1.10",
          "form": "건과일은 어떤거드시나요",
          "original_form": "건과일은 어떤거드시나요",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033597.1.11",
          "form": "바나나를 제일 좋아하고~",
          "original_form": "바나나를 제일 좋아하고~",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033597.1.12",
          "form": "바나나칩 맛있죠",
          "original_form": "바나나칩 맛있죠",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.13",
          "form": "여유있는 날에는 좀 더 비싼 것도 사먹곤 해요~ㅎ",
          "original_form": "여유있는 날에는 좀 더 비싼 것도 사먹곤 해요~ㅎ",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.14",
          "form": "건망고좋아하시나요",
          "original_form": "건망고좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.15",
          "form": "예전에는 좋아했었는데",
          "original_form": "예전에는 좋아했었는데",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.16",
          "form": "망고알러지 생긴후로느뉴ㅠ",
          "original_form": "망고알러지 생긴후로느뉴ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.17",
          "form": "못 먹고 있어요..ㅠ",
          "original_form": "못 먹고 있어요..ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.18",
          "form": "아.. ㅠㅠ",
          "original_form": "아.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.19",
          "form": "어떤 거 즐겨 드세요??",
          "original_form": "어떤 거 즐겨 드세요??",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.20",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.21",
          "form": "저는 약간 불량식품 좋아해요",
          "original_form": "저는 약간 불량식품 좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033597.1.22",
