      "utterance": [
        {
          "id": "MDRW2100026613.1.1",
          "form": "안녕하세요~ 스포츠 뭐 보세요?",
          "original_form": "안녕하세요~ 스포츠 뭐 보세요?",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026613.1.2",
          "form": "안녕하세요!ㅎㅎ",
          "original_form": "안녕하세요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026613.1.3",
          "form": "저는 보는건 다 좋아해요!!",
          "original_form": "저는 보는건 다 좋아해요!!",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026613.1.4",
          "form": "특히 좋아하는건 농구랑 야구 조아해요!",
          "original_form": "특히 좋아하는건 농구랑 야구 조아해요!",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026613.1.5",
          "form": "오 저는 야구는 직관도 해봤어요!",
          "original_form": "오 저는 야구는 직관도 해봤어요!",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026613.1.6",
          "form": "농구! 슬램덩크만 알아요 ㅋㅋㅋㅋ",
          "original_form": "농구! 슬램덩크만 알아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026613.1.7",
          "form": "ㅋㅋㅋㅋ슬램덩크 재미있죠",
          "original_form": "ㅋㅋㅋㅋ슬램덩크 재미있죠",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026613.1.8",
          "form": "야구 어디서 직관하셨나요?",
          "original_form": "야구 어디서 직관하셨나요?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026613.1.9",
          "form": "저 여기저기 인천도 가고 서울도 가고 대전도 가고 ㅋㅋㅋ",
          "original_form": "저 여기저기 인천도 가고 서울도 가고 대전도 가고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026613.1.10",
          "form": "아 수원도 갔었나? 그래요",
          "original_form": "아 수원도 갔었나? 그래요",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026613.1.11",
          "form": "오 진짜 여기저기 가셨네요!",
          "original_form": "오 진짜 여기저기 가셨네요!",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026613.1.12",
          "form": "저는 광주에서만 엄청 많이봤어요!!ㅋㅋㅋ",
          "original_form": "저는 광주에서만 엄청 많이봤어요!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026613.1.13",
          "form": "혹시 어디팬이세요?",
          "original_form": "혹시 어디팬이세요?",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026613.1.14",
          "form": "저 한ㅋㅋ화ㅋㅋㅋ 요ㅋㅋㅋㅋ",
          "original_form": "저 한ㅋㅋ화ㅋㅋㅋ 요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.15",
          "form": "사실 요즘 안봐요",
          "original_form": "사실 요즘 안봐요",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ그러시군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러시군요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.18",
          "form": "고해성사합니다ㅋㅋㅋㅋㅋ",
          "original_form": "고해성사합니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.19",
          "form": "저는 기아팬이에요ㅋㅋㅋ",
          "original_form": "저는 기아팬이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.20",
          "form": "기아팬이세요?",
          "original_form": "기아팬이세요?",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.21",
          "form": "역시 ㅋㅋㅋㅋ",
          "original_form": "역시 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026613.1.22",
