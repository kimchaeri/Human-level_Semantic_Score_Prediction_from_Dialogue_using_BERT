      "utterance": [
        {
          "id": "MDRW2100034632.1.1",
          "form": "와 신기해요",
          "original_form": "와 신기해요",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034632.1.2",
          "form": "그럴수도 있겠어요",
          "original_form": "그럴수도 있겠어요",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034632.1.3",
          "form": "네 통닭을 구울때 간지나게",
          "original_form": "네 통닭을 구울때 간지나게",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034632.1.4",
          "form": "ㅋㅋ 네 포옥 찌르기",
          "original_form": "ㅋㅋ 네 포옥 찌르기",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034632.1.5",
          "form": "온도계를 찔러넣고",
          "original_form": "온도계를 찔러넣고",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034632.1.6",
          "form": "오븐 장갑기고 꺼내고 이런게 뭐너가 전문가 같던ㅋㅋㅋ",
          "original_form": "오븐 장갑기고 꺼내고 이런게 뭐너가 전문가 같던ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034632.1.7",
          "form": "오 고기 오븐에 구워먹기",
          "original_form": "오 고기 오븐에 구워먹기",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034632.1.8",
          "form": "본새나요ㅋㅋ",
          "original_form": "본새나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034632.1.9",
          "form": "좀 간지나고 서양인 같죠ㅋㅋㅋㅋ 미국 영화에 낭로것 같구",
          "original_form": "좀 간지나고 서양인 같죠ㅋㅋㅋㅋ 미국 영화에 낭로것 같구",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034632.1.10",
          "form": "그러나 전 집안 공간상 에프로 ,,,,",
          "original_form": "그러나 전 집안 공간상 에프로 ,,,,",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034632.1.11",
          "form": "ㅋㅋ요즘은 에프가 생활필수품!!이죠",
          "original_form": "ㅋㅋ요즘은 에프가 생활필수품!!이죠",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034632.1.12",
          "form": "에프가 오븐 대체 품으로 보급될 줄은 몰랐어요 ㅋㅋ",
          "original_form": "에프가 오븐 대체 품으로 보급될 줄은 몰랐어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034632.1.13",
          "form": "저는 오히려 에프가 없어요!",
          "original_form": "저는 오히려 에프가 없어요!",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034632.1.14",
          "form": "미니오븐도 있고 하심 필요없죠",
          "original_form": "미니오븐도 있고 하심 필요없죠",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100034632.1.15",
          "form": "에프로 튀김도 데워봤는데 미니오븐에서 데우는거랑 걍 비슷했어요",
          "original_form": "에프로 튀김도 데워봤는데 미니오븐에서 데우는거랑 걍 비슷했어요",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100034632.1.16",
          "form": "ㅎㅎ 그래서 아직은 안사고있어요 근데 손이 드릉드릉합니다",
          "original_form": "ㅎㅎ 그래서 아직은 안사고있어요 근데 손이 드릉드릉합니다",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034632.1.17",
          "form": "에프가 더 접근성?이 좋아보여서요",
          "original_form": "에프가 더 접근성?이 좋아보여서요",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100034632.1.18",
          "form": "집에 미니오븐이 있음 좀 애매하죠 그리고 에프가 바닥 면적은 좁아도 위로는 좀 커서",
          "original_form": "집에 미니오븐이 있음 좀 애매하죠 그리고 에프가 바닥 면적은 좁아도 위로는 좀 커서",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100034632.1.19",
          "form": "대형 에프는 미니오븐보다 좀 더 자리를 차지할수도 있어요",
          "original_form": "대형 에프는 미니오븐보다 좀 더 자리를 차지할수도 있어요",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100034632.1.20",
          "form": "아 위로 꽤크군요",
          "original_form": "아 위로 꽤크군요",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100034632.1.21",
          "form": "그리고 보통 작은건 사면 후회하니까 무조건 큰거 사라고 하더라고요",
          "original_form": "그리고 보통 작은건 사면 후회하니까 무조건 큰거 사라고 하더라고요",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100034632.1.22",
          "form": "큰걸 사야 닭도 구워먹고 그러긴 하는데 생활 규모따라 다른 것 같아요",
          "original_form": "큰걸 사야 닭도 구워먹고 그러긴 하는데 생활 규모따라 다른 것 같아요",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034632.1.23",
          "form": "혼자 자취하면 보통크기도 좋고,,,",
          "original_form": "혼자 자취하면 보통크기도 좋고,,,",
