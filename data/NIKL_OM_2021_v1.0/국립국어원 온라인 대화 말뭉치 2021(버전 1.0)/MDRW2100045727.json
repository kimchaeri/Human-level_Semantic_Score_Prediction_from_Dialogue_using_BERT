      "utterance": [
        {
          "id": "MDRW2100045727.1.1",
          "form": "저는 자취생인데",
          "original_form": "저는 자취생인데",
          "speaker_id": "2",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045727.1.2",
          "form": "초반에는 열심히 만들어먹다가",
          "original_form": "초반에는 열심히 만들어먹다가",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045727.1.3",
          "form": "요새는 방탕하게 살고있습니다",
          "original_form": "요새는 방탕하게 살고있습니다",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045727.1.4",
          "form": "요리 잘하시나요?",
          "original_form": "요리 잘하시나요?",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045727.1.5",
          "form": "아뇨 완전 허접이지만",
          "original_form": "아뇨 완전 허접이지만",
          "speaker_id": "1",
          "time": "20210908 06:12"
        },
        {
          "id": "MDRW2100045727.1.6",
          "form": "파는 음식이 다 너무 짜서요ㅠㅠ",
          "original_form": "파는 음식이 다 너무 짜서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:12"
        },
        {
          "id": "MDRW2100045727.1.7",
          "form": "맞아요 파는음식은 너무 간이세죠",
          "original_form": "맞아요 파는음식은 너무 간이세죠",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045727.1.8",
          "form": "저도 짠거 안좋아해서",
          "original_form": "저도 짠거 안좋아해서",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045727.1.9",
          "form": "제일 잘하는게 뭐세요?",
          "original_form": "제일 잘하는게 뭐세요?",
          "speaker_id": "2",
          "time": "20210908 06:13"
        },
        {
          "id": "MDRW2100045727.1.10",
          "form": "저는...간단한 거요ㅋㅋㅋ",
          "original_form": "저는...간단한 거요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045727.1.11",
          "form": "도토리묵 같이 걍 섞기만하는거...",
          "original_form": "도토리묵 같이 걍 섞기만하는거...",
          "speaker_id": "1",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045727.1.12",
          "form": "도토리묵을 만들어드세요?",
          "original_form": "도토리묵을 만들어드세요?",
          "speaker_id": "2",
          "time": "20210908 06:14"
        },
        {
          "id": "MDRW2100045727.1.13",
          "form": "네 마트에서 사와서",
          "original_form": "네 마트에서 사와서",
          "speaker_id": "1",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100045727.1.14",
          "form": "김이랑 간장이랑해서 버무리면 돼요",
          "original_form": "김이랑 간장이랑해서 버무리면 돼요",
          "speaker_id": "1",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045727.1.15",
          "form": "아 저는 도토리묵 자체를 쑨다는지 알았네요 ㅋㅋ",
          "original_form": "아 저는 도토리묵 자체를 쑨다는지 알았네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045727.1.16",
          "form": "도토리묵 얘기하니까 묵밥먹고싶네요",
          "original_form": "도토리묵 얘기하니까 묵밥먹고싶네요",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045727.1.17",
          "form": "와 그거 맛있죠ㅠㅠㅠ",
          "original_form": "와 그거 맛있죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045727.1.18",
          "form": "청포묵도 먹고싶네요",
          "original_form": "청포묵도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045727.1.19",
          "form": "청포묵은 뭘로만드는건가요? 갑자기 궁금하네요",
          "original_form": "청포묵은 뭘로만드는건가요? 갑자기 궁금하네요",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045727.1.20",
          "form": "청포로요! 우뭇가사리인가?",
          "original_form": "청포로요! 우뭇가사리인가?",
          "speaker_id": "1",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045727.1.21",
          "form": "아 그렇군요 이름에 재료가...ㅎㅎ",
          "original_form": "아 그렇군요 이름에 재료가...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045727.1.22",
          "form": "저는 도토리묵이 씁쓸헤서 더 맛있는것같아요",
          "original_form": "저는 도토리묵이 씁쓸헤서 더 맛있는것같아요",
          "speaker_id": "2",
          "time": "20210908 06:23"
        },
        {
          "id": "MDRW2100045727.1.23",
          "form": "맞아요ㅋㅋ씁쓸한 맛이 있죠",
          "original_form": "맞아요ㅋㅋ씁쓸한 맛이 있죠",
          "speaker_id": "1",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045727.1.24",
          "form": "오늘 점심은 뭐드실\r\n 예정이신가요?",
          "original_form": "오늘 점심은 뭐드실\r\n 예정이신가요?",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045727.1.25",
          "form": "오늘은 돈까스요!",
          "original_form": "오늘은 돈까스요!",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045727.1.26",
          "form": "비오니까 튀김이 땡기네요",
          "original_form": "비오니까 튀김이 땡기네요",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045727.1.27",
          "form": "돈까스 맛있겠네오",
          "original_form": "돈까스 맛있겠네오",
          "speaker_id": "2",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100045727.1.28",
          "form": "저는 낙지볶음 먹으려구요",
          "original_form": "저는 낙지볶음 먹으려구요",
          "speaker_id": "2",
          "time": "20210908 06:28"
        }
      ]
    }
  ]
