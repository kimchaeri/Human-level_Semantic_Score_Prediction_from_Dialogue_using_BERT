        {
          "id": "MDRW2100020033.1.1",
          "form": "봤지 본 방으로 봤어",
          "original_form": "봤지 본 방으로 봤어",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100020033.1.2",
          "form": "정답 맞췄어?",
          "original_form": "정답 맞췄어?",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020033.1.3",
          "form": "맞추긴 했는데 오늘 꺼 좀 어렵지 않았어",
          "original_form": "맞추긴 했는데 오늘 꺼 좀 어렵지 않았어",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020033.1.4",
          "form": "난 좀 어려워서 틀릴뻔함",
          "original_form": "난 좀 어려워서 틀릴뻔함",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020033.1.5",
          "form": "일번이랑 이번이 헷갈리더라",
          "original_form": "일번이랑 이번이 헷갈리더라",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020033.1.6",
          "form": "오늘 게스트 귀엽더라",
          "original_form": "오늘 게스트 귀엽더라",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020033.1.7",
          "form": "이름이 뭐더라",
          "original_form": "이름이 뭐더라",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020033.1.8",
          "form": "채종협 이라는 배우 래",
          "original_form": "채종협 이라는 배우 래",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020033.1.9",
          "form": "웃는 게 귀엽던데",
          "original_form": "웃는 게 귀엽던데",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020033.1.10",
          "form": "요즘 인기 많은 배우상인 듯",
          "original_form": "요즘 인기 많은 배우상인 듯",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020033.1.11",
          "form": "맞아 키도 무척 크더라",
          "original_form": "맞아 키도 무척 크더라",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020033.1.12",
          "form": "다음 주 거워도 재밌을 듯",
          "original_form": "다음 주 거워도 재밌을 듯",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020033.1.13",
          "form": "다음 주 꺼",
          "original_form": "다음 주 꺼",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020033.1.14",
          "form": "맞아 나도 기대돼",
          "original_form": "맞아 나도 기대돼",
          "speaker_id": "2",
          "time": "20210827 23:25"
