      "utterance": [
        {
          "id": "MDRW2100025004.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100025004.1.2",
          "form": "안녕하세요 동물키우시나요?",
          "original_form": "안녕하세요 동물키우시나요?",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100025004.1.3",
          "form": "네 저는 고양이 두마리 키워요",
          "original_form": "네 저는 고양이 두마리 키워요",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100025004.1.4",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100025004.1.5",
          "form": "아 진짜요? 전 아니요ㅜ",
          "original_form": "아 진짜요? 전 아니요ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100025004.1.6",
          "form": "자랑좀해주세요",
          "original_form": "자랑좀해주세요",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100025004.1.7",
          "form": "암컷인가요??",
          "original_form": "암컷인가요??",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100025004.1.8",
          "form": "앗ㅎㅎ 네 둘다 암컷이예요",
          "original_form": "앗ㅎㅎ 네 둘다 암컷이예요",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100025004.1.9",
          "form": "첫째는 보호소 데려왓고 둘째는 미용샵에서 데려왔는데",
          "original_form": "첫째는 보호소 데려왓고 둘째는 미용샵에서 데려왔는데",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100025004.1.10",
          "form": "성격이 전혀 달라서 심심하지 않아요 ㅎㅎ",
          "original_form": "성격이 전혀 달라서 심심하지 않아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100025004.1.11",
          "form": "동물 키우실 생각은 없으신가요?",
          "original_form": "동물 키우실 생각은 없으신가요?",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100025004.1.12",
          "form": "예전에 어릴땐 멍멍이 키웠었는데 끝까지 책임지지못했어요ㅜ 반성하는 마음으로 랜선으로 만족해야죠",
          "original_form": "예전에 어릴땐 멍멍이 키웠었는데 끝까지 책임지지못했어요ㅜ 반성하는 마음으로 랜선으로 만족해야죠",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025004.1.13",
          "form": "고양이 너무 이뻐요 동네에도 길냥이들 많거든요",
          "original_form": "고양이 너무 이뻐요 동네에도 길냥이들 많거든요",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025004.1.14",
          "form": "맞아요 요즘 길냥이들 많죠!",
          "original_form": "맞아요 요즘 길냥이들 많죠!",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025004.1.15",
          "form": "길냥이들도 애교 많더라구요",
          "original_form": "길냥이들도 애교 많더라구요",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025004.1.16",
          "form": "네 바닥구르면서 꼬리로 다리 스칠때 기분 좋아요!",
          "original_form": "네 바닥구르면서 꼬리로 다리 스칠때 기분 좋아요!",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025004.1.17",
          "form": "둘이 싸우진 않아요?",
          "original_form": "둘이 싸우진 않아요?",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025004.1.18",
          "form": "처음엔 싸우고 그랬는데 요즘은 장난치고 잘 놀아요 ㅎㅎ",
          "original_form": "처음엔 싸우고 그랬는데 요즘은 장난치고 잘 놀아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025004.1.19",
          "form": "그땐 어려서 싸웠나봐요",
          "original_form": "그땐 어려서 싸웠나봐요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025004.1.20",
          "form": "그럼 나이가 어려요??",
          "original_form": "그럼 나이가 어려요??",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025004.1.21",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100025004.1.22",
          "form": "3살 2살이예요",
          "original_form": "3살 2살이예요",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100025004.1.23",
          "form": "이젠 애기가 아니네요 한참 이쁠 나이네요",
          "original_form": "이젠 애기가 아니네요 한참 이쁠 나이네요",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025004.1.24",
          "form": "네 ㅎㅎ 놀아주면 힐링받는 기분이예요!",
          "original_form": "네 ㅎㅎ 놀아주면 힐링받는 기분이예요!",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100025004.1.25",
          "form": "너무 이쁠거같아요",
          "original_form": "너무 이쁠거같아요",
          "speaker_id": "2",
          "time": "20210831 22:15"
        }
      ]
    }
  ]
