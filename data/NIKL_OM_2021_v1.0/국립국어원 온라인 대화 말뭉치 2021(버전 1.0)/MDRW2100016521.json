      "utterance": [
        {
          "id": "MDRW2100016521.1.1",
          "form": "배달음식은 뭐좋아하세요?",
          "original_form": "배달음식은 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016521.1.2",
          "form": "반올림피자에서 더블포크쉬림프콘치즈라고 있는데",
          "original_form": "반올림피자에서 더블포크쉬림프콘치즈라고 있는데",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016521.1.3",
          "form": "진짜 맛있어서 최애피자 등극했어요 ㅠ",
          "original_form": "진짜 맛있어서 최애피자 등극했어요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016521.1.4",
          "form": "헉 저도 반올림피자 좋아하는데",
          "original_form": "헉 저도 반올림피자 좋아하는데",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016521.1.5",
          "form": "어떤거 좋아하세요??",
          "original_form": "어떤거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016521.1.6",
          "form": "더블포크쉬림프콘치즈는 아직 안먹어봣네요",
          "original_form": "더블포크쉬림프콘치즈는 아직 안먹어봣네요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016521.1.7",
          "form": "그거 토마토소스대신 크림소스 들어가는 거 같던데 진짜 맛나요 토핑 종류도 많고",
          "original_form": "그거 토마토소스대신 크림소스 들어가는 거 같던데 진짜 맛나요 토핑 종류도 많고",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016521.1.8",
          "form": "저는 보통 베이컨체다치즈나 고구마 먹엇던것같아요",
          "original_form": "저는 보통 베이컨체다치즈나 고구마 먹엇던것같아요",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016521.1.9",
          "form": "크림소스...!!",
          "original_form": "크림소스...!!",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016521.1.10",
          "form": "듣기만해도 맛있을 각이 서네요",
          "original_form": "듣기만해도 맛있을 각이 서네요",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016521.1.11",
          "form": "끝에 콘치즈가 신의 한수입니다 ㅠㅠ 꼭 드셔보세요",
          "original_form": "끝에 콘치즈가 신의 한수입니다 ㅠㅠ 꼭 드셔보세요",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016521.1.12",
          "form": "추천감사해요 듣기만해도 입에 침고여요 ㅋㅋㅋㅋㅋ",
          "original_form": "추천감사해요 듣기만해도 입에 침고여요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016521.1.13",
          "form": "좋아하시는 배달음식점 있으세요??",
          "original_form": "좋아하시는 배달음식점 있으세요??",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016521.1.14",
          "form": "저도 피자 좋아해서",
          "original_form": "저도 피자 좋아해서",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016521.1.15",
          "form": "반올림도 먹고",
          "original_form": "반올림도 먹고",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016521.1.16",
          "form": "피자알볼로도 먹어요",
          "original_form": "피자알볼로도 먹어요",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016521.1.17",
          "form": "알볼로는 못 먹어봤어요",
          "original_form": "알볼로는 못 먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016521.1.18",
          "form": "뭐가 맛있나요??",
          "original_form": "뭐가 맛있나요??",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016521.1.19",
          "form": "전 단호박을 좋아해서",
          "original_form": "전 단호박을 좋아해서",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016521.1.20",
          "form": "피자알볼로 단호박골드 엄청 좋아해요",
          "original_form": "피자알볼로 단호박골드 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016521.1.21",
          "form": "단호박피자도 있군여 ㅋㅋㅋ",
          "original_form": "단호박피자도 있군여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:00"
        }
      ]
