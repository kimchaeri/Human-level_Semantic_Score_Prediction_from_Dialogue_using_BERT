      "utterance": [
        {
          "id": "MDRW2100046873.1.1",
          "form": "안녕하세요! 요샌 학교에서 뭐 배우나요??",
          "original_form": "안녕하세요! 요샌 학교에서 뭐 배우나요??",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100046873.1.2",
          "form": "요새도 똑같아요",
          "original_form": "요새도 똑같아요",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100046873.1.3",
          "form": "국수역과영에",
          "original_form": "국수역과영에",
          "speaker_id": "2",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100046873.1.4",
          "form": "기타과목들 배우고",
          "original_form": "기타과목들 배우고",
          "speaker_id": "2",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100046873.1.5",
          "form": "졸업한지 1년밖에 안됐는데",
          "original_form": "졸업한지 1년밖에 안됐는데",
          "speaker_id": "1",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100046873.1.6",
          "form": "오래된거같아용ㅎㅎㅎ",
          "original_form": "오래된거같아용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100046873.1.7",
          "form": "무슨 과목 좋아하세요?!",
          "original_form": "무슨 과목 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100046873.1.8",
          "form": "저는 체육 제일 좋아해요 !",
          "original_form": "저는 체육 제일 좋아해요 !",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100046873.1.9",
          "form": "친구들이랑 다같이 움직이면서",
          "original_form": "친구들이랑 다같이 움직이면서",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100046873.1.10",
          "form": "단합하면",
          "original_form": "단합하면",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100046873.1.11",
          "form": "그게 그렇게 좋더라구요",
          "original_form": "그게 그렇게 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100046873.1.12",
          "form": "헉 맞아요!! 저도 체육하면서 많이 친해졌어요!!",
          "original_form": "헉 맞아요!! 저도 체육하면서 많이 친해졌어요!!",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100046873.1.13",
          "form": "전 배드민턴 열심히 했었던 기억이 나네요ㅋㅋ",
          "original_form": "전 배드민턴 열심히 했었던 기억이 나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100046873.1.14",
          "form": "배드민턴 완전 좋죠!ㅋㅋㅋ",
          "original_form": "배드민턴 완전 좋죠!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100046873.1.15",
          "form": "저도 가끔 체육 짜투리 시간에",
          "original_form": "저도 가끔 체육 짜투리 시간에",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100046873.1.16",
          "form": "친구들이랑 배드민턴 하거든요",
          "original_form": "친구들이랑 배드민턴 하거든요",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100046873.1.17",
          "form": "ㅋㅋㅋ맞아요 시험 끝나고 나면",
          "original_form": "ㅋㅋㅋ맞아요 시험 끝나고 나면",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100046873.1.18",
          "form": "할 것도 없어서 맨날 배드민턴 했어요ㅋㅋㅋ",
          "original_form": "할 것도 없어서 맨날 배드민턴 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100046873.1.19",
          "form": "학교 다니실 때",
          "original_form": "학교 다니실 때",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100046873.1.20",
          "form": "동아리 활동 같은거 해보셨어요?",
          "original_form": "동아리 활동 같은거 해보셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100046873.1.21",
          "form": "네! 학종 쓰려고 동아리 많이 들었어요!",
          "original_form": "네! 학종 쓰려고 동아리 많이 들었어요!",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100046873.1.22",
          "form": "학원도 많이 다니셨어요?",
          "original_form": "학원도 많이 다니셨어요?",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100046873.1.23",
          "form": "저는 전과목 학원 하나 다니는데",
          "original_form": "저는 전과목 학원 하나 다니는데",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100046873.1.24",
          "form": "힘드네요 ",
          "original_form": "힘드네요 ",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100046873.1.25",
          "form": "아 저는 학원은 안 다녔어요",
          "original_form": "아 저는 학원은 안 다녔어요",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100046873.1.26",
          "form": "대신 야자를 10시까지 했어요ㅋㅋ",
          "original_form": "대신 야자를 10시까지 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100046873.1.27",
          "form": "학원 다니는거 힘내세요!!",
          "original_form": "학원 다니는거 힘내세요!!",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100046873.1.28",
          "form": "조금만 더 하면 쉴수 있을 거에요!!",
          "original_form": "조금만 더 하면 쉴수 있을 거에요!!",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100046873.1.29",
          "form": "시험 볼 때",
          "original_form": "시험 볼 때",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100046873.1.30",
          "form": "무슨 과목이 제일 점수 잘나오셨어요!",
          "original_form": "무슨 과목이 제일 점수 잘나오셨어요!",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100046873.1.31",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100046873.1.32",
          "form": "저는 국어요!!",
          "original_form": "저는 국어요!!",
          "speaker_id": "1",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100046873.1.33",
          "form": "아무래도 수학같은 건... 좀 낮았죠ㅋㅋ큐ㅠㅠ",
          "original_form": "아무래도 수학같은 건... 좀 낮았죠ㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100046873.1.34",
          "form": "수학은 진짜 싫죠..",
          "original_form": "수학은 진짜 싫죠..",
          "speaker_id": "2",
          "time": "20211001 21:46"
        },
        {
          "id": "MDRW2100046873.1.35",
          "form": "이러면 안되지만",
          "original_form": "이러면 안되지만",
          "speaker_id": "2",
          "time": "20211001 21:46"
        },
        {
          "id": "MDRW2100046873.1.36",
          "form": "수학과 관련된 이론을 세우고 ~의 법칙 이런거 만든 사람들",
          "original_form": "수학과 관련된 이론을 세우고 ~의 법칙 이런거 만든 사람들",
