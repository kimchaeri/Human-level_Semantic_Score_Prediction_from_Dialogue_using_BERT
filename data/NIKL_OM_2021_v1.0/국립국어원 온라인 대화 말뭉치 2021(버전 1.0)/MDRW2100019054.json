      "utterance": [
        {
          "id": "MDRW2100019054.1.1",
          "form": "꽁돈이 생기면 뭐사실거에요?",
          "original_form": "꽁돈이 생기면 뭐사실거에요?",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019054.1.2",
          "form": "음 명품카드지갑?",
          "original_form": "음 명품카드지갑?",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019054.1.3",
          "form": "명품  좋아하세요?",
          "original_form": "명품  좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019054.1.4",
          "form": "하나쯤 있으면 좋지않을까요?",
          "original_form": "하나쯤 있으면 좋지않을까요?",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019054.1.5",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019054.1.6",
          "form": "가방 신발 지갑 종류별로 하나씩 지니고있는 거 나쁘지않다고 생각해요",
          "original_form": "가방 신발 지갑 종류별로 하나씩 지니고있는 거 나쁘지않다고 생각해요",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019054.1.7",
          "form": "전 아직 명품이랄게 없어서요",
          "original_form": "전 아직 명품이랄게 없어서요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019054.1.8",
          "form": "그럼 님은 뭐사고 싶으세요",
          "original_form": "그럼 님은 뭐사고 싶으세요",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019054.1.9",
          "form": "전뭐 생필품이요",
          "original_form": "전뭐 생필품이요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019054.1.10",
          "form": "화장품이나 그런건요?",
          "original_form": "화장품이나 그런건요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019054.1.11",
          "form": "그것도 괜찮네요 바디로션같은 거",
          "original_form": "그것도 괜찮네요 바디로션같은 거",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019054.1.12",
          "form": "소소하시네요ㅋㅋ",
          "original_form": "소소하시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019054.1.13",
          "form": "소소한 게 제일 큰 행복이죠",
          "original_form": "소소한 게 제일 큰 행복이죠",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019054.1.14",
          "form": "전 요즘 열심히 절약중이라 사실 소비를 많이하진않아요",
          "original_form": "전 요즘 열심히 절약중이라 사실 소비를 많이하진않아요",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019054.1.15",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019054.1.16",
          "form": "돈모아서 집사는 게 제일의 목표",
