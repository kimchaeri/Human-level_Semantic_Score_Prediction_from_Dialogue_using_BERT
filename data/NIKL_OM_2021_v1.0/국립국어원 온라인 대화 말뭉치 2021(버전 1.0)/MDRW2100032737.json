      "utterance": [
        {
          "id": "MDRW2100032737.1.1",
          "form": "오늘은 야외에서 시작하네",
          "original_form": "오늘은 야외에서 시작하네",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032737.1.2",
          "form": "여름인가봐",
          "original_form": "여름인가봐",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032737.1.3",
          "form": "덥겠네",
          "original_form": "덥겠네",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032737.1.4",
          "form": "저번에 했던 부암동 저택 뭐시기랑 비슷하네",
          "original_form": "저번에 했던 부암동 저택 뭐시기랑 비슷하네",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032737.1.5",
          "form": "차 이쁘다",
          "original_form": "차 이쁘다",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032737.1.6",
          "form": "저런차로 바꾸고 싶다",
          "original_form": "저런차로 바꾸고 싶다",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032737.1.7",
          "form": "앗 검은탑 오징어",
          "original_form": "앗 검은탑 오징어",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032737.1.8",
          "form": "식혜는 광고야??",
          "original_form": "식혜는 광고야??",
          "speaker_id": "2",
          "time": "20210905 23:18"
        },
        {
          "id": "MDRW2100032737.1.9",
          "form": "응 저렇게 나온거보니까 광고같은데",
          "original_form": "응 저렇게 나온거보니까 광고같은데",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100032737.1.10",
          "form": "와 신기하네 방이 연결되어있네",
          "original_form": "와 신기하네 방이 연결되어있네",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032737.1.11",
          "form": "깜짝이야",
          "original_form": "깜짝이야",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032737.1.12",
          "form": "총은 어디에서 구했을까",
          "original_form": "총은 어디에서 구했을까",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032737.1.13",
          "form": "밀수했을까",
          "original_form": "밀수했을까",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032737.1.14",
          "form": "다 묶어버렸네",
          "original_form": "다 묶어버렸네",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100032737.1.15",
          "form": "풀수있을까",
          "original_form": "풀수있을까",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100032737.1.16",
          "form": "나도 종목 추천해줘",
          "original_form": "나도 종목 추천해줘",
          "speaker_id": "1",
          "time": "20210905 23:30"
        },
        {
          "id": "MDRW2100032737.1.17",
          "form": "바이오?!",
          "original_form": "바이오?!",
          "speaker_id": "1",
          "time": "20210905 23:30"
        },
        {
          "id": "MDRW2100032737.1.18",
          "form": "오늘 대탈출은 문제푸는게 별로 없네~",
          "original_form": "오늘 대탈출은 문제푸는게 별로 없네~",
          "speaker_id": "2",
          "time": "20210905 23:35"
        },
        {
          "id": "MDRW2100032737.1.19",
          "form": "저번주에는 문제푸는게 많았는디",
          "original_form": "저번주에는 문제푸는게 많았는디",
