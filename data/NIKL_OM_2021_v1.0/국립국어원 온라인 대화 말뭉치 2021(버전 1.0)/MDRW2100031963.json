      "utterance": [
        {
          "id": "MDRW2100031963.1.1",
          "form": "전 가끔 들어가서",
          "original_form": "전 가끔 들어가서",
          "speaker_id": "2",
          "time": "20210905 09:48"
        },
        {
          "id": "MDRW2100031963.1.2",
          "form": "뭐 있나보고 펀딩하는거라 ㅎㅎ",
          "original_form": "뭐 있나보고 펀딩하는거라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:48"
        },
        {
          "id": "MDRW2100031963.1.3",
          "form": "저 좀전에 어플 깔았어요 ㅋㅋ",
          "original_form": "저 좀전에 어플 깔았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031963.1.4",
          "form": "심심할때 들어가셔서 펀딩 인기순으로 보시면",
          "original_form": "심심할때 들어가셔서 펀딩 인기순으로 보시면",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031963.1.5",
          "form": "알짜배기들 있어요 ㅎㅎ",
          "original_form": "알짜배기들 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031963.1.6",
          "form": "인기순 꿀팁이네요",
          "original_form": "인기순 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210905 10:09"
        },
        {
          "id": "MDRW2100031963.1.7",
          "form": "앵콜펀딩하는것들은 평도 이미 있어서",
          "original_form": "앵콜펀딩하는것들은 평도 이미 있어서",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100031963.1.8",
          "form": "그런거 위주로 ㅎㅎ",
          "original_form": "그런거 위주로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100031963.1.9",
          "form": "앵콜해도 금액은 일정할까요?",
          "original_form": "앵콜해도 금액은 일정할까요?",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100031963.1.10",
          "form": "네 그렇던데요?",
          "original_form": "네 그렇던데요?",
          "speaker_id": "2",
          "time": "20210905 12:57"
        },
        {
          "id": "MDRW2100031963.1.11",
          "form": "오~ 잘보고있어야겠어요",
          "original_form": "오~ 잘보고있어야겠어요",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100031963.1.12",
          "form": "앨범같은거 올라올수도 있으니",
          "original_form": "앨범같은거 올라올수도 있으니",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031963.1.13",
          "form": "잘 보셔요 ㅎㅎ",
          "original_form": "잘 보셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100031963.1.14",
          "form": "진짜요?음반도 나와요? 카테고리 꼼꼼히봐야겠어요",
          "original_form": "진짜요?음반도 나와요? 카테고리 꼼꼼히봐야겠어요",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100031963.1.15",
          "form": "최근에 영화 보신거 있나요?",
          "original_form": "최근에 영화 보신거 있나요?",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100031963.1.16",
          "form": "좀전에 드림걸즈봤어요",
          "original_form": "좀전에 드림걸즈봤어요",
          "speaker_id": "1",
          "time": "20210905 15:01"
        },
        {
          "id": "MDRW2100031963.1.17",
          "form": "잼있었나요?",
          "original_form": "잼있었나요?",
          "speaker_id": "2",
          "time": "20210905 15:41"
        },
        {
          "id": "MDRW2100031963.1.18",
          "form": "넹 재개봉해서 보러갔는데 재밌더라구요 쫌 감동",
          "original_form": "넹 재개봉해서 보러갔는데 재밌더라구요 쫌 감동",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100031963.1.19",
          "form": "극장요즘 어때요?",
          "original_form": "극장요즘 어때요?",
          "speaker_id": "2",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100031963.1.20",
          "form": "마스크쓰고 보는 거죠",
          "original_form": "마스크쓰고 보는 거죠",
          "speaker_id": "1",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100031963.1.21",
          "form": "팝콘도못먹고",
          "original_form": "팝콘도못먹고",
          "speaker_id": "1",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100031963.1.22",
          "form": "음료는 마실수있어요",
          "original_form": "음료는 마실수있어요",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100031963.1.23",
          "form": "아 음료는 가능해요?",
          "original_form": "아 음료는 가능해요?",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100031963.1.24",
          "form": "마시고 쓰고?",
          "original_form": "마시고 쓰고?",
          "speaker_id": "2",
          "time": "20210905 18:03"
        },
        {
          "id": "MDRW2100031963.1.25",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210905 19:35"
        }
      ]
    }
  ]
