      "utterance": [
        {
          "id": "MDRW2100023022.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023022.1.2",
          "form": "네네 너무 재밌게봤어요!!!",
          "original_form": "네네 너무 재밌게봤어요!!!",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023022.1.3",
          "form": "초반엔 안보다가 ㅎㅎ",
          "original_form": "초반엔 안보다가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023022.1.4",
          "form": "끝에 관림가져서 재밌게봤어요 ㅎㅎ",
          "original_form": "끝에 관림가져서 재밌게봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023022.1.5",
          "form": "관심",
          "original_form": "관심",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023022.1.6",
          "form": "이번에 시차 맞아서 넘 좋지 않았어요?ㅋㅋㅋㅋㅋ",
          "original_form": "이번에 시차 맞아서 넘 좋지 않았어요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023022.1.7",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023022.1.8",
          "form": "다음 올림픽은 중국이라",
          "original_form": "다음 올림픽은 중국이라",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023022.1.9",
          "form": "시차맞는 올림픽들 아주 꿀입니다",
          "original_form": "시차맞는 올림픽들 아주 꿀입니다",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023022.1.10",
          "form": "평창부터 실시간으로 아주 좋아요",
          "original_form": "평창부터 실시간으로 아주 좋아요",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023022.1.11",
          "form": "평창에 구경 가셨나요?",
          "original_form": "평창에 구경 가셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023022.1.12",
          "form": "아니용 ㅠㅠ",
          "original_form": "아니용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023022.1.13",
          "form": "평창 구경다녀오셨어요?",
          "original_form": "평창 구경다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023022.1.14",
          "form": "경기는 못 봤고 가서 수호랑 인형만 사왔어요 ㅋㅋㅋㅋ",
          "original_form": "경기는 못 봤고 가서 수호랑 인형만 사왔어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023022.1.15",
          "form": "오오 인형 사셨구나!!",
          "original_form": "오오 인형 사셨구나!!",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023022.1.16",
          "form": "갑자기 평창올림픽때 패딩 엄청 유행했던거 생각나네요 ㅎㅎ",
          "original_form": "갑자기 평창올림픽때 패딩 엄청 유행했던거 생각나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023022.1.17",
          "form": "가성비가 좋았죠 ㅋㅋ 전 좀 후반에 가서 인형만 샀어요. 품절된 것도 있더라고요",
          "original_form": "가성비가 좋았죠 ㅋㅋ 전 좀 후반에 가서 인형만 샀어요. 품절된 것도 있더라고요",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023022.1.18",
          "form": "맞아요 흰색은 예쁘기까지해서 ㅎㅎ",
          "original_form": "맞아요 흰색은 예쁘기까지해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023022.1.19",
          "form": "품절된게 많았군요",
          "original_form": "품절된게 많았군요",
          "speaker_id": "2",
          "time": "20210829 22:52"
        },
        {
          "id": "MDRW2100023022.1.20",
          "form": "많지는 않았는데 사고 싶었던 뱃지가 없어서 기억에 남아요",
          "original_form": "많지는 않았는데 사고 싶었던 뱃지가 없어서 기억에 남아요",
          "speaker_id": "1",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100023022.1.21",
          "form": "아 그런거 속상하죠",
          "original_form": "아 그런거 속상하죠",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023022.1.22",
          "form": "사고싶은게 딱 없으면,,",
          "original_form": "사고싶은게 딱 없으면,,",
          "speaker_id": "2",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023022.1.23",
          "form": "그러니까요 별거 아닌데 여즉 기억에 남아요ㅠㅠ",
          "original_form": "그러니까요 별거 아닌데 여즉 기억에 남아요ㅠㅠ",
          "speaker_id": "1",
