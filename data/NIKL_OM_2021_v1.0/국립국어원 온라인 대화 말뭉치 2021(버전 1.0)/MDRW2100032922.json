      "utterance": [
        {
          "id": "MDRW2100032922.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 07:38"
        },
        {
          "id": "MDRW2100032922.1.2",
          "form": "안녕하세요 스벅 종이빨때 써보셨나여?",
          "original_form": "안녕하세요 스벅 종이빨때 써보셨나여?",
          "speaker_id": "2",
          "time": "20210906 07:49"
        },
        {
          "id": "MDRW2100032922.1.3",
          "form": "네 몇 번 이용해 봤어요",
          "original_form": "네 몇 번 이용해 봤어요",
          "speaker_id": "1",
          "time": "20210906 07:50"
        },
        {
          "id": "MDRW2100032922.1.4",
          "form": "솔직히 조금 불편하긴 하더라고요...",
          "original_form": "솔직히 조금 불편하긴 하더라고요...",
          "speaker_id": "1",
          "time": "20210906 07:50"
        },
        {
          "id": "MDRW2100032922.1.5",
          "form": "저도요 환경 생각해서 좋긴한데 익숙치 않더라구요",
          "original_form": "저도요 환경 생각해서 좋긴한데 익숙치 않더라구요",
          "speaker_id": "2",
          "time": "20210906 07:51"
        },
        {
          "id": "MDRW2100032922.1.6",
          "form": "근데 저는 애초에 스타벅스를 많이 안 가서...",
          "original_form": "근데 저는 애초에 스타벅스를 많이 안 가서...",
          "speaker_id": "1",
          "time": "20210906 07:52"
        },
        {
          "id": "MDRW2100032922.1.7",
          "form": "혹시 배달음식 많이 시키시나요??",
          "original_form": "혹시 배달음식 많이 시키시나요??",
          "speaker_id": "1",
          "time": "20210906 07:53"
        },
        {
          "id": "MDRW2100032922.1.8",
          "form": "요즘 배달음식에 일회용품이 많이 나오던데",
          "original_form": "요즘 배달음식에 일회용품이 많이 나오던데",
          "speaker_id": "1",
          "time": "20210906 07:53"
        },
        {
          "id": "MDRW2100032922.1.9",
          "form": "음 가끔 시키는데 일회용 수저 빼고 달라고해요ㅋ",
          "original_form": "음 가끔 시키는데 일회용 수저 빼고 달라고해요ㅋ",
          "speaker_id": "2",
          "time": "20210906 07:55"
        },
        {
          "id": "MDRW2100032922.1.10",
          "form": "저도 그러긴 하는데",
          "original_form": "저도 그러긴 하는데",
          "speaker_id": "1",
          "time": "20210906 07:59"
        },
        {
          "id": "MDRW2100032922.1.11",
          "form": "까먹고 같이 주는 곳도 많더라고요...",
          "original_form": "까먹고 같이 주는 곳도 많더라고요...",
          "speaker_id": "1",
          "time": "20210906 07:59"
        },
        {
          "id": "MDRW2100032922.1.12",
          "form": "집에 나무젓가락 쌓였는데 쓸 곳이 업어요",
          "original_form": "집에 나무젓가락 쌓였는데 쓸 곳이 업어요",
          "speaker_id": "1",
          "time": "20210906 07:59"
        },
        {
          "id": "MDRW2100032922.1.13",
          "form": "처리가 쉽지 않죠..다 쓰레기되니까ㅜ",
          "original_form": "처리가 쉽지 않죠..다 쓰레기되니까ㅜ",
          "speaker_id": "2",
          "time": "20210906 07:59"
        },
        {
          "id": "MDRW2100032922.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100032922.1.15",
          "form": "그리고 솔직히 나무젓가락보다 일반 젓가락이 더 편한 거 같더라고요",
          "original_form": "그리고 솔직히 나무젓가락보다 일반 젓가락이 더 편한 거 같더라고요",
          "speaker_id": "1",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100032922.1.16",
          "form": "익숙해져서 그런가...",
          "original_form": "익숙해져서 그런가...",
          "speaker_id": "1",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100032922.1.17",
          "form": "저는  고급 나무젓가락 좋아해요ㅋ 스시나 회 먹을때ㅋ",
          "original_form": "저는  고급 나무젓가락 좋아해요ㅋ 스시나 회 먹을때ㅋ",
          "speaker_id": "2",
          "time": "20210906 10:39"
        },
        {
          "id": "MDRW2100032922.1.18",
          "form": "아 좀 매끈한 젓가락은 괜찮죠",
          "original_form": "아 좀 매끈한 젓가락은 괜찮죠",
          "speaker_id": "1",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100032922.1.19",
          "form": "그건 재활용 되지 않나요?? 아닌가..",
          "original_form": "그건 재활용 되지 않나요?? 아닌가..",
          "speaker_id": "1",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100032922.1.20",
          "form": "모르겠어요 나무젓가락도 재활용은 될 거 같은디ㅋ",
          "original_form": "모르겠어요 나무젓가락도 재활용은 될 거 같은디ㅋ",
          "speaker_id": "2",
          "time": "20210906 11:21"
        },
        {
          "id": "MDRW2100032922.1.21",
          "form": "그 네모 나무젓가락이요?",
          "original_form": "그 네모 나무젓가락이요?",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100032922.1.22",
          "form": "비벼서 사용하는(?)",
          "original_form": "비벼서 사용하는(?)",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100032922.1.23",
          "form": "네 그거 안 되나요?",
          "original_form": "네 그거 안 되나요?",
          "speaker_id": "2",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100032922.1.24",
          "form": "그렇지 않을까요",
          "original_form": "그렇지 않을까요",
          "speaker_id": "1",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100032922.1.25",
          "form": "젓가락에 양념이 밴다고 해야 하나...",
          "original_form": "젓가락에 양념이 밴다고 해야 하나...",
          "speaker_id": "1",
          "time": "20210906 11:25"
        }
      ]
