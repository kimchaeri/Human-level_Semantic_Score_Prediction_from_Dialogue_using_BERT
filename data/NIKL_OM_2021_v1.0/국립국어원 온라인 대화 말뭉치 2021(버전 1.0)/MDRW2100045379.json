      "utterance": [
        {
          "id": "MDRW2100045379.1.1",
          "form": "네, 아 ..",
          "original_form": "네, 아 ..",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045379.1.2",
          "form": "ㅜㅜㅋㅋ",
          "original_form": "ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045379.1.3",
          "form": "하지만 꿋꿋하게",
          "original_form": "하지만 꿋꿋하게",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045379.1.4",
          "form": "그거 은근 스트레스라던데",
          "original_form": "그거 은근 스트레스라던데",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045379.1.5",
          "form": "괜찮으세요?",
          "original_form": "괜찮으세요?",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045379.1.6",
          "form": "제가 한 번 밥상에서 깽판을 쳐가지고",
          "original_form": "제가 한 번 밥상에서 깽판을 쳐가지고",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045379.1.7",
          "form": "놀라시더니 그 다음부터는 말씀 없으시더라고요",
          "original_form": "놀라시더니 그 다음부터는 말씀 없으시더라고요",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045379.1.8",
          "form": "가족들한테 감정적으로 나가는 경우가 거의 없었어서 더 그런지",
          "original_form": "가족들한테 감정적으로 나가는 경우가 거의 없었어서 더 그런지",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045379.1.9",
          "form": "ㅜㅜㅋㅋ",
          "original_form": "ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045379.1.10",
          "form": "저도 몇번했는데ㅋㅋ",
          "original_form": "저도 몇번했는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045379.1.11",
          "form": "도통 포기를....ㅜ",
          "original_form": "도통 포기를....ㅜ",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045379.1.12",
          "form": "오 .. 진짜로 숟가락을 던져야 합니다.",
          "original_form": "오 .. 진짜로 숟가락을 던져야 합니다.",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045379.1.13",
          "form": "눈에 살기를 담아서 ㅋㅋ",
          "original_form": "눈에 살기를 담아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045379.1.14",
          "form": "ㅋㅋ 그럼 엄빠 쓰러져오ㅜㅜ",
          "original_form": "ㅋㅋ 그럼 엄빠 쓰러져오ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045379.1.15",
          "form": "암환자ㅜㅋㅋ시고 나이드셔서ㅜㅠ",
          "original_form": "암환자ㅜㅋㅋ시고 나이드셔서ㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045379.1.16",
          "form": "제가 전에는 식사예절 따지고 또래보다 보수적인 편이었는데",
          "original_form": "제가 전에는 식사예절 따지고 또래보다 보수적인 편이었는데",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045379.1.17",
          "form": "아..ㅜ",
          "original_form": "아..ㅜ",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045379.1.18",
          "form": "저흰\r\n 완전 유교",
          "original_form": "저흰\r\n 완전 유교",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045379.1.19",
          "form": "으 저는 특정 파트에서 제가 그런편이었거든요 ㅋㅋ 아 식사예절은 지금도 중요하지만",
          "original_form": "으 저는 특정 파트에서 제가 그런편이었거든요 ㅋㅋ 아 식사예절은 지금도 중요하지만",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045379.1.20",
          "form": "여자는 생존권과 자유가 먼저라",
          "original_form": "여자는 생존권과 자유가 먼저라",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045379.1.21",
          "form": "깽판이 효과적일 땐 해야하는 것 같습니다.",
          "original_form": "깽판이 효과적일 땐 해야하는 것 같습니다.",
          "speaker_id": "2",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045379.1.22",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045379.1.23",
          "form": "당차시네요!!!",
          "original_form": "당차시네요!!!",
          "speaker_id": "1",
          "time": "20210908 02:18"
        },
        {
          "id": "MDRW2100045379.1.24",
          "form": "저도 맘속으로",
          "original_form": "저도 맘속으로",
          "speaker_id": "1",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045379.1.25",
          "form": "그 스트레스는 혼자 다 받고 아무도 안 알아주고 알아줘도 소용 없고",
          "original_form": "그 스트레스는 혼자 다 받고 아무도 안 알아주고 알아줘도 소용 없고",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045379.1.26",
          "form": "ㅅㅏ실 저러셔도 강제결혼은 못하자나요",
          "original_form": "ㅅㅏ실 저러셔도 강제결혼은 못하자나요",
