      "utterance": [
        {
          "id": "MDRW2100024131.1.1",
          "form": "봉사 시간 얻으려고 해봤을때 빼곤 안해봤네요ㅜ",
          "original_form": "봉사 시간 얻으려고 해봤을때 빼곤 안해봤네요ㅜ",
          "speaker_id": "2",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100024131.1.2",
          "form": "저도 봉사시간 채워야돼서ㅋㅋ",
          "original_form": "저도 봉사시간 채워야돼서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024131.1.3",
          "form": "어떤봉사하셨어요?",
          "original_form": "어떤봉사하셨어요?",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100024131.1.4",
          "form": "피켓이랑 어린이집이요ㅋㅋ",
          "original_form": "피켓이랑 어린이집이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024131.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024131.1.6",
          "form": "저는 시각장애인 도서입력봉사요!",
          "original_form": "저는 시각장애인 도서입력봉사요!",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024131.1.7",
          "form": "우왕!",
          "original_form": "우왕!",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024131.1.8",
          "form": "졸업하니 안하게 되는게 생각해보니 씁쓸하네요",
          "original_form": "졸업하니 안하게 되는게 생각해보니 씁쓸하네요",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100024131.1.9",
          "form": "헉 맞아요...",
          "original_form": "헉 맞아요...",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024131.1.10",
          "form": "졸업하곤 한번도 안했어요ㅠ",
          "original_form": "졸업하곤 한번도 안했어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024131.1.11",
          "form": "활동하는 동호회있으세요?",
          "original_form": "활동하는 동호회있으세요?",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024131.1.12",
          "form": "저도..ㅠ",
          "original_form": "저도..ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024131.1.13",
          "form": "없어요ㅠㅜ 있으세요?",
          "original_form": "없어요ㅠㅜ 있으세요?",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024131.1.14",
          "form": "하고싶은동호회는 있어요!",
          "original_form": "하고싶은동호회는 있어요!",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024131.1.15",
          "form": "오 어떤거요??",
          "original_form": "오 어떤거요??",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024131.1.16",
          "form": "동네에서 여자들끼리 같이 운동하는 동호회있으면 하고싶어요ㅎㅎ",
          "original_form": "동네에서 여자들끼리 같이 운동하는 동호회있으면 하고싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024131.1.17",
          "form": "name2님은 있으세요?",
          "original_form": "&name2&님은 있으세요?",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024131.1.18",
          "form": "오와.. 그거 좋을거 같네요.!",
          "original_form": "오와.. 그거 좋을거 같네요.!",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024131.1.19",
          "form": "저는 공예쪽 해보고싶어요!",
          "original_form": "저는 공예쪽 해보고싶어요!",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024131.1.20",
          "form": "동네 공방 동호회같은거",
          "original_form": "동네 공방 동호회같은거",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024131.1.21",
          "form": "있으면좋겠네요!",
          "original_form": "있으면좋겠네요!",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024131.1.22",
          "form": "맞아요!! 하고픈거 가져가서 하고~ 그러고 싶어요",
          "original_form": "맞아요!! 하고픈거 가져가서 하고~ 그러고 싶어요",
          "speaker_id": "2",
