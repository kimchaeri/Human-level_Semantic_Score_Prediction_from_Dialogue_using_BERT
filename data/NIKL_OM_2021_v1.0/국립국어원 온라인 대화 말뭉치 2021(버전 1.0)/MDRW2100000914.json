      "utterance": [
        {
          "id": "MDRW2100000914.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000914.1.2",
          "form": "안녕하세용 ㅎㅎ",
          "original_form": "안녕하세용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000914.1.3",
          "form": "요즘 날씨 너무 습하죠 ㅠ",
          "original_form": "요즘 날씨 너무 습하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000914.1.4",
          "form": "그래도 가을날씨 비슷해졌어요",
          "original_form": "그래도 가을날씨 비슷해졌어요",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000914.1.5",
          "form": "맞아요 ㅎㅎ 그런데 여긴 갑자기 비도오고 하더라구용 ..",
          "original_form": "맞아요 ㅎㅎ 그런데 여긴 갑자기 비도오고 하더라구용 ..",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000914.1.6",
          "form": "맞아요 비도 오고 그래서",
          "original_form": "맞아요 비도 오고 그래서",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000914.1.7",
          "form": "이제 가을옷 준비도해야할거같아용 ㅠㅠ",
          "original_form": "이제 가을옷 준비도해야할거같아용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000914.1.8",
          "form": "준비하면 바로 겨울와요",
          "original_form": "준비하면 바로 겨울와요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000914.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아여 그러네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아여 그러네요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000914.1.10",
          "form": "롱패딩꺼낼때가 됐어요",
          "original_form": "롱패딩꺼낼때가 됐어요",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000914.1.11",
          "form": "여름옷 넣고 겨울옷 꺼내기도 기찮..",
          "original_form": "여름옷 넣고 겨울옷 꺼내기도 기찮..",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000914.1.12",
          "form": "그것도 일이에요",
          "original_form": "그것도 일이에요",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000914.1.13",
          "form": "그쵸 봄가을 날씨가 젤 좋아요",
          "original_form": "그쵸 봄가을 날씨가 젤 좋아요",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000914.1.14",
          "form": "전 여름이 제일 좋아요",
          "original_form": "전 여름이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210819 20:42"
