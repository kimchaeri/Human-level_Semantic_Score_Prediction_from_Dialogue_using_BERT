      "utterance": [
        {
          "id": "MDRW2100037018.1.1",
          "form": "병원자주다니시나요?",
          "original_form": "병원자주다니시나요?",
          "speaker_id": "1",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100037018.1.2",
          "form": "자주까진 아니지만.. 아프면 바로바로 가는 편이에요",
          "original_form": "자주까진 아니지만.. 아프면 바로바로 가는 편이에요",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100037018.1.3",
          "form": "저도그래여 건강검진하셨나요?",
          "original_form": "저도그래여 건강검진하셨나요?",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100037018.1.4",
          "form": "올해 받아야는데 아직 못했어요",
          "original_form": "올해 받아야는데 아직 못했어요",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037018.1.5",
          "form": "치과도 가야는데.. 코로나 때문에 자꾸 미루게 되네요",
          "original_form": "치과도 가야는데.. 코로나 때문에 자꾸 미루게 되네요",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037018.1.6",
          "form": "저도요..사실 앞니가 지금 시큰거려서",
          "original_form": "저도요..사실 앞니가 지금 시큰거려서",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100037018.1.7",
          "form": "치과빼박으로 가야하는데...",
          "original_form": "치과빼박으로 가야하는데...",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100037018.1.8",
          "form": "앗 더 심해지기 전에 가셔야 할 것 같아요",
          "original_form": "앗 더 심해지기 전에 가셔야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100037018.1.9",
          "form": "네ㅜㅜ근데 하 치과 넘 무섭고..",
          "original_form": "네ㅜㅜ근데 하 치과 넘 무섭고..",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100037018.1.10",
          "form": "돈이넘무섭네요",
          "original_form": "돈이넘무섭네요",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100037018.1.11",
          "form": "치과는 비용이 무서워서 전 1년에 한 번씩은 가고 있어요",
          "original_form": "치과는 비용이 무서워서 전 1년에 한 번씩은 가고 있어요",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100037018.1.12",
          "form": "네 안가는게 더 돈많이들텐데ㅜ",
          "original_form": "네 안가는게 더 돈많이들텐데ㅜ",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100037018.1.13",
          "form": "스케일링 보험처리 되서 얼마 안 하니까 꼭 받으세요",
          "original_form": "스케일링 보험처리 되서 얼마 안 하니까 꼭 받으세요",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100037018.1.14",
          "form": "네 만원정도죠? 1년에1번인가",
          "original_form": "네 만원정도죠? 1년에1번인가",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100037018.1.15",
          "form": "네 만원대였어요",
          "original_form": "네 만원대였어요",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037018.1.16",
          "form": "흑 이번에가서 스케일링도 같이받아야겠네요",
