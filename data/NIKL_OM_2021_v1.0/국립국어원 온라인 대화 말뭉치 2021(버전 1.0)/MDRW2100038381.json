      "utterance": [
        {
          "id": "MDRW2100038381.1.1",
          "form": "전 내장 안좋아하는데..흑흑",
          "original_form": "전 내장 안좋아하는데..흑흑",
          "speaker_id": "1",
          "time": "20210902 11:59"
        },
        {
          "id": "MDRW2100038381.1.2",
          "form": "네 그래서 전 천엽을 넣어 먹었고",
          "original_form": "네 그래서 전 천엽을 넣어 먹었고",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038381.1.3",
          "form": "전 쫄깃쫄깃하니 참 좋았는데",
          "original_form": "전 쫄깃쫄깃하니 참 좋았는데",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038381.1.4",
          "form": "오 그렇군여???!",
          "original_form": "오 그렇군여???!",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038381.1.5",
          "form": "한국에선 있는 경우가 없어서 아쉬워요",
          "original_form": "한국에선 있는 경우가 없어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038381.1.6",
          "form": "마자여 아예 없는거같아요",
          "original_form": "마자여 아예 없는거같아요",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100038381.1.7",
          "form": "그 맛은 비슷한가요?",
          "original_form": "그 맛은 비슷한가요?",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100038381.1.8",
          "form": "맛은 비슷했던거같아요",
          "original_form": "맛은 비슷했던거같아요",
          "speaker_id": "2",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100038381.1.9",
          "form": "마라탕도 로컬라이징 많이 한곳이 있다지만",
          "original_form": "마라탕도 로컬라이징 많이 한곳이 있다지만",
          "speaker_id": "2",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100038381.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100038381.1.11",
          "form": "자기네는 고춧가루 넣었다 하는곳 아니면 거의 유사한듯?",
          "original_form": "자기네는 고춧가루 넣었다 하는곳 아니면 거의 유사한듯?",
          "speaker_id": "2",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100038381.1.12",
          "form": "그래도 그 마라가 기본 베이스라 비슷한가보군요",
          "original_form": "그래도 그 마라가 기본 베이스라 비슷한가보군요",
          "speaker_id": "1",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100038381.1.13",
          "form": "네, 그런가봐요",
          "original_form": "네, 그런가봐요",
          "speaker_id": "2",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100038381.1.14",
          "form": "개인적으로 전 탕파인데",
          "original_form": "개인적으로 전 탕파인데",
          "speaker_id": "2",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100038381.1.15",
          "form": "저는 샹궈욬ㅋㅋㅋ",
          "original_form": "저는 샹궈욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038381.1.16",
          "form": "제친구는 샹궈파에요",
          "original_form": "제친구는 샹궈파에요",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038381.1.17",
          "form": "근데 마라탕을 저 자주먹어요",
          "original_form": "근데 마라탕을 저 자주먹어요",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038381.1.18",
          "form": "뭔가 친구들이랑 같이 먹고 이래서 ㅋㅋㅋ",
          "original_form": "뭔가 친구들이랑 같이 먹고 이래서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038381.1.19",
          "form": "하나 푸짐하게 시키는게 돈도 덜드는데",
          "original_form": "하나 푸짐하게 시키는게 돈도 덜드는데",
          "speaker_id": "2",
          "time": "20210902 12:03"
