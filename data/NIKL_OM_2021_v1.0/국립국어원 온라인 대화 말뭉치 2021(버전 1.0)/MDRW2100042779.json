      "utterance": [
        {
          "id": "MDRW2100042779.1.1",
          "form": "운동하시는거 있으세요",
          "original_form": "운동하시는거 있으세요",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042779.1.2",
          "form": "저 홈트랑 런닝이요?",
          "original_form": "저 홈트랑 런닝이요?",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042779.1.3",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042779.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042779.1.5",
          "form": "집에서 땀나게.운동하시는분들 존경합니다",
          "original_form": "집에서 땀나게.운동하시는분들 존경합니다",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042779.1.6",
          "form": "하시는 운동 잇으세요??",
          "original_form": "하시는 운동 잇으세요??",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042779.1.7",
          "form": "진짜 뿌듯해요! ㅋㅋ",
          "original_form": "진짜 뿌듯해요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042779.1.8",
          "form": "저는 이제 동네 산책 정도??",
          "original_form": "저는 이제 동네 산책 정도??",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042779.1.9",
          "form": "아ㅋㅋ저도 귀찮을땐 동네한바퀴돌고 아이스크림할인점 돌아요ㅋㅋ",
          "original_form": "아ㅋㅋ저도 귀찮을땐 동네한바퀴돌고 아이스크림할인점 돌아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042779.1.10",
          "form": "아이스크림 할인점 돌면",
          "original_form": "아이스크림 할인점 돌면",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042779.1.11",
          "form": "결국 아이스크림 먹는거 아닌가요",
          "original_form": "결국 아이스크림 먹는거 아닌가요",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042779.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042779.1.13",
          "form": "ㅋㅋ 그게 묘미죠!!! 집에만 있음 죄책감들어 못먹지만ㅋㅋ 산책후엔 가능합니다!!",
          "original_form": "ㅋㅋ 그게 묘미죠!!! 집에만 있음 죄책감들어 못먹지만ㅋㅋ 산책후엔 가능합니다!!",
          "speaker_id": "2",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042779.1.14",
          "form": "좋네여!!",
          "original_form": "좋네여!!",
          "speaker_id": "1",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042779.1.15",
          "form": "추천코스ㅋㅋ",
          "original_form": "추천코스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042779.1.16",
          "form": "산책하면 몇분걸려요?",
          "original_form": "산책하면 몇분걸려요?",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042779.1.17",
          "form": "아 제주 사라봉 근처 살아서",
          "original_form": "아 제주 사라봉 근처 살아서",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042779.1.18",
          "form": "1시간 정도 돌아요",
          "original_form": "1시간 정도 돌아요",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042779.1.19",
          "form": "아 그럼 완전 운동되네요!",
          "original_form": "아 그럼 완전 운동되네요!",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042779.1.20",
          "form": "오름도 오르시나요?",
          "original_form": "오름도 오르시나요?",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042779.1.21",
          "form": "네네 거기가면 좋은데 맨날은 안가구",
          "original_form": "네네 거기가면 좋은데 맨날은 안가구",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042779.1.22",
          "form": "이제부터 가야죠",
          "original_form": "이제부터 가야죠",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042779.1.23",
          "form": "ㅋㅋ오름 진짜 좋더라고요",
          "original_form": "ㅋㅋ오름 진짜 좋더라고요",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042779.1.24",
          "form": "한라산 가기전에 몸을 좀 만들어야 해요",
          "original_form": "한라산 가기전에 몸을 좀 만들어야 해요",
          "speaker_id": "1",
          "time": "20210907 09:19"
