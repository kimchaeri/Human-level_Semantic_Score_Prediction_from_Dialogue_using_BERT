      "utterance": [
        {
          "id": "MDRW2100003728.1.1",
          "form": "지금 비가 엄청오네 ㅜㅜ 어제 밤부터",
          "original_form": "지금 비가 엄청오네 ㅜㅜ 어제 밤부터",
          "speaker_id": "1",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003728.1.2",
          "form": "급 내리기 시작하더니... 장마 시작인가봐",
          "original_form": "급 내리기 시작하더니... 장마 시작인가봐",
          "speaker_id": "1",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003728.1.3",
          "form": "2차장마 시작이라고 하더라 ㅜ",
          "original_form": "2차장마 시작이라고 하더라 ㅜ",
          "speaker_id": "2",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003728.1.4",
          "form": "주말에 무슨 비야 진짜",
          "original_form": "주말에 무슨 비야 진짜",
          "speaker_id": "2",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003728.1.5",
          "form": "징글징글하다 ㅠㅜ",
          "original_form": "징글징글하다 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003728.1.6",
          "form": "당분간 빨래도 안마르겠네 ㅜㅜ",
          "original_form": "당분간 빨래도 안마르겠네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003728.1.7",
          "form": "진짜 빨래에 선풍기 틀어야겠다 ㅜㅜ",
          "original_form": "진짜 빨래에 선풍기 틀어야겠다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003728.1.8",
          "form": "다음주 다~~ 비야",
          "original_form": "다음주 다~~ 비야",
          "speaker_id": "2",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003728.1.9",
          "form": "꿉꿉해서 냄새도 날텐데 ㅜㅜ",
          "original_form": "꿉꿉해서 냄새도 날텐데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003728.1.10",
          "form": "비 좀 그만와줘...",
          "original_form": "비 좀 그만와줘...",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003728.1.11",
          "form": "기상청 예보 틀려달라고 ㅜㅜ",
          "original_form": "기상청 예보 틀려달라고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003728.1.12",
          "form": "내말이 ㅠㅜ 이제 가을날씨인가",
          "original_form": "내말이 ㅠㅜ 이제 가을날씨인가",
          "speaker_id": "2",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003728.1.13",
          "form": "생각했는디 갑자기 장마라니",
          "original_form": "생각했는디 갑자기 장마라니",
          "speaker_id": "2",
          "time": "20210821 11:30"
        },
        {
          "id": "MDRW2100003728.1.14",
          "form": "진짜 걍 가을 생각하고 있었는데 ㅜㅜ",
          "original_form": "진짜 걍 가을 생각하고 있었는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003728.1.15",
          "form": "이따 일보러 가야되는데 너무 가기 싫다 ㅜㅜ",
          "original_form": "이따 일보러 가야되는데 너무 가기 싫다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003728.1.16",
          "form": "그치 트렌치코트 입어야지 ㅎㅎ",
          "original_form": "그치 트렌치코트 입어야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:34"
        },
        {
          "id": "MDRW2100003728.1.17",
          "form": "아진짜?나도 쪼리 신었는데 찜찜하다",
          "original_form": "아진짜?나도 쪼리 신었는데 찜찜하다",
          "speaker_id": "2",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003728.1.18",
          "form": "나도 쪼리 신을랬는데 ㅜㅜ 미끄러질거같아서 걱정이네 너무 쏟아져서",
          "original_form": "나도 쪼리 신을랬는데 ㅜㅜ 미끄러질거같아서 걱정이네 너무 쏟아져서",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003728.1.19",
          "form": "발가락에 힘줘야해 ㅠ",
          "original_form": "발가락에 힘줘야해 ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003728.1.20",
          "form": "비가 더 오네 아까보다",
          "original_form": "비가 더 오네 아까보다",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003728.1.21",
          "form": "완전 쏟아져.....",
          "original_form": "완전 쏟아져.....",
          "speaker_id": "1",
          "time": "20210821 12:08"
        },
        {
          "id": "MDRW2100003728.1.22",
          "form": "못나갈거같아 ㅜㅜ",
          "original_form": "못나갈거같아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 12:08"
        },
        {
          "id": "MDRW2100003728.1.23",
          "form": "진짜.. 집 나올때 창문 잘 닫고 나와서 다행이다",
          "original_form": "진짜.. 집 나올때 창문 잘 닫고 나와서 다행이다",
          "speaker_id": "2",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003728.1.24",
          "form": "ㅋㅋㅋㅋ 맞아 비 많이 들이칠듯 열리몈",
          "original_form": "ㅋㅋㅋㅋ 맞아 비 많이 들이칠듯 열리몈",
