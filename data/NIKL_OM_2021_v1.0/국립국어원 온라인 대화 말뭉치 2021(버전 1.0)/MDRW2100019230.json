      "utterance": [
        {
          "id": "MDRW2100019230.1.1",
          "form": "아냐아냐",
          "original_form": "아냐아냐",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019230.1.2",
          "form": "안잘릴꺼야",
          "original_form": "안잘릴꺼야",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019230.1.3",
          "form": "그래야 해ㅠㅠ 알바 구하기가 너무 힘드니까ㅠㅠ",
          "original_form": "그래야 해ㅠㅠ 알바 구하기가 너무 힘드니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019230.1.4",
          "form": "마저...",
          "original_form": "마저...",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019230.1.5",
          "form": "2년은 버텨야 해...",
          "original_form": "2년은 버텨야 해...",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019230.1.6",
          "form": "엉엉ㅠㅠ",
          "original_form": "엉엉ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019230.1.7",
          "form": "으허ㅓ엉...",
          "original_form": "으허ㅓ엉...",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019230.1.8",
          "form": "진짜 서럽다...",
          "original_form": "진짜 서럽다...",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019230.1.9",
          "form": "슬퍼...",
          "original_form": "슬퍼...",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019230.1.10",
          "form": "근데 ㄹㅇ 공부 안 할 생각 아니면 과외 하는 게 나아 보인 거든?",
          "original_form": "근데 ㄹㅇ 공부 안 할 생각 아니면 과외 하는 게 나아 보인 거든?",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019230.1.11",
          "form": "공부를 좀 못해서 1:1로 배워야 좀 알아 듣는 거 같은데",
          "original_form": "공부를 좀 못해서 1:1로 배워야 좀 알아 듣는 거 같은데",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019230.1.12",
          "form": "어우..",
          "original_form": "어우..",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019230.1.13",
          "form": "게임밖에 안 하는 거 같음",
          "original_form": "게임밖에 안 하는 거 같음",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019230.1.14",
          "form": "어우 정말 안좋은데",
          "original_form": "어우 정말 안좋은데",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019230.1.15",
          "form": "배그? 같은 거 하나본데 잠도 제대로 안 자고 게임하고 게임 할 때는 말도 험하게 하나봐",
          "original_form": "배그? 같은 거 하나본데 잠도 제대로 안 자고 게임하고 게임 할 때는 말도 험하게 하나봐",
          "speaker_id": "1",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019230.1.16",
          "form": "아니...",
          "original_form": "아니...",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019230.1.17",
          "form": "그냥 게임 중독에 폭력적이야...",
          "original_form": "그냥 게임 중독에 폭력적이야...",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019230.1.18",
          "form": "과외할 때는 안 그러거든",
          "original_form": "과외할 때는 안 그러거든",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019230.1.19",
          "form": "내가 어릴 때부터 많이 눌러놔서",
          "original_form": "내가 어릴 때부터 많이 눌러놔서",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019230.1.20",
          "form": "내 앞에서 자기 마음대로 못하니까 이러는 거 같기도 하고",
          "original_form": "내 앞에서 자기 마음대로 못하니까 이러는 거 같기도 하고",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
