      "utterance": [
        {
          "id": "MDRW2100021992.1.1",
          "form": "안녕하세용 어제 뭐 드셨어용?",
          "original_form": "안녕하세용 어제 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021992.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021992.1.3",
          "form": "저는 어제 저녁으로 간단하게 감튀랑 과일먹었어요!",
          "original_form": "저는 어제 저녁으로 간단하게 감튀랑 과일먹었어요!",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021992.1.4",
          "form": "name1은 뭐 드셨나요??",
          "original_form": "&name1&은 뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021992.1.5",
          "form": "헉 감튀! 저는 맘스터치 감튀 좋아해요ㅋㅋ",
          "original_form": "헉 감튀! 저는 맘스터치 감튀 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021992.1.6",
          "form": "저는 빵이랑",
          "original_form": "저는 빵이랑",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021992.1.7",
          "form": "라면먹었어용ㅎㅎ",
          "original_form": "라면먹었어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021992.1.8",
          "form": "라면은 어떤 라면 드셨나용??",
          "original_form": "라면은 어떤 라면 드셨나용??",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021992.1.9",
          "form": "오잉?? 전에 맘터 감튀얘기를 보냈는데 그거는 안갔네요ㅠㅠ",
          "original_form": "오잉?? 전에 맘터 감튀얘기를 보냈는데 그거는 안갔네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021992.1.10",
          "form": "저는 참깨라면 좋아해요",
          "original_form": "저는 참깨라면 좋아해요",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021992.1.11",
          "form": "엇!! 참깨라면!!",
          "original_form": "엇!! 참깨라면!!",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.12",
          "form": "네네ㅜㅜ 왤까용ㅎㅎ",
          "original_form": "네네ㅜㅜ 왤까용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.13",
          "form": "어떤 라면 좋아하세요?",
          "original_form": "어떤 라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.14",
          "form": "혹시 GS편의점에서 파는 참깨라면누룽지 드셔보셨나요???",
          "original_form": "혹시 GS편의점에서 파는 참깨라면누룽지 드셔보셨나요???",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.15",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.16",
          "form": "참깨라면 누룽지요???",
          "original_form": "참깨라면 누룽지요???",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.17",
          "form": "전 그거 먹고 참깨누룽지가 원픽됐어요!ㅠㅠ",
          "original_form": "전 그거 먹고 참깨누룽지가 원픽됐어요!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.18",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.19",
          "form": "어떤 쩝쩝박사가 그런걸 만들었대요",
          "original_form": "어떤 쩝쩝박사가 그런걸 만들었대요",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.20",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021992.1.21",
          "form": "그러니까요ㅠㅠ 그런데 너무 슬프게도 GS편의점에만 판다는게ㅠㅠㅠㅠㅠㅠ",
          "original_form": "그러니까요ㅠㅠ 그런데 너무 슬프게도 GS편의점에만 판다는게ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.22",
          "form": "아아 저 내일 집근처 gs 털러갑니다....",
          "original_form": "아아 저 내일 집근처 gs 털러갑니다....",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.23",
          "form": "누룽지는 못참죠ㅋㅋㅋㅋㅋ",
          "original_form": "누룽지는 못참죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.24",
          "form": "동네에 gs는 한군데 밖에 없는데 거기에 없으면 전 이 라면을 먹질 못해요ㅠㅠㅠㅠ",
          "original_form": "동네에 gs는 한군데 밖에 없는데 거기에 없으면 전 이 라면을 먹질 못해요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.25",
          "form": "아 진짜요?ㅠㅠㅠ",
          "original_form": "아 진짜요?ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.26",
          "form": "침고여요ㅋㅋㅋㅋ",
          "original_form": "침고여요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.27",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210829 00:36"
        },
        {
          "id": "MDRW2100021992.1.28",
          "form": "저 편의점 라면중에",
          "original_form": "저 편의점 라면중에",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.29",
          "form": "세븐일레븐에서 파는",
          "original_form": "세븐일레븐에서 파는",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.30",
          "form": "교동짬뽕 진짜 맛있게 먹었는데ㅋㅋㅋ",
          "original_form": "교동짬뽕 진짜 맛있게 먹었는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.31",
          "form": "아직도 팔아요 불맛나고 맛있어용",
          "original_form": "아직도 팔아요 불맛나고 맛있어용",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.32",
          "form": "집근처에 기쁘게도 세븐이 두군데 있죠.",
          "original_form": "집근처에 기쁘게도 세븐이 두군데 있죠.",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.33",
          "form": "내일 날밝으면 한 번 가봐야겠네요ㅠㅠ",
          "original_form": "내일 날밝으면 한 번 가봐야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.34",
          "form": "흐흐ㅋㅋㅋ 추천드려용",
          "original_form": "흐흐ㅋㅋㅋ 추천드려용",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021992.1.35",
          "form": "치킨은 어디꺼 젤 좋아하세용?",
          "original_form": "치킨은 어디꺼 젤 좋아하세용?",
