      "utterance": [
        {
          "id": "MDRW2100045274.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045274.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045274.1.3",
          "form": "요즘 날씨가 너무 오락가락한거 같아요ㅠ",
          "original_form": "요즘 날씨가 너무 오락가락한거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045274.1.4",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045274.1.5",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045274.1.6",
          "form": "지금은 그쳤는데 9시까지는 왔었어요!",
          "original_form": "지금은 그쳤는데 9시까지는 왔었어요!",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045274.1.7",
          "form": "거기는 아직도 비가 오나요?",
          "original_form": "거기는 아직도 비가 오나요?",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045274.1.8",
          "form": "여기는 계속 오다가 한 한시간전쯤 그친것 같아요",
          "original_form": "여기는 계속 오다가 한 한시간전쯤 그친것 같아요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045274.1.9",
          "form": "어제 비 많이 왔어요?",
          "original_form": "어제 비 많이 왔어요?",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045274.1.10",
          "form": "여긴 많이 왔어요",
          "original_form": "여긴 많이 왔어요",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045274.1.11",
          "form": "네 한 5시까지는 좀 많이 왔어요ㅠ",
          "original_form": "네 한 5시까지는 좀 많이 왔어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045274.1.12",
          "form": "바람도 많이 불었어요?",
          "original_form": "바람도 많이 불었어요?",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045274.1.13",
          "form": "바람은 12시에만 많이 불고 그 다음엔 많이 안불었어요",
          "original_form": "바람은 12시에만 많이 불고 그 다음엔 많이 안불었어요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045274.1.14",
          "form": "거긴 많이 불었나요?",
          "original_form": "거긴 많이 불었나요?",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045274.1.15",
          "form": "네 나무가지들이 휘청휘청하고 나뭇잎들 부딪히묜서 쏴아아아아아하는 소리 있죠",
          "original_form": "네 나무가지들이 휘청휘청하고 나뭇잎들 부딪히묜서 쏴아아아아아하는 소리 있죠",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045274.1.16",
          "form": "계속 그랬어요",
          "original_form": "계속 그랬어요",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045274.1.17",
          "form": "헐 그럴 땐 밖에 나가기 싫어지죠ㅠ",
          "original_form": "헐 그럴 땐 밖에 나가기 싫어지죠ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045274.1.18",
          "form": "제가 바로 밖에 있었답니다 ㅋㅋㅋㅋ",
          "original_form": "제가 바로 밖에 있었답니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045274.1.19",
          "form": "옷 펄럭펄럭하고 너무 추웠어요..",
          "original_form": "옷 펄럭펄럭하고 너무 추웠어요..",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045274.1.20",
          "form": "으악! 듣기만해도 추워지네요ㅠㅜ",
          "original_form": "으악! 듣기만해도 추워지네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045274.1.21",
          "form": "그쵸ㅠㅠㅠ그래서 자면서 추울까봐 털 후드 입었어요 ㅋㅋㅋㅋ",
          "original_form": "그쵸ㅠㅠㅠ그래서 자면서 추울까봐 털 후드 입었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045274.1.22",
          "form": "앜ㅋㅋㅋㅋㅋ 차라리 장판 틀고 주무세요ㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 차라리 장판 틀고 주무세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045274.1.23",
          "form": "털 후드 입고 주무시면 답답하지 않으세요? 저는 답답할 거 같은데ㅠ",
          "original_form": "털 후드 입고 주무시면 답답하지 않으세요? 저는 답답할 거 같은데ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045274.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 장판꺼내는건 외부 도움을 받는거라 아직 자존심상해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 장판꺼내는건 외부 도움을 받는거라 아직 자존심상해요",
          "speaker_id": "1",
          "time": "20210908 01:54"
        }
      ]
    }
