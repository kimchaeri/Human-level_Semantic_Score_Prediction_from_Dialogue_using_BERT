        },
        {
          "id": "MDRW2100022913.1.7",
          "form": "금",
          "original_form": "금",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.8",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.9",
          "form": "아기교정하는거",
          "original_form": "아기교정하는거",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.10",
          "form": "문제 아동이라고 보면 다 부모 문제임",
          "original_form": "문제 아동이라고 보면 다 부모 문제임",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.11",
          "form": "ㅇㅇㅇ맞음",
          "original_form": "ㅇㅇㅇ맞음",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.12",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.13",
          "form": "아빠잘못임",
          "original_form": "아빠잘못임",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.14",
          "form": "대부분이",
          "original_form": "대부분이",
          "speaker_id": "1",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.15",
          "form": "나 저번에 애가 발톱 문다고 했던 고 봤는데",
          "original_form": "나 저번에 애가 발톱 문다고 했던 고 봤는데",
          "speaker_id": "2",
          "time": "20210829 19:06"
        },
        {
          "id": "MDRW2100022913.1.16",
          "form": "애를 진짜 개보다 못하게 대함",
          "original_form": "애를 진짜 개보다 못하게 대함",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022913.1.17",
          "form": "ㅇㅇㅇㅇ엉",
          "original_form": "ㅇㅇㅇㅇ엉",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022913.1.18",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022913.1.19",
          "form": "그래놓고 피해요 막 이 난리",
          "original_form": "그래놓고 피해요 막 이 난리",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022913.1.20",
          "form": "엄마때리고욕하는",
          "original_form": "엄마때리고욕하는",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022913.1.21",
          "form": "남자야들도",
          "original_form": "남자야들도",
          "speaker_id": "1",
          "time": "20210829 19:07"
        },
        {
