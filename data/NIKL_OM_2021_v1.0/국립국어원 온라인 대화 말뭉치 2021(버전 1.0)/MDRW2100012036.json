      "utterance": [
        {
          "id": "MDRW2100012036.1.1",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100012036.1.2",
          "form": "이제 먹어야해요~",
          "original_form": "이제 먹어야해요~",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012036.1.3",
          "form": "메뉴 고민중입니다",
          "original_form": "메뉴 고민중입니다",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012036.1.4",
          "form": "저는 카레에 김이예요",
          "original_form": "저는 카레에 김이예요",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012036.1.5",
          "form": "카레에 김싸드시나요?",
          "original_form": "카레에 김싸드시나요?",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012036.1.6",
          "form": "카레 맛있겠네요",
          "original_form": "카레 맛있겠네요",
          "speaker_id": "1",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012036.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.8",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.9",
          "form": "애땜에 맨날 짜장만하다가",
          "original_form": "애땜에 맨날 짜장만하다가",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.10",
          "form": "김이 질척거리는게 싫어서ㅋㅋ",
          "original_form": "김이 질척거리는게 싫어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.11",
          "form": "카레 먹고싶어서 카레했어요",
          "original_form": "카레 먹고싶어서 카레했어요",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.12",
          "form": "아...김이랑 카레 조합 좋아요 ㅎ",
          "original_form": "아...김이랑 카레 조합 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.13",
          "form": "카레, 짜장 좋아하시나봐요",
          "original_form": "카레, 짜장 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.14",
          "form": "하긴 김이랑은 머든 맛나긴해요 ㅎ",
          "original_form": "하긴 김이랑은 머든 맛나긴해요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100012036.1.15",
          "form": "짜장은별로 안좋아해요",
          "original_form": "짜장은별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012036.1.16",
          "form": "김도 좋아하시나봐요",
          "original_form": "김도 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012036.1.17",
          "form": "애가 좋아해서 ㅎㅎ",
          "original_form": "애가 좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012036.1.18",
          "form": "카레는 맵다고 안먹더라구요",
          "original_form": "카레는 맵다고 안먹더라구요",
          "speaker_id": "2",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012036.1.19",
          "form": "저는 짜장해먹을때",
          "original_form": "저는 짜장해먹을때",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012036.1.20",
          "form": "고추랑 마늘 기름내서",
          "original_form": "고추랑 마늘 기름내서",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012036.1.21",
          "form": "짜장 소스볶아서 해먹으니까",
          "original_form": "짜장 소스볶아서 해먹으니까",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012036.1.22",
          "form": "맛있더라고요",
          "original_form": "맛있더라고요",
          "speaker_id": "1",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012036.1.23",
          "form": "매콤하게드시는군요",
          "original_form": "매콤하게드시는군요",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012036.1.24",
          "form": "와 맛있겠네요",
          "original_form": "와 맛있겠네요",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012036.1.25",
          "form": "매운건못먹지만 고추향을 입히는거예요",
          "original_form": "매운건못먹지만 고추향을 입히는거예요",
          "speaker_id": "1",
          "time": "20210824 18:40"
        }
      ]
