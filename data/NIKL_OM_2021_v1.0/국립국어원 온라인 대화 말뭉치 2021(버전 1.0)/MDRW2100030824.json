          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.30",
          "form": "최근에 바꿔가지고 플립으로 바꾸고 싶었는데",
          "original_form": "최근에 바꿔가지고 플립으로 바꾸고 싶었는데",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.31",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.32",
          "form": "완전 얼리어덥터시네요",
          "original_form": "완전 얼리어덥터시네요",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.33",
          "form": "아 저 참고로 쓰리잡이라가지고",
          "original_form": "아 저 참고로 쓰리잡이라가지고",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.34",
          "form": "본업은 기획자고",
          "original_form": "본업은 기획자고",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.35",
          "form": "아 z플립이쁘죠",
          "original_form": "아 z플립이쁘죠",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.36",
          "form": "흐억",
          "original_form": "흐억",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.37",
          "form": "부업은 보험도 하구, 휴대폰도 해요",
          "original_form": "부업은 보험도 하구, 휴대폰도 해요",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030824.1.38",
          "form": "ㅋ ㅋ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋ ㅋ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030824.1.39",
