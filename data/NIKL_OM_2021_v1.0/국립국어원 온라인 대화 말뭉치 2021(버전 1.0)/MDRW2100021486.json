        },
        {
          "id": "MDRW2100021486.1.2",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021486.1.3",
          "form": "네~ 최근에 킹덤 아신전봤어요 name1은요",
          "original_form": "네~ 최근에 킹덤 아신전봤어요 &name1&은요",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021486.1.4",
          "form": "저는 키싱부스2요!",
          "original_form": "저는 키싱부스2요!",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021486.1.5",
          "form": "3보기 위해 열심히 봤어요ㅋㅋ",
          "original_form": "3보기 위해 열심히 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021486.1.6",
          "form": "우와 전 키싱부스 3부터 거꾸로 봐요",
          "original_form": "우와 전 키싱부스 3부터 거꾸로 봐요",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021486.1.7",
          "form": "오오 3보셨군요?!",
          "original_form": "오오 3보셨군요?!",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021486.1.8",
          "form": "네네 근데 틀어두고 다른일 하고 해서다시정주행해여해요",
          "original_form": "네네 근데 틀어두고 다른일 하고 해서다시정주행해여해요",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021486.1.9",
          "form": "비슷하게 보실 것 같네욥!!",
          "original_form": "비슷하게 보실 것 같네욥!!",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100021486.1.10",
          "form": "그러네요 ㅋㅋ",
          "original_form": "그러네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021486.1.11",
          "form": "왕대륙 나오는 장난스런키스도 좋아요",
          "original_form": "왕대륙 나오는 장난스런키스도 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021486.1.12",
          "form": "유치한데 재미나요",
          "original_form": "유치한데 재미나요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021486.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021486.1.14",
          "form": "나의 소녀시대로 왕대륙에 빠졌었죠ㅋㅋ",
          "original_form": "나의 소녀시대로 왕대륙에 빠졌었죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021486.1.15",
          "form": "오 그것도 봤어요ㅠ 유치잼 ㅋㅋㅋ",
          "original_form": "오 그것도 봤어요ㅠ 유치잼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100021486.1.16",
          "form": "맞아욬ㅋㄲㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㄲㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100021486.1.17",
          "form": "대만이 은근 하이틴이 유치해도 좋더라구요ㅋㅋ",
          "original_form": "대만이 은근 하이틴이 유치해도 좋더라구요ㅋㅋ",
