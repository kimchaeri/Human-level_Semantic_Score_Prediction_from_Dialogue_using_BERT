      "utterance": [
        {
          "id": "MDRW2100025258.1.1",
          "form": "비냉은 물냉 먹다가 가끔 한번",
          "original_form": "비냉은 물냉 먹다가 가끔 한번",
          "speaker_id": "2",
          "time": "20210901 03:20"
        },
        {
          "id": "MDRW2100025258.1.2",
          "form": "먹어주는거죠",
          "original_form": "먹어주는거죠",
          "speaker_id": "2",
          "time": "20210901 03:20"
        },
        {
          "id": "MDRW2100025258.1.3",
          "form": "혹시 평양냉면 드셔보셨어요?",
          "original_form": "혹시 평양냉면 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210901 03:20"
        },
        {
          "id": "MDRW2100025258.1.4",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025258.1.5",
          "form": "네 평양냉면",
          "original_form": "네 평양냉면",
          "speaker_id": "2",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025258.1.6",
          "form": "드셔보셨구나!",
          "original_form": "드셔보셨구나!",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025258.1.7",
          "form": "음 먹어보긴하는데 가끔",
          "original_form": "음 먹어보긴하는데 가끔",
          "speaker_id": "2",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025258.1.8",
          "form": "굳이 이 돈으로...",
          "original_form": "굳이 이 돈으로...",
          "speaker_id": "2",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025258.1.9",
          "form": "전 못먹어봤어요 ㅜ",
          "original_form": "전 못먹어봤어요 ㅜ",
          "speaker_id": "1",
          "time": "20210901 03:21"
        },
        {
          "id": "MDRW2100025258.1.10",
          "form": "느낌이라서요 ㅋㅋ",
          "original_form": "느낌이라서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.11",
          "form": "아 왜요? ㅋㅋㅋ ㅋㅋㅋ",
          "original_form": "아 왜요? ㅋㅋㅋ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.12",
          "form": "보통 평양냉면 12000정도 합니다",
          "original_form": "보통 평양냉면 12000정도 합니다",
          "speaker_id": "2",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.13",
          "form": "근데 싱겁고 음",
          "original_form": "근데 싱겁고 음",
          "speaker_id": "2",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.14",
          "form": "아니 예찬론자들이 있잖아요 ㅋㅋㅋ",
          "original_form": "아니 예찬론자들이 있잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.15",
          "form": "그거 약간 비냉쟁이들 같은 느낌 같은데요 ㅋㅋ",
          "original_form": "그거 약간 비냉쟁이들 같은 느낌 같은데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.16",
          "form": "티비에도 많이 나오고 ㅋㅋ 그래서 궁금했어요 ㅋㅋㅋ",
          "original_form": "티비에도 많이 나오고 ㅋㅋ 그래서 궁금했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:22"
        },
        {
          "id": "MDRW2100025258.1.17",
          "form": "제가사는곳엔 없어서 ㅋ가게가 ㅋㅋㅋ",
          "original_form": "제가사는곳엔 없어서 ㅋ가게가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025258.1.18",
          "form": "굳이니까",
          "original_form": "굳이니까",
          "speaker_id": "2",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025258.1.19",
          "form": "그냥 물냉 드세요!!!",
          "original_form": "그냥 물냉 드세요!!!",
          "speaker_id": "2",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025258.1.20",
          "form": "굳이 먹지 않는걸로 ㅋㅋㅋㅋㅋ",
          "original_form": "굳이 먹지 않는걸로 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025258.1.21",
          "form": "나중에 기회 있어서",
          "original_form": "나중에 기회 있어서",
          "speaker_id": "2",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025258.1.22",
          "form": "드시면 무슨 말인지",
          "original_form": "드시면 무슨 말인지",
          "speaker_id": "2",
          "time": "20210901 03:23"
        },
        {
          "id": "MDRW2100025258.1.23",
          "form": "알게 되실겁니다",
          "original_form": "알게 되실겁니다",
          "speaker_id": "2",
          "time": "20210901 03:23"
        },
