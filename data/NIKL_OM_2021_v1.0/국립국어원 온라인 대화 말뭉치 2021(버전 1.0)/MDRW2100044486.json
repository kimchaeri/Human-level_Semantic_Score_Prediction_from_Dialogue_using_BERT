      "utterance": [
        {
          "id": "MDRW2100044486.1.1",
          "form": "요즘 사신거 잇으세요?",
          "original_form": "요즘 사신거 잇으세요?",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044486.1.2",
          "form": "최근에 가을옷 샀어요!",
          "original_form": "최근에 가을옷 샀어요!",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044486.1.3",
          "form": "우아 어떤거요",
          "original_form": "우아 어떤거요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044486.1.4",
          "form": "후드티 샀어요 후드맨투맨 좋아해서요 ㅎㅎ",
          "original_form": "후드티 샀어요 후드맨투맨 좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044486.1.5",
          "form": "오 !! 무슨색이요?",
          "original_form": "오 !! 무슨색이요?",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044486.1.6",
          "form": "아이보리색 샀어요 ㅎㅎ",
          "original_form": "아이보리색 샀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044486.1.7",
          "form": "귀엽겟다",
          "original_form": "귀엽겟다",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044486.1.8",
          "form": "전 인형삿어요",
          "original_form": "전 인형삿어요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044486.1.9",
          "form": "오 어떤 인형이요?",
          "original_form": "오 어떤 인형이요?",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044486.1.10",
          "form": "쿼카요!!!!ㅋㅋㅋㅋ 아세요?",
          "original_form": "쿼카요!!!!ㅋㅋㅋㅋ 아세요?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044486.1.11",
          "form": "오 ㅋㅋ 그 다이노탱 사셨나요?!",
          "original_form": "오 ㅋㅋ 그 다이노탱 사셨나요?!",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044486.1.12",
          "form": "행복한 쿼카인형 치면 나와요 ㅋㅋㅋㅋ",
          "original_form": "행복한 쿼카인형 치면 나와요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044486.1.13",
          "form": "와 넘 귀엽네요 저도 사고싶어요 ㅋㅋ",
          "original_form": "와 넘 귀엽네요 저도 사고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044486.1.14",
          "form": "ㅋㅋㅋㅋㅋ 닉넴이 쿼카라서 ㅋㅋㅋ반갑",
          "original_form": "ㅋㅋㅋㅋㅋ 닉넴이 쿼카라서 ㅋㅋㅋ반갑",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044486.1.15",
          "form": "쿼카 좋아해요 귀여워서요 그래서 최근에 다이노탱 전시회가서 쿼카 물건도 샀네요 ㅋㅋ",
          "original_form": "쿼카 좋아해요 귀여워서요 그래서 최근에 다이노탱 전시회가서 쿼카 물건도 샀네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044486.1.16",
          "form": "오 ..다이노탱도 찾아보니 귀여워요",
          "original_form": "오 ..다이노탱도 찾아보니 귀여워요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044486.1.17",
          "form": "전시회도 따로잇나요",
          "original_form": "전시회도 따로잇나요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044486.1.18",
          "form": "네 저번에 있었어요.. 담에도 한번 더 하지않을까 싶네요!",
          "original_form": "네 저번에 있었어요.. 담에도 한번 더 하지않을까 싶네요!",
