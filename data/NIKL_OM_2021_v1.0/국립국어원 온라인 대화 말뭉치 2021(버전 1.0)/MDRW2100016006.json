      "utterance": [
        {
          "id": "MDRW2100016006.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100016006.1.2",
          "form": "아침 먹었고 이제 점심시간이 가까워지네요",
          "original_form": "아침 먹었고 이제 점심시간이 가까워지네요",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016006.1.3",
          "form": "점심메뉴 정하셨냐요",
          "original_form": "점심메뉴 정하셨냐요",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016006.1.4",
          "form": "점심 고민중이에요, 샌드위치를 먹을지 쌀국수를 먹을지요ㅋㅋ",
          "original_form": "점심 고민중이에요, 샌드위치를 먹을지 쌀국수를 먹을지요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016006.1.5",
          "form": "쌀국수 좋아하세요?",
          "original_form": "쌀국수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100016006.1.6",
          "form": "저는 쌀국수 쿨타임이 있어서 주기적으로 먹어요!ㅎㅎ",
          "original_form": "저는 쌀국수 쿨타임이 있어서 주기적으로 먹어요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016006.1.7",
          "form": "저는 쌀국수의 고수가 좀 어러워서 ㅎ",
          "original_form": "저는 쌀국수의 고수가 좀 어러워서 ㅎ",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016006.1.8",
          "form": "고수도 좋아하시나요?",
          "original_form": "고수도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016006.1.9",
          "form": "아직 고수를 직접적으로 못 먹어봐서요ㅋㅋ 쌀국수는 고수 안 넣어먹는 편이에요!",
          "original_form": "아직 고수를 직접적으로 못 먹어봐서요ㅋㅋ 쌀국수는 고수 안 넣어먹는 편이에요!",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016006.1.10",
          "form": "아무래도 그렇죠ㅎ",
          "original_form": "아무래도 그렇죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016006.1.11",
          "form": "전 고수 뭣도 모르고 씹었다가 그대로 뱉었거든요",
          "original_form": "전 고수 뭣도 모르고 씹었다가 그대로 뱉었거든요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016006.1.12",
          "form": "약간 퐁퐁맛..",
          "original_form": "약간 퐁퐁맛..",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016006.1.13",
          "form": "유전자에 따라 퐁퐁맛과 고수맛으로 갈린다고 하더라구요ㅋㅋㅋ",
          "original_form": "유전자에 따라 퐁퐁맛과 고수맛으로 갈린다고 하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016006.1.14",
          "form": "name2은 어떤 쪽인거 같으세요",
          "original_form": "&name2&은 어떤 쪽인거 같으세요",
