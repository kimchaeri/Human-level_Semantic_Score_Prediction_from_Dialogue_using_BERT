      "utterance": [
        {
          "id": "MDRW2100022612.1.1",
          "form": "3시간이나? 시켜요..",
          "original_form": "3시간이나? 시켜요..",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022612.1.2",
          "form": "그냥 알아서 사람도 운동 될듯..",
          "original_form": "그냥 알아서 사람도 운동 될듯..",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022612.1.3",
          "form": "ㅋㅋㄱㅋㄱ진짜요 심지어 뛰는 개들도 있잖아요 ㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㄱ진짜요 심지어 뛰는 개들도 있잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022612.1.4",
          "form": "다이어트가 절로 되지않을까요 ㅋㅋ",
          "original_form": "다이어트가 절로 되지않을까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022612.1.5",
          "form": "ㅋㅋㅋ 예전엔 개랑 같이 운동하는게 꿈이었는데..",
          "original_form": "ㅋㅋㅋ 예전엔 개랑 같이 운동하는게 꿈이었는데..",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022612.1.6",
          "form": "막 바구니에 강아지 넣고 자전거 타는거.. ㅋㅋ",
          "original_form": "막 바구니에 강아지 넣고 자전거 타는거.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022612.1.7",
          "form": "아 맞아요 ㅋㅋㅋㅋ 근데 그런건 다 허상인거같아요",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ 근데 그런건 다 허상인거같아요",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022612.1.8",
          "form": "강아지가 가만히 있질 않던데욬ㅋㄱㅋ",
          "original_form": "강아지가 가만히 있질 않던데욬ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022612.1.9",
          "form": "한강가며 은근히 많아요..",
          "original_form": "한강가며 은근히 많아요..",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022612.1.10",
          "form": "그거 보고 저도.. ㅋㅋㅋ 교육을 따로시킨건지",
          "original_form": "그거 보고 저도.. ㅋㅋㅋ 교육을 따로시킨건지",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022612.1.11",
          "form": "엥 진짜요 ㅋㅋㅋ ?",
          "original_form": "엥 진짜요 ㅋㅋㅋ ?",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022612.1.12",
          "form": "좀 강아지한테도 스트레스일거같은데 ㅋㅋㅋ",
          "original_form": "좀 강아지한테도 스트레스일거같은데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022612.1.13",
          "form": "교육을 시키나보네요 ,,",
          "original_form": "교육을 시키나보네요 ,,",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022612.1.14",
          "form": "밖에서 런닝 같은거 하시나요?",
          "original_form": "밖에서 런닝 같은거 하시나요?",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022612.1.15",
          "form": "아니요 ! 집 주변에 공원이 없어서 ㅠㅠ",
          "original_form": "아니요 ! 집 주변에 공원이 없어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022612.1.16",
          "form": "근데 또 걷는건 좋아해서 ㅋㅋ 그냥 집 주변 걸어다녀요 ㅋㅋ",
          "original_form": "근데 또 걷는건 좋아해서 ㅋㅋ 그냥 집 주변 걸어다녀요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022612.1.17",
          "form": "늙으면 걷는게.최고 운동 같아요.. ㅋㅋㅋ",
          "original_form": "늙으면 걷는게.최고 운동 같아요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022612.1.18",
          "form": "걷다가 다친사람은 없으니까..",
          "original_form": "걷다가 다친사람은 없으니까..",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022612.1.19",
          "form": "맞아요 ㅋㅋㅋ 또 뛰는건 매일하면 무릎 나간다던데요 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ 또 뛰는건 매일하면 무릎 나간다던데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022612.1.20",
          "form": "또 운동은 해야되고 ㅋㅋ 대신 많이 걸어야되죠 ㅠ",
          "original_form": "또 운동은 해야되고 ㅋㅋ 대신 많이 걸어야되죠 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022612.1.21",
          "form": "요즘 비와서 밖에서 운동을 못하니.. 갑갑하네요..",
          "original_form": "요즘 비와서 밖에서 운동을 못하니.. 갑갑하네요..",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022612.1.22",
          "form": "맞아요 ㅠㅠ 또 장마라던걸요 ???",
          "original_form": "맞아요 ㅠㅠ 또 장마라던걸요 ???",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022612.1.23",
          "form": "전 그래서 실내자전거타요 요즘은 ㅎㅎ",
          "original_form": "전 그래서 실내자전거타요 요즘은 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022612.1.24",
          "form": "와.. 그런거 있음. 엄청 편하겠네요.. \r\n집에서 시간날때하면 되고..",
          "original_form": "와.. 그런거 있음. 엄청 편하겠네요.. \r\n집에서 시간날때하면 되고..",
          "speaker_id": "1",
          "time": "20210829 16:47"
        }
      ]
