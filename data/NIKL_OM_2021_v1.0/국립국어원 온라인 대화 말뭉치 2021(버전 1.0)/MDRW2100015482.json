      "utterance": [
        {
          "id": "MDRW2100015482.1.1",
          "form": "아빠가 텔레비전 꽉 잡고 있다,",
          "original_form": "아빠가 텔레비전 꽉 잡고 있다,",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015482.1.2",
          "form": "ㅋㅋㅋㅋ 뭐 보는데?",
          "original_form": "ㅋㅋㅋㅋ 뭐 보는데?",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015482.1.3",
          "form": "뭐겠어. 먄날 스포츠 채널이지.",
          "original_form": "뭐겠어. 먄날 스포츠 채널이지.",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015482.1.4",
          "form": "또 야구 보는 구나~~",
          "original_form": "또 야구 보는 구나~~",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015482.1.5",
          "form": "근데 지금 야구 끝나지 않았어?",
          "original_form": "근데 지금 야구 끝나지 않았어?",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015482.1.6",
          "form": "티비에서 하이라이트 다시보기는 해주잖아.",
          "original_form": "티비에서 하이라이트 다시보기는 해주잖아.",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015482.1.7",
          "form": "맨날 다시보기로 또 보는 거지.",
          "original_form": "맨날 다시보기로 또 보는 거지.",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015482.1.8",
          "form": "ㅋㅋㅋㅋ티비 뺏꼈으면 엄마는 폰으로 넷플 봐",
          "original_form": "ㅋㅋㅋㅋ티비 뺏꼈으면 엄마는 폰으로 넷플 봐",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015482.1.9",
          "form": "너는 뭐 보는데",
          "original_form": "너는 뭐 보는데",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015482.1.10",
          "form": "난 넷플릭스에서 다큐 보는 중",
          "original_form": "난 넷플릭스에서 다큐 보는 중",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015482.1.11",
          "form": "밥 로스 알지?? 그 그림 그리는 밥 아저씨!",
          "original_form": "밥 로스 알지?? 그 그림 그리는 밥 아저씨!",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015482.1.12",
          "form": "그 사람 다큐 나왔길래 보고 있어",
          "original_form": "그 사람 다큐 나왔길래 보고 있어",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015482.1.13",
          "form": "그 화가 알지.",
          "original_form": "그 화가 알지.",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015482.1.14",
          "form": "그림 그리는 거 보여주는 다큐멘터리야?",
          "original_form": "그림 그리는 거 보여주는 다큐멘터리야?",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015482.1.15",
          "form": "ㅋㅋㅋ아닝",
          "original_form": "ㅋㅋㅋ아닝",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015482.1.16",
          "form": "밥 아저씨의 뒷 이야기를 담은 다큐야",
          "original_form": "밥 아저씨의 뒷 이야기를 담은 다큐야",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015482.1.17",
          "form": "나도 아직 초반 보는 중이라 잘 몰라",
          "original_form": "나도 아직 초반 보는 중이라 잘 몰라",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015482.1.18",
          "form": "그림 그리는 티비 쇼도 넷플에 올라오면 좋겠다ㅠ",
          "original_form": "그림 그리는 티비 쇼도 넷플에 올라오면 좋겠다ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015482.1.19",
          "form": "그건 80년대 쇼일 텐데",
          "original_form": "그건 80년대 쇼일 텐데",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015482.1.20",
          "form": "막내가 그거 유튜브에 있대.",
          "original_form": "막내가 그거 유튜브에 있대.",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015482.1.21",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015482.1.22",
          "form": "다큐 다 보고 찾아 봐야게ㅛ다",
          "original_form": "다큐 다 보고 찾아 봐야게ㅛ다",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015482.1.23",
          "form": "다큐 보고 있는데 밥 아저씨도 사업하다가 문제가 많았나봐",
          "original_form": "다큐 보고 있는데 밥 아저씨도 사업하다가 문제가 많았나봐",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015482.1.24",
          "form": "그런 내용이네 다큐가...",
          "original_form": "그런 내용이네 다큐가...",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015482.1.25",
          "form": "엄마도 나중에 한번 봐야겠다.",
          "original_form": "엄마도 나중에 한번 봐야겠다.",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015482.1.26",
          "form": "뒷내용이 궁금해.",
          "original_form": "뒷내용이 궁금해.",
          "speaker_id": "2",
          "time": "20210825 23:28"
        }
      ]
    }
  ]
