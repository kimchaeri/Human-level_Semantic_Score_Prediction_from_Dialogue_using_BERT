      "utterance": [
        {
          "id": "MDRW2100008484.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008484.1.2",
          "form": "중고거래 하죠! 책같은거는 거의 중고로 사요",
          "original_form": "중고거래 하죠! 책같은거는 거의 중고로 사요",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008484.1.3",
          "form": "책 수요 있나뇨?",
          "original_form": "책 수요 있나뇨?",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008484.1.4",
          "form": "네! 특히 교재같은거요",
          "original_form": "네! 특히 교재같은거요",
          "speaker_id": "2",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100008484.1.5",
          "form": "대학교재의 경우 한학기에 쓰고 안쓰는데 사면 아까워요",
          "original_form": "대학교재의 경우 한학기에 쓰고 안쓰는데 사면 아까워요",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008484.1.6",
          "form": "네 교수들 책 사라고",
          "original_form": "네 교수들 책 사라고",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100008484.1.7",
          "form": "맞아요 돈 아까워서 중고나라나 알라딘에서 사요",
          "original_form": "맞아요 돈 아까워서 중고나라나 알라딘에서 사요",
          "speaker_id": "2",
          "time": "20210822 16:33"
        },
        {
          "id": "MDRW2100008484.1.8",
          "form": "있던가요?",
          "original_form": "있던가요?",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008484.1.9",
          "form": "네! 어떻게든 찾아보면 있더라구요",
          "original_form": "네! 어떻게든 찾아보면 있더라구요",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008484.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008484.1.11",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008484.1.12",
          "form": "name1님도 중고거래 하세요?",
          "original_form": "&name1&님도 중고거래 하세요?",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008484.1.13",
          "form": "네 하죠",
          "original_form": "네 하죠",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008484.1.14",
          "form": "주로 어떤거 거래해요?",
          "original_form": "주로 어떤거 거래해요?",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008484.1.15",
          "form": "공기청정기 최근요",
          "original_form": "공기청정기 최근요",
          "speaker_id": "1",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008484.1.16",
          "form": "오~ 좋네요",
          "original_form": "오~ 좋네요",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008484.1.17",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "1",
