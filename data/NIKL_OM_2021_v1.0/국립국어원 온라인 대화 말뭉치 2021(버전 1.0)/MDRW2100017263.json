        {
          "id": "MDRW2100017263.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017263.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 19:46"
        },
        {
          "id": "MDRW2100017263.1.3",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017263.1.4",
          "form": "여행 좋아해요 ㅎㅎ국내해외다좋아요",
          "original_form": "여행 좋아해요 ㅎㅎ국내해외다좋아요",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017263.1.5",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017263.1.6",
          "form": "다양하게 많지만 젤 기억에남는곳은 국내는 여수 해외는 미국이요 ㅎㅎ",
          "original_form": "다양하게 많지만 젤 기억에남는곳은 국내는 여수 해외는 미국이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017263.1.7",
          "form": "미국 너무가보고싶어요",
          "original_form": "미국 너무가보고싶어요",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017263.1.8",
          "form": "미국 좋았어요 ㅜㅜ 특유의 바이브! 또가고싶은나라에요",
          "original_form": "미국 좋았어요 ㅜㅜ 특유의 바이브! 또가고싶은나라에요",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017263.1.9",
          "form": "뉴욕 가고싶네요",
          "original_form": "뉴욕 가고싶네요",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100017263.1.10",
          "form": "뉴욕좋죠~ 혹시 국내여행은 어디가보셨어요?",
          "original_form": "뉴욕좋죠~ 혹시 국내여행은 어디가보셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100017263.1.11",
          "form": "제주 너무좋아요~",
          "original_form": "제주 너무좋아요~",
          "speaker_id": "1",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100017263.1.12",
          "form": "아 그쵸! 제주도 힐링만땅 ㅜ",
          "original_form": "아 그쵸! 제주도 힐링만땅 ㅜ",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100017263.1.13",
          "form": "섭지코지 넘좋아요",
          "original_form": "섭지코지 넘좋아요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100017263.1.14",
          "form": "노 첨들어보는데 메모해둬야겠군요!",
          "original_form": "노 첨들어보는데 메모해둬야겠군요!",
