      "utterance": [
        {
          "id": "MDRW2100012481.1.1",
          "form": "여행은 항상좋았지",
          "original_form": "여행은 항상좋았지",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012481.1.2",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100012481.1.3",
          "form": "나는 상주도 좋았어~생각해보니",
          "original_form": "나는 상주도 좋았어~생각해보니",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012481.1.4",
          "form": "여유졌어 얘들 잘 놀고",
          "original_form": "여유졌어 얘들 잘 놀고",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100012481.1.5",
          "form": "제주도 조차 좋았다",
          "original_form": "제주도 조차 좋았다",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012481.1.6",
          "form": "엉맞다",
          "original_form": "엉맞다",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012481.1.7",
          "form": "메뚜기 잡고",
          "original_form": "메뚜기 잡고",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012481.1.8",
          "form": "사이판도 좋았던거 같다",
          "original_form": "사이판도 좋았던거 같다",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012481.1.9",
          "form": "사이판도 기억에 남는다",
          "original_form": "사이판도 기억에 남는다",
          "speaker_id": "1",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012481.1.10",
          "form": "같이 갔던곳이 사이판 필리핀 또 있나?",
          "original_form": "같이 갔던곳이 사이판 필리핀 또 있나?",
          "speaker_id": "1",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100012481.1.11",
          "form": "없는거같네",
          "original_form": "없는거같네",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012481.1.12",
          "form": "아가씨때 일본여행도 좋았어",
          "original_form": "아가씨때 일본여행도 좋았어",
          "speaker_id": "2",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012481.1.13",
          "form": "일본 어디 갔었어?",
          "original_form": "일본 어디 갔었어?",
          "speaker_id": "1",
          "time": "20210825 07:00"
        },
        {
          "id": "MDRW2100012481.1.14",
          "form": "도쿄, 오사카",
          "original_form": "도쿄, 오사카",
          "speaker_id": "2",
          "time": "20210825 09:21"
        },
        {
          "id": "MDRW2100012481.1.15",
          "form": "참..1학년에 오키나와여행 좋았다",
          "original_form": "참..1학년에 오키나와여행 좋았다",
          "speaker_id": "2",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012481.1.16",
          "form": "오키나와~한번 가보고싶네",
          "original_form": "오키나와~한번 가보고싶네",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012481.1.17",
          "form": "꼭가봐 너무 좋았다",
          "original_form": "꼭가봐 너무 좋았다",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012481.1.18",
          "form": "다음에 하와이 가보고 싶다",
          "original_form": "다음에 하와이 가보고 싶다",
          "speaker_id": "2",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012481.1.19",
          "form": "하와이도 좋지",
          "original_form": "하와이도 좋지",
          "speaker_id": "1",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012481.1.20",
          "form": "ㅇㅇ비싸겠지 다녀온사람들 완전만족하데",
          "original_form": "ㅇㅇ비싸겠지 다녀온사람들 완전만족하데",
          "speaker_id": "2",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012481.1.21",
          "form": "숙박시설이 오래되서",
          "original_form": "숙박시설이 오래되서",
          "speaker_id": "1",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012481.1.22",
          "form": "코로나 끝남 여행 장난아니겠다",
          "original_form": "코로나 끝남 여행 장난아니겠다",
          "speaker_id": "1",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012481.1.23",
          "form": "ㅇㅇ더비싸지겠다",
          "original_form": "ㅇㅇ더비싸지겠다",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012481.1.24",
          "form": "엄청 비쌀듯",
          "original_form": "엄청 비쌀듯",
          "speaker_id": "1",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012481.1.25",
          "form": "하와이는 숙박시설이 그렇긴 하겠네",
          "original_form": "하와이는 숙박시설이 그렇긴 하겠네",
          "speaker_id": "2",
          "time": "20210825 09:27"
        }
      ]
    }
