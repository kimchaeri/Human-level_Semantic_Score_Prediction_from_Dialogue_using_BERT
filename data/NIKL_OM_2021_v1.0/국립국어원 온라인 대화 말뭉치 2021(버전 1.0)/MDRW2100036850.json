      "utterance": [
        {
          "id": "MDRW2100036850.1.1",
          "form": "첫째 게임하나??",
          "original_form": "첫째 게임하나??",
          "speaker_id": "1",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036850.1.2",
          "form": "요즘 무한의 계단에 푹 빠졌습니다",
          "original_form": "요즘 무한의 계단에 푹 빠졌습니다",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100036850.1.3",
          "form": "첫째랑 같이 다니는 그 작은 아이가 게임을 참 잘한다고 합니다",
          "original_form": "첫째랑 같이 다니는 그 작은 아이가 게임을 참 잘한다고 합니다",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100036850.1.4",
          "form": "이기고 싶어 얼마나 하는지 모릅니다ㅋㅋㅋ",
          "original_form": "이기고 싶어 얼마나 하는지 모릅니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100036850.1.5",
          "form": "ㄹㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 무한의계단",
          "original_form": "ㄹㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 무한의계단",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036850.1.6",
          "form": "그래 우리원도 무한의 계단 매니아다",
          "original_form": "그래 우리원도 무한의 계단 매니아다",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036850.1.7",
          "form": "진짜 손가락 든 튼 해질 거 같음",
          "original_form": "진짜 손가락 든 튼 해질 거 같음",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036850.1.8",
          "form": "ㅋㅋㅋㅋ 그아이 게임을 잘하는군",
          "original_form": "ㅋㅋㅋㅋ 그아이 게임을 잘하는군",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036850.1.9",
          "form": "엄청 잘한대요 뭐 신기한 것도 많이 알고",
          "original_form": "엄청 잘한대요 뭐 신기한 것도 많이 알고",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036850.1.10",
          "form": "그래서 그 아이 엄마가 게임 깔아줌;",
          "original_form": "그래서 그 아이 엄마가 게임 깔아줌;",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.11",
          "form": "오 ㅋㅋㅋㄱㅋㅋㄱ",
          "original_form": "오 ㅋㅋㅋㄱㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.12",
          "form": "게임 깔아 오면 나는 지우고ㅋㅋㅋㅋ",
          "original_form": "게임 깔아 오면 나는 지우고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.13",
          "form": "엄마가 ㅋㅋㅋㄱㅋㄱ",
          "original_form": "엄마가 ㅋㅋㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.14",
          "form": "ㅋㅋㅋㄱㅋㅋㅋ 웃기노 ㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋ 웃기노 ㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.15",
          "form": "창과 방패의 대결이에요ㅋㅋㅋㅋㅋ",
          "original_form": "창과 방패의 대결이에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.16",
          "form": "그엄마는 왜 남애 폰에 게임을 까노",
          "original_form": "그엄마는 왜 남애 폰에 게임을 까노",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.17",
          "form": "마음대로",
          "original_form": "마음대로",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036850.1.18",
          "form": "내가 그런 거 안 해주니",
          "original_form": "내가 그런 거 안 해주니",
          "speaker_id": "2",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.19",
          "form": "우리 원이랑 수도 매일 무한의 계단 땜에 경쟁하거든",
          "original_form": "우리 원이랑 수도 매일 무한의 계단 땜에 경쟁하거든",
          "speaker_id": "1",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.20",
          "form": "첫째가 그 엄마한테 해달라고 부탁하는 듯",
          "original_form": "첫째가 그 엄마한테 해달라고 부탁하는 듯",
          "speaker_id": "2",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.21",
          "form": "맨날 와서 나보고 인증 좀 해달라고",
          "original_form": "맨날 와서 나보고 인증 좀 해달라고",
          "speaker_id": "2",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.22",
          "form": "아 첫째가",
          "original_form": "아 첫째가",
          "speaker_id": "1",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.23",
          "form": "부탁하는 거였구만",
          "original_form": "부탁하는 거였구만",
          "speaker_id": "1",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.24",
          "form": "안 해줘요ㅋㅋㅌㅌ",
          "original_form": "안 해줘요ㅋㅋㅌㅌ",
          "speaker_id": "2",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.25",
          "form": "인증ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "인증ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.26",
          "form": "첫째가 옛날에 그 아이가 아니에요",
          "original_form": "첫째가 옛날에 그 아이가 아니에요",
          "speaker_id": "2",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036850.1.27",
          "form": "하하....",
          "original_form": "하하....",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.28",
          "form": "게임에 눈멈 아이ㅋㅋㅋㅋ",
          "original_form": "게임에 눈멈 아이ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.29",
          "form": "나도 하도 게임 시간 엄마 제발 십분만 엄마 제발 20분만",
          "original_form": "나도 하도 게임 시간 엄마 제발 십분만 엄마 제발 20분만",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.30",
          "form": "계속 전화 와가지고",
          "original_form": "계속 전화 와가지고",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.31",
          "form": "언니 우리 애는 그래서 안 들어와요",
          "original_form": "언니 우리 애는 그래서 안 들어와요",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.32",
          "form": "이제 그냥 12시부터 6시까지 게임 풀어놨음",
          "original_form": "이제 그냥 12시부터 6시까지 게임 풀어놨음",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.34",
          "form": "실시간으로 전화 온다",
          "original_form": "실시간으로 전화 온다",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.36",
          "form": "그래서 다들 반도에서 방황하고 있구만",
          "original_form": "그래서 다들 반도에서 방황하고 있구만",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.37",
          "form": "자기 아파트는 안감",
          "original_form": "자기 아파트는 안감",
          "speaker_id": "1",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.38",
          "form": "많이 내려놔야 합니다ㅋㅋㅋㅋ",
          "original_form": "많이 내려놔야 합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036850.1.39",
          "form": "엄마 눈에 안 띄게 위해서",
          "original_form": "엄마 눈에 안 띄게 위해서",
          "speaker_id": "1",
          "time": "20210831 14:26"
        }
      ]
