      "utterance": [
        {
          "id": "MDRW2100003621.1.1",
          "form": "군대요?",
          "original_form": "군대요?",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003621.1.2",
          "form": "어떨거 같아요?",
          "original_form": "어떨거 같아요?",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003621.1.3",
          "form": "가끔 아빠가 해주는 이야기 들으면",
          "original_form": "가끔 아빠가 해주는 이야기 들으면",
          "speaker_id": "1",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003621.1.4",
          "form": "뭐 시대가 다르긴한데..",
          "original_form": "뭐 시대가 다르긴한데..",
          "speaker_id": "1",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003621.1.5",
          "form": "저런곳이 다 있나 싶은",
          "original_form": "저런곳이 다 있나 싶은",
          "speaker_id": "1",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003621.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.7",
          "form": "그정도까진 아닌데",
          "original_form": "그정도까진 아닌데",
          "speaker_id": "2",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.8",
          "form": "요즘은 또 다르더라구요",
          "original_form": "요즘은 또 다르더라구요",
          "speaker_id": "1",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.9",
          "form": "그닥 좋지 않아요 ㅋㅋ",
          "original_form": "그닥 좋지 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.10",
          "form": "요즘은 천국 아닌가요?",
          "original_form": "요즘은 천국 아닌가요?",
          "speaker_id": "2",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.11",
          "form": "근데 또 전화오는 친구이야기들으면",
          "original_form": "근데 또 전화오는 친구이야기들으면",
          "speaker_id": "1",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.12",
          "form": "폰도 쓰고",
          "original_form": "폰도 쓰고",
          "speaker_id": "2",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.13",
          "form": "이게 뭐하는 짓인지 모르겠다고",
          "original_form": "이게 뭐하는 짓인지 모르겠다고",
          "speaker_id": "1",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.14",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "1",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003621.1.15",
          "form": "막 음.. 제초작업하고",
          "original_form": "막 음.. 제초작업하고",
          "speaker_id": "1",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003621.1.16",
          "form": "저때는",
          "original_form": "저때는",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003621.1.17",
          "form": "2년 꽉 이었고",
          "original_form": "2년 꽉 이었고",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003621.1.18",
          "form": "전 운전병이었어서",
          "original_form": "전 운전병이었어서",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003621.1.19",
          "form": "그냥 운전만 미친듯이 했어요",
          "original_form": "그냥 운전만 미친듯이 했어요",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003621.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:57"
        },
        {
          "id": "MDRW2100003621.1.21",
