      "utterance": [
        {
          "id": "MDRW2100013554.1.1",
          "form": "배달 음식 좋아하시나요??",
          "original_form": "배달 음식 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013554.1.2",
          "form": "좋아해요! 특히 피자 햄버거 ㅋㅋ",
          "original_form": "좋아해요! 특히 피자 햄버거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013554.1.3",
          "form": "뭘 좋아하세요?",
          "original_form": "뭘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013554.1.4",
          "form": "저도 피자 햄버거 치킨 떡볶이 좋아해요 ㅋㅋ",
          "original_form": "저도 피자 햄버거 치킨 떡볶이 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013554.1.5",
          "form": "요즘 로제떡볶이에 꽂혔어요",
          "original_form": "요즘 로제떡볶이에 꽂혔어요",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013554.1.6",
          "form": "아 저도 배떡이랑 엽떡 로제떡볶이 먹어봤어요",
          "original_form": "아 저도 배떡이랑 엽떡 로제떡볶이 먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013554.1.7",
          "form": "배떡 로제떡볶이 떡이 특이하더라구요",
          "original_form": "배떡 로제떡볶이 떡이 특이하더라구요",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013554.1.8",
          "form": "얇으면서 쫄깃쫄깃 ㅋㅋ",
          "original_form": "얇으면서 쫄깃쫄깃 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013554.1.9",
          "form": "배떡 진짜 맛있어요 ㅋㅋ",
          "original_form": "배떡 진짜 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013554.1.10",
          "form": "신전도 맛나더라고요",
          "original_form": "신전도 맛나더라고요",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013554.1.11",
          "form": "그래요? 신전은 아직 못 먹어봤는데 담에 시켜봐야겠어요! ㅋㅋ",
          "original_form": "그래요? 신전은 아직 못 먹어봤는데 담에 시켜봐야겠어요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013554.1.12",
          "form": "네 ㅋㅋ 청년다방도 맛있고",
          "original_form": "네 ㅋㅋ 청년다방도 맛있고",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013554.1.13",
          "form": "스쿨푸드는 그냥 그랬어요 ㅋㅋ",
          "original_form": "스쿨푸드는 그냥 그랬어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013554.1.14",
          "form": "종류별로 다 드셔보셨구나 ㅋㅋㅋ",
          "original_form": "종류별로 다 드셔보셨구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013554.1.15",
          "form": "꽂혀가지고 진짜 배달할때마다 시켰네요 ㅋㅋ",
          "original_form": "꽂혀가지고 진짜 배달할때마다 시켰네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013554.1.16",
          "form": "청년다방 차돌이랑 오징어튀김떡볶이 참 맛있었는데",
          "original_form": "청년다방 차돌이랑 오징어튀김떡볶이 참 맛있었는데",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013554.1.17",
          "form": "거기도 담에 시켜봐야겠어요 ㅋㅋ",
          "original_form": "거기도 담에 시켜봐야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013554.1.18",
          "form": "사실 너무 맛은 있는데 칼로리가 어마어마",
          "original_form": "사실 너무 맛은 있는데 칼로리가 어마어마",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013554.1.19",
          "form": "ㅋㅋㅋㅋ청년다방 다른 맛있는 메뉴가 많네요",
          "original_form": "ㅋㅋㅋㅋ청년다방 다른 맛있는 메뉴가 많네요",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013554.1.20",
          "form": "아 맛있는 건 다 그렇더라구요ㅠㅠ",
          "original_form": "아 맛있는 건 다 그렇더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013554.1.21",
          "form": "다먹어봐야겠어요",
          "original_form": "다먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013554.1.22",
          "form": "맞아요ㅜ 그래도 맛있게 먹으면 0칼로리라고 생각하려고요",
          "original_form": "맞아요ㅜ 그래도 맛있게 먹으면 0칼로리라고 생각하려고요",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013554.1.23",
          "form": "맛있어요 한번 드셔보세요 ㅋㅋ",
          "original_form": "맛있어요 한번 드셔보세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013554.1.24",
          "form": "그쳐 맛있으면 0칼로리 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쳐 맛있으면 0칼로리 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:08"
        }
      ]
