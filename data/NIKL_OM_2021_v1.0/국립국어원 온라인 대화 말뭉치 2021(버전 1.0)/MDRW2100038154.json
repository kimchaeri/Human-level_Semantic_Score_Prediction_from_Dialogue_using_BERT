      "utterance": [
        {
          "id": "MDRW2100038154.1.1",
          "form": "오늘 식사하셨어요?",
          "original_form": "오늘 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038154.1.2",
          "form": "전 미역국 백반을 먹었어요 맛이 심심하고 속은 편안하네요 ㅋㅋ",
          "original_form": "전 미역국 백반을 먹었어요 맛이 심심하고 속은 편안하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038154.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038154.1.4",
          "form": "전 닭갈비어제먹고남은거 데워먹고 오늘 카페가서 테이크아웃해왔어요 ㅋㅋㅋ",
          "original_form": "전 닭갈비어제먹고남은거 데워먹고 오늘 카페가서 테이크아웃해왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038154.1.5",
          "form": "커피만요? 아니면 케이크도요?",
          "original_form": "커피만요? 아니면 케이크도요?",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100038154.1.6",
          "form": "궁금합니다 ㅎㅎㅎ",
          "original_form": "궁금합니다 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100038154.1.7",
          "form": "ㅋㅋㅋ스벅 기프티콘쓰러갔었어요",
          "original_form": "ㅋㅋㅋ스벅 기프티콘쓰러갔었어요",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038154.1.8",
          "form": "저도 좀 더 열심히 해서  후딱 후딱 커피도 먹고 해야되는데",
          "original_form": "저도 좀 더 열심히 해서  후딱 후딱 커피도 먹고 해야되는데",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038154.1.9",
          "form": "돌체밀크티를 마셨어요 시럽완전넣어서 케이크는 다음으로ㅜ",
          "original_form": "돌체밀크티를 마셨어요 시럽완전넣어서 케이크는 다음으로ㅜ",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038154.1.10",
          "form": "좀더 파이팅하겠어요 ㅋㅋ",
          "original_form": "좀더 파이팅하겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038154.1.11",
          "form": "돌체밀크티도 맛있죠 저도 담에 그걸 먹어야겠어요",
          "original_form": "돌체밀크티도 맛있죠 저도 담에 그걸 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038154.1.12",
          "form": "네네 ㅋㅋㅋㅋㅋㅋ받고나면 의욕뿜뿜",
          "original_form": "네네 ㅋㅋㅋㅋㅋㅋ받고나면 의욕뿜뿜",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038154.1.13",
          "form": "근데 시럽더넣으니까 별로였어요ㅜ",
          "original_form": "근데 시럽더넣으니까 별로였어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038154.1.14",
          "form": "연유가 아마 이미 들어가서 안 뇽는게 나을 것 같아요",
          "original_form": "연유가 아마 이미 들어가서 안 뇽는게 나을 것 같아요",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100038154.1.15",
          "form": "네ㅜ맛이 이상해지더라구여",
          "original_form": "네ㅜ맛이 이상해지더라구여",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100038154.1.16",
          "form": "연유메뉴는 노시럽을 하는 걸루 해요 ㅋㅋㅋ",
          "original_form": "연유메뉴는 노시럽을 하는 걸루 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100038154.1.17",
          "form": "매장서 먹으면 먹어보고 시럽을 넣는데",
          "original_form": "매장서 먹으면 먹어보고 시럽을 넣는데",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100038154.1.18",
          "form": "코로나때문에!",
          "original_form": "코로나때문에!",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100038154.1.19",
          "form": "네에ㅜㅜㅜㅜ",
          "original_form": "네에ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100038154.1.20",
          "form": "그니까여 그래도 싸게먹었으니까 위안을..",
          "original_form": "그니까여 그래도 싸게먹었으니까 위안을..",
          "speaker_id": "1",
          "time": "20210902 17:13"
        }
      ]
