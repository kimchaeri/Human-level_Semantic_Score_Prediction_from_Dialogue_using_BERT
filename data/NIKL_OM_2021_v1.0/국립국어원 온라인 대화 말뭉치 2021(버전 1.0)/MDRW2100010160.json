      "utterance": [
        {
          "id": "MDRW2100010160.1.1",
          "form": "카페가면 보통 뭐드세요?",
          "original_form": "카페가면 보통 뭐드세요?",
          "speaker_id": "1",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100010160.1.2",
          "form": "저는 무조건 아아요",
          "original_form": "저는 무조건 아아요",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010160.1.3",
          "form": "저는 커피를 잘안마셔서 아이스초코나 민초먹어요",
          "original_form": "저는 커피를 잘안마셔서 아이스초코나 민초먹어요",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010160.1.4",
          "form": "민초 저두 좋아해요",
          "original_form": "민초 저두 좋아해요",
          "speaker_id": "1",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010160.1.5",
          "form": "정말요? 요즘 민초시장 활발해서 너무 행복해요",
          "original_form": "정말요? 요즘 민초시장 활발해서 너무 행복해요",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010160.1.6",
          "form": "민초과자도 엄청 많이 나와서 좋아요",
          "original_form": "민초과자도 엄청 많이 나와서 좋아요",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010160.1.7",
          "form": "저 시중에 나오는 민초 디저트 다 먹어보는 중이에요ㅋㅋ",
          "original_form": "저 시중에 나오는 민초 디저트 다 먹어보는 중이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010160.1.8",
          "form": "앗 저돈데ㅋㅋ",
          "original_form": "앗 저돈데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010160.1.9",
          "form": "스벅 민초프라푸치노 맛있지않나요?ㅜㅠ",
          "original_form": "스벅 민초프라푸치노 맛있지않나요?ㅜㅠ",
          "speaker_id": "1",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010160.1.10",
          "form": "저 스벅민초만  엄청 사먹었어요",
          "original_form": "저 스벅민초만  엄청 사먹었어요",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010160.1.11",
          "form": "갑자기 저도 민초 먹고싶은데요ㅠㅠ",
          "original_form": "갑자기 저도 민초 먹고싶은데요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010160.1.12",
          "form": "배달시킬까....",
          "original_form": "배달시킬까....",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010160.1.13",
          "form": "근처에 카페 없으세요?",
          "original_form": "근처에 카페 없으세요?",
          "speaker_id": "2",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010160.1.14",
          "form": "집근처에 있는 카페는 민초를 안팔아요",
          "original_form": "집근처에 있는 카페는 민초를 안팔아요",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010160.1.15",
          "form": "아쉽네요ㅜㅡㅜ 비안오면 산책삼아 스벅 다녀오세요!",
          "original_form": "아쉽네요ㅜㅡㅜ 비안오면 산책삼아 스벅 다녀오세요!",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010160.1.16",
          "form": "그럴까요ㅋㅋ 배달의 민족 어플 보고있긴한데...",
          "original_form": "그럴까요ㅋㅋ 배달의 민족 어플 보고있긴한데...",
          "speaker_id": "1",
          "time": "20210823 15:04"
        }
      ]
