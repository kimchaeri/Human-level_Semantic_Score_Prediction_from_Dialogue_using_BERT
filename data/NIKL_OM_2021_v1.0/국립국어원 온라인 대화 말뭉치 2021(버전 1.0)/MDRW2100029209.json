      "utterance": [
        {
          "id": "MDRW2100029209.1.1",
          "form": "대학동기",
          "original_form": "대학동기",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029209.1.2",
          "form": "자만추까지만",
          "original_form": "자만추까지만",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029209.1.3",
          "form": "name2 주무셔야 되는거 아니에요?ㅋㅋㅋ",
          "original_form": "&name2& 주무셔야 되는거 아니에요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029209.1.4",
          "form": "빨리 얘기해주세요",
          "original_form": "빨리 얘기해주세요",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029209.1.5",
          "form": "이것만 듣고 자게",
          "original_form": "이것만 듣고 자게",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029209.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029209.1.7",
          "form": "밥도 아니고 너무 뜸들이신다",
          "original_form": "밥도 아니고 너무 뜸들이신다",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029209.1.8",
          "form": "대학교때 남편이",
          "original_form": "대학교때 남편이",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029209.1.9",
          "form": "저랑 썸타다가 군대를 갔어요ㅋㅋㅋㅋ",
          "original_form": "저랑 썸타다가 군대를 갔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029209.1.10",
          "form": "지금도 있는지 모르겠지만\r\n그땐 백일휴가가 있었거든요",
          "original_form": "지금도 있는지 모르겠지만\r\n그땐 백일휴가가 있었거든요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029209.1.11",
          "form": "있죠",
          "original_form": "있죠",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029209.1.12",
          "form": "나땐 있었음 ㅋㅋㅋ",
          "original_form": "나땐 있었음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029209.1.13",
          "form": "입대하고 백일휴가 나올때까지\r\n매일 같은 시간에 문자가 한 통ㅆㅣㄱ 왔었죠",
          "original_form": "입대하고 백일휴가 나올때까지\r\n매일 같은 시간에 문자가 한 통ㅆㅣㄱ 왔었죠",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029209.1.14",
          "form": "모라구요?",
          "original_form": "모라구요?",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029209.1.15",
          "form": "그냥 일상적인거요ㅋㅋ",
          "original_form": "그냥 일상적인거요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029209.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029209.1.17",
          "form": "예약문자를 걸어두고 갔더라구여",
          "original_form": "예약문자를 걸어두고 갔더라구여",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029209.1.18",
          "form": "무튼 그땐 썸이었으니까",
          "original_form": "무튼 그땐 썸이었으니까",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029209.1.19",
          "form": "휴가나옴 같이 뭐 밥도 먹고\r\n군대에서 연락도 하고 지내다",
          "original_form": "휴가나옴 같이 뭐 밥도 먹고\r\n군대에서 연락도 하고 지내다",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029209.1.20",
          "form": "졸업하고 각자 다른 사람들이랑\r\n연애도 하고 그랬죠 ㅋㅋ",
          "original_form": "졸업하고 각자 다른 사람들이랑\r\n연애도 하고 그랬죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029209.1.21",
          "form": "회사에서 만났나요?",
          "original_form": "회사에서 만났나요?",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029209.1.22",
          "form": "근데 제가 성인되고 첫 연애다운\r\n연애를 하고 아주 큰 상처와 함께\r\n깨달음을 얻고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 제가 성인되고 첫 연애다운\r\n연애를 하고 아주 큰 상처와 함께\r\n깨달음을 얻고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029209.1.23",
          "form": "지금의 남편을 찾게 됩니다",
          "original_form": "지금의 남편을 찾게 됩니다",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029209.1.24",
          "form": "찾아요?",
          "original_form": "찾아요?",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029209.1.25",
          "form": "어케아셨죠",
          "original_form": "어케아셨죠",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029209.1.26",
          "form": "사내연애를 했었죠 제가",
          "original_form": "사내연애를 했었죠 제가",
          "speaker_id": "1",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100029209.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029209.1.28",
          "form": "그런분위기로 가고있자나요",
          "original_form": "그런분위기로 가고있자나요",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029209.1.29",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029209.1.30",
          "form": "어디서 만났겠어요 ㅋㅋㅋ",
          "original_form": "어디서 만났겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029209.1.31",
          "form": "아! 얘가 찐이구나!\r\n나 얘 다시 만나야될거같아!!!",
          "original_form": "아! 얘가 찐이구나!\r\n나 얘 다시 만나야될거같아!!!",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029209.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029209.1.33",
          "form": "하는 생각에 남편과 같이\r\n동반입대했던 친구에게\r\n네이트온으로 근황을 캐물어봅니다ㅋㅋ",
          "original_form": "하는 생각에 남편과 같이\r\n동반입대했던 친구에게\r\n네이트온으로 근황을 캐물어봅니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029209.1.34",
          "form": "여자친구가 있고\r\n아주 잘 만나고 있다고 하더라구여",
          "original_form": "여자친구가 있고\r\n아주 잘 만나고 있다고 하더라구여",
          "speaker_id": "1",
          "time": "20210903 00:28"
        }
      ]
