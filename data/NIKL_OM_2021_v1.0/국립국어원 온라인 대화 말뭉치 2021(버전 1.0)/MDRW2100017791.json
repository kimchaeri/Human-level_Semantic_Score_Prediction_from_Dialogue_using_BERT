      "utterance": [
        {
          "id": "MDRW2100017791.1.1",
          "form": "어. 식구들이랑 같이 먹었어.",
          "original_form": "어. 식구들이랑 같이 먹었어.",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017791.1.2",
          "form": "감자탕 먹고 싶대서 그거 먹었지.",
          "original_form": "감자탕 먹고 싶대서 그거 먹었지.",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017791.1.3",
          "form": "헐 감자탕?? 나도 먹고 싶다~",
          "original_form": "헐 감자탕?? 나도 먹고 싶다~",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017791.1.4",
          "form": "시켜 먹었어?",
          "original_form": "시켜 먹었어?",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017791.1.5",
          "form": "아니. 직접 해줬어.",
          "original_form": "아니. 직접 해줬어.",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017791.1.6",
          "form": "어제부터 얘기해서 미리 등뼈 핏물 빼놓고.",
          "original_form": "어제부터 얘기해서 미리 등뼈 핏물 빼놓고.",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017791.1.7",
          "form": "그거 손 많이 가잖아~~",
          "original_form": "그거 손 많이 가잖아~~",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017791.1.8",
          "form": "그냥 배달 시켜 먹지! 엄마가 고생했겠네",
          "original_form": "그냥 배달 시켜 먹지! 엄마가 고생했겠네",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017791.1.9",
          "form": "그러게.",
          "original_form": "그러게.",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017791.1.10",
          "form": "내일은 라면 먹으라고 할 거야.",
          "original_form": "내일은 라면 먹으라고 할 거야.",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017791.1.11",
          "form": "저녁메뉴로 컵라면 추천합니다",
          "original_form": "저녁메뉴로 컵라면 추천합니다",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017791.1.12",
          "form": "욱개장 사발면 맛있잖아 그거 줘",
          "original_form": "욱개장 사발면 맛있잖아 그거 줘",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017791.1.13",
          "form": "그래, 그래야겠다.",
          "original_form": "그래, 그래야겠다.",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017791.1.14",
          "form": "국 끓이는 것도 육수 내고 재로 손질하고 하다보면 한참 걸려.",
          "original_form": "국 끓이는 것도 육수 내고 재로 손질하고 하다보면 한참 걸려.",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017791.1.15",
          "form": "그럼 원래 요리가 제일 힘든 건데",
          "original_form": "그럼 원래 요리가 제일 힘든 건데",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017791.1.16",
          "form": "그리고 다들 고기 없으면 밥 안 먹잖아",
          "original_form": "그리고 다들 고기 없으면 밥 안 먹잖아",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017791.1.17",
          "form": "다들 고급입맛이라 그래.",
          "original_form": "다들 고급입맛이라 그래.",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017791.1.18",
          "form": "밥도 흰쌀밥만 좋아하고. 잡곡밥 싫어하고.",
          "original_form": "밥도 흰쌀밥만 좋아하고. 잡곡밥 싫어하고.",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017791.1.19",
          "form": "ㅋㅋㅋㅋ나만 현미밥 먹잖아",
          "original_form": "ㅋㅋㅋㅋ나만 현미밥 먹잖아",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017791.1.20",
          "form": "그니까. 현미가 건강에 훨씬 좋고 맛도 고소하기만 한데.",
          "original_form": "그니까. 현미가 건강에 훨씬 좋고 맛도 고소하기만 한데.",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017791.1.21",
          "form": "치킨 시켜주고 말아야지.",
          "original_form": "치킨 시켜주고 말아야지.",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017791.1.22",
          "form": "주말에 하루는 치킨 말고 피자 시켜 먹자 해~~ 엄마 치킨 안 먹잖아",
          "original_form": "주말에 하루는 치킨 말고 피자 시켜 먹자 해~~ 엄마 치킨 안 먹잖아",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017791.1.23",
          "form": "그래 안 그래도 요새 리치골드피자가 먹고 싶었어.",
          "original_form": "그래 안 그래도 요새 리치골드피자가 먹고 싶었어.",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017791.1.24",
          "form": "냉장고에 먹을 것 좀 있어? 반찬거리는?",
          "original_form": "냉장고에 먹을 것 좀 있어? 반찬거리는?",
          "speaker_id": "2",
          "time": "20210826 21:20"
        }
      ]
    }
