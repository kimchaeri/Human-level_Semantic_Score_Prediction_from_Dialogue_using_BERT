      "utterance": [
        {
          "id": "MDRW2100024457.1.1",
          "form": "고양이 키우시나봐요!!",
          "original_form": "고양이 키우시나봐요!!",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024457.1.2",
          "form": "넹!!!!!!",
          "original_form": "넹!!!!!!",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024457.1.3",
          "form": "ㅎㅎ 몇살인가요?",
          "original_form": "ㅎㅎ 몇살인가요?",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024457.1.4",
          "form": "진짜 사진만 보낼 수 있음 자랑하고 싶어용",
          "original_form": "진짜 사진만 보낼 수 있음 자랑하고 싶어용",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024457.1.5",
          "form": "3살됐어요!",
          "original_form": "3살됐어요!",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024457.1.6",
          "form": "전 반려동물은 안키우는데 고양이 영상 엄청 봐요ㅎㅎ",
          "original_form": "전 반려동물은 안키우는데 고양이 영상 엄청 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024457.1.7",
          "form": "동물 키우시나요??",
          "original_form": "동물 키우시나요??",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024457.1.8",
          "form": "ㅜㅠㅠㅠ넘 보고 싶네요",
          "original_form": "ㅜㅠㅠㅠ넘 보고 싶네요",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024457.1.9",
          "form": "오오 매력있죠~",
          "original_form": "오오 매력있죠~",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024457.1.10",
          "form": "부모님도 첨엔 시큰둥하시더니 엄청 이뻐하세요ㅋㅋㅋ",
          "original_form": "부모님도 첨엔 시큰둥하시더니 엄청 이뻐하세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024457.1.11",
          "form": "앟ㅋㅋㅋㅋ 원래 부모님들이 더 좋아하신다고 하더라구요",
          "original_form": "앟ㅋㅋㅋㅋ 원래 부모님들이 더 좋아하신다고 하더라구요",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024457.1.12",
          "form": "유행짤 모든집 국룰인가봐요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "유행짤 모든집 국룰인가봐요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024457.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024457.1.14",
          "form": "고양이들은 무슨생각하는지 모르겠는데",
          "original_form": "고양이들은 무슨생각하는지 모르겠는데",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024457.1.15",
          "form": "가끔 행동하는게 너무 똑똑해요",
          "original_form": "가끔 행동하는게 너무 똑똑해요",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024457.1.16",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024457.1.17",
          "form": "맞아요ㅋㅋㄲ",
          "original_form": "맞아요ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024457.1.18",
          "form": "잔소리하기도하구ㅋㅋ 서운한거 말하기도하고ㅋㅋ",
          "original_form": "잔소리하기도하구ㅋㅋ 서운한거 말하기도하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024457.1.19",
          "form": "은근 애교도 많아요!",
          "original_form": "은근 애교도 많아요!",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024457.1.20",
