      "utterance": [
        {
          "id": "MDRW2100029717.1.1",
          "form": "안녕하세요 가장 좋아하는 음식이 무엇인가요?",
          "original_form": "안녕하세요 가장 좋아하는 음식이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100029717.1.2",
          "form": "저는 달걀이요ㅎㅎ",
          "original_form": "저는 달걀이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100029717.1.3",
          "form": "어떤걸 좋아하시나요?",
          "original_form": "어떤걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100029717.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ달걀관련된건 다 좋아하시는 군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ달걀관련된건 다 좋아하시는 군요",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029717.1.5",
          "form": "저는 등촌칼국수 좋아합니다 드셔보셨나요?",
          "original_form": "저는 등촌칼국수 좋아합니다 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029717.1.6",
          "form": "네ㅎㅎ 찜 프라이 반숙 말이 구운계란까지 더있으려나요",
          "original_form": "네ㅎㅎ 찜 프라이 반숙 말이 구운계란까지 더있으려나요",
          "speaker_id": "2",
          "time": "20210903 12:11"
        },
        {
          "id": "MDRW2100029717.1.7",
          "form": "이름만 들어본거 같아요",
          "original_form": "이름만 들어본거 같아요",
          "speaker_id": "2",
          "time": "20210903 12:11"
        },
        {
          "id": "MDRW2100029717.1.8",
          "form": "헉 안드셔보셨구나",
          "original_form": "헉 안드셔보셨구나",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029717.1.9",
          "form": "그럼 오늘 아침도 달걀 드셨나요?",
          "original_form": "그럼 오늘 아침도 달걀 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029717.1.10",
          "form": "물론이죠 오늘은 말이로 먹었어요",
          "original_form": "물론이죠 오늘은 말이로 먹었어요",
          "speaker_id": "2",
          "time": "20210903 12:13"
        },
        {
          "id": "MDRW2100029717.1.11",
          "form": "어젠 반숙으로 먹었고",
          "original_form": "어젠 반숙으로 먹었고",
          "speaker_id": "2",
          "time": "20210903 12:13"
        },
        {
          "id": "MDRW2100029717.1.12",
          "form": "그제는 간장달걀밥먹었네요ㅎ",
          "original_form": "그제는 간장달걀밥먹었네요ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:13"
        },
        {
          "id": "MDRW2100029717.1.13",
          "form": "와 정말 사랑하시는구나",
          "original_form": "와 정말 사랑하시는구나",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029717.1.14",
          "form": "입맛이 없을때는 프라이만 먹기도 해요",
          "original_form": "입맛이 없을때는 프라이만 먹기도 해요",
          "speaker_id": "2",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029717.1.15",
          "form": "그럼 요즘 속상하시겠어요",
          "original_form": "그럼 요즘 속상하시겠어요",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029717.1.16",
          "form": "달걀값이 엄청 올랐던데",
          "original_form": "달걀값이 엄청 올랐던데",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029717.1.17",
          "form": "달걀값이 많이 올랐죠...",
          "original_form": "달걀값이 많이 올랐죠...",
          "speaker_id": "2",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029717.1.18",
          "form": "아껴먹습니다ㅎㅎ",
          "original_form": "아껴먹습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029717.1.19",
          "form": "그쵸.. 요즘 살떄마다 손이 덜덜떨려요",
          "original_form": "그쵸.. 요즘 살떄마다 손이 덜덜떨려요",
          "speaker_id": "1",
          "time": "20210903 12:15"
        },
        {
          "id": "MDRW2100029717.1.20",
          "form": "저도 자주 먹는데 요즘은 너무 비싸서 삶지도 못합니다ㅜ 한번 삶을때 많이 삶으니까ㅜ",
          "original_form": "저도 자주 먹는데 요즘은 너무 비싸서 삶지도 못합니다ㅜ 한번 삶을때 많이 삶으니까ㅜ",
          "speaker_id": "1",
          "time": "20210903 12:16"
        },
        {
          "id": "MDRW2100029717.1.21",
          "form": "아 삶은계란!! 3일전에 열알 삶아서 어제 다먹었ㅎㅎ",
          "original_form": "아 삶은계란!! 3일전에 열알 삶아서 어제 다먹었ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:16"
        },
        {
          "id": "MDRW2100029717.1.22",
          "form": "아껴먹느라 혼났네요",
          "original_form": "아껴먹느라 혼났네요",
          "speaker_id": "2",
          "time": "20210903 12:17"
        },
        {
          "id": "MDRW2100029717.1.23",
          "form": "그쵸 한번에 많이 삶아서 근데 또 신기한게 구운계란을 아예 구입하는건 그렇게 엄청 비싼건 또 아닌거같아요",
          "original_form": "그쵸 한번에 많이 삶아서 근데 또 신기한게 구운계란을 아예 구입하는건 그렇게 엄청 비싼건 또 아닌거같아요",
          "speaker_id": "1",
          "time": "20210903 12:18"
        },
        {
          "id": "MDRW2100029717.1.24",
          "form": "저는 그래서 구운계란 2판씩 사서 엄청 먹거든요",
          "original_form": "저는 그래서 구운계란 2판씩 사서 엄청 먹거든요",
          "speaker_id": "1",
          "time": "20210903 12:18"
        },
        {
          "id": "MDRW2100029717.1.25",
          "form": "오... 구운계란 땡기네요ㅎㅎ",
          "original_form": "오... 구운계란 땡기네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:18"
        }
      ]
    }
