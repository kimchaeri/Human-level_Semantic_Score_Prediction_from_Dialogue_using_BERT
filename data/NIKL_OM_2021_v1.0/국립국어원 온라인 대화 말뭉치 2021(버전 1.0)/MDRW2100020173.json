      },
      "utterance": [
        {
          "id": "MDRW2100020173.1.1",
          "form": "그거 보고 잠깐",
          "original_form": "그거 보고 잠깐",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.2",
          "form": "돈 얘기로 엄마아빠한테 물어볼거있어서",
          "original_form": "돈 얘기로 엄마아빠한테 물어볼거있어서",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.3",
          "form": "물었는데",
          "original_form": "물었는데",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.5",
          "form": "갑자기 친구한테 돈빌려주면 안된다",
          "original_form": "갑자기 친구한테 돈빌려주면 안된다",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.6",
          "form": "사람은 거짓말을 안해도 돈은 거짓말을 한다며",
          "original_form": "사람은 거짓말을 안해도 돈은 거짓말을 한다며",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.7",
          "form": "갑자기 인생 조언",
          "original_form": "갑자기 인생 조언",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.8",
          "form": "해주고",
          "original_form": "해주고",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020173.1.9",
          "form": "근데 너무 삶의..",
          "original_form": "근데 너무 삶의..",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.10",
          "form": "처지는 부분을 또",
          "original_form": "처지는 부분을 또",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.11",
          "form": "ㅇㄴ",
          "original_form": "ㅇㄴ",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.12",
          "form": "조언에서 듣게되는거임",
          "original_form": "조언에서 듣게되는거임",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.13",
          "form": "그러게..",
          "original_form": "그러게..",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.14",
          "form": "그래서 영화랑 오버랩 되가지고",
          "original_form": "그래서 영화랑 오버랩 되가지고",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.15",
          "form": "하필왜 그 타이밍에 말하는 거야",
          "original_form": "하필왜 그 타이밍에 말하는 거야",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.16",
          "form": "영화가지고 그러면 안된다며 빨리 털어내자고",
          "original_form": "영화가지고 그러면 안된다며 빨리 털어내자고",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.17",
          "form": "ㄱ영화 왜 끝까지 봣냐고 해서",
          "original_form": "ㄱ영화 왜 끝까지 봣냐고 해서",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.18",
          "form": "그러게.. 싶엇다",
          "original_form": "그러게.. 싶엇다",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.19",
          "form": "근데 끝에 ㄹㅇ 반전이 있을 줄 ㅎㅎ",
          "original_form": "근데 끝에 ㄹㅇ 반전이 있을 줄 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020173.1.20",
          "form": "ㄱㄴㄲ 보기 힘들면",
          "original_form": "ㄱㄴㄲ 보기 힘들면",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020173.1.21",
          "form": "끊자하지",
          "original_form": "끊자하지",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020173.1.22",
          "form": "하긴 나도",
          "original_form": "하긴 나도",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020173.1.23",
          "form": "끝에 뭐다를까 싶어서",
          "original_form": "끝에 뭐다를까 싶어서",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020173.1.24",
          "form": "계속봤다",
          "original_form": "계속봤다",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020173.1.25",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.26",
          "form": "솔까 그만큼 봣으면",
          "original_form": "솔까 그만큼 봣으면",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.27",
          "form": "차라리 끙까지 봐야겟다 싶은 맘이 들엇고",
          "original_form": "차라리 끙까지 봐야겟다 싶은 맘이 들엇고",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.28",
          "form": "글고 영화 =끝에선 좀",
          "original_form": "글고 영화 =끝에선 좀",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.29",
          "form": "여태 이해안됏던것들이 그나마",
          "original_form": "여태 이해안됏던것들이 그나마",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.30",
          "form": "해소가 되면서",
          "original_form": "해소가 되면서",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.31",
          "form": "앞에만큼 답답하진 않았던거...??? 모르겟다",
          "original_form": "앞에만큼 답답하진 않았던거...??? 모르겟다",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020173.1.32",
          "form": "그냥 이해는 됐음 (공감말고)",
          "original_form": "그냥 이해는 됐음 (공감말고)",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.33",
          "form": "앞에는 ㄹㅇ 사고방식도 하나도 이해도 안가고",
          "original_form": "앞에는 ㄹㅇ 사고방식도 하나도 이해도 안가고",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.34",
          "form": "ㄱㄹ..먼지알겟어요",
          "original_form": "ㄱㄹ..먼지알겟어요",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.35",
          "form": "무슨 생각인지도 모르겠는데",
          "original_form": "무슨 생각인지도 모르겠는데",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.36",
          "form": "불쾌하기만 해가지고",
          "original_form": "불쾌하기만 해가지고",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.37",
          "form": "근데 끝에가니깐 조금",
          "original_form": "근데 끝에가니깐 조금",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.38",
          "form": "이해하고싶지도 않긴햇는데",
          "original_form": "이해하고싶지도 않긴햇는데",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.39",
          "form": "왜 이랬는지 파악이",
          "original_form": "왜 이랬는지 파악이",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.40",
          "form": "되어서",
          "original_form": "되어서",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.41",
          "form": "끝까지 본게 낫긴 한거같음",
          "original_form": "끝까지 본게 낫긴 한거같음",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.42",
          "form": "영화를 보는 이상 받아지는 정보가잇어서",
          "original_form": "영화를 보는 이상 받아지는 정보가잇어서",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020173.1.43",
          "form": "이해되는..그런거",
          "original_form": "이해되는..그런거",
          "speaker_id": "2",
          "time": "20210828 00:08"
        }
      ]
    }
