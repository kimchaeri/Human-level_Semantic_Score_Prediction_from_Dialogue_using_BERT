      "utterance": [
        {
          "id": "MDRW2100034122.1.1",
          "form": "알밤동동은 안먹어봤네요",
          "original_form": "알밤동동은 안먹어봤네요",
          "speaker_id": "1",
          "time": "20210827 16:39"
        },
        {
          "id": "MDRW2100034122.1.2",
          "form": "엇 바밤바는 안먹어봤는데 맛있나요?",
          "original_form": "엇 바밤바는 안먹어봤는데 맛있나요?",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100034122.1.3",
          "form": "아빠 취향이라 샀는데\r\n냉동실에 남은 아스크림이\r\n그거 뿐이라 먹었었어요ㅋㅋㅋ",
          "original_form": "아빠 취향이라 샀는데\r\n냉동실에 남은 아스크림이\r\n그거 뿐이라 먹었었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100034122.1.4",
          "form": "그냥 쏘쏘 해요ㅋㅋㅋ",
          "original_form": "그냥 쏘쏘 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100034122.1.5",
          "form": "앗 역시 아버지 취향 아이스크림이였군요ㅋㅋㅋ",
          "original_form": "앗 역시 아버지 취향 아이스크림이였군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.6",
          "form": "저희아버지도 그런 종류 좋아하세요",
          "original_form": "저희아버지도 그런 종류 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.7",
          "form": "민트초코 아이스크림 좋아하시나요?",
          "original_form": "민트초코 아이스크림 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.8",
          "form": "바밤바 비비빅",
          "original_form": "바밤바 비비빅",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.9",
          "form": "헐 민쵸파신가요?",
          "original_form": "헐{emoji:♡} 민쵸파신가요?",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.10",
          "form": "비비빅이 원탑이죠 역시",
          "original_form": "비비빅이 원탑이죠 역시",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.11",
          "form": "네 저는 민초파랍니다..!",
          "original_form": "네 저는 민초파랍니다..!",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100034122.1.12",
          "form": "민초파를 만나니 반갑네여ㅎㅎㅎㅎㅎ!",
          "original_form": "민초파를 만나니 반갑네여ㅎㅎㅎㅎㅎ!",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100034122.1.13",
          "form": "하 여기서 민쵸파를 또 먀나네요",
          "original_form": "하 여기서 민쵸파를 또 먀나네요",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100034122.1.14",
          "form": "앜ㅋㅋㅈㅋㅈㅋㅈ 반갑습니다ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅈㅋㅈㅋㅈ 반갑습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100034122.1.15",
          "form": "왜 호불호가 갈리는지 모르겠어요 짱맛있는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜 호불호가 갈리는지 모르겠어요 짱맛있는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100034122.1.16",
          "form": "구니까요",
          "original_form": "구니까요",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100034122.1.17",
          "form": "어떻게 그게 불호가 있을수\r\n있는지 아직도 의문입니다ㅋㅋ",
          "original_form": "어떻게 그게 불호가 있을수\r\n있는지 아직도 의문입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100034122.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ엔초 민트초코맛 있는데 그거 엄청 맛있어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ엔초 민트초코맛 있는데 그거 엄청 맛있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034122.1.19",
          "form": "구하기 힘든게 함정이지만..",
          "original_form": "구하기 힘든게 함정이지만..",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034122.1.20",
          "form": "편의점에서 보이면 드셔보세요",
          "original_form": "편의점에서 보이면 드셔보세요",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034122.1.21",
          "form": "홀 엔초가 일냈군요!!!!",
          "original_form": "홀 엔초가 일냈군요!!!!",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034122.1.22",
          "form": "어디 편의점으로 가면 되요?",
          "original_form": "어디 편의점으로 가면 되요?",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034122.1.23",
          "form": "사실.. 사실 전 마트서 한번 보이길래 사먹었는데 그뒤로 본 적이 없답니다...",
          "original_form": "사실.. 사실 전 마트서 한번 보이길래 사먹었는데 그뒤로 본 적이 없답니다...",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100034122.1.24",
          "form": "편의점에는 있지 않을까요....?",
          "original_form": "편의점에는 있지 않을까요....?",
          "speaker_id": "2",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100034122.1.25",
          "form": "ㅠㅠ 없어진건 아니겠죠?\r\n대한민국에 민쵸단이 얼마나 많은뎅",
          "original_form": "ㅠㅠ 없어진건 아니겠죠?\r\n대한민국에 민쵸단이 얼마나 많은뎅",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100034122.1.26",
          "form": "그쵸 없앤다면 시대의 흐름을 못읽는 거예요",
          "original_form": "그쵸 없앤다면 시대의 흐름을 못읽는 거예요",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100034122.1.27",
          "form": "분명 어딘가에선 팔고 있을거예요",
          "original_form": "분명 어딘가에선 팔고 있을거예요",
          "speaker_id": "2",
          "time": "20210827 16:50"
        }
