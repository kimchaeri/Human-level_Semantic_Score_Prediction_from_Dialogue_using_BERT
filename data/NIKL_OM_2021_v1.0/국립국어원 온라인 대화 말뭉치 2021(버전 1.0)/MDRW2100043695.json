      "utterance": [
        {
          "id": "MDRW2100043695.1.1",
          "form": "헐 어떤 이유인지 물어봐도 될까요?",
          "original_form": "헐 어떤 이유인지 물어봐도 될까요?",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043695.1.2",
          "form": "너무 제 취향이 아니어서요... ㅜㅜ",
          "original_form": "너무 제 취향이 아니어서요... ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043695.1.3",
          "form": "전 초코나무숲이나 민트...?",
          "original_form": "전 초코나무숲이나 민트...?",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043695.1.4",
          "form": "name2님이랑 반대지요?",
          "original_form": "&name2&님이랑 반대지요?",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043695.1.5",
          "form": "아하! 저도 민트랑 초코나무숲도 좋아해요!",
          "original_form": "아하! 저도 민트랑 초코나무숲도 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043695.1.6",
          "form": "ㅋㅋㅋㅋㅋ약간 안가리고 다 먹는거 같내요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ약간 안가리고 다 먹는거 같내요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043695.1.7",
          "form": "ㅋㅋㅋㅋ좋죠ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043695.1.8",
          "form": "민초단이세요?",
          "original_form": "민초단이세요?",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043695.1.9",
          "form": "앗 당연합니다",
          "original_form": "앗 당연합니다",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043695.1.10",
          "form": "민초사랑해~!",
          "original_form": "민초사랑해~!",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043695.1.11",
          "form": "갸악갸악",
          "original_form": "갸악갸악",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.12",
          "form": "역시 민초 최고죠",
          "original_form": "역시 민초 최고죠",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.13",
          "form": "민초모여~!!",
          "original_form": "민초모여~!!",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.14",
          "form": "오이는 드세요?",
          "original_form": "오이는 드세요?",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.15",
          "form": "민초 좋아하는사람들은 다 귀엽다구요~!",
          "original_form": "민초 좋아하는사람들은 다 귀엽다구요~!",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.16",
          "form": "좋아하진않지만 있으면 먹어요ㅋㅋ",
          "original_form": "좋아하진않지만 있으면 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.17",
          "form": "아 아니 이런 ㅋㅋㅋㅋㅋ",
          "original_form": "아 아니 이런 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043695.1.18",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043695.1.19",
          "form": "오이좋아하세요?",
          "original_form": "오이좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043695.1.20",
          "form": "저도 막 좋아하는건 아닌데 딱히 빼지않고 먹는거같라요!",
          "original_form": "저도 막 좋아하는건 아닌데 딱히 빼지않고 먹는거같라요!",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043695.1.21",
          "form": "또 좋아하는 디저트 잇으세요?",
          "original_form": "또 좋아하는 디저트 잇으세요?",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043695.1.22",
          "form": "저 서브웨이에선 오이 안넣어요ㅠ",
          "original_form": "저 서브웨이에선 오이 안넣어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043695.1.23",
          "form": "오! 저눈 구냥 다 넣어서 먹어요!!",
          "original_form": "오! 저눈 구냥 다 넣어서 먹어요!!",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043695.1.24",
          "form": "전 오이토마토 빼요ㅋㅋ",
          "original_form": "전 오이토마토 빼요ㅋㅋ",
