      "utterance": [
        {
          "id": "MDRW2100021624.1.1",
          "form": "엇 그냥 다른곡도 연습해 본다는 말이였어요 ㅋㅋㅋ",
          "original_form": "엇 그냥 다른곡도 연습해 본다는 말이였어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021624.1.2",
          "form": "아아 ㅎㅎ",
          "original_form": "아아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021624.1.3",
          "form": "저는 완전 음치박치라",
          "original_form": "저는 완전 음치박치라",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021624.1.4",
          "form": "악기는 젬병이에요ㅠㅠ",
          "original_form": "악기는 젬병이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021624.1.5",
          "form": "저두..박치인걸",
          "original_form": "저두..박치인걸",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021624.1.6",
          "form": "기타배우면서 뼈저리게 느꼈어요 ㅋㅋㅋㅋ",
          "original_form": "기타배우면서 뼈저리게 느꼈어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021624.1.7",
          "form": "혼자만의 박자로 딩가딩가치는거에요 ㅎㅎ",
          "original_form": "혼자만의 박자로 딩가딩가치는거에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021624.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021624.1.9",
          "form": "제가 잘하는건 리코더뿐 ㅎ",
          "original_form": "제가 잘하는건 리코더뿐 ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021624.1.10",
          "form": "혹시 단소도 잘 부시나요",
          "original_form": "혹시 단소도 잘 부시나요",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021624.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.12",
          "form": "후",
          "original_form": "후",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.13",
          "form": "푸우",
          "original_form": "푸우",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.14",
          "form": "소리만 내요",
          "original_form": "소리만 내요",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.15",
          "form": "단소가 소리내기힘들죠",
          "original_form": "단소가 소리내기힘들죠",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.16",
          "form": "요즘애들은 리코더처럼 불면 소리나는 단소를 쓴다고 하더라구용",
          "original_form": "요즘애들은 리코더처럼 불면 소리나는 단소를 쓴다고 하더라구용",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.17",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021624.1.18",
          "form": "세상좋아졌네요",
          "original_form": "세상좋아졌네요",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021624.1.19",
          "form": "(꼰대)",
          "original_form": "(꼰대)",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021624.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ라떼는...입술쫙피고 겨우겨우 불었는데...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ라떼는...입술쫙피고 겨우겨우 불었는데...",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021624.1.21",
          "form": "잘 못불어서... 수행평가 안좋게 받고..",
          "original_form": "잘 못불어서... 수행평가 안좋게 받고..",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021624.1.22",
          "form": "ㅋㅋㅋ맞아요..불기만해도 수행평가 잘나오고 그랬었는데",
          "original_form": "ㅋㅋㅋ맞아요..불기만해도 수행평가 잘나오고 그랬었는데",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021624.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021624.1.24",
          "form": "오카리나도 배웠었죠",
          "original_form": "오카리나도 배웠었죠",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021624.1.25",
          "form": "헉 재밌는거 배우셨네요",
          "original_form": "헉 재밌는거 배우셨네요",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021624.1.26",
          "form": "저희는 장구배웠어요",
          "original_form": "저희는 장구배웠어요",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021624.1.27",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021624.1.28",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021624.1.29",
          "form": "헉 또 뭐배우셨나요??",
          "original_form": "헉 또 뭐배우셨나요??",
          "speaker_id": "2",
