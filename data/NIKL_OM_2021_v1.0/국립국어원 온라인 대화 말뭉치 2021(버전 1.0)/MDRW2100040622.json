      "utterance": [
        {
          "id": "MDRW2100040622.1.1",
          "form": "동네 마트에서 파는거야?",
          "original_form": "동네 마트에서 파는거야?",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040622.1.2",
          "form": "응응 동네마트에거 살수있는 과자임 ㅎㅎ",
          "original_form": "응응 동네마트에거 살수있는 과자임 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040622.1.3",
          "form": "봉지과자야?",
          "original_form": "봉지과자야?",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040622.1.4",
          "form": "응응 봉지과자야 작은것도 큰것도 있어 ㅋㅋ",
          "original_form": "응응 봉지과자야 작은것도 큰것도 있어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040622.1.5",
          "form": "농심꺼야?ㅋㅋㄱㅋㅋ",
          "original_form": "농심꺼야?ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040622.1.6",
          "form": "어 ㅋㅋㅋ 왜이렇게 좁혀와? ㅋㅋ농심과자맞아 ㅋㅋㅋ",
          "original_form": "어 ㅋㅋㅋ 왜이렇게 좁혀와? ㅋㅋ농심과자맞아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040622.1.7",
          "form": "매운맛이야?",
          "original_form": "매운맛이야?",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040622.1.8",
          "form": "아니아니 매운맛 전혀 안나",
          "original_form": "아니아니 매운맛 전혀 안나",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100040622.1.9",
          "form": "감자맛이야?",
          "original_form": "감자맛이야?",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040622.1.10",
          "form": "아니! 감자와 멀어! ㅋㅋㅋ이제 질문하나남앗어",
          "original_form": "아니! 감자와 멀어! ㅋㅋㅋ이제 질문하나남앗어",
          "speaker_id": "1",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100040622.1.11",
          "form": "어떤모양이야 ㅋㅋㅋㅋㅋ",
          "original_form": "어떤모양이야 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040622.1.12",
          "form": "길쭉한 모양이야 ㅋㅋㅋㅋ",
          "original_form": "길쭉한 모양이야 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100040622.1.13",
          "form": "새우깡",
          "original_form": "새우깡",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040622.1.14",
          "form": "밖에 기억 안나ㅋㅋㅋㅋㅋ",
          "original_form": "밖에 기억 안나ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040622.1.15",
          "form": "정답은 바나나킥이야 ㅋㅋㅋㅋㅋ 이제 너가생각해봐 ㅋㅋ",
          "original_form": "정답은 바나나킥이야 ㅋㅋㅋㅋㅋ 이제 너가생각해봐 ㅋㅋ",
          "speaker_id": "1",
