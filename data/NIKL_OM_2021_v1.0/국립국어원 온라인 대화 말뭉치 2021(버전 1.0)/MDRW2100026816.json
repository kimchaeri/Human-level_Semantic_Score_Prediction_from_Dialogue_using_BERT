      "utterance": [
        {
          "id": "MDRW2100026816.1.1",
          "form": "여친 눈돌아가더라고요",
          "original_form": "여친 눈돌아가더라고요",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026816.1.2",
          "form": "아닠 추억이 있다매요ㅋㅋ",
          "original_form": "아닠 추억이 있다매요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026816.1.3",
          "form": "그렇게 열정적인 눈깔은 첨봣서요",
          "original_form": "그렇게 열정적인 눈깔은 첨봣서요",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026816.1.4",
          "form": "사랑했다!",
          "original_form": "사랑했다!",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026816.1.5",
          "form": "여친이 수학푸는 님보다 바지락을 더 사랑했군요ㅋㅋ",
          "original_form": "여친이 수학푸는 님보다 바지락을 더 사랑했군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026816.1.6",
          "form": "들리냐!",
          "original_form": "들리냐!",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026816.1.7",
          "form": "아 제주도",
          "original_form": "아 제주도",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.8",
          "form": "여신님의 예상 못한 정체ㅋㅋㅋ",
          "original_form": "여신님의 예상 못한 정체ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.9",
          "form": "제주도에는 다금바리가 유명하다고 하던데",
          "original_form": "제주도에는 다금바리가 유명하다고 하던데",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.10",
          "form": "드셔봣나요?",
          "original_form": "드셔봣나요?",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.11",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.12",
          "form": "사실 공대 윤아였죠",
          "original_form": "사실 공대 윤아였죠",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.13",
          "form": "소시 윤아 닮아서",
          "original_form": "소시 윤아 닮아서",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.14",
          "form": "비싸더라구요",
          "original_form": "비싸더라구요",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026816.1.15",
          "form": "아닝 여신 맞네오",
          "original_form": "아닝 여신 맞네오",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026816.1.16",
          "form": "다금바리",
          "original_form": "다금바리",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026816.1.17",
          "form": "얼마?",
          "original_form": "얼마?",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026816.1.18",
          "form": "그냥 바지락과ㅇ셋이 사귀시지ㅜ",
          "original_form": "그냥 바지락과ㅇ셋이 사귀시지ㅜ",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026816.1.19",
          "form": "누구랑 가서 드셨나요 그 좋은걸",
          "original_form": "누구랑 가서 드셨나요 그 좋은걸",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026816.1.20",
          "form": "이제는 캘수있다 바지락!",
          "original_form": "이제는 캘수있다 바지락!",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026816.1.21",
          "form": "저는..누구랑 가도 반드시 먹습니다ㅋㅋ",
          "original_form": "저는..누구랑 가도 반드시 먹습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026816.1.22",
          "form": "먹기 위해 가는 거라서요ㅎㅎ",
          "original_form": "먹기 위해 가는 거라서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026816.1.23",
          "form": "가격은...비싼것만 기억납니다",
          "original_form": "가격은...비싼것만 기억납니다",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026816.1.24",
          "form": "부자신가봐요",
          "original_form": "부자신가봐요",
          "speaker_id": "2",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100026816.1.25",
          "form": "대신 밑반찬이 우니 갈치회 전복 온갖게 나와 돈아깝진 않네요ㅎ",
          "original_form": "대신 밑반찬이 우니 갈치회 전복 온갖게 나와 돈아깝진 않네요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026816.1.26",
          "form": "다금바리 좋아하세요?",
          "original_form": "다금바리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026816.1.27",
          "form": "다금바리 사주세요",
          "original_form": "다금바리 사주세요",
          "speaker_id": "2",
          "time": "20210902 00:24"
        }
      ]
    }
  ]
