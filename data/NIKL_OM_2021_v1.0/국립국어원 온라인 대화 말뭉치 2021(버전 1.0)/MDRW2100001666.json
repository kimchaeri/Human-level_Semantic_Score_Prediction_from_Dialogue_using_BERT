      "utterance": [
        {
          "id": "MDRW2100001666.1.1",
          "form": "코로나 끝나면 어디 제일 가고싶으세요?",
          "original_form": "코로나 끝나면 어디 제일 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100001666.1.2",
          "form": "저는 아직 해외에 나가본 경험이 없어서",
          "original_form": "저는 아직 해외에 나가본 경험이 없어서",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001666.1.3",
          "form": "구체적으로 가고싶은 나라는 없지만 해외여행 가보고싶어요",
          "original_form": "구체적으로 가고싶은 나라는 없지만 해외여행 가보고싶어요",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100001666.1.4",
          "form": "아 해외여행 좋죠",
          "original_form": "아 해외여행 좋죠",
          "speaker_id": "1",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001666.1.5",
          "form": "저도 딱한번 다녀왔어요",
          "original_form": "저도 딱한번 다녀왔어요",
          "speaker_id": "1",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001666.1.6",
          "form": "대만 갔다온 적 있어요!",
          "original_form": "대만 갔다온 적 있어요!",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001666.1.7",
          "form": "우리나라에서 많이들 가는 여행지중 하난것 같아요 대만이",
          "original_form": "우리나라에서 많이들 가는 여행지중 하난것 같아요 대만이",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001666.1.8",
          "form": "가장 좋았던건 뭔가요??",
          "original_form": "가장 좋았던건 뭔가요??",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001666.1.9",
          "form": "저는 아무래도 딤섬이 너어무 맛있더라구요",
          "original_form": "저는 아무래도 딤섬이 너어무 맛있더라구요",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001666.1.10",
          "form": "한국에서 먹는거랑은 달랐던거같아요",
          "original_form": "한국에서 먹는거랑은 달랐던거같아요",
          "speaker_id": "1",
          "time": "20210819 18:15"
        },
        {
          "id": "MDRW2100001666.1.11",
          "form": "역시 아무리 유명한 맛집이어도 현지의 맛을 따라가긴 한계가 있나봐요",
          "original_form": "역시 아무리 유명한 맛집이어도 현지의 맛을 따라가긴 한계가 있나봐요",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001666.1.12",
          "form": "말만 들었는데 군침이 싹 도네요ㅎㅎ",
          "original_form": "말만 들었는데 군침이 싹 도네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:17"
        },
        {
          "id": "MDRW2100001666.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다음에 대만 가시면 꼭 드셔보세요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다음에 대만 가시면 꼭 드셔보세요!",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001666.1.14",
          "form": "가고싶은 나라는 있으세요?",
          "original_form": "가고싶은 나라는 있으세요?",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001666.1.15",
          "form": "저는 오로라 직관하러 가보고싶어요!",
          "original_form": "저는 오로라 직관하러 가보고싶어요!",
          "speaker_id": "2",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001666.1.16",
          "form": "오로라면 남극인가요??",
          "original_form": "오로라면 남극인가요??",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100001666.1.17",
          "form": "너무 좋죠 근데 엄청 춥대요ㅠㅠ",
          "original_form": "너무 좋죠 근데 엄청 춥대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100001666.1.18",
          "form": "저도 꼭 한번 보고싶어요",
          "original_form": "저도 꼭 한번 보고싶어요",
          "speaker_id": "1",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100001666.1.19",
          "form": "꽃청춘 아이슬란드편 보고 든 생각이에요!ㅎㅎ",
          "original_form": "꽃청춘 아이슬란드편 보고 든 생각이에요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100001666.1.20",
          "form": "전 더운나라 여행하는거 보다는 추운게 나아서 괜찮을거 같아요",
          "original_form": "전 더운나라 여행하는거 보다는 추운게 나아서 괜찮을거 같아요",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100001666.1.21",
          "form": "아 저도요!",
          "original_form": "아 저도요!",
          "speaker_id": "1",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100001666.1.22",
          "form": "저도 더위를 많이 타거든요",
          "original_form": "저도 더위를 많이 타거든요",
          "speaker_id": "1",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100001666.1.23",
          "form": "대만 진짜 덥고 습한데ㅜㅠ 저도 다음에는 추운 나라로 가보고싶어요",
          "original_form": "대만 진짜 덥고 습한데ㅜㅠ 저도 다음에는 추운 나라로 가보고싶어요",
          "speaker_id": "1",
          "time": "20210820 11:38"
        },
        {
          "id": "MDRW2100001666.1.24",
          "form": "대만 가셨을때 많이 힘드셨겠어요!",
          "original_form": "대만 가셨을때 많이 힘드셨겠어요!",
          "speaker_id": "2",
          "time": "20210820 12:02"
        },
        {
          "id": "MDRW2100001666.1.25",
          "form": "전 가을을 가장 좋아해서.. 우리나라 가을이랑 날씨 비슷한 나라로 가고싶어요ㅎㅎ",
          "original_form": "전 가을을 가장 좋아해서.. 우리나라 가을이랑 날씨 비슷한 나라로 가고싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100001666.1.26",
          "form": "아 여행할때 가을날씨 너뮤 좋죠",
          "original_form": "아 여행할때 가을날씨 너뮤 좋죠",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100001666.1.27",
          "form": "근데 또 여름이면 옷이 얇아서 짐이 줄어드는 장점도 있긴한거같아요",
          "original_form": "근데 또 여름이면 옷이 얇아서 짐이 줄어드는 장점도 있긴한거같아요",
          "speaker_id": "1",
          "time": "20210820 14:28"
        }
      ]
    }
