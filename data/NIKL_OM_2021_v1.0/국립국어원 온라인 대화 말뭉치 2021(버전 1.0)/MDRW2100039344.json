      "utterance": [
        {
          "id": "MDRW2100039344.1.1",
          "form": "제 나이요?",
          "original_form": "제 나이요?",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039344.1.2",
          "form": "아아 조카들이요!",
          "original_form": "아아 조카들이요!",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039344.1.3",
          "form": "제일 큰 아이가 지금 11살이고 둘째는 8살",
          "original_form": "제일 큰 아이가 지금 11살이고 둘째는 8살",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039344.1.4",
          "form": "작은언니네 조카는 10살이에요! 한국나이로요 ㅎㅎ",
          "original_form": "작은언니네 조카는 10살이에요! 한국나이로요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039344.1.5",
          "form": "아 다들 초등학생이군요 학교생활은 재밌대요?",
          "original_form": "아 다들 초등학생이군요 학교생활은 재밌대요?",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039344.1.6",
          "form": "원래 재밌어하곤 했는데",
          "original_form": "원래 재밌어하곤 했는데",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039344.1.7",
          "form": "코로나 때문에 못가더라구요 ㅠ",
          "original_form": "코로나 때문에 못가더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039344.1.8",
          "form": "음... 여행 이야기를 하자면",
          "original_form": "음... 여행 이야기를 하자면",
          "speaker_id": "2",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100039344.1.9",
          "form": "그래서 둘째 언니네 있는 미국에 가는게 소원이에요",
          "original_form": "그래서 둘째 언니네 있는 미국에 가는게 소원이에요",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039344.1.10",
          "form": "시카고랑 가까운데 ㅠ",
          "original_form": "시카고랑 가까운데 ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100039344.1.11",
          "form": "아ㅎㅎ 이전에 가보신 적 있으세요?",
          "original_form": "아ㅎㅎ 이전에 가보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039344.1.12",
          "form": "아니요!!! 한 번도 안가봤답니다ㅜ",
          "original_form": "아니요!!! 한 번도 안가봤답니다ㅜ",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039344.1.13",
          "form": "비행기값이 정말 만만치 않더라구요..",
          "original_form": "비행기값이 정말 만만치 않더라구요..",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039344.1.14",
          "form": "그쵸 저도 비행기값이랑 비행시간땜에 미국은 엄두를 못내고있네요",
          "original_form": "그쵸 저도 비행기값이랑 비행시간땜에 미국은 엄두를 못내고있네요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039344.1.15",
          "form": "맞아요 거기다 저 혼자 가는 것도 아니고",
          "original_form": "맞아요 거기다 저 혼자 가는 것도 아니고",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039344.1.16",
          "form": "부모님도 모시고 가야하니,,,",
          "original_form": "부모님도 모시고 가야하니,,,",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039344.1.17",
          "form": "부모님도 많이 보고싶어하시겠어요",
          "original_form": "부모님도 많이 보고싶어하시겠어요",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039344.1.18",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039344.1.19",
          "form": "name1님은 부산 다녀오신 적 있으신가요?",
          "original_form": "&name1&님은 부산 다녀오신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039344.1.20",
          "form": "부산 멀진 않은데 많이는 아니고 두번 정도 간 것 같아요ㅋㅋ 부산 좋아하세요?",
          "original_form": "부산 멀진 않은데 많이는 아니고 두번 정도 간 것 같아요ㅋㅋ 부산 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 07:18"
        },
        {
          "id": "MDRW2100039344.1.21",
          "form": "저는 한 번도 안가봤는데 너무 가고싶어요!",
          "original_form": "저는 한 번도 안가봤는데 너무 가고싶어요!",
          "speaker_id": "2",
          "time": "20210904 09:13"
        },
        {
          "id": "MDRW2100039344.1.22",
          "form": "해운대 가고싶네요ㅠ 센텀시티요!",
          "original_form": "해운대 가고싶네요ㅠ 센텀시티요!",
          "speaker_id": "2",
          "time": "20210904 09:14"
        },
        {
          "id": "MDRW2100039344.1.23",
          "form": "아 거기 좋아요 밤되면 진짜 분위기있어요",
          "original_form": "아 거기 좋아요 밤되면 진짜 분위기있어요",
          "speaker_id": "1",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100039344.1.24",
