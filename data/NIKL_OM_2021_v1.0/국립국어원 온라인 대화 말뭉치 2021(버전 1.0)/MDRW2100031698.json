      "utterance": [
        {
          "id": "MDRW2100031698.1.1",
          "form": "맞춰 볼게요 혹시 귤인가요",
          "original_form": "맞춰 볼게요 혹시 귤인가요",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031698.1.2",
          "form": "아니에요 육지가 훨씬 싸요",
          "original_form": "아니에요 육지가 훨씬 싸요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.3",
          "form": "여기서 생산되는 것이긴 합니다",
          "original_form": "여기서 생산되는 것이긴 합니다",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.4",
          "form": "제주도 빵인가요",
          "original_form": "제주도 빵인가요",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.5",
          "form": "오메기떡 이요 그건 부산에서도 만들어 팔더라고요",
          "original_form": "오메기떡 이요 그건 부산에서도 만들어 팔더라고요",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.6",
          "form": "다들 귤나무 하나씩 가지고 있다고 하던데요",
          "original_form": "다들 귤나무 하나씩 가지고 있다고 하던데요",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.7",
          "form": "오메기떡 맛있죠 저도 좋아합니다",
          "original_form": "오메기떡 맛있죠 저도 좋아합니다",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.8",
          "form": "제주 보리빵도 맛있죠",
          "original_form": "제주 보리빵도 맛있죠",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100031698.1.9",
          "form": "저는 성게 멍게 이런 향이가 나 강한 음식 빼고는 다 잘 먹어요",
          "original_form": "저는 성게 멍게 이런 향이가 나 강한 음식 빼고는 다 잘 먹어요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.10",
          "form": "가장 싼 것은 삼다수입니다",
          "original_form": "가장 싼 것은 삼다수입니다",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.11",
          "form": "2리터에 700원",
          "original_form": "2리터에 700원",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.12",
          "form": "삼다수 개이득",
          "original_form": "삼다수 개이득",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.13",
          "form": "삼다수 맛있지요",
          "original_form": "삼다수 맛있지요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.14",
          "form": "삼다수만 먹어요",
          "original_form": "삼다수만 먹어요",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.15",
          "form": "전에 누가 레몬을 줘서 고모가 청으로 담그셨어요",
          "original_form": "전에 누가 레몬을 줘서 고모가 청으로 담그셨어요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.16",
          "form": "다 육지에서 오니까 비싸거든요",
          "original_form": "다 육지에서 오니까 비싸거든요",
          "speaker_id": "1",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100031698.1.17",
          "form": "상큼 하겠다",
          "original_form": "상큼 하겠다",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031698.1.18",
          "form": "회원 엄마가 청귤로 청담 가셔서 주셨는데두 번만에 다 먹어 버렸어요",
          "original_form": "회원 엄마가 청귤로 청담 가셔서 주셨는데두 번만에 다 먹어 버렸어요",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031698.1.19",
          "form": "그렇게 맛있었나요",
          "original_form": "그렇게 맛있었나요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        }
