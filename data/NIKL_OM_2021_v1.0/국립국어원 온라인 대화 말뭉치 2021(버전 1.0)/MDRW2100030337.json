      "utterance": [
        {
          "id": "MDRW2100030337.1.1",
          "form": "돈까스김밥이",
          "original_form": "돈까스김밥이",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.2",
          "form": "존맛입니다",
          "original_form": "존맛입니다",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.3",
          "form": "대신동 고봉민은",
          "original_form": "대신동 고봉민은",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.4",
          "form": "고봉민 김밥 이요",
          "original_form": "고봉민 김밥 이요",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.5",
          "form": "주위 다른업체랑 제휴해서",
          "original_form": "주위 다른업체랑 제휴해서",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.6",
          "form": "맛이 다양한 가봐요",
          "original_form": "맛이 다양한 가봐요",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.7",
          "form": "영수즘들고가면",
          "original_form": "영수즘들고가면",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.8",
          "form": "김밥 맛집 아시나요",
          "original_form": "김밥 맛집 아시나요",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.9",
          "form": "서로 할인해주고",
          "original_form": "서로 할인해주고",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030337.1.10",
          "form": "그런이벤드앴어요",
          "original_form": "그런이벤드앴어요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.11",
          "form": "아이디어 좋네요",
          "original_form": "아이디어 좋네요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.12",
          "form": "김밥천국도 맛나요",
          "original_form": "김밥천국도 맛나요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.13",
          "form": "오타인가요",
          "original_form": "오타인가요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.14",
          "form": "음성인식인가요",
          "original_form": "음성인식인가요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.15",
          "form": "주변에 바른김밥생겼어묘",
          "original_form": "주변에 바른김밥생겼어묘",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.16",
          "form": "좋은재료",
          "original_form": "좋은재료",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.17",
          "form": "오타요",
          "original_form": "오타요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.18",
          "form": "비싼가견",
          "original_form": "비싼가견",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.19",
          "form": "격",
          "original_form": "격",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.20",
          "form": "부전동에 시장에 맛집 있어요",
          "original_form": "부전동에 시장에 맛집 있어요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.21",
          "form": "그래서 안 먹어 봤어요",
          "original_form": "그래서 안 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030337.1.22",
          "form": "한 줄에 2천원으로 저렴해요",
          "original_form": "한 줄에 2천원으로 저렴해요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        }
