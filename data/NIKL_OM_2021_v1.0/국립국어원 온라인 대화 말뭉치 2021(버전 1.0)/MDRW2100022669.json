      "utterance": [
        {
          "id": "MDRW2100022669.1.1",
          "form": "꿀복숭아 냉동실에 넣어서 더 사각사각거릴때 맛있어요",
          "original_form": "꿀복숭아 냉동실에 넣어서 더 사각사각거릴때 맛있어요",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022669.1.2",
          "form": "그거 진짜 달아서",
          "original_form": "그거 진짜 달아서",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022669.1.3",
          "form": "마시면 진짜 당충전 확 되는 느낌",
          "original_form": "마시면 진짜 당충전 확 되는 느낌",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022669.1.4",
          "form": "너무 좋아요ㅜㅜ",
          "original_form": "너무 좋아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022669.1.5",
          "form": "달달한걸 좋아하는건 아닌데",
          "original_form": "달달한걸 좋아하는건 아닌데",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022669.1.6",
          "form": "꿀복숭아는 계속먹을수 있더라고요!",
          "original_form": "꿀복숭아는 계속먹을수 있더라고요!",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022669.1.7",
          "form": "그 시럽만 사고 싶어요",
          "original_form": "그 시럽만 사고 싶어요",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022669.1.8",
          "form": "집에 믹서기 있으면 얼음 넣고 해먹을수있게",
          "original_form": "집에 믹서기 있으면 얼음 넣고 해먹을수있게",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022669.1.9",
          "form": "집에서 해먹으면 그맛이 안날거같아요!",
          "original_form": "집에서 해먹으면 그맛이 안날거같아요!",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022669.1.10",
          "form": "뭔가 밖에서 사먹어야 맛있는 느낌이에요!!",
          "original_form": "뭔가 밖에서 사먹어야 맛있는 느낌이에요!!",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022669.1.11",
          "form": "왜요!",
          "original_form": "왜요!",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022669.1.12",
          "form": "어차피 카페에서 파는 시럽 똑같은데요~",
          "original_form": "어차피 카페에서 파는 시럽 똑같은데요~",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022669.1.13",
          "form": "저는 밖에서 사먹는걸 똑같이 못하겠더라구요...",
          "original_form": "저는 밖에서 사먹는걸 똑같이 못하겠더라구요...",
          "speaker_id": "2",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100022669.1.14",
          "form": "사먹는게 그래도 편해서 사먹는편이에요",
          "original_form": "사먹는게 그래도 편해서 사먹는편이에요",
          "speaker_id": "2",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100022669.1.15",
          "form": "오늘은 맛있는거 드셨나요?",
          "original_form": "오늘은 맛있는거 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100022669.1.16",
          "form": "연어랑 광어먹었어요!",
          "original_form": "연어랑 광어먹었어요!",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100022669.1.17",
          "form": "엄청 두툼하더라고요!",
          "original_form": "엄청 두툼하더라고요!",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100022669.1.18",
          "form": "우와...... 횟집에서요?",
          "original_form": "우와...... 횟집에서요?",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100022669.1.19",
          "form": "코스트코에서요!",
          "original_form": "코스트코에서요!",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022669.1.20",
          "form": "주말에 갔다왔어요",
          "original_form": "주말에 갔다왔어요",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022669.1.21",
          "form": "아 코스트코 연어는",
          "original_form": "아 코스트코 연어는",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022669.1.22",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022669.1.23",
          "form": "진짜 두툼하잖아요",
          "original_form": "진짜 두툼하잖아요",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100022669.1.24",
          "form": "원래 초밥먹을려고했는데",
          "original_form": "원래 초밥먹을려고했는데",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100022669.1.25",
          "form": "회가 땡길 때가 있잖아요",
          "original_form": "회가 땡길 때가 있잖아요",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100022669.1.26",
          "form": "그래서 회만있는거 사와서 먹었어요 ㅎ",
          "original_form": "그래서 회만있는거 사와서 먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100022669.1.27",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 10:45"
        }
      ]
