        {
          "id": "MDRW2100031850.1.1",
          "form": "오 어디꺼요?",
          "original_form": "오 어디꺼요?",
          "speaker_id": "2",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.2",
          "form": "굽네치킨이요",
          "original_form": "굽네치킨이요",
          "speaker_id": "1",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.3",
          "form": "뭘로요?",
          "original_form": "뭘로요?",
          "speaker_id": "2",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.4",
          "form": "오리지널!",
          "original_form": "오리지널!",
          "speaker_id": "1",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.5",
          "form": "순살?",
          "original_form": "순살?",
          "speaker_id": "2",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.6",
          "form": "뼈요 ㅋㅋ",
          "original_form": "뼈요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.7",
          "form": "저랑 취향같으시네요",
          "original_form": "저랑 취향같으시네요",
          "speaker_id": "2",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.8",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.9",
          "form": "네 뼈가 맛있죠",
          "original_form": "네 뼈가 맛있죠",
          "speaker_id": "2",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.10",
          "form": "네 순살은 넘 양 적어요",
          "original_form": "네 순살은 넘 양 적어요",
          "speaker_id": "1",
          "time": "20210905 02:53"
        },
        {
          "id": "MDRW2100031850.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031850.1.12",
          "form": "넘 비싸요",
          "original_form": "넘 비싸요",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031850.1.13",
          "form": "그래서 저도 요기요 할인으로 먹었어요 ㅋㅋㅋ",
          "original_form": "그래서 저도 요기요 할인으로 먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031850.1.14",
          "form": "그럼 싼가요?",
          "original_form": "그럼 싼가요?",
          "speaker_id": "2",
          "time": "20210905 02:54"
        },
        {
          "id": "MDRW2100031850.1.15",
