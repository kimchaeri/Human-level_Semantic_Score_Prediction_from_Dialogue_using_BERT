      "utterance": [
        {
          "id": "MDRW2100016915.1.1",
          "form": "ㅋㅋ 그거 좀 아까워서 할인 위주로",
          "original_form": "ㅋㅋ 그거 좀 아까워서 할인 위주로",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016915.1.2",
          "form": "저도...ㅎㅎㅎ",
          "original_form": "저도...ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100016915.1.3",
          "form": "시켜먹을때 오늘 할인해주는 곳으로ㅋㅋ",
          "original_form": "시켜먹을때 오늘 할인해주는 곳으로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100016915.1.4",
          "form": "막 3~4천원ㅋㅋㅋ",
          "original_form": "막 3~4천원ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100016915.1.5",
          "form": "배달비 없는 곳으로 고르기도 하는데",
          "original_form": "배달비 없는 곳으로 고르기도 하는데",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100016915.1.6",
          "form": "많이 없어서 힘들더라구요ㅜ",
          "original_form": "많이 없어서 힘들더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100016915.1.7",
          "form": "배달비는 다 있죠 보통",
          "original_form": "배달비는 다 있죠 보통",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100016915.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100016915.1.9",
          "form": "다 교촌치킨 때문입니다..이",
          "original_form": "다 교촌치킨 때문입니다..이",
          "speaker_id": "1",
          "time": "20210827 09:24"
        },
        {
          "id": "MDRW2100016915.1.10",
          "form": "이 모든 원인은...",
          "original_form": "이 모든 원인은...",
          "speaker_id": "1",
          "time": "20210827 09:24"
        },
        {
          "id": "MDRW2100016915.1.11",
          "form": "근데 교촌이 맛있긴 해서...",
          "original_form": "근데 교촌이 맛있긴 해서...",
          "speaker_id": "2",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100016915.1.12",
          "form": "그래서... 올리나봐요ㅜ",
          "original_form": "그래서... 올리나봐요ㅜ",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016915.1.13",
          "form": "또 올린다던데ㅜ",
          "original_form": "또 올린다던데ㅜ",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016915.1.14",
          "form": "ㅋㅋ안먹어야징",
          "original_form": "ㅋㅋ안먹어야징",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100016915.1.15",
          "form": "근데 그러기엔 물가가",
          "original_form": "근데 그러기엔 물가가",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100016915.1.16",
          "form": "다 올라서ㅠ",
          "original_form": "다 올라서ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100016915.1.17",
          "form": "그렇죠...ㅠㅜ",
          "original_form": "그렇죠...ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100016915.1.18",
          "form": "저는 교촌 괘씸해서",
          "original_form": "저는 교촌 괘씸해서",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100016915.1.19",
          "form": "잘 안먹으려고 해요ㅠㅜ",
          "original_form": "잘 안먹으려고 해요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100016915.1.20",
          "form": "진짜 참다 참다 참다 안되면...ㅜ",
          "original_form": "진짜 참다 참다 참다 안되면...ㅜ",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100016915.1.21",
          "form": "ㅋㅋ굽네 좋던데요",
          "original_form": "ㅋㅋ굽네 좋던데요",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100016915.1.22",
          "form": "굽네 갈비천왕 맛있더라구요ㅋㅋ",
          "original_form": "굽네 갈비천왕 맛있더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100016915.1.23",
          "form": "마지막에 밥도 비벼먹고 ㅋㅋ",
          "original_form": "마지막에 밥도 비벼먹고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100016915.1.24",
          "form": "살 덜 찌겠지하고ㅋㅋ",
          "original_form": "살 덜 찌겠지하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100016915.1.25",
          "form": "구우면 좀 낳을까....ㅋㅋㅋㅋ",
          "original_form": "구우면 좀 낳을까....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100016915.1.26",
          "form": "싶은마음에...ㅋㅋ",
          "original_form": "싶은마음에...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100016915.1.27",
          "form": "튀김보다는 ㅎㅎㅎ",
          "original_form": "튀김보다는 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100016915.1.28",
          "form": "하지만ㅋㅋㅋㅋ 많이 먹으면 똑같죠ㅋㅋㅋ",
          "original_form": "하지만ㅋㅋㅋㅋ 많이 먹으면 똑같죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100016915.1.29",
          "form": "굽네말고 좋아하는 브랜드 있으세요?",
          "original_form": "굽네말고 좋아하는 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100016915.1.30",
          "form": "교촌이.....",
          "original_form": "교촌이.....",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100016915.1.31",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100016915.1.32",
          "form": "교촌의 노예가 되셔야겠네요ㅠㅜ",
          "original_form": "교촌의 노예가 되셔야겠네요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100016915.1.33",
          "form": "피자는 뭐 좋아하세요???",
          "original_form": "피자는 뭐 좋아하세요???",
