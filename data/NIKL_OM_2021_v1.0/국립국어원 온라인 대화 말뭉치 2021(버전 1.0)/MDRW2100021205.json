      "utterance": [
        {
          "id": "MDRW2100021205.1.1",
          "form": "아 할리스는 가보셨나요",
          "original_form": "아 할리스는 가보셨나요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021205.1.2",
          "form": "집근처에 없어서요",
          "original_form": "집근처에 없어서요",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021205.1.3",
          "form": "거기도 디저트가 괜찮나요",
          "original_form": "거기도 디저트가 괜찮나요",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021205.1.4",
          "form": "디저트보다...끼니 떼우기 좋아요",
          "original_form": "디저트보다...끼니 떼우기 좋아요",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021205.1.5",
          "form": "아 샐러드같은 게 많나요?",
          "original_form": "아 샐러드같은 게 많나요?",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021205.1.6",
          "form": "아니ㅋㅋ 그런 웰빙 말고요",
          "original_form": "아니ㅋㅋ 그런 웰빙 말고요",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021205.1.7",
          "form": "함박을 팝니다",
          "original_form": "함박을 팝니다",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021205.1.8",
          "form": "헐 함박스테이크를요?",
          "original_form": "헐 함박스테이크를요?",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021205.1.9",
          "form": "너무 좋네요",
          "original_form": "너무 좋네요",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021205.1.10",
          "form": "거기 리조또도 맛나요",
          "original_form": "거기 리조또도 맛나요",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021205.1.11",
          "form": "전 매콤한 해산물리조또 좋아해요",
          "original_form": "전 매콤한 해산물리조또 좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021205.1.12",
          "form": "카페서 별거를 다 파네요",
          "original_form": "카페서 별거를 다 파네요",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021205.1.13",
          "form": "식당같아요",
          "original_form": "식당같아요",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021205.1.14",
          "form": "그래서 카페식이라고 이름 붙였더라고요",
          "original_form": "그래서 카페식이라고 이름 붙였더라고요",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021205.1.15",
          "form": "가격도 착해요",
          "original_form": "가격도 착해요",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021205.1.16",
          "form": "아 일부러라도 찾아가야겠어요",
          "original_form": "아 일부러라도 찾아가야겠어요",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021205.1.17",
          "form": "집근처 없어서 아쉽네요",
          "original_form": "집근처 없어서 아쉽네요",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021205.1.18",
          "form": "아앗 찾아갈 정도 찐 맛집은 아닌데",
          "original_form": "아앗 찾아갈 정도 찐 맛집은 아닌데",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021205.1.19",
          "form": "아 그 정도는 아녜요?",
          "original_form": "아 그 정도는 아녜요?",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021205.1.20",
          "form": "맛있긴 한데 양이 좀 많이 적어요ㅜ",
          "original_form": "맛있긴 한데 양이 좀 많이 적어요ㅜ",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021205.1.21",
          "form": "대식가라 아쉽네요",
          "original_form": "대식가라 아쉽네요",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021205.1.22",
          "form": "카페 생각하면 좋고 식당 생각하면 양이 아쉽죠",
          "original_form": "카페 생각하면 좋고 식당 생각하면 양이 아쉽죠",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021205.1.23",
          "form": "혹시 우연히 들르면 드셔보시라고요ㅎ",
          "original_form": "혹시 우연히 들르면 드셔보시라고요ㅎ",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021205.1.24",
          "form": "네ㅋ 일부러는 안가고 우연히만 들를게요ㅋ",
          "original_form": "네ㅋ 일부러는 안가고 우연히만 들를게요ㅋ",
          "speaker_id": "2",
          "time": "20210828 16:53"
        }
      ]
