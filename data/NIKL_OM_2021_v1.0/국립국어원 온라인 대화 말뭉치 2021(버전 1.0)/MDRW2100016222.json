      "utterance": [
        {
          "id": "MDRW2100016222.1.1",
          "form": "쉬익 네 좋아해요",
          "original_form": "쉬익 네 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ콧김을 뿜으시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ콧김을 뿜으시네요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.3",
          "form": "저 옷 많이 샀거든요",
          "original_form": "저 옷 많이 샀거든요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.4",
          "form": "오 뭐사셨어요?",
          "original_form": "오 뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.5",
          "form": "뭐냐면요",
          "original_form": "뭐냐면요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.6",
          "form": "트레이닝 셋업이랑요",
          "original_form": "트레이닝 셋업이랑요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.7",
          "form": "검정 재킷이랑요",
          "original_form": "검정 재킷이랑요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.8",
          "form": "답장해주세요",
          "original_form": "답장해주세요",
          "speaker_id": "1",
          "time": "20210826 11:08"
        },
        {
          "id": "MDRW2100016222.1.9",
          "form": "보고있능데요",
          "original_form": "보고있능데요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.10",
          "form": "신발샀어요",
          "original_form": "신발샀어요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.11",
          "form": "오 무슨신발 사셨어요",
          "original_form": "오 무슨신발 사셨어요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.12",
          "form": "운동화요",
          "original_form": "운동화요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.13",
          "form": "닭이요",
          "original_form": "닭이요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.14",
          "form": "옷에 돈을 굉장히 많이 쓰시는군요",
          "original_form": "옷에 돈을 굉장히 많이 쓰시는군요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.15",
          "form": "흰색에 주황색 라인이 있어요",
          "original_form": "흰색에 주황색 라인이 있어요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.16",
          "form": "옙쁘겠ㅈ6",
          "original_form": "옙쁘겠ㅈ6",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.17",
          "form": "엄마가 썼죠",
          "original_form": "엄마가 썼죠",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.18",
          "form": "닭이 흰색에 주황색 라인이요 ?",
          "original_form": "닭이 흰색에 주황색 라인이요 ?",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.19",
          "form": "아뇨;",
          "original_form": "아뇨;",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.20",
          "form": "르꼬끄 거라고요",
          "original_form": "르꼬끄 거라고요",
          "speaker_id": "1",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.21",
          "form": "르꼬끄가 닭인가요",
          "original_form": "르꼬끄가 닭인가요",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016222.1.22",
          "form": "폭신해요",
          "original_form": "폭신해요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.23",
          "form": "저는 몰라서",
          "original_form": "저는 몰라서",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.25",
          "form": "하핫",
          "original_form": "하핫",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.26",
          "form": "어우 예",
          "original_form": "어우 예",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.27",
          "form": "퓨마는 뭐게요?",
          "original_form": "퓨마는 뭐게요?",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.28",
          "form": "표범이요",
          "original_form": "표범이요",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.29",
          "form": "장난하세요",
          "original_form": "장난하세요",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.30",
          "form": "아뇨...",
          "original_form": "아뇨...",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.31",
          "form": "퓨마예요",
          "original_form": "퓨마예요",
          "speaker_id": "1",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.32",
          "form": "나이키 아디다스 퓨~마",
          "original_form": "나이키 아디다스 퓨~마",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.33",
          "form": "짝짝짝",
          "original_form": "짝짝짝",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100016222.1.34",
          "form": "데상트 르꼬끄 라꼬스테",
          "original_form": "데상트 르꼬끄 라꼬스테",
          "speaker_id": "2",
          "time": "20210826 11:11"
        }
      ]
    }
