        },
        {
          "id": "MDRW2100016229.1.3",
          "form": "멜빵",
          "original_form": "멜빵",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100016229.1.4",
          "form": "너무커요",
          "original_form": "너무커요",
          "speaker_id": "2",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100016229.1.5",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100016229.1.6",
          "form": "누가입어요",
          "original_form": "누가입어요",
          "speaker_id": "1",
          "time": "20210826 11:12"
        },
        {
          "id": "MDRW2100016229.1.7",
          "form": "너무 커요?",
          "original_form": "너무 커요?",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016229.1.8",
          "form": "멜빵",
          "original_form": "멜빵",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016229.1.9",
          "form": "멜빵 너무",
          "original_form": "멜빵 너무",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016229.1.10",
          "form": "멜빵 너무 커요",
          "original_form": "멜빵 너무 커요",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016229.1.11",
          "form": "되는데요???",
          "original_form": "되는데요???",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016229.1.12",
          "form": "무커",
          "original_form": "무커",
          "speaker_id": "1",
          "time": "20210826 11:13"
        },
        {
          "id": "MDRW2100016229.1.13",
          "form": "? 뭐지",
          "original_form": "? 뭐지",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016229.1.14",
          "form": "여튼 줄여야됨",
          "original_form": "여튼 줄여야됨",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016229.1.15",
          "form": "오토바이 부품도 사야되는데",
          "original_form": "오토바이 부품도 사야되는데",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016229.1.16",
          "form": "돈 많이 필요하넹",
          "original_form": "돈 많이 필요하넹",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016229.1.17",
          "form": "부품은",
          "original_form": "부품은",
          "speaker_id": "1",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016229.1.18",
          "form": "생일선물로 받는거 아니엿나요",
          "original_form": "생일선물로 받는거 아니엿나요",
          "speaker_id": "1",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016229.1.19",
          "form": "그냥 애플왓치달라하고",
          "original_form": "그냥 애플왓치달라하고",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016229.1.20",
          "form": "부품은 제가사죠 뭐",
          "original_form": "부품은 제가사죠 뭐",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016229.1.21",
          "form": "왓치!왓치!",
          "original_form": "왓치!왓치!",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.22",
          "form": "시계는 왓치!",
          "original_form": "시계는 왓치!",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.23",
          "form": "왜요 간지 필요없다면서요",
          "original_form": "왜요 간지 필요없다면서요",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.24",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.25",
          "form": "30짜리",
          "original_form": "30짜리",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.26",
          "form": "뭐사달라하면될까요",
          "original_form": "뭐사달라하면될까요",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.27",
          "form": "백화점 상품권이요",
          "original_form": "백화점 상품권이요",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.28",
          "form": "근데 가족도 아니고 친구한테ㅋㅋㅋ",
          "original_form": "근데 가족도 아니고 친구한테ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.29",
          "form": "머니머니해도",
          "original_form": "머니머니해도",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.30",
          "form": "머니..",
          "original_form": "머니..",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.31",
          "form": "이러기도쫌",
          "original_form": "이러기도쫌",
          "speaker_id": "2",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.32",
          "form": "근데 친구가",
          "original_form": "근데 친구가",
          "speaker_id": "1",
          "time": "20210826 11:24"
        },
        {
          "id": "MDRW2100016229.1.33",
          "form": "뭔잘못을 햇길래",
          "original_form": "뭔잘못을 햇길래",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016229.1.34",
          "form": "드립 제발...",
          "original_form": "드립 제발...",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016229.1.35",
          "form": "30만원짜리를 선물로 줄라고 하나요??",
          "original_form": "30만원짜리를 선물로 줄라고 하나요??",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016229.1.36",
          "form": "제가 됐다니까",
          "original_form": "제가 됐다니까",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016229.1.37",
          "form": "요즘 돈 많이 번다고",
          "original_form": "요즘 돈 많이 번다고",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016229.1.38",
          "form": "돈많을때 받아가라는데요?",
          "original_form": "돈많을때 받아가라는데요?",
          "speaker_id": "2",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100016229.1.39",
          "form": "좋은 친구 두셧네요",
          "original_form": "좋은 친구 두셧네요",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016229.1.40",
          "form": "저같아도 그렇게 말하는 친구있으몬",
          "original_form": "저같아도 그렇게 말하는 친구있으몬",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016229.1.41",
          "form": "비싼거 받을듯",
          "original_form": "비싼거 받을듯",
          "speaker_id": "1",
          "time": "20210826 11:26"
        }
      ]
    }
