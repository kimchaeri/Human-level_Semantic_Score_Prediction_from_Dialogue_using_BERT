      "utterance": [
        {
          "id": "MDRW2100036207.1.1",
          "form": "일하시면서 스트레스 받으신건 없으셨어요?",
          "original_form": "일하시면서 스트레스 받으신건 없으셨어요?",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036207.1.2",
          "form": "저희팀 직장상사가 회사에서 유명한 또라이였어요",
          "original_form": "저희팀 직장상사가 회사에서 유명한 또라이였어요",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036207.1.3",
          "form": "매일 눈물로 존버했어요,..",
          "original_form": "매일 눈물로 존버했어요,..",
          "speaker_id": "1",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100036207.1.4",
          "form": "하필.. 상사가....",
          "original_form": "하필.. 상사가....",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036207.1.5",
          "form": "name2님은 지금 재직중이신가요?!",
          "original_form": "&name2&님은 지금 재직중이신가요?!",
          "speaker_id": "1",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036207.1.6",
          "form": "유명한데 왜 상사.. 왜 안짤리시고..",
          "original_form": "유명한데 왜 상사.. 왜 안짤리시고..",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036207.1.7",
          "form": "취준중이에욥!!",
          "original_form": "취준중이에욥!!",
          "speaker_id": "2",
          "time": "20210830 19:25"
        },
        {
          "id": "MDRW2100036207.1.8",
          "form": "나름 철밥통 직종이라ㅎ,,",
          "original_form": "나름 철밥통 직종이라ㅎ,,",
          "speaker_id": "1",
          "time": "20210830 19:25"
        },
        {
          "id": "MDRW2100036207.1.9",
          "form": "희망 분야있으세요?!",
          "original_form": "희망 분야있으세요?!",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100036207.1.10",
          "form": "앗 그렇구나..",
          "original_form": "앗 그렇구나..",
          "speaker_id": "2",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036207.1.11",
          "form": "저 공무원 준비하고 있어요!",
          "original_form": "저 공무원 준비하고 있어요!",
          "speaker_id": "2",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036207.1.12",
          "form": "전공은 못살리겠어서..ㅠ",
          "original_form": "전공은 못살리겠어서..ㅠ",
          "speaker_id": "2",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036207.1.13",
          "form": "저도 공시했었어요!",
          "original_form": "저도 공시했었어요!",
          "speaker_id": "1",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036207.1.14",
          "form": "해커스공무원",
          "original_form": "해커스공무원",
          "speaker_id": "1",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036207.1.15",
          "form": "다녔어ㅛㅋㅋㅋㄱ",
          "original_form": "다녔어ㅛㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 19:29"
        },
        {
          "id": "MDRW2100036207.1.16",
          "form": "오오! 하시다 다른거 하시는거에요??",
          "original_form": "오오! 하시다 다른거 하시는거에요??",
          "speaker_id": "2",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100036207.1.17",
          "form": "저는 해커스 인강 듣고있어요!!!!",
          "original_form": "저는 해커스 인강 듣고있어요!!!!",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036207.1.18",
          "form": "우와!!!",
          "original_form": "우와!!!",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036207.1.19",
          "form": "이중석선생님!!",
          "original_form": "이중석선생님!!",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036207.1.20",
          "form": "네  ㅠ 공시안되서",
          "original_form": "네  ㅠ 공시안되서",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036207.1.21",
          "form": "이곳저곳 기웃",
          "original_form": "이곳저곳 기웃",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100036207.1.22",
          "form": "저도..ㅋㅋ 그러다갸 도전해보려구요",
          "original_form": "저도..ㅋㅋ 그러다갸 도전해보려구요",
          "speaker_id": "2",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100036207.1.23",
          "form": "근데 후회해요! 공시좀더열심히할걸싶어요ㅋㅋ",
          "original_form": "근데 후회해요! 공시좀더열심히할걸싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036207.1.24",
          "form": "좀 놀면서 공부했어서,,",
          "original_form": "좀 놀면서 공부했어서,,",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036207.1.25",
          "form": "한량",
          "original_form": "한량",
          "speaker_id": "1",
          "time": "20210830 19:42"
        },
        {
          "id": "MDRW2100036207.1.26",
          "form": "사실 저도 지금 그러는중이에요..",
          "original_form": "사실 저도 지금 그러는중이에요..",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100036207.1.27",
          "form": "이거 이벤트ㅈ한다고ㅋㅋㅋ 열과 성을 다하고 있어요ㅋㅋ",
          "original_form": "이거 이벤트ㅈ한다고ㅋㅋㅋ 열과 성을 다하고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:46"
        }
      ]
    }
