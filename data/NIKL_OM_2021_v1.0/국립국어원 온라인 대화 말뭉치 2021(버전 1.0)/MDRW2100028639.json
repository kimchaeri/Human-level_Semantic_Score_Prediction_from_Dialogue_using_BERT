      "utterance": [
        {
          "id": "MDRW2100028639.1.1",
          "form": "신발왔다!",
          "original_form": "신발왔다!",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100028639.1.2",
          "form": "오 디자인 괜찮아??",
          "original_form": "오 디자인 괜찮아??",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028639.1.3",
          "form": "사진 찍어주고싶은데 지금 회사보안어플이랑 카메라 충돌로 안찍히더라",
          "original_form": "사진 찍어주고싶은데 지금 회사보안어플이랑 카메라 충돌로 안찍히더라",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028639.1.4",
          "form": "음 괜찮은지 모르겠어ㅋㅋㅋ",
          "original_form": "음 괜찮은지 모르겠어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028639.1.5",
          "form": "안괜찮은지도 몰겠고ㅋ",
          "original_form": "안괜찮은지도 몰겠고ㅋ",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028639.1.6",
          "form": "고무 냄새는 남ㅋ",
          "original_form": "고무 냄새는 남ㅋ",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100028639.1.7",
          "form": "애매한가보네ㅜㅜ",
          "original_form": "애매한가보네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028639.1.8",
          "form": "비올때만 신는걸로",
          "original_form": "비올때만 신는걸로",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028639.1.9",
          "form": "그러게 비올때신을수있기를ㅎ",
          "original_form": "그러게 비올때신을수있기를ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100028639.1.10",
          "form": "비올때는 괜찮을것 같아 돈주고 샀으니 어쨌든 신어야한다는 생각",
          "original_form": "비올때는 괜찮을것 같아 돈주고 샀으니 어쨌든 신어야한다는 생각",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028639.1.11",
          "form": "응 요즘고무신 디자인 진짜 다양하더라",
          "original_form": "응 요즘고무신 디자인 진짜 다양하더라",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028639.1.12",
          "form": "그렇구만 비오는날에 편할듯!",
          "original_form": "그렇구만 비오는날에 편할듯!",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028639.1.13",
          "form": "나 대게김이랑 명태채 샀어",
          "original_form": "나 대게김이랑 명태채 샀어",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028639.1.14",
          "form": "대게김? 맛 다른지 궁금ㅎ",
          "original_form": "대게김? 맛 다른지 궁금ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028639.1.15",
          "form": "난 곱창김 맛있더라",
          "original_form": "난 곱창김 맛있더라",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028639.1.16",
          "form": "오 곱창김 한창 유행했었지",
          "original_form": "오 곱창김 한창 유행했었지",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100028639.1.17",
          "form": "대게김 오늘시켜봄 가격 48봉에 사천얼마라",
          "original_form": "대게김 오늘시켜봄 가격 48봉에 사천얼마라",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100028639.1.18",
          "form": "이상한맛 날것 같아 다들안산거같아",
          "original_form": "이상한맛 날것 같아 다들안산거같아",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028639.1.19",
          "form": "재고남으니 저렴하게판듯",
          "original_form": "재고남으니 저렴하게판듯",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028639.1.20",
          "form": "잘샀다",
          "original_form": "잘샀다",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100028639.1.21",
          "form": "가격이 사야하네ㅎ",
          "original_form": "가격이 사야하네ㅎ",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100028639.1.22",
          "form": "ㅇㅇ 그냥 김맛일것 같아 나 흑마늘김도 먹어봤지만 흑마늘맛 1도 안나더라",
          "original_form": "ㅇㅇ 그냥 김맛일것 같아 나 흑마늘김도 먹어봤지만 흑마늘맛 1도 안나더라",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100028639.1.23",
          "form": "엥? 독특하다ㅎㅎ",
          "original_form": "엥? 독특하다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100028639.1.24",
          "form": "근데 김맛이 안남 김 먹는게 아니니까",
          "original_form": "근데 김맛이 안남 김 먹는게 아니니까",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100028639.1.25",
          "form": "맛에 위화감없이 만들거같기도하네",
          "original_form": "맛에 위화감없이 만들거같기도하네",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100028639.1.26",
          "form": "나 진짜 요즘 엄청삼",
          "original_form": "나 진짜 요즘 엄청삼",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100028639.1.27",
          "form": "록시땅샘플샀는데 포장 이쁘게해서 왔더라",
          "original_form": "록시땅샘플샀는데 포장 이쁘게해서 왔더라",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100028639.1.28",
          "form": "2500원 안 아까웠엉",
          "original_form": "2500원 안 아까웠엉",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100028639.1.29",
          "form": "양도 많고 패키지도 고급스러워서",
          "original_form": "양도 많고 패키지도 고급스러워서",
          "speaker_id": "1",
          "time": "20210903 00:21"
        }
      ]
