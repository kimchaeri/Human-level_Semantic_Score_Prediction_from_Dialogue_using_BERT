        {
          "id": "MDRW2100040052.1.1",
          "form": "먹고싶네요 뭔가",
          "original_form": "먹고싶네요 뭔가",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040052.1.2",
          "form": "ㅋㅋㅋㅋㅋ배고픈시간이에요",
          "original_form": "ㅋㅋㅋㅋㅋ배고픈시간이에요",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040052.1.3",
          "form": "그치만 졸려요",
          "original_form": "그치만 졸려요",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040052.1.4",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040052.1.5",
          "form": "낼먹어야죵",
          "original_form": "낼먹어야죵",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100040052.1.6",
          "form": "마자요 지금 먹으면 바로자서 소화안될거에요",
          "original_form": "마자요 지금 먹으면 바로자서 소화안될거에요",
          "speaker_id": "1",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040052.1.7",
          "form": "낼뭐먹을지 찾아보며 참아봅시다",
          "original_form": "낼뭐먹을지 찾아보며 참아봅시다",
          "speaker_id": "1",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100040052.1.8",
          "form": "아 오늘 검도장에서 먹는얘기햇는데",
          "original_form": "아 오늘 검도장에서 먹는얘기햇는데",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040052.1.9",
          "form": "방어 곱창",
          "original_form": "방어 곱창",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040052.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ방어!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ방어!!!!",
          "speaker_id": "1",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100040052.1.11",
          "form": "세상에나 벌써 또 방어철이라니",
          "original_form": "세상에나 벌써 또 방어철이라니",
          "speaker_id": "1",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100040052.1.12",
          "form": "그죠 전어도요",
          "original_form": "그죠 전어도요",
          "speaker_id": "2",
          "time": "20210904 09:37"
        },
        {
          "id": "MDRW2100040052.1.13",
          "form": "우왕 가을전어..!! 대박",
          "original_form": "우왕 가을전어..!! 대박",
          "speaker_id": "1",
          "time": "20210904 09:37"
        },
        {
          "id": "MDRW2100040052.1.14",
          "form": "저 옛날엔 곱창 막창좋아했는데",
          "original_form": "저 옛날엔 곱창 막창좋아했는데",
          "speaker_id": "1",
          "time": "20210904 09:38"
        },
        {
          "id": "MDRW2100040052.1.15",
          "form": "뭔가 언제부턴가 못먹게됐어요",
          "original_form": "뭔가 언제부턴가 못먹게됐어요",
          "speaker_id": "1",
          "time": "20210904 09:39"
        },
        {
          "id": "MDRW2100040052.1.16",
          "form": "ㅠ",
