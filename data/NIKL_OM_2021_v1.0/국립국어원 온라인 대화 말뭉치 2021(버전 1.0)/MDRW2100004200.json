          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.5",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.6",
          "form": "맛있어?",
          "original_form": "맛있어?",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.7",
          "form": "맛없으면 끔찍할듯",
          "original_form": "맛없으면 끔찍할듯",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.8",
          "form": "ㄴㄴ 맛압서",
          "original_form": "ㄴㄴ 맛압서",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.9",
          "form": "근데 건강에 졸댜",
          "original_form": "근데 건강에 졸댜",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.10",
          "form": "아 끔찍하지는않다",
          "original_form": "아 끔찍하지는않다",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.11",
          "form": "무맛",
          "original_form": "무맛",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.12",
          "form": "흡수율도 빠르고",
          "original_form": "흡수율도 빠르고",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.13",
          "form": "물 없이 먹어도되고",
          "original_form": "물 없이 먹어도되고",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004200.1.14",
