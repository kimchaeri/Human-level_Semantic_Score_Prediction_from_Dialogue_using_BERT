      "utterance": [
        {
          "id": "MDRW2100018093.1.1",
          "form": "중고거래 하시나요?",
          "original_form": "중고거래 하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018093.1.2",
          "form": "예전에 해본 적 있는데 최근엔 없네요.",
          "original_form": "예전에 해본 적 있는데 최근엔 없네요.",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018093.1.3",
          "form": "저는 최근에 당근마켓에 글 올렸는데 답이 없네요ㅠ",
          "original_form": "저는 최근에 당근마켓에 글 올렸는데 답이 없네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018093.1.4",
          "form": "어떤거 판매하려구요?",
          "original_form": "어떤거 판매하려구요?",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018093.1.5",
          "form": "홈트로 필라테스 하려고 산 용품이요!",
          "original_form": "홈트로 필라테스 하려고 산 용품이요!",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018093.1.6",
          "form": "헉 그 요가링 같은거요?",
          "original_form": "헉 그 요가링 같은거요?",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018093.1.7",
          "form": "폼블러처럼 생긴거요!",
          "original_form": "폼블러처럼 생긴거요!",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018093.1.8",
          "form": "은근 시원한데 잘 안하게 되네요ㅠ",
          "original_form": "은근 시원한데 잘 안하게 되네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018093.1.9",
          "form": "아 ㅋㅋ 저 그거 발받침대로 쓰고 있어요",
          "original_form": "아 ㅋㅋ 저 그거 발받침대로 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018093.1.10",
          "form": "강추 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "강추 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018093.1.11",
          "form": "ㅋㅋㅋ앗ㅋㅋ",
          "original_form": "ㅋㅋㅋ앗ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018093.1.12",
          "form": "해봐야겠어요",
          "original_form": "해봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018093.1.13",
          "form": "발바닥으로 굴려도 좀 시원해요",
          "original_form": "발바닥으로 굴려도 좀 시원해요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100018093.1.14",
          "form": "오 정말요! 지금 해보고 있는데 좋네요",
          "original_form": "오 정말요! 지금 해보고 있는데 좋네요",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018093.1.15",
          "form": "그쵸? 중고거래 될 때까지 그렇게라도 써봐요 ㅎㅎ",
          "original_form": "그쵸? 중고거래 될 때까지 그렇게라도 써봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018093.1.16",
          "form": "넵ㅎㅎ 감사합니다",
          "original_form": "넵ㅎㅎ 감사합니다",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018093.1.17",
          "form": "이러다 제가 계속 쓸거 같아요",
          "original_form": "이러다 제가 계속 쓸거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018093.1.18",
          "form": "저도 생각난 김에 지금 종아리에 하는데 폼블러 역시 좋네요 ㅎㅎ",
