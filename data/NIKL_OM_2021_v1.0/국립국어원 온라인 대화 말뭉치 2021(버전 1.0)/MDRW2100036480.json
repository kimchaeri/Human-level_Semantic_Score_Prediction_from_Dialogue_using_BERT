        },
        {
          "id": "MDRW2100036480.1.2",
          "form": "엉 바르셀로나 가면 다 가우디 얘기하던데",
          "original_form": "엉 바르셀로나 가면 다 가우디 얘기하던데",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036480.1.3",
          "form": "진짜 천재인듯",
          "original_form": "진짜 천재인듯",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036480.1.4",
          "form": "엉 천재 건축가 지금 태어나도 천재라고 불릴듯",
          "original_form": "엉 천재 건축가 지금 태어나도 천재라고 불릴듯",
          "speaker_id": "1",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036480.1.5",
          "form": "그렇지 시대를 앞선 건축가다",
          "original_form": "그렇지 시대를 앞선 건축가다",
          "speaker_id": "2",
          "time": "20210830 23:39"
        },
        {
          "id": "MDRW2100036480.1.6",
          "form": "응 사르라다 파밀리아 성당도 좋고",
          "original_form": "응 사르라다 파밀리아 성당도 좋고",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036480.1.7",
          "form": "구엘 공원은 꼭 가야한다",
          "original_form": "구엘 공원은 꼭 가야한다",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036480.1.8",
          "form": "맞다 구엘공원",
          "original_form": "맞다 구엘공원",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036480.1.9",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036480.1.10",
          "form": "카사 밀라도 좋잖아",
          "original_form": "카사 밀라도 좋잖아",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
