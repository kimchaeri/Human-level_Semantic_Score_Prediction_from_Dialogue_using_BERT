        {
          "id": "MDRW2100009836.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210823 10:44"
        },
        {
          "id": "MDRW2100009836.1.2",
          "form": "최소한만 해요",
          "original_form": "최소한만 해요",
          "speaker_id": "2",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100009836.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100009836.1.4",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100009836.1.5",
          "form": "저는 싱크대청소담당이에요",
          "original_form": "저는 싱크대청소담당이에요",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009836.1.6",
          "form": "음쓰 냄새 많이 나죠?",
          "original_form": "음쓰 냄새 많이 나죠?",
          "speaker_id": "1",
          "time": "20210823 10:50"
        },
        {
          "id": "MDRW2100009836.1.7",
          "form": "냉동실보관해서 냄새앉나요",
          "original_form": "냉동실보관해서 냄새앉나요",
          "speaker_id": "2",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009836.1.8",
          "form": "안나요",
          "original_form": "안나요",
          "speaker_id": "2",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100009836.1.9",
          "form": "냉동실 꿀팁",
          "original_form": "냉동실 꿀팁",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009836.1.10",
          "form": "네 상하기전에 빨리넣으면 냄새안나요",
          "original_form": "네 상하기전에 빨리넣으면 냄새안나요",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009836.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009836.1.12",
          "form": "냉동실에 음식인채로 있는거라서요ㅎ",
          "original_form": "냉동실에 음식인채로 있는거라서요ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009836.1.13",
          "form": "저는 바로바로 버려요",
          "original_form": "저는 바로바로 버려요",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009836.1.14",
          "form": "그게 제일좋죵",
          "original_form": "그게 제일좋죵",
