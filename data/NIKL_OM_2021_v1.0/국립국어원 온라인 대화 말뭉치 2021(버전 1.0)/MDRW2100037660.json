      "utterance": [
        {
          "id": "MDRW2100037660.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037660.1.2",
          "form": "쌀밥 좋아하세요?",
          "original_form": "쌀밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100037660.1.3",
          "form": "오ㅑ요",
          "original_form": "오ㅑ요",
          "speaker_id": "2",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100037660.1.4",
          "form": "제가 안남미를 샀어요",
          "original_form": "제가 안남미를 샀어요",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037660.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037660.1.6",
          "form": "정확하게는 태국 자스민 쌀이죠",
          "original_form": "정확하게는 태국 자스민 쌀이죠",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100037660.1.7",
          "form": "그래세여? 목적이 뭡니까",
          "original_form": "그래세여? 목적이 뭡니까",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100037660.1.8",
          "form": "밥으로만 해먹기 아깝다??",
          "original_form": "밥으로만 해먹기 아깝다??",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037660.1.9",
          "form": "휴.. 어렵네요",
          "original_form": "휴.. 어렵네요",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037660.1.10",
          "form": "소영이네 돈까스물회라는 곳 아세요?",
          "original_form": "소영이네 돈까스물회라는 곳 아세요?",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037660.1.11",
          "form": "그렇게 드세면되요",
          "original_form": "그렇게 드세면되요",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037660.1.12",
          "form": "밥으로 일단 먹어봐야죠",
          "original_form": "밥으로 일단 먹어봐야죠",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037660.1.13",
          "form": "국수요?",
          "original_form": "국수요?",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037660.1.14",
          "form": "일단 네",
          "original_form": "일단 네",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100037660.1.15",
          "form": "님은 어때요?",
          "original_form": "님은 어때요?",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100037660.1.16",
          "form": "저는 차라리 새로운시도를 해보려고요",
          "original_form": "저는 차라리 새로운시도를 해보려고요",
          "speaker_id": "2",
          "time": "20210901 14:44"
