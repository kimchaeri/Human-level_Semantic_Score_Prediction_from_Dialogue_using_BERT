      "utterance": [
        {
          "id": "MDRW2100004551.1.1",
          "form": "이렇게 비올때 땡기는 음식같은거 있으신가요?",
          "original_form": "이렇게 비올때 땡기는 음식같은거 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004551.1.2",
          "form": "혹시 이렇게 비올때",
          "original_form": "혹시 이렇게 비올때",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004551.1.3",
          "form": "땡기시는 음식 있으신가요?",
          "original_form": "땡기시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004551.1.4",
          "form": "비오면 부침개?ㅎㅎ",
          "original_form": "비오면 부침개?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004551.1.5",
          "form": "호박썰어넣고 냠냠",
          "original_form": "호박썰어넣고 냠냠",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004551.1.6",
          "form": "그것도 좋죠",
          "original_form": "그것도 좋죠",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004551.1.7",
          "form": "이렇게 비올땐 다들 전 생각하는건 국룰인가봐요 ㅎㅎ",
          "original_form": "이렇게 비올땐 다들 전 생각하는건 국룰인가봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004551.1.8",
          "form": "특히 해물파전이나 김치전에 동동주 아니면 막걸리 한잔 먹는걸 선호하죠",
          "original_form": "특히 해물파전이나 김치전에 동동주 아니면 막걸리 한잔 먹는걸 선호하죠",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004551.1.9",
          "form": "저도 그렇고요!",
          "original_form": "저도 그렇고요!",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004551.1.10",
          "form": "김치전도 먹고싶네요 ㅎ 감자전도 맛난데",
          "original_form": "김치전도 먹고싶네요 ㅎ 감자전도 맛난데",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004551.1.11",
          "form": "점심은 뭐드셨어요?",
          "original_form": "점심은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004551.1.12",
          "form": "때마침 점심을 전을 먹었죠",
          "original_form": "때마침 점심을 전을 먹었죠",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004551.1.13",
          "form": "파전먹었어요!",
          "original_form": "파전먹었어요!",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004551.1.14",
          "form": "오ㅎ 파전 맛있죠 집에서 해드셨어요?",
          "original_form": "오ㅎ 파전 맛있죠 집에서 해드셨어요?",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004551.1.15",
          "form": "네! 맛있더라구요 ㅎㅎ",
          "original_form": "네! 맛있더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004551.1.16",
          "form": "막걸리 안산게 좀 흠이긴하지만요",
          "original_form": "막걸리 안산게 좀 흠이긴하지만요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004551.1.17",
          "form": "ㅎㅎ 이럴땐 역시 막걸리인데 아쉽긴하네요",
          "original_form": "ㅎㅎ 이럴땐 역시 막걸리인데 아쉽긴하네요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004551.1.18",
          "form": "직접 요리하신거예요?ㅎㅎ",
          "original_form": "직접 요리하신거예요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004551.1.19",
          "form": "어머니랑 같이 했어요!",
          "original_form": "어머니랑 같이 했어요!",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004551.1.20",
          "form": "저도 전 먹고싶네요 ㅎ",
          "original_form": "저도 전 먹고싶네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.21",
          "form": "낮에 감자고로케랑 곱창볶음 먹었어요",
          "original_form": "낮에 감자고로케랑 곱창볶음 먹었어요",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.22",
          "form": "만들어서 먹는것도 좋구",
          "original_form": "만들어서 먹는것도 좋구",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.23",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.24",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.25",
          "form": "이런 날씨에 곱창도 또 어울리죠~",
          "original_form": "이런 날씨에 곱창도 또 어울리죠~",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.26",
          "form": "인스턴트죠ㅋ",
          "original_form": "인스턴트죠ㅋ",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004551.1.27",
          "form": "안주야 곱창볶음",
          "original_form": "안주야 곱창볶음",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004551.1.28",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004551.1.29",
          "form": "술은 못해서  식사대용이요ㅋ",
          "original_form": "술은 못해서  식사대용이요ㅋ",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004551.1.30",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004551.1.31",
          "form": "식사로도 괜찮긴하죠 ㅎㅎ",
          "original_form": "식사로도 괜찮긴하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004551.1.32",
          "form": "밥볶아먹을려다 참았어요",
          "original_form": "밥볶아먹을려다 참았어요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004551.1.33",
          "form": "허억",
          "original_form": "허억",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004551.1.34",
          "form": "밥볶는거 진짜 최고긴한데 ㅠㅠ",
          "original_form": "밥볶는거 진짜 최고긴한데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:15"
        }
      ]
    }
