      "utterance": [
        {
          "id": "MDRW2100020063.1.1",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020063.1.2",
          "form": "머야 얼굴을 모르는거야",
          "original_form": "머야 얼굴을 모르는거야",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020063.1.3",
          "form": "ㅇㅇ 주인공은 일곱명 중에 누가 악역인지 몰라",
          "original_form": "ㅇㅇ 주인공은 일곱명 중에 누가 악역인지 몰라",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020063.1.4",
          "form": "자기 잃어버린 쌍둥이 찾아서 그 악역 찾는게",
          "original_form": "자기 잃어버린 쌍둥이 찾아서 그 악역 찾는게",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020063.1.5",
          "form": "이 게임의 메인스토리임",
          "original_form": "이 게임의 메인스토리임",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020063.1.6",
          "form": "일곱명 얼굴을 몰라.... 세상에 내 픽을 몰라",
          "original_form": "일곱명 얼굴을 몰라.... 세상에 내 픽을 몰라",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020063.1.7",
          "form": "아냐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아냐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020063.1.8",
          "form": "일곱명중에",
          "original_form": "일곱명중에",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020063.1.9",
          "form": "이제 세 명 공개됐음",
          "original_form": "이제 세 명 공개됐음",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020063.1.10",
          "form": "(땅침)",
          "original_form": "(땅침)",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020063.1.11",
          "form": "같은 얼굴일..테니까... 그래... 일단 맞춰봅시다...",
          "original_form": "같은 얼굴일..테니까... 그래... 일단 맞춰봅시다...",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020063.1.12",
          "form": "꼭",
          "original_form": "꼭",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020063.1.13",
          "form": "같은 얼굴이라는 법은 없지",
          "original_form": "같은 얼굴이라는 법은 없지",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020063.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020063.1.15",
          "form": "그치만 이 게임 캐릭터 디자인이 다양하니까",
          "original_form": "그치만 이 게임 캐릭터 디자인이 다양하니까",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020063.1.16",
          "form": "이 중에 네 취향이 하나 쯤은 있겠지",
          "original_form": "이 중에 네 취향이 하나 쯤은 있겠지",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020063.1.17",
          "form": "머..!? ㅋㅋㅋㅋㅋㅋㅋㅋ 그래 내 취향 늘 다양한걸.. 여기 중에 하나 골라 하면 고를 사람이 나야",
          "original_form": "머..!? ㅋㅋㅋㅋㅋㅋㅋㅋ 그래 내 취향 늘 다양한걸.. 여기 중에 하나 골라 하면 고를 사람이 나야",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020063.1.18",
          "form": "응 그럴거같아서 ㅋㅋㅋㅋㅋ",
          "original_form": "응 그럴거같아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020063.1.19",
          "form": "새로 나온 캐릭터 중에 코코미 라고 있는대",
          "original_form": "새로 나온 캐릭터 중에 코코미 라고 있는대",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020063.1.20",
          "form": "분홍머리에 좀 데리 취향으로 생긴거같아",
          "original_form": "분홍머리에 좀 데리 취향으로 생긴거같아",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020063.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 얼굴 미리 찾아봐야 하나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 얼굴 미리 찾아봐야 하나",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020063.1.22",
          "form": "제 취향 이렇게 적나라하게 드러나고",
          "original_form": "제 취향 이렇게 적나라하게 드러나고",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020063.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020063.1.24",
          "form": "데리 약간 샤랄라하게 생긴 캐릭터 좋아하잖아",
          "original_form": "데리 약간 샤랄라하게 생긴 캐릭터 좋아하잖아",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020063.1.25",
          "form": "샤랄라도 좋아하지만 남캐는 투박한 것도...정도껏...좋아해(?",
          "original_form": "샤랄라도 좋아하지만 남캐는 투박한 것도...정도껏...좋아해(?",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020063.1.26",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020063.1.27",
          "form": "여긴 투박한 남캐 기대하면 안돼...",
          "original_form": "여긴 투박한 남캐 기대하면 안돼...",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.28",
          "form": "그나마 골격 있는 남캐가 종려야",
          "original_form": "그나마 골격 있는 남캐가 종려야",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.29",
          "form": "음 괜찮아 미형도 좋아..하는데 넘 마른건 고민을",
          "original_form": "음 괜찮아 미형도 좋아..하는데 넘 마른건 고민을",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.30",
          "form": "종려..잘생기긴 했더라(?",
          "original_form": "종려..잘생기긴 했더라(?",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.31",
          "form": "코코미 보고 왔고",
          "original_form": "코코미 보고 왔고",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.32",
          "form": "제 취향 넘 투리구슬",
          "original_form": "제 취향 넘 투리구슬",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.33",
          "form": "그치 종려 잘생겼어",
          "original_form": "그치 종려 잘생겼어",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.34",
          "form": "당했네요",
          "original_form": "당했네요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.35",
          "form": "어때 코코미 좀 혹하니?",
          "original_form": "어때 코코미 좀 혹하니?",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.36",
          "form": "응 잘생겼어",
          "original_form": "응 잘생겼어",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020063.1.37",
          "form": "가끔 볼때마다 종려가 쟤군 하고 알아봐",
          "original_form": "가끔 볼때마다 종려가 쟤군 하고 알아봐",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020063.1.38",
          "form": "갈발남캐 잘생긴 놈 흔하지 않다",
          "original_form": "갈발남캐 잘생긴 놈 흔하지 않다",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020063.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020063.1.40",
          "form": "갈발남캐가 날카롭게 잘생기기 쉽지 않다구",
          "original_form": "갈발남캐가 날카롭게 잘생기기 쉽지 않다구",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020063.1.41",
          "form": "맞아 대부분 무해한 댕댕이처럼 나온다구",
          "original_form": "맞아 대부분 무해한 댕댕이처럼 나온다구",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020063.1.42",
          "form": "김백기는 그나마 좀 날카롭나 싶었지만 요즘은 무한 댕댕이에",
          "original_form": "김백기는 그나마 좀 날카롭나 싶었지만 요즘은 무한 댕댕이에",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020063.1.43",
          "form": "스피나 쓰알에서 계속 덮치는 놈인데(?",
          "original_form": "스피나 쓰알에서 계속 덮치는 놈인데(?",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020063.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 요즘 럽프듀 너무 au 많이해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 요즘 럽프듀 너무 au 많이해서",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020063.1.45",
          "form": "내가 알던 게임이 아닌데... 싶고",
          "original_form": "내가 알던 게임이 아닌데... 싶고",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
