      "utterance": [
        {
          "id": "MDRW2100043835.1.1",
          "form": "네 ㅎㅎ 좋아하죠 ㅎㅎ",
          "original_form": "네 ㅎㅎ 좋아하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043835.1.2",
          "form": "최근에 제주도 갔었는데",
          "original_form": "최근에 제주도 갔었는데",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043835.1.3",
          "form": "좋았어요 ㅎㅎ",
          "original_form": "좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043835.1.4",
          "form": "와 제주도",
          "original_form": "와 제주도",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043835.1.5",
          "form": "좋으셨겠네요",
          "original_form": "좋으셨겠네요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043835.1.6",
          "form": "맞아요 ㅎㅎ 정말 오랜만에 갔는데",
          "original_form": "맞아요 ㅎㅎ 정말 오랜만에 갔는데",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043835.1.7",
          "form": "좋았어요 ㅎㅎ",
          "original_form": "좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043835.1.8",
          "form": "제주도 안간지도 몇년 됐네요",
          "original_form": "제주도 안간지도 몇년 됐네요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043835.1.9",
          "form": "작년에 가려다 못갔어요",
          "original_form": "작년에 가려다 못갔어요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043835.1.10",
          "form": "그쵸 코로나라 기회보기가 힘드니 ㅠ",
          "original_form": "그쵸 코로나라 기회보기가 힘드니 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043835.1.11",
          "form": "네 맞아요ㅜ",
          "original_form": "네 맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043835.1.12",
          "form": "코로나 전엔 해외도 자주 가고 했는데",
          "original_form": "코로나 전엔 해외도 자주 가고 했는데",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043835.1.13",
          "form": "ㅠㅠ곧 갈수있을겁니다",
          "original_form": "ㅠㅠ곧 갈수있을겁니다",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043835.1.14",
          "form": "네 코로나 잠잠해지는대로 국내여행이라도 다녀오려고요!",
          "original_form": "네 코로나 잠잠해지는대로 국내여행이라도 다녀오려고요!",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043835.1.15",
          "form": "잽싸게 가셔야해요 ㅎㅎ 그러다 다시 단계올라가니 ㅠ",
          "original_form": "잽싸게 가셔야해요 ㅎㅎ 그러다 다시 단계올라가니 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043835.1.16",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043835.1.17",
          "form": "국내여행은 어디가고싶으신가요",
          "original_form": "국내여행은 어디가고싶으신가요",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043835.1.18",
          "form": "음 부산 가고 싶어요",
          "original_form": "음 부산 가고 싶어요",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043835.1.19",
          "form": "오우 부산 어디 가장 가고싶으신가요?",
          "original_form": "오우 부산 어디 가장 가고싶으신가요?",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043835.1.20",
          "form": "광안리 해수욕장이랑",
          "original_form": "광안리 해수욕장이랑",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043835.1.21",
          "form": "맛집들이요ㅎㅎ",
          "original_form": "맛집들이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043835.1.22",
          "form": "좋아요 이쁘고 맛난거많아요!",
          "original_form": "좋아요 이쁘고 맛난거많아요!",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043835.1.23",
          "form": "부산놀러오세요 ㅎㅎ",
          "original_form": "부산놀러오세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043835.1.24",
          "form": "아 name2님 부산 사시나요?ㅋㅋ",
