        },
        {
          "id": "MDRW2100027473.1.2",
          "form": "체육시간이 좋았어",
          "original_form": "체육시간이 좋았어",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027473.1.3",
          "form": "나는 미술시간이 좋았어",
          "original_form": "나는 미술시간이 좋았어",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027473.1.4",
          "form": "미술시간은 싫었어",
          "original_form": "미술시간은 싫었어",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027473.1.5",
          "form": "학교생활 추억이 많아",
          "original_form": "학교생활 추억이 많아",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027473.1.6",
          "form": "수업은 길고",
          "original_form": "수업은 길고",
          "speaker_id": "2",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027473.1.7",
          "form": "쉬는시간은 짧았지",
          "original_form": "쉬는시간은 짧았지",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027473.1.8",
          "form": "수업이 몇분이었지",
          "original_form": "수업이 몇분이었지",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027473.1.9",
          "form": "아마 45분일거야",
          "original_form": "아마 45분일거야",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027473.1.10",
          "form": "한반에 몇명이엇지",
          "original_form": "한반에 몇명이엇지",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027473.1.11",
          "form": "45명은 되지",
          "original_form": "45명은 되지",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027473.1.12",
          "form": "요즘 학교는 몇명이야",
          "original_form": "요즘 학교는 몇명이야",
