      "utterance": [
        {
          "id": "MDRW2100025460.1.1",
          "form": "최근 구매 물건 있으신가요",
          "original_form": "최근 구매 물건 있으신가요",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025460.1.2",
          "form": "저 버즈2 구매했어요",
          "original_form": "저 버즈2 구매했어요",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025460.1.3",
          "form": "구매하신거있나요?",
          "original_form": "구매하신거있나요?",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025460.1.4",
          "form": "오 그거 신제품 아닌가요",
          "original_form": "오 그거 신제품 아닌가요",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025460.1.5",
          "form": "사고싶던데",
          "original_form": "사고싶던데",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025460.1.6",
          "form": "저는 어제 홈플러스에서 장봤어요",
          "original_form": "저는 어제 홈플러스에서 장봤어요",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025460.1.7",
          "form": "맞아요맞아요 색에 혹해서 질러버렸죠",
          "original_form": "맞아요맞아요 색에 혹해서 질러버렸죠",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025460.1.8",
          "form": "이제컴퓨터도 살거같아요 ㅋㅋ요즘 물욕이 올라서 ㅠㅠ",
          "original_form": "이제컴퓨터도 살거같아요 ㅋㅋ요즘 물욕이 올라서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100025460.1.9",
          "form": "와우 저도 노트북이나 컴퓨터 사고싶은데 돈이 부족해서",
          "original_form": "와우 저도 노트북이나 컴퓨터 사고싶은데 돈이 부족해서",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025460.1.10",
          "form": "저는 꽃게 철이라 꽃게 샀어요 살아있데요",
          "original_form": "저는 꽃게 철이라 꽃게 샀어요 살아있데요",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025460.1.11",
          "form": "꽃게 맛있죠.. 저도 게장 먹구싶네요 ㅠㅠㅠ",
          "original_form": "꽃게 맛있죠.. 저도 게장 먹구싶네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025460.1.12",
          "form": "손질이 쫌 무섭지만 ㅎㅎ",
          "original_form": "손질이 쫌 무섭지만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025460.1.13",
          "form": "뭐 또 구매 희망 물건 있으신가요",
          "original_form": "뭐 또 구매 희망 물건 있으신가요",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025460.1.14",
          "form": "컴퓨터 말고는 돈을 아껴야해서",
          "original_form": "컴퓨터 말고는 돈을 아껴야해서",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025460.1.15",
          "form": "아직엊ㅅ어요",
          "original_form": "아직엊ㅅ어요",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025460.1.16",
          "form": "구지 뽑자면 옷??",
          "original_form": "구지 뽑자면 옷??",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025460.1.17",
          "form": "이제 가을이라 옷쫌 질러야 할것같아요",
          "original_form": "이제 가을이라 옷쫌 질러야 할것같아요",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025460.1.18",
          "form": "저는 실내자전거 사고싶어요",
          "original_form": "저는 실내자전거 사고싶어요",
          "speaker_id": "1",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025460.1.19",
          "form": "운동기구요??",
          "original_form": "운동기구요??",
          "speaker_id": "2",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025460.1.20",
          "form": "저도 그런거 사고싶은데",
          "original_form": "저도 그런거 사고싶은데",
          "speaker_id": "2",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025460.1.21",
          "form": "네 집에서 하는",
          "original_form": "네 집에서 하는",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025460.1.22",
          "form": "나둘 자리가없어요 ㅋㅋㅋ",
          "original_form": "나둘 자리가없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025460.1.23",
          "form": "조립이 어럽데요 ㅜㅠ",
          "original_form": "조립이 어럽데요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100025460.1.24",
          "form": "그것도 그렇네요 자리도 좁아서 ㅋㅋ",
          "original_form": "그것도 그렇네요 자리도 좁아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:57"
        }
      ]
    }
