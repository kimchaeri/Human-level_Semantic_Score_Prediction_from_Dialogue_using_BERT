      "utterance": [
        {
          "id": "MDRW2100021780.1.1",
          "form": "여행 조아하시나요",
          "original_form": "여행 조아하시나요",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021780.1.2",
          "form": "네 요즘은 못가지만ㅜ",
          "original_form": "네 요즘은 못가지만ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021780.1.3",
          "form": "어디 갔다온곳있으신가요",
          "original_form": "어디 갔다온곳있으신가요",
          "speaker_id": "2",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021780.1.4",
          "form": "코로나전 대만이요!",
          "original_form": "코로나전 대만이요!",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021780.1.5",
          "form": "대만 좋았어요?",
          "original_form": "대만 좋았어요?",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021780.1.6",
          "form": "음식도맛있고 좋았네요ㅎㅎ",
          "original_form": "음식도맛있고 좋았네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021780.1.7",
          "form": "저도 나중에 가보고싶네요",
          "original_form": "저도 나중에 가보고싶네요",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021780.1.8",
          "form": "밀크티도 마시러",
          "original_form": "밀크티도 마시러",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021780.1.9",
          "form": "뭐가 제일 맛있었어요?",
          "original_form": "뭐가 제일 맛있었어요?",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021780.1.10",
          "form": "우육면이요ㅎㅎ",
          "original_form": "우육면이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100021780.1.11",
          "form": "그것도 유명하더라고요",
          "original_form": "그것도 유명하더라고요",
          "speaker_id": "2",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100021780.1.12",
          "form": "그맛을 잊을수없네요ㅠ",
          "original_form": "그맛을 잊을수없네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100021780.1.13",
          "form": "빨리 해외여행 자유롭게 갈수있었음 좋겠네요",
          "original_form": "빨리 해외여행 자유롭게 갈수있었음 좋겠네요",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100021780.1.14",
          "form": "어디가보셨나요?",
          "original_form": "어디가보셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100021780.1.15",
          "form": "저는 홍콩 일본 가봤어요",
          "original_form": "저는 홍콩 일본 가봤어요",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100021780.1.16",
          "form": "오 저도 둘다요!!",
          "original_form": "오 저도 둘다요!!",
          "speaker_id": "1",
          "time": "20210829 12:49"
        }
