      "utterance": [
        {
          "id": "MDRW2100033584.1.1",
          "form": "지금 나혼산 오상욱 나왔는데 커피머신기 샀네요.",
          "original_form": "지금 나혼산 오상욱 나왔는데 커피머신기 샀네요.",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033584.1.2",
          "form": "갖고싶네요~~",
          "original_form": "갖고싶네요~~",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033584.1.3",
          "form": "아 나혼자산다 나온거요?",
          "original_form": "아 나혼자산다 나온거요?",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100033584.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100033584.1.5",
          "form": "어디건가요?",
          "original_form": "어디건가요?",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033584.1.6",
          "form": "ㅎㅎ 울신랑 계좌가 회복해야 몰하나살텐데...",
          "original_form": "ㅎㅎ 울신랑 계좌가 회복해야 몰하나살텐데...",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033584.1.7",
          "form": "필립스 2200.",
          "original_form": "필립스 2200.",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033584.1.8",
          "form": "50만원 넘더라구요",
          "original_form": "50만원 넘더라구요",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033584.1.9",
          "form": "헉 비싼데요",
          "original_form": "헉 비싼데요",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033584.1.10",
          "form": "자동이라서요",
          "original_form": "자동이라서요",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033584.1.11",
          "form": "아 보통 10 만원대하던데요",
          "original_form": "아 보통 10 만원대하던데요",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033584.1.12",
          "form": "일리커피머신 요런거",
          "original_form": "일리커피머신 요런거",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033584.1.13",
          "form": "그건 캡슐이요.  싸더라구요. 캡슐이 비싸서그런지",
          "original_form": "그건 캡슐이요.  싸더라구요. 캡슐이 비싸서그런지",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033584.1.14",
          "form": "아 캡슐이라다른거예요?",
          "original_form": "아 캡슐이라다른거예요?",
          "speaker_id": "1",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033584.1.15",
          "form": "원두갈아나온건가요",
          "original_form": "원두갈아나온건가요",
          "speaker_id": "1",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033584.1.16",
          "form": "자동은 원두로~~",
          "original_form": "자동은 원두로~~",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033584.1.17",
          "form": "ㅎ 사고싶은데 내년이나..사놓고 안쓸까 걱정도되구요",
          "original_form": "ㅎ 사고싶은데 내년이나..사놓고 안쓸까 걱정도되구요",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033584.1.18",
          "form": "오ㅎ 카페에 있는그런간가요",
          "original_form": "오ㅎ 카페에 있는그런간가요",
          "speaker_id": "1",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033584.1.19",
          "form": "ㅎ 카페 미니버젼~~저건 편의점처럼 갈아서 바로 원액 나오는거같아요.",
          "original_form": "ㅎ 카페 미니버젼~~저건 편의점처럼 갈아서 바로 원액 나오는거같아요.",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033584.1.20",
          "form": "원두 맛있는거사면 집에서 먹어도 좋겠어요",
          "original_form": "원두 맛있는거사면 집에서 먹어도 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033584.1.21",
          "form": "전 커피메이커로 스벅에서 원두사다 먹었어요",
          "original_form": "전 커피메이커로 스벅에서 원두사다 먹었어요",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033584.1.22",
          "form": "원두는 커피빈이 더좋대요",
          "original_form": "원두는 커피빈이 더좋대요",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033584.1.23",
          "form": "귀찮아서 스틱원두로~~ 투썸꺼 맛있더라구요",
          "original_form": "귀찮아서 스틱원두로~~ 투썸꺼 맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033584.1.24",
          "form": "커피빈은 없어서...",
          "original_form": "커피빈은 없어서...",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033584.1.25",
          "form": "커피빈은 주로 서울에만있더라구요",
          "original_form": "커피빈은 주로 서울에만있더라구요",
