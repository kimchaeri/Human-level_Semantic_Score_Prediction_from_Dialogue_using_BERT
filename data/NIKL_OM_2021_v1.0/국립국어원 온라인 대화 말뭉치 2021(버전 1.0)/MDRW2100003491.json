      "utterance": [
        {
          "id": "MDRW2100003491.1.1",
          "form": "비혼인데 너는?",
          "original_form": "비혼인데 너는?",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003491.1.2",
          "form": "나도 비혼이야",
          "original_form": "나도 비혼이야",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003491.1.3",
          "form": "결혼은 좀 그래",
          "original_form": "결혼은 좀 그래",
          "speaker_id": "1",
          "time": "20210820 23:46"
        },
        {
          "id": "MDRW2100003491.1.4",
          "form": "혼자사는게 더 안정적일것 같아",
          "original_form": "혼자사는게 더 안정적일것 같아",
          "speaker_id": "1",
          "time": "20210820 23:47"
        },
        {
          "id": "MDRW2100003491.1.5",
          "form": "맞아 나는 혼자 사는게 더 편하고 좋을것 같아",
          "original_form": "맞아 나는 혼자 사는게 더 편하고 좋을것 같아",
          "speaker_id": "2",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003491.1.6",
          "form": "그럼 연애도 안하겠네?",
          "original_form": "그럼 연애도 안하겠네?",
          "speaker_id": "1",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003491.1.7",
          "form": "아마 그렇지 않을까?",
          "original_form": "아마 그렇지 않을까?",
          "speaker_id": "2",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003491.1.8",
          "form": "뭔가 큰일이 있지 않으면 그렇겠지",
          "original_form": "뭔가 큰일이 있지 않으면 그렇겠지",
          "speaker_id": "2",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003491.1.9",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003491.1.10",
          "form": "난 옛날에 태어났으면 외로웠을거야",
          "original_form": "난 옛날에 태어났으면 외로웠을거야",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003491.1.11",
          "form": "친구들 다 결혼해서",
          "original_form": "친구들 다 결혼해서",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003491.1.12",
          "form": "우리만은 결혼하지 말자 ㅎㅎ",
          "original_form": "우리만은 결혼하지 말자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:51"
        },
        {
          "id": "MDRW2100003491.1.13",
          "form": "ㅋㅋㅋ 그렇겠네 옛날이면",
          "original_form": "ㅋㅋㅋ 그렇겠네 옛날이면",
          "speaker_id": "2",
          "time": "20210820 23:52"
        },
        {
          "id": "MDRW2100003491.1.14",
          "form": "그래 우린 하지 말자",
          "original_form": "그래 우린 하지 말자",
          "speaker_id": "2",
          "time": "20210820 23:52"
        },
        {
          "id": "MDRW2100003491.1.15",
          "form": "결혼하는 친구들은 다들 어떻게 결혼하는 걸까",
          "original_form": "결혼하는 친구들은 다들 어떻게 결혼하는 걸까",
          "speaker_id": "1",
          "time": "20210821 00:42"
        },
        {
          "id": "MDRW2100003491.1.16",
          "form": "맞아 너무 신기해",
          "original_form": "맞아 너무 신기해",
          "speaker_id": "2",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003491.1.17",
          "form": "다들 결혼하겠다는 생각이 팍 드는걸까?",
          "original_form": "다들 결혼하겠다는 생각이 팍 드는걸까?",
          "speaker_id": "1",
          "time": "20210821 00:50"
        },
        {
          "id": "MDRW2100003491.1.18",
          "form": "심리가 궁금해",
          "original_form": "심리가 궁금해",
          "speaker_id": "1",
          "time": "20210823 10:53"
        },
        {
          "id": "MDRW2100003491.1.19",
          "form": "너는 어떻게 생각해?",
          "original_form": "너는 어떻게 생각해?",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100003491.1.20",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100003491.1.21",
          "form": "상대방이 엄청 마음에 든걸까?",
          "original_form": "상대방이 엄청 마음에 든걸까?",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100003491.1.22",
          "form": "아니면 부모님의 압박 아닐까?",
          "original_form": "아니면 부모님의 압박 아닐까?",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100003491.1.23",
          "form": "하긴 그럴수도 있겠네",
          "original_form": "하긴 그럴수도 있겠네",
          "speaker_id": "1",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100003491.1.24",
          "form": "넌 부모님이 하라고 하면 할거야?",
          "original_form": "넌 부모님이 하라고 하면 할거야?",
          "speaker_id": "1",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100003491.1.25",
          "form": "아니 절대",
          "original_form": "아니 절대",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100003491.1.26",
          "form": "안해",
          "original_form": "안해",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100003491.1.27",
          "form": "ㅋㅋ 그래",
