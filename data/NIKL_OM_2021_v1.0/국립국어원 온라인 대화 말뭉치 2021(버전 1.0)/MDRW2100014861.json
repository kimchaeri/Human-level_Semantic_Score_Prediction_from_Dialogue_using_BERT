      "utterance": [
        {
          "id": "MDRW2100014861.1.1",
          "form": "엄마 게임좀그만해",
          "original_form": "엄마 게임좀그만해",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014861.1.2",
          "form": "나 게임 안하는데",
          "original_form": "나 게임 안하는데",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014861.1.3",
          "form": "거짓말하지마 그놈의 도끼던지기",
          "original_form": "거짓말하지마 그놈의 도끼던지기",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014861.1.4",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014861.1.5",
          "form": "그 게임 한 오년은 한듯",
          "original_form": "그 게임 한 오년은 한듯",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014861.1.6",
          "form": "중독성있어",
          "original_form": "중독성있어",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014861.1.7",
          "form": "그래서문제야 중독!",
          "original_form": "그래서문제야 중독!",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014861.1.8",
          "form": "그정도는 아니야",
          "original_form": "그정도는 아니야",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014861.1.9",
          "form": "드라마 보면서 계속하더만 하나만 집중해",
          "original_form": "드라마 보면서 계속하더만 하나만 집중해",
          "speaker_id": "2",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014861.1.10",
          "form": "손이 심심해",
          "original_form": "손이 심심해",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014861.1.11",
          "form": "게임많이하면 안좋은거 알잖아",
          "original_form": "게임많이하면 안좋은거 알잖아",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014861.1.12",
          "form": "알겠어 자중할게",
          "original_form": "알겠어 자중할게",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014861.1.13",
          "form": "응 게임은 좀만 하는걸로~",
          "original_form": "응 게임은 좀만 하는걸로~",
          "speaker_id": "2",
          "time": "20210825 21:20"
        }
