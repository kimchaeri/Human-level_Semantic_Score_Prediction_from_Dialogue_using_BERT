      "utterance": [
        {
          "id": "MDRW2100045978.1.1",
          "form": "저는 강릉을 가장 좋아해요!",
          "original_form": "저는 강릉을 가장 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045978.1.2",
          "form": "매년 다녀와요",
          "original_form": "매년 다녀와요",
          "speaker_id": "2",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045978.1.3",
          "form": "강릉바다 이쁘죠",
          "original_form": "강릉바다 이쁘죠",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045978.1.4",
          "form": "와 매년!!",
          "original_form": "와 매년!!",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045978.1.5",
          "form": "누구랑가세요 보통~?",
          "original_form": "누구랑가세요 보통~?",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045978.1.6",
          "form": "가족들이랑도 가고 친구들이랑도 가요!",
          "original_form": "가족들이랑도 가고 친구들이랑도 가요!",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.7",
          "form": "올해는 못갔네요ㅠㅠ..",
          "original_form": "올해는 못갔네요ㅠㅠ..",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.8",
          "form": "어디 가장 좋아하세요?",
          "original_form": "어디 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.9",
          "form": "저는 많이 안가봤는데",
          "original_form": "저는 많이 안가봤는데",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.10",
          "form": "거제가 좋았어요",
          "original_form": "거제가 좋았어요",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.12",
          "form": "오오 거제도",
          "original_form": "오오 거제도",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045978.1.13",
          "form": "거기 해산물도 무지 싱싱하구 맛있구 ㅎㅎ",
          "original_form": "거기 해산물도 무지 싱싱하구 맛있구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045978.1.14",
          "form": "거제도는 아직 안가봤는데",
          "original_form": "거제도는 아직 안가봤는데",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045978.1.15",
          "form": "가서 꼭 먹고올게요 해산물 ㅎㅎ",
          "original_form": "가서 꼭 먹고올게요 해산물 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045978.1.16",
          "form": "ㅎㅎ 통영도 좋았어요",
          "original_form": "ㅎㅎ 통영도 좋았어요",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045978.1.17",
          "form": "헉 저도 통영 다녀왔어요!!",
          "original_form": "헉 저도 통영 다녀왔어요!!",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045978.1.18",
          "form": "통영 너무 좋았던 여행지에요",
          "original_form": "통영 너무 좋았던 여행지에요",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100045978.1.19",
          "form": "ㅎㅎ뭐가 젤 조으셨어요?",
          "original_form": "ㅎㅎ뭐가 젤 조으셨어요?",
          "speaker_id": "1",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045978.1.20",
          "form": "강릉 맛집은 어디가 있을까요~~?",
          "original_form": "강릉 맛집은 어디가 있을까요~~?",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045978.1.21",
          "form": "통영은 사실 다른것보다",
          "original_form": "통영은 사실 다른것보다",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045978.1.22",
          "form": "제가 첨 갔던 혼여행지라서 좋았던 것 같아요",
          "original_form": "제가 첨 갔던 혼여행지라서 좋았던 것 같아요",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045978.1.23",
          "form": "전 강릉에 해미가 자주가요! 물회집",
          "original_form": "전 강릉에 해미가 자주가요! 물회집",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045978.1.24",
          "form": "아앗물회 못먹어서 아쉬워요",
          "original_form": "아앗물회 못먹어서 아쉬워요",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045978.1.25",
          "form": "ㅎㅎ 혼자 여행 자주가세요?",
          "original_form": "ㅎㅎ 혼자 여행 자주가세요?",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045978.1.26",
          "form": "아뇨 딱 한번 가봤어요 그때 ㅎㅎ",
          "original_form": "아뇨 딱 한번 가봤어요 그때 ㅎㅎ",
