      "utterance": [
        {
          "id": "MDRW2100015466.1.1",
          "form": "반려동물 키워? ㅋㅋ",
          "original_form": "반려동물 키워? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015466.1.2",
          "form": "아뇨 생각만요 name2님은 키우세요?",
          "original_form": "아뇨 생각만요 &name2&님은 키우세요?",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015466.1.3",
          "form": "아뇨 저도 생각만..ㅋㅋㅋ",
          "original_form": "아뇨 저도 생각만..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015466.1.4",
          "form": "ㅎㅎ 강아지파예요? 고양이파예요?",
          "original_form": "ㅎㅎ 강아지파예요? 고양이파예요?",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015466.1.5",
          "form": "전 50 50...넘어려워요",
          "original_form": "전 50 50...넘어려워요",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015466.1.6",
          "form": "강아지에 조금 기울기도해요",
          "original_form": "강아지에 조금 기울기도해요",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015466.1.7",
          "form": "산책가능해서!",
          "original_form": "산책가능해서!",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015466.1.8",
          "form": "아 저는 고양이파요 강아지도 귀엽긴해요ㅜㅜ",
          "original_form": "아 저는 고양이파요 강아지도 귀엽긴해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015466.1.9",
          "form": "무슨 종 좋아하세요?",
          "original_form": "무슨 종 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015466.1.10",
          "form": "꺄..... 강아지는 골든리트리버 ㅠㅠ 꿈이지만 ...  고양이는 샴이 매력잇구용",
          "original_form": "꺄..... 강아지는 골든리트리버 ㅠㅠ 꿈이지만 ...  고양이는 샴이 매력잇구용",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015466.1.11",
          "form": "매력있죠ㅜㅜㅜ",
          "original_form": "매력있죠ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015466.1.12",
          "form": "전 스코티시폴드 기르고싶어요",
          "original_form": "전 스코티시폴드 기르고싶어요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015466.1.13",
          "form": "개는 큰개가 멋진거 같긴해요",
          "original_form": "개는 큰개가 멋진거 같긴해요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015466.1.14",
          "form": "스코티시폴드 넘 사랑스러운거같아요ㅠㅠ ㅋㅋ",
          "original_form": "스코티시폴드 넘 사랑스러운거같아요ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015466.1.15",
          "form": "진짜..언제쯤 들일수있을지 ㅠㅋㅋㅋ",
          "original_form": "진짜..언제쯤 들일수있을지 ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015466.1.16",
          "form": "맨날 생각만해요ㅜㅜ",
          "original_form": "맨날 생각만해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015466.1.17",
          "form": "책임이 뒤따르다보니 확실히 ㅎㅎ",
          "original_form": "책임이 뒤따르다보니 확실히 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015466.1.18",
          "form": "그죠 ㅠㅠ 혼자 살기도 하고 불안전해서",
          "original_form": "그죠 ㅠㅠ 혼자 살기도 하고 불안전해서",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015466.1.19",
          "form": "불안정해서 쉽지않네요",
          "original_form": "불안정해서 쉽지않네요",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015466.1.20",
          "form": "맞아요 돈도 많이 들어가고 ㅜㅜㅡ",
          "original_form": "맞아요 돈도 많이 들어가고 ㅜㅜㅡ",
