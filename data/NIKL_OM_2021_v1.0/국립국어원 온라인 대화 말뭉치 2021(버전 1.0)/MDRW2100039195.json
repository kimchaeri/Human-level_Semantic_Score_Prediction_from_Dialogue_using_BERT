      "utterance": [
        {
          "id": "MDRW2100039195.1.1",
          "form": "와 이번 이나즈마",
          "original_form": "와 이번 이나즈마",
          "speaker_id": "1",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039195.1.2",
          "form": "스토리 구리다고 욕 엄청먹네",
          "original_form": "스토리 구리다고 욕 엄청먹네",
          "speaker_id": "1",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039195.1.3",
          "form": "근데 그럴만 해(?",
          "original_form": "근데 그럴만 해(?",
          "speaker_id": "1",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039195.1.4",
          "form": "ㅇㅇ그니까",
          "original_form": "ㅇㅇ그니까",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039195.1.5",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039195.1.6",
          "form": "그렇게나 구려?",
          "original_form": "그렇게나 구려?",
          "speaker_id": "2",
          "time": "20210903 13:39"
        },
        {
          "id": "MDRW2100039195.1.7",
          "form": "욕먹을만 해 ㅇㅇ",
          "original_form": "욕먹을만 해 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100039195.1.8",
          "form": "멘스가 진짜",
          "original_form": "멘스가 진짜",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100039195.1.9",
          "form": "대체 뭔짓을 해놨길래......",
          "original_form": "대체 뭔짓을 해놨길래......",
          "speaker_id": "2",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100039195.1.10",
          "form": "뭐가 없어",
          "original_form": "뭐가 없어",
          "speaker_id": "1",
          "time": "20210903 13:42"
        },
        {
          "id": "MDRW2100039195.1.11",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100039195.1.12",
          "form": "라이덴 왜 갑자기 동료가되는지 그런거 업서?",
          "original_form": "라이덴 왜 갑자기 동료가되는지 그런거 업서?",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100039195.1.13",
          "form": "초기 삼봉행 얘기 없어?",
          "original_form": "초기 삼봉행 얘기 없어?",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100039195.1.14",
          "form": "나 아직",
          "original_form": "나 아직",
          "speaker_id": "1",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100039195.1.15",
          "form": "엔딩은 못봤는데",
          "original_form": "엔딩은 못봤는데",
          "speaker_id": "1",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100039195.1.16",
          "form": "초기삼봉행은 둘째치고",
          "original_form": "초기삼봉행은 둘째치고",
          "speaker_id": "1",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100039195.1.17",
          "form": "코코미 분량부터가..ㅋㅋㅋㅋ",
          "original_form": "코코미 분량부터가..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100039195.1.18",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100039195.1.19",
          "form": "걔 분량이 적을수가 있나?",
          "original_form": "걔 분량이 적을수가 있나?",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100039195.1.20",
          "form": "놀랍게도...",
          "original_form": "놀랍게도...",
          "speaker_id": "1",
          "time": "20210903 13:45"
        },
        {
          "id": "MDRW2100039195.1.21",
          "form": "진짜 이번 겜 스토리 누가 짰냐 ㅎ",
          "original_form": "진짜 이번 겜 스토리 누가 짰냐 ㅎ",
          "speaker_id": "1",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100039195.1.22",
          "form": "진짜 여기도",
          "original_form": "진짜 여기도",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100039195.1.23",
          "form": "스작 나간건",
          "original_form": "스작 나간건",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100039195.1.24",
          "form": "아니겠지.......",
          "original_form": "아니겠지.......",
          "speaker_id": "2",
