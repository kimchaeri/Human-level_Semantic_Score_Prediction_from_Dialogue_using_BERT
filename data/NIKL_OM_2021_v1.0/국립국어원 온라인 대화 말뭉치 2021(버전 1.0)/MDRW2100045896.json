      "utterance": [
        {
          "id": "MDRW2100045896.1.1",
          "form": "나 식빵에 잼발라섴ㅋㅋㅋ",
          "original_form": "나 식빵에 잼발라섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045896.1.2",
          "form": "ㅋㅋㅋㅋ 기본적인 아침이군",
          "original_form": "ㅋㅋㅋㅋ 기본적인 아침이군",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045896.1.3",
          "form": "지난번에 말한 치즈랑 먹을까 하다가 일단 잼만 발랐당",
          "original_form": "지난번에 말한 치즈랑 먹을까 하다가 일단 잼만 발랐당",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045896.1.4",
          "form": "ㅋㅋㅋㅋ 언니는 아침 안먹고 나갔어?",
          "original_form": "ㅋㅋㅋㅋ 언니는 아침 안먹고 나갔어?",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045896.1.5",
          "form": "ㅇㅇ 그 치즈는 함부로 먹을게 아닌듯...",
          "original_form": "ㅇㅇ 그 치즈는 함부로 먹을게 아닌듯...",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045896.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045896.1.7",
          "form": "약간 최종보스 느낌으로다가..",
          "original_form": "약간 최종보스 느낌으로다가..",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045896.1.8",
          "form": "ㅇㅇ ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045896.1.9",
          "form": "난 원래 아침 잘 안먹어",
          "original_form": "난 원래 아침 잘 안먹어",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045896.1.10",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045896.1.11",
          "form": "나도 학교 다닐때나 먹었지",
          "original_form": "나도 학교 다닐때나 먹었지",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045896.1.12",
          "form": "대학다니고는 안먹었는데",
          "original_form": "대학다니고는 안먹었는데",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045896.1.13",
          "form": "빈속으로 버스타면 죽겠더라고 이제는...",
          "original_form": "빈속으로 버스타면 죽겠더라고 이제는...",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045896.1.14",
          "form": "ㅇㅎ 버스 이용하면 좀 그런게 잇지...",
          "original_form": "ㅇㅎ 버스 이용하면 좀 그런게 잇지...",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045896.1.15",
          "form": "ㅇㅇ....",
          "original_form": "ㅇㅇ....",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045896.1.16",
          "form": "그래서 간단하게라도 먹고 나가",
          "original_form": "그래서 간단하게라도 먹고 나가",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045896.1.17",
          "form": "초콜릿 하나라도 욱여넣으면 좀 낫더라고",
          "original_form": "초콜릿 하나라도 욱여넣으면 좀 낫더라고",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045896.1.18",
          "form": "난 보통 회사가서",
          "original_form": "난 보통 회사가서",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045896.1.19",
          "form": "우유마심...",
          "original_form": "우유마심...",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045896.1.20",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045896.1.21",
          "form": "우유..유당불내증 약간 있어서 힘들당..",
          "original_form": "우유..유당불내증 약간 있어서 힘들당..",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045896.1.22",
          "form": "그래서 두유를(?",
          "original_form": "그래서 두유를(?",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045896.1.23",
          "form": "아닠ㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045896.1.24",
          "form": "난 두유 콩냄새 땜에 잘 못마심",
          "original_form": "난 두유 콩냄새 땜에 잘 못마심",
          "speaker_id": "1",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045896.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045896.1.26",
          "form": "맞아 잘못 먹으면 비린내 확 나더라",
          "original_form": "맞아 잘못 먹으면 비린내 확 나더라",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045896.1.27",
          "form": "나 그래서",
          "original_form": "나 그래서",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045896.1.28",
          "form": "소화가 잘되는 우유? 먹어",
          "original_form": "소화가 잘되는 우유? 먹어",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045896.1.29",
          "form": "유당 제거한거!",
          "original_form": "유당 제거한거!",
          "speaker_id": "1",
          "time": "20210908 08:36"
        }
      ]
    }
