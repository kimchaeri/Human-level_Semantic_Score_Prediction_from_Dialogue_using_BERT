      "utterance": [
        {
          "id": "MDRW2100021930.1.1",
          "form": "음방 같은거 자주보세요?",
          "original_form": "음방 같은거 자주보세요?",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021930.1.2",
          "form": "네 저 즐겨봐요 !",
          "original_form": "네 저 즐겨봐요 !",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021930.1.3",
          "form": "근데 요즘 신인들은 또 잘 모르지만 ㅋㅋㅋ",
          "original_form": "근데 요즘 신인들은 또 잘 모르지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021930.1.4",
          "form": "저는 음방읔 정신 없어서 못보겠어요.. \r\n화면을 너무 흔들어싸서..",
          "original_form": "저는 음방읔 정신 없어서 못보겠어요.. \r\n화면을 너무 흔들어싸서..",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021930.1.5",
          "form": "아 ㅋㅋㅋㅋ 맞아요",
          "original_form": "아 ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021930.1.6",
          "form": "요즘 진짜 심해요 ㅋㄱㅋ",
          "original_form": "요즘 진짜 심해요 ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021930.1.7",
          "form": "그래서 저도 직캠을 더 좋아해요 ㅎㅎ",
          "original_form": "그래서 저도 직캠을 더 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021930.1.8",
          "form": "외국 방송은 카메라 심심하긴한데.. 그게 더 나은듯.. \r\n직캠 요즘 화질 엄청나던데요..",
          "original_form": "외국 방송은 카메라 심심하긴한데.. 그게 더 나은듯.. \r\n직캠 요즘 화질 엄청나던데요..",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021930.1.9",
          "form": "아예 방송국에서 올려주니까...",
          "original_form": "아예 방송국에서 올려주니까...",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021930.1.10",
          "form": "맞아요 ㅋㅋ 요즘은 오히려 직캠을 많이 보니까요",
          "original_form": "맞아요 ㅋㅋ 요즘은 오히려 직캠을 많이 보니까요",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021930.1.11",
          "form": "팬들끼리 싸우더라구요 ㅋㅋ",
          "original_form": "팬들끼리 싸우더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021930.1.12",
          "form": "조회수로 ..",
          "original_form": "조회수로 ..",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021930.1.13",
          "form": "아.. 그런거로도 싸워요? 악개들?",
          "original_form": "아.. 그런거로도 싸워요? 악개들?",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021930.1.14",
          "form": "네네 악개들 ㅋㅋㅋ 갠팬들 ㅋㅋ",
          "original_form": "네네 악개들 ㅋㅋㅋ 갠팬들 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021930.1.15",
          "form": "서로 자기 최애가 더 인기 많다고 ㅋㅋ",
          "original_form": "서로 자기 최애가 더 인기 많다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021930.1.16",
          "form": "역시 돌판은.. 악개 땜에 항상 문제네여..",
          "original_form": "역시 돌판은.. 악개 땜에 항상 문제네여..",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021930.1.17",
          "form": "악개 때문에 막 안좋은 소문도 나잖아요 ㅋㅋ",
          "original_form": "악개 때문에 막 안좋은 소문도 나잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021930.1.18",
          "form": "헛소문같은거 ,,",
          "original_form": "헛소문같은거 ,,",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021930.1.19",
          "form": "요즘도 음방투표 조작 되나여?",
          "original_form": "요즘도 음방투표 조작 되나여?",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021930.1.20",
          "form": "옛날엔 가짜 아이디로 엄청 했다고 하던데..",
          "original_form": "옛날엔 가짜 아이디로 엄청 했다고 하던데..",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021930.1.21",
          "form": "아 요즘은 예전보다는 안심해요 ㅋㅋ",
          "original_form": "아 요즘은 예전보다는 안심해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021930.1.22",
          "form": "요즘은 투표보다 음원 위주로 보니까",
          "original_form": "요즘은 투표보다 음원 위주로 보니까",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021930.1.23",
          "form": "예전보다 투표 비중이 줄었어요 !",
          "original_form": "예전보다 투표 비중이 줄었어요 !",
          "speaker_id": "2",
          "time": "20210829 00:21"
        }
      ]
    }
