      "utterance": [
        {
          "id": "MDRW2100039588.1.1",
          "form": "안녕하세요 저녁드셨나요?",
          "original_form": "안녕하세요 저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039588.1.2",
          "form": "아뇨 지금 먹어요",
          "original_form": "아뇨 지금 먹어요",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039588.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039588.1.4",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039588.1.5",
          "form": "어떤거드세요?",
          "original_form": "어떤거드세요?",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039588.1.6",
          "form": "순대국이요ㅎㅎ",
          "original_form": "순대국이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100039588.1.7",
          "form": "헉 너무맛있죠",
          "original_form": "헉 너무맛있죠",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039588.1.8",
          "form": "저는 아바이순대 좋아해요",
          "original_form": "저는 아바이순대 좋아해요",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039588.1.9",
          "form": "ㅎㅎ안먹어봤어요ㅜ",
          "original_form": "ㅎㅎ안먹어봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039588.1.10",
          "form": "기회가없네요ㅋㅋㅋ",
          "original_form": "기회가없네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039588.1.11",
          "form": "순대국 좋아하세요?!",
          "original_form": "순대국 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039588.1.12",
          "form": "즐겨드세요?!",
          "original_form": "즐겨드세요?!",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039588.1.13",
          "form": "그냥 냉동실에 있길래요ㅋㅋㅋㄱ",
          "original_form": "그냥 냉동실에 있길래요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039588.1.14",
          "form": "점심시간에 먹기도하조ㅡㅋ",
          "original_form": "점심시간에 먹기도하조ㅡㅋ",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039588.1.15",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039588.1.16",
          "form": "배달음식은 좋아하세요?!",
          "original_form": "배달음식은 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039588.1.17",
          "form": "좋죠 치킨 곱창 닭발 쭈꾸미ㅋㅋㅋ",
          "original_form": "좋죠 치킨 곱창 닭발 쭈꾸미ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039588.1.18",
          "form": "아 저도 다좋아하는 음식입니다",
          "original_form": "아 저도 다좋아하는 음식입니다",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039588.1.19",
          "form": "매운것도 잘드세요?",
          "original_form": "매운것도 잘드세요?",
