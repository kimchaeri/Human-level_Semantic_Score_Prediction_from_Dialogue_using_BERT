        {
          "id": "MDRW2100005425.1.1",
          "form": "영화?",
          "original_form": "영화?",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005425.1.2",
          "form": "플립하고 라라랜드",
          "original_form": "플립하고 라라랜드",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005425.1.3",
          "form": "느낌 알겠다",
          "original_form": "느낌 알겠다",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005425.1.4",
          "form": "그치?",
          "original_form": "그치?",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005425.1.5",
          "form": "영상미가 너무 좋아",
          "original_form": "영상미가 너무 좋아",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005425.1.6",
          "form": "맞아 영상이 예쁘더라",
          "original_form": "맞아 영상이 예쁘더라",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005425.1.7",
          "form": "넌 어떤 장르 영화 좋아해?",
          "original_form": "넌 어떤 장르 영화 좋아해?",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005425.1.8",
          "form": "장르를 딱히 가리지는 않고 골고루 잘 보는 것 같아",
          "original_form": "장르를 딱히 가리지는 않고 골고루 잘 보는 것 같아",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005425.1.9",
          "form": "좋아하는 영화 있어?",
          "original_form": "좋아하는 영화 있어?",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005425.1.10",
          "form": "주성치 영화 좋아해 ㅋㅋ",
          "original_form": "주성치 영화 좋아해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005425.1.11",
          "form": "오래됐긴 한데 재밌어",
          "original_form": "오래됐긴 한데 재밌어",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005425.1.12",
          "form": "아 옛날 영화",
          "original_form": "아 옛날 영화",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005425.1.13",
          "form": "나 최근에 오드리햅번 나오는 영화 봤어",
          "original_form": "나 최근에 오드리햅번 나오는 영화 봤어",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005425.1.14",
          "form": "뭐 봤어?",
          "original_form": "뭐 봤어?",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005425.1.15",
          "form": "티파니에서 아침을",
          "original_form": "티파니에서 아침을",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005425.1.16",
          "form": "엄청 오래된 영화지?",
          "original_form": "엄청 오래된 영화지?",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005425.1.17",
          "form": "응 그렇긴 한데 오드리햅번 너무 사랑스럽더라",
          "original_form": "응 그렇긴 한데 오드리햅번 너무 사랑스럽더라",
          "speaker_id": "2",
