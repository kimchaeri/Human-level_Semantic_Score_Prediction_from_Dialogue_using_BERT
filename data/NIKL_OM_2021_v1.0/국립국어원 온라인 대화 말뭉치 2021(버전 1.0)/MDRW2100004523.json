      "utterance": [
        {
          "id": "MDRW2100004523.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004523.1.2",
          "form": "아니요~ 없어요!",
          "original_form": "아니요~ 없어요!",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004523.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004523.1.4",
          "form": "저는 없고 부모님은 불겨요",
          "original_form": "저는 없고 부모님은 불겨요",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004523.1.5",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004523.1.6",
          "form": "불교도 좋죠",
          "original_form": "불교도 좋죠",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004523.1.7",
          "form": "부모님 따라서 절에 가보셨어요?",
          "original_form": "부모님 따라서 절에 가보셨어요?",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004523.1.8",
          "form": "아니요 친구따라 교회요",
          "original_form": "아니요 친구따라 교회요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004523.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004523.1.10",
          "form": "친구따라 교회 가보셨군요",
          "original_form": "친구따라 교회 가보셨군요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004523.1.11",
          "form": "절은 가봤는데 교회는 한번도 가본 적 없어서",
          "original_form": "절은 가봤는데 교회는 한번도 가본 적 없어서",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004523.1.12",
          "form": "조금 궁금하네요",
          "original_form": "조금 궁금하네요",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004523.1.13",
          "form": "어렸을 때라 재밌어요",
          "original_form": "어렸을 때라 재밌어요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004523.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004523.1.15",
          "form": "지금은 안가요",
          "original_form": "지금은 안가요",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004523.1.16",
          "form": "이미지가ㅜㅜ",
          "original_form": "이미지가ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004523.1.17",
          "form": "이미지가 많이 안 좋아지긴 했죠ㅠㅠ",
          "original_form": "이미지가 많이 안 좋아지긴 했죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004523.1.18",
          "form": "종교를 다 존중하지만..",
          "original_form": "종교를 다 존중하지만..",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004523.1.19",
          "form": "네 지침은 따라줬으몀",
          "original_form": "네 지침은 따라줬으몀",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004523.1.20",
          "form": "교회에서 '오세요'라고 강요하는 것도",
          "original_form": "교회에서 '오세요'라고 강요하는 것도",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004523.1.21",
          "form": "별로인거 같아요..",
          "original_form": "별로인거 같아요..",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004523.1.22",
          "form": "계속 붙잡고 안 놓아주시는...ㅠㅠ",
          "original_form": "계속 붙잡고 안 놓아주시는...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:09"
        }
      ]
