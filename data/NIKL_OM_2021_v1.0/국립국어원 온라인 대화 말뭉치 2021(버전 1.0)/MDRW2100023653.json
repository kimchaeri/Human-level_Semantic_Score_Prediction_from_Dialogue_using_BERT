      "utterance": [
        {
          "id": "MDRW2100023653.1.1",
          "form": "너는 뭐먹는다고?",
          "original_form": "너는 뭐먹는다고?",
          "speaker_id": "2",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023653.1.2",
          "form": "마자 매운걸로 느끼한거 중화시켜줘야지",
          "original_form": "마자 매운걸로 느끼한거 중화시켜줘야지",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023653.1.3",
          "form": "나는 요새 너무 탄수화물 많이 먹은거같아서",
          "original_form": "나는 요새 너무 탄수화물 많이 먹은거같아서",
          "speaker_id": "1",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023653.1.4",
          "form": "야채?",
          "original_form": "야채?",
          "speaker_id": "2",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023653.1.5",
          "form": "응 야채 좀 먹게",
          "original_form": "응 야채 좀 먹게",
          "speaker_id": "1",
          "time": "20210830 13:55"
        },
        {
          "id": "MDRW2100023653.1.6",
          "form": "샐러드? 과일?",
          "original_form": "샐러드? 과일?",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023653.1.7",
          "form": "그냥 파프리카랑 토마토 같은거 사서 먹으려고",
          "original_form": "그냥 파프리카랑 토마토 같은거 사서 먹으려고",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023653.1.8",
          "form": "요리해먹기도 귀찮다",
          "original_form": "요리해먹기도 귀찮다",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023653.1.9",
          "form": "ㅋㅋㅋ 두부마요네즈 먹어봤어?",
          "original_form": "ㅋㅋㅋ 두부마요네즈 먹어봤어?",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023653.1.10",
          "form": "그거에 파프리카 찍어먹음 맛있는뎅",
          "original_form": "그거에 파프리카 찍어먹음 맛있는뎅",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023653.1.11",
          "form": "오 그래?  \n레시피가 뭔데?",
          "original_form": "오 그래?  \n레시피가 뭔데?",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023653.1.12",
          "form": "두부 소금 올리고당 식초 이정도만 있음 돼!",
          "original_form": "두부 소금 올리고당 식초 이정도만 있음 돼!",
          "speaker_id": "2",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100023653.1.13",
          "form": "오 별거없네",
          "original_form": "오 별거없네",
          "speaker_id": "1",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100023653.1.14",
          "form": "기름은 안들어가?",
          "original_form": "기름은 안들어가?",
          "speaker_id": "1",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100023653.1.15",
          "form": "아까 검색해보니까 넣는것도 있긴한데 난 안넣었어",
          "original_form": "아까 검색해보니까 넣는것도 있긴한데 난 안넣었어",
          "speaker_id": "2",
          "time": "20210830 14:01"
        },
        {
          "id": "MDRW2100023653.1.16",
          "form": "마요네즈랑 큰차이없어?",
          "original_form": "마요네즈랑 큰차이없어?",
          "speaker_id": "1",
          "time": "20210830 14:02"
        },
        {
          "id": "MDRW2100023653.1.17",
          "form": "두부 고소한 맛 나는? 마요네즈라고 하지만 마요네즈 느낌보다는 두부스프레드같은 느낌이야",
          "original_form": "두부 고소한 맛 나는? 마요네즈라고 하지만 마요네즈 느낌보다는 두부스프레드같은 느낌이야",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100023653.1.18",
          "form": "채소스틱 찍어먹기 좋더라고",
          "original_form": "채소스틱 찍어먹기 좋더라고",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100023653.1.19",
          "form": "믹서기있어 근데?",
          "original_form": "믹서기있어 근데?",
          "speaker_id": "2",
          "time": "20210830 14:03"
        }
      ]
