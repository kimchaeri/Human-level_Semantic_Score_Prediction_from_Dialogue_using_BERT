      "utterance": [
        {
          "id": "MDRW2100034044.1.1",
          "form": "요새 어떤음악들으시나요?",
          "original_form": "요새 어떤음악들으시나요?",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034044.1.2",
          "form": "저 오늘은",
          "original_form": "저 오늘은",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034044.1.3",
          "form": "태연노래 들었어요 ㅎ",
          "original_form": "태연노래 들었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100034044.1.4",
          "form": "오! 태연목소리 저도 참 좋아해요!",
          "original_form": "오! 태연목소리 저도 참 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100034044.1.5",
          "form": "어떤 노래",
          "original_form": "어떤 노래",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100034044.1.6",
          "form": "좋아하세옹",
          "original_form": "좋아하세옹",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100034044.1.7",
          "form": "태연노래요? 저는 태연노래는 수록곡까지도 다 좋아해요 ㅋㅋ 11:11만 빼고요 ㅋㅋ",
          "original_form": "태연노래요? 저는 태연노래는 수록곡까지도 다 좋아해요 ㅋㅋ 11:11만 빼고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100034044.1.8",
          "form": "아 저도요 ㅎ",
          "original_form": "아 저도요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034044.1.9",
          "form": "노래 다 좋아해요 !",
          "original_form": "노래 다 좋아해요 !",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034044.1.10",
          "form": "요새는 중국노래도 듣는데 좋은거많더라구요",
          "original_form": "요새는 중국노래도 듣는데 좋은거많더라구요",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034044.1.11",
          "form": "어떤 거 들으세요?",
          "original_form": "어떤 거 들으세요?",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034044.1.12",
          "form": "이유는 없어 라는 곡인데 유투브 검색해보셔요! 너무좋더라구요~~",
          "original_form": "이유는 없어 라는 곡인데 유투브 검색해보셔요! 너무좋더라구요~~",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034044.1.13",
          "form": "오 중국노래에요?",
          "original_form": "오 중국노래에요?",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034044.1.14",
          "form": "한번 들어볼게요 ㅎ",
          "original_form": "한번 들어볼게요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034044.1.15",
          "form": "네! 아마 예상과 다른 느낌이라 깜짝놀라실지도몰라요~",
          "original_form": "네! 아마 예상과 다른 느낌이라 깜짝놀라실지도몰라요~",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034044.1.16",
          "form": "아ㅋㅋㅋㅋㅋㄴ 궁금하네요",
          "original_form": "아ㅋㅋㅋㅋㅋㄴ 궁금하네요",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100034044.1.17",
          "form": "ㅋㅋㅋㅋ 시간되시면 검색해보셔요~",
          "original_form": "ㅋㅋㅋㅋ 시간되시면 검색해보셔요~",
