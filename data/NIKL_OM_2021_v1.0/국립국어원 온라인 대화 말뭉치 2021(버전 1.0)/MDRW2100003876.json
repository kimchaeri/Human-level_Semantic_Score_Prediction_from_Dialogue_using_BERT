        {
          "id": "MDRW2100003876.1.1",
          "form": "점심 먹고 올게",
          "original_form": "점심 먹고 올게",
          "speaker_id": "1",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003876.1.2",
          "form": "응 나도 먹어야겠다",
          "original_form": "응 나도 먹어야겠다",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003876.1.3",
          "form": "다 먹었다 ㅋㅋ",
          "original_form": "다 먹었다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003876.1.4",
          "form": "뭐 먹었어",
          "original_form": "뭐 먹었어",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003876.1.5",
          "form": "김치부침개 부쳐 먹었어",
          "original_form": "김치부침개 부쳐 먹었어",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003876.1.6",
          "form": "나는 그냥 밥에다 반찬",
          "original_form": "나는 그냥 밥에다 반찬",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003876.1.7",
          "form": "먹었어",
          "original_form": "먹었어",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003876.1.8",
          "form": "맛있게 먹었어?",
          "original_form": "맛있게 먹었어?",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003876.1.9",
          "form": "응 그냥저냥 맛있게 먹었어",
          "original_form": "응 그냥저냥 맛있게 먹었어",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003876.1.10",
          "form": "무슨 반찬인데",
          "original_form": "무슨 반찬인데",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003876.1.11",
          "form": "가지나물이랑 콩나물이랑 멸치랑",
          "original_form": "가지나물이랑 콩나물이랑 멸치랑",
          "speaker_id": "2",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003876.1.12",
          "form": "계란후라이",
          "original_form": "계란후라이",
          "speaker_id": "2",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003876.1.13",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003876.1.14",
          "form": "응 김치전 맛있게 먹었어?",
          "original_form": "응 김치전 맛있게 먹었어?",
          "speaker_id": "2",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003876.1.15",
          "form": "응 튀김가루로 만들었더니 바삭해서 좋았어",
          "original_form": "응 튀김가루로 만들었더니 바삭해서 좋았어",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003876.1.16",
          "form": "튀김가루로 부침개 하면 바삭하구나",
          "original_form": "튀김가루로 부침개 하면 바삭하구나",
