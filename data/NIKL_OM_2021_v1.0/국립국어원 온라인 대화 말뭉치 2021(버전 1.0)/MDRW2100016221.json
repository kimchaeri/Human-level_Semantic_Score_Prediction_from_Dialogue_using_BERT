      "utterance": [
        {
          "id": "MDRW2100016221.1.1",
          "form": "저 최근에 옷 많이 샀는데",
          "original_form": "저 최근에 옷 많이 샀는데",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016221.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016221.1.3",
          "form": "오늘 날씨 좋죠?",
          "original_form": "오늘 날씨 좋죠?",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016221.1.4",
          "form": "왜 비가 계속 오는거죠 ?",
          "original_form": "왜 비가 계속 오는거죠 ?",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100016221.1.5",
          "form": "비오는 날 좋아하세요 ?",
          "original_form": "비오는 날 좋아하세요 ?",
          "speaker_id": "1",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100016221.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100016221.1.7",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100016221.1.8",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100016221.1.9",
          "form": "사실 저는 비오는 걸 좋아하는게 아니라",
          "original_form": "사실 저는 비오는 걸 좋아하는게 아니라",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100016221.1.10",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100016221.1.11",
          "form": "비오는날의 감성을 좋아해요",
          "original_form": "비오는날의 감성을 좋아해요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100016221.1.12",
          "form": "비오는날 산책나가는게 그렇게 좋더라고요",
          "original_form": "비오는날 산책나가는게 그렇게 좋더라고요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100016221.1.13",
          "form": "이따 저녁에 나갔다와야겠어요",
          "original_form": "이따 저녁에 나갔다와야겠어요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100016221.1.14",
          "form": "님이요?",
          "original_form": "님이요?",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100016221.1.15",
          "form": "님 귀찮아서 잘 안 나가시잖아요",
          "original_form": "님 귀찮아서 잘 안 나가시잖아요",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100016221.1.16",
          "form": "절 아세요 ?",
          "original_form": "절 아세요 ?",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100016221.1.17",
          "form": "소름돋네",
          "original_form": "소름돋네",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100016221.1.18",
          "form": "사생이세요?",
          "original_form": "사생이세요?",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100016221.1.19",
          "form": "그럼 이따",
          "original_form": "그럼 이따",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100016221.1.20",
          "form": "저녁에 전 드시러 나가실 건가요?",
          "original_form": "저녁에 전 드시러 나가실 건가요?",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100016221.1.21",
          "form": "갈까요 말까요",
          "original_form": "갈까요 말까요",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100016221.1.22",
          "form": "비오는 날 나가는거 진짜 근데 좋아해요 저",
          "original_form": "비오는 날 나가는거 진짜 근데 좋아해요 저",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.23",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.24",
          "form": "우산 쓰고 걸어가야해요",
          "original_form": "우산 쓰고 걸어가야해요",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.25",
          "form": "비오는 날은",
          "original_form": "비오는 날은",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.26",
          "form": "버스죠;;",
          "original_form": "버스죠;;",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.27",
          "form": "누가 우산쓰고 30분 걸어요",
          "original_form": "누가 우산쓰고 30분 걸어요",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.28",
          "form": "비오는 날 누가 버스를 타요",
          "original_form": "비오는 날 누가 버스를 타요",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100016221.1.29",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100016221.1.30",
          "form": "뭐하시나요?",
          "original_form": "뭐하시나요?",
          "speaker_id": "2",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100016221.1.31",
          "form": "아까 사진 찍은거 보고 있었어요",
          "original_form": "아까 사진 찍은거 보고 있었어요",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100016221.1.32",
          "form": "비오면 뭔가 사진도 습하게 나와서 좋아요",
          "original_form": "비오면 뭔가 사진도 습하게 나와서 좋아요",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100016221.1.33",
          "form": "뭔느낌인지 아세요 ?",
          "original_form": "뭔느낌인지 아세요 ?",
          "speaker_id": "1",
          "time": "20210831 22:45"
        }
      ]
    }
  ]
