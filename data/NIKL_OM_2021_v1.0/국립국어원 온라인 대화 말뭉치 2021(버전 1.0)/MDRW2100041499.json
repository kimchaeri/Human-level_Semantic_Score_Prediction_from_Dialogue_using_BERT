      "utterance": [
        {
          "id": "MDRW2100041499.1.1",
          "form": "그러게요 추워요",
          "original_form": "그러게요 추워요",
          "speaker_id": "2",
          "time": "20210906 07:45"
        },
        {
          "id": "MDRW2100041499.1.2",
          "form": "긴팔입으실 건가요?",
          "original_form": "긴팔입으실 건가요?",
          "speaker_id": "2",
          "time": "20210906 07:45"
        },
        {
          "id": "MDRW2100041499.1.3",
          "form": "속에 반팔입고 셔츠 걸쳤어요!!",
          "original_form": "속에 반팔입고 셔츠 걸쳤어요!!",
          "speaker_id": "1",
          "time": "20210906 07:46"
        },
        {
          "id": "MDRW2100041499.1.4",
          "form": "잘하셨네요!! 저는 오늘 회사나가려다가 그냥 재택할라고요 ㅠㅠ 아직 잠옷차림이네여",
          "original_form": "잘하셨네요!! 저는 오늘 회사나가려다가 그냥 재택할라고요 ㅠㅠ 아직 잠옷차림이네여",
          "speaker_id": "2",
          "time": "20210906 07:48"
        },
        {
          "id": "MDRW2100041499.1.5",
          "form": "긴팔 긴바지인데 안더워요",
          "original_form": "긴팔 긴바지인데 안더워요",
          "speaker_id": "2",
          "time": "20210906 07:50"
        },
        {
          "id": "MDRW2100041499.1.6",
          "form": "ㅋㅋㅋ잘하셨어요 재택이좋죠",
          "original_form": "ㅋㅋㅋ잘하셨어요 재택이좋죠",
          "speaker_id": "1",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041499.1.7",
          "form": "저 창문열고잤더니 목아파요 ㅋㅋㅋ",
          "original_form": "저 창문열고잤더니 목아파요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041499.1.8",
          "form": "감기 조심해야해요!!!",
          "original_form": "감기 조심해야해요!!!",
          "speaker_id": "2",
          "time": "20210906 08:57"
        },
        {
          "id": "MDRW2100041499.1.9",
          "form": "고마워요 ㅋㅋㅋ",
          "original_form": "고마워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 09:10"
        },
        {
          "id": "MDRW2100041499.1.10",
          "form": "근데 오늘 저녁부터 또 비오나봐요",
          "original_form": "근데 오늘 저녁부터 또 비오나봐요",
          "speaker_id": "1",
          "time": "20210906 09:10"
        },
        {
          "id": "MDRW2100041499.1.11",
          "form": "헐 진짜요??",
          "original_form": "헐 진짜요??",
          "speaker_id": "2",
          "time": "20210906 09:10"
        },
        {
          "id": "MDRW2100041499.1.12",
          "form": "설마... 내일 출근하는데...비오려나요..",
          "original_form": "설마... 내일 출근하는데...비오려나요..",
          "speaker_id": "2",
          "time": "20210906 09:11"
        },
        {
          "id": "MDRW2100041499.1.13",
          "form": "아ㅜ왜 하필이면 ㅠㅠㅠㅠㅠ",
          "original_form": "아ㅜ왜 하필이면 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 09:11"
        },
        {
          "id": "MDRW2100041499.1.14",
          "form": "..........네......예보상으론...........ㅠㅠㅠㅠㅠ",
          "original_form": "..........네......예보상으론...........ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 09:12"
        },
        {
          "id": "MDRW2100041499.1.15",
          "form": "아ㅜ정말 ㅜㅜㅜ저번주에도 이랬는데..근데 월요일에 진짜 눈이 안떠져서ㅜㅜ 내일 비맞겠네요 또..",
          "original_form": "아ㅜ정말 ㅜㅜㅜ저번주에도 이랬는데..근데 월요일에 진짜 눈이 안떠져서ㅜㅜ 내일 비맞겠네요 또..",
          "speaker_id": "2",
          "time": "20210906 09:18"
        },
        {
          "id": "MDRW2100041499.1.16",
          "form": "ㅠㅠㅠㅠㅠ후 그래도 예보상으로는 저번처럼 많이오진않을것샅은데",
          "original_form": "ㅠㅠㅠㅠㅠ후 그래도 예보상으로는 저번처럼 많이오진않을것샅은데",
          "speaker_id": "1",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041499.1.17",
          "form": "벌써 싫네요 ㅠㅠㅠㅠㅠ",
          "original_form": "벌써 싫네요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041499.1.18",
          "form": "아 진짜요?? 추적추적오는 가을비인가... 호호",
          "original_form": "아 진짜요?? 추적추적오는 가을비인가... 호호",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041499.1.19",
          "form": "비오고나면 더 추워질것 같아요",
          "original_form": "비오고나면 더 추워질것 같아요",
          "speaker_id": "1",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100041499.1.20",
          "form": "ㅠㅡㅜ",
          "original_form": "ㅠㅡㅜ",
          "speaker_id": "1",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100041499.1.21",
          "form": "아 정말ㅜㅜ 가을옷정리한번 해야겠네요",
          "original_form": "아 정말ㅜㅜ 가을옷정리한번 해야겠네요",
