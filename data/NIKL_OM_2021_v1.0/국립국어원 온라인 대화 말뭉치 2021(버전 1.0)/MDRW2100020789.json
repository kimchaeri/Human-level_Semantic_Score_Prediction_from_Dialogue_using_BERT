      "utterance": [
        {
          "id": "MDRW2100020789.1.1",
          "form": "아 왜 그생각을 못했죠 저",
          "original_form": "아 왜 그생각을 못했죠 저",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020789.1.2",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020789.1.3",
          "form": "샌드위치를 좋아하세요 ?",
          "original_form": "샌드위치를 좋아하세요 ?",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020789.1.4",
          "form": "네 샌드위치 좋아해요 빵류 다 좋아해요ㅋㅋ",
          "original_form": "네 샌드위치 좋아해요 빵류 다 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020789.1.5",
          "form": "최애는 에그타르트네요",
          "original_form": "최애는 에그타르트네요",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020789.1.6",
          "form": "지금 요기요에서 파리바게트 5천원 할인 중이에요 ,, !",
          "original_form": "지금 요기요에서 파리바게트 5천원 할인 중이에요 ,, !",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020789.1.7",
          "form": "저도 어제 에그타르트 두개 사먹었어요",
          "original_form": "저도 어제 에그타르트 두개 사먹었어요",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020789.1.8",
          "form": "오!!!!",
          "original_form": "오!!!!",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020789.1.9",
          "form": "꿀정보 감사해요ㅋㅋ",
          "original_form": "꿀정보 감사해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020789.1.10",
          "form": "빠바 에그타르트 맛있죠",
          "original_form": "빠바 에그타르트 맛있죠",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020789.1.11",
          "form": "바닥이 파이지가 아니라 타르트지라 개인적으로 더 좋아요",
          "original_form": "바닥이 파이지가 아니라 타르트지라 개인적으로 더 좋아요",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020789.1.12",
          "form": "아 너무맛있었어요 부드럽고 ,,",
          "original_form": "아 너무맛있었어요 부드럽고 ,,",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020789.1.13",
          "form": "비린내도 안 나구요",
          "original_form": "비린내도 안 나구요",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020789.1.14",
          "form": "빠바 호두 타르트도 맛있고 은근 타르트 맛집이에요",
          "original_form": "빠바 호두 타르트도 맛있고 은근 타르트 맛집이에요",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020789.1.15",
          "form": "어 맛아요 호두도 맛있어요",
          "original_form": "어 맛아요 호두도 맛있어요",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020789.1.16",
          "form": "어쩔수없네요 요기요 들어가야되겠다 이건 ..",
          "original_form": "어쩔수없네요 요기요 들어가야되겠다 이건 ..",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020789.1.17",
          "form": "샌드위치랑 토스트 중에 뭐가 더 좋으세요 ?",
          "original_form": "샌드위치랑 토스트 중에 뭐가 더 좋으세요 ?",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020789.1.18",
          "form": "전 샌드위치 완승이요!ㅋㅋ",
          "original_form": "전 샌드위치 완승이요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020789.1.19",
          "form": "오 완승까지 ㅋㅋㅋㅋ",
          "original_form": "오 완승까지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020789.1.20",
          "form": "ㅋㅋㅋㅋ 아 그런데 생각해보니까",
          "original_form": "ㅋㅋㅋㅋ 아 그런데 생각해보니까",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020789.1.21",
          "form": "프렌치토스트는 또 좋아하네요ㅋㅋ",
          "original_form": "프렌치토스트는 또 좋아하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020789.1.22",
          "form": "아 역시맛있죠",
          "original_form": "아 역시맛있죠",
