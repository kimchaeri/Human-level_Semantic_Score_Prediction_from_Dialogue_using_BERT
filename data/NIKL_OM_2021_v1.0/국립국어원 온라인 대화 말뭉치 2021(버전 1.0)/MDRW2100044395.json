      "utterance": [
        {
          "id": "MDRW2100044395.1.1",
          "form": "회 뭐 좋아하세요?",
          "original_form": "회 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044395.1.2",
          "form": "세꼬시 좋아해요ㅎ",
          "original_form": "세꼬시 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044395.1.3",
          "form": "님은 드시나요",
          "original_form": "님은 드시나요",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044395.1.4",
          "form": "아 세꼬시 꼬들꼬들하죠",
          "original_form": "아 세꼬시 꼬들꼬들하죠",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044395.1.5",
          "form": "저 우럭광어ㅎㅎ",
          "original_form": "저 우럭광어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044395.1.6",
          "form": "깔끔한 맛 좋아합니다",
          "original_form": "깔끔한 맛 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044395.1.7",
          "form": "광어 세꼬시 드셔보셨어요?",
          "original_form": "광어 세꼬시 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044395.1.8",
          "form": "아니오. 저는 아나고만ㅜㅜ",
          "original_form": "아니오. 저는 아나고만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044395.1.9",
          "form": "아 아나고세꼬시는 진리죠",
          "original_form": "아 아나고세꼬시는 진리죠",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044395.1.10",
          "form": "광어도 있구나ㅜㅜㅜ",
          "original_form": "광어도 있구나ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044395.1.11",
          "form": "담에 먹어볼게용",
          "original_form": "담에 먹어볼게용",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044395.1.12",
          "form": "네네 뼈째 써는게 세꼬시니",
          "original_form": "네네 뼈째 써는게 세꼬시니",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044395.1.13",
          "form": "강추요",
          "original_form": "강추요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.14",
          "form": "전어는 드셨나요",
          "original_form": "전어는 드셨나요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.15",
          "form": "회 뭐랑 드세요? 간장? 초장?",
          "original_form": "회 뭐랑 드세요? 간장? 초장?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.16",
          "form": "간장!",
          "original_form": "간장!",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.17",
          "form": "전어는 저한텐 좀 기름져요ㅜ",
          "original_form": "전어는 저한텐 좀 기름져요ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.18",
          "form": "아ㅜ 아쉽네요",
          "original_form": "아ㅜ 아쉽네요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.19",
          "form": "과메기 좋아해요?",
          "original_form": "과메기 좋아해요?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.20",
          "form": "님은 초장?",
          "original_form": "님은 초장?",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.21",
          "form": "네네 사랑해요",
          "original_form": "네네 사랑해요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.22",
          "form": "님은 드세요?",
          "original_form": "님은 드세요?",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044395.1.23",
          "form": "사실 제 취향은...",
          "original_form": "사실 제 취향은...",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044395.1.24",
          "form": "저는 입맛이 싸구려인가봐요ㅜ",
          "original_form": "저는 입맛이 싸구려인가봐요ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044395.1.25",
          "form": "저번에 병어세꼬시 먹었는데 맛있었어요. 드셔보셨나요?",
          "original_form": "저번에 병어세꼬시 먹었는데 맛있었어요. 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044395.1.26",
          "form": "네네 맛좋아요ㅎㅎ",
          "original_form": "네네 맛좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044395.1.27",
          "form": "드문걸 드셨네요",
          "original_form": "드문걸 드셨네요",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044395.1.28",
          "form": "전어도 비슷한데",
          "original_form": "전어도 비슷한데",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044395.1.29",
          "form": "아 저 간장과 막장 좋아합니다. 죄송해요. 질문이 늦게 떠요ㅜ",
          "original_form": "아 저 간장과 막장 좋아합니다. 죄송해요. 질문이 늦게 떠요ㅜ",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044395.1.30",
          "form": "전어는 좀 기름지더라구요ㅎㅎ",
          "original_form": "전어는 좀 기름지더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044395.1.31",
          "form": "그래도 회는 맛있어요",
          "original_form": "그래도 회는 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:37"
        }
      ]
    }
