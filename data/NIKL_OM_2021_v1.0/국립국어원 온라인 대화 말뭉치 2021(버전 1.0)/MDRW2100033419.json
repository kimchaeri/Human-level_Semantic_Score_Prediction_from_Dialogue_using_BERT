      "utterance": [
        {
          "id": "MDRW2100033419.1.1",
          "form": "오늘 행사 화이팅입니다",
          "original_form": "오늘 행사 화이팅입니다",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100033419.1.2",
          "form": "넵 다행히 비가 와서 작게할거같네요 ㅎㅎ",
          "original_form": "넵 다행히 비가 와서 작게할거같네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100033419.1.3",
          "form": "오호 야외 행사에여??",
          "original_form": "오호 야외 행사에여??",
          "speaker_id": "1",
          "time": "20210907 08:11"
        },
        {
          "id": "MDRW2100033419.1.4",
          "form": "야외에서 작게하려고했는데",
          "original_form": "야외에서 작게하려고했는데",
          "speaker_id": "2",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100033419.1.5",
          "form": "비와서 실내에서 더 작게 ㅋㅋ",
          "original_form": "비와서 실내에서 더 작게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100033419.1.6",
          "form": "아아 ㅋㅋㅋ 점점 작아졌네요",
          "original_form": "아아 ㅋㅋㅋ 점점 작아졌네요",
          "speaker_id": "1",
          "time": "20210907 08:13"
        },
        {
          "id": "MDRW2100033419.1.7",
          "form": "너무좋습니다 ㅎㅎㅎㅎㅎ",
          "original_form": "너무좋습니다 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 08:16"
        },
        {
          "id": "MDRW2100033419.1.8",
          "form": "다행이에요 ㅋㅋㅋ 행사나 회식은 작을 수록 좋아요",
          "original_form": "다행이에요 ㅋㅋㅋ 행사나 회식은 작을 수록 좋아요",
          "speaker_id": "1",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100033419.1.9",
          "form": "코로나땜에 회식안해서 너무 좋아요 ㅎㅎ",
          "original_form": "코로나땜에 회식안해서 너무 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100033419.1.10",
          "form": "저도요 단계 내려가서 하자하는거 싫어요",
          "original_form": "저도요 단계 내려가서 하자하는거 싫어요",
          "speaker_id": "1",
          "time": "20210907 08:33"
        },
        {
          "id": "MDRW2100033419.1.11",
          "form": "백신맞아서 8명까지 된다고 회식하자고 하는거 너무 싫어요 진짜 ㅎ",
          "original_form": "백신맞아서 8명까지 된다고 회식하자고 하는거 너무 싫어요 진짜 ㅎ",
          "speaker_id": "2",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100033419.1.12",
          "form": "아 그쵸 그거 너무 시름",
          "original_form": "아 그쵸 그거 너무 시름",
          "speaker_id": "1",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100033419.1.13",
          "form": "차라리 다 끝나면 몰라도ㅠㅠ",
          "original_form": "차라리 다 끝나면 몰라도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100033419.1.14",
          "form": "자기들음 나이있어서 먼저맞았으니까 상관없지만....ㅋㅋㅋㅋㅋ",
          "original_form": "자기들음 나이있어서 먼저맞았으니까 상관없지만....ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:09"
        },
        {
          "id": "MDRW2100033419.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇군요",
          "speaker_id": "1",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100033419.1.16",
          "form": "백신 접종 하셨나요?",
          "original_form": "백신 접종 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 11:24"
        },
        {
          "id": "MDRW2100033419.1.17",
