      "utterance": [
        {
          "id": "MDRW2100021281.1.1",
          "form": "스포츠 좋아하시나요",
          "original_form": "스포츠 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021281.1.2",
          "form": "안좋아하는데 올림픽은 좀 봤어요",
          "original_form": "안좋아하는데 올림픽은 좀 봤어요",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021281.1.3",
          "form": "여자배구보셨나용",
          "original_form": "여자배구보셨나용",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021281.1.4",
          "form": "네 김연경이 잘하더라구요",
          "original_form": "네 김연경이 잘하더라구요",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021281.1.5",
          "form": "이다영 자매건때문에 기대를 안했는데",
          "original_form": "이다영 자매건때문에 기대를 안했는데",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021281.1.6",
          "form": "갓연경선수ㅠ 자매들 넘 시러요",
          "original_form": "갓연경선수ㅠ 자매들 넘 시러요",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021281.1.7",
          "form": "자매들의 ㅈ ㅅ 시도 공격으로 멘탈이 깨졌읉텐데도",
          "original_form": "자매들의 ㅈ ㅅ 시도 공격으로 멘탈이 깨졌읉텐데도",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021281.1.8",
          "form": "리더십있게 잘하더라구요",
          "original_form": "리더십있게 잘하더라구요",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021281.1.9",
          "form": "마자요ㅠㅠㅠ 그것들 공백 하나도 안느껴지고",
          "original_form": "마자요ㅠㅠㅠ 그것들 공백 하나도 안느껴지고",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021281.1.10",
          "form": "다른 선수들 다독이는걸 보고 감동 ㅠ",
          "original_form": "다른 선수들 다독이는걸 보고 감동 ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021281.1.11",
          "form": "국대은퇴라니ㅠ",
          "original_form": "국대은퇴라니ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021281.1.12",
          "form": "그러게요 ㅠㅠ 슬퍼요",
          "original_form": "그러게요 ㅠㅠ 슬퍼요",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021281.1.13",
          "form": "벌써부터 걱정돼요",
          "original_form": "벌써부터 걱정돼요",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021281.1.14",
          "form": "뭐 스타선수가 없어도 종목은 이어지니가요ㅋㅋ",
          "original_form": "뭐 스타선수가 없어도 종목은 이어지니가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021281.1.15",
          "form": "그래도 영향력이 워낙컸어서ㅠ",
          "original_form": "그래도 영향력이 워낙컸어서ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:50"
        }
