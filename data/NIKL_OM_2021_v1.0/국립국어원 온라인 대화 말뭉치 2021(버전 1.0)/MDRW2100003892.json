      "utterance": [
        {
          "id": "MDRW2100003892.1.1",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003892.1.2",
          "form": "ㅇㅇ 넘 시끄러웠어 ㅠ 선풍기 한 대 사야할듯",
          "original_form": "ㅇㅇ 넘 시끄러웠어 ㅠ 선풍기 한 대 사야할듯",
          "speaker_id": "1",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003892.1.3",
          "form": "집에있는 한일 선풍기는 다 고장났네 ㅠ",
          "original_form": "집에있는 한일 선풍기는 다 고장났네 ㅠ",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003892.1.4",
          "form": "회전이 안되거나 버튼이 안눌리거나 ㅋㅋ",
          "original_form": "회전이 안되거나 버튼이 안눌리거나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003892.1.5",
          "form": "신일 한 대만 멀쩡하군",
          "original_form": "신일 한 대만 멀쩡하군",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003892.1.6",
          "form": "써큘레이터는 또 위아래가 안움직여 ㅠ",
          "original_form": "써큘레이터는 또 위아래가 안움직여 ㅠ",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003892.1.7",
          "form": "하아.. 왜 이리 고장이 잘나지",
          "original_form": "하아.. 왜 이리 고장이 잘나지",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003892.1.8",
          "form": "암튼 이번에 신일로 한 대 사야겠어",
          "original_form": "암튼 이번에 신일로 한 대 사야겠어",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003892.1.9",
          "form": "우리 지금 쓰고 있는 모델 아직 안없어졌나 알아봐",
          "original_form": "우리 지금 쓰고 있는 모델 아직 안없어졌나 알아봐",
          "speaker_id": "2",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003892.1.10",
          "form": "그게 아직도 있을라나 10년 됐는데 ㅋㅋ",
          "original_form": "그게 아직도 있을라나 10년 됐는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003892.1.11",
          "form": "그래도 10년이나 혼자 버틴 선풍기니 지금 사도 성능 좋을듯",
          "original_form": "그래도 10년이나 혼자 버틴 선풍기니 지금 사도 성능 좋을듯",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003892.1.12",
          "form": "ㅎㅎ 모델 어떤 건지 살펴봐야겠네",
          "original_form": "ㅎㅎ 모델 어떤 건지 살펴봐야겠네",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003892.1.13",
          "form": "회전 안되는건 그냥 내가 평소에 쓸테니",
          "original_form": "회전 안되는건 그냥 내가 평소에 쓸테니",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003892.1.14",
          "form": "선풍이 안버리고 계속 쓰게?",
          "original_form": "선풍이 안버리고 계속 쓰게?",
          "speaker_id": "1",
