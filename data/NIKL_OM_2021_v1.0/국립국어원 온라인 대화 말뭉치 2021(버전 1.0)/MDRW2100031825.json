      "utterance": [
        {
          "id": "MDRW2100031825.1.1",
          "form": "가장 최근 게임은",
          "original_form": "가장 최근 게임은",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031825.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210905 01:29"
        },
        {
          "id": "MDRW2100031825.1.3",
          "form": "음 오버워치?",
          "original_form": "음 오버워치?",
          "speaker_id": "2",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031825.1.4",
          "form": "아 피파 잠깐 했었다",
          "original_form": "아 피파 잠깐 했었다",
          "speaker_id": "2",
          "time": "20210905 01:32"
        },
        {
          "id": "MDRW2100031825.1.5",
          "form": "피파! 어느나라로?",
          "original_form": "피파! 어느나라로?",
          "speaker_id": "1",
          "time": "20210905 01:35"
        },
        {
          "id": "MDRW2100031825.1.6",
          "form": "ㅋㅋㅋㅋㅋ나라는 아니었고",
          "original_form": "ㅋㅋㅋㅋㅋ나라는 아니었고",
          "speaker_id": "2",
          "time": "20210905 01:38"
        },
        {
          "id": "MDRW2100031825.1.7",
          "form": "해외축구 팀으로!",
          "original_form": "해외축구 팀으로!",
          "speaker_id": "2",
          "time": "20210905 01:39"
        },
        {
          "id": "MDRW2100031825.1.8",
          "form": "최근 게임이 무엇이신가요?? 궁금하다",
          "original_form": "최근 게임이 무엇이신가요?? 궁금하다",
          "speaker_id": "2",
          "time": "20210905 01:39"
        },
        {
          "id": "MDRW2100031825.1.9",
          "form": "그 카톡에 있는 겜",
          "original_form": "그 카톡에 있는 겜",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031825.1.10",
          "form": "콘의 네모너모팡",
          "original_form": "콘의 네모너모팡",
          "speaker_id": "1",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100031825.1.11",
          "form": "네모네모팡",
          "original_form": "네모네모팡",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100031825.1.12",
          "form": "아하 스낵게임이네 잘해??",
          "original_form": "아하 스낵게임이네 잘해??",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031825.1.13",
          "form": "폰게임도 포함해주면 난 모바일 카트라이더!!",
          "original_form": "폰게임도 포함해주면 난 모바일 카트라이더!!",
          "speaker_id": "2",
          "time": "20210905 10:32"
        },
        {
          "id": "MDRW2100031825.1.14",
          "form": "응응 못하는편은 아닌듯",
          "original_form": "응응 못하는편은 아닌듯",
          "speaker_id": "1",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100031825.1.15",
          "form": "난그거 안해봣어",
          "original_form": "난그거 안해봣어",
          "speaker_id": "1",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100031825.1.16",
          "form": "이열 자신감 최고네?",
          "original_form": "이열 자신감 최고네?",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100031825.1.17",
          "form": "예전엔 재밌게 했었는데 어느순간 삭제했어",
          "original_form": "예전엔 재밌게 했었는데 어느순간 삭제했어",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100031825.1.18",
          "form": "나도 잘해!ㅋㅋㅋ",
          "original_form": "나도 잘해!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100031825.1.19",
          "form": "그거 유행이긴 했었지 카트",
          "original_form": "그거 유행이긴 했었지 카트",
          "speaker_id": "1",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100031825.1.20",
          "form": "맞아 게임 그만둔지 좀 되긴했는데 요즘 다시 흥미가 당겨서",
          "original_form": "맞아 게임 그만둔지 좀 되긴했는데 요즘 다시 흥미가 당겨서",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100031825.1.21",
          "form": "참고있어",
          "original_form": "참고있어",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100031825.1.22",
          "form": "폰겜은 괜찮지 뭐",
          "original_form": "폰겜은 괜찮지 뭐",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100031825.1.23",
          "form": "하여튼 현질은 안하는게 맞는듯",
          "original_form": "하여튼 현질은 안하는게 맞는듯",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100031825.1.24",
          "form": "계속할 가능성따위 없어",
          "original_form": "계속할 가능성따위 없어",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100031825.1.25",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100031825.1.26",
          "form": "현질도 해봤어??",
          "original_form": "현질도 해봤어??",
          "speaker_id": "2",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100031825.1.27",
          "form": "응 해봤지",
          "original_form": "응 해봤지",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100031825.1.28",
          "form": "옵치 하다가 그 누구지 밥 친구 걔 스킨 받으려고 했는데",
          "original_form": "옵치 하다가 그 누구지 밥 친구 걔 스킨 받으려고 했는데",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100031825.1.29",
          "form": "못받은 기억 있어 한 몇달전에",
          "original_form": "못받은 기억 있어 한 몇달전에",
          "speaker_id": "1",
          "time": "20210908 02:03"
        }
      ]
    }
  ]
