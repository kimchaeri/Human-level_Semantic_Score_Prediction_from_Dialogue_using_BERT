        },
        {
          "id": "MDRW2100028442.1.2",
          "form": "name2이 하고싶은 데이트는 뭐가 있나요?",
          "original_form": "&name2&이 하고싶은 데이트는 뭐가 있나요?",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028442.1.3",
          "form": "저도 좀 따라하게 ㅋㅋ",
          "original_form": "저도 좀 따라하게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028442.1.4",
          "form": "5년차셔서 어지간하면 다 하셨을 거 같은데",
          "original_form": "5년차셔서 어지간하면 다 하셨을 거 같은데",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028442.1.5",
          "form": "일단 제가 9월에 계획중인 데이트는",
          "original_form": "일단 제가 9월에 계획중인 데이트는",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028442.1.6",
          "form": "방탈출과 일몰보기, 루지타기",
          "original_form": "방탈출과 일몰보기, 루지타기",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028442.1.7",
          "form": "경주여행 다녀오기",
          "original_form": "경주여행 다녀오기",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028442.1.8",
          "form": "도 하고싶어용",
          "original_form": "도 하고싶어용",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028442.1.9",
          "form": "루지?",
          "original_form": "루지?",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028442.1.10",
          "form": "루지는어디서 타요?ㅇ",
          "original_form": "루지는어디서 타요?ㅇ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028442.1.11",
          "form": "안해봤어요 루지는",
          "original_form": "안해봤어요 루지는",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028442.1.12",
          "form": "방탈출 일몰은 자주 해봤는데",
          "original_form": "방탈출 일몰은 자주 해봤는데",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028442.1.13",
          "form": "루지 부산에 이번에 개장해가지구요",
          "original_form": "루지 부산에 이번에 개장해가지구요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028442.1.14",
          "form": "가보려구요 추워지기전에",
          "original_form": "가보려구요 추워지기전에",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028442.1.15",
          "form": "아 타는곳이 생겼군여",
          "original_form": "아 타는곳이 생겼군여",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028442.1.16",
          "form": "그동안 계속 연애하셨으면서 완전 오랜만에 연애하는것처럼 하고싶은게 많으시네요",
          "original_form": "그동안 계속 연애하셨으면서 완전 오랜만에 연애하는것처럼 하고싶은게 많으시네요",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028442.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028442.1.18",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028442.1.19",
          "form": "그전 연애하던 사람이 좀 바쁜 직업이어서",
          "original_form": "그전 연애하던 사람이 좀 바쁜 직업이어서",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028442.1.20",
          "form": "만나도 맨날 같이 일했거나",
          "original_form": "만나도 맨날 같이 일했거나",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028442.1.21",
          "form": "그냥 집에서 넷플릭스나 보면서 놀거나",
          "original_form": "그냥 집에서 넷플릭스나 보면서 놀거나",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028442.1.22",
          "form": "만나서 같이 일을해요?",
          "original_form": "만나서 같이 일을해요?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028442.1.23",
          "form": "넹...",
          "original_form": "넹...",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028442.1.24",
          "form": "전 수업준비하고 전남친은 일하고..",
          "original_form": "전 수업준비하고 전남친은 일하고..",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028442.1.25",
          "form": "그게무슨...",
          "original_form": "그게무슨...",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.26",
          "form": "그래서 연애초에나 좀 데이트했지",
          "original_form": "그래서 연애초에나 좀 데이트했지",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.27",
          "form": "수업준비라는말이 낯설지 않네요",
          "original_form": "수업준비라는말이 낯설지 않네요",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.28",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.30",
          "form": "암튼 그래서 데이트다운 데이트라 좀 오랜만이에요",
          "original_form": "암튼 그래서 데이트다운 데이트라 좀 오랜만이에요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.31",
          "form": "신날만하네요",
          "original_form": "신날만하네요",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.32",
          "form": "저 또 임용 준비할때 만나던 연애라",
          "original_form": "저 또 임용 준비할때 만나던 연애라",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.33",
          "form": "공부도 계속 해야했고",
          "original_form": "공부도 계속 해야했고",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028442.1.34",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028442.1.35",
          "form": "근데 너무 일 좋아하는 사람 만나니까",
          "original_form": "근데 너무 일 좋아하는 사람 만나니까",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028442.1.36",
          "form": "데이트때  공부하거나 일하는거 싫어요ㅋㅋㅋ",
          "original_form": "데이트때  공부하거나 일하는거 싫어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028442.1.37",
          "form": "결혼생활을 그려봐도 별로 행복하지가 않은거에요",
          "original_form": "결혼생활을 그려봐도 별로 행복하지가 않은거에요",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028442.1.38",
          "form": "그래서 아 결혼할 사이는 아니겠다",
          "original_form": "그래서 아 결혼할 사이는 아니겠다",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028442.1.39",
          "form": "그러면서도 6년을 만났어오 ㅋㅋㅋ",
          "original_form": "그러면서도 6년을 만났어오 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028442.1.40",
          "form": "너무 텔레파시가 잘 통하던 사이라 운명이라 믿어가지고 ㅋㅋㅋ",
          "original_form": "너무 텔레파시가 잘 통하던 사이라 운명이라 믿어가지고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028442.1.41",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028442.1.42",
          "form": "잘통했어요?",
          "original_form": "잘통했어요?",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028442.1.43",
          "form": "네네 그냥 생각이 똑같았어요",
          "original_form": "네네 그냥 생각이 똑같았어요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028442.1.44",
          "form": "동시에 똑같은 말 내뱉을 정도로",
          "original_form": "동시에 똑같은 말 내뱉을 정도로",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028442.1.45",
          "form": "그냥 조용히 있다가 동시에 흥얼거리는데 똑같은 노래 똑같은 구절 흥얼거리고",
          "original_form": "그냥 조용히 있다가 동시에 흥얼거리는데 똑같은 노래 똑같은 구절 흥얼거리고",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.46",
          "form": "모지",
          "original_form": "모지",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.47",
          "form": "ㅋㅋㅋㅋ신기했어요",
          "original_form": "ㅋㅋㅋㅋ신기했어요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.48",
          "form": "근데도 헤어지는군여",
          "original_form": "근데도 헤어지는군여",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.49",
          "form": "장거리란.... 그런것인가",
          "original_form": "장거리란.... 그런것인가",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.50",
          "form": "네네 결혼할 상황이 전혀 안되고",
          "original_form": "네네 결혼할 상황이 전혀 안되고",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.51",
          "form": "그리고 일에 너무 매달리니 점점 내가 1순위가 아니게 되고",
          "original_form": "그리고 일에 너무 매달리니 점점 내가 1순위가 아니게 되고",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028442.1.52",
          "form": "그러더니 회사 동료랑 결혼 하더라구요 ^.^",
          "original_form": "그러더니 회사 동료랑 결혼 하더라구요 ^.^",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.53",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.54",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.55",
          "form": "한 2년뒤에 들어 보니 결혼했더라구요",
          "original_form": "한 2년뒤에 들어 보니 결혼했더라구요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.56",
          "form": "긴 연애 후에 짧게 만나고 결혼하는게 정설인지",
          "original_form": "긴 연애 후에 짧게 만나고 결혼하는게 정설인지",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.57",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.58",
          "form": "그얘기할랬는데",
          "original_form": "그얘기할랬는데",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028442.1.59",
          "form": "ㅋㅋㅋㅋㅋ씁쓸",
          "original_form": "ㅋㅋㅋㅋㅋ씁쓸",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028442.1.60",
          "form": "씁쓸해하지마요",
