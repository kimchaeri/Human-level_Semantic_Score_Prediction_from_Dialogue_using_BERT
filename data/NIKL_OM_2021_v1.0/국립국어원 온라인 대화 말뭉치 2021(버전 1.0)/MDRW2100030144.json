      "utterance": [
        {
          "id": "MDRW2100030144.1.1",
          "form": "일이",
          "original_form": "일이",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030144.1.2",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030144.1.3",
          "form": "내꺼라니",
          "original_form": "내꺼라니",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030144.1.4",
          "form": "그냥 기상이변 노립니다",
          "original_form": "그냥 기상이변 노립니다",
          "speaker_id": "1",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100030144.1.5",
          "form": "ㅋㅋㅋ 그런 끔찍한 일",
          "original_form": "ㅋㅋㅋ 그런 끔찍한 일",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030144.1.6",
          "form": "절대 안돼요",
          "original_form": "절대 안돼요",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030144.1.7",
          "form": "다같이 안나오고 말지",
          "original_form": "다같이 안나오고 말지",
          "speaker_id": "2",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100030144.1.8",
          "form": "절대안되조.....",
          "original_form": "절대안되조.....",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030144.1.9",
          "form": "전부 내 일이라니",
          "original_form": "전부 내 일이라니",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030144.1.10",
          "form": "진짜 재앙입니다",
          "original_form": "진짜 재앙입니다",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100030144.1.11",
          "form": "끔 찍 해여",
          "original_form": "끔 찍 해여",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.12",
          "form": "강조하려고 띄어쓰기 했어요",
          "original_form": "강조하려고 띄어쓰기 했어요",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.15",
          "form": "저 이직하고 싶은데",
          "original_form": "저 이직하고 싶은데",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.16",
          "form": "할줄 아는게 없어서 못해요",
          "original_form": "할줄 아는게 없어서 못해요",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.17",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100030144.1.18",
          "form": "찾다보면 다 이직할 수 있을꺼에요!",
          "original_form": "찾다보면 다 이직할 수 있을꺼에요!",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030144.1.19",
          "form": "도전해봐야 이직할수있지않을까요?",
          "original_form": "도전해봐야 이직할수있지않을까요?",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100030144.1.20",
          "form": "그랬으면 좋겠는데 여기서 뭘 하고 있는건지",
          "original_form": "그랬으면 좋겠는데 여기서 뭘 하고 있는건지",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030144.1.21",
          "form": "잡무담당이나 다름없어요",
          "original_form": "잡무담당이나 다름없어요",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030144.1.22",
          "form": "아..ㅠㅠ 근데 회사가 다그래요 ㅠ 뭔가 잡부...뭐든지 내가하는거같은",
          "original_form": "아..ㅠㅠ 근데 회사가 다그래요 ㅠ 뭔가 잡부...뭐든지 내가하는거같은",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100030144.1.23",
          "form": "다들 그런게 맞는거겠죠? ㅜㅜ 별거 안했는데 훅 시간가고",
          "original_form": "다들 그런게 맞는거겠죠? ㅜㅜ 별거 안했는데 훅 시간가고",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100030144.1.24",
          "form": "ㅠㅠ그쵸 ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠ그쵸 ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030144.1.25",
          "form": "못끝내서 늦게 있다가 오고 흑흑",
          "original_form": "못끝내서 늦게 있다가 오고 흑흑",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030144.1.26",
          "form": "ㅠㅜㅜㅜㅜㅠㅠㅠ아이고 ㅠㅠㅠㅠ",
          "original_form": "ㅠㅜㅜㅜㅜㅠㅠㅠ아이고 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030144.1.27",
          "form": "사람도 그지같아요",
          "original_form": "사람도 그지같아요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030144.1.28",
          "form": "슬퍼요",
          "original_form": "슬퍼요",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030144.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030144.1.30",
          "form": "그게 제일 큰거 같아요",
          "original_form": "그게 제일 큰거 같아요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030144.1.31",
          "form": "짜증...",
          "original_form": "짜증...",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030144.1.32",
          "form": "그쵸..뭐든 퇴근은 정각에시켜라 ㅠ",
          "original_form": "그쵸..뭐든 퇴근은 정각에시켜라 ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:42"
        }
      ]
