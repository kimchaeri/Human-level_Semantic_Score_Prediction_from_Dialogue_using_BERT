      "utterance": [
        {
          "id": "MDRW2100036180.1.1",
          "form": "오일파스타도 좋아하시나요",
          "original_form": "오일파스타도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100036180.1.2",
          "form": "저 8시쯤 다시 올텐데 괜찮으심 그 때 대화해주실 수 있나요?",
          "original_form": "저 8시쯤 다시 올텐데 괜찮으심 그 때 대화해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100036180.1.3",
          "form": "오 네네 제가 하고잇을진 모르겟는데 편한시간에 연락주세요!",
          "original_form": "오 네네 제가 하고잇을진 모르겟는데 편한시간에 연락주세요!",
          "speaker_id": "2",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100036180.1.4",
          "form": "오일파스타도 좋아하는데요",
          "original_form": "오일파스타도 좋아하는데요",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100036180.1.5",
          "form": "맛있게 하려면 미원이 필요해요",
          "original_form": "맛있게 하려면 미원이 필요해요",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100036180.1.6",
          "form": "아 진짜요??",
          "original_form": "아 진짜요??",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100036180.1.7",
          "form": "미원맛이엇던건가요 ㅋㅋㅋㅋㅋ 전 제가 요리못하는줄",
          "original_form": "미원맛이엇던건가요 ㅋㅋㅋㅋㅋ 전 제가 요리못하는줄",
          "speaker_id": "2",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100036180.1.8",
          "form": "네 식당건 많이그래요",
          "original_form": "네 식당건 많이그래요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100036180.1.9",
          "form": "집에서 아무리 조개랑 올리브오일을 뉴ㅔㅇㅎ어도",
          "original_form": "집에서 아무리 조개랑 올리브오일을 뉴ㅔㅇㅎ어도",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036180.1.10",
          "form": "어쩐지 맛잇더라구요",
          "original_form": "어쩐지 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036180.1.11",
          "form": "식당만이 나올라면 미워너을 1티스푼^^",
          "original_form": "식당만이 나올라면 미워너을 1티스푼^^",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036180.1.12",
          "form": "한 번 해보세요ㅋㅋㅋ",
          "original_form": "한 번 해보세요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036180.1.13",
          "form": "마법의가루..",
          "original_form": "마법의가루..",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036180.1.14",
          "form": "저는 마늘 모자란줄알고 마늘을 더넣고 그랫어요",
          "original_form": "저는 마늘 모자란줄알고 마늘을 더넣고 그랫어요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036180.1.15",
          "form": "마늘도 좋긴한데 전 미원과 충분한 소금ㅋㅋㅋ",
          "original_form": "마늘도 좋긴한데 전 미원과 충분한 소금ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100036180.1.16",
          "form": "면이 짜면 안되는데 짭짤해야해요",
          "original_form": "면이 짜면 안되는데 짭짤해야해요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100036180.1.17",
          "form": "면 삶을떄 소금 좀 넣어요",
          "original_form": "면 삶을떄 소금 좀 넣어요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100036180.1.18",
          "form": "아 맞아요 저도 소금 한스푼넣어요",
          "original_form": "아 맞아요 저도 소금 한스푼넣어요",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036180.1.19",
          "form": "그ㅡ리고 마늘 태우듯이 볶구요 ㅋㅋㅋ",
          "original_form": "그ㅡ리고 마늘 태우듯이 볶구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036180.1.20",
          "form": "네 그리고 새우나 조갯살도 추가하시나요?",
          "original_form": "네 그리고 새우나 조갯살도 추가하시나요?",
          "speaker_id": "1",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100036180.1.21",
          "form": "그래도 부족했다면 미원이 필요하십니다 하하",
          "original_form": "그래도 부족했다면 미원이 필요하십니다 하하",
          "speaker_id": "1",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100036180.1.22",
          "form": "미원 사야겠네요 ㅋㅋㅋㅋ 다시다라도 넣어볼까봐요",
          "original_form": "미원 사야겠네요 ㅋㅋㅋㅋ 다시다라도 넣어볼까봐요",
          "speaker_id": "2",
          "time": "20210831 13:31"
        }
      ]
    }
