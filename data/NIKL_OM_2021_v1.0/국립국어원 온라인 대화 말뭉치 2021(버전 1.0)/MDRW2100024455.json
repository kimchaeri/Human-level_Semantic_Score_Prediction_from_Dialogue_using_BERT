      "utterance": [
        {
          "id": "MDRW2100024455.1.1",
          "form": "저 서울 살때",
          "original_form": "저 서울 살때",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100024455.1.2",
          "form": "가산디지털단지쪽 살앗는데",
          "original_form": "가산디지털단지쪽 살앗는데",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.3",
          "form": "거기서 광명이 가깝거든요?",
          "original_form": "거기서 광명이 가깝거든요?",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.5",
          "form": "거기 돼지고기 전골이 참 맛있었습니다",
          "original_form": "거기 돼지고기 전골이 참 맛있었습니다",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.6",
          "form": "소하동인가",
          "original_form": "소하동인가",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.7",
          "form": "기아자동차 근처였는데",
          "original_form": "기아자동차 근처였는데",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.8",
          "form": "헉 말만 들어도 침고이는 메뉴네요",
          "original_form": "헉 말만 들어도 침고이는 메뉴네요",
          "speaker_id": "1",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024455.1.9",
          "form": "맛집은 서울에 다있습니다",
          "original_form": "맛집은 서울에 다있습니다",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024455.1.10",
          "form": "식당이름이 뭔가요!!",
          "original_form": "식당이름이 뭔가요!!",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024455.1.11",
          "form": "잠시",
          "original_form": "잠시",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024455.1.12",
          "form": "소하동 돼지집!",
          "original_form": "소하동 돼지집!",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024455.1.13",
          "form": "저장했어요!",
          "original_form": "저장했어요!",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024455.1.14",
          "form": "친구들과 가봐야겠어요ㅎ 벌써 기대되네요ㅎㅎ",
          "original_form": "친구들과 가봐야겠어요ㅎ 벌써 기대되네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024455.1.15",
          "form": "멀어서 괜찮나요?",
          "original_form": "멀어서 괜찮나요?",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024455.1.16",
          "form": "그리 안 멀어요!",
          "original_form": "그리 안 멀어요!",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024455.1.17",
          "form": "오 한번 가보세요",
          "original_form": "오 한번 가보세요",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024455.1.18",
          "form": "거기 사리 추가하셔야됩니다",
          "original_form": "거기 사리 추가하셔야됩니다",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024455.1.19",
          "form": "무슨 사리요?",
          "original_form": "무슨 사리요?",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024455.1.20",
          "form": "라면 먹엇는데 면치기 조심!\n사리 종류 많아요",
          "original_form": "라면 먹엇는데 면치기 조심!\n사리 종류 많아요",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024455.1.21",
          "form": "떡사리도 있구",
          "original_form": "떡사리도 있구",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024455.1.22",
          "form": "아 두가지인듯 ㅋㅋㅋ",
          "original_form": "아 두가지인듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024455.1.23",
          "form": "넵넵 명심할게요ㅎ 팁 감사ㅎㅎ",
          "original_form": "넵넵 명심할게요ㅎ 팁 감사ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:34"
        }
      ]
