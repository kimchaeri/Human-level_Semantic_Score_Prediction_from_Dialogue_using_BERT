        {
          "id": "MDRW2100012664.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012664.1.2",
          "form": "요즘은 가끔 스트레칭 정도만요",
          "original_form": "요즘은 가끔 스트레칭 정도만요",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012664.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012664.1.4",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012664.1.5",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012664.1.6",
          "form": "유투브로",
          "original_form": "유투브로",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012664.1.7",
          "form": "어떤 채널 보세요?",
          "original_form": "어떤 채널 보세요?",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012664.1.8",
          "form": "땅크여",
          "original_form": "땅크여",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012664.1.9",
          "form": "거기가 진짜 유명한가봐요. 이름은 많이 들어봤어요",
          "original_form": "거기가 진짜 유명한가봐요. 이름은 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012664.1.10",
          "form": "부부케미가",
          "original_form": "부부케미가",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012664.1.11",
          "form": "헬스장도 다녀보셨어요?",
          "original_form": "헬스장도 다녀보셨어요?",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012664.1.12",
          "form": "코로나전에",
          "original_form": "코로나전에",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012664.1.13",
          "form": "전 한 번도 안 가봤어요",
          "original_form": "전 한 번도 안 가봤어요",
          "speaker_id": "2",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012664.1.14",
          "form": "가면 분위기가너무좋아요",
          "original_form": "가면 분위기가너무좋아요",
          "speaker_id": "1",
          "time": "20210825 07:08"
        },
        {
          "id": "MDRW2100012664.1.15",
          "form": "활기찬 분위기?",
          "original_form": "활기찬 분위기?",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100012664.1.16",
          "form": "코로나 종식되면 가보고 싶네요",
          "original_form": "코로나 종식되면 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210825 12:41"
