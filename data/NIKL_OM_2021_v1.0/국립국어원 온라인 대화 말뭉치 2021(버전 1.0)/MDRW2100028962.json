      "utterance": [
        {
          "id": "MDRW2100028962.1.1",
          "form": "난 오늘 냉라면해먹었어!",
          "original_form": "난 오늘 냉라면해먹었어!",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028962.1.2",
          "form": "와 맛잇었겟다ㅠㅜㅜ 나는 오늘 간식으로 떡먹었어! 너는?",
          "original_form": "와 맛잇었겟다ㅠㅜㅜ 나는 오늘 간식으로 떡먹었어! 너는?",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028962.1.3",
          "form": "나는 간식으로 만두 먹었어",
          "original_form": "나는 간식으로 만두 먹었어",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028962.1.4",
          "form": "만두생각하니까 튀김만두 먹고싶네ㅋㅋㅋ",
          "original_form": "만두생각하니까 튀김만두 먹고싶네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028962.1.5",
          "form": "와 나는 비빔만두",
          "original_form": "와 나는 비빔만두",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100028962.1.6",
          "form": "쫄면도ㅠ",
          "original_form": "쫄면도ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100028962.1.7",
          "form": "면하면 골뱅이 무침에 소면도 맛있지",
          "original_form": "면하면 골뱅이 무침에 소면도 맛있지",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100028962.1.8",
          "form": "골뱅이 먹을 수 있어?",
          "original_form": "골뱅이 먹을 수 있어?",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100028962.1.9",
          "form": "와 완전좋아 응응",
          "original_form": "와 완전좋아 응응",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100028962.1.10",
          "form": "나는 인생을 골뱅이 소면을 먹기전과 후로 나눌 수 있어",
          "original_form": "나는 인생을 골뱅이 소면을 먹기전과 후로 나눌 수 있어",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100028962.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100028962.1.12",
          "form": "그정도야?! 엄청 좋아하나보다 ㅋㅋㅋㅋ",
          "original_form": "그정도야?! 엄청 좋아하나보다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028962.1.13",
          "form": "골뱅이 맛있는데 비싸서ㅋㅋ ㅠㅠ",
          "original_form": "골뱅이 맛있는데 비싸서ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100028962.1.14",
          "form": "아.....",
          "original_form": "아.....",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100028962.1.15",
          "form": "슬프다....",
          "original_form": "슬프다....",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100028962.1.16",
          "form": "맛있는건 다 비싼것같아ㅜㅜ",
          "original_form": "맛있는건 다 비싼것같아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100028962.1.17",
          "form": "맞아 일단 맛있으면 좀 비싸...",
          "original_form": "맞아 일단 맛있으면 좀 비싸...",
          "speaker_id": "1",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100028962.1.18",
          "form": "그치...ㅜㅜ 물가 넘 올ㄹ랐다",
          "original_form": "그치...ㅜㅜ 물가 넘 올ㄹ랐다",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100028962.1.19",
          "form": "분식까지 올라버려서 예전 물가 생각하면ㅠㅠ",
          "original_form": "분식까지 올라버려서 예전 물가 생각하면ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100028962.1.20",
          "form": "그치ㅜㅋㅋ",
          "original_form": "그치ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100028962.1.21",
          "form": "떡볶이 만원이 말이냐고 ㅋㅋ",
          "original_form": "떡볶이 만원이 말이냐고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100028962.1.22",
          "form": "맞아 떡볶이를 만원내고 먹으라니",
          "original_form": "맞아 떡볶이를 만원내고 먹으라니",
