      "utterance": [
        {
          "id": "MDRW2100002886.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002886.1.2",
          "form": "네 안녕하세요 ㅎㅎ",
          "original_form": "네 안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002886.1.3",
          "form": "중고거래 많이 하시나요?",
          "original_form": "중고거래 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002886.1.4",
          "form": "자주 안하는데 요새 폭풍으로 몰아서 팔았어요",
          "original_form": "자주 안하는데 요새 폭풍으로 몰아서 팔았어요",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002886.1.5",
          "form": "한참 바빴어요",
          "original_form": "한참 바빴어요",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002886.1.6",
          "form": "보통 어디를 이용하시나요?중고나라나 당근?",
          "original_form": "보통 어디를 이용하시나요?중고나라나 당근?",
          "speaker_id": "1",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002886.1.7",
          "form": "둘다요",
          "original_form": "둘다요",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002886.1.8",
          "form": "물품따라 팔리는게 다른거 같아요",
          "original_form": "물품따라 팔리는게 다른거 같아요",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002886.1.9",
          "form": "저는 중고나라는 이용많이하는데 당근은 아직 안해봤어요~",
          "original_form": "저는 중고나라는 이용많이하는데 당근은 아직 안해봤어요~",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002886.1.10",
          "form": "당슨은 좀 봐야해서 그래요",
          "original_form": "당슨은 좀 봐야해서 그래요",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002886.1.11",
          "form": "근데 또 택배 안보내니 편해요",
          "original_form": "근데 또 택배 안보내니 편해요",
          "speaker_id": "2",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002886.1.12",
          "form": "저는 택배 보내기가 되게 편한 상황이라 중고나라가 편해요! 집에서 계약 택배 매일 보내고 있어서 ..",
          "original_form": "저는 택배 보내기가 되게 편한 상황이라 중고나라가 편해요! 집에서 계약 택배 매일 보내고 있어서 ..",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002886.1.13",
          "form": "저희는 편의점 택배가 안돼요 ㅜㅜ",
          "original_form": "저희는 편의점 택배가 안돼요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002886.1.14",
          "form": "아 계약택배요",
          "original_form": "아 계약택배요",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002886.1.15",
          "form": "그럼 중고나라가 편하죠",
          "original_form": "그럼 중고나라가 편하죠",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002886.1.16",
          "form": "네 이천오백원에 보내고 있으니.. 중고거래 할거 있으면 끼워서 보내면 되용",
          "original_form": "네 이천오백원에 보내고 있으니.. 중고거래 할거 있으면 끼워서 보내면 되용",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002886.1.17",
          "form": "당근은 얼굴보고 지역이라 좀 신경여요",
          "original_form": "당근은 얼굴보고 지역이라 좀 신경여요",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002886.1.18",
          "form": "네 돌아다니기가 좀 그럴거 같아요~",
          "original_form": "네 돌아다니기가 좀 그럴거 같아요~",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002886.1.19",
          "form": "그래두 장점도 있으니",
          "original_form": "그래두 장점도 있으니",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002886.1.20",
          "form": "오라고하면 오셔서  괜찮았어요",
          "original_form": "오라고하면 오셔서  괜찮았어요",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002886.1.21",
          "form": "주로 판매를 많이 하시나요 구매를 많이 하시나요/",
          "original_form": "주로 판매를 많이 하시나요 구매를 많이 하시나요/",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002886.1.22",
          "form": "전 구매는 세제품 하고요 팔기만 중고요",
          "original_form": "전 구매는 세제품 하고요 팔기만 중고요",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002886.1.23",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002886.1.24",
          "form": "전자제품같은건 진짜 중고품 구매하면 안될거 같더라구요",
          "original_form": "전자제품같은건 진짜 중고품 구매하면 안될거 같더라구요",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002886.1.25",
          "form": "그럼요 새것도 망가지는데",
          "original_form": "그럼요 새것도 망가지는데",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002886.1.26",
          "form": "절대 안돼요",
          "original_form": "절대 안돼요",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002886.1.27",
          "form": "반품도 안되고",
          "original_form": "반품도 안되고",
          "speaker_id": "2",
          "time": "20210820 16:26"
        }
      ]
    }
  ]
