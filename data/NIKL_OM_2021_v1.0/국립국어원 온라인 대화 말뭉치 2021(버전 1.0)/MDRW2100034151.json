      "utterance": [
        {
          "id": "MDRW2100034151.1.1",
          "form": "어떤 게임을 즐기시나요?",
          "original_form": "어떤 게임을 즐기시나요?",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034151.1.2",
          "form": "저는 요즘 폰게임은 잘안하는데 젤 최근에 한건 카트라이더러쉬요 !",
          "original_form": "저는 요즘 폰게임은 잘안하는데 젤 최근에 한건 카트라이더러쉬요 !",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034151.1.3",
          "form": "오래 하셨나여?",
          "original_form": "오래 하셨나여?",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034151.1.4",
          "form": "저 한 1년하다가 지금은 안해요 ㅎㅎ",
          "original_form": "저 한 1년하다가 지금은 안해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100034151.1.5",
          "form": "카트도 템이 중요한가요?",
          "original_form": "카트도 템이 중요한가요?",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034151.1.6",
          "form": "네 카트빨..이 좀 있긴해요!",
          "original_form": "네 카트빨..이 좀 있긴해요!",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100034151.1.7",
          "form": "근데 실력도 있어야하는거 같아서 ㅋㅋ",
          "original_form": "근데 실력도 있어야하는거 같아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034151.1.8",
          "form": "한 50대 50 느낌이에요 ㅎㅎ",
          "original_form": "한 50대 50 느낌이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034151.1.9",
          "form": "컴퓨터 카트는 워낙 고인물들이 많았어서ㅋㅋㅋㅋ 폰카트는 이제 좀 덜하려나요ㅠ",
          "original_form": "컴퓨터 카트는 워낙 고인물들이 많았어서ㅋㅋㅋㅋ 폰카트는 이제 좀 덜하려나요ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100034151.1.10",
          "form": "아 맞아요 ㅋㅋ 그거 버그도 있잖아요 !",
          "original_form": "아 맞아요 ㅋㅋ 그거 버그도 있잖아요 !",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100034151.1.11",
          "form": "정말요?? 어떤 버그인가여?",
          "original_form": "정말요?? 어떤 버그인가여?",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100034151.1.12",
          "form": "그 게임하다가 갑자기",
          "original_form": "그 게임하다가 갑자기",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034151.1.13",
          "form": "하늘로 날아가요 ㅋㅋㅋ",
          "original_form": "하늘로 날아가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100034151.1.14",
          "form": "상대편이 버그 써서 ,,",
          "original_form": "상대편이 버그 써서 ,,",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100034151.1.15",
          "form": "와...걸리면 정말 어이가 없을 것 같은 버그네요...영상 찾아봐야겠어요ㅋㅋㅋㅋ",
          "original_form": "와...걸리면 정말 어이가 없을 것 같은 버그네요...영상 찾아봐야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034151.1.16",
          "form": "그거 때문에 막 난리였어요ㅋㅋ",
          "original_form": "그거 때문에 막 난리였어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034151.1.17",
          "form": "게임할 맛 안난다구..",
          "original_form": "게임할 맛 안난다구..",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034151.1.18",
          "form": "고인물들이 모이면 버그도 생기죠ㅠ",
          "original_form": "고인물들이 모이면 버그도 생기죠ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100034151.1.19",
          "form": "진짜요 ㅠㅠ 고인물들 많으면 보통 잘 망하죠 ..",
          "original_form": "진짜요 ㅠㅠ 고인물들 많으면 보통 잘 망하죠 ..",
          "speaker_id": "2",
          "time": "20210827 20:43"
        }
      ]
