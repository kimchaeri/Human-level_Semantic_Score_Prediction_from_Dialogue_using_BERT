      "utterance": [
        {
          "id": "MDRW2100005689.1.1",
          "form": "술마실거야",
          "original_form": "술마실거야",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100005689.1.2",
          "form": "맥주",
          "original_form": "맥주",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100005689.1.3",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100005689.1.4",
          "form": "지금 먹어?",
          "original_form": "지금 먹어?",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100005689.1.5",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100005689.1.6",
          "form": "나도 맛있는 술 먹고싶다",
          "original_form": "나도 맛있는 술 먹고싶다",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100005689.1.7",
          "form": "진 마셔유",
          "original_form": "진 마셔유",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005689.1.8",
          "form": "안주로 완자 줄까?",
          "original_form": "안주로 완자 줄까?",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005689.1.9",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005689.1.10",
          "form": "완자에 대한 집착 stop",
          "original_form": "완자에 대한 집착 stop",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100005689.1.11",
          "form": "진 어떤거 마셔유?",
          "original_form": "진 어떤거 마셔유?",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100005689.1.12",
          "form": "나는 맥주",
          "original_form": "나는 맥주",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100005689.1.13",
          "form": "너 진 마시라구ㅎㅎ",
          "original_form": "너 진 마시라구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100005689.1.14",
          "form": "아 나한테 마시라고",
          "original_form": "아 나한테 마시라고",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100005689.1.15",
          "form": "근데 뭔가",
          "original_form": "근데 뭔가",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100005689.1.16",
          "form": "술이 즐겁게 들어가고",
          "original_form": "술이 즐겁게 들어가고",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100005689.1.17",
          "form": "잘 넘어갈 텐션이 아니야",
          "original_form": "잘 넘어갈 텐션이 아니야",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100005689.1.18",
          "form": "예전에는 술이라면 그저 좋아서 혼술 막 했는데",
          "original_form": "예전에는 술이라면 그저 좋아서 혼술 막 했는데",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100005689.1.19",
          "form": "이젠..건강 챙겨서인지 안 땡긴다",
          "original_form": "이젠..건강 챙겨서인지 안 땡긴다",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100005689.1.20",
          "form": "그냥 맛있는 게 없어서 그런가?",
          "original_form": "그냥 맛있는 게 없어서 그런가?",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100005689.1.21",
          "form": "봄베이 사파이어 좀 질려",
          "original_form": "봄베이 사파이어 좀 질려",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100005689.1.22",
          "form": "나는 그냥 주정뱅이여",
          "original_form": "나는 그냥 주정뱅이여",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100005689.1.23",
          "form": "아유..",
          "original_form": "아유..",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100005689.1.24",
          "form": "나도 맛있는 와인 좀 사와서",
          "original_form": "나도 맛있는 와인 좀 사와서",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100005689.1.25",
          "form": "행복한 주정뱅이가 되고 싶구먼",
          "original_form": "행복한 주정뱅이가 되고 싶구먼",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100005689.1.26",
          "form": "건강한 주정뱅이가 되어라",
          "original_form": "건강한 주정뱅이가 되어라",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100005689.1.27",
          "form": "행복하고 건강한 주정뱅이가 되는 거제",
          "original_form": "행복하고 건강한 주정뱅이가 되는 거제",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100005689.1.28",
          "form": "지금 술 안마신지 두달 다 되어간당께",
          "original_form": "지금 술 안마신지 두달 다 되어간당께",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100005689.1.29",
          "form": "한번 마셔도 괜찮여",
          "original_form": "한번 마셔도 괜찮여",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100005689.1.30",
          "form": "나 갑상선저하증이랑 콜레스테롤 수치도 정상 됐어",
          "original_form": "나 갑상선저하증이랑 콜레스테롤 수치도 정상 됐어",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005689.1.31",
          "form": "근데 오늘은 아니야",
          "original_form": "근데 오늘은 아니야",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005689.1.32",
          "form": "맛있는 술을 즐겁게 먹고싶어",
          "original_form": "맛있는 술을 즐겁게 먹고싶어",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005689.1.33",
          "form": "은지가 오면 하자",
          "original_form": "은지가 오면 하자",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100005689.1.34",
          "form": "술이 마구 탱긴다",
          "original_form": "술이 마구 탱긴다",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100005689.1.35",
          "form": "곧 죽을 건가봐",
          "original_form": "곧 죽을 건가봐",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100005689.1.36",
          "form": "뭔 소리여",
          "original_form": "뭔 소리여",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100005689.1.37",
          "form": "술이 와 땡기는디",
          "original_form": "술이 와 땡기는디",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100005689.1.38",
          "form": "오늘 좀 잘 들어가는 날인가벼",
          "original_form": "오늘 좀 잘 들어가는 날인가벼",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100005689.1.39",
          "form": "드라마엔 비어지",
          "original_form": "드라마엔 비어지",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100005689.1.40",
          "form": "드라마 보는겨?",
          "original_form": "드라마 보는겨?",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100005689.1.41",
          "form": "그럼 맥주 하나 까야제",
          "original_form": "그럼 맥주 하나 까야제",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100005689.1.42",
          "form": "즐거운 시간 보내더라고~",
          "original_form": "즐거운 시간 보내더라고~",
          "speaker_id": "1",
          "time": "20210822 01:12"
        }
      ]
    }
