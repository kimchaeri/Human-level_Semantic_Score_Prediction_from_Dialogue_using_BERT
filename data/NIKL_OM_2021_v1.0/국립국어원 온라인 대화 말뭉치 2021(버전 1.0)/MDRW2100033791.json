      "utterance": [
        {
          "id": "MDRW2100033791.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100033791.1.2",
          "form": "아니요 님은요?",
          "original_form": "아니요 님은요?",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100033791.1.3",
          "form": "저는 휴식하러거는건 좋아요ㅡ호캉스나 스파?",
          "original_form": "저는 휴식하러거는건 좋아요ㅡ호캉스나 스파?",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100033791.1.4",
          "form": "호캉스 좋죠",
          "original_form": "호캉스 좋죠",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100033791.1.5",
          "form": "그러나 현실은 애들 뒤치닥거리ㅠ",
          "original_form": "그러나 현실은 애들 뒤치닥거리ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100033791.1.6",
          "form": "어디 호텔 가셨나요?",
          "original_form": "어디 호텔 가셨나요?",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100033791.1.7",
          "form": "애들 땜에 체험위주로ㅠ 다녔어요",
          "original_form": "애들 땜에 체험위주로ㅠ 다녔어요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100033791.1.8",
          "form": "근데 그마저도 코로나 때문에ㅠ",
          "original_form": "근데 그마저도 코로나 때문에ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100033791.1.9",
          "form": "어떤 체험하셨어요?",
          "original_form": "어떤 체험하셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100033791.1.10",
          "form": "제주도 가도 돌고래 악수..키티박물관인가.ㅋㅋ가고",
          "original_form": "제주도 가도 돌고래 악수..키티박물관인가.ㅋㅋ가고",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100033791.1.11",
          "form": "알차게 보내셨네요",
          "original_form": "알차게 보내셨네요",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100033791.1.12",
          "form": "남들은 제주도가면 명소다닌다던데ㅎㅎ 전 체험만 주구장창 한것 같ㅇ아요ㅋㅋ",
          "original_form": "남들은 제주도가면 명소다닌다던데ㅎㅎ 전 체험만 주구장창 한것 같ㅇ아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100033791.1.13",
          "form": "체험도 좋죠",
          "original_form": "체험도 좋죠",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100033791.1.14",
          "form": "제주도가고싶네요",
          "original_form": "제주도가고싶네요",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100033791.1.15",
          "form": "코로나 끝나면 더 가고 싶네요ㅠ",
          "original_form": "코로나 끝나면 더 가고 싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        }
      ]
