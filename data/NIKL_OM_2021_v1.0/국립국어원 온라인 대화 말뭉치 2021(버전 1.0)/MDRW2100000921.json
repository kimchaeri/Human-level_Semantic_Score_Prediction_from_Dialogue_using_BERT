      "utterance": [
        {
          "id": "MDRW2100000921.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000921.1.2",
          "form": "여행에 미치고싶습니다",
          "original_form": "여행에 미치고싶습니다",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000921.1.3",
          "form": "가고싶은 여행지는요?",
          "original_form": "가고싶은 여행지는요?",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000921.1.4",
          "form": "저 영국 가고싶어요",
          "original_form": "저 영국 가고싶어요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000921.1.5",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000921.1.6",
          "form": "문화가 많은 도시라 궁금해요",
          "original_form": "문화가 많은 도시라 궁금해요",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000921.1.7",
          "form": "영국가서 뭐하실예정이신가요?",
          "original_form": "영국가서 뭐하실예정이신가요?",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000921.1.8",
          "form": "영국하면 빅벤이죠",
          "original_form": "영국하면 빅벤이죠",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000921.1.9",
          "form": "시계탑 보고싶어요",
          "original_form": "시계탑 보고싶어요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000921.1.10",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000921.1.11",
          "form": "저는 런던에 힙합 클럽 가보고싶어요",
          "original_form": "저는 런던에 힙합 클럽 가보고싶어요",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000921.1.12",
          "form": "그라임이라고 영국 힙합이 있는데",
          "original_form": "그라임이라고 영국 힙합이 있는데",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000921.1.13",
          "form": "한때 거기에 미쳤어요",
          "original_form": "한때 거기에 미쳤어요",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000921.1.14",
          "form": "누치님 덕에 새로운걸 많이 알아가네요",
          "original_form": "누치님 덕에 새로운걸 많이 알아가네요",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000921.1.15",
          "form": "힙합클럽이라..듣기만해도 힙한데요",
          "original_form": "힙합클럽이라..듣기만해도 힙한데요",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000921.1.16",
          "form": "영국 발음 특징이 거기에 쏙 들어가있어요",
          "original_form": "영국 발음 특징이 거기에 쏙 들어가있어요",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000921.1.17",
          "form": "쏜워너콩콩콕 궝웨너범범봄",
          "original_form": "쏜워너콩콩콕 궝웨너범범봄",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000921.1.18",
          "form": "매력적이죠",
          "original_form": "매력적이죠",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000921.1.19",
          "form": "약간 딱딱한듯 격조있는 발음이 매력적이에요",
          "original_form": "약간 딱딱한듯 격조있는 발음이 매력적이에요",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.20",
          "form": "오우터 발음 멋있어요 그래서 가보고싶네요",
          "original_form": "오우터 발음 멋있어요 그래서 가보고싶네요",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.21",
          "form": "미국과는 다른 그 느낌",
          "original_form": "미국과는 다른 그 느낌",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.22",
          "form": "작년엔 너무나 가보고싶어서",
          "original_form": "작년엔 너무나 가보고싶어서",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.23",
          "form": "너무 좋아요ㅎㅎ",
          "original_form": "너무 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.24",
          "form": "영국인에게 인종차별 당하는 꿈까지 꿨어요",
          "original_form": "영국인에게 인종차별 당하는 꿈까지 꿨어요",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.25",
          "form": "근데 좋았어요",
          "original_form": "근데 좋았어요",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.26",
          "form": "ㅋㅋㅋ 인종차별 당하는 상상하지말아요 우리",
          "original_form": "ㅋㅋㅋ 인종차별 당하는 상상하지말아요 우리",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000921.1.27",
          "form": "생각만으로도 슬퍼요",
          "original_form": "생각만으로도 슬퍼요",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000921.1.28",
          "form": "영국 되게 매력있는거같아요",
          "original_form": "영국 되게 매력있는거같아요",
