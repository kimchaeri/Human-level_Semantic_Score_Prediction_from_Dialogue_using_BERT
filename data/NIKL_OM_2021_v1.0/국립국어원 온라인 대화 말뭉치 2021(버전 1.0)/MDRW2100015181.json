      "utterance": [
        {
          "id": "MDRW2100015181.1.1",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015181.1.2",
          "form": "아뇨 연애 쉬지않고 해오다가 근1년넘게 안하고있습니다",
          "original_form": "아뇨 연애 쉬지않고 해오다가 근1년넘게 안하고있습니다",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015181.1.3",
          "form": "name2님은 어떠신가요",
          "original_form": "&name2&님은 어떠신가요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015181.1.4",
          "form": "전 연애중이에요",
          "original_form": "전 연애중이에요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015181.1.5",
          "form": "일부러 쉬시는거에요?",
          "original_form": "일부러 쉬시는거에요?",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015181.1.6",
          "form": "ㅋㅋ아뇨 안하는 게 아니라 못....",
          "original_form": "ㅋㅋ아뇨 안하는 게 아니라 못....",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015181.1.7",
          "form": "근데 선배들이 같이 밥먹고나 술먹자고 해도 안나가긴 합니다ㅎ",
          "original_form": "근데 선배들이 같이 밥먹고나 술먹자고 해도 안나가긴 합니다ㅎ",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015181.1.8",
          "form": "그냥 직장에 또래 이성도 없고 마음에 드는 사람이 딱히 있는 것도 아니라",
          "original_form": "그냥 직장에 또래 이성도 없고 마음에 드는 사람이 딱히 있는 것도 아니라",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.9",
          "form": "계속 연애를 안하게된 것 같아요",
          "original_form": "계속 연애를 안하게된 것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.10",
          "form": "소개팅은요?",
          "original_form": "소개팅은요?",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.11",
          "form": "사귄지는 얼마나 되셨나요?",
          "original_form": "사귄지는 얼마나 되셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.12",
          "form": "아...소개팅은 해본적이 없어서 ㅋㅋ",
          "original_form": "아...소개팅은 해본적이 없어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.13",
          "form": "헛 그러시구나",
          "original_form": "헛 그러시구나",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.14",
          "form": "제가 말수가 굉장히 없는 편이라서요",
          "original_form": "제가 말수가 굉장히 없는 편이라서요",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015181.1.15",
          "form": "전 5년째 만나고있어요",
          "original_form": "전 5년째 만나고있어요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015181.1.16",
          "form": "나가게 되면 호평은 못받을 것 같아요 ㅋㅋ",
          "original_form": "나가게 되면 호평은 못받을 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015181.1.17",
          "form": "내성적이시군요",
          "original_form": "내성적이시군요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015181.1.18",
          "form": "네 엄청 조용한 것 같아요 특히 모르는 사람 앞에서는ㅋㅋ",
          "original_form": "네 엄청 조용한 것 같아요 특히 모르는 사람 앞에서는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015181.1.19",
          "form": "와 5년째면",
          "original_form": "와 5년째면",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015181.1.20",
          "form": "정말 인연이네요 전 2년이 최고였어요",
          "original_form": "정말 인연이네요 전 2년이 최고였어요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015181.1.21",
          "form": "길게 만나셨네요",
          "original_form": "길게 만나셨네요",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015181.1.22",
          "form": "외롭진 않으세요?",
          "original_form": "외롭진 않으세요?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015181.1.23",
          "form": "외로운 건 잘 모르겠는데 전 친구가 정말 없는 편이라",
          "original_form": "외로운 건 잘 모르겠는데 전 친구가 정말 없는 편이라",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015181.1.24",
          "form": "애인이 있으면 애인이랑 했을 여러 활동들을 못하는 게 아쉽더라고요",
          "original_form": "애인이 있으면 애인이랑 했을 여러 활동들을 못하는 게 아쉽더라고요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015181.1.25",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015181.1.26",
          "form": "게다가 요즘 코로나라",
          "original_form": "게다가 요즘 코로나라",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015181.1.27",
          "form": "친구한텐 이거하자 저거하자 내 취향에 맞춰달라고하기가 어려워서요ㅋㅋ",
          "original_form": "친구한텐 이거하자 저거하자 내 취향에 맞춰달라고하기가 어려워서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015181.1.28",
          "form": "제약이 많아서",
          "original_form": "제약이 많아서",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015181.1.29",
          "form": "맞아요ㅋㅋㅠㅠ 뭘 새로하기도 힘들더라고요 특히 마스크를 계속 쓰고 있어야해서 요즘은 새 사람들 만나기도 꺼려지고요",
          "original_form": "맞아요ㅋㅋㅠㅠ 뭘 새로하기도 힘들더라고요 특히 마스크를 계속 쓰고 있어야해서 요즘은 새 사람들 만나기도 꺼려지고요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015181.1.30",
          "form": "저희 지역은 다음에 코로나 4단계 격상된다고 해서 특히나 그래요 ㄷㄷ",
          "original_form": "저희 지역은 다음에 코로나 4단계 격상된다고 해서 특히나 그래요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015181.1.31",
          "form": "저흰 이미 4단계에 9시까지만 영업하고",
          "original_form": "저흰 이미 4단계에 9시까지만 영업하고",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015181.1.32",
          "form": "너무 갑갑해요 코로나땜시",
          "original_form": "너무 갑갑해요 코로나땜시",
          "speaker_id": "2",
          "time": "20210825 23:34"
        }
      ]
    }
