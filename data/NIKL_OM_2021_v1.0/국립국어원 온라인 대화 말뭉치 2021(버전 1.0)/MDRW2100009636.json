      "utterance": [
        {
          "id": "MDRW2100009636.1.1",
          "form": "전문적인 거 배우는 학생들이 부러워져요",
          "original_form": "전문적인 거 배우는 학생들이 부러워져요",
          "speaker_id": "2",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009636.1.2",
          "form": "특히 1학년은",
          "original_form": "특히 1학년은",
          "speaker_id": "2",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009636.1.3",
          "form": "교양만 배우거든요",
          "original_form": "교양만 배우거든요",
          "speaker_id": "2",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009636.1.4",
          "form": "교양 지루.",
          "original_form": "교양 지루.",
          "speaker_id": "1",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009636.1.5",
          "form": "너무 지루..",
          "original_form": "너무 지루..",
          "speaker_id": "1",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009636.1.6",
          "form": "그나마 2학년 3학년 와서 교육학 배우고 실습 가서",
          "original_form": "그나마 2학년 3학년 와서 교육학 배우고 실습 가서",
          "speaker_id": "2",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009636.1.7",
          "form": "교대생이구나 하고",
          "original_form": "교대생이구나 하고",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009636.1.8",
          "form": "1학년 때는",
          "original_form": "1학년 때는",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009636.1.9",
          "form": "배구하고 피아노 치고 수학 놀이 하고",
          "original_form": "배구하고 피아노 치고 수학 놀이 하고",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009636.1.10",
          "form": "장구 치고 먹 갈고",
          "original_form": "장구 치고 먹 갈고",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009636.1.11",
          "form": "근데 생각보다 나쁘지잃은거 같은데요",
          "original_form": "근데 생각보다 나쁘지잃은거 같은데요",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009636.1.12",
          "form": "초파리 관찰하고",
          "original_form": "초파리 관찰하고",
          "speaker_id": "2",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009636.1.13",
          "form": "아 근데",
          "original_form": "아 근데",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.14",
          "form": "생각보다 빡세요",
          "original_form": "생각보다 빡세요",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.15",
          "form": "과제가 많나요",
          "original_form": "과제가 많나요",
          "speaker_id": "1",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.16",
          "form": "가르쳐야하는 입장이니까",
          "original_form": "가르쳐야하는 입장이니까",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.17",
          "form": "진짜 빡세게 가르치세요",
          "original_form": "진짜 빡세게 가르치세요",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.18",
          "form": "지금은 진짜 많은데",
          "original_form": "지금은 진짜 많은데",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.19",
          "form": "1학년 때는 할만했어요",
          "original_form": "1학년 때는 할만했어요",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.20",
          "form": "그리고 1학년은 교양 뿐이라",
          "original_form": "그리고 1학년은 교양 뿐이라",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.21",
          "form": "성적이 덜 중요해서",
          "original_form": "성적이 덜 중요해서",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009636.1.22",
          "form": "다들 놀거든요",
          "original_form": "다들 놀거든요",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.23",
          "form": "누군가를 가르친다는건 어떤 느낌이에요",
          "original_form": "누군가를 가르친다는건 어떤 느낌이에요",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.24",
          "form": "과제도 죄다 대충 했어요",
          "original_form": "과제도 죄다 대충 했어요",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.27",
          "form": "음 별 거 없어요",
          "original_form": "음 별 거 없어요",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.28",
          "form": "과제는 너무 많으니까",
          "original_form": "과제는 너무 많으니까",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.29",
          "form": "근데 확실히",
          "original_form": "근데 확실히",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.30",
          "form": "사람들이 생각하는 것보다",
          "original_form": "사람들이 생각하는 것보다",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.31",
          "form": "전문적이어야 하는 게",
          "original_form": "전문적이어야 하는 게",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.32",
          "form": "교과서만 들고",
          "original_form": "교과서만 들고",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009636.1.33",
          "form": "내용만 알려준다고 끝이 아니더라고요",
          "original_form": "내용만 알려준다고 끝이 아니더라고요",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.34",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.35",
          "form": "특히 어린애들을 가르치는건",
          "original_form": "특히 어린애들을 가르치는건",
          "speaker_id": "1",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.36",
          "form": "그 아이들의 사희성에도 영향을 주니까",
          "original_form": "그 아이들의 사희성에도 영향을 주니까",
          "speaker_id": "1",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.37",
          "form": "가장 어려운 건",
          "original_form": "가장 어려운 건",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.38",
          "form": "관계인 것 같아요",
          "original_form": "관계인 것 같아요",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.39",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.40",
          "form": "학생들이랑 맺는 관계 학부모랑 맺는 관계",
          "original_form": "학생들이랑 맺는 관계 학부모랑 맺는 관계",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.41",
          "form": "학생들끼리 맺는 관계도",
          "original_form": "학생들끼리 맺는 관계도",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009636.1.42",
          "form": "교사가 큰 영향을 미쳐서요",
          "original_form": "교사가 큰 영향을 미쳐서요",
          "speaker_id": "2",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009636.1.43",
          "form": "대학교 때 학교 열심히 다니셨어요?",
          "original_form": "대학교 때 학교 열심히 다니셨어요?",
          "speaker_id": "2",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009636.1.44",
          "form": "저는..",
          "original_form": "저는..",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009636.1.45",
          "form": "일단 의무감에 열심히 나가기는 했어요",
          "original_form": "일단 의무감에 열심히 나가기는 했어요",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009636.1.46",
          "form": "뭔지알죠",
          "original_form": "뭔지알죠",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009636.1.47",
          "form": "학교 나가기는 싫은데 그래도 온거니",
          "original_form": "학교 나가기는 싫은데 그래도 온거니",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009636.1.48",
          "form": "일단은 나가자",
          "original_form": "일단은 나가자",
          "speaker_id": "1",
          "time": "20210823 02:07"
        }
      ]
    }
