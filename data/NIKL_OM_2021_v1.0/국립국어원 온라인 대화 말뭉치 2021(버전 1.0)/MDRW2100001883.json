      "utterance": [
        {
          "id": "MDRW2100001883.1.1",
          "form": "요즘 취업하기 힘든가봐요",
          "original_form": "요즘 취업하기 힘든가봐요",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001883.1.2",
          "form": "그러게요.  실업률이 최고인가봐요",
          "original_form": "그러게요.  실업률이 최고인가봐요",
          "speaker_id": "2",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001883.1.3",
          "form": "코로나땜시 경기가 안돌아가니..",
          "original_form": "코로나땜시 경기가 안돌아가니..",
          "speaker_id": "2",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001883.1.4",
          "form": "아는동생  좋은회사 관두더니 1년반째 놀고있어서 걱정이예요",
          "original_form": "아는동생  좋은회사 관두더니 1년반째 놀고있어서 걱정이예요",
          "speaker_id": "1",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001883.1.5",
          "form": "이직할람 더 좋은곳으로 가야하니...낮춰서가긴싫고~",
          "original_form": "이직할람 더 좋은곳으로 가야하니...낮춰서가긴싫고~",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001883.1.6",
          "form": "그렇다고 힘든일은 더 하기싫고~~",
          "original_form": "그렇다고 힘든일은 더 하기싫고~~",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001883.1.7",
          "form": "제가 뜯어말렸는데 취업잘될줄 알았나봐요",
          "original_form": "제가 뜯어말렸는데 취업잘될줄 알았나봐요",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001883.1.8",
          "form": "그러더니 어디 한군데 취업했다 몇달안돼서 또사표썼어요ㅡㅡ",
          "original_form": "그러더니 어디 한군데 취업했다 몇달안돼서 또사표썼어요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001883.1.9",
          "form": "에고ㅠ 요즘 같을때 너무 성급했네요",
          "original_form": "에고ㅠ 요즘 같을때 너무 성급했네요",
          "speaker_id": "2",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001883.1.10",
          "form": "그것도 말렸는데 결국 사표를 쓰고 또 놀고있네요",
          "original_form": "그것도 말렸는데 결국 사표를 쓰고 또 놀고있네요",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001883.1.11",
          "form": "얼렁 잘되야할텐데요. 우선은 지긋지긋한 코로나먼저 끝나야 가능할꺼같아요",
          "original_form": "얼렁 잘되야할텐데요. 우선은 지긋지긋한 코로나먼저 끝나야 가능할꺼같아요",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001883.1.12",
          "form": "그니까요 나이도 있고 요즘 안그래도 힘든데 더힘들겠더라구요",
          "original_form": "그니까요 나이도 있고 요즘 안그래도 힘든데 더힘들겠더라구요",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001883.1.13",
          "form": "그래도 알바는 잘구하던데요ㅋ",
          "original_form": "그래도 알바는 잘구하던데요ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:11"
        },
        {
          "id": "MDRW2100001883.1.14",
          "form": "알바는 많더라구요. 울큰애도 고르고골라서 쿠팡~~",
          "original_form": "알바는 많더라구요. 울큰애도 고르고골라서 쿠팡~~",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001883.1.15",
          "form": "얼마전 통계청알바하는데 일당이 쎄더라구요",
          "original_form": "얼마전 통계청알바하는데 일당이 쎄더라구요",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001883.1.16",
          "form": "몇백벌었던데요ㅋ",
          "original_form": "몇백벌었던데요ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001883.1.17",
          "form": "인구조사? 그런거 수당도많데요",
          "original_form": "인구조사? 그런거 수당도많데요",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001883.1.18",
          "form": "그런쪽 하면 좋긴하겠더라구요",
          "original_form": "그런쪽 하면 좋긴하겠더라구요",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001883.1.19",
          "form": "네그런거같아요 업체방문해서 서류받고 그런거",
          "original_form": "네그런거같아요 업체방문해서 서류받고 그런거",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001883.1.20",
          "form": "ㅎ 나라에서하는게 떼일일도없고 수당도 받고 좋은거같아요",
          "original_form": "ㅎ 나라에서하는게 떼일일도없고 수당도 받고 좋은거같아요",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001883.1.21",
          "form": "어디서 정부알바 잘찾아서 하더라구요ㅎ",
          "original_form": "어디서 정부알바 잘찾아서 하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210819 22:17"
        }
      ]
    }
