      "utterance": [
        {
          "id": "MDRW2100043633.1.1",
          "form": "그러고보니 곧이네",
          "original_form": "그러고보니 곧이네",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.2",
          "form": "웅 ㅋ 한달밖에 안남았다",
          "original_form": "웅 ㅋ 한달밖에 안남았다",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.3",
          "form": "그러게 뭐 하지",
          "original_form": "그러게 뭐 하지",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.4",
          "form": "딸램이가 호캉스 보내줄까",
          "original_form": "딸램이가 호캉스 보내줄까",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.5",
          "form": "호캉스 좋지",
          "original_form": "호캉스 좋지",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.6",
          "form": "ㅋㅋㅋ 그럼 알아서 호캉스 골라줄게",
          "original_form": "ㅋㅋㅋ 그럼 알아서 호캉스 골라줄게",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.7",
          "form": "고마워",
          "original_form": "고마워",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.8",
          "form": "ㅋㅋ 엄마아빠는 뭐하면서 놀지만 생각해둬유",
          "original_form": "ㅋㅋ 엄마아빠는 뭐하면서 놀지만 생각해둬유",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.9",
          "form": "그래 호캉스 조식뷔페 있는 걸로",
          "original_form": "그래 호캉스 조식뷔페 있는 걸로",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.10",
          "form": "알았다 ㅋㅋ 뷔페는 포함해야지!",
          "original_form": "알았다 ㅋㅋ 뷔페는 포함해야지!",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.11",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043633.1.12",
          "form": "ㅋㅋ 근데 분위기있는데로 가야할텐데",
          "original_form": "ㅋㅋ 근데 분위기있는데로 가야할텐데",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043633.1.13",
          "form": "됐다 너무 비싼 데는 치워라",
          "original_form": "됐다 너무 비싼 데는 치워라",
