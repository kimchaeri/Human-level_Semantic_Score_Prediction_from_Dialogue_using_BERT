      "utterance": [
        {
          "id": "MDRW2100018867.1.1",
          "form": "좋아하는 음식있나요?",
          "original_form": "좋아하는 음식있나요?",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018867.1.2",
          "form": "저는 가리는건 없는데 육류좋아합니다",
          "original_form": "저는 가리는건 없는데 육류좋아합니다",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018867.1.3",
          "form": "음.. 삼겹살? 좋아해요",
          "original_form": "음.. 삼겹살? 좋아해요",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018867.1.4",
          "form": "name2님은 뭐 좋아하시나요",
          "original_form": "&name2&님은 뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018867.1.5",
          "form": "저도 고기좋아하고 해산물류도 좋아하구",
          "original_form": "저도 고기좋아하고 해산물류도 좋아하구",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018867.1.6",
          "form": "파스타도 좋아해여",
          "original_form": "파스타도 좋아해여",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018867.1.7",
          "form": "어떤 파스타 좋아하시나요",
          "original_form": "어떤 파스타 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018867.1.8",
          "form": "오일파스타요 ㅎㅎ 님은?",
          "original_form": "오일파스타요 ㅎㅎ 님은?",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018867.1.9",
          "form": "저는 새우들어간거.. 그..",
          "original_form": "저는 새우들어간거.. 그..",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018867.1.10",
          "form": "아..알리오 올리오 좋아했었는데 새우알러지가 생겨서",
          "original_form": "아..알리오 올리오 좋아했었는데 새우알러지가 생겨서",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018867.1.11",
          "form": "아.. 갑자기 생기샸어용?",
          "original_form": "아.. 갑자기 생기샸어용?",
          "speaker_id": "2",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018867.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018867.1.13",
          "form": "원래 갑갑류 잘 먹었거든요",
          "original_form": "원래 갑갑류 잘 먹었거든요",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018867.1.14",
          "form": "게장 이런거 되게 좋아했는데 갑자기 생기더라구요",
          "original_form": "게장 이런거 되게 좋아했는데 갑자기 생기더라구요",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018867.1.15",
          "form": "저도 엄청좋아하는데 ...",
          "original_form": "저도 엄청좋아하는데 ...",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018867.1.16",
          "form": "무섭네용 ㅠㅠ 계속 먹어야하는데 ㅠㅋㅋ",
          "original_form": "무섭네용 ㅠㅠ 계속 먹어야하는데 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018867.1.17",
          "form": "먹을 수 있을 때",
          "original_form": "먹을 수 있을 때",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018867.1.18",
          "form": "마음껏 드세요",
          "original_form": "마음껏 드세요",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018867.1.19",
          "form": "저도 알고싶지 않았습니다",
          "original_form": "저도 알고싶지 않았습니다",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018867.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아쉽겠어요 ㅠㅠ진짜",
