      "utterance": [
        {
          "id": "MDRW2100004089.1.1",
          "form": "글쎄 찾아봐야하는데",
          "original_form": "글쎄 찾아봐야하는데",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004089.1.2",
          "form": "그럼 내꺼만 산다",
          "original_form": "그럼 내꺼만 산다",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004089.1.3",
          "form": "왜? 그러니",
          "original_form": "왜? 그러니",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004089.1.4",
          "form": "적립금이 있어서 이걸로 쇼핑할려고 하는데",
          "original_form": "적립금이 있어서 이걸로 쇼핑할려고 하는데",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004089.1.5",
          "form": "그럼 세제",
          "original_form": "그럼 세제",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004089.1.6",
          "form": "세제? 무슨세제를 말하는 건데",
          "original_form": "세제? 무슨세제를 말하는 건데",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004089.1.7",
          "form": "너희 아빠가 세탁세제가 없다든데 유연제랑",
          "original_form": "너희 아빠가 세탁세제가 없다든데 유연제랑",
          "speaker_id": "2",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004089.1.8",
          "form": "그게 없다고 ? 다른건 없고 ?",
          "original_form": "그게 없다고 ? 다른건 없고 ?",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004089.1.9",
          "form": "응 지금 생각나는건 그거야",
          "original_form": "응 지금 생각나는건 그거야",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004089.1.10",
          "form": "알았어 그거 검색해볼게",
          "original_form": "알았어 그거 검색해볼게",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004089.1.11",
          "form": "응 사지 말라고 이야기 해둬야 겟다 너가 사준다고",
          "original_form": "응 사지 말라고 이야기 해둬야 겟다 너가 사준다고",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004089.1.12",
          "form": "어 이야기 해둬",
          "original_form": "어 이야기 해둬",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004089.1.13",
          "form": "잠시후에 뭐 샀는지 알려줄게",
          "original_form": "잠시후에 뭐 샀는지 알려줄게",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004089.1.14",
          "form": "그래 알았다",
          "original_form": "그래 알았다",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004089.1.15",
          "form": "저번에 산지 얼마 안된거 같은데 금방 떨어지네",
          "original_form": "저번에 산지 얼마 안된거 같은데 금방 떨어지네",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004089.1.16",
          "form": "너희 아빠에게물어봐",
          "original_form": "너희 아빠에게물어봐",
