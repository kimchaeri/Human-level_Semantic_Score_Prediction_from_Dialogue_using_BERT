      "utterance": [
        {
          "id": "MDRW2100004892.1.1",
          "form": "야근하는데 2만원이라니 안좋네요.",
          "original_form": "야근하는데 2만원이라니 안좋네요.",
          "speaker_id": "1",
          "time": "20210821 19:08"
        },
        {
          "id": "MDRW2100004892.1.2",
          "form": "중소기업이라 그런가봐요 ㅡㅎ",
          "original_form": "중소기업이라 그런가봐요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004892.1.3",
          "form": "월급은 좋은데 이런 짜잘짜잘한거는 아끼더라구요ㅎ",
          "original_form": "월급은 좋은데 이런 짜잘짜잘한거는 아끼더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004892.1.4",
          "form": "월급이라도 좋으니 다행이넹요",
          "original_form": "월급이라도 좋으니 다행이넹요",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004892.1.5",
          "form": "저도 중소기업인데 야근이 많아서 팀을 바꾸니 그뒤로 야근을 안해요",
          "original_form": "저도 중소기업인데 야근이 많아서 팀을 바꾸니 그뒤로 야근을 안해요",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004892.1.6",
          "form": "우와 팀을 바꾸시네요ㅎ",
          "original_form": "우와 팀을 바꾸시네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004892.1.7",
          "form": "저희는 안되요ㅠㅠ자격증도 있어야 할 뿐만아니라",
          "original_form": "저희는 안되요ㅠㅠ자격증도 있어야 할 뿐만아니라",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004892.1.8",
          "form": "인원이 많지 않아서ㅠ",
          "original_form": "인원이 많지 않아서ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004892.1.9",
          "form": "부럽습니다ㅠ",
          "original_form": "부럽습니다ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004892.1.10",
          "form": "저는 관리직 직원이 육아휴직 들어간다고 해서",
          "original_form": "저는 관리직 직원이 육아휴직 들어간다고 해서",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004892.1.11",
          "form": "관리팀으로 넘어갔어요... 전공버렸죠",
          "original_form": "관리팀으로 넘어갔어요... 전공버렸죠",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004892.1.12",
          "form": "에고ㅠ 적응하기 힘드셨겟다ㅠ",
          "original_form": "에고ㅠ 적응하기 힘드셨겟다ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004892.1.13",
          "form": "전공이있는데 관리쪽으로 가시면 새로다 셋팅하고 배워야하는데",
          "original_form": "전공이있는데 관리쪽으로 가시면 새로다 셋팅하고 배워야하는데",
          "speaker_id": "2",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004892.1.14",
          "form": "관리쪽은 자금 관련된거에요? 아니면 인사? 세무?",
          "original_form": "관리쪽은 자금 관련된거에요? 아니면 인사? 세무?",
          "speaker_id": "2",
          "time": "20210821 19:14"
        },
        {
          "id": "MDRW2100004892.1.15",
          "form": "작은 회사라 다해요 ㅋ 근데 돈관련은 상무님이 따로 있는데 그분이 해서 전 편해졌어요",
          "original_form": "작은 회사라 다해요 ㅋ 근데 돈관련은 상무님이 따로 있는데 그분이 해서 전 편해졌어요",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004892.1.16",
          "form": "옮긴지도 벌써 3년되가고 적응은 끝났죠 뭐",
          "original_form": "옮긴지도 벌써 3년되가고 적응은 끝났죠 뭐",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004892.1.17",
          "form": "작은회사일수록 힘들어요ㅠㅠ",
          "original_form": "작은회사일수록 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004892.1.18",
          "form": "그래도 다행이네요",
          "original_form": "그래도 다행이네요",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004892.1.19",
          "form": "중소기업의 최대 장점이죠ㅎ 뭐든 할수있다는거ㅎ",
          "original_form": "중소기업의 최대 장점이죠ㅎ 뭐든 할수있다는거ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004892.1.20",
          "form": "네 옮겨서도 할수있다는게 다행이었어요 ㅋ",
          "original_form": "네 옮겨서도 할수있다는게 다행이었어요 ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004892.1.21",
          "form": "다른데 이직은 생각안하세요?",
          "original_form": "다른데 이직은 생각안하세요?",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004892.1.22",
          "form": "매일 전 해요ㅠㅠ",
          "original_form": "매일 전 해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004892.1.23",
          "form": "나이도 있는데다가",
          "original_form": "나이도 있는데다가",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004892.1.24",
          "form": "여기사 하는일이 관리 전부를 하는게 아니라서",
          "original_form": "여기사 하는일이 관리 전부를 하는게 아니라서",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004892.1.25",
          "form": "급여랑 그런거 생각하면 .. 애매해져요",
          "original_form": "급여랑 그런거 생각하면 .. 애매해져요",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004892.1.26",
          "form": "맞네요ㅠ 쉽게 이직을 못하는 나이대죠",
          "original_form": "맞네요ㅠ 쉽게 이직을 못하는 나이대죠",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004892.1.27",
          "form": "요즘 취직도 힘들도 그래서",
          "original_form": "요즘 취직도 힘들도 그래서",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004892.1.28",
          "form": "자격증은 많이 따놓으셨어요?",
          "original_form": "자격증은 많이 따놓으셨어요?",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004892.1.29",
          "form": "아니요 자격증이 없어요. 따긴 해야 하는데 생각만 하고 있고 실천이 안되네요",
          "original_form": "아니요 자격증이 없어요. 따긴 해야 하는데 생각만 하고 있고 실천이 안되네요",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004892.1.30",
          "form": "자격증은 따셔요ㅎ 언제 우리가 어떻게 될지 모르잖아요ㅎ",
          "original_form": "자격증은 따셔요ㅎ 언제 우리가 어떻게 될지 모르잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004892.1.31",
          "form": "요즘 코로나라 더더욱 그렇구요 ㅡㅎ",
          "original_form": "요즘 코로나라 더더욱 그렇구요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004892.1.32",
          "form": "다음? 시험이 언제 있나 보긴해야 겠네요.",
          "original_form": "다음? 시험이 언제 있나 보긴해야 겠네요.",
          "speaker_id": "1",
          "time": "20210821 19:48"
        }
      ]
