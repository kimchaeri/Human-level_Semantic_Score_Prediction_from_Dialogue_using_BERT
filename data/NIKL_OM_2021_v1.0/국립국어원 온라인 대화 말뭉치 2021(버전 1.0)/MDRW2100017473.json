      "utterance": [
        {
          "id": "MDRW2100017473.1.1",
          "form": "여행 가면 기념품 사는 편이에요?",
          "original_form": "여행 가면 기념품 사는 편이에요?",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017473.1.2",
          "form": "저는 기념품보다는 면세점에서 선물을 구매해요ㅋㅋ",
          "original_form": "저는 기념품보다는 면세점에서 선물을 구매해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017473.1.3",
          "form": "아 그렇구나 보통 해외여행 다니시나봐요?",
          "original_form": "아 그렇구나 보통 해외여행 다니시나봐요?",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017473.1.4",
          "form": "많이 가보진 못했어요ㅠ name2님은요??",
          "original_form": "많이 가보진 못했어요ㅠ &name2&님은요??",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017473.1.5",
          "form": "저는 보통 근거리 여행 많이가요~",
          "original_form": "저는 보통 근거리 여행 많이가요~",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017473.1.6",
          "form": "요즘엔 여행 더더욱 못가는 것 같아요..",
          "original_form": "요즘엔 여행 더더욱 못가는 것 같아요..",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017473.1.7",
          "form": "접종 후에도 좀 지켜보려구요ㅠ",
          "original_form": "접종 후에도 좀 지켜보려구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017473.1.8",
          "form": "백신 1차 맞으셨어요?",
          "original_form": "백신 1차 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017473.1.9",
          "form": "아직이요ㅠㅠ 9월 말에 맞아요ㅠㅠ",
          "original_form": "아직이요ㅠㅠ 9월 말에 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017473.1.10",
          "form": "허걱!",
          "original_form": "허걱!",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017473.1.11",
          "form": "백신 맞게 된다면 가장 가고 싶은 여행지는?",
          "original_form": "백신 맞게 된다면 가장 가고 싶은 여행지는?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017473.1.12",
          "form": "코타 다시 가고싶어요!",
          "original_form": "코타 다시 가고싶어요!",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017473.1.13",
          "form": "코타가 어디에요?",
          "original_form": "코타가 어디에요?",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017473.1.14",
          "form": "코타니발루..?",
          "original_form": "코타니발루..?",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017473.1.15",
          "form": "네네! 그 곳에 다시 가고 싶네요ㅠ",
          "original_form": "네네! 그 곳에 다시 가고 싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017473.1.16",
          "form": "인도네시아에 있는 것 맞죠?",
          "original_form": "인도네시아에 있는 것 맞죠?",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017473.1.17",
          "form": "어떤 점이 좋았어요?",
          "original_form": "어떤 점이 좋았어요?",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017473.1.18",
          "form": "말레이시아에 있어요! 바닷속 풍경이 정말 아름다웠어요",
          "original_form": "말레이시아에 있어요! 바닷속 풍경이 정말 아름다웠어요",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017473.1.19",
          "form": "바닷 속은 어떻게 봤어요? 스노쿨링?",
          "original_form": "바닷 속은 어떻게 봤어요? 스노쿨링?",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017473.1.20",
          "form": "네네, 스노쿨링으로 물 위를 떠다니면서 밑을 봣어요!",
          "original_form": "네네, 스노쿨링으로 물 위를 떠다니면서 밑을 봣어요!",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017473.1.21",
          "form": "와.. 거기 물이 엄청 맑죠?",
          "original_form": "와.. 거기 물이 엄청 맑죠?",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017473.1.22",
          "form": "동해처럼 엄청 맑고 청록빛으로 빛나는데 더 좋은 건 물 솓에 산호와 물고기가 매우 많다는 점이었어요!",
          "original_form": "동해처럼 엄청 맑고 청록빛으로 빛나는데 더 좋은 건 물 솓에 산호와 물고기가 매우 많다는 점이었어요!",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017473.1.23",
          "form": "와..여행경비는 얼마나 들었어요?",
          "original_form": "와..여행경비는 얼마나 들었어요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017473.1.24",
          "form": "인당 100-150정도 든 것 같은데 잘 기억은 안 나네요ㅠ",
          "original_form": "인당 100-150정도 든 것 같은데 잘 기억은 안 나네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:08"
        }
      ]
    }
