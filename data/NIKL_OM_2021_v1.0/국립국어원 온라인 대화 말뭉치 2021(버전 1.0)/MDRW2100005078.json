        },
        {
          "id": "MDRW2100005078.1.2",
          "form": "군대 근처도 간 적이 없어요...",
          "original_form": "군대 근처도 간 적이 없어요...",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005078.1.3",
          "form": "군대 갔다 오셨어요?",
          "original_form": "군대 갔다 오셨어요?",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005078.1.4",
          "form": "네 다녀왔습니답",
          "original_form": "네 다녀왔습니답",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005078.1.5",
          "form": "육군으로다가",
          "original_form": "육군으로다가",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005078.1.6",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005078.1.7",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005078.1.8",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005078.1.9",
          "form": "북진통일 한다 vs  안한다",
          "original_form": "북진통일 한다 vs  안한다",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005078.1.10",
          "form": "어... 직접적으로",
          "original_form": "어... 직접적으로",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005078.1.11",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005078.1.12",
          "form": "혹시 오킹 아세요?",
          "original_form": "혹시 오킹 아세요?",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005078.1.13",
          "form": "군대 썰 푸는 유튜버인데.. 아시나요?",
          "original_form": "군대 썰 푸는 유튜버인데.. 아시나요?",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005078.1.14",
          "form": "그분 잘은 모르는데 유명하신가요",
          "original_form": "그분 잘은 모르는데 유명하신가요",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005078.1.15",
          "form": "좀 유명해요!",
          "original_form": "좀 유명해요!",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005078.1.16",
          "form": "군대썰 들을 때 안 가봐서 잘 모르지만",
          "original_form": "군대썰 들을 때 안 가봐서 잘 모르지만",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005078.1.17",
          "form": "그래도 재밌더라구요!",
          "original_form": "그래도 재밌더라구요!",
          "speaker_id": "2",
