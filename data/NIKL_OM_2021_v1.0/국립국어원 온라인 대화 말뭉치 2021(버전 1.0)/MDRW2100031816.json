      "utterance": [
        {
          "id": "MDRW2100031816.1.1",
          "form": "군대....?",
          "original_form": "군대....?",
          "speaker_id": "1",
          "time": "20210905 01:20"
        },
        {
          "id": "MDRW2100031816.1.2",
          "form": "군인이신가요?",
          "original_form": "군인이신가요?",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.3",
          "form": "아뇨 여자,,,",
          "original_form": "아뇨 여자,,,",
          "speaker_id": "2",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.5",
          "form": "군인이신가요?ㅋㅋ",
          "original_form": "군인이신가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.6",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.7",
          "form": "아니 저 여자인 친구 중에도 직업군인인 친구가 있어서...",
          "original_form": "아니 저 여자인 친구 중에도 직업군인인 친구가 있어서...",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.8",
          "form": "요즘 여자들도 군대 꽤 많이 가더라고요",
          "original_form": "요즘 여자들도 군대 꽤 많이 가더라고요",
          "speaker_id": "2",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.9",
          "form": "어 저도 있어욬ㅋㅋㅋ",
          "original_form": "어 저도 있어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇네요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031816.1.11",
          "form": "제 친구는 해군 부사관으로 들어갔어욬ㅋㅋ",
          "original_form": "제 친구는 해군 부사관으로 들어갔어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031816.1.12",
          "form": "저도 친한 친구 중에도 한 명 있고 친구 동생 중에도 한 명 있어요",
          "original_form": "저도 친한 친구 중에도 한 명 있고 친구 동생 중에도 한 명 있어요",
          "speaker_id": "1",
          "time": "20210905 01:22"
        },
        {
          "id": "MDRW2100031816.1.13",
          "form": "친구는 공군 친구 동생은 육군!",
          "original_form": "친구는 공군 친구 동생은 육군!",
          "speaker_id": "1",
          "time": "20210905 01:23"
        },
        {
          "id": "MDRW2100031816.1.14",
          "form": "요즘 뉴스보면 군대에 안좋은 일이 많아서 연락해보니까 다른 사람들한테도 연락이 많이 온다더라고요",
          "original_form": "요즘 뉴스보면 군대에 안좋은 일이 많아서 연락해보니까 다른 사람들한테도 연락이 많이 온다더라고요",
          "speaker_id": "2",
          "time": "20210905 01:24"
        },
        {
          "id": "MDRW2100031816.1.15",
          "form": "주위 선임이나 후임 동기들이 정말 중요한거같아요",
          "original_form": "주위 선임이나 후임 동기들이 정말 중요한거같아요",
          "speaker_id": "2",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100031816.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100031816.1.17",
          "form": "너무 폐쇄적인 조직이라",
          "original_form": "너무 폐쇄적인 조직이라",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100031816.1.18",
          "form": "구조 자체를 어떻게 할 수도 없고 사람을 잘 만나야겠더라고요 ㅠ",
          "original_form": "구조 자체를 어떻게 할 수도 없고 사람을 잘 만나야겠더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 01:28"
        },
        {
          "id": "MDRW2100031816.1.19",
          "form": "맞아요 그리고 당연히 그런일은 애초에 없어야 하겠지만",
          "original_form": "맞아요 그리고 당연히 그런일은 애초에 없어야 하겠지만",
          "speaker_id": "2",
          "time": "20210905 01:33"
        },
        {
          "id": "MDRW2100031816.1.20",
          "form": "일이 생기더라도 철저하게 대처할 수 있는 방안도 부족한거 같더라고요",
          "original_form": "일이 생기더라도 철저하게 대처할 수 있는 방안도 부족한거 같더라고요",
          "speaker_id": "2",
          "time": "20210905 01:34"
        },
        {
          "id": "MDRW2100031816.1.21",
          "form": "진짜 그런 것 같아요...",
          "original_form": "진짜 그런 것 같아요...",
          "speaker_id": "1",
          "time": "20210905 01:36"
        },
        {
          "id": "MDRW2100031816.1.22",
          "form": "피해자 보호도 제대로 안 되고",
          "original_form": "피해자 보호도 제대로 안 되고",
          "speaker_id": "1",
          "time": "20210905 01:36"
        },
        {
          "id": "MDRW2100031816.1.23",
          "form": "이미 많이 늦긴했지만 지금이라도 빨리 방안이 생겨야할텐데,,",
          "original_form": "이미 많이 늦긴했지만 지금이라도 빨리 방안이 생겨야할텐데,,",
          "speaker_id": "2",
          "time": "20210905 01:38"
        },
        {
          "id": "MDRW2100031816.1.24",
          "form": "보통 가해자가 상급자니까 안 그래도 피해에 대처하기 힘들텐데 남초집단이라 피해자 시각에서 대처가 안 되는 것 같아요 대체로",
          "original_form": "보통 가해자가 상급자니까 안 그래도 피해에 대처하기 힘들텐데 남초집단이라 피해자 시각에서 대처가 안 되는 것 같아요 대체로",
          "speaker_id": "1",
          "time": "20210905 01:40"
        },
        {
          "id": "MDRW2100031816.1.25",
          "form": "맞아요 기사보니까 대수롭지 않게여기거나 그냥 넘어가라는 식으로 덮으려고 하더라고요",
          "original_form": "맞아요 기사보니까 대수롭지 않게여기거나 그냥 넘어가라는 식으로 덮으려고 하더라고요",
          "speaker_id": "2",
          "time": "20210905 01:44"
        },
        {
          "id": "MDRW2100031816.1.26",
          "form": "에효...진짜 문제예요 ㅠ",
          "original_form": "에효...진짜 문제예요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 02:48"
        }
      ]
    }
