      "utterance": [
        {
          "id": "MDRW2100011735.1.1",
          "form": "점심은 드셨나요",
          "original_form": "점심은 드셨나요",
          "speaker_id": "1",
          "time": "20210824 15:34"
        },
        {
          "id": "MDRW2100011735.1.2",
          "form": "12시에 먹었어요",
          "original_form": "12시에 먹었어요",
          "speaker_id": "2",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011735.1.3",
          "form": "바오밥님은 드셨어요?",
          "original_form": "바오밥님은 드셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:35"
        },
        {
          "id": "MDRW2100011735.1.4",
          "form": "네! 저는 학교에서 일해서 급식 먹는데",
          "original_form": "네! 저는 학교에서 일해서 급식 먹는데",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011735.1.5",
          "form": "오늘 장조림 나왔어요!",
          "original_form": "오늘 장조림 나왔어요!",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011735.1.6",
          "form": "장조림만 있어도 그날 급식은 끝났었는데ㅋㅋ",
          "original_form": "장조림만 있어도 그날 급식은 끝났었는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011735.1.7",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ 장조림에 소금김 나와서 후루룩 했어요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ 장조림에 소금김 나와서 후루룩 했어요",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011735.1.8",
          "form": "급식 좋아하던 메뉴 잇으신가요",
          "original_form": "급식 좋아하던 메뉴 잇으신가요",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011735.1.9",
          "form": "저는 돈까스요! 초중고 돈까스가 전부 같은 맛이어서 편하게 먹었어요",
          "original_form": "저는 돈까스요! 초중고 돈까스가 전부 같은 맛이어서 편하게 먹었어요",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011735.1.10",
          "form": "헉 돈까스 나오는 날에 무조건 스프 나오던거 생각나요",
          "original_form": "헉 돈까스 나오는 날에 무조건 스프 나오던거 생각나요",
          "speaker_id": "1",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011735.1.11",
          "form": "맞아요 그것도 꼭 수요일에만 나오고요ㅋㅋ",
          "original_form": "맞아요 그것도 꼭 수요일에만 나오고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100011735.1.12",
          "form": "맞아욬ㅋㅋㅋ 글고 꼭 깍두기가 같이나왓엇어요 저희는",
          "original_form": "맞아욬ㅋㅋㅋ 글고 꼭 깍두기가 같이나왓엇어요 저희는",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011735.1.13",
          "form": "저희는 김치만 매일 바뀌었던거 같아요",
          "original_form": "저희는 김치만 매일 바뀌었던거 같아요",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011735.1.14",
          "form": "그리고 급식하면 쫄면! 졸업하고 쫄면 한번도 못먹어봤어요ㅠ",
          "original_form": "그리고 급식하면 쫄면! 졸업하고 쫄면 한번도 못먹어봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011735.1.15",
          "form": "아 헐 진짜요??? 김밥천국이라도 가셔서 쫄면 수혈하세요",
          "original_form": "아 헐 진짜요??? 김밥천국이라도 가셔서 쫄면 수혈하세요",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011735.1.16",
          "form": "쫄면 짱맛있는데..",
          "original_form": "쫄면 짱맛있는데..",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011735.1.17",
          "form": "그럴까봐요 쫄면 나오면 급식 두그릇 먹기도 했었는데ㅠ",
          "original_form": "그럴까봐요 쫄면 나오면 급식 두그릇 먹기도 했었는데ㅠ",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011735.1.18",
          "form": "삶은달걀이랑 쫄면 ㅜㅜ",
          "original_form": "삶은달걀이랑 쫄면 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 18:29"
        },
        {
          "id": "MDRW2100011735.1.19",
          "form": "조만간 사먹고 말겠습니다",
          "original_form": "조만간 사먹고 말겠습니다",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100011735.1.20",
          "form": "아 저도 쫄면땡기네요",
          "original_form": "아 저도 쫄면땡기네요",
          "speaker_id": "1",
          "time": "20210824 21:30"
        }
      ]
