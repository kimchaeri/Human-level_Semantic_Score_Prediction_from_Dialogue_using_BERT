      "utterance": [
        {
          "id": "MDRW2100023479.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023479.1.2",
          "form": "안녕하세요! name1은 운동 좋아하시나요?",
          "original_form": "안녕하세요! &name1&은 운동 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023479.1.3",
          "form": "네 운동 좋아해요 name2도 운동 좋아하시나요?",
          "original_form": "네 운동 좋아해요 &name2&도 운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023479.1.4",
          "form": "전 배우고 싶은 건 많은데 실천력이 좀 부족한 것 같아요 ㅋㅋㅋ",
          "original_form": "전 배우고 싶은 건 많은데 실천력이 좀 부족한 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023479.1.5",
          "form": "주로 어떤 운동 하시나요?",
          "original_form": "주로 어떤 운동 하시나요?",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100023479.1.6",
          "form": "저는 근처에서 배울 수 있는건 다 배우는 것 같아요",
          "original_form": "저는 근처에서 배울 수 있는건 다 배우는 것 같아요",
          "speaker_id": "1",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100023479.1.7",
          "form": "와 대단하시네요!",
          "original_form": "와 대단하시네요!",
          "speaker_id": "2",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100023479.1.8",
          "form": "필라테스 수영 배드민턴 골프 등등",
          "original_form": "필라테스 수영 배드민턴 골프 등등",
          "speaker_id": "1",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100023479.1.9",
          "form": "시간이 되는한 다 배우려고해요",
          "original_form": "시간이 되는한 다 배우려고해요",
          "speaker_id": "1",
          "time": "20210830 08:45"
        },
        {
          "id": "MDRW2100023479.1.10",
          "form": "무엇이든 시작해보세요",
          "original_form": "무엇이든 시작해보세요",
          "speaker_id": "1",
          "time": "20210830 08:46"
        },
        {
          "id": "MDRW2100023479.1.11",
          "form": "그 중에 뭐가 제일 재밌으셨나요?",
          "original_form": "그 중에 뭐가 제일 재밌으셨나요?",
          "speaker_id": "2",
          "time": "20210830 08:47"
        },
        {
          "id": "MDRW2100023479.1.12",
          "form": "수영이 제일 재미있었어요",
          "original_form": "수영이 제일 재미있었어요",
          "speaker_id": "1",
          "time": "20210830 08:47"
        },
        {
          "id": "MDRW2100023479.1.13",
          "form": "저도 수영 배우고 싶어요!!",
          "original_form": "저도 수영 배우고 싶어요!!",
          "speaker_id": "2",
          "time": "20210830 08:47"
        },
        {
          "id": "MDRW2100023479.1.14",
          "form": "코로나때문에 요즘은 못하지만 물에 떠있는게 좋더라고요",
          "original_form": "코로나때문에 요즘은 못하지만 물에 떠있는게 좋더라고요",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023479.1.15",
          "form": "완전 맥주병이라서 ㅠㅠ ㅎㅎ",
          "original_form": "완전 맥주병이라서 ㅠㅠ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023479.1.16",
          "form": "저도 물 무서워했어요 극복하니 재미있어요",
          "original_form": "저도 물 무서워했어요 극복하니 재미있어요",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023479.1.17",
          "form": "한번 배워보세요ㅋㅋ 초보반가면 다들 못해요",
          "original_form": "한번 배워보세요ㅋㅋ 초보반가면 다들 못해요",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100023479.1.18",
          "form": "ㅎㅎ 근데 문을 열긴 하는데 코로나 때문에 꺼려져요 ㅠㅠ",
          "original_form": "ㅎㅎ 근데 문을 열긴 하는데 코로나 때문에 꺼려져요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100023479.1.19",
          "form": "맞아요 저도 코로나 시작되고부터는 수영은 안가게 되더라고요ㅜㅜ",
          "original_form": "맞아요 저도 코로나 시작되고부터는 수영은 안가게 되더라고요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100023479.1.20",
          "form": "그쵸? ㅠㅠ 코로나가 좀 잠잠해져야 배울 수 있을것같아요 ㅠㅠ",
          "original_form": "그쵸? ㅠㅠ 코로나가 좀 잠잠해져야 배울 수 있을것같아요 ㅠㅠ",
