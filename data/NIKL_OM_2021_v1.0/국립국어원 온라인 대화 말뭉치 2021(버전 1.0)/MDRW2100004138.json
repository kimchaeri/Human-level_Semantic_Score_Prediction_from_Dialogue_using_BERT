        },
        {
          "id": "MDRW2100004138.1.2",
          "form": "혹시 여행 좋아하세요~?",
          "original_form": "혹시 여행 좋아하세요~?",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004138.1.3",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004138.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004138.1.5",
          "form": "저도 여행 다니는거 좋아해요~",
          "original_form": "저도 여행 다니는거 좋아해요~",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004138.1.6",
          "form": "코로나 때문에 밖에 다닐 수가 없어서 아쉬워요 ㅠㅠ",
          "original_form": "코로나 때문에 밖에 다닐 수가 없어서 아쉬워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004138.1.7",
          "form": "그러니까요 ㅎ",
          "original_form": "그러니까요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004138.1.8",
          "form": "여행 어디가 좋으셨나요?",
          "original_form": "여행 어디가 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004138.1.9",
          "form": "저는 개인적으로 태국이요!",
          "original_form": "저는 개인적으로 태국이요!",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004138.1.10",
          "form": "1일 1마사지 너무 행복했습니다",
          "original_form": "1일 1마사지 너무 행복했습니다",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004138.1.11",
          "form": "우와 마사지 되게 시원할 거 같아요",
          "original_form": "우와 마사지 되게 시원할 거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004138.1.12",
          "form": "여행가서 힐링 제대로네요",
          "original_form": "여행가서 힐링 제대로네요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004138.1.13",
          "form": "최고예요 ㅠㅠㅠㅠ 태국 왕실 마사지가 진짜 찐이었어요",
          "original_form": "최고예요 ㅠㅠㅠㅠ 태국 왕실 마사지가 진짜 찐이었어요",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004138.1.14",
          "form": "와 저도 한번 받아보고 싶어요...",
          "original_form": "와 저도 한번 받아보고 싶어요...",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004138.1.15",
          "form": "왕실마사지라니..",
          "original_form": "왕실마사지라니..",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004138.1.16",
          "form": "약간 로컬이랑 다르게",
          "original_form": "약간 로컬이랑 다르게",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004138.1.17",
          "form": "쭈물쭈물 안 해줘서",
          "original_form": "쭈물쭈물 안 해줘서",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004138.1.18",
          "form": "아 이게 뭐지 돈 아깝다 하는데",
          "original_form": "아 이게 뭐지 돈 아깝다 하는데",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004138.1.19",
          "form": "다 받고 일어나면 몸이 가벼워요",
          "original_form": "다 받고 일어나면 몸이 가벼워요",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004138.1.20",
          "form": "혈자리를 눌린건지",
          "original_form": "혈자리를 눌린건지",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004138.1.21",
          "form": "와..... 전뮨가의 솜씨가 대단 할 거 같아요",
          "original_form": "와..... 전뮨가의 솜씨가 대단 할 거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004138.1.22",
          "form": "태국가서 받고싶네요",
          "original_form": "태국가서 받고싶네요",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004138.1.23",
          "form": "ㅠㅠㅠ name2님은 어디가 제일 좋으셨어요?",
          "original_form": "ㅠㅠㅠ &name2&님은 어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004138.1.24",
          "form": "전 해외여행을 일본만 가봤어요",
          "original_form": "전 해외여행을 일본만 가봤어요",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004138.1.25",
          "form": "어링 때 가서 그런지 좋았어요",
          "original_form": "어링 때 가서 그런지 좋았어요",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004138.1.26",
          "form": "일본 좋죠~",
          "original_form": "일본 좋죠~",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004138.1.27",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004138.1.28",
          "form": "저 오사카 가봤어요",
          "original_form": "저 오사카 가봤어요",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004138.1.29",
          "form": "저때 일본 가는게 유행아닌 유행이였거든요",
          "original_form": "저때 일본 가는게 유행아닌 유행이였거든요",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004138.1.30",
          "form": "헐 오사카 재밌으셨겠다",
          "original_form": "헐 오사카 재밌으셨겠다",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004138.1.31",
          "form": "오.. 그런 때가 있었나봐요",
          "original_form": "오.. 그런 때가 있었나봐요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004138.1.32",
          "form": "유니버셜 가보셨겠다",
          "original_form": "유니버셜 가보셨겠다",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004138.1.33",
          "form": "네 ㅋㅋ ㅋㅋㅋㅋㅋ 해리포터 옷 입고 아주",
          "original_form": "네 ㅋㅋ ㅋㅋㅋㅋㅋ 해리포터 옷 입고 아주",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004138.1.34",
          "form": "좋았어요 !!!!",
          "original_form": "좋았어요 !!!!",
          "speaker_id": "2",
          "time": "20210821 15:20"
        }
      ]
