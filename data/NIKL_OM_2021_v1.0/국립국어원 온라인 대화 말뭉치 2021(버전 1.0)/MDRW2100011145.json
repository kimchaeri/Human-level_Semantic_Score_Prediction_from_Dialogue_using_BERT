      "utterance": [
        {
          "id": "MDRW2100011145.1.1",
          "form": "혹시 대학생이야?",
          "original_form": "혹시 대학생이야?",
          "speaker_id": "1",
          "time": "20210824 01:44"
        },
        {
          "id": "MDRW2100011145.1.2",
          "form": "응응 너도?",
          "original_form": "응응 너도?",
          "speaker_id": "2",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011145.1.3",
          "form": "응응 난 3학년이야 너는?",
          "original_form": "응응 난 3학년이야 너는?",
          "speaker_id": "1",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011145.1.4",
          "form": "어 나는... 화석이야... ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 나는... 화석이야... ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011145.1.5",
          "form": "앜ㅋㅋㅋㅋ 그렇구나 혹시 무슨과야?",
          "original_form": "앜ㅋㅋㅋㅋ 그렇구나 혹시 무슨과야?",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011145.1.6",
          "form": "나는 인문대야!! 너는?!",
          "original_form": "나는 인문대야!! 너는?!",
          "speaker_id": "2",
          "time": "20210824 01:51"
        },
        {
          "id": "MDRW2100011145.1.7",
          "form": "나는 초등교육과야~",
          "original_form": "나는 초등교육과야~",
          "speaker_id": "1",
          "time": "20210824 01:51"
        },
        {
          "id": "MDRW2100011145.1.8",
          "form": "그렇구나!! 그럼 초등교과 다 배우는거야?!",
          "original_form": "그렇구나!! 그럼 초등교과 다 배우는거야?!",
          "speaker_id": "2",
          "time": "20210824 01:52"
        },
        {
          "id": "MDRW2100011145.1.9",
          "form": "음 그렇다고 볼 수 있지 ㅎㅎㅎ",
          "original_form": "음 그렇다고 볼 수 있지 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:53"
        },
        {
          "id": "MDRW2100011145.1.10",
          "form": "글쿠나 우리 학교에는 사범대가 없어서 신기하다~!!",
          "original_form": "글쿠나 우리 학교에는 사범대가 없어서 신기하다~!!",
          "speaker_id": "2",
          "time": "20210824 01:53"
        },
        {
          "id": "MDRW2100011145.1.11",
          "form": "수강신청으로 시간표 짜 아니면 정해져서 나와?",
          "original_form": "수강신청으로 시간표 짜 아니면 정해져서 나와?",
          "speaker_id": "2",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100011145.1.12",
          "form": "정해져서 나오는데~",
          "original_form": "정해져서 나오는데~",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100011145.1.13",
          "form": "몇개는 신청해!!",
          "original_form": "몇개는 신청해!!",
          "speaker_id": "1",
          "time": "20210824 01:55"
        },
        {
          "id": "MDRW2100011145.1.14",
          "form": "동아리 활동해?! 어떤 동아리 들었어?",
          "original_form": "동아리 활동해?! 어떤 동아리 들었어?",
          "speaker_id": "2",
          "time": "20210824 01:56"
        },
        {
          "id": "MDRW2100011145.1.15",
          "form": "나는 동아리 활동 공연동아리 1학년때 하고",
          "original_form": "나는 동아리 활동 공연동아리 1학년때 하고",
