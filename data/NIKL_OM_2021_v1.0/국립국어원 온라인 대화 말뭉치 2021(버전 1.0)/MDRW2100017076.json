      "utterance": [
        {
          "id": "MDRW2100017076.1.1",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "2",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017076.1.2",
          "form": "아니요 가족들과 같이 살아요",
          "original_form": "아니요 가족들과 같이 살아요",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017076.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017076.1.4",
          "form": "저는 혼자 살아요",
          "original_form": "저는 혼자 살아요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100017076.1.5",
          "form": "그럼 기숙사나 자취하신적 한번도 없으세요?",
          "original_form": "그럼 기숙사나 자취하신적 한번도 없으세요?",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100017076.1.6",
          "form": "우와~ 부러워요",
          "original_form": "우와~ 부러워요",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017076.1.7",
          "form": "네 통학에 통근에",
          "original_form": "네 통학에 통근에",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017076.1.8",
          "form": "해외연수때 정도?",
          "original_form": "해외연수때 정도?",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017076.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017076.1.10",
          "form": "전 나와산지가 너무 오래됏어욬ㅋㅋㅋㅋㅋ",
          "original_form": "전 나와산지가 너무 오래됏어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017076.1.11",
          "form": "만랩자취인이신거예요?",
          "original_form": "만랩자취인이신거예요?",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017076.1.12",
          "form": "전 자신이 없어서....",
          "original_form": "전 자신이 없어서....",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017076.1.13",
          "form": "지출도 많고",
          "original_form": "지출도 많고",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017076.1.14",
          "form": "전 집이 지방이라서 선택지가 없엇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 집이 지방이라서 선택지가 없엇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017076.1.15",
          "form": "그것도 기회인것 같아요",
          "original_form": "그것도 기회인것 같아요",
          "speaker_id": "1",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017076.1.16",
          "form": "장단점이 있는 것 같아요",
          "original_form": "장단점이 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017076.1.17",
          "form": "자취해서 여러모로 편한점도 잇고",
          "original_form": "자취해서 여러모로 편한점도 잇고",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017076.1.18",
          "form": "지출이 많은 것도 잇고",
          "original_form": "지출이 많은 것도 잇고",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017076.1.19",
          "form": "나만의 공간이 있잖아요",
          "original_form": "나만의 공간이 있잖아요",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017076.1.20",
          "form": "늦게들어와도 되고 먹고픈거 다먹고 자고싶은 만큼 자고",
          "original_form": "늦게들어와도 되고 먹고픈거 다먹고 자고싶은 만큼 자고",
          "speaker_id": "1",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017076.1.21",
          "form": "주말한정이지만...",
          "original_form": "주말한정이지만...",
          "speaker_id": "1",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017076.1.22",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 나만의 공간이 있다는게 큰 장점이죠",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 나만의 공간이 있다는게 큰 장점이죠",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017076.1.23",
          "form": "근데 그만큼 감당해야하는 것도 많아요",
          "original_form": "근데 그만큼 감당해야하는 것도 많아요",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017076.1.24",
          "form": "전 지금도 제 빨래는 스스로 하는데",
          "original_form": "전 지금도 제 빨래는 스스로 하는데",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017076.1.25",
          "form": "청소가 ㅜㅠ",
          "original_form": "청소가 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017076.1.26",
          "form": "뭐 빨래나 청소도 당연하고",
          "original_form": "뭐 빨래나 청소도 당연하고",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017076.1.27",
          "form": "끼니 때우는 것도 일이구요 ㅋㅋㅋ",
          "original_form": "끼니 때우는 것도 일이구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017076.1.28",
          "form": "끼니 라니",
          "original_form": "끼니 라니",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017076.1.29",
          "form": "하긴 그것도 문제예요",
          "original_form": "하긴 그것도 문제예요",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017076.1.30",
          "form": "저라면 맨날 배달아님 인스턴트일듯",
          "original_form": "저라면 맨날 배달아님 인스턴트일듯",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017076.1.31",
          "form": "몇년 그렇게 먹엇더니 몸이 너무 상하더라구요",
          "original_form": "몇년 그렇게 먹엇더니 몸이 너무 상하더라구요",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017076.1.32",
          "form": "그럼 직접 만들어 드세요?",
          "original_form": "그럼 직접 만들어 드세요?",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017076.1.33",
          "form": "대단쓰",
          "original_form": "대단쓰",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017076.1.34",
          "form": "아니요 간단하게 만들어먹거나 샐러드를 먹어요",
          "original_form": "아니요 간단하게 만들어먹거나 샐러드를 먹어요",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017076.1.35",
          "form": "샐러드라니",
          "original_form": "샐러드라니",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017076.1.36",
          "form": "그건 그냥 건강간식이죠?",
          "original_form": "그건 그냥 건강간식이죠?",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017076.1.37",
          "form": "위장이 엉망이 되면 살려고 건강하게 먹게 됩니다 ㅠㅠ",
          "original_form": "위장이 엉망이 되면 살려고 건강하게 먹게 됩니다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017076.1.38",
          "form": "저도 원래 배달음식 킬러였어요...",
          "original_form": "저도 원래 배달음식 킬러였어요...",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017076.1.39",
          "form": "살려고 먹는거군요",
          "original_form": "살려고 먹는거군요",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017076.1.40",
          "form": "큰 깨달음",
          "original_form": "큰 깨달음",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017076.1.41",
          "form": "네 어차피 낮에는 또 외식을 해야하고 저녁도 외식일 때도 있으니까",
          "original_form": "네 어차피 낮에는 또 외식을 해야하고 저녁도 외식일 때도 있으니까",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017076.1.42",
          "form": "집에 있을때라도 그나마 건강하게 먹어야 몸이 덜 상해요",
          "original_form": "집에 있을때라도 그나마 건강하게 먹어야 몸이 덜 상해요",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017076.1.43",
          "form": "그렇네요 날위해 유산균도 챙기고 ..ㅋㅋ",
          "original_form": "그렇네요 날위해 유산균도 챙기고 ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017076.1.44",
          "form": "혼자 살면 건강을 더 챙겨야하더라구요",
          "original_form": "혼자 살면 건강을 더 챙겨야하더라구요",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017076.1.45",
          "form": "아파도 응급실 혼자가야함ㅋㅋㅋㅋㅋ",
          "original_form": "아파도 응급실 혼자가야함ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017076.1.46",
          "form": "아~ 응급실",
          "original_form": "아~ 응급실",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017076.1.47",
          "form": "너무 슬퍼요",
          "original_form": "너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017076.1.48",
          "form": "네 안전문제도 더 신경써야 하고 ㅋㅋ",
          "original_form": "네 안전문제도 더 신경써야 하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100017076.1.49",
          "form": "요즘 세상이 흉흉해서",
          "original_form": "요즘 세상이 흉흉해서",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017076.1.50",
          "form": "문단속도 잘 하시구요",
          "original_form": "문단속도 잘 하시구요",
          "speaker_id": "1",
          "time": "20210826 19:28"
        }
      ]
