      "utterance": [
        {
          "id": "MDRW2100025274.1.1",
          "form": "취업하셨나요",
          "original_form": "취업하셨나요",
          "speaker_id": "1",
          "time": "20210901 06:31"
        },
        {
          "id": "MDRW2100025274.1.2",
          "form": "알바식으로 일하는 중인데",
          "original_form": "알바식으로 일하는 중인데",
          "speaker_id": "2",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025274.1.3",
          "form": "정식 계약은 아니라 취준중이예요!",
          "original_form": "정식 계약은 아니라 취준중이예요!",
          "speaker_id": "2",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025274.1.4",
          "form": "어떤 알바인지 여쭤봐도되나요",
          "original_form": "어떤 알바인지 여쭤봐도되나요",
          "speaker_id": "1",
          "time": "20210901 06:32"
        },
        {
          "id": "MDRW2100025274.1.5",
          "form": "영상쪽....?ㅎㅎ",
          "original_form": "영상쪽....?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 06:33"
        },
        {
          "id": "MDRW2100025274.1.6",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "2",
          "time": "20210901 06:33"
        },
        {
          "id": "MDRW2100025274.1.7",
          "form": "네 저는 직장다녀요",
          "original_form": "네 저는 직장다녀요",
          "speaker_id": "1",
          "time": "20210904 14:22"
        },
        {
          "id": "MDRW2100025274.1.8",
          "form": "회사생활 괜찮으세요?",
          "original_form": "회사생활 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100025274.1.9",
          "form": "네 할만해요 영상은 어떤 알바였나요?",
          "original_form": "네 할만해요 영상은 어떤 알바였나요?",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100025274.1.10",
          "form": "그냥 편집이요~",
          "original_form": "그냥 편집이요~",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100025274.1.11",
          "form": "취업하신지는",
          "original_form": "취업하신지는",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100025274.1.12",
          "form": "얼마나 되셨어요?",
          "original_form": "얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100025274.1.13",
          "form": "10년정도 됐네요",
          "original_form": "10년정도 됐네요",
          "speaker_id": "1",
          "time": "20210905 06:28"
        },
        {
          "id": "MDRW2100025274.1.14",
          "form": "오래다니셨네요~",
          "original_form": "오래다니셨네요~",
          "speaker_id": "2",
          "time": "20210906 09:22"
        },
        {
          "id": "MDRW2100025274.1.15",
          "form": "한 회사에요?",
          "original_form": "한 회사에요?",
          "speaker_id": "2",
          "time": "20210906 09:22"
        },
        {
          "id": "MDRW2100025274.1.16",
          "form": "네 같은 회사요",
          "original_form": "네 같은 회사요",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100025274.1.17",
          "form": "정말 대단하세요!!",
          "original_form": "정말 대단하세요!!",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100025274.1.18",
          "form": "그럄 직급도 높으시겠어요",
          "original_form": "그럄 직급도 높으시겠어요",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100025274.1.19",
          "form": "직급이 큰 의미가 없는지라...\n취준도 영상쪽으로 하시나요",
          "original_form": "직급이 큰 의미가 없는지라...\n취준도 영상쪽으로 하시나요",
          "speaker_id": "1",
          "time": "20210906 20:00"
        },
        {
          "id": "MDRW2100025274.1.20",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "2",
