      "utterance": [
        {
          "id": "MDRW2100035131.1.1",
          "form": "아침엔 제법 쌀쌀하네요",
          "original_form": "아침엔 제법 쌀쌀하네요",
          "speaker_id": "1",
          "time": "20210829 06:23"
        },
        {
          "id": "MDRW2100035131.1.2",
          "form": "그러게용ㅎㅎ",
          "original_form": "그러게용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 06:24"
        },
        {
          "id": "MDRW2100035131.1.3",
          "form": "선풍기 돌리면서 자다가 껐어용",
          "original_form": "선풍기 돌리면서 자다가 껐어용",
          "speaker_id": "2",
          "time": "20210829 06:24"
        },
        {
          "id": "MDRW2100035131.1.4",
          "form": "날은 개었나요?",
          "original_form": "날은 개었나요?",
          "speaker_id": "1",
          "time": "20210829 06:24"
        },
        {
          "id": "MDRW2100035131.1.5",
          "form": "저희 동네는 한시간전까지도 비오다가",
          "original_form": "저희 동네는 한시간전까지도 비오다가",
          "speaker_id": "2",
          "time": "20210829 06:24"
        },
        {
          "id": "MDRW2100035131.1.6",
          "form": "아직 흐리네요",
          "original_form": "아직 흐리네요",
          "speaker_id": "2",
          "time": "20210829 06:24"
        },
        {
          "id": "MDRW2100035131.1.7",
          "form": "name1님 동네는요?",
          "original_form": "&name1&님 동네는요?",
          "speaker_id": "2",
          "time": "20210829 06:24"
        },
        {
          "id": "MDRW2100035131.1.8",
          "form": "저도 눈떠보니 갠 것 같긴 한데 땅이 젖어있어요",
          "original_form": "저도 눈떠보니 갠 것 같긴 한데 땅이 젖어있어요",
          "speaker_id": "1",
          "time": "20210829 06:26"
        },
        {
          "id": "MDRW2100035131.1.9",
          "form": "흐리긴하지만 날이 선선해서 좋네요~",
          "original_form": "흐리긴하지만 날이 선선해서 좋네요~",
          "speaker_id": "2",
          "time": "20210829 06:27"
        },
        {
          "id": "MDRW2100035131.1.10",
          "form": "월요일부터는 계속 비온다네용",
          "original_form": "월요일부터는 계속 비온다네용",
          "speaker_id": "2",
          "time": "20210829 06:27"
        },
        {
          "id": "MDRW2100035131.1.11",
          "form": "네~^^ 가을 느낌 물씬이예요",
          "original_form": "네~^^ 가을 느낌 물씬이예요",
          "speaker_id": "1",
          "time": "20210829 06:27"
        },
        {
          "id": "MDRW2100035131.1.12",
          "form": "또요..?? 또르르...",
          "original_form": "또요..?? 또르르...",
          "speaker_id": "1",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035131.1.13",
          "form": "네네ㅠㅠ 그래서 오늘 밀린 빨래 전부 다하려구요ㅎㅎ",
          "original_form": "네네ㅠㅠ 그래서 오늘 밀린 빨래 전부 다하려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035131.1.14",
          "form": "일기예보 찾아보니 진짜 또 내내 비네요",
          "original_form": "일기예보 찾아보니 진짜 또 내내 비네요",
          "speaker_id": "1",
          "time": "20210829 06:29"
        },
        {
          "id": "MDRW2100035131.1.15",
          "form": "그쵸?ㅋㅋㅋ 월요일부터는 현관에 우산두고 다니셔요",
          "original_form": "그쵸?ㅋㅋㅋ 월요일부터는 현관에 우산두고 다니셔요",
          "speaker_id": "2",
          "time": "20210829 06:30"
        },
        {
          "id": "MDRW2100035131.1.16",
          "form": "비맞으면 괜히 서럽더라구요ㅜㅠ",
          "original_form": "비맞으면 괜히 서럽더라구요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 06:30"
        },
        {
          "id": "MDRW2100035131.1.17",
          "form": "맞아요 그리고 창문도 꼭꼭 잘 닫아야해요 비 들이치면 으악 ㅠ",
          "original_form": "맞아요 그리고 창문도 꼭꼭 잘 닫아야해요 비 들이치면 으악 ㅠ",
          "speaker_id": "1",
          "time": "20210829 06:31"
        },
        {
          "id": "MDRW2100035131.1.18",
          "form": "네... 빨래 널었는데 모르고 창문을 열어놔서ㅋㅋㅋㅋㅋ",
          "original_form": "네... 빨래 널었는데 모르고 창문을 열어놔서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035131.1.19",
          "form": "반만 마르고 반은 축축해요",
          "original_form": "반만 마르고 반은 축축해요",
          "speaker_id": "2",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035131.1.20",
          "form": "그럼 냄새나기도 하잖아요 비오면 빨래냄새..ㅠㅠ",
          "original_form": "그럼 냄새나기도 하잖아요 비오면 빨래냄새..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035131.1.21",
          "form": "오늘은 비가 엄청 와요",
