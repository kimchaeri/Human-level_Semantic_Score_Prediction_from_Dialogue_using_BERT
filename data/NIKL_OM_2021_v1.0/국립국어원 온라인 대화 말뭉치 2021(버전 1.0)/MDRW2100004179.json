      "utterance": [
        {
          "id": "MDRW2100004179.1.1",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004179.1.2",
          "form": "폭우주의보래",
          "original_form": "폭우주의보래",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004179.1.3",
          "form": "어제도 흐리긴했지",
          "original_form": "어제도 흐리긴했지",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004179.1.4",
          "form": "이러다 태풍오면",
          "original_form": "이러다 태풍오면",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004179.1.5",
          "form": "큰일이지",
          "original_form": "큰일이지",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004179.1.6",
          "form": "열돔이 막아준다더니 그것도 아닌듯",
          "original_form": "열돔이 막아준다더니 그것도 아닌듯",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.7",
          "form": "얼마전에 비내린이후로 시원해져서 그런거 아니가",
          "original_form": "얼마전에 비내린이후로 시원해져서 그런거 아니가",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.8",
          "form": "가을태풍이 올지도 몰라",
          "original_form": "가을태풍이 올지도 몰라",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.9",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.10",
          "form": "8월인데 예년에 비해서 많이 시원하지않나",
          "original_form": "8월인데 예년에 비해서 많이 시원하지않나",
          "speaker_id": "2",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.11",
          "form": "요즘 편의점갈때 우비입고 나감",
          "original_form": "요즘 편의점갈때 우비입고 나감",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.12",
          "form": "좀 추워",
          "original_form": "좀 추워",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004179.1.13",
          "form": "우비가 비닐이라서 보온도되는건가",
          "original_form": "우비가 비닐이라서 보온도되는건가",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004179.1.14",
          "form": "바람을 막아주지",
          "original_form": "바람을 막아주지",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004179.1.15",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004179.1.16",
          "form": "잠옷위에 바지랑 우비입고",
          "original_form": "잠옷위에 바지랑 우비입고",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004179.1.17",
          "form": "그대로 편의점 갔다오는거야",
          "original_form": "그대로 편의점 갔다오는거야",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004179.1.18",
          "form": "따뜻하고 간편하지",
          "original_form": "따뜻하고 간편하지",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004179.1.19",
          "form": "올해는 여름 더울거라고했던거같은데",
          "original_form": "올해는 여름 더울거라고했던거같은데",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004179.1.20",
          "form": "한국은 그렇게까지 덥진않았던거같다",
          "original_form": "한국은 그렇게까지 덥진않았던거같다",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004179.1.21",
          "form": "덥긴했는데",
          "original_form": "덥긴했는데",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004179.1.22",
          "form": "바로 시원해졌어",
          "original_form": "바로 시원해졌어",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004179.1.23",
          "form": "미국은 기온엄청올라가서 양식조개들이 익었다던데",
          "original_form": "미국은 기온엄청올라가서 양식조개들이 익었다던데",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.24",
          "form": "진짜냐",
          "original_form": "진짜냐",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.25",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.26",
          "form": "지구온난화네",
          "original_form": "지구온난화네",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.27",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.28",
          "form": "무슨 북극 얼음도 다 녹았대",
          "original_form": "무슨 북극 얼음도 다 녹았대",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.29",
          "form": "분리수거열심히해야돼",
          "original_form": "분리수거열심히해야돼",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004179.1.30",
          "form": "너무 더워서",
          "original_form": "너무 더워서",
          "speaker_id": "1",
          "time": "20210821 15:26"
        },
        {
