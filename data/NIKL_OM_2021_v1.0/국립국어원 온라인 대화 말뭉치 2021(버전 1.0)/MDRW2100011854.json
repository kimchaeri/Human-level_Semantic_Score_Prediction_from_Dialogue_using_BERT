      "utterance": [
        {
          "id": "MDRW2100011854.1.1",
          "form": "오 부럽다",
          "original_form": "오 부럽다",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100011854.1.2",
          "form": "노량진에 있어",
          "original_form": "노량진에 있어",
          "speaker_id": "1",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100011854.1.3",
          "form": "나중에 한 번 가보고 싶다",
          "original_form": "나중에 한 번 가보고 싶다",
          "speaker_id": "2",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100011854.1.4",
          "form": "코로나 괜찮아지면 가보자 진짜 귀여워",
          "original_form": "코로나 괜찮아지면 가보자 진짜 귀여워",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011854.1.5",
          "form": "별로 안무서워?",
          "original_form": "별로 안무서워?",
          "speaker_id": "2",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011854.1.6",
          "form": "고양이가 무섭냐고?",
          "original_form": "고양이가 무섭냐고?",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011854.1.7",
          "form": "아니 나 겁이 좀 많아서",
          "original_form": "아니 나 겁이 좀 많아서",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011854.1.8",
          "form": "근데 고양이가 일단 잘 다가오진 않아",
          "original_form": "근데 고양이가 일단 잘 다가오진 않아",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011854.1.9",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011854.1.10",
          "form": "근데 또 오긴 와",
          "original_form": "근데 또 오긴 와",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011854.1.11",
          "form": "머야;;",
          "original_form": "머야;;",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011854.1.12",
          "form": "강아지처럼 막 치대지 않는다는 거지",
          "original_form": "강아지처럼 막 치대지 않는다는 거지",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011854.1.13",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "2",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011854.1.14",
          "form": "막상 가까이 오면 무섭기보다 너무 귀여워",
          "original_form": "막상 가까이 오면 무섭기보다 너무 귀여워",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011854.1.15",
          "form": "수달 키우는 사람도 있더라",
          "original_form": "수달 키우는 사람도 있더라",
