      "utterance": [
        {
          "id": "MDRW2100019046.1.1",
          "form": "학교생활.",
          "original_form": "학교생활.",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.2",
          "form": "졸업하셨어요?",
          "original_form": "졸업하셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.3",
          "form": "네 졸업했어요 ^.^",
          "original_form": "네 졸업했어요 ^.^",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.4",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.5",
          "form": "저도요~",
          "original_form": "저도요~",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.6",
          "form": "요즘 유행 졸업이잖어",
          "original_form": "요즘 유행 졸업이잖어",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.7",
          "form": "누가 졸업을 안해",
          "original_form": "누가 졸업을 안해",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019046.1.8",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019046.1.9",
          "form": "마 다들 졸업 안하나 ㅋㅋ",
          "original_form": "마 다들 졸업 안하나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019046.1.10",
          "form": "내말이 그말이다",
          "original_form": "내말이 그말이다",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019046.1.11",
          "form": "아 배고파",
          "original_form": "아 배고파",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019046.1.12",
          "form": "나도..",
          "original_form": "나도..",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019046.1.13",
          "form": "학교다닐때",
          "original_form": "학교다닐때",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019046.1.14",
          "form": "삼김 많이먹억는데",
          "original_form": "삼김 많이먹억는데",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.15",
          "form": "갑지기 먹고싳네",
          "original_form": "갑지기 먹고싳네",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.16",
          "form": "그땐 질렸는데",
          "original_form": "그땐 질렸는데",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.17",
          "form": "글고 가성비 떨어져서",
          "original_form": "글고 가성비 떨어져서",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.18",
          "form": "좀 인정이다",
          "original_form": "좀 인정이다",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.19",
          "form": "요새 삼김 천원함",
          "original_form": "요새 삼김 천원함",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.20",
          "form": "후반엔 잘 안 먹음",
          "original_form": "후반엔 잘 안 먹음",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.21",
          "form": "누가먹어",
          "original_form": "누가먹어",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.22",
          "form": "아 ㄹㅇ",
          "original_form": "아 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.23",
          "form": "천원도아니고",
          "original_form": "천원도아니고",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.24",
          "form": "걍 라면 사먹고말지",
          "original_form": "걍 라면 사먹고말지",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.25",
          "form": "천이백 이러지 않나요",
          "original_form": "천이백 이러지 않나요",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019046.1.26",
          "form": "라면이 ㄹㅇ..국물도있고 배채우긴 좋다",
          "original_form": "라면이 ㄹㅇ..국물도있고 배채우긴 좋다",
          "speaker_id": "2",
          "time": "20210827 14:51"
        }
      ]
