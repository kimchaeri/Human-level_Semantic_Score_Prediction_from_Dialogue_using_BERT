      "utterance": [
        {
          "id": "MDRW2100035679.1.1",
          "form": "요즘 즐겨듣는 노래 있으세요?",
          "original_form": "요즘 즐겨듣는 노래 있으세요?",
          "speaker_id": "2",
          "time": "20210829 22:18"
        },
        {
          "id": "MDRW2100035679.1.2",
          "form": "오늘 문세윤 노래 들었는데 그게 계속 귓가에 맴도네요ㅋㅋ",
          "original_form": "오늘 문세윤 노래 들었는데 그게 계속 귓가에 맴도네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035679.1.3",
          "form": "아 부끄뚱ㅋㅋㅋㅋ",
          "original_form": "아 부끄뚱ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035679.1.4",
          "form": "인사이드아웃이라는 애니메이션영화 보셨어요?",
          "original_form": "인사이드아웃이라는 애니메이션영화 보셨어요?",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035679.1.5",
          "form": "아니요 어떤애니에요??",
          "original_form": "아니요 어떤애니에요??",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035679.1.6",
          "form": "문세윤이 거기나오는 빙봉이란 캐릭터랑",
          "original_form": "문세윤이 거기나오는 빙봉이란 캐릭터랑",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035679.1.7",
          "form": "엄청닮았어요",
          "original_form": "엄청닮았어요",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035679.1.8",
          "form": "빙봉ㅇㅣ춤추고노래하는코끼리에요",
          "original_form": "빙봉ㅇㅣ춤추고노래하는코끼리에요",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100035679.1.9",
          "form": "이름도 귀엽네요ㅋㅋㅋ",
          "original_form": "이름도 귀엽네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035679.1.10",
          "form": "담에 보기리스트에 넣어야겠어요ㅋㅋㅋ",
          "original_form": "담에 보기리스트에 넣어야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035679.1.11",
          "form": "문세윤이 꿈을 이룬빙봉같다는 댓글이 많더라고요ㅋㅋㅋㄱㄱ",
          "original_form": "문세윤이 꿈을 이룬빙봉같다는 댓글이 많더라고요ㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035679.1.12",
          "form": "name2님은 요즘 듣는 노래 있으세요?",
          "original_form": "&name2&님은 요즘 듣는 노래 있으세요?",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035679.1.13",
          "form": "ㅋㅋㅋㄲㄱㄲㅋ앜ㄲㅋㅋㅋㅋ 너무 웃기네요",
          "original_form": "ㅋㅋㅋㄲㄱㄲㅋ앜ㄲㅋㅋㅋㅋ 너무 웃기네요",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035679.1.14",
          "form": "권진아 운이좋았지 좋아합니다!",
          "original_form": "권진아 운이좋았지 좋아합니다!",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035679.1.15",
          "form": "권진아하면 발라드인데 맞나요?!",
          "original_form": "권진아하면 발라드인데 맞나요?!",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100035679.1.16",
          "form": "정확합니다",
          "original_form": "정확합니다",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100035679.1.17",
          "form": "크으 목소리가 참 좋죠~",
          "original_form": "크으 목소리가 참 좋죠~",
