      "utterance": [
        {
          "id": "MDRW2100001893.1.1",
          "form": "강아지기저귀 잘 쓰고 있으신거예요?",
          "original_form": "강아지기저귀 잘 쓰고 있으신거예요?",
          "speaker_id": "1",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001893.1.2",
          "form": "전 강아지 안키워서 잘모르겠어요 ㅎ 애완견키우세요?",
          "original_form": "전 강아지 안키워서 잘모르겠어요 ㅎ 애완견키우세요?",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001893.1.3",
          "form": "아니요 고등학교때 키운 강아지 20년 살다 간후 안키워요 ㅜ.ㅜ",
          "original_form": "아니요 고등학교때 키운 강아지 20년 살다 간후 안키워요 ㅜ.ㅜ",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001893.1.4",
          "form": "그게 젤 힘들죠 정들었는데 보내기가ㅠ",
          "original_form": "그게 젤 힘들죠 정들었는데 보내기가ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001893.1.5",
          "form": "많이 생각나요 첨에는 슬펐지만",
          "original_form": "많이 생각나요 첨에는 슬펐지만",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001893.1.6",
          "form": "엄마가 넘슬퍼해서 다시는안키운다 했다가 유기견보면 또불쌍해서 데꾸와요",
          "original_form": "엄마가 넘슬퍼해서 다시는안키운다 했다가 유기견보면 또불쌍해서 데꾸와요",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001893.1.7",
          "form": "아 부모님강아지때문에 기저귀 사셨군요?",
          "original_form": "아 부모님강아지때문에 기저귀 사셨군요?",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001893.1.8",
          "form": "네 자꾸 머가 줄줄 흐른다고 생리도 아닌거같은데",
          "original_form": "네 자꾸 머가 줄줄 흐른다고 생리도 아닌거같은데",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001893.1.9",
          "form": "오늘내일하다 요즘 다시 살아났어요",
          "original_form": "오늘내일하다 요즘 다시 살아났어요",
          "speaker_id": "2",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001893.1.10",
          "form": "다행이네요 병원에는 가보셨어요?",
          "original_form": "다행이네요 병원에는 가보셨어요?",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001893.1.11",
          "form": "나이먹었다고 이젠 걍 아프면 안락사 시킨데요ㅠㅠ",
          "original_form": "나이먹었다고 이젠 걍 아프면 안락사 시킨데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001893.1.12",
          "form": "슬프네요",
          "original_form": "슬프네요",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001893.1.13",
          "form": "이제 진짜 다시는 안키운대요 보내는게 넘 맘아파서",
          "original_form": "이제 진짜 다시는 안키운대요 보내는게 넘 맘아파서",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001893.1.14",
          "form": "전 병원비도 엄청 들었어요 승용차값",
          "original_form": "전 병원비도 엄청 들었어요 승용차값",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001893.1.15",
          "form": "매년 한마리씩보낸거같아요 시츄보내고 말티즈보내고 얼마전 고양이보내고ㅠㅠ",
          "original_form": "매년 한마리씩보낸거같아요 시츄보내고 말티즈보내고 얼마전 고양이보내고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001893.1.16",
          "form": "육아보다 돈이 더드는거같아요",
          "original_form": "육아보다 돈이 더드는거같아요",
          "speaker_id": "2",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001893.1.17",
          "form": "병원비에 부가가치세가 붙어서",
          "original_form": "병원비에 부가가치세가 붙어서",
          "speaker_id": "1",
          "time": "20210819 22:17"
        },
        {
          "id": "MDRW2100001893.1.18",
          "form": "보험이 안되니 진짜비싸더라구요",
          "original_form": "보험이 안되니 진짜비싸더라구요",
          "speaker_id": "2",
          "time": "20210819 22:18"
        },
        {
          "id": "MDRW2100001893.1.19",
          "form": "보험들려고했는데 그돈이 그돈이요",
          "original_form": "보험들려고했는데 그돈이 그돈이요",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001893.1.20",
          "form": "사료나 간식비도 비싸고",
          "original_form": "사료나 간식비도 비싸고",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001893.1.21",
          "form": "이미용에도 돈이 엄청 들고",
          "original_form": "이미용에도 돈이 엄청 들고",
          "speaker_id": "1",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001893.1.22",
          "form": "맞아요ㅎ 저 머리 자르는것보다 비싸더라구요",
          "original_form": "맞아요ㅎ 저 머리 자르는것보다 비싸더라구요",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001893.1.23",
          "form": "부분미용만 해도 2만원",
          "original_form": "부분미용만 해도 2만원",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001893.1.24",
          "form": "돈많으면 그때 애완견키워야겠어요ㅎ",
          "original_form": "돈많으면 그때 애완견키워야겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:23"
        }
      ]
