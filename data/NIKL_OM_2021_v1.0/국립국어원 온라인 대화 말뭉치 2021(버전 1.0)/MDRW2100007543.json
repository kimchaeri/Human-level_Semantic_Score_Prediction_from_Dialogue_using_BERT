      "utterance": [
        {
          "id": "MDRW2100007543.1.1",
          "form": "무슨 이상한 게임 한거기억나?",
          "original_form": "무슨 이상한 게임 한거기억나?",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.2",
          "form": "20시간충전하고",
          "original_form": "20시간충전하고",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.3",
          "form": "나랑은 오버워치를 했을걸??!",
          "original_form": "나랑은 오버워치를 했을걸??!",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.4",
          "form": "범물동에서",
          "original_form": "범물동에서",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.5",
          "form": "어떤?",
          "original_form": "어떤?",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.6",
          "form": "아니 그거말구",
          "original_form": "아니 그거말구",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.7",
          "form": "RPG게임",
          "original_form": "RPG게임",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.8",
          "form": "그 충전한 피방",
          "original_form": "그 충전한 피방",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.9",
          "form": "20살때했던거 같이",
          "original_form": "20살때했던거 같이",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.10",
          "form": "폐업해버린건 기억나",
          "original_form": "폐업해버린건 기억나",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.11",
          "form": "망한 피방",
          "original_form": "망한 피방",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.12",
          "form": "코코호도 바로 앞에있던 피방",
          "original_form": "코코호도 바로 앞에있던 피방",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.13",
          "form": "게임 되게 이상한거",
          "original_form": "게임 되게 이상한거",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.14",
          "form": "아 무슨 사막",
          "original_form": "아 무슨 사막",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.15",
          "form": "아아아 검은사막!",
          "original_form": "아아아 검은사막!",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.16",
          "form": "우리 캐릭터만 만들다가",
          "original_form": "우리 캐릭터만 만들다가",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.17",
          "form": "검은사막햇엇다 참",
          "original_form": "검은사막햇엇다 참",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.18",
          "form": "시간다갔잖어 ㅋㅋㅋ",
          "original_form": "시간다갔잖어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.19",
          "form": "직업선택 잘못하고",
          "original_form": "직업선택 잘못하고",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.20",
          "form": "어렵고",
          "original_form": "어렵고",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007543.1.21",
          "form": "ㅋㅋㅋ그래서 하루이틀하고 헤어졌지",
          "original_form": "ㅋㅋㅋ그래서 하루이틀하고 헤어졌지",
          "speaker_id": "2",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007543.1.22",
          "form": "너랑은 모두의마블이 제일 재밌었어",
          "original_form": "너랑은 모두의마블이 제일 재밌었어",
          "speaker_id": "2",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007543.1.23",
          "form": "난...재미없었어",
          "original_form": "난...재미없었어",
          "speaker_id": "1",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007543.1.24",
          "form": "모두의 마블은 안좋아",
          "original_form": "모두의 마블은 안좋아",
          "speaker_id": "1",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007543.1.25",
          "form": "그치만 쿠킹덤은 조아",
          "original_form": "그치만 쿠킹덤은 조아",
          "speaker_id": "1",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007543.1.26",
          "form": "지금 내가 널 이기고있기에",
          "original_form": "지금 내가 널 이기고있기에",
          "speaker_id": "1",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007543.1.27",
          "form": "모두의마블작년에",
          "original_form": "모두의마블작년에",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007543.1.28",
          "form": "내 자취방에서할때",
          "original_form": "내 자취방에서할때",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007543.1.29",
          "form": "좋았잖아",
          "original_form": "좋았잖아",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007543.1.30",
          "form": "고3끝나고할때도ㅠ",
          "original_form": "고3끝나고할때도ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007543.1.31",
          "form": "액션퍼즐패밀리도",
          "original_form": "액션퍼즐패밀리도",
          "speaker_id": "2",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007543.1.32",
          "form": "안좋았어",
          "original_form": "안좋았어",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007543.1.33",
          "form": "그거 고혈압 유발 게임",
          "original_form": "그거 고혈압 유발 게임",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007543.1.34",
          "form": "현질 유도 게임",
          "original_form": "현질 유도 게임",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007543.1.35",
          "form": "아니 왜",
          "original_form": "아니 왜",
          "speaker_id": "2",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007543.1.36",
          "form": "게임에 그렇게 부정적이야",
          "original_form": "게임에 그렇게 부정적이야",
          "speaker_id": "2",
          "time": "20210822 03:09"
        }
      ]
