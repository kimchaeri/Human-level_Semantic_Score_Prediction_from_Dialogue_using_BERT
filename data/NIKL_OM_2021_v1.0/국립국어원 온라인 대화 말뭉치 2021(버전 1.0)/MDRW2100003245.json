        {
          "id": "MDRW2100003245.1.1",
          "form": "요즘 유행하는 음싣",
          "original_form": "요즘 유행하는 음싣",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003245.1.2",
          "form": "음싣",
          "original_form": "음싣",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003245.1.3",
          "form": "음식",
          "original_form": "음식",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003245.1.4",
          "form": "뭐가 있을까",
          "original_form": "뭐가 있을까",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003245.1.5",
          "form": "마라탕은 좀 유행 지났나",
          "original_form": "마라탕은 좀 유행 지났나",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003245.1.6",
          "form": "그렇지 ㅋㅋ",
          "original_form": "그렇지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003245.1.7",
          "form": "한창 유행이었지",
          "original_form": "한창 유행이었지",
          "speaker_id": "2",
          "time": "20210820 21:02"
        },
        {
          "id": "MDRW2100003245.1.8",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003245.1.9",
          "form": "매운 거는 꾸준히 유행하는 듯",
          "original_form": "매운 거는 꾸준히 유행하는 듯",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003245.1.10",
          "form": "그릭요거트 많이 먹더라",
          "original_form": "그릭요거트 많이 먹더라",
          "speaker_id": "2",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003245.1.11",
          "form": "맞아 그릭요거트나 브라타치즈",
          "original_form": "맞아 그릭요거트나 브라타치즈",
          "speaker_id": "1",
          "time": "20210820 21:03"
        },
        {
          "id": "MDRW2100003245.1.12",
          "form": "브라타치즈 맛있어",
          "original_form": "브라타치즈 맛있어",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003245.1.13",
          "form": "진짜 유행하는 음식 많다",
          "original_form": "진짜 유행하는 음식 많다",
          "speaker_id": "1",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003245.1.14",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003245.1.15",
          "form": "유행하는 음식 따르는 편이야?",
          "original_form": "유행하는 음식 따르는 편이야?",
          "speaker_id": "1",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003245.1.16",
          "form": "궁금하긴 하지",
          "original_form": "궁금하긴 하지",
          "speaker_id": "2",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003245.1.17",
          "form": "맞아 근데 항상 찾아먹진 않아",
          "original_form": "맞아 근데 항상 찾아먹진 않아",
          "speaker_id": "1",
          "time": "20210820 21:05"
