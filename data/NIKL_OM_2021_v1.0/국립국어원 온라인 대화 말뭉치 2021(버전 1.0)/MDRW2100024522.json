      "utterance": [
        {
          "id": "MDRW2100024522.1.1",
          "form": "재택근무해보셨어요?",
          "original_form": "재택근무해보셨어요?",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100024522.1.2",
          "form": "아니오 한 번도 못 해봤어요",
          "original_form": "아니오 한 번도 못 해봤어요",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024522.1.3",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024522.1.4",
          "form": "네네 저 두어번해봤어요",
          "original_form": "네네 저 두어번해봤어요",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024522.1.5",
          "form": "너무젛아요..",
          "original_form": "너무젛아요..",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024522.1.6",
          "form": "와 어떤 식으로 하셨는데요?",
          "original_form": "와 어떤 식으로 하셨는데요?",
          "speaker_id": "2",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024522.1.7",
          "form": "그냥 노트북에 원격 연결해서 업무보는데",
          "original_form": "그냥 노트북에 원격 연결해서 업무보는데",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024522.1.8",
          "form": "출퇴근 지옥철에 시달리지않아도되고",
          "original_form": "출퇴근 지옥철에 시달리지않아도되고",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024522.1.9",
          "form": "점심에 드러누워 잘수있어요!",
          "original_form": "점심에 드러누워 잘수있어요!",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024522.1.10",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100024522.1.11",
          "form": "출퇴근 시간이 진짜 아까운 것 같아요ㅠㅠ",
          "original_form": "출퇴근 시간이 진짜 아까운 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100024522.1.12",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210831 13:21"
        },
        {
          "id": "MDRW2100024522.1.13",
          "form": "저는 환승도 많이 하는 편이라 길바닥에 버리는 시간이 줄어드는게 너무 좋더라고요",
          "original_form": "저는 환승도 많이 하는 편이라 길바닥에 버리는 시간이 줄어드는게 너무 좋더라고요",
          "speaker_id": "1",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100024522.1.14",
          "form": "지난 달에 며칠 편도 한시간 반 두시간? 출퇴근 했는데 죽는 줄 알았어요ㅠㅠ",
          "original_form": "지난 달에 며칠 편도 한시간 반 두시간? 출퇴근 했는데 죽는 줄 알았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100024522.1.15",
          "form": "다른곳에 지원나가셨었나봐요!",
          "original_form": "다른곳에 지원나가셨었나봐요!",
          "speaker_id": "1",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100024522.1.16",
          "form": "머 비슷해요! 근데 버스가 출퇴근 시간에 젤 막히는 곳을 통과해서 엄청 밀리더라고요",
          "original_form": "머 비슷해요! 근데 버스가 출퇴근 시간에 젤 막히는 곳을 통과해서 엄청 밀리더라고요",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024522.1.17",
          "form": "재택근무들 시작하면서 지하철들이 좀 널널해진 것 같아요",
          "original_form": "재택근무들 시작하면서 지하철들이 좀 널널해진 것 같아요",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100024522.1.18",
          "form": "그정도로 차이 나나요?",
          "original_form": "그정도로 차이 나나요?",
          "speaker_id": "2",
          "time": "20210902 09:42"
        }
      ]
