      "utterance": [
        {
          "id": "MDRW2100010392.1.1",
          "form": "입맛이 없어서 물말아서 오이지 얹어서 먹을까 해요 ㅎㅎ",
          "original_form": "입맛이 없어서 물말아서 오이지 얹어서 먹을까 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:29"
        },
        {
          "id": "MDRW2100010392.1.2",
          "form": "오 술술 잘넘어가겠네요",
          "original_form": "오 술술 잘넘어가겠네요",
          "speaker_id": "1",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100010392.1.3",
          "form": "한국의 입맛!",
          "original_form": "한국의 입맛!",
          "speaker_id": "1",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100010392.1.4",
          "form": "ㅋㅋㅋ 네 여름철에 딱이에요",
          "original_form": "ㅋㅋㅋ 네 여름철에 딱이에요",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010392.1.5",
          "form": "name1님은 뭐 드시나요",
          "original_form": "&name1&님은 뭐 드시나요",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010392.1.6",
          "form": "저는 김하고 두부랑 먹었어요",
          "original_form": "저는 김하고 두부랑 먹었어요",
          "speaker_id": "1",
          "time": "20210823 17:55"
        },
        {
          "id": "MDRW2100010392.1.7",
          "form": "넘 맛있네요",
          "original_form": "넘 맛있네요",
          "speaker_id": "1",
          "time": "20210823 17:55"
        },
        {
          "id": "MDRW2100010392.1.8",
          "form": "두부 맛있죠 저도 두부 사랑해요",
          "original_form": "두부 맛있죠 저도 두부 사랑해요",
          "speaker_id": "2",
          "time": "20210823 18:05"
        },
        {
          "id": "MDRW2100010392.1.9",
          "form": "새송이버섯이랑 좋아하는 식재료가 몇개 있어요 ㅋㅋㅋ",
          "original_form": "새송이버섯이랑 좋아하는 식재료가 몇개 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:05"
        },
        {
          "id": "MDRW2100010392.1.10",
          "form": "오 역시 야채사랑ㅋㅋ",
          "original_form": "오 역시 야채사랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010392.1.11",
          "form": "버섯도 맛있죠",
          "original_form": "버섯도 맛있죠",
          "speaker_id": "1",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010392.1.12",
          "form": "저는 올해 죽순사는걸 놓쳐서ㅜㅜ 내년에 꼭 살꺼에요",
          "original_form": "저는 올해 죽순사는걸 놓쳐서ㅜㅜ 내년에 꼭 살꺼에요",
          "speaker_id": "1",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010392.1.13",
          "form": "네 두부랑 버섯 구워먹으면 맛나요.. 아니면 된장찌개에 넣어서 ㅎㅎ",
          "original_form": "네 두부랑 버섯 구워먹으면 맛나요.. 아니면 된장찌개에 넣어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:12"
        },
        {
          "id": "MDRW2100010392.1.14",
          "form": "오 죽순!!!",
          "original_form": "오 죽순!!!",
          "speaker_id": "2",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010392.1.15",
          "form": "안 비싼가요?",
          "original_form": "안 비싼가요?",
          "speaker_id": "2",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010392.1.16",
          "form": "그게오월인가 딱 한때 나오던데요",
          "original_form": "그게오월인가 딱 한때 나오던데요",
          "speaker_id": "1",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010392.1.17",
          "form": "아 그때가 철이군요",
          "original_form": "아 그때가 철이군요",
          "speaker_id": "2",
          "time": "20210823 18:13"
        },
        {
          "id": "MDRW2100010392.1.18",
          "form": "음 비싸서 고민하다가 판매기간 지나서 못샀던거 같아요ㅜㅜ",
          "original_form": "음 비싸서 고민하다가 판매기간 지나서 못샀던거 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010392.1.19",
          "form": "죽순으로 뭐 해드시나요 구워드시나요",
          "original_form": "죽순으로 뭐 해드시나요 구워드시나요",
          "speaker_id": "2",
          "time": "20210823 18:14"
        },
        {
          "id": "MDRW2100010392.1.20",
          "form": "저는 굽기밖에 할줄몰라요 키키",
          "original_form": "저는 굽기밖에 할줄몰라요 키키",
          "speaker_id": "1",
          "time": "20210823 18:15"
        },
        {
          "id": "MDRW2100010392.1.21",
          "form": "굽기 볶기 ㅋㅋ",
          "original_form": "굽기 볶기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:15"
        },
        {
          "id": "MDRW2100010392.1.22",
          "form": "ㅋㅋㅋㅋ 그게 최고죠 뭐 하 죽순 듣고나서 군침나네요..",
          "original_form": "ㅋㅋㅋㅋ 그게 최고죠 뭐 하 죽순 듣고나서 군침나네요..",
          "speaker_id": "2",
          "time": "20210823 18:17"
        },
        {
          "id": "MDRW2100010392.1.23",
          "form": "제철 나온거 얼려둿다 녹혀먹는대요",
          "original_form": "제철 나온거 얼려둿다 녹혀먹는대요",
          "speaker_id": "1",
          "time": "20210823 18:17"
        },
        {
          "id": "MDRW2100010392.1.24",
          "form": "대나무는 하우스재배안되남ㅋㅋㅋ",
          "original_form": "대나무는 하우스재배안되남ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:17"
        },
        {
          "id": "MDRW2100010392.1.25",
          "form": "아스파라거스도 강원도에서 제철에 싸게나오더래요",
          "original_form": "아스파라거스도 강원도에서 제철에 싸게나오더래요",
          "speaker_id": "1",
