      "utterance": [
        {
          "id": "MDRW2100039018.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 01:33"
        },
        {
          "id": "MDRW2100039018.1.2",
          "form": "전 포테이토도 좋은데 가끔 콤비가 땡겨요 올리브 너무 좋아해서",
          "original_form": "전 포테이토도 좋은데 가끔 콤비가 땡겨요 올리브 너무 좋아해서",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100039018.1.3",
          "form": "맞아요 콤비도 좋죠 \r\n그래서 포테이토아니면 콤비고르는듯 해요",
          "original_form": "맞아요 콤비도 좋죠 \r\n그래서 포테이토아니면 콤비고르는듯 해요",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100039018.1.4",
          "form": "저는 그 동글동글한 고기도 좋아해요ㅋㅋ",
          "original_form": "저는 그 동글동글한 고기도 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100039018.1.5",
          "form": "동글동글 고기라 잘 안 떠오르네요\r\n알것같기도 하고..",
          "original_form": "동글동글 고기라 잘 안 떠오르네요\r\n알것같기도 하고..",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100039018.1.6",
          "form": "싼 콤비 피자에 주로 있는 고기죠ㅎ",
          "original_form": "싼 콤비 피자에 주로 있는 고기죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100039018.1.7",
          "form": "전 패퍼로니도 좋아해요",
          "original_form": "전 패퍼로니도 좋아해요",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100039018.1.8",
          "form": "오 패퍼로니가 동글 햄 들어있던건가요?",
          "original_form": "오 패퍼로니가 동글 햄 들어있던건가요?",
          "speaker_id": "2",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100039018.1.9",
          "form": "네 얇게 썬 동그란 햄ㅎㅎ",
          "original_form": "네 얇게 썬 동그란 햄ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:42"
        },
        {
          "id": "MDRW2100039018.1.10",
          "form": "전 패퍼로니는 주문해본적이 없어요",
          "original_form": "전 패퍼로니는 주문해본적이 없어요",
          "speaker_id": "2",
          "time": "20210903 01:43"
        },
        {
          "id": "MDRW2100039018.1.11",
          "form": "콤비에 들어있는거만으로 충분하긴 하죠ㅎㅎ",
          "original_form": "콤비에 들어있는거만으로 충분하긴 하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:44"
        },
        {
          "id": "MDRW2100039018.1.12",
          "form": "그렇다면 뭐ㅎㅎ\r\n따로 안 먹어봐도 되겠네요",
          "original_form": "그렇다면 뭐ㅎㅎ\r\n따로 안 먹어봐도 되겠네요",
          "speaker_id": "2",
          "time": "20210903 01:45"
        },
        {
          "id": "MDRW2100039018.1.13",
          "form": "ㅋㅋㅋ그쵸ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ그쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:51"
        },
        {
          "id": "MDRW2100039018.1.14",
          "form": "그치만 저같은 피자매니아는 가끔 땡갸요",
          "original_form": "그치만 저같은 피자매니아는 가끔 땡갸요",
          "speaker_id": "1",
          "time": "20210903 01:51"
        },
        {
          "id": "MDRW2100039018.1.15",
          "form": "피자 많이 좋아하시나봐요",
          "original_form": "피자 많이 좋아하시나봐요",
          "speaker_id": "2",
