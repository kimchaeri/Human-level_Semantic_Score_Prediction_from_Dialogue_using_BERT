      "utterance": [
        {
          "id": "MDRW2100047428.1.1",
          "form": "띠다렉인가",
          "original_form": "띠다렉인가",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047428.1.2",
          "form": "그목깁스 ㅋㅋ",
          "original_form": "그목깁스 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047428.1.3",
          "form": "그거 넘 웃겨요",
          "original_form": "그거 넘 웃겨요",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047428.1.4",
          "form": "결국 목깁스죠?",
          "original_form": "결국 목깁스죠?",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047428.1.5",
          "form": "중고거래는 즐겨하세요?",
          "original_form": "중고거래는 즐겨하세요?",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047428.1.6",
          "form": "가끔해요",
          "original_form": "가끔해요",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047428.1.7",
          "form": "저번에 ㅋㅋ옷을 팔아서 ㅋㅋ",
          "original_form": "저번에 ㅋㅋ옷을 팔아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100047428.1.8",
          "form": "9만원 벌었어요",
          "original_form": "9만원 벌었어요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.9",
          "form": "아 옷을 파셧구나",
          "original_form": "아 옷을 파셧구나",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.10",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.11",
          "form": "정말 넘 행복",
          "original_form": "정말 넘 행복",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.12",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.13",
          "form": "비싼 옷이엇나봐요",
          "original_form": "비싼 옷이엇나봐요",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.14",
          "form": "한정판옷이라 괜찮았어요",
          "original_form": "한정판옷이라 괜찮았어요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.15",
          "form": "아뇨 싸요",
          "original_form": "아뇨 싸요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.16",
          "form": "근데 한정판이라",
          "original_form": "근데 한정판이라",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.17",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.18",
          "form": "추첨구매요",
          "original_form": "추첨구매요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.19",
          "form": "뭘 산 적도 잇으세요?",
          "original_form": "뭘 산 적도 잇으세요?",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.20",
          "form": "운이 좋았어요",
          "original_form": "운이 좋았어요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100047428.1.22",
          "form": "저 중고는 안사봤어요",
          "original_form": "저 중고는 안사봤어요",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.23",
          "form": "저도 사본적은 없어요 ㅋㅋㅋㅋ",
          "original_form": "저도 사본적은 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.25",
          "form": "생각해보니",
          "original_form": "생각해보니",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.26",
          "form": "장난감 샀어요 ㅎㅎ",
          "original_form": "장난감 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.27",
          "form": "그건 또 버리고 하니사봤어요",
          "original_form": "그건 또 버리고 하니사봤어요",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.28",
          "form": "아 육아템 ㅋㅋㅋㅋㅋ",
          "original_form": "아 육아템 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.29",
          "form": "뭐 파세요?",
          "original_form": "뭐 파세요?",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047428.1.30",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.31",
          "form": "저 동전도 팔아보고",
          "original_form": "저 동전도 팔아보고",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.32",
          "form": "저는 최근에는 그릇팔앗어요",
          "original_form": "저는 최근에는 그릇팔앗어요",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.33",
          "form": "얼마전 장난감도 팔고요",
          "original_form": "얼마전 장난감도 팔고요",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.34",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.35",
          "form": "어찌 포장해요",
          "original_form": "어찌 포장해요",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.36",
          "form": "첨봤어요",
          "original_form": "첨봤어요",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.37",
          "form": "대박인데요",
          "original_form": "대박인데요",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.38",
          "form": "뽁뽁이로 ㅋㅋㅋㅋㅋ",
          "original_form": "뽁뽁이로 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.39",
          "form": "안깨져요?",
          "original_form": "안깨져요?",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100047428.1.40",
          "form": "이쁜그릇인가봐요",
          "original_form": "이쁜그릇인가봐요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100047428.1.41",
          "form": "그릇 이쁜건 진짜 갖고 싶던데",
          "original_form": "그릇 이쁜건 진짜 갖고 싶던데",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100047428.1.42",
          "form": "고수신가봐요 ㅎㅎ",
          "original_form": "고수신가봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100047428.1.43",
          "form": "전 포장을 못하고 택배 부치는게 힘들어요",
          "original_form": "전 포장을 못하고 택배 부치는게 힘들어요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100047428.1.44",
          "form": "그릇은 직거래로만 팔앗어요",
          "original_form": "그릇은 직거래로만 팔앗어요",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100047428.1.45",
          "form": "택배로 보내다간 깨질까봐 ㅎㅎ",
          "original_form": "택배로 보내다간 깨질까봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.46",
          "form": "그래야돼요",
          "original_form": "그래야돼요",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.47",
          "form": "진상 걸리면 안되니",
          "original_form": "진상 걸리면 안되니",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.48",
          "form": "그렇죠 현명하세요",
          "original_form": "그렇죠 현명하세요",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.49",
          "form": "많이 버셨어요?",
          "original_form": "많이 버셨어요?",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.50",
          "form": "네 칠만원인가",
          "original_form": "네 칠만원인가",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.51",
          "form": "그릇 세트엿거든요",
          "original_form": "그릇 세트엿거든요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.52",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047428.1.53",
          "form": "대박인데요",
          "original_form": "대박인데요",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100047428.1.54",
          "form": "그래도 진짜 잘팔았네요",
          "original_form": "그래도 진짜 잘팔았네요",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100047428.1.55",
          "form": "전 옷빼곤 ㅜㅜ",
          "original_form": "전 옷빼곤 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100047428.1.56",
          "form": "다 헐값에 능력부족요",
          "original_form": "다 헐값에 능력부족요",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100047428.1.57",
          "form": "잘파는분 부러워요",
          "original_form": "잘파는분 부러워요",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100047428.1.58",
          "form": "ㅋㅋㅋㅋㅋㅋ아니에요 저도 능력부족",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니에요 저도 능력부족",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100047428.1.59",
          "form": "지금도 안팔리고 잇는게 잇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "지금도 안팔리고 잇는게 잇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:43"
        }
