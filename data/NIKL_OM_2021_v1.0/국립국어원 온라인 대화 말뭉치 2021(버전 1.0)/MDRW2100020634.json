      "utterance": [
        {
          "id": "MDRW2100020634.1.1",
          "form": "근데 안 부 러워요",
          "original_form": "근데 안 부 러워요",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020634.1.2",
          "form": "님은 점심 뭐드실건데요 ?",
          "original_form": "님은 점심 뭐드실건데요 ?",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020634.1.3",
          "form": "쌀국수요",
          "original_form": "쌀국수요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020634.1.4",
          "form": "되게 좋아하시네요",
          "original_form": "되게 좋아하시네요",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020634.1.5",
          "form": "쌀국수랑 마라탕이랑 잘어울리는 거 알죠",
          "original_form": "쌀국수랑 마라탕이랑 잘어울리는 거 알죠",
          "speaker_id": "2",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020634.1.6",
          "form": "둘다 고수 친구들이에요",
          "original_form": "둘다 고수 친구들이에요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020634.1.7",
          "form": "음 별로요",
          "original_form": "음 별로요",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020634.1.8",
          "form": "단호하시네요",
          "original_form": "단호하시네요",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020634.1.9",
          "form": "마라탕에 고수도 들어가요?",
          "original_form": "마라탕에 고수도 들어가요?",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020634.1.10",
          "form": "마라도 향이잖아요",
          "original_form": "마라도 향이잖아요",
          "speaker_id": "2",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100020634.1.11",
          "form": "엥 왜 보낼수 없죠 ? 저 친게 없는데 ..",
          "original_form": "엥 왜 보낼수 없죠 ? 저 친게 없는데 ..",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020634.1.12",
          "form": "마라랑 고수는 다르죠",
          "original_form": "마라랑 고수는 다르죠",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020634.1.13",
          "form": "고수는 약간 똥 냄새",
          "original_form": "고수는 약간 똥 냄새",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020634.1.14",
          "form": "다르지만 같아요 둘다 향신료잖아요",
          "original_form": "다르지만 같아요 둘다 향신료잖아요",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020634.1.15",
          "form": "아니 왜 못보낸다는거지",
          "original_form": "아니 왜 못보낸다는거지",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020634.1.16",
          "form": "고수초보한테 듣고싶지 않네요",
          "original_form": "고수초보한테 듣고싶지 않네요",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020634.1.17",
          "form": "허참 본인은 고수 고수세요?",
          "original_form": "허참 본인은 고수 고수세요?",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020634.1.18",
          "form": "네 전 고수 고수에요",
          "original_form": "네 전 고수 고수에요",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020634.1.19",
          "form": "고수매니아라 할 서 있조",
          "original_form": "고수매니아라 할 서 있조",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020634.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020634.1.21",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020634.1.22",
          "form": "다음주제는 제가 정해도 될까요",
          "original_form": "다음주제는 제가 정해도 될까요",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020634.1.23",
          "form": "저도 한번 해보고 싶어서요",
          "original_form": "저도 한번 해보고 싶어서요",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020634.1.24",
          "form": "아예",
          "original_form": "아예",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020634.1.25",
          "form": "슈붕이 최고임",
          "original_form": "슈붕이 최고임",
          "speaker_id": "1",
          "time": "20210829 18:45"
        }
      ]
