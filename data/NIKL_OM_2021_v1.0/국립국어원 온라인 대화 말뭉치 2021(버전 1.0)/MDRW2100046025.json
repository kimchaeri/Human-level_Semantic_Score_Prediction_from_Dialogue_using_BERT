      "utterance": [
        {
          "id": "MDRW2100046025.1.1",
          "form": "빽다방은 아이스크림만 먹어봤는데",
          "original_form": "빽다방은 아이스크림만 먹어봤는데",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046025.1.2",
          "form": "나쁘지 않았던 것 같아요",
          "original_form": "나쁘지 않았던 것 같아요",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100046025.1.3",
          "form": "쥬씨도 분명 뭔가를 먹었는데 기억이 안 나요ㅋㅋㅋ",
          "original_form": "쥬씨도 분명 뭔가를 먹었는데 기억이 안 나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100046025.1.4",
          "form": "딸기바나나는 드셨겠죠? 이거 인기메뉴던데 저 여기에 코코넛 젤리 꼭 넣어먹어요",
          "original_form": "딸기바나나는 드셨겠죠? 이거 인기메뉴던데 저 여기에 코코넛 젤리 꼭 넣어먹어요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046025.1.5",
          "form": "헉 딸바는 어딜 가도 사랑이죠",
          "original_form": "헉 딸바는 어딜 가도 사랑이죠",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046025.1.6",
          "form": "코코넛젤리도 좋아해요!",
          "original_form": "코코넛젤리도 좋아해요!",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046025.1.7",
          "form": "꼬들꼬들한 식감 너무 좋아요",
          "original_form": "꼬들꼬들한 식감 너무 좋아요",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046025.1.8",
          "form": "맞아요. 여기꺼가 맛있는것 같아요",
          "original_form": "맞아요. 여기꺼가 맛있는것 같아요",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046025.1.9",
          "form": "헉 진짜요? 쥬씨딸바에 코코넛은 한 번도 안 먹어봤는데",
          "original_form": "헉 진짜요? 쥬씨딸바에 코코넛은 한 번도 안 먹어봤는데",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046025.1.10",
          "form": "그렇게 말하시니 먹어보고싶어졌어요",
          "original_form": "그렇게 말하시니 먹어보고싶어졌어요",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046025.1.11",
          "form": "침 나온다...",
          "original_form": "침 나온다...",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046025.1.12",
          "form": "아 한번 드시면 500원 그까이꺼! 하실거에요",
          "original_form": "아 한번 드시면 500원 그까이꺼! 하실거에요",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046025.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ좋아요 영업당해부렀어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ좋아요 영업당해부렀어요",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046025.1.14",
          "form": "딸바 코코펄 메모해둘거예요ㅋㅋㅋㅋㅋ",
          "original_form": "딸바 코코펄 메모해둘거예요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046025.1.15",
          "form": "아! 저 뭐 먹었었는지 기억났엇",
          "original_form": "아! 저 뭐 먹었었는지 기억났엇",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046025.1.16",
          "form": "기억났어요 키위먹었나봐요",
          "original_form": "기억났어요 키위먹었나봐요",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046025.1.17",
          "form": "홍시도 맛있는거 아세요? 제철일 때 드세요",
          "original_form": "홍시도 맛있는거 아세요? 제철일 때 드세요",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046025.1.18",
          "form": "키위! 새콤한 맛이 강하죠?",
          "original_form": "키위! 새콤한 맛이 강하죠?",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046025.1.19",
          "form": "홍시 진짜 맛있겠다 으악",
          "original_form": "홍시 진짜 맛있겠다 으악",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046025.1.20",
          "form": "마침 홍시철 돌아오는데 홍시주스 꼭 먹어볼게요",
          "original_form": "마침 홍시철 돌아오는데 홍시주스 꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046025.1.21",
          "form": "안 그래도 홍시 좋아하거든요!",
          "original_form": "안 그래도 홍시 좋아하거든요!",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046025.1.22",
          "form": "키위 새콤달콤해요ㅋㅋㅋ 씨가 토독토독 씹히던데 드셔보셨어요?",
          "original_form": "키위 새콤달콤해요ㅋㅋㅋ 씨가 토독토독 씹히던데 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100046025.1.23",
          "form": "아니요 키위주스는 아니요 이삭에서 먹고 그 뒤로 키위주스는 안 먹었어요. 키위가 저하고는 안 맞았던것 같아요",
          "original_form": "아니요 키위주스는 아니요 이삭에서 먹고 그 뒤로 키위주스는 안 먹었어요. 키위가 저하고는 안 맞았던것 같아요",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046025.1.24",
          "form": "아 물론 골든키위는 먹어요",
          "original_form": "아 물론 골든키위는 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046025.1.25",
          "form": "헉 알러지가 있으신가요..?",
          "original_form": "헉 알러지가 있으신가요..?",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046025.1.26",
          "form": "그건 아닌데 좋은 기억이 아니었던것 같아요",
          "original_form": "그건 아닌데 좋은 기억이 아니었던것 같아요",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046025.1.27",
          "form": "ㅋㅋㅋ몹시..구리셨군요",
          "original_form": "ㅋㅋㅋ몹시..구리셨군요",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046025.1.28",
          "form": "앜 .. 들켰다 ㅎㅎ",
          "original_form": "앜 .. 들켰다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046025.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046025.1.30",
          "form": "과일주스류를 좋아하시는 건가요!",
          "original_form": "과일주스류를 좋아하시는 건가요!",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046025.1.31",
          "form": "네 달달한 음료는 다 좋아해요",
          "original_form": "네 달달한 음료는 다 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046025.1.32",
          "form": "뭔가 쥬씨 메뉴에 애정이 있어보이세요ㅋㅋㅋㅋ",
          "original_form": "뭔가 쥬씨 메뉴에 애정이 있어보이세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:08"
        }
      ]
