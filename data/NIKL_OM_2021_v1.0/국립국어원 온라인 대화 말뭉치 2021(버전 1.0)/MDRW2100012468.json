      "utterance": [
        {
          "id": "MDRW2100012468.1.1",
          "form": "수술하고도 입원이 안되여ㅠ",
          "original_form": "수술하고도 입원이 안되여ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012468.1.2",
          "form": "이게 수술이 아니고 시술로 드간다고..",
          "original_form": "이게 수술이 아니고 시술로 드간다고..",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012468.1.3",
          "form": "너무 짜증이가 나요ㅠ",
          "original_form": "너무 짜증이가 나요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012468.1.4",
          "form": "아 수술 아니고 시술이구나 어깨에 염증 생겨서 그런 건데??",
          "original_form": "아 수술 아니고 시술이구나 어깨에 염증 생겨서 그런 건데??",
          "speaker_id": "1",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012468.1.5",
          "form": "그래 화날 만하지 엠알아이 너무 비 쌈",
          "original_form": "그래 화날 만하지 엠알아이 너무 비 쌈",
          "speaker_id": "1",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012468.1.6",
          "form": "글테요~수술도 100인데 이거 실비하면 어차피 80정도만 나온다 아녀요??",
          "original_form": "글테요~수술도 100인데 이거 실비하면 어차피 80정도만 나온다 아녀요??",
          "speaker_id": "2",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012468.1.7",
          "form": "신랑 수술하고 왔는데 실비 못받아서 또 화낸 거 아니가??",
          "original_form": "신랑 수술하고 왔는데 실비 못받아서 또 화낸 거 아니가??",
          "speaker_id": "1",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012468.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ언니 나 그정도는 아녀요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ언니 나 그정도는 아녀요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012468.1.9",
          "form": "실비에 따라서 다르겠지만 얼마나 올라는고...",
          "original_form": "실비에 따라서 다르겠지만 얼마나 올라는고...",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.10",
          "form": "속으로 화냈지 ㅋㅋㅋㅋㅋ",
          "original_form": "속으로 화냈지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.11",
          "form": "미안",
          "original_form": "미안",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.12",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.13",
          "form": "휴지로 봤제 ㅋㅋㅋㅋㄱ",
          "original_form": "휴지로 봤제 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.14",
          "form": "보험 돈만 맨날내고 뭐 제대로 받아먹지도 못하고 ㅋㅋㅋㅋ",
          "original_form": "보험 돈만 맨날내고 뭐 제대로 받아먹지도 못하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.15",
          "form": "쓰 가 안되네",
          "original_form": "쓰 가 안되네",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.16",
          "form": "휴지 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "휴지 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.17",
          "form": "아욱겨라 ㅋㅋㅋㅋㅋ",
          "original_form": "아욱겨라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.18",
          "form": "어 안 된단다 그건",
          "original_form": "어 안 된단다 그건",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012468.1.19",
          "form": "너무 제약이 많네요 우리에겐 ㅋㅋㅋㅋㅋ",
          "original_form": "너무 제약이 많네요 우리에겐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012468.1.20",
          "form": "그래 일상적인 대화를 할 수가 없다",
          "original_form": "그래 일상적인 대화를 할 수가 없다",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012468.1.21",
          "form": "우리 투 보험도 진짜 안 좋아",
          "original_form": "우리 투 보험도 진짜 안 좋아",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012468.1.22",
          "form": "비급여는 전부 안됨",
          "original_form": "비급여는 전부 안됨",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012468.1.23",
          "form": "보험이 갈수록 진짜별로ㅠ",
          "original_form": "보험이 갈수록 진짜별로ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.24",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.25",
          "form": "우리꺼도 그래요ㅠ",
          "original_form": "우리꺼도 그래요ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.26",
          "form": "원이랑 똑같은 거 들었는데도",
          "original_form": "원이랑 똑같은 거 들었는데도",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.27",
          "form": "이건뭐 솔직히 받을수있는게ㅜ없음..",
          "original_form": "이건뭐 솔직히 받을수있는게ㅜ없음..",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.28",
          "form": "원은 비급여 되는데 투는 안됨",
          "original_form": "원은 비급여 되는데 투는 안됨",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.29",
          "form": "돈은 더 비싸고ㅠ",
          "original_form": "돈은 더 비싸고ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.30",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.31",
          "form": "입원해도 입원실비 없으니까 거의 반도 안나오고",
          "original_form": "입원해도 입원실비 없으니까 거의 반도 안나오고",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012468.1.32",
          "form": "돈아까운데..없음 불안하고ㅠ",
          "original_form": "돈아까운데..없음 불안하고ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012468.1.33",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012468.1.34",
          "form": "근데 진짜 내는 게 더 너무 많음",
          "original_form": "근데 진짜 내는 게 더 너무 많음",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100012468.1.35",
          "form": "언니 난 이만 퇴근합니다~",
          "original_form": "언니 난 이만 퇴근합니다~",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100012468.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋ오늘도 수고 많으셨으요^^",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오늘도 수고 많으셨으요^^",
