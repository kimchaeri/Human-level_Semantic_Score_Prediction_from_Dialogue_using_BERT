      "utterance": [
        {
          "id": "MDRW2100042161.1.1",
          "form": "나는 치킨먹으러 가고싶었는데 솔직히 지금 생각하면 안 간게 다행인거 같아",
          "original_form": "나는 치킨먹으러 가고싶었는데 솔직히 지금 생각하면 안 간게 다행인거 같아",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042161.1.2",
          "form": "오늘 날씨 어때요?",
          "original_form": "오늘 날씨 어때요?",
          "speaker_id": "2",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042161.1.3",
          "form": "쌀쌀하고 흐리네요ㅠㅠ",
          "original_form": "쌀쌀하고 흐리네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042161.1.4",
          "form": "시원한 건 좋지만 비가 오는 건 싫어요",
          "original_form": "시원한 건 좋지만 비가 오는 건 싫어요",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042161.1.5",
          "form": "맞아요 여기는 흐리고 후텁지근 해요",
          "original_form": "맞아요 여기는 흐리고 후텁지근 해요",
          "speaker_id": "2",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042161.1.6",
          "form": "어떤 계절을 좋아해요?",
          "original_form": "어떤 계절을 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 14:11"
        },
        {
          "id": "MDRW2100042161.1.7",
          "form": "저는 겨울 좋아해요",
          "original_form": "저는 겨울 좋아해요",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042161.1.8",
          "form": "땀나는 거 너무 싫어해서요ㅠㅠ",
          "original_form": "땀나는 거 너무 싫어해서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042161.1.9",
          "form": "와 저도요!!",
          "original_form": "와 저도요!!",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.10",
          "form": "여름 싫어해요ㅠㅠ",
          "original_form": "여름 싫어해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.11",
          "form": "가을겨울이 좋죠??",
          "original_form": "가을겨울이 좋죠??",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.12",
          "form": "겨울에 찬바람 맞는거 너무 좋지 않나요",
          "original_form": "겨울에 찬바람 맞는거 너무 좋지 않나요",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.13",
          "form": "진심 여름 길어져서 짜증날때쯤 시웡헤졌어요",
          "original_form": "진심 여름 길어져서 짜증날때쯤 시웡헤졌어요",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.14",
          "form": "저도요 그 스산함 차분함 좋죠",
          "original_form": "저도요 그 스산함 차분함 좋죠",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.15",
          "form": "네... 옷 입는것도 뭔가 가을겨울이 좋고",
          "original_form": "네... 옷 입는것도 뭔가 가을겨울이 좋고",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042161.1.17",
          "form": "셔츠만 입어도ㅠ이쁜데",
          "original_form": "셔츠만 입어도ㅠ이쁜데",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042161.1.18",
          "form": "여름은 뭘 입어도 후줄근 하고 땀나고 ㅌㅋㅋ",
          "original_form": "여름은 뭘 입어도 후줄근 하고 땀나고 ㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042161.1.19",
          "form": "못생겨져요",
          "original_form": "못생겨져요",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042161.1.20",
          "form": "맞아요ㅋㅋㅋㅋㅋ 전 더운 게 너무 싫어서 여름옷은 그냥 기능성만 보고 사요...",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ 전 더운 게 너무 싫어서 여름옷은 그냥 기능성만 보고 사요...",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042161.1.21",
          "form": "네네 저도 그래요",
          "original_form": "네네 저도 그래요",
          "speaker_id": "2",
          "time": "20210907 15:13"
        }
      ]
