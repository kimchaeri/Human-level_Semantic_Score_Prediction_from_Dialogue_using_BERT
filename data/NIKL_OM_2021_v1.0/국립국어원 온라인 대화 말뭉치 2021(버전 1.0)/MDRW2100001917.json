      "utterance": [
        {
          "id": "MDRW2100001917.1.1",
          "form": "나 오늘 여름 투피스 왔는데",
          "original_form": "나 오늘 여름 투피스 왔는데",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001917.1.2",
          "form": "어디서 샀어?",
          "original_form": "어디서 샀어?",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001917.1.3",
          "form": "이랜드몰에서 여름특가로 투피스",
          "original_form": "이랜드몰에서 여름특가로 투피스",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001917.1.4",
          "form": "저렴하게 구입했겠네? 어떻는데?",
          "original_form": "저렴하게 구입했겠네? 어떻는데?",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001917.1.5",
          "form": "언니말로는 2%부족하다는데",
          "original_form": "언니말로는 2%부족하다는데",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001917.1.6",
          "form": "2프로 부족? 어떻길래?",
          "original_form": "2프로 부족? 어떻길래?",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001917.1.7",
          "form": "투피스 9만원인데 15,000원에 사서 2%부족해도 된다고 했다",
          "original_form": "투피스 9만원인데 15,000원에 사서 2%부족해도 된다고 했다",
          "speaker_id": "1",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001917.1.8",
          "form": "엄청 저렴하게 샀네..  그래 부족해도 되겠다",
          "original_form": "엄청 저렴하게 샀네..  그래 부족해도 되겠다",
          "speaker_id": "2",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001917.1.9",
          "form": "그렇제? 그냥 입다 버리고 ㅋㅋ",
          "original_form": "그렇제? 그냥 입다 버리고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001917.1.10",
          "form": "그래 한철 입고 버려도 되지ㅋ",
          "original_form": "그래 한철 입고 버려도 되지ㅋ",
          "speaker_id": "2",
          "time": "20210819 22:20"
        },
        {
          "id": "MDRW2100001917.1.11",
          "form": "올 여름가기전에 한번은 입어야할텐데",
          "original_form": "올 여름가기전에 한번은 입어야할텐데",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001917.1.12",
          "form": "지금 입고 다녀라",
          "original_form": "지금 입고 다녀라",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001917.1.13",
          "form": "밖에 나가야지 입고 나가지",
          "original_form": "밖에 나가야지 입고 나가지",
