      "utterance": [
        {
          "id": "MDRW2100043781.1.1",
          "form": "영양제드세용",
          "original_form": "영양제드세용",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043781.1.2",
          "form": "영양제 비타민 디만 먹어요",
          "original_form": "영양제 비타민 디만 먹어요",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043781.1.3",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043781.1.4",
          "form": "저는 유산균이요",
          "original_form": "저는 유산균이요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043781.1.5",
          "form": "유산균 먹으니까 효과 있는거 같으신가요??",
          "original_form": "유산균 먹으니까 효과 있는거 같으신가요??",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043781.1.6",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043781.1.7",
          "form": "플라시보죵..",
          "original_form": "플라시보죵..",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043781.1.8",
          "form": "ㅋㅋㅋ 아 그래도 있으신가보네요",
          "original_form": "ㅋㅋㅋ 아 그래도 있으신가보네요",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043781.1.9",
          "form": "전 비타민 디 먹어도 없어요 ㅋㅋㅋ",
          "original_form": "전 비타민 디 먹어도 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043781.1.10",
          "form": "ㅎㅎ그거왜드세요 뼈약하신가요",
          "original_form": "ㅎㅎ그거왜드세요 뼈약하신가요",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043781.1.11",
          "form": "네 그래서 먹기 시작했어요",
          "original_form": "네 그래서 먹기 시작했어요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043781.1.12",
          "form": "햇빛많이못보시나요",
          "original_form": "햇빛많이못보시나요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043781.1.13",
          "form": "맨날 컴퓨터 앞에 있기도 하고",
          "original_form": "맨날 컴퓨터 앞에 있기도 하고",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043781.1.14",
          "form": "어디에 좋다고 해서 먹기 시작했는데",
          "original_form": "어디에 좋다고 해서 먹기 시작했는데",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043781.1.15",
          "form": "기억이 잘 안나네요 ㅋㅋㅋㅋ",
          "original_form": "기억이 잘 안나네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043781.1.16",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043781.1.17",
          "form": "뼈에좋죵",
          "original_form": "뼈에좋죵",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043781.1.18",
          "form": "그랬던거 같은데 ㅋㅋㅋㅋ",
          "original_form": "그랬던거 같은데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043781.1.19",
          "form": "ㅎㅎ운동하시나요",
          "original_form": "ㅎㅎ운동하시나요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043781.1.20",
          "form": "ㅎㅎ 햇빛 안받는것보다도 운동을 하지 않는게 문제같기도",
          "original_form": "ㅎㅎ 햇빛 안받는것보다도 운동을 하지 않는게 문제같기도",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043781.1.21",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043781.1.22",
