          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027508.1.4",
          "form": "나 사고싶은거있어",
          "original_form": "나 사고싶은거있어",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027508.1.5",
          "form": "이니스프리 노세범 선크림",
          "original_form": "이니스프리 노세범 선크림",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027508.1.6",
          "form": "사줘",
          "original_form": "사줘",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027508.1.7",
          "form": "어디다 쓰는데",
          "original_form": "어디다 쓰는데",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027508.1.8",
          "form": "얼굴에",
          "original_form": "얼굴에",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.9",
          "form": "저번에도 삿잖아?",
          "original_form": "저번에도 삿잖아?",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.10",
          "form": "거의 다 썼어",
          "original_form": "거의 다 썼어",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.11",
          "form": "모르겠구 사고싶은게 너무 많아..",
          "original_form": "모르겠구 사고싶은게 너무 많아..",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.12",
          "form": "친구 생일선물도 사야하는데..",
          "original_form": "친구 생일선물도 사야하는데..",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.13",
          "form": "선크림 없어두 잘살아!",
          "original_form": "선크림 없어두 잘살아!",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.14",
          "form": "생일선물로는 뭐를 사야할까?",
          "original_form": "생일선물로는 뭐를 사야할까?",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.15",
          "form": "생일선물 뭐사줄지 물어봣어?",
          "original_form": "생일선물 뭐사줄지 물어봣어?",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.16",
          "form": "직접 물어보고 사줘",
          "original_form": "직접 물어보고 사줘",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.17",
          "form": "그럼 편하잖아",
          "original_form": "그럼 편하잖아",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.18",
          "form": "10대 청소년이 좋아하는 생일선물이 뭐지?",
          "original_form": "10대 청소년이 좋아하는 생일선물이 뭐지?",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.19",
          "form": "그럼 노잼이잖아",
          "original_form": "그럼 노잼이잖아",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.20",
          "form": "너가 좋아하는게 뭔지 생각해봐",
          "original_form": "너가 좋아하는게 뭔지 생각해봐",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027508.1.21",
          "form": "먹을게 좋겟지",
          "original_form": "먹을게 좋겟지",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.22",
          "form": "초밥세트 하나 사다주면",
          "original_form": "초밥세트 하나 사다주면",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.23",
          "form": "생일선물인데두?",
          "original_form": "생일선물인데두?",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.24",
          "form": "행복할거같아.",
          "original_form": "행복할거같아.",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.25",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.26",
          "form": "10000원짜리 인형보다는",
          "original_form": "10000원짜리 인형보다는",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.27",
          "form": "10000원어치 초밥이지",
          "original_form": "10000원어치 초밥이지",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.28",
          "form": "먹을거는 잠깐의 행복이야",
          "original_form": "먹을거는 잠깐의 행복이야",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.29",
          "form": "그럼 됐지",
          "original_form": "그럼 됐지",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.30",
          "form": "인형사면 몇년동안 데굴데굴 굴릴 수 잇어",
          "original_form": "인형사면 몇년동안 데굴데굴 굴릴 수 잇어",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027508.1.31",
          "form": "하지만 금방질리고",
          "original_form": "하지만 금방질리고",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.32",
          "form": "중고로 팔게될꺼야",
          "original_form": "중고로 팔게될꺼야",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.33",
          "form": "너무해..",
          "original_form": "너무해..",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.34",
          "form": "근데 그거 알아?",
          "original_form": "근데 그거 알아?",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.35",
          "form": "요기요에서",
          "original_form": "요기요에서",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.36",
          "form": "만원 할인쿠폰준대",
          "original_form": "만원 할인쿠폰준대",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.37",
          "form": "알겠지 모",
          "original_form": "알겠지 모",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.38",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027508.1.39",
          "form": "많이 써",
          "original_form": "많이 써",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027508.1.40",
