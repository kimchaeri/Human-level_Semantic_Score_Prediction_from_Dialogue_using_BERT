      "utterance": [
        {
          "id": "MDRW2100031569.1.1",
          "form": "피부관리좀 해야할거같아요 ㅠ",
          "original_form": "피부관리좀 해야할거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100031569.1.2",
          "form": "갑자기 피부관리야 무슨",
          "original_form": "갑자기 피부관리야 무슨",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100031569.1.3",
          "form": "저 여드름 너무 심해가지고 ㅠㅠ",
          "original_form": "저 여드름 너무 심해가지고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100031569.1.4",
          "form": "피부과 갔니",
          "original_form": "피부과 갔니",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100031569.1.5",
          "form": "아뇨 ㅎ 돈없어가지구...",
          "original_form": "아뇨 ㅎ 돈없어가지구...",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100031569.1.6",
          "form": "그래도 병원은 가야지",
          "original_form": "그래도 병원은 가야지",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100031569.1.7",
          "form": "음 그래두 놔두면 나을까싶어서",
          "original_form": "음 그래두 놔두면 나을까싶어서",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100031569.1.8",
          "form": "잘 바르고 있어? 로션이랑",
          "original_form": "잘 바르고 있어? 로션이랑",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100031569.1.9",
          "form": "당근이죠 근데 저랑 잘 안맞나봐요 ㅋㅋㅋㅠ",
          "original_form": "당근이죠 근데 저랑 잘 안맞나봐요 ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100031569.1.10",
          "form": "어떡하냐",
          "original_form": "어떡하냐",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100031569.1.11",
          "form": "엄마는 그거 잘 바르고 계세요? 제가 드린거",
          "original_form": "엄마는 그거 잘 바르고 계세요? 제가 드린거",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100031569.1.12",
          "form": "이름 잘 생각 안나는데",
          "original_form": "이름 잘 생각 안나는데",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100031569.1.13",
          "form": "레티놀??",
          "original_form": "레티놀??",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100031569.1.14",
          "form": "네 그거요 ㅋㅋㅋ 괜탆나요",
          "original_form": "네 그거요 ㅋㅋㅋ 괜탆나요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100031569.1.15",
          "form": "기미 꽤 줄어든거같아 볼래?",
          "original_form": "기미 꽤 줄어든거같아 볼래?",
          "speaker_id": "2",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100031569.1.16",
          "form": "네 찍어주셍요",
          "original_form": "네 찍어주셍요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100031569.1.17",
          "form": "보냈다",
          "original_form": "보냈다",
          "speaker_id": "2",
          "time": "20210906 14:32"
        }
      ]
