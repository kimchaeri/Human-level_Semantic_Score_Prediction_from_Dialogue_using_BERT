      "utterance": [
        {
          "id": "MDRW2100016622.1.1",
          "form": "설거지는 왜이리 해도해도 끝이없지",
          "original_form": "설거지는 왜이리 해도해도 끝이없지",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016622.1.2",
          "form": "식기세척기 사고싶어",
          "original_form": "식기세척기 사고싶어",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016622.1.3",
          "form": "ㅎ..저두",
          "original_form": "ㅎ..저두",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016622.1.4",
          "form": "로봇청소기도..",
          "original_form": "로봇청소기도..",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016622.1.5",
          "form": "로봇청소기는 문틀이 없어야한다며?",
          "original_form": "로봇청소기는 문틀이 없어야한다며?",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016622.1.6",
          "form": "헐 턱에 걸려서 못넘어가나",
          "original_form": "헐 턱에 걸려서 못넘어가나",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016622.1.7",
          "form": "응 그래서 문틀있는 집들은 사도 무용지물이래",
          "original_form": "응 그래서 문틀있는 집들은 사도 무용지물이래",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016622.1.8",
          "form": "헐...힘내서 넘어주라..",
          "original_form": "헐...힘내서 넘어주라..",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016622.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016622.1.10",
          "form": "사려고했는데 포기각이네",
          "original_form": "사려고했는데 포기각이네",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016622.1.11",
          "form": "문틀을 없애는 방법이 있습니다",
          "original_form": "문틀을 없애는 방법이 있습니다",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016622.1.12",
          "form": "그런데 그런 가전들 다 사면 집안일 좀 쉬워질것같아 ",
          "original_form": "그런데 그런 가전들 다 사면 집안일 좀 쉬워질것같아 ",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016622.1.13",
          "form": "ㅎㅎ....장판다시깔아야하잖아요",
          "original_form": "ㅎㅎ....장판다시깔아야하잖아요",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016622.1.14",
          "form": "일이2배",
          "original_form": "일이2배",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016622.1.15",
          "form": "그럼 열심히 청소를 하자^^",
          "original_form": "그럼 열심히 청소를 하자^^",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016622.1.16",
          "form": "로봇청소기는 또미 사줘",
          "original_form": "로봇청소기는 또미 사줘",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016622.1.17",
          "form": "ㅋㅋㅋㅎㅎ?",
          "original_form": "ㅋㅋㅋㅎㅎ?",
          "speaker_id": "2",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016622.1.18",
          "form": "날강도네요",
          "original_form": "날강도네요",
          "speaker_id": "2",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016622.1.19",
          "form": "아버지도 맞으셨다햤나??",
          "original_form": "아버지도 맞으셨다햤나??",
          "speaker_id": "2",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016622.1.20",
          "form": "다같이 쉬는중?",
          "original_form": "다같이 쉬는중?",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016622.1.21",
          "form": "아빠는..고추 약주는 즁...",
          "original_form": "아빠는..고추 약주는 즁...",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016622.1.22",
          "form": "또미만 누워있어",
          "original_form": "또미만 누워있어",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016622.1.23",
          "form": "어머니아버지는 다 수확 하셨니?",
          "original_form": "어머니아버지는 다 수확 하셨니?",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016622.1.24",
          "form": "ㅋㅋㅋ휴..왜너만 쉬냐",
          "original_form": "ㅋㅋㅋ휴..왜너만 쉬냐",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016622.1.25",
          "form": "아니 요새 맨날 밭에가심",
          "original_form": "아니 요새 맨날 밭에가심",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.26",
          "form": "ㅋㅋㅋ지금 젤 바쁠철이짘ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ지금 젤 바쁠철이짘ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.27",
          "form": "어제도 고구마먹었어..그랴서",
          "original_form": "어제도 고구마먹었어..그랴서",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.28",
          "form": "왜냐면 내 여름휴가기때문에",
          "original_form": "왜냐면 내 여름휴가기때문에",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.29",
          "form": "밤인지 고구만지 ..",
          "original_form": "밤인지 고구만지 ..",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.30",
          "form": "엥 벌써 수확하셔?",
          "original_form": "엥 벌써 수확하셔?",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.31",
          "form": "ㅋㅋㅋ웅몇개가져왔던데",
          "original_form": "ㅋㅋㅋ웅몇개가져왔던데",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.32",
          "form": "울집은.아직...",
          "original_form": "울집은.아직...",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.33",
          "form": "이틀째 에후기 돌리는중",
          "original_form": "이틀째 에후기 돌리는중",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.34",
          "form": "아.몇개만 하셨나보다",
          "original_form": "아.몇개만 하셨나보다",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016622.1.35",
          "form": "오늘 본가간다",
          "original_form": "오늘 본가간다",
          "speaker_id": "1",
          "time": "20210903 10:50"
        }
      ]
