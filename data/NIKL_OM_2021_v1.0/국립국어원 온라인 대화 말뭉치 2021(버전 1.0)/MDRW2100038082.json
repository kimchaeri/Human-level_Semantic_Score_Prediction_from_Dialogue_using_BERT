      "utterance": [
        {
          "id": "MDRW2100038082.1.1",
          "form": "키우는 동물 있어요?",
          "original_form": "키우는 동물 있어요?",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038082.1.2",
          "form": "아니오,, ㅠ name1님은요?",
          "original_form": "아니오,, ㅠ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038082.1.3",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038082.1.4",
          "form": "키우고싶은 동물은 있네요",
          "original_form": "키우고싶은 동물은 있네요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038082.1.5",
          "form": "요즘은 전세난이라 애완동물이 있음",
          "original_form": "요즘은 전세난이라 애완동물이 있음",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038082.1.6",
          "form": "이사도힘들고 그래서 당분간은 못키워요",
          "original_form": "이사도힘들고 그래서 당분간은 못키워요",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038082.1.7",
          "form": "키울수 있는 동물은 햄스터 정도,,ㅋㅋ",
          "original_form": "키울수 있는 동물은 햄스터 정도,,ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038082.1.8",
          "form": "앗ㅋㅋㅋ하긴 원룸에서 애완동물은 좀 그러니까요",
          "original_form": "앗ㅋㅋㅋ하긴 원룸에서 애완동물은 좀 그러니까요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038082.1.9",
          "form": "잘 생각하시는것같아요",
          "original_form": "잘 생각하시는것같아요",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038082.1.10",
          "form": "제 주변에는 원룸에서도 기르긴하는데",
          "original_form": "제 주변에는 원룸에서도 기르긴하는데",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038082.1.11",
          "form": "마냥 좋아보이진 않더라구요..",
          "original_form": "마냥 좋아보이진 않더라구요..",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100038082.1.12",
          "form": "고양이들은 어찌 어찌 살긴 하는데",
          "original_form": "고양이들은 어찌 어찌 살긴 하는데",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038082.1.13",
          "form": "사람도 동물도 좀 괴롭죠",
          "original_form": "사람도 동물도 좀 괴롭죠",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038082.1.14",
          "form": "원룸 개는 진짜 넌무하구요",
          "original_form": "원룸 개는 진짜 넌무하구요",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100038082.1.15",
          "form": "원룸 개는 진짜ㅋㅋ.. 너무 심한거 같아요",
          "original_form": "원룸 개는 진짜ㅋㅋ.. 너무 심한거 같아요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038082.1.16",
          "form": "저는 고양이 키우는 친구 몇 있는데",
          "original_form": "저는 고양이 키우는 친구 몇 있는데",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038082.1.17",
          "form": "고양이가 스트레스받는게 좀 보여요..",
          "original_form": "고양이가 스트레스받는게 좀 보여요..",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038082.1.18",
          "form": "일단 환기가 안되구",
          "original_form": "일단 환기가 안되구",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038082.1.19",
          "form": "캣타워 해놔도 똥냄새가 안빠져요",
          "original_form": "캣타워 해놔도 똥냄새가 안빠져요",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038082.1.20",
          "form": "공간도 좁아서 애들이 그냥 누워만 있더라구요",
          "original_form": "공간도 좁아서 애들이 그냥 누워만 있더라구요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038082.1.21",
          "form": "그래도 캣타워는 좀 안타나요?",
          "original_form": "그래도 캣타워는 좀 안타나요?",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038082.1.22",
          "form": "수직공간만 있음 된다고 하던데 그짓부렁이었는지,,",
          "original_form": "수직공간만 있음 된다고 하던데 그짓부렁이었는지,,",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038082.1.23",
          "form": "제 친구들 집은 원룸중에서도 좁은 편이라서요",
          "original_form": "제 친구들 집은 원룸중에서도 좁은 편이라서요",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038082.1.24",
          "form": "조금 큰 원룸은 괜찮을것 같기도 해요",
          "original_form": "조금 큰 원룸은 괜찮을것 같기도 해요",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038082.1.25",
          "form": "일쩜오름이오두 뭐,, 근데 나이든 고양이면",
          "original_form": "일쩜오름이오두 뭐,, 근데 나이든 고양이면",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038082.1.26",
          "form": "활동을 안하니 괜찮을거소 같아요",
          "original_form": "활동을 안하니 괜찮을거소 같아요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038082.1.27",
          "form": "맞아요 나이든 고양이는 오히려",
          "original_form": "맞아요 나이든 고양이는 오히려",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038082.1.28",
          "form": "좁은곳이 더 나을수도 있을거같아요ㅎㅎ",
          "original_form": "좁은곳이 더 나을수도 있을거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:25"
        }
      ]
