      "utterance": [
        {
          "id": "MDRW2100003888.1.1",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003888.1.2",
          "form": "몇시에 보러가세요",
          "original_form": "몇시에 보러가세요",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003888.1.3",
          "form": "흠 한 3.4시쯤 시간보고 예약하려구용",
          "original_form": "흠 한 3.4시쯤 시간보고 예약하려구용",
          "speaker_id": "1",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003888.1.4",
          "form": "혼자보세요?",
          "original_form": "혼자보세요?",
          "speaker_id": "2",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003888.1.5",
          "form": "오늘 비 많이 오던데",
          "original_form": "오늘 비 많이 오던데",
          "speaker_id": "2",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003888.1.6",
          "form": "아뇨아뇨 친구랑요 ~",
          "original_form": "아뇨아뇨 친구랑요 ~",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003888.1.7",
          "form": "오다가 지금은 그친거같아요!",
          "original_form": "오다가 지금은 그친거같아요!",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003888.1.8",
          "form": "영화 보고나면 후기 알려주세요",
          "original_form": "영화 보고나면 후기 알려주세요",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003888.1.9",
          "form": "신파없이 잘 만들었다는 후기가 많더라구요",
          "original_form": "신파없이 잘 만들었다는 후기가 많더라구요",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003888.1.10",
          "form": "네네 ㅎㅎ 최근에 보신영화 있으세요?",
          "original_form": "네네 ㅎㅎ 최근에 보신영화 있으세요?",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003888.1.11",
          "form": "저 최근에 정직한후보봤어요",
          "original_form": "저 최근에 정직한후보봤어요",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003888.1.12",
          "form": "혹시 보셨나요",
          "original_form": "혹시 보셨나요",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003888.1.13",
          "form": "아 봤어요 ㅋㅋ그거웃기죠",
          "original_form": "아 봤어요 ㅋㅋ그거웃기죠",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003888.1.14",
          "form": "라미란씨  감칠난 연기를 잘하더라구요",
          "original_form": "라미란씨  감칠난 연기를 잘하더라구요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003888.1.15",
          "form": "이런 코미디 영화는 부담이 없어서 좋은거같아요",
          "original_form": "이런 코미디 영화는 부담이 없어서 좋은거같아요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003888.1.16",
          "form": "네ㅋㅋ 라미란씨 좋아용",
          "original_form": "네ㅋㅋ 라미란씨 좋아용",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003888.1.17",
          "form": "가벼운 영화 좋아해요 저둥",
          "original_form": "가벼운 영화 좋아해요 저둥",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003888.1.18",
          "form": "딱 시간떼우기도 좋고",
          "original_form": "딱 시간떼우기도 좋고",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003888.1.19",
          "form": "그래서 더 좋더라구요",
          "original_form": "그래서 더 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003888.1.20",
          "form": "맞아요 진짜 ㅋㅋ",
          "original_form": "맞아요 진짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003888.1.21",
          "form": "콘서트도 가보고싶네용",
          "original_form": "콘서트도 가보고싶네용",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003888.1.22",
          "form": "아 저두요ㅠㅠ",
          "original_form": "아 저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100003888.1.23",
          "form": "코로나끝나면 가고싶어요",
