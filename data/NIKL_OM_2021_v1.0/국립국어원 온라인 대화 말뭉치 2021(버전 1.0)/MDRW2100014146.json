      "utterance": [
        {
          "id": "MDRW2100014146.1.1",
          "form": "강원도 철원 말하는거지",
          "original_form": "강원도 철원 말하는거지",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014146.1.2",
          "form": "응 거기에 한탕강이 있는데 한국의 그랜드캐니언이라고 불린다",
          "original_form": "응 거기에 한탕강이 있는데 한국의 그랜드캐니언이라고 불린다",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014146.1.3",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014146.1.4",
          "form": "엉 근데 폭은 긴데 높이가 아주 많이 짧다",
          "original_form": "엉 근데 폭은 긴데 높이가 아주 많이 짧다",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014146.1.5",
          "form": "ㅋ ㅋ그게 뭐야",
          "original_form": "ㅋ ㅋ그게 뭐야",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014146.1.6",
          "form": "가까이 송소대라고 있는 거기서 한탕강이 젤 잘 보인데",
          "original_form": "가까이 송소대라고 있는 거기서 한탕강이 젤 잘 보인데",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014146.1.7",
          "form": "아 그럼 송소대에서 사진을 찍어야겠네",
          "original_form": "아 그럼 송소대에서 사진을 찍어야겠네",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014146.1.8",
          "form": "엉 잘하면 인생사지느을 건질수 있지",
          "original_form": "엉 잘하면 인생사지느을 건질수 있지",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014146.1.9",
          "form": "우리나라에도 있다고 하니 신기하다",
          "original_form": "우리나라에도 있다고 하니 신기하다",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014146.1.10",
          "form": "그러게 우리나라도 구석구석 다니면 볼게 많을듯해",
          "original_form": "그러게 우리나라도 구석구석 다니면 볼게 많을듯해",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014146.1.11",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014146.1.12",
          "form": "근데 다들 외국에 많이 나가니",
          "original_form": "근데 다들 외국에 많이 나가니",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014146.1.13",
          "form": "어쩔수 없지 뭐",
          "original_form": "어쩔수 없지 뭐",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014146.1.14",
          "form": "우리나라 한반도 지형을 닯았다고 해서 한반도 폭포라고 불리는 곳이 있데",
          "original_form": "우리나라 한반도 지형을 닯았다고 해서 한반도 폭포라고 불리는 곳이 있데",
          "speaker_id": "1",
          "time": "20210825 17:35"
        }
