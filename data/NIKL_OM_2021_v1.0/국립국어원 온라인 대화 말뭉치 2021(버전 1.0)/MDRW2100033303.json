      "utterance": [
        {
          "id": "MDRW2100033303.1.1",
          "form": "암튼 언니가 전에도 말했지만 코로나는 완전히 종식되지 않고",
          "original_form": "암튼 언니가 전에도 말했지만 코로나는 완전히 종식되지 않고",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033303.1.2",
          "form": "우리 지역은 9월 1일부터인가 3단계로",
          "original_form": "우리 지역은 9월 1일부터인가 3단계로",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033303.1.3",
          "form": "누구나 걸리는 감기처럼",
          "original_form": "누구나 걸리는 감기처럼",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033303.1.4",
          "form": "잇을 거야",
          "original_form": "잇을 거야",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033303.1.5",
          "form": "바꼈어",
          "original_form": "바꼈어",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033303.1.6",
          "form": "마장",
          "original_form": "마장",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033303.1.7",
          "form": "이미 마스크 벗은 나라들도 많고",
          "original_form": "이미 마스크 벗은 나라들도 많고",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033303.1.8",
          "form": "유럽은 치사하더라",
          "original_form": "유럽은 치사하더라",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033303.1.9",
          "form": "지들이 방역 잘 안지켜서",
          "original_form": "지들이 방역 잘 안지켜서",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033303.1.10",
          "form": "백신때문에?",
          "original_form": "백신때문에?",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033303.1.11",
          "form": "변이에 기여해놓고",
          "original_form": "변이에 기여해놓고",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033303.1.12",
          "form": "백신 혼자서 다 챙기고",
          "original_form": "백신 혼자서 다 챙기고",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100033303.1.13",
          "form": "그니까..",
          "original_form": "그니까..",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.14",
          "form": "결국 백신은 공중 보건을 위한 건데",
          "original_form": "결국 백신은 공중 보건을 위한 건데",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.15",
          "form": "못맞는 나라가 있으면",
          "original_form": "못맞는 나라가 있으면",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.16",
          "form": "공중 보건에 위해가 가서",
          "original_form": "공중 보건에 위해가 가서",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.17",
          "form": "지들한테도 안좋은 건데 말야",
          "original_form": "지들한테도 안좋은 건데 말야",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.18",
          "form": "암튼 코로나 이후로 유럽이 너무너무 싫어졌어",
          "original_form": "암튼 코로나 이후로 유럽이 너무너무 싫어졌어",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.19",
          "form": "요즘에 글고 한국에서",
          "original_form": "요즘에 글고 한국에서",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.20",
          "form": "아니 한국이 아닌가?",
          "original_form": "아니 한국이 아닌가?",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.21",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033303.1.22",
          "form": "암튼 유럽에 여행가는 사람등 많대",
          "original_form": "암튼 유럽에 여행가는 사람등 많대",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.23",
          "form": "가서 격리 안해도 되니까",
          "original_form": "가서 격리 안해도 되니까",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.24",
          "form": "엥 진짜?",
          "original_form": "엥 진짜?",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.25",
          "form": "갈 수 있어???",
          "original_form": "갈 수 있어???",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.26",
          "form": "헐라링",
          "original_form": "헐라링",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.27",
          "form": "그리고 한국에서 접종 끝낸 뒤 가면",
          "original_form": "그리고 한국에서 접종 끝낸 뒤 가면",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.28",
          "form": "귀국해서도 격리 안해도 되거든",
          "original_form": "귀국해서도 격리 안해도 되거든",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.29",
          "form": "웃긴다.. 어디는 코로나 방역에 열심히인데",
          "original_form": "웃긴다.. 어디는 코로나 방역에 열심히인데",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.30",
          "form": "누구는 열심히 놀러 다니네",
          "original_form": "누구는 열심히 놀러 다니네",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100033303.1.31",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.32",
          "form": "빨리 사람들이 접종 다 하고",
          "original_form": "빨리 사람들이 접종 다 하고",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.33",
          "form": "우리도 더 이상 3단계 4단계 이런 거 없이",
          "original_form": "우리도 더 이상 3단계 4단계 이런 거 없이",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.34",
          "form": "돌아다녓음 조켓다ㅜㅜ",
          "original_form": "돌아다녓음 조켓다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.35",
          "form": "그러게 ㅜㅠㅠ",
          "original_form": "그러게 ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.36",
          "form": "생각보다 개인 위생만 잘 하면",
          "original_form": "생각보다 개인 위생만 잘 하면",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.37",
          "form": "쉽게 안걸리던데",
          "original_form": "쉽게 안걸리던데",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100033303.1.38",
          "form": "오히려 걸리기가 더 어려운 것 같애",
          "original_form": "오히려 걸리기가 더 어려운 것 같애",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100033303.1.39",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100033303.1.40",
          "form": "나는 언니가 저번에 코로나 확진자랑 밥 먹고도 안걸려서",
          "original_form": "나는 언니가 저번에 코로나 확진자랑 밥 먹고도 안걸려서",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100033303.1.41",
          "form": "엄청 신기했어",
          "original_form": "엄청 신기했어",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100033303.1.42",
          "form": "물론 다행인 일이지만!!",
          "original_form": "물론 다행인 일이지만!!",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100033303.1.43",
          "form": "내말이... 사실 나도 놀랏어",
          "original_form": "내말이... 사실 나도 놀랏어",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이번 연구실 교수님은 코로나에 민감해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이번 연구실 교수님은 코로나에 민감해서",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.45",
          "form": "타지역 갈 때마다 검사 받아야 한대",
          "original_form": "타지역 갈 때마다 검사 받아야 한대",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.46",
          "form": "자가키트로 하거나",
          "original_form": "자가키트로 하거나",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.47",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.48",
          "form": "타지역에서 자고 오면",
          "original_form": "타지역에서 자고 오면",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.49",
          "form": "pcr검사까지...",
          "original_form": "pcr검사까지...",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.50",
          "form": "근데 사실 그렇게만 사람들이 해도",
          "original_form": "근데 사실 그렇게만 사람들이 해도",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.51",
          "form": "나 매번 코 찌르기 싫은뎅",
          "original_form": "나 매번 코 찌르기 싫은뎅",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100033303.1.52",
          "form": "확진자가 늘지는 않을것같은데",
          "original_form": "확진자가 늘지는 않을것같은데",
          "speaker_id": "2",
