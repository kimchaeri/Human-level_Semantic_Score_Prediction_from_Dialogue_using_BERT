      "utterance": [
        {
          "id": "MDRW2100018161.1.1",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018161.1.2",
          "form": "엄청 신기하게 생겼던데",
          "original_form": "엄청 신기하게 생겼던데",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018161.1.3",
          "form": "좀 징그럽게 생겼어",
          "original_form": "좀 징그럽게 생겼어",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018161.1.4",
          "form": "많이 징그럽던데?",
          "original_form": "많이 징그럽던데?",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018161.1.5",
          "form": "맞아 근데 귀여운 것도 있어",
          "original_form": "맞아 근데 귀여운 것도 있어",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018161.1.6",
          "form": "귀여운 괴물도 있어?",
          "original_form": "귀여운 괴물도 있어?",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018161.1.7",
          "form": "응 연근이 프로틴 괴물이랑",
          "original_form": "응 연근이 프로틴 괴물이랑",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018161.1.8",
          "form": "프로틴? 단백질?",
          "original_form": "프로틴? 단백질?",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018161.1.9",
          "form": "근육맨",
          "original_form": "근육맨",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018161.1.10",
          "form": "ㅋㅋㅋ그거 당나귀귀 나온 사람이 연기하는 거 아냐?",
          "original_form": "ㅋㅋㅋ그거 당나귀귀 나온 사람이 연기하는 거 아냐?",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018161.1.11",
          "form": "당나귀 귀에 나온 사람 누구?",
          "original_form": "당나귀 귀에 나온 사람 누구?",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018161.1.12",
          "form": "광재인가? 박광재?",
          "original_form": "광재인가? 박광재?",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018161.1.13",
          "form": "그 사람 누군지 잘 몰라",
          "original_form": "그 사람 누군지 잘 몰라",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018161.1.14",
          "form": "그래 그 괴물이 근데 귀여워?",
          "original_form": "그래 그 괴물이 근데 귀여워?",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018161.1.15",
          "form": "그거 엄청 덩치 큰 괴물 아냐?",
          "original_form": "그거 엄청 덩치 큰 괴물 아냐?",
          "speaker_id": "1",
          "time": "20210826 23:04"
        }
