      "utterance": [
        {
          "id": "MDRW2100042737.1.1",
          "form": "점심 식사하시나요?",
          "original_form": "점심 식사하시나요?",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042737.1.2",
          "form": "네 점심 먹었어요! name2 님도 드셨나요?!",
          "original_form": "네 점심 먹었어요! &name2& 님도 드셨나요?!",
          "speaker_id": "1",
          "time": "20210907 16:42"
        },
        {
          "id": "MDRW2100042737.1.3",
          "form": "저도 먹었어요!!",
          "original_form": "저도 먹었어요!!",
          "speaker_id": "2",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100042737.1.4",
          "form": "오늘은 어떤 점심 드셨나요?",
          "original_form": "오늘은 어떤 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100042737.1.5",
          "form": "저는 토스트 만들어먹었어요 name2 님은 뭐 드셨나욥",
          "original_form": "저는 토스트 만들어먹었어요 &name2& 님은 뭐 드셨나욥",
          "speaker_id": "1",
          "time": "20210907 16:56"
        },
        {
          "id": "MDRW2100042737.1.6",
          "form": "저는 라면이요!",
          "original_form": "저는 라면이요!",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100042737.1.7",
          "form": "신라면 건면이 궁금해서 먹어봤어요",
          "original_form": "신라면 건면이 궁금해서 먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100042737.1.8",
          "form": "토스트!",
          "original_form": "토스트!",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100042737.1.9",
          "form": "맛있었을 것 같아요 ㅎㅎ",
          "original_form": "맛있었을 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100042737.1.10",
          "form": "오오 신라면 건면 맛있을 거 같네요",
          "original_form": "오오 신라면 건면 맛있을 거 같네요",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100042737.1.11",
          "form": "토스트에 메이플시럽 바르니까 더 맛있더라구요ㅋㅋㅋ",
          "original_form": "토스트에 메이플시럽 바르니까 더 맛있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100042737.1.12",
          "form": "메이플시럽 상상만 해도 벌써 달콤해요!",
          "original_form": "메이플시럽 상상만 해도 벌써 달콤해요!",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100042737.1.13",
          "form": "저도 내일은 토스트에 마누카 허니 발라 먹어야 겠어요 ㅠ",
          "original_form": "저도 내일은 토스트에 마누카 허니 발라 먹어야 겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100042737.1.14",
          "form": "마누카 허니가 뭔가요! 꿀 종류인가...??",
          "original_form": "마누카 허니가 뭔가요! 꿀 종류인가...??",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100042737.1.15",
          "form": "뉴질랜드에서 나는 특산꿀인데 정말 달콤해요!",
          "original_form": "뉴질랜드에서 나는 특산꿀인데 정말 달콤해요!",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100042737.1.16",
          "form": "오오오 name2 님 단 거 좋아하시나요?",
          "original_form": "오오오 &name2& 님 단 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100042737.1.17",
          "form": "짱좋아해요!",
          "original_form": "짱좋아해요!",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100042737.1.18",
          "form": "디저트 맛집 찾아서 다니는거 좋아했어요",
          "original_form": "디저트 맛집 찾아서 다니는거 좋아했어요",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100042737.1.19",
          "form": "오오오 저도 서울 근처 살면 디저트 맛집 죄다 찾아다녔을 텐데",
          "original_form": "오오오 저도 서울 근처 살면 디저트 맛집 죄다 찾아다녔을 텐데",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100042737.1.20",
          "form": "어.. 괜찮으시다면 어디에 거주중이신가요?",
          "original_form": "어.. 괜찮으시다면 어디에 거주중이신가요?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100042737.1.21",
          "form": "저는 경기 남부 살아요!",
          "original_form": "저는 경기 남부 살아요!",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100042737.1.22",
          "form": "아 ㅠㅠ 서울에서 제일 멀다고 하는 곳이 경기도죠 ㅠㅠ",
          "original_form": "아 ㅠㅠ 서울에서 제일 멀다고 하는 곳이 경기도죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100042737.1.23",
          "form": "진짜 교통편 무슨일인지 모르겠어요",
          "original_form": "진짜 교통편 무슨일인지 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100042737.1.24",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:43"
        }
      ]
    }
