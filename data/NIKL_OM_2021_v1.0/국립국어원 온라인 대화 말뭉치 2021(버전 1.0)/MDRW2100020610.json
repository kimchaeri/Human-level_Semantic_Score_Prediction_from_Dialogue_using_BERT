      "utterance": [
        {
          "id": "MDRW2100020610.1.1",
          "form": "훗 진실된 소통 좋아요",
          "original_form": "훗 진실된 소통 좋아요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.2",
          "form": "저를 시험하시다니 긴장을 늦추지 말아야겠습니다",
          "original_form": "저를 시험하시다니 긴장을 늦추지 말아야겠습니다",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.3",
          "form": "후후후",
          "original_form": "후후후",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.4",
          "form": "사실 한국과일이 맛있긴해요",
          "original_form": "사실 한국과일이 맛있긴해요",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.5",
          "form": "태국망고는 맛있나요",
          "original_form": "태국망고는 맛있나요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.6",
          "form": "제가 말레이시아 망고까진 먹어봤어요",
          "original_form": "제가 말레이시아 망고까진 먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.7",
          "form": "네 남독마이라고..",
          "original_form": "네 남독마이라고..",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.8",
          "form": "태국망고가 제일 맛있어요..",
          "original_form": "태국망고가 제일 맛있어요..",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.9",
          "form": "오 이름이 있군요",
          "original_form": "오 이름이 있군요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020610.1.10",
          "form": "친구들이 애플망고 찬양하던데 드셔보셨나요",
          "original_form": "친구들이 애플망고 찬양하던데 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.11",
          "form": "애플망고는",
          "original_form": "애플망고는",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.12",
          "form": "저는 기회가 안돼서 아직 못먹어봤어요",
          "original_form": "저는 기회가 안돼서 아직 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.13",
          "form": "궁금한데..",
          "original_form": "궁금한데..",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.14",
          "form": "얼려도 안얼려도 맛이 똑같아요",
          "original_form": "얼려도 안얼려도 맛이 똑같아요",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.15",
          "form": "설빙에서 망고빙수드셔보셨어요?",
          "original_form": "설빙에서 망고빙수드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.16",
          "form": "와 그거 큰 장점이네요",
          "original_form": "와 그거 큰 장점이네요",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.17",
          "form": "그 망고맛이에요",
          "original_form": "그 망고맛이에요",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.18",
          "form": "헐 너무 신기한데요",
          "original_form": "헐 너무 신기한데요",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.19",
          "form": "그 일반망고는 얼리면 비린내나거든요",
          "original_form": "그 일반망고는 얼리면 비린내나거든요",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020610.1.20",
          "form": "그럼 망고빙수먹지 왜 애플망고먹지..!",
          "original_form": "그럼 망고빙수먹지 왜 애플망고먹지..!",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020610.1.21",
          "form": "애플망고는 식감차이만 나고 비슷해요",
          "original_form": "애플망고는 식감차이만 나고 비슷해요",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020610.1.22",
          "form": "아무래도 신선하니까..?",
          "original_form": "아무래도 신선하니까..?",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020610.1.23",
          "form": "약간 물렁이라고 해야하나",
          "original_form": "약간 물렁이라고 해야하나",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020610.1.24",
          "form": "찐득이라고 해야하나",
          "original_form": "찐득이라고 해야하나",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020610.1.25",
          "form": "약간 생과는 쫄깃하거든요",
          "original_form": "약간 생과는 쫄깃하거든요",
          "speaker_id": "1",
          "time": "20210828 10:26"
        }
      ]
    }
  ]
