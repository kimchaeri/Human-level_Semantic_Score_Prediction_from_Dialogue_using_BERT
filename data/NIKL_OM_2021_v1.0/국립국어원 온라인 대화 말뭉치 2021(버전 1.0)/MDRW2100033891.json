      "utterance": [
        {
          "id": "MDRW2100033891.1.1",
          "form": "무섭네요 부작용",
          "original_form": "무섭네요 부작용",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100033891.1.2",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100033891.1.3",
          "form": "성형이나 시술엔 관심없으세요?",
          "original_form": "성형이나 시술엔 관심없으세요?",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033891.1.4",
          "form": "저 아스트라1차맞았는데",
          "original_form": "저 아스트라1차맞았는데",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033891.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033891.1.6",
          "form": "2차가 모더라래서 아침에 1339  전화했잖아요",
          "original_form": "2차가 모더라래서 아침에 1339  전화했잖아요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033891.1.7",
          "form": "세상에 교차접종안되는백신이었더라구요",
          "original_form": "세상에 교차접종안되는백신이었더라구요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033891.1.8",
          "form": "뭐라구요?",
          "original_form": "뭐라구요?",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033891.1.9",
          "form": "어제 기사보고 깜짝 놀랐어요",
          "original_form": "어제 기사보고 깜짝 놀랐어요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033891.1.10",
          "form": "교차접종이 안되는 백신이 있나요?",
          "original_form": "교차접종이 안되는 백신이 있나요?",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100033891.1.11",
          "form": "모더나랑 아스트라제네카는 안된다구하더라구요 그래서 화이자 맞았어요 name1님도 조심하세요",
          "original_form": "모더나랑 아스트라제네카는 안된다구하더라구요 그래서 화이자 맞았어요 &name1&님도 조심하세요",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100033891.1.12",
          "form": "네 말씀 감사합니다 아직 안맞았는데 맞기싫어요",
          "original_form": "네 말씀 감사합니다 아직 안맞았는데 맞기싫어요",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100033891.1.13",
          "form": "그쵸 불안하쥬",
          "original_form": "그쵸 불안하쥬",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100033891.1.14",
          "form": "밖에 별로 안나가서 걸릴일 없다싶거든요",
          "original_form": "밖에 별로 안나가서 걸릴일 없다싶거든요",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100033891.1.15",
          "form": "전 부작용전혀없었는데 주위에 몸살좀 타시더라구요ㅜ",
          "original_form": "전 부작용전혀없었는데 주위에 몸살좀 타시더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100033891.1.16",
          "form": "하혈한다는 말도있더라구요",
          "original_form": "하혈한다는 말도있더라구요",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100033891.1.17",
          "form": "말이 백신이지 임상실험이랑뭐가다른지",
          "original_form": "말이 백신이지 임상실험이랑뭐가다른지",
          "speaker_id": "1",
          "time": "20210901 18:58"
        },
        {
          "id": "MDRW2100033891.1.18",
          "form": "음 맞아요 미국에 백신반대자들이 글케 많대요",
          "original_form": "음 맞아요 미국에 백신반대자들이 글케 많대요",
          "speaker_id": "2",
          "time": "20210901 19:04"
        },
        {
          "id": "MDRW2100033891.1.19",
          "form": "백신도 글코 바이러스도 다 알지못하니까 다들 불안해하는거 같아요",
          "original_form": "백신도 글코 바이러스도 다 알지못하니까 다들 불안해하는거 같아요",
          "speaker_id": "2",
          "time": "20210901 19:04"
        }
      ]
