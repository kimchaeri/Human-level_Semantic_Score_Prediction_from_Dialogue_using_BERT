      "utterance": [
        {
          "id": "MDRW2100030732.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030732.1.2",
          "form": "유투브 좋아하세요?",
          "original_form": "유투브 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030732.1.3",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030732.1.4",
          "form": "네! 유튜브 자주 봐요!",
          "original_form": "네! 유튜브 자주 봐요!",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030732.1.5",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030732.1.6",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100030732.1.7",
          "form": "해쭈랑 일주어터 좋아해요",
          "original_form": "해쭈랑 일주어터 좋아해요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030732.1.8",
          "form": "햄튜브도 좋아했는데 게임영상이 많아서 요즘은 잘 안봐요",
          "original_form": "햄튜브도 좋아했는데 게임영상이 많아서 요즘은 잘 안봐요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030732.1.9",
          "form": "우왁! 대박 저도 해쭈 일주어터 짱 좋아해요!",
          "original_form": "우왁! 대박 저도 해쭈 일주어터 짱 좋아해요!",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030732.1.10",
          "form": "거기에 예랑가랑이랑 쓰레기걸까지!",
          "original_form": "거기에 예랑가랑이랑 쓰레기걸까지!",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030732.1.11",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030732.1.12",
          "form": "요즘은 해쭈 가족들 유투브도 많이 봐요ㅋㅋ",
          "original_form": "요즘은 해쭈 가족들 유투브도 많이 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030732.1.13",
          "form": "앗 맞아요 그 카야네가 채널 만들었더라고요!!",
          "original_form": "앗 맞아요 그 카야네가 채널 만들었더라고요!!",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030732.1.14",
          "form": "조앤 도 있어요!ㅋㅋ",
          "original_form": "조앤 도 있어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030732.1.15",
          "form": "카야 너무 귀여워요ㅜㅜ",
          "original_form": "카야 너무 귀여워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030732.1.16",
          "form": "우왁 조앤도요!?! 그건 몰랐네",
          "original_form": "우왁 조앤도요!?! 그건 몰랐네",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030732.1.17",
          "form": "그러니까요 카야랑 그 쁘큐보이 조카",
          "original_form": "그러니까요 카야랑 그 쁘큐보이 조카",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030732.1.18",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030732.1.19",
          "form": "ㅋㅋㅋㅋ 쁘보 조카 생일파티 영상 보셨어요?",
          "original_form": "ㅋㅋㅋㅋ 쁘보 조카 생일파티 영상 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030732.1.20",
          "form": "둘이 성격이 반대라 너무 귀여워요ㅋㅋ",
          "original_form": "둘이 성격이 반대라 너무 귀여워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030732.1.21",
          "form": "또 어떤 컨텐츠 보세요?",
          "original_form": "또 어떤 컨텐츠 보세요?",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030732.1.22",
          "form": "요즘 환승연애가 재밌던데요ㅋㅋㅋ",
          "original_form": "요즘 환승연애가 재밌던데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030732.1.23",
          "form": "맞아요!! 활발한 카야랑 얌전한 민준?이",
          "original_form": "맞아요!! 활발한 카야랑 얌전한 민준?이",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030732.1.24",
          "form": "헐 전 환승연애 안 보는데 재밌낭ㅅ?",
          "original_form": "헐 전 환승연애 안 보는데 재밌낭ㅅ?",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030732.1.25",
          "form": "그 코코는 알아요!",
          "original_form": "그 코코는 알아요!",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030732.1.26",
          "form": "네네 코코 나와요 ㅋㅋ",
          "original_form": "네네 코코 나와요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100030732.1.27",
          "form": "저는 재밌어요ㅋㅋ",
          "original_form": "저는 재밌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:15"
        },
        {
          "id": "MDRW2100030732.1.28",
          "form": "끝이 어찌될지 궁금해서요ㅋㅋㅋㅋㅋ",
          "original_form": "끝이 어찌될지 궁금해서요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100030732.1.29",
          "form": "헛 다들 재밋다고 하던데",
          "original_form": "헛 다들 재밋다고 하던데",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030732.1.30",
          "form": "담에 저도 함 봐야겠어요 ㅎ.ㅎ",
          "original_form": "담에 저도 함 봐야겠어요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030732.1.31",
          "form": "초코님 또 어떤 프로 보세요!",
          "original_form": "초코님 또 어떤 프로 보세요!",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030732.1.32",
          "form": "전 요새 런닝맨 챙겨보아ㅛ",
          "original_form": "전 요새 런닝맨 챙겨보아ㅛ",
          "speaker_id": "2",
          "time": "20210904 12:18"
        },
        {
          "id": "MDRW2100030732.1.33",
          "form": "런닝맨 예전에 많이 봤는데",
          "original_form": "런닝맨 예전에 많이 봤는데",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030732.1.34",
          "form": "요즘은 안봐욬ㅋㅋ",
          "original_form": "요즘은 안봐욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030732.1.35",
          "form": "최근에 끝났는데",
          "original_form": "최근에 끝났는데",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030732.1.36",
          "form": "꼬꼬무도 재맜게 봤어요ㅋㅋ",
          "original_form": "꼬꼬무도 재맜게 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030732.1.37",
          "form": "여고추리반도요!",
          "original_form": "여고추리반도요!",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030732.1.38",
          "form": "식스센스도 재밌고욬ㅋㅋㅋㅋ",
          "original_form": "식스센스도 재밌고욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:21"
        }
      ]
    }
