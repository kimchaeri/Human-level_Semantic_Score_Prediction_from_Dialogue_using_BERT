      "utterance": [
        {
          "id": "MDRW2100008649.1.1",
          "form": "학교 생활?",
          "original_form": "학교 생활?",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008649.1.2",
          "form": "대학생이세요?",
          "original_form": "대학생이세요?",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008649.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008649.1.4",
          "form": "요즘 방학이죠?",
          "original_form": "요즘 방학이죠?",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008649.1.5",
          "form": "네네 근데 곧 개강해요ㅜ",
          "original_form": "네네 근데 곧 개강해요ㅜ",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008649.1.6",
          "form": "코로나인데 수업은 어찌해요? 온라인?",
          "original_form": "코로나인데 수업은 어찌해요? 온라인?",
          "speaker_id": "1",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008649.1.7",
          "form": "대부분 온라인이긴 해요",
          "original_form": "대부분 온라인이긴 해요",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008649.1.8",
          "form": "근데 2학기부터는 대면 고집하려는 게 보여서",
          "original_form": "근데 2학기부터는 대면 고집하려는 게 보여서",
          "speaker_id": "2",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008649.1.9",
          "form": "4단계만 내려가면",
          "original_form": "4단계만 내려가면",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008649.1.10",
          "form": "거의 대면 할 거 같네요...",
          "original_form": "거의 대면 할 거 같네요...",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008649.1.11",
          "form": "몇학년이세요? 신입생들은 대학생활 기대하고 갔을텐데",
          "original_form": "몇학년이세요? 신입생들은 대학생활 기대하고 갔을텐데",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008649.1.12",
          "form": "엠티도 못가보고 넘우울할거 같아요ㅎㅎ",
          "original_form": "엠티도 못가보고 넘우울할거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008649.1.13",
          "form": "새내기예요",
          "original_form": "새내기예요",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008649.1.14",
          "form": "맞아요 엠티ㅜ 너무 아쉬웟어요",
          "original_form": "맞아요 엠티ㅜ 너무 아쉬웟어요",
          "speaker_id": "2",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008649.1.15",
          "form": "새내기면 진짜 아쉬울듯요",
          "original_form": "새내기면 진짜 아쉬울듯요",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008649.1.16",
          "form": "친구들이랑 친해질 기회이기도 한데",
          "original_form": "친구들이랑 친해질 기회이기도 한데",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008649.1.17",
          "form": "아쉬운 게 크긴 하죠ㅠㅠ",
          "original_form": "아쉬운 게 크긴 하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100008649.1.18",
          "form": "시험은 그럼 어찌보나요?",
          "original_form": "시험은 그럼 어찌보나요?",
          "speaker_id": "1",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008649.1.19",
          "form": "온라인으로 보면 오픈북인가요?",
          "original_form": "온라인으로 보면 오픈북인가요?",
          "speaker_id": "1",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008649.1.20",
          "form": "시험은 다 대면으로 했어요",
          "original_form": "시험은 다 대면으로 했어요",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008649.1.21",
          "form": "아무래도 컨닝 위험이 많아서 그런가봐요",
          "original_form": "아무래도 컨닝 위험이 많아서 그런가봐요",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008649.1.22",
          "form": "그니까요 감독없음 모여서 할수도있고",
          "original_form": "그니까요 감독없음 모여서 할수도있고",
          "speaker_id": "1",
          "time": "20210822 18:52"
        }
      ]
