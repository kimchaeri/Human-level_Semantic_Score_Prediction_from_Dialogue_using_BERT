      "utterance": [
        {
          "id": "MDRW2100041593.1.1",
          "form": "날씨가 좋아용",
          "original_form": "날씨가 좋아용",
          "speaker_id": "1",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041593.1.2",
          "form": "앗 날씨 좋은가여?",
          "original_form": "앗 날씨 좋은가여?",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041593.1.3",
          "form": "제가 있는 곳은 아까 비가 조금 와서",
          "original_form": "제가 있는 곳은 아까 비가 조금 와서",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041593.1.4",
          "form": "지금은 많이 흐려요!",
          "original_form": "지금은 많이 흐려요!",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041593.1.5",
          "form": "헉 정말요??",
          "original_form": "헉 정말요??",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041593.1.6",
          "form": "여기는 햇빝이 쨍쨍햐요!!",
          "original_form": "여기는 햇빝이 쨍쨍햐요!!",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041593.1.7",
          "form": "와 어느지역이세요? ㅋㅋ부러워요...",
          "original_form": "와 어느지역이세요? ㅋㅋ부러워요...",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100041593.1.8",
          "form": "요즘 계속 비와서 ㅠ",
          "original_form": "요즘 계속 비와서 ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:03"
        },
        {
          "id": "MDRW2100041593.1.9",
          "form": "저는 서울이요!!",
          "original_form": "저는 서울이요!!",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041593.1.10",
          "form": "ㅜㅜ 우산 챙겨 다녀야 겟어요 ㅠㅠ",
          "original_form": "ㅜㅜ 우산 챙겨 다녀야 겟어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041593.1.11",
          "form": "아하 요즘 날씨 정말 알 수 없죠...",
          "original_form": "아하 요즘 날씨 정말 알 수 없죠...",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041593.1.12",
          "form": "어제도 카페 갔다가 나오니까 비오더라구요 ㅋㅋ큐ㅜ",
          "original_form": "어제도 카페 갔다가 나오니까 비오더라구요 ㅋㅋ큐ㅜ",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041593.1.13",
          "form": "우산 사니까 또 그치구",
          "original_form": "우산 사니까 또 그치구",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041593.1.14",
          "form": "아구 ㅠㅠ 그래도 쏟아져 내리는거보단 나은거같아여 ㅠㅠ",
          "original_form": "아구 ㅠㅠ 그래도 쏟아져 내리는거보단 나은거같아여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041593.1.15",
          "form": "몇일전만 해도 엄청 와서 ㅋㅋ",
          "original_form": "몇일전만 해도 엄청 와서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041593.1.16",
          "form": "ㅋㅋㅋ글킨해요",
          "original_form": "ㅋㅋㅋ글킨해요",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041593.1.17",
          "form": "요즘 여름때보다 제습기 더 열심히 써요",
          "original_form": "요즘 여름때보다 제습기 더 열심히 써요",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041593.1.18",
          "form": "이제 겉옷도 준비해야겟어요!!",
          "original_form": "이제 겉옷도 준비해야겟어요!!",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041593.1.19",
          "form": "맞아요 ㅎㅎ출근할 때 보니까 긴팔이 많이 늘었더라구요",
          "original_form": "맞아요 ㅎㅎ출근할 때 보니까 긴팔이 많이 늘었더라구요",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100041593.1.20",
          "form": "어느 계절 좋아하세요?",
          "original_form": "어느 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100041593.1.21",
          "form": "저는 가을이요..!!",
          "original_form": "저는 가을이요..!!",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041593.1.22",
          "form": "겨울은 너뮤 추위를 많이타서 ㅠㅠ",
          "original_form": "겨울은 너뮤 추위를 많이타서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:17"
        },
        {
          "id": "MDRW2100041593.1.23",
          "form": "아하 저도 가을이요!",
          "original_form": "아하 저도 가을이요!",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041593.1.24",
          "form": "가을이 제일 공기도 맑고 적당한 거 같은데",
          "original_form": "가을이 제일 공기도 맑고 적당한 거 같은데",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041593.1.25",
          "form": "너무 짧죠..ㅠ",
          "original_form": "너무 짧죠..ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041593.1.26",
          "form": "맞아요 ㅠㅠ 금방 겨울 되요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 금방 겨울 되요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100041593.1.27",
          "form": "여름 겨울 .. 길게 느껴져요 ㅋㅋㅋ",
          "original_form": "여름 겨울 .. 길게 느껴져요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100041593.1.28",
          "form": "진짜 길기도 하져 ㅋㅋㅋㅋ 겨울이 진짜 길어진 느낌이예요",
          "original_form": "진짜 길기도 하져 ㅋㅋㅋㅋ 겨울이 진짜 길어진 느낌이예요",
