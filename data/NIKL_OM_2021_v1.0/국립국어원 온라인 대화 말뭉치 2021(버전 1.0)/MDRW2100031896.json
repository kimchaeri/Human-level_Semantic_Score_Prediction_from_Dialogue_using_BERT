      "utterance": [
        {
          "id": "MDRW2100031896.1.1",
          "form": "도너츠 좋아하세요?",
          "original_form": "도너츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 08:05"
        },
        {
          "id": "MDRW2100031896.1.2",
          "form": "저는 안좋아하고 와이프가 좋아해요",
          "original_form": "저는 안좋아하고 와이프가 좋아해요",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031896.1.3",
          "form": "맛집 자주 다니는 편이세요?",
          "original_form": "맛집 자주 다니는 편이세요?",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031896.1.4",
          "form": "아뇨 외식을 자주 못하다보니..\n맛집 자주 가시나요?",
          "original_form": "아뇨 외식을 자주 못하다보니..\n맛집 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100031896.1.5",
          "form": "아니요 저도 자주 안 가요",
          "original_form": "아니요 저도 자주 안 가요",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100031896.1.6",
          "form": "그러시군요 배달은요?",
          "original_form": "그러시군요 배달은요?",
          "speaker_id": "2",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100031896.1.7",
          "form": "배달 아니면 포장해와서 먹어요",
          "original_form": "배달 아니면 포장해와서 먹어요",
          "speaker_id": "1",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100031896.1.8",
          "form": "주로 어떤 걸 드시나요?",
          "original_form": "주로 어떤 걸 드시나요?",
          "speaker_id": "2",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100031896.1.9",
          "form": "피자 치킨 주로 먹는거같아요",
          "original_form": "피자 치킨 주로 먹는거같아요",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100031896.1.10",
          "form": "김밥도 사먹었어요 포장으로요",
          "original_form": "김밥도 사먹었어요 포장으로요",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100031896.1.11",
          "form": "저도 피자.햄버거 배달로 자주 먹는 메뉴네요",
          "original_form": "저도 피자.햄버거 배달로 자주 먹는 메뉴네요",
          "speaker_id": "2",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100031896.1.12",
          "form": "네 젤 자주 먹게되는 메뉴더라구요",
          "original_form": "네 젤 자주 먹게되는 메뉴더라구요",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100031896.1.13",
          "form": "김밥에 라볶이..분식 조합도 좋죠...",
          "original_form": "김밥에 라볶이..분식 조합도 좋죠...",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100031896.1.14",
          "form": "네 맛있어요 ㅎㅎㅎ",
          "original_form": "네 맛있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100031896.1.15",
          "form": "갑자기 먹고싶네요",
          "original_form": "갑자기 먹고싶네요",
          "speaker_id": "1",
