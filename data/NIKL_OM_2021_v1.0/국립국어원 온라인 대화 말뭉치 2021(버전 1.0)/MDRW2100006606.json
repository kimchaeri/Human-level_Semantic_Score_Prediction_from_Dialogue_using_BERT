          "form": "가깝네",
          "original_form": "가깝네",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006606.1.29",
          "form": "글치....",
          "original_form": "글치....",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006606.1.30",
          "form": "택시 잡으면",
          "original_form": "택시 잡으면",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006606.1.31",
          "form": "거부당할만한",
          "original_form": "거부당할만한",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006606.1.32",
          "form": "그런 거리지....",
          "original_form": "그런 거리지....",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006606.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 천천히 걸어와....",
