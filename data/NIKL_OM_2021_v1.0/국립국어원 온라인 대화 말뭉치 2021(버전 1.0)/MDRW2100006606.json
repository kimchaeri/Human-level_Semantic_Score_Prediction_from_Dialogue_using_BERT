      "utterance": [
        {
          "id": "MDRW2100006606.1.1",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
