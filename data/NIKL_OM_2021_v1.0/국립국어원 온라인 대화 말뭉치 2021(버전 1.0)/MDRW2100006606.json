        },
        {
          "id": "MDRW2100006606.1.2",
          "form": "그러면 걍",
          "original_form": "그러면 걍",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006606.1.3",
          "form": "난 이런 기사 봐선",
          "original_form": "난 이런 기사 봐선",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006606.1.4",
          "form": "타이레놀 먹는데",
          "original_form": "타이레놀 먹는데",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006606.1.5",
          "form": "모더나 맞을지도...",
          "original_form": "모더나 맞을지도...",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006606.1.6",
          "form": "나도 그럴줄알았는데",
          "original_form": "나도 그럴줄알았는데",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006606.1.7",
          "form": "화이자더라구",
          "original_form": "화이자더라구",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006606.1.8",
          "form": "좀 긴장돼",
          "original_form": "좀 긴장돼",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006606.1.9",
          "form": "걍 오후에 잡을걸",
          "original_form": "걍 오후에 잡을걸",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006606.1.10",
          "form": "뭐하러 굳이 11시에 잡아서",
          "original_form": "뭐하러 굳이 11시에 잡아서",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006606.1.11",
          "form": "오전?",
          "original_form": "오전?",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006606.1.12",
          "form": "일찍 일어나기까지 해야하지",
          "original_form": "일찍 일어나기까지 해야하지",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006606.1.13",
          "form": "ㅇㅇ오전",
          "original_form": "ㅇㅇ오전",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006606.1.14",
          "form": "예약할때",
          "original_form": "예약할때",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006606.1.15",
          "form": "ㄴ",
          "original_form": "ㄴ",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006606.1.16",
          "form": "빨리 맞고 끝내버리자엿겟지...",
          "original_form": "빨리 맞고 끝내버리자엿겟지...",
          "speaker_id": "2",
          "time": "20210908 03:13"
        },
        {
          "id": "MDRW2100006606.1.17",
          "form": "그렇긴 해...",
          "original_form": "그렇긴 해...",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.18",
          "form": "나도 백신 이번에 공급돼서 맞으면",
          "original_form": "나도 백신 이번에 공급돼서 맞으면",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.19",
          "form": "걍 빨리 맞고 빨리 끝내버리고싶음",
          "original_form": "걍 빨리 맞고 빨리 끝내버리고싶음",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.20",
          "form": "으으 과거의 나는 바보야",
          "original_form": "으으 과거의 나는 바보야",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.21",
          "form": "내가 게으름 부리고싶을걸",
          "original_form": "내가 게으름 부리고싶을걸",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.22",
          "form": "미리 관심법으로",
          "original_form": "미리 관심법으로",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.23",
          "form": "알앗어야지....",
          "original_form": "알앗어야지....",
          "speaker_id": "2",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006606.1.24",
          "form": "벌써 가기싫다",
          "original_form": "벌써 가기싫다",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006606.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ백신 병원이 집에서 멀어?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ백신 병원이 집에서 멀어?",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006606.1.26",
          "form": "걸어서 가면 30분",
          "original_form": "걸어서 가면 30분",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006606.1.27",
