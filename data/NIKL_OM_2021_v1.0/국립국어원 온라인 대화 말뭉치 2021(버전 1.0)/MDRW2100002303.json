      "utterance": [
        {
          "id": "MDRW2100002303.1.1",
          "form": "책 읽는거 좋아해요?",
          "original_form": "책 읽는거 좋아해요?",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002303.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002303.1.3",
          "form": "최근엔 시간이 안맞아서 자주 못읽었지만..",
          "original_form": "최근엔 시간이 안맞아서 자주 못읽었지만..",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002303.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002303.1.5",
          "form": "저는 추리소설같은거 좋아해요",
          "original_form": "저는 추리소설같은거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002303.1.6",
          "form": "추리소설 중 재밌는거 있나요?",
          "original_form": "추리소설 중 재밌는거 있나요?",
          "speaker_id": "1",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002303.1.7",
          "form": "용의자x의 헌신 알아요?",
          "original_form": "용의자x의 헌신 알아요?",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002303.1.8",
          "form": "아니요 처음들어봐요 무슨내용인가요?",
          "original_form": "아니요 처음들어봐요 무슨내용인가요?",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002303.1.9",
          "form": "국내 영화로도 나왔어요!",
          "original_form": "국내 영화로도 나왔어요!",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002303.1.10",
          "form": "재밌어요 함 봐바요",
          "original_form": "재밌어요 함 봐바요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002303.1.11",
          "form": "방금 검색해보고왔는데 재밌어보여요",
          "original_form": "방금 검색해보고왔는데 재밌어보여요",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002303.1.12",
          "form": "꼭 한 번 봐볼게요",
          "original_form": "꼭 한 번 봐볼게요",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002303.1.13",
          "form": "저는 최근에 데미안을 읽었는데 고전소설이라 그런가 어렵더라구요",
          "original_form": "저는 최근에 데미안을 읽었는데 고전소설이라 그런가 어렵더라구요",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002303.1.14",
          "form": "아마 세번을 읽어도 어려울거같아요",
          "original_form": "아마 세번을 읽어도 어려울거같아요",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002303.1.15",
          "form": "데미안 엄청 두꺼운 책 아녜요?",
          "original_form": "데미안 엄청 두꺼운 책 아녜요?",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002303.1.16",
          "form": "엄청까지는 아니고 약간?",
          "original_form": "엄청까지는 아니고 약간?",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002303.1.17",
          "form": "사실 그 책 어려워서 첫장 읽고 덮고싶었어요.",
          "original_form": "사실 그 책 어려워서 첫장 읽고 덮고싶었어요.",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002303.1.18",
          "form": "가독성이 너무 떨어지더라구요..",
          "original_form": "가독성이 너무 떨어지더라구요..",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002303.1.19",
          "form": "넘 두꺼워서 시작도 못한 책이예요ㅋㅋ",
          "original_form": "넘 두꺼워서 시작도 못한 책이예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002303.1.20",
          "form": "ㅎ 그래서 데미안 읽는다 그러면 굳이 추천은 안드려요",
          "original_form": "ㅎ 그래서 데미안 읽는다 그러면 굳이 추천은 안드려요",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002303.1.21",
          "form": "ㅋㅋㅋ다른 재밌는 책을 더 읽는걸루",
          "original_form": "ㅋㅋㅋ다른 재밌는 책을 더 읽는걸루",
          "speaker_id": "2",
          "time": "20210820 12:53"
        }
      ]
