      "utterance": [
        {
          "id": "MDRW2100026538.1.1",
          "form": "안녕 저녁 먹었어?",
          "original_form": "안녕 저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026538.1.2",
          "form": "안녕 나 저녁 떡볶이 먹었어!",
          "original_form": "안녕 나 저녁 떡볶이 먹었어!",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026538.1.3",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026538.1.4",
          "form": "나는 저녁에 카레해먹었어!",
          "original_form": "나는 저녁에 카레해먹었어!",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026538.1.5",
          "form": "나 떡볶이 엄청 좋아하는데 어디꺼 먹었어??",
          "original_form": "나 떡볶이 엄청 좋아하는데 어디꺼 먹었어??",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026538.1.6",
          "form": "카레 맛있겠다!",
          "original_form": "카레 맛있겠다!",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026538.1.7",
          "form": "어머니가 직접 해주셨어",
          "original_form": "어머니가 직접 해주셨어",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026538.1.8",
          "form": "중국당면도 넣구 ㅎㅎ",
          "original_form": "중국당면도 넣구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026538.1.9",
          "form": "어머니가 음식 잘하시나보다!",
          "original_form": "어머니가 음식 잘하시나보다!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026538.1.10",
          "form": "중국당면 너무 맛있어....!",
          "original_form": "중국당면 너무 맛있어....!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026538.1.11",
          "form": "너도 요리 좋아해???",
          "original_form": "너도 요리 좋아해???",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026538.1.12",
          "form": "그치 중국당면 너무 좋아...",
          "original_form": "그치 중국당면 너무 좋아...",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026538.1.13",
          "form": "음 좋아하는데 잘 못해 ㅠㅠ",
          "original_form": "음 좋아하는데 잘 못해 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026538.1.14",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026538.1.15",
          "form": "나도 열심히 배우려고는 하는데 잘 못해...",
          "original_form": "나도 열심히 배우려고는 하는데 잘 못해...",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026538.1.16",
          "form": "파스타정도가 딱 내가할수있는 최대치같아ㅋㅋㅋ",
          "original_form": "파스타정도가 딱 내가할수있는 최대치같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026538.1.17",
          "form": "헐 그정도면 잘 하는 거 아니야??",
          "original_form": "헐 그정도면 잘 하는 거 아니야??",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026538.1.18",
          "form": "아니야ㅜㅜ 소스 그냥 파는거 면에다 넣기만 하는 파스타만 할줄알아!",
          "original_form": "아니야ㅜㅜ 소스 그냥 파는거 면에다 넣기만 하는 파스타만 할줄알아!",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026538.1.19",
          "form": "나는 뭔가 한식이 참 어려운거같아",
          "original_form": "나는 뭔가 한식이 참 어려운거같아",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026538.1.20",
          "form": "그래도 멋있다!!",
          "original_form": "그래도 멋있다!!",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026538.1.21",
          "form": "맞아 뭔가 복잡한 것 같고 그래",
          "original_form": "맞아 뭔가 복잡한 것 같고 그래",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100026538.1.22",
          "form": "응 된장찌개 김치찌개 이런거도 간단해보이는데 손이 엄청 가는것같더라고!",
          "original_form": "응 된장찌개 김치찌개 이런거도 간단해보이는데 손이 엄청 가는것같더라고!",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026538.1.23",
          "form": "집애서 배달음식은 자주 안시켜먹어??",
          "original_form": "집애서 배달음식은 자주 안시켜먹어??",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026538.1.24",
          "form": "그치 재료도 많구",
          "original_form": "그치 재료도 많구",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026538.1.25",
          "form": "음 가끔씩 치킨 시켜먹고 하긴 하는데",
          "original_form": "음 가끔씩 치킨 시켜먹고 하긴 하는데",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026538.1.26",
          "form": "요리해서 먹을 때가 더 많은 것 같아!",
          "original_form": "요리해서 먹을 때가 더 많은 것 같아!",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026538.1.27",
          "form": "집밥이 맛있어 ㅎㅎ",
          "original_form": "집밥이 맛있어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026538.1.28",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026538.1.29",
          "form": "나도 주로 집밥 먹어!",
          "original_form": "나도 주로 집밥 먹어!",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026538.1.30",
          "form": "배달음식은 점점 물리더라구!",
          "original_form": "배달음식은 점점 물리더라구!",
          "speaker_id": "1",
          "time": "20210901 23:46"
        }
      ]
    }
