      "utterance": [
        {
          "id": "MDRW2100027851.1.1",
          "form": "이번에 여군 사건도",
          "original_form": "이번에 여군 사건도",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027851.1.2",
          "form": "제대로 조사안하고 묻으려고만 하고",
          "original_form": "제대로 조사안하고 묻으려고만 하고",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027851.1.3",
          "form": "그러다 결국 피해자는 극단적 선택하게되고",
          "original_form": "그러다 결국 피해자는 극단적 선택하게되고",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027851.1.4",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027851.1.5",
          "form": "어유",
          "original_form": "어유",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027851.1.6",
          "form": "요새 여군문제가 심각하드라구여",
          "original_form": "요새 여군문제가 심각하드라구여",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027851.1.7",
          "form": "말이안됩니다ㅠ",
          "original_form": "말이안됩니다ㅠ",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027851.1.8",
          "form": "정말로 보고 있으면 답답하고 화가나요",
          "original_form": "정말로 보고 있으면 답답하고 화가나요",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027851.1.9",
          "form": "군수뇌부들은 왜 다 그렇게 나쁜 사람들로만 가득한지",
          "original_form": "군수뇌부들은 왜 다 그렇게 나쁜 사람들로만 가득한지",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027851.1.10",
          "form": "그쳐..",
          "original_form": "그쳐..",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027851.1.11",
          "form": "답답 답답",
          "original_form": "답답 답답",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027851.1.12",
          "form": "싹 잘라버리면 좋을텐데",
          "original_form": "싹 잘라버리면 좋을텐데",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027851.1.13",
          "form": "맞아여ㅠㅠ",
          "original_form": "맞아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027851.1.14",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027851.1.15",
          "form": "딸이랑 비슷한나이일텐데 ㅠㅠ",
          "original_form": "딸이랑 비슷한나이일텐데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027851.1.16",
          "form": "그니깐요 후안무치한것들",
          "original_form": "그니깐요 후안무치한것들",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027851.1.17",
          "form": "사람들이",
          "original_form": "사람들이",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027851.1.18",
          "form": "어떻게해서라도 군대 빠지려고하는데",
          "original_form": "어떻게해서라도 군대 빠지려고하는데",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027851.1.19",
          "form": "그 이유가 보이네욤",
          "original_form": "그 이유가 보이네욤",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027851.1.20",
