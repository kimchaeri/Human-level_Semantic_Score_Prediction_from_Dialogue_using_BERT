      "utterance": [
        {
          "id": "MDRW2100002234.1.1",
          "form": "요즘 코로나에 데이트 어떻게 해?",
          "original_form": "요즘 코로나에 데이트 어떻게 해?",
          "speaker_id": "1",
          "time": "20210820 01:15"
        },
        {
          "id": "MDRW2100002234.1.2",
          "form": "아무래도 집 데이트 말고는 어렵지",
          "original_form": "아무래도 집 데이트 말고는 어렵지",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002234.1.3",
          "form": "어딜가든 조심스러우니까",
          "original_form": "어딜가든 조심스러우니까",
          "speaker_id": "2",
          "time": "20210820 01:23"
        },
        {
          "id": "MDRW2100002234.1.4",
          "form": "그치 ㅠㅠ 나두 그렇다",
          "original_form": "그치 ㅠㅠ 나두 그렇다",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002234.1.5",
          "form": "그래도 좋아",
          "original_form": "그래도 좋아",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002234.1.6",
          "form": "밤산책정도가 좋은것같아",
          "original_form": "밤산책정도가 좋은것같아",
          "speaker_id": "2",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002234.1.7",
          "form": "웅 밤산책도 못하면 너무 답답하지 ㅠㅠ",
          "original_form": "웅 밤산책도 못하면 너무 답답하지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002234.1.8",
          "form": "뭘하느냐보다 누구랑 같이있느냐가 더 중요한거니까",
          "original_form": "뭘하느냐보다 누구랑 같이있느냐가 더 중요한거니까",
          "speaker_id": "2",
          "time": "20210820 01:24"
        },
        {
          "id": "MDRW2100002234.1.9",
          "form": "넌 데이트 자주해?",
          "original_form": "넌 데이트 자주해?",
          "speaker_id": "2",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002234.1.10",
          "form": "웅 맞아 ㅠㅠ 따뜻하네",
          "original_form": "웅 맞아 ㅠㅠ 따뜻하네",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002234.1.11",
          "form": "나두 집에서만 하지 거의",
          "original_form": "나두 집에서만 하지 거의",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002234.1.12",
          "form": "밥먹으러 외식하는정도?",
          "original_form": "밥먹으러 외식하는정도?",
          "speaker_id": "1",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002234.1.13",
          "form": "같이 요리해먹기도 하고?",
          "original_form": "같이 요리해먹기도 하고?",
          "speaker_id": "2",
          "time": "20210820 01:25"
        },
        {
          "id": "MDRW2100002234.1.14",
          "form": "맞아맞아 요리도 하는데",
          "original_form": "맞아맞아 요리도 하는데",
          "speaker_id": "1",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002234.1.15",
          "form": "요즘 여름에는 넘더워서",
          "original_form": "요즘 여름에는 넘더워서",
          "speaker_id": "1",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002234.1.16",
          "form": "잘못하겠더라 ㅠㅠ",
          "original_form": "잘못하겠더라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 01:26"
        },
        {
          "id": "MDRW2100002234.1.17",
          "form": "그치ㅋㅋㅋ 그럼 차가운 요리는 어때? 둥지냉면같은거ㅋㅋㅋ",
          "original_form": "그치ㅋㅋㅋ 그럼 차가운 요리는 어때? 둥지냉면같은거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002234.1.18",
          "form": "둥지냉면ㅋㅋㅋㅋ 그거도 면 삶을때 더울걸?ㅠㅠ",
          "original_form": "둥지냉면ㅋㅋㅋㅋ 그거도 면 삶을때 더울걸?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 01:27"
        },
        {
          "id": "MDRW2100002234.1.19",
          "form": "그러게ㅋㅋ 그생각을 못했구나",
          "original_form": "그러게ㅋㅋ 그생각을 못했구나",
          "speaker_id": "2",
          "time": "20210820 02:00"
        },
        {
          "id": "MDRW2100002234.1.20",
          "form": "그래두 같이 요리해먹는 데이트가 확실히 좋아",
          "original_form": "그래두 같이 요리해먹는 데이트가 확실히 좋아",
          "speaker_id": "2",
          "time": "20210820 02:01"
        },
        {
          "id": "MDRW2100002234.1.21",
          "form": "편하기도하구",
          "original_form": "편하기도하구",
          "speaker_id": "2",
          "time": "20210820 02:02"
        },
        {
          "id": "MDRW2100002234.1.22",
          "form": "맞아 요리 재밌어",
          "original_form": "맞아 요리 재밌어",
          "speaker_id": "1",
          "time": "20210820 02:02"
        }
      ]
