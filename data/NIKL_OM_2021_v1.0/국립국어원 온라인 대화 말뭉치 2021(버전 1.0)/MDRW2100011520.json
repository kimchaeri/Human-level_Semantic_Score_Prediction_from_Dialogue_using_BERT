      "utterance": [
        {
          "id": "MDRW2100011520.1.1",
          "form": "태풍이시작되었어요",
          "original_form": "태풍이시작되었어요",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011520.1.2",
          "form": "그쪽은 바람 많이부나요",
          "original_form": "그쪽은 바람 많이부나요",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011520.1.3",
          "form": "네 바람도불고",
          "original_form": "네 바람도불고",
          "speaker_id": "1",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011520.1.4",
          "form": "비도엄청와요",
          "original_form": "비도엄청와요",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011520.1.5",
          "form": "오후에나가야하는데큰일이예요",
          "original_form": "오후에나가야하는데큰일이예요",
          "speaker_id": "1",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011520.1.6",
          "form": "여긴 서울이라 평온한데 빨리",
          "original_form": "여긴 서울이라 평온한데 빨리",
          "speaker_id": "2",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011520.1.7",
          "form": "소멸되야 할텐데요ㅠ",
          "original_form": "소멸되야 할텐데요ㅠ",
          "speaker_id": "2",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011520.1.8",
          "form": "아 거긴태풍이아닌가요?",
          "original_form": "아 거긴태풍이아닌가요?",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011520.1.9",
          "form": "비가그치질않아요ㅋㅋ",
          "original_form": "비가그치질않아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011520.1.10",
          "form": "서울은비올땐엄청오는데 이번엔너무찔끔찔끔오더라구요",
          "original_form": "서울은비올땐엄청오는데 이번엔너무찔끔찔끔오더라구요",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011520.1.11",
          "form": "네 지금도 안오네요",
          "original_form": "네 지금도 안오네요",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011520.1.12",
          "form": "여긴오늘도세차게왓답니다",
          "original_form": "여긴오늘도세차게왓답니다",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100011520.1.13",
          "form": "홀딱맞앗어용",
          "original_form": "홀딱맞앗어용",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100011520.1.14",
          "form": "헐 지방이신가요",
          "original_form": "헐 지방이신가요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100011520.1.15",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100011520.1.16",
          "form": "본가왔더니 비만오네요",
          "original_form": "본가왔더니 비만오네요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100011520.1.17",
          "form": "서울은비아예안와요?",
          "original_form": "서울은비아예안와요?",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100011520.1.18",
          "form": "지금 안오네요",
          "original_form": "지금 안오네요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100011520.1.19",
          "form": "오 여기는오늘은 쨍쨍하네요",
          "original_form": "오 여기는오늘은 쨍쨍하네요",
          "speaker_id": "1",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100011520.1.20",
          "form": "네네 여기도 비 안오네요",
          "original_form": "네네 여기도 비 안오네요",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100011520.1.21",
          "form": "드디어태풍끝인가요",
          "original_form": "드디어태풍끝인가요",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100011520.1.22",
          "form": "근데더워져서",
          "original_form": "근데더워져서",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100011520.1.23",
          "form": "빨리가을왔음좋겟네요",
          "original_form": "빨리가을왔음좋겟네요",
          "speaker_id": "1",
          "time": "20210826 20:30"
        }
      ]
