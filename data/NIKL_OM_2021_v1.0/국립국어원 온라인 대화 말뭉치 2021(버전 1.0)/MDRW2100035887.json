      "utterance": [
        {
          "id": "MDRW2100035887.1.1",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100035887.1.2",
          "form": "그런...천국이 있나요?",
          "original_form": "그런...천국이 있나요?",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100035887.1.3",
          "form": "신기하다...진짜 저 양념게장 뷔페는 봤거든요!",
          "original_form": "신기하다...진짜 저 양념게장 뷔페는 봤거든요!",
          "speaker_id": "2",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100035887.1.4",
          "form": "있더라구여... 넘 좋아요ㅋㅋㅋㅋ",
          "original_form": "있더라구여... 넘 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:35"
        },
        {
          "id": "MDRW2100035887.1.5",
          "form": "체인이라 주위에 있을수도 있어요!",
          "original_form": "체인이라 주위에 있을수도 있어요!",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100035887.1.6",
          "form": "여수가 꽃게로 유명하니까",
          "original_form": "여수가 꽃게로 유명하니까",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100035887.1.7",
          "form": "코로나 좀 잠잠해지면 내려가서 실컷 먹고오려구여",
          "original_form": "코로나 좀 잠잠해지면 내려가서 실컷 먹고오려구여",
          "speaker_id": "1",
          "time": "20210830 10:36"
        },
        {
          "id": "MDRW2100035887.1.8",
          "form": "우와 진짜 맛있겠어요ㅠ 저는 코로나 잠잠해지면... 부산가서 이재모피자를 다시먹는게 소원이에요",
          "original_form": "우와 진짜 맛있겠어요ㅠ 저는 코로나 잠잠해지면... 부산가서 이재모피자를 다시먹는게 소원이에요",
          "speaker_id": "2",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100035887.1.9",
          "form": "너무 맛있었거든요,,,,",
          "original_form": "너무 맛있었거든요,,,,",
          "speaker_id": "2",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100035887.1.10",
          "form": "이재모피자요??",
          "original_form": "이재모피자요??",
          "speaker_id": "1",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100035887.1.11",
          "form": "헐 처음들어봤어용",
          "original_form": "헐 처음들어봤어용",
          "speaker_id": "1",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100035887.1.12",
          "form": "혹시 화덕피자인가용?",
          "original_form": "혹시 화덕피자인가용?",
          "speaker_id": "1",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100035887.1.13",
          "form": "아뇨! 그냥 피자..?? 인데 진짜...",
          "original_form": "아뇨! 그냥 피자..?? 인데 진짜...",
          "speaker_id": "2",
          "time": "20210830 10:38"
        },
        {
          "id": "MDRW2100035887.1.14",
          "form": "여기 너무 맛있어요ㅠ 다음날 먹어도 촉촉하고....",
          "original_form": "여기 너무 맛있어요ㅠ 다음날 먹어도 촉촉하고....",
          "speaker_id": "2",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.15",
          "form": "아아 꼭 갈게요ㅠㅠㅠ 와 촉촉한 피자라니",
          "original_form": "아아 꼭 갈게요ㅠㅠㅠ 와 촉촉한 피자라니",
          "speaker_id": "1",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.16",
          "form": "그 피자 도우는",
          "original_form": "그 피자 도우는",
          "speaker_id": "1",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.17",
          "form": "저 피자 완전 좋아해서 여기저기 다 먹어봤는데 이재모가 진짜 최고더라고요...",
          "original_form": "저 피자 완전 좋아해서 여기저기 다 먹어봤는데 이재모가 진짜 최고더라고요...",
          "speaker_id": "2",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.18",
          "form": "적당한가용? 두께가!",
          "original_form": "적당한가용? 두께가!",
          "speaker_id": "1",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.19",
          "form": "네!!! 너무 두껍지도 않고!!!!!!!",
          "original_form": "네!!! 너무 두껍지도 않고!!!!!!!",
          "speaker_id": "2",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.20",
          "form": "아앗 이재모피자... 메모..",
          "original_form": "아앗 이재모피자... 메모..",
          "speaker_id": "1",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.21",
          "form": "진짜 사람들이 부산가서 왜 피자먹고오냐고했지만...",
          "original_form": "진짜 사람들이 부산가서 왜 피자먹고오냐고했지만...",
          "speaker_id": "2",
          "time": "20210830 10:39"
        },
        {
          "id": "MDRW2100035887.1.22",
          "form": "넘 두꺼운건 안좋아하는데 부산가면 꼭 먹기...",
          "original_form": "넘 두꺼운건 안좋아하는데 부산가면 꼭 먹기...",
          "speaker_id": "1",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100035887.1.23",
          "form": "그피자맛은 어디서도 느낄수가 없더라고요ㅠㅠ",
          "original_form": "그피자맛은 어디서도 느낄수가 없더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100035887.1.24",
          "form": "진짜 파스타도 같이 드셔야해요....",
          "original_form": "진짜 파스타도 같이 드셔야해요....",
          "speaker_id": "2",
          "time": "20210830 10:40"
        }
      ]
