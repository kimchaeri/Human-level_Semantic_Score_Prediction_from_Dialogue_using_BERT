      "utterance": [
        {
          "id": "MDRW2100044242.1.1",
          "form": "장영실은 이천희 ㅋㅋ",
          "original_form": "장영실은 이천희 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044242.1.2",
          "form": "김상경 하니깐 기억나요",
          "original_form": "김상경 하니깐 기억나요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044242.1.3",
          "form": "김상경이 세종으로 나와서 난 안본듯",
          "original_form": "김상경이 세종으로 나와서 난 안본듯",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044242.1.4",
          "form": "중년 남자들이 많이 봤자나요",
          "original_form": "중년 남자들이 많이 봤자나요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044242.1.5",
          "form": "엉 사극은 다 중년남성이 팬이다",
          "original_form": "엉 사극은 다 중년남성이 팬이다",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044242.1.6",
          "form": "용의 눈물부터 주구장창 봤죠",
          "original_form": "용의 눈물부터 주구장창 봤죠",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044242.1.7",
          "form": "09년에 미워도 다시 한번",
          "original_form": "09년에 미워도 다시 한번",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044242.1.8",
          "form": "영화예요?",
          "original_form": "영화예요?",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044242.1.9",
          "form": "중년드라마를 표방해서 최명길이 다시 주연",
          "original_form": "중년드라마를 표방해서 최명길이 다시 주연",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044242.1.10",
          "form": "오ㅋ 중년 남주겠네요",
          "original_form": "오ㅋ 중년 남주겠네요",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
