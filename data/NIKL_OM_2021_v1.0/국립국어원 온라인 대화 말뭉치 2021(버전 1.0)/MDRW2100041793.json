      "utterance": [
        {
          "id": "MDRW2100041793.1.1",
          "form": "다른사람 물건에 손대는 건 안 된다",
          "original_form": "다른사람 물건에 손대는 건 안 된다",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.2",
          "form": "그렇게 얘기하고 그냥 지나갔어",
          "original_form": "그렇게 얘기하고 그냥 지나갔어",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.3",
          "form": "근데 그 다음날",
          "original_form": "근데 그 다음날",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.4",
          "form": "그 아빠는 맘이 확 상했뿟네",
          "original_form": "그 아빠는 맘이 확 상했뿟네",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.5",
          "form": "name3 어린이집 친구 중에 제일 친하게 지내는 name4이라는 여자애가 있거든",
          "original_form": "&name3& 어린이집 친구 중에 제일 친하게 지내는 &name4&이라는 여자애가 있거든",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.6",
          "form": "걔가 영어도 같이 다니고 태권도 도 같이 다녀",
          "original_form": "걔가 영어도 같이 다니고 태권도 도 같이 다녀",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.7",
          "form": "네 네",
          "original_form": "네 네",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.8",
          "form": "근데 요새 name3가 그 name4이랑 계속 노는거야",
          "original_form": "근데 요새 &name3&가 그 &name4&이랑 계속 노는거야",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.9",
          "form": "태권도 마치면 항상 name5이랑 여기 와서 놀았는데",
          "original_form": "태권도 마치면 항상 &name5&이랑 여기 와서 놀았는데",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.10",
          "form": "요새 몇 번을 태권도 마치면",
          "original_form": "요새 몇 번을 태권도 마치면",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041793.1.11",
          "form": "우리 name5한테 말도 안하고 둘이서 address에서 내려 버리는 거야",
          "original_form": "우리 &name5&한테 말도 안하고 둘이서 &address&에서 내려 버리는 거야",
