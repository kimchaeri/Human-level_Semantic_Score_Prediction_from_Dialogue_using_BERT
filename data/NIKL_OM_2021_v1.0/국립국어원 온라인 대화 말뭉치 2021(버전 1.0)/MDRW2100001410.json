      "utterance": [
        {
          "id": "MDRW2100001410.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20211015 13:29"
        },
        {
          "id": "MDRW2100001410.1.2",
          "form": "안녕하세요~ 점심은 드셨나요?",
          "original_form": "안녕하세요~ 점심은 드셨나요?",
          "speaker_id": "2",
          "time": "20211015 13:30"
        },
        {
          "id": "MDRW2100001410.1.3",
          "form": "네 점심 라면에 계란 풀어서 밥이랑 같이 먹었어요.",
          "original_form": "네 점심 라면에 계란 풀어서 밥이랑 같이 먹었어요.",
          "speaker_id": "1",
          "time": "20211015 13:30"
        },
        {
          "id": "MDRW2100001410.1.4",
          "form": "점심은 맛있게 드셨나요?",
          "original_form": "점심은 맛있게 드셨나요?",
          "speaker_id": "1",
          "time": "20211015 13:30"
        },
        {
          "id": "MDRW2100001410.1.5",
          "form": "와 맛있었겠네요!",
          "original_form": "와 맛있었겠네요!",
          "speaker_id": "2",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.6",
          "form": "저는 간식을 많이 먹어서 패스요 ㅋㅋ",
          "original_form": "저는 간식을 많이 먹어서 패스요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.7",
          "form": "라면은 먹어도 먹어도 질리지가 않네요.",
          "original_form": "라면은 먹어도 먹어도 질리지가 않네요.",
          "speaker_id": "1",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.8",
          "form": "무슨 간식 드셨나요?",
          "original_form": "무슨 간식 드셨나요?",
          "speaker_id": "1",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.9",
          "form": "음.. 빼빼로랑 빅파이요",
          "original_form": "음.. 빼빼로랑 빅파이요",
          "speaker_id": "2",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.10",
          "form": "초코과자들 좋아하시나요?",
          "original_form": "초코과자들 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.11",
          "form": "초코과자는 못 참습니다.",
          "original_form": "초코과자는 못 참습니다.",
          "speaker_id": "1",
          "time": "20211015 13:31"
        },
        {
          "id": "MDRW2100001410.1.12",
          "form": "빅파이는 이제 이름값 못 하지 않나요?",
          "original_form": "빅파이는 이제 이름값 못 하지 않나요?",
          "speaker_id": "1",
          "time": "20211015 13:32"
        },
        {
          "id": "MDRW2100001410.1.13",
          "form": "스몰파이 같아요.",
          "original_form": "스몰파이 같아요.",
          "speaker_id": "1",
          "time": "20211015 13:32"
        },
        {
          "id": "MDRW2100001410.1.14",
          "form": "ㅋㅋㅋㅋ맞아요 그치만 맛은 그대로잖아요?",
          "original_form": "ㅋㅋㅋㅋ맞아요 그치만 맛은 그대로잖아요?",
          "speaker_id": "2",
          "time": "20211015 13:32"
        },
        {
          "id": "MDRW2100001410.1.15",
          "form": "제일 좋아하는 초코과자는 뭔가요?",
          "original_form": "제일 좋아하는 초코과자는 뭔가요?",
          "speaker_id": "2",
          "time": "20211015 13:32"
        },
        {
          "id": "MDRW2100001410.1.16",
          "form": "저는 홈런볼 좋아해요.",
          "original_form": "저는 홈런볼 좋아해요.",
          "speaker_id": "1",
          "time": "20211015 13:32"
        },
        {
          "id": "MDRW2100001410.1.17",
          "form": "홈런볼 먹다보면 순식간에 삭제 해요.",
          "original_form": "홈런볼 먹다보면 순식간에 삭제 해요.",
          "speaker_id": "1",
          "time": "20211015 13:32"
        },
        {
          "id": "MDRW2100001410.1.18",
          "form": "홈런볼은 입에 넣으면 녹아버리죠",
          "original_form": "홈런볼은 입에 넣으면 녹아버리죠",
          "speaker_id": "2",
          "time": "20211015 13:33"
        },
        {
          "id": "MDRW2100001410.1.19",
          "form": "홈런볼이 야구공 만하면 좋겠어요.",
          "original_form": "홈런볼이 야구공 만하면 좋겠어요.",
          "speaker_id": "1",
          "time": "20211015 13:34"
        },
        {
          "id": "MDRW2100001410.1.20",
          "form": "지금은 회사에 계세요?",
          "original_form": "지금은 회사에 계세요?",
          "speaker_id": "1",
          "time": "20211015 13:34"
        },
        {
          "id": "MDRW2100001410.1.21",
          "form": "ㅋㅋ홈런볼이 야구공만하면 슈크림볼 같겠네요",
          "original_form": "ㅋㅋ홈런볼이 야구공만하면 슈크림볼 같겠네요",
          "speaker_id": "2",
          "time": "20211015 13:35"
        },
        {
          "id": "MDRW2100001410.1.22",
          "form": "전 회삽니다",
          "original_form": "전 회삽니다",
          "speaker_id": "2",
          "time": "20211015 13:35"
        },
        {
          "id": "MDRW2100001410.1.23",
          "form": "저는 회 팝니다.",
          "original_form": "저는 회 팝니다.",
          "speaker_id": "1",
          "time": "20211015 13:35"
        },
        {
          "id": "MDRW2100001410.1.24",
          "form": "농담입니다~",
          "original_form": "농담입니다~",
          "speaker_id": "1",
          "time": "20211015 13:35"
        },
        {
          "id": "MDRW2100001410.1.25",
          "form": "춘추가 어떻게 되시나요?",
          "original_form": "춘추가 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20211015 13:35"
        },
        {
          "id": "MDRW2100001410.1.26",
          "form": "불혹입니다",
          "original_form": "불혹입니다",
          "speaker_id": "2",
          "time": "20211015 13:36"
        },
        {
          "id": "MDRW2100001410.1.27",
          "form": "농담입니다~ㅋㅋ",
          "original_form": "농담입니다~ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 13:36"
        },
        {
          "id": "MDRW2100001410.1.28",
          "form": "아이고 형님이셨네요.",
          "original_form": "아이고 형님이셨네요.",
          "speaker_id": "1",
          "time": "20211015 13:36"
        },
        {
          "id": "MDRW2100001410.1.29",
          "form": "제가 오만 방자했다면",
          "original_form": "제가 오만 방자했다면",
          "speaker_id": "1",
          "time": "20211015 13:36"
        },
        {
          "id": "MDRW2100001410.1.30",
          "form": "너그럽게 용서해주십시오.",
          "original_form": "너그럽게 용서해주십시오.",
          "speaker_id": "1",
          "time": "20211015 13:36"
        },
        {
          "id": "MDRW2100001410.1.31",
          "form": "오늘 금요일 이니까 넓은 아량으로 이해 바랍니다.",
          "original_form": "오늘 금요일 이니까 넓은 아량으로 이해 바랍니다.",
          "speaker_id": "1",
          "time": "20211015 13:37"
        },
        {
          "id": "MDRW2100001410.1.32",
          "form": "ㅋㅋㅋ뭘요",
          "original_form": "ㅋㅋㅋ뭘요",
          "speaker_id": "2",
          "time": "20211015 13:37"
        },
        {
          "id": "MDRW2100001410.1.33",
          "form": "오늘 금요일인데 뭐 약속 있으신가요",
          "original_form": "오늘 금요일인데 뭐 약속 있으신가요",
          "speaker_id": "2",
          "time": "20211015 13:37"
        },
        {
          "id": "MDRW2100001410.1.34",
          "form": "금요일 저녁마다 친구들이랑 풋살 약속 있어요.",
          "original_form": "금요일 저녁마다 친구들이랑 풋살 약속 있어요.",
          "speaker_id": "1",
          "time": "20211015 13:38"
        },
        {
          "id": "MDRW2100001410.1.35",
          "form": "오 건강한 취미네요",
          "original_form": "오 건강한 취미네요",
          "speaker_id": "2",
          "time": "20211015 13:38"
        }
      ]
