      "utterance": [
        {
          "id": "MDRW2100014204.1.1",
          "form": "요즘 날씨 너무 별로에요",
          "original_form": "요즘 날씨 너무 별로에요",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014204.1.2",
          "form": "비가 너무 많이와",
          "original_form": "비가 너무 많이와",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014204.1.3",
          "form": "name2 지역은 어때요",
          "original_form": "&name2& 지역은 어때요",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014204.1.4",
          "form": "저희는 비는 안오는데",
          "original_form": "저희는 비는 안오는데",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014204.1.5",
          "form": "꿉꿉한거 미쳣어요",
          "original_form": "꿉꿉한거 미쳣어요",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014204.1.6",
          "form": "제습기 가동했슴다",
          "original_form": "제습기 가동했슴다",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014204.1.7",
          "form": "습한거 너무 싫죠",
          "original_form": "습한거 너무 싫죠",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014204.1.8",
          "form": "제습기 부럽다",
          "original_form": "제습기 부럽다",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014204.1.9",
          "form": "전 제습기없어서 에어컨틀어놨는데도 습해요",
          "original_form": "전 제습기없어서 에어컨틀어놨는데도 습해요",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014204.1.10",
          "form": "비오면 글쵸ㅜㅠㅠㅠㅠ",
          "original_form": "비오면 글쵸ㅜㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014204.1.11",
          "form": "긍까요 빨래도 잘 안마르고",
          "original_form": "긍까요 빨래도 잘 안마르고",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014204.1.12",
          "form": "좋아하는 계절 뭐라하셨죠",
          "original_form": "좋아하는 계절 뭐라하셨죠",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014204.1.13",
          "form": "아직 말 안해줬어요",
          "original_form": "아직 말 안해줬어요",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014204.1.14",
          "form": "뭘거같아요",
          "original_form": "뭘거같아요",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014204.1.15",
          "form": "쓰읍",
          "original_form": "쓰읍",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014204.1.16",
          "form": "제삘에",
          "original_form": "제삘에",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.17",
          "form": "가을.",
          "original_form": "가을.",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.18",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.19",
          "form": "독심술사인가",
          "original_form": "독심술사인가",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.20",
          "form": "맞췄나요????",
          "original_form": "맞췄나요????",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.21",
          "form": "왐마",
          "original_form": "왐마",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.22",
          "form": "가을 치고있었거든요",
          "original_form": "가을 치고있었거든요",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.23",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.24",
          "form": "대단하다",
          "original_form": "대단하다",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.25",
          "form": "한국사람이면 가을이죠.",
          "original_form": "한국사람이면 가을이죠.",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.26",
          "form": "그때만 살맛난다",
          "original_form": "그때만 살맛난다",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.27",
          "form": "그쵸 시원하고",
          "original_form": "그쵸 시원하고",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.28",
          "form": "맛있는것도 많고",
          "original_form": "맛있는것도 많고",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.30",
          "form": "가을 최고",
          "original_form": "가을 최고",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014204.1.31",
          "form": "오늘 날씨어때요",
          "original_form": "오늘 날씨어때요",
