      "utterance": [
        {
          "id": "MDRW2100034688.1.1",
          "form": "진짜 요리가 젱 어렵다 ㅋㅋㅋㅋ",
          "original_form": "진짜 요리가 젱 어렵다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034688.1.2",
          "form": "안 는다 ㅋㅋㅋㅋ",
          "original_form": "안 는다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100034688.1.3",
          "form": "나도 그렇다ㅋ",
          "original_form": "나도 그렇다ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034688.1.4",
          "form": "오늘 콩나물밥하는데",
          "original_form": "오늘 콩나물밥하는데",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034688.1.5",
          "form": "레시피가 다 고기드간거야",
          "original_form": "레시피가 다 고기드간거야",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034688.1.6",
          "form": "다른 레시피는 또 액젓드간대",
          "original_form": "다른 레시피는 또 액젓드간대",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034688.1.7",
          "form": "이거저거 찾다가",
          "original_form": "이거저거 찾다가",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100034688.1.8",
          "form": "콩나물밥에 액젓이??",
          "original_form": "콩나물밥에 액젓이??",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.9",
          "form": "시간 좀 보냈지",
          "original_form": "시간 좀 보냈지",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.10",
          "form": "오빠가",
          "original_form": "오빠가",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.11",
          "form": "그냥 콩나물만 넣는거 아이가 ㅋㅋㅋㅋ",
          "original_form": "그냥 콩나물만 넣는거 아이가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.12",
          "form": "그냥 콩나물넣고",
          "original_form": "그냥 콩나물넣고",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.13",
          "form": "소스하면되지",
          "original_form": "소스하면되지",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.14",
          "form": "뭔고기타령이냐",
          "original_form": "뭔고기타령이냐",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.15",
          "form": "그래~간장에 파쫌넣고 ㅋㅋㅋㅋ",
          "original_form": "그래~간장에 파쫌넣고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.16",
          "form": "그래서 내가 고기넣음 더 맛있는데",
          "original_form": "그래서 내가 고기넣음 더 맛있는데",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.17",
          "form": "그래만 먹어도 맛있다 ㅋㅋㅋㅋㅋ",
          "original_form": "그래만 먹어도 맛있다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.18",
          "form": "고기안드간 레시피가없다",
          "original_form": "고기안드간 레시피가없다",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034688.1.19",
          "form": "이랬더니만 간단한건데도",
          "original_form": "이랬더니만 간단한건데도",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.20",
          "form": "레시피에의존하니ㅋㅋ",
          "original_form": "레시피에의존하니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.21",
          "form": "신랑이 답답했겠지ㅋㅋ",
          "original_form": "신랑이 답답했겠지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ레시피에 의존 안하면 맛이 천차만별 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ레시피에 의존 안하면 맛이 천차만별 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.23",
          "form": "그래서 내가 저리 가있으라구ㅋㅋ얘기하고 진짜 콩나물넣고",
          "original_form": "그래서 내가 저리 가있으라구ㅋㅋ얘기하고 진짜 콩나물넣고",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.24",
          "form": "간장소스하는 레시피찾아서함",
          "original_form": "간장소스하는 레시피찾아서함",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ그러이ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034688.1.26",
          "form": "우린 맛에 쫌 관대하잖아 ㅋㅋㅋㅋ",
          "original_form": "우린 맛에 쫌 관대하잖아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034688.1.27",
          "form": "맛이없어. 저번에는 약간 콩나물비린내 나는거야",
          "original_form": "맛이없어. 저번에는 약간 콩나물비린내 나는거야",
          "speaker_id": "1",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034688.1.28",
          "form": "뭐든 맛있음 ㅋㅋㅋ",
          "original_form": "뭐든 맛있음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034688.1.29",
          "form": "난감하더라고ㅋㅋ",
          "original_form": "난감하더라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034688.1.30",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034688.1.31",
          "form": "근데 이번엔 간단하게했는데도 괜찮더라ㅋㅋ",
          "original_form": "근데 이번엔 간단하게했는데도 괜찮더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:39"
        }
      ]
    }
