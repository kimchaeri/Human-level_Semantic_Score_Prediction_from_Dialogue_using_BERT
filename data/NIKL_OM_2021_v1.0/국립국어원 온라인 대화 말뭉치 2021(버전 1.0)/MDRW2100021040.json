      "utterance": [
        {
          "id": "MDRW2100021040.1.1",
          "form": "타란튤라는 독있지 않나요?ㄷㄷ",
          "original_form": "타란튤라는 독있지 않나요?ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100021040.1.2",
          "form": "크기만 크고 독은 없을걸요?",
          "original_form": "크기만 크고 독은 없을걸요?",
          "speaker_id": "2",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100021040.1.3",
          "form": "키우는 형 본 적 있어요 ㅋㅋㅋ",
          "original_form": "키우는 형 본 적 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100021040.1.4",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100021040.1.5",
          "form": "그래도 거미는 왠지 벌레종류인거 같아서 ㅎㅎ",
          "original_form": "그래도 거미는 왠지 벌레종류인거 같아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021040.1.6",
          "form": "파충류 좋아하세요?",
          "original_form": "파충류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021040.1.7",
          "form": "좋아하기보단 흥미롭게 보고 있어요",
          "original_form": "좋아하기보단 흥미롭게 보고 있어요",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021040.1.8",
          "form": "막 산책 시켜줘야되고 그런게 아니니까 키우기 쉽지 않을까 싶어서요",
          "original_form": "막 산책 시켜줘야되고 그런게 아니니까 키우기 쉽지 않을까 싶어서요",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021040.1.9",
          "form": "오 ᆢ 어떤 종류요? 뱀 이구아나?",
          "original_form": "오 ᆢ 어떤 종류요? 뱀 이구아나?",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021040.1.10",
          "form": "전 카멜레온이요",
          "original_form": "전 카멜레온이요",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021040.1.11",
          "form": "색깔 바뀌는 것도 되게 신기해요",
          "original_form": "색깔 바뀌는 것도 되게 신기해요",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021040.1.12",
          "form": "키우기 쉬울거같긴하네요",
          "original_form": "키우기 쉬울거같긴하네요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021040.1.13",
          "form": "국내에서 쉽게 구할수있나요?",
          "original_form": "국내에서 쉽게 구할수있나요?",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021040.1.14",
          "form": "시도는 안해봐서 모르겠어요",
          "original_form": "시도는 안해봐서 모르겠어요",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021040.1.15",
          "form": "아마 전문적으로 파는데가 따로 있다고 들었어요",
          "original_form": "아마 전문적으로 파는데가 따로 있다고 들었어요",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021040.1.16",
          "form": "하긴 좋아하는 사람들을 위합 샵이 있겠네요",
          "original_form": "하긴 좋아하는 사람들을 위합 샵이 있겠네요",
          "speaker_id": "1",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100021040.1.17",
          "form": "근데 물고기도 수족관 전문 샵 있잖아요 ㅎ",
          "original_form": "근데 물고기도 수족관 전문 샵 있잖아요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100021040.1.18",
          "form": "다양한 반려동물 키우기 좋은 시대 같아요",
          "original_form": "다양한 반려동물 키우기 좋은 시대 같아요",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100021040.1.19",
          "form": "아 맞아요 요즘은 대형마트에만가도 수족관 있더라구요",
          "original_form": "아 맞아요 요즘은 대형마트에만가도 수족관 있더라구요",
          "speaker_id": "1",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021040.1.20",
          "form": "구피 같은 것도 키워보고 싶어요!",
          "original_form": "구피 같은 것도 키워보고 싶어요!",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100021040.1.21",
          "form": "수족관 크게 키우면 멋있죠ㅎ",
          "original_form": "수족관 크게 키우면 멋있죠ㅎ",
          "speaker_id": "1",
          "time": "20210828 16:02"
        }
      ]
