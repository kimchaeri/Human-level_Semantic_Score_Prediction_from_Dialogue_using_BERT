      "utterance": [
        {
          "id": "MDRW2100034046.1.1",
          "form": "오늘 점심은 어떤거 드셨어요?",
          "original_form": "오늘 점심은 어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100034046.1.2",
          "form": "샐러드와 크로와상 먹었어요",
          "original_form": "샐러드와 크로와상 먹었어요",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100034046.1.3",
          "form": "오 맛있겠네요 ㅎㅎ !",
          "original_form": "오 맛있겠네요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100034046.1.4",
          "form": "어떤 샐러드 드셨어요?",
          "original_form": "어떤 샐러드 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100034046.1.5",
          "form": "닭가슴살 샌드위치 먹었어요",
          "original_form": "닭가슴살 샌드위치 먹었어요",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100034046.1.6",
          "form": "지금 다이어트 하시나요 ??!",
          "original_form": "지금 다이어트 하시나요 ??!",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100034046.1.7",
          "form": "아뇨! 집에 있어서 들고 왔어요ㅋㅋ",
          "original_form": "아뇨! 집에 있어서 들고 왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100034046.1.8",
          "form": "아아 ! 저도 샌드위치 좋아하는데",
          "original_form": "아아 ! 저도 샌드위치 좋아하는데",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100034046.1.9",
          "form": "주변에 샌드위치 맛집이 없네요 ㅋㅋ",
          "original_form": "주변에 샌드위치 맛집이 없네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100034046.1.10",
          "form": "저도 코스트코에서 사온거예요!",
          "original_form": "저도 코스트코에서 사온거예요!",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100034046.1.11",
          "form": "오오 코스트코꺼 맛있나요??",
          "original_form": "오오 코스트코꺼 맛있나요??",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100034046.1.12",
          "form": "가성비있고 괜찮아요!",
          "original_form": "가성비있고 괜찮아요!",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100034046.1.13",
          "form": "담에 먹어봐야겠네요! 코스트코에서 거의 초밥만 먹은거 같아요 ㅋㅋ",
          "original_form": "담에 먹어봐야겠네요! 코스트코에서 거의 초밥만 먹은거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100034046.1.14",
          "form": "코스트코 치즈케익도 맛있죠ㅋㅋ",
          "original_form": "코스트코 치즈케익도 맛있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100034046.1.15",
          "form": "아 맞아요 ! 맛있고 완전 가성비 있죠 ㅋㅋ",
          "original_form": "아 맞아요 ! 맛있고 완전 가성비 있죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100034046.1.16",
          "form": "베이글 종류도 꽤 괜찮았어요 ㅎㅎ",
          "original_form": "베이글 종류도 꽤 괜찮았어요 ㅎㅎ",
