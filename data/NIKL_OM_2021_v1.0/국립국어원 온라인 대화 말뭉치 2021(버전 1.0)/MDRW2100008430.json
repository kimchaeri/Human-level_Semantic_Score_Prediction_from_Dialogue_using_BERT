        {
          "id": "MDRW2100008430.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008430.1.2",
          "form": "네 폰게임 합니다",
          "original_form": "네 폰게임 합니다",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008430.1.3",
          "form": "저도 폰",
          "original_form": "저도 폰",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008430.1.4",
          "form": "브롤해요",
          "original_form": "브롤해요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008430.1.5",
          "form": "전 퍼즐쪽이나 퀴즈게임을 좋아해요",
          "original_form": "전 퍼즐쪽이나 퀴즈게임을 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008430.1.6",
          "form": "머리 아픈거 아녜요?",
          "original_form": "머리 아픈거 아녜요?",
          "speaker_id": "1",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008430.1.7",
          "form": "혼자서 이것 저것 고민해서 푸는 게임이 좋아요",
          "original_form": "혼자서 이것 저것 고민해서 푸는 게임이 좋아요",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008430.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008430.1.9",
          "form": "혼자하는 거 좋아하세여",
          "original_form": "혼자하는 거 좋아하세여",
          "speaker_id": "1",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008430.1.10",
          "form": "팀플은 못하면 피해줄까봐 좀 그러네요",
          "original_form": "팀플은 못하면 피해줄까봐 좀 그러네요",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008430.1.11",
          "form": "실력키워서 합류하면 되죠",
          "original_form": "실력키워서 합류하면 되죠",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008430.1.12",
          "form": "실력이 늘지않아요",
          "original_form": "실력이 늘지않아요",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008430.1.13",
          "form": "자주 안하나 봐요",
          "original_form": "자주 안하나 봐요",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008430.1.14",
          "form": "혼자하는 게임 주로 하니깐 그렇겠죠",
          "original_form": "혼자하는 게임 주로 하니깐 그렇겠죠",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008430.1.15",
          "form": "혼자해도 재밌으면 그만이죠",
          "original_form": "혼자해도 재밌으면 그만이죠",
