        },
        {
          "id": "MDRW2100004752.1.58",
          "form": "name2님도 그러신가요?",
          "original_form": "&name2&님도 그러신가요?",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004752.1.59",
          "form": "전 여자가 아니라 궁금해서요ㅠ",
          "original_form": "전 여자가 아니라 궁금해서요ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
