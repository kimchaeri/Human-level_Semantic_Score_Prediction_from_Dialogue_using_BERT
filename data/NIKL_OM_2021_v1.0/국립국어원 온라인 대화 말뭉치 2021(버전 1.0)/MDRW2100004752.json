      "utterance": [
        {
          "id": "MDRW2100004752.1.1",
          "form": "아네네네",
          "original_form": "아네네네",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004752.1.2",
          "form": "지금 결혼 몇년차세요? ㅎ",
          "original_form": "지금 결혼 몇년차세요? ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004752.1.3",
          "form": "지금 3년차네요ㅎㅎ",
          "original_form": "지금 3년차네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004752.1.4",
          "form": "아이한명 있구요ㅎ",
          "original_form": "아이한명 있구요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004752.1.5",
          "form": "곧 이혼할 예정이구요ㅎㅎㅎㅎㅎㄹ",
          "original_form": "곧 이혼할 예정이구요ㅎㅎㅎㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004752.1.6",
          "form": "이혼이여?????",
          "original_form": "이혼이여?????",
          "speaker_id": "2",
          "time": "20210821 18:24"
        },
        {
          "id": "MDRW2100004752.1.7",
          "form": "이럴수가  왜요???.",
          "original_form": "이럴수가  왜요???.",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004752.1.8",
          "form": "앗 죄송해요. 이런것까지 물으몬 신뢰인데",
          "original_form": "앗 죄송해요. 이런것까지 물으몬 신뢰인데",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004752.1.9",
          "form": "애기도 있는데 너무 놀래서요",
          "original_form": "애기도 있는데 너무 놀래서요",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004752.1.10",
          "form": "나름 이유가 있으시겠죠 ㅜㅜㅜ",
          "original_form": "나름 이유가 있으시겠죠 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 18:25"
        },
        {
          "id": "MDRW2100004752.1.11",
          "form": "저도 결혼 5년차인데 매일 천당 지옥이에요",
          "original_form": "저도 결혼 5년차인데 매일 천당 지옥이에요",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004752.1.12",
          "form": "과거로 돌아간다면 솔로이고 십네요",
          "original_form": "과거로 돌아간다면 솔로이고 십네요",
          "speaker_id": "2",
          "time": "20210821 18:26"
        },
        {
          "id": "MDRW2100004752.1.13",
          "form": "아니에요ㅎ 죄송할건 없어요ㅎㄹ",
          "original_form": "아니에요ㅎ 죄송할건 없어요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004752.1.14",
          "form": "이혼하게 된데는 이유가없나봐요ㅎ 서로의 문제이지 누굴 탓하고 그런게 아니라ㅎㅎ",
          "original_form": "이혼하게 된데는 이유가없나봐요ㅎ 서로의 문제이지 누굴 탓하고 그런게 아니라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004752.1.15",
          "form": "아이를 너무 사랑하지만 서로 상처를 줬기 때문에 이혼하는거라 생각해요ㄹㄹ",
          "original_form": "아이를 너무 사랑하지만 서로 상처를 줬기 때문에 이혼하는거라 생각해요ㄹㄹ",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004752.1.16",
          "form": "아마 저랑 같은 경험을 하셨을꺼에요ㅎㅎ 솔로로 돌아가고 싶다는ㅎㅎ",
          "original_form": "아마 저랑 같은 경험을 하셨을꺼에요ㅎㅎ 솔로로 돌아가고 싶다는ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004752.1.17",
          "form": "힘내세요",
          "original_form": "힘내세요",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004752.1.18",
          "form": "앞으로 더 좋은일만 있을실거에요",
          "original_form": "앞으로 더 좋은일만 있을실거에요",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.19",
          "form": "힘내야죠ㅎ 아이에게 좋은 아빠가 되어야하니깐요ㅎ",
          "original_form": "힘내야죠ㅎ 아이에게 좋은 아빠가 되어야하니깐요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.20",
          "form": "결혼생활은 나만에 뜻대로만은 안되는거같아요",
          "original_form": "결혼생활은 나만에 뜻대로만은 안되는거같아요",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.21",
          "form": "정말 아이를 위해서는 아닌데ㅎ 아내의 입장도 고려해야하잖아요ㅎ",
          "original_form": "정말 아이를 위해서는 아닌데ㅎ 아내의 입장도 고려해야하잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.22",
          "form": "멋지세요!",
          "original_form": "멋지세요!",
          "speaker_id": "2",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.23",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.24",
          "form": "내마음대로 안되죠 정말",
          "original_form": "내마음대로 안되죠 정말",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004752.1.25",
          "form": "연애할땐 안그랬는데 말이죠",
          "original_form": "연애할땐 안그랬는데 말이죠",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004752.1.26",
          "form": "뭐가 가장힘들어요?ㅎ",
          "original_form": "뭐가 가장힘들어요?ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004752.1.27",
          "form": "전 지금 남편이 주말부부라 독박육아거든요 ㅜ",
          "original_form": "전 지금 남편이 주말부부라 독박육아거든요 ㅜ",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004752.1.28",
          "form": "주말도 한달에 한번 볼까말까해서",
          "original_form": "주말도 한달에 한번 볼까말까해서",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.29",
          "form": "애키우다 늙어요",
          "original_form": "애키우다 늙어요",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.30",
          "form": "그건 정말 힘들겟네요ㅠ",
          "original_form": "그건 정말 힘들겟네요ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.31",
          "form": "애기 한명이에요?",
          "original_form": "애기 한명이에요?",
          "speaker_id": "1",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.32",
          "form": "네 그래서 그게 지금 재일 힘들어요",
          "original_form": "네 그래서 그게 지금 재일 힘들어요",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.33",
          "form": "애기 둘이용 연년생 ㅜ",
          "original_form": "애기 둘이용 연년생 ㅜ",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.34",
          "form": "어쩌다보니",
          "original_form": "어쩌다보니",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004752.1.35",
          "form": "결혼생활은 참으로 어려워요",
          "original_form": "결혼생활은 참으로 어려워요",
          "speaker_id": "2",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004752.1.36",
          "form": "에고ㅠ 힘드시겟어요ㅠㅠ",
          "original_form": "에고ㅠ 힘드시겟어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004752.1.37",
          "form": "어렵죠ㅍ",
          "original_form": "어렵죠ㅍ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004752.1.38",
          "form": "30몇년을 다르게 살아온사람인데",
          "original_form": "30몇년을 다르게 살아온사람인데",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004752.1.39",
          "form": "하루아침에 맞춰지지도 않을 뿐더러 아이도 내맘대로 안되고",
          "original_form": "하루아침에 맞춰지지도 않을 뿐더러 아이도 내맘대로 안되고",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004752.1.40",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004752.1.41",
          "form": "결혼은 서로 다른사람끼리 맞춰가는거라는데",
          "original_form": "결혼은 서로 다른사람끼리 맞춰가는거라는데",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004752.1.42",
          "form": "그과정이 힘든거 같아요",
          "original_form": "그과정이 힘든거 같아요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004752.1.43",
          "form": "저도 그렇게 생각해요 결혼은 정말 힘들어요ㅎ",
          "original_form": "저도 그렇게 생각해요 결혼은 정말 힘들어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004752.1.44",
          "form": "그래도 아이보면 너무행복해요ㅎ",
          "original_form": "그래도 아이보면 너무행복해요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004752.1.45",
          "form": "저도 그건 공감해요",
          "original_form": "저도 그건 공감해요",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004752.1.46",
          "form": "연애하고 결혼 했지만 아이만 보게되는 아이러니한 상황이 생기게되네요ㅎ",
          "original_form": "연애하고 결혼 했지만 아이만 보게되는 아이러니한 상황이 생기게되네요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004752.1.47",
          "form": "아이가 오늘도 아빠를 이세상에서 최고로 사랑해 하는데ㅎㅎ 얼마나 기쁘던지",
          "original_form": "아이가 오늘도 아빠를 이세상에서 최고로 사랑해 하는데ㅎㅎ 얼마나 기쁘던지",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004752.1.48",
          "form": "저도 연애하고 결혼하 케이스에요",
          "original_form": "저도 연애하고 결혼하 케이스에요",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004752.1.49",
          "form": "결혼한",
          "original_form": "결혼한",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004752.1.50",
          "form": "정말 그런말 해주면 감동이에요",
          "original_form": "정말 그런말 해주면 감동이에요",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004752.1.51",
          "form": "하트뿅뿅이죠",
          "original_form": "하트뿅뿅이죠",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004752.1.52",
          "form": "맞아요ㅎ 아이때문에 사는것 같애요ㅎ",
          "original_form": "맞아요ㅎ 아이때문에 사는것 같애요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004752.1.53",
          "form": "정말 슬프네요 ㅜ",
          "original_form": "정말 슬프네요 ㅜ",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004752.1.54",
          "form": "그게 전 좀 이해가 가지않아요ㅠㅠ",
          "original_form": "그게 전 좀 이해가 가지않아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004752.1.55",
          "form": "이혼한 사유중에 하나라",
          "original_form": "이혼한 사유중에 하나라",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004752.1.56",
          "form": "아이에겐 사랑이 넘치는데 저에겐 아니더라구요ㅎ",
          "original_form": "아이에겐 사랑이 넘치는데 저에겐 아니더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004752.1.57",
          "form": "정말슬프네요",
          "original_form": "정말슬프네요",
