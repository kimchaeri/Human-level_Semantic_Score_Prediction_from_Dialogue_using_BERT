      "utterance": [
        {
          "id": "MDRW2100001994.1.1",
          "form": "날씨어떼요?",
          "original_form": "날씨어떼요?",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001994.1.2",
          "form": "덥네여 ㅎㅎ  언제쯤 가을될련지",
          "original_form": "덥네여 ㅎㅎ  언제쯤 가을될련지",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001994.1.3",
          "form": "밤에는 쌀쌀하죠",
          "original_form": "밤에는 쌀쌀하죠",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001994.1.4",
          "form": "새벽엔 춥더라구요\r\n아침마다 기침을 ㅋ",
          "original_form": "새벽엔 춥더라구요\r\n아침마다 기침을 ㅋ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001994.1.5",
          "form": "잘때 창문 닫고 자요 저도",
          "original_form": "잘때 창문 닫고 자요 저도",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001994.1.6",
          "form": "창문닫아노 썰렁하더라구요",
          "original_form": "창문닫아노 썰렁하더라구요",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001994.1.7",
          "form": "이불돌돌이죠",
          "original_form": "이불돌돌이죠",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001994.1.8",
          "form": "ㅋ 이불은 자다보먄 어디론가 사라진 !!",
          "original_form": "ㅋ 이불은 자다보먄 어디론가 사라진 !!",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100001994.1.9",
          "form": "전 시체처럼 자서요 ㅎㅎ",
          "original_form": "전 시체처럼 자서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100001994.1.10",
          "form": "전 자는건 그대론데\r\n던지나봐요 야구선순가 ㅋㅋㅋㅋ",
          "original_form": "전 자는건 그대론데\r\n던지나봐요 야구선순가 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100001994.1.11",
          "form": "ㅋㅋ 멀리 가나 보네요",
          "original_form": "ㅋㅋ 멀리 가나 보네요",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100001994.1.12",
          "form": "얇아서 그런지 ㅋ\r\n가을됐음 좋겠어요 ㅎㅎ",
          "original_form": "얇아서 그런지 ㅋ\r\n가을됐음 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100001994.1.13",
          "form": "낮에도 선선했으면",
          "original_form": "낮에도 선선했으면",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100001994.1.14",
          "form": "가을이 넘짧아서 탈이지만요",
          "original_form": "가을이 넘짧아서 탈이지만요",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100001994.1.15",
          "form": "금방 겨울오더라그요",
          "original_form": "금방 겨울오더라그요",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100001994.1.16",
          "form": "그래도 더운 보단 ~ 추운게 좋아요 ㅎㅎ",
