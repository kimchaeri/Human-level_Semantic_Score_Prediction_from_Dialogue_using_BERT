      "utterance": [
        {
          "id": "MDRW2100006046.1.1",
          "form": "집안일 하시나요?",
          "original_form": "집안일 하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100006046.1.2",
          "form": "네~ 분담해서 해요ㅋㅋㅋㅋ",
          "original_form": "네~ 분담해서 해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006046.1.3",
          "form": "집안일을 같이 하시나요?",
          "original_form": "집안일을 같이 하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006046.1.4",
          "form": "네 같이 합니다 주로 어떤거 하세요",
          "original_form": "네 같이 합니다 주로 어떤거 하세요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006046.1.5",
          "form": "전 집에서 설겆이 당담이에요~ 빨래도 일부 맡고 있죠ㅋㅋㅋㅋ",
          "original_form": "전 집에서 설겆이 당담이에요~ 빨래도 일부 맡고 있죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100006046.1.6",
          "form": "요리는 누가 하나요?",
          "original_form": "요리는 누가 하나요?",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006046.1.7",
          "form": "어떤걸 당담?",
          "original_form": "어떤걸 당담?",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006046.1.8",
          "form": "요리는 엄마요ㅋㅋㅋㅋ",
          "original_form": "요리는 엄마요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006046.1.9",
          "form": "재료준비랑은 나누어서 하지만요ㅎㅎ",
          "original_form": "재료준비랑은 나누어서 하지만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006046.1.10",
          "form": "그래도 이것저것 같이 도우시네요!",
          "original_form": "그래도 이것저것 같이 도우시네요!",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006046.1.11",
          "form": "요리당담이신가요!?",
          "original_form": "요리당담이신가요!?",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006046.1.12",
          "form": "저는 아이 목욕.양치.분리수거.화장실청소 담당입니다..ㅎㅎ 다른것 도 같이하구요 ㅎㅎ",
          "original_form": "저는 아이 목욕.양치.분리수거.화장실청소 담당입니다..ㅎㅎ 다른것 도 같이하구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100006046.1.13",
          "form": "손이 많이 가는걸 챙기시네요 ㅎㅎ",
          "original_form": "손이 많이 가는걸 챙기시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006046.1.14",
          "form": "아이가 name1님을 잘 따르나봐요 ㅎㅎ",
          "original_form": "아이가 &name1&님을 잘 따르나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006046.1.15",
          "form": "네..첫째는 아빠를 더 좋아하네요 ㅎㅎ",
          "original_form": "네..첫째는 아빠를 더 좋아하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100006046.1.16",
          "form": "좋은 아빠인가봐요? ",
          "original_form": "좋은 아빠인가봐요? ",
          "speaker_id": "2",
          "time": "20210907 20:18"
