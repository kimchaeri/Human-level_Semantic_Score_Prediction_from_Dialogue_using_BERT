      "utterance": [
        {
          "id": "MDRW2100020603.1.1",
          "form": "이제 어떤걸로 가볼까요",
          "original_form": "이제 어떤걸로 가볼까요",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020603.1.2",
          "form": "보자..",
          "original_form": "보자..",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020603.1.3",
          "form": "과일 뭐 좋아하세요",
          "original_form": "과일 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020603.1.4",
          "form": "수박이요",
          "original_form": "수박이요",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020603.1.5",
          "form": "참외두요",
          "original_form": "참외두요",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020603.1.6",
          "form": "메론도?",
          "original_form": "메론도?",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020603.1.7",
          "form": "걱정하지말아요 음식은 주제가 질려\n미\n때 까지 있으니까",
          "original_form": "걱정하지말아요 음식은 주제가 질려\n미\n때 까지 있으니까",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.8",
          "form": "약간 비슷한 계열 과일 좋아하시는구나",
          "original_form": "약간 비슷한 계열 과일 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ음식으로 말이 잘 통하는 분을 만나서 다행이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음식으로 말이 잘 통하는 분을 만나서 다행이에요",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.10",
          "form": "박종류",
          "original_form": "박종류",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.11",
          "form": "저도 박종류 좋아합니다",
          "original_form": "저도 박종류 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.12",
          "form": "예 그렇답니다",
          "original_form": "예 그렇답니다",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.13",
          "form": "물기많은 과일을 좋아해요",
          "original_form": "물기많은 과일을 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.14",
          "form": "약간 시원하고",
          "original_form": "약간 시원하고",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020603.1.15",
          "form": "과즙넘치고",
          "original_form": "과즙넘치고",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.16",
          "form": "아삭거리는",
          "original_form": "아삭거리는",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.17",
          "form": "네 플러스로 청포도도 좋아해요",
          "original_form": "네 플러스로 청포도도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.18",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.19",
          "form": "청포도 완전좋아",
          "original_form": "청포도 완전좋아",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.20",
          "form": "망고 좋아하시나요",
          "original_form": "망고 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.21",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.22",
          "form": "저는 약간",
          "original_form": "저는 약간",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.23",
          "form": "전 망고는 태국망고좋아합니다",
          "original_form": "전 망고는 태국망고좋아합니다",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.24",
          "form": "한국과일파랍니다",
          "original_form": "한국과일파랍니다",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.25",
          "form": "망고젤리는 좋아해요",
          "original_form": "망고젤리는 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020603.1.26",
          "form": "앗.. 낚였다",
          "original_form": "앗.. 낚였다",
          "speaker_id": "1",
          "time": "20210828 10:23"
        }
      ]
