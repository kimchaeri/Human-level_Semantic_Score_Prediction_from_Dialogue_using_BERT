      "utterance": [
        {
          "id": "MDRW2100045867.1.1",
          "form": "기차는 무조건 창가석으로 예매하세요 ㅎㅎ",
          "original_form": "기차는 무조건 창가석으로 예매하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.2",
          "form": "경비 대충만 여쭤보면 실례가 될까요? 코로나가 진정되면 스위스만큼은 꼭 가보고싶어서요",
          "original_form": "경비 대충만 여쭤보면 실례가 될까요? 코로나가 진정되면 스위스만큼은 꼭 가보고싶어서요",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.3",
          "form": "5박이요!",
          "original_form": "5박이요!",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.4",
          "form": "진짜 창가석 무조건!!!!!!!",
          "original_form": "진짜 창가석 무조건!!!!!!!",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.5",
          "form": "우와 5박!",
          "original_form": "우와 5박!",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.6",
          "form": "아 다른곳이랑 같이 가서 ㅠ 경비가 기억이 안나요 ㅠ",
          "original_form": "아 다른곳이랑 같이 가서 ㅠ 경비가 기억이 안나요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.7",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045867.1.8",
          "form": "다른 여행지랑 같이 다녀오셨군용",
          "original_form": "다른 여행지랑 같이 다녀오셨군용",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.9",
          "form": "식비를 아끼면 근데",
          "original_form": "식비를 아끼면 근데",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.10",
          "form": "괜찮을거같아요 ㅎㅎ",
          "original_form": "괜찮을거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.11",
          "form": "오호 ㅎㅋㅋㅋ",
          "original_form": "오호 ㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.12",
          "form": "편의점 샌드위치가 만원인가 그랬어요 ㅋㅋㅋ",
          "original_form": "편의점 샌드위치가 만원인가 그랬어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.13",
          "form": "저는 코타키나발루 갔었거든요",
          "original_form": "저는 코타키나발루 갔었거든요",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.14",
          "form": "거기두 진짜 음식이 음... 이더라구요 ㅎ",
          "original_form": "거기두 진짜 음식이 음... 이더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.15",
          "form": "와..거기 진짜 진짜",
          "original_form": "와..거기 진짜 진짜",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.16",
          "form": "이쁠거같아요",
          "original_form": "이쁠거같아요",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.17",
          "form": "편의점 샌드위치가 반원..",
          "original_form": "편의점 샌드위치가 반원..",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.18",
          "form": "아 그런가요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그런가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045867.1.19",
          "form": "음식은 포기해야하는 여행지인가요 ..하하",
          "original_form": "음식은 포기해야하는 여행지인가요 ..하하",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045867.1.20",
          "form": "전 반딧불이가 너무 보고싶어서 가게됐는데",
          "original_form": "전 반딧불이가 너무 보고싶어서 가게됐는데",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045867.1.21",
          "form": "반딧불이 하나만으로도 정말 만족한 여행이었어요",
          "original_form": "반딧불이 하나만으로도 정말 만족한 여행이었어요",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045867.1.22",
          "form": "네네 해산물도 별로셨어요?",
          "original_form": "네네 해산물도 별로셨어요?",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045867.1.23",
          "form": "너무너무너무 예뻤거등요.. 하늘은 별이 수놓이고",
          "original_form": "너무너무너무 예뻤거등요.. 하늘은 별이 수놓이고",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045867.1.24",
          "form": "와 그정도로 이뻤어요? ㅠ ㅠ 그림같았겠다",
          "original_form": "와 그정도로 이뻤어요? ㅠ ㅠ 그림같았겠다",
          "speaker_id": "2",
          "time": "20210908 08:16"
