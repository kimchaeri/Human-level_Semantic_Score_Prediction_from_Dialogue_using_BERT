      "utterance": [
        {
          "id": "MDRW2100039012.1.1",
          "form": "아줌마가 이상해요",
          "original_form": "아줌마가 이상해요",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.2",
          "form": "내가 보기엔",
          "original_form": "내가 보기엔",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.3",
          "form": "둘이 잘된다면",
          "original_form": "둘이 잘된다면",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.4",
          "form": "보기얼",
          "original_form": "보기얼",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.5",
          "form": "엄빠소개부터해야하는데",
          "original_form": "엄빠소개부터해야하는데",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.6",
          "form": "잘되도 문제네",
          "original_form": "잘되도 문제네",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.7",
          "form": "결혼식장 잡는댔나",
          "original_form": "결혼식장 잡는댔나",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.8",
          "form": "그얘기한거보니",
          "original_form": "그얘기한거보니",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.9",
          "form": "뭐고",
          "original_form": "뭐고",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100039012.1.10",
          "form": "둘째가",
          "original_form": "둘째가",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.11",
          "form": "둘째가",
          "original_form": "둘째가",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.12",
          "form": "식까지 잡아놓은여자가 있는데",
          "original_form": "식까지 잡아놓은여자가 있는데",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.14",
          "form": "싫어서?",
          "original_form": "싫어서?",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.15",
          "form": "바람이든 무엇이든",
          "original_form": "바람이든 무엇이든",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.16",
          "form": "최악이다",
          "original_form": "최악이다",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.17",
          "form": "결격사유가 있어서",
          "original_form": "결격사유가 있어서",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.18",
          "form": "결혼이 취소가 되었고",
          "original_form": "결혼이 취소가 되었고",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.19",
          "form": "여자만 오면 되는 상황에",
          "original_form": "여자만 오면 되는 상황에",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.20",
          "form": "다들 결혼하기 진짜 싫겠다",
          "original_form": "다들 결혼하기 진짜 싫겠다",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.21",
          "form": "내칭구를 끌어들이려한건",
          "original_form": "내칭구를 끌어들이려한건",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039012.1.22",
          "form": "아..그래서 첫째.",
          "original_form": "아..그래서 첫째.",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.23",
          "form": "아닐까 하는",
          "original_form": "아닐까 하는",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.24",
          "form": "라도",
          "original_form": "라도",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.25",
          "form": "내추측",
          "original_form": "내추측",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.26",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.27",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.28",
          "form": "식장취소는안되고",
          "original_form": "식장취소는안되고",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.29",
          "form": "여자만있음되니까",
          "original_form": "여자만있음되니까",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.30",
          "form": "엄청난 얘기다..",
          "original_form": "엄청난 얘기다..",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.31",
          "form": "둘째가 안되니 첫째라도",
          "original_form": "둘째가 안되니 첫째라도",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.32",
          "form": "와...멘붕",
          "original_form": "와...멘붕",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.33",
          "form": "만나보라는 소리가 나온게 아닐까",
          "original_form": "만나보라는 소리가 나온게 아닐까",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100039012.1.34",
          "form": "더 결혼하기 싫어진다",
          "original_form": "더 결혼하기 싫어진다",
          "speaker_id": "2",
          "time": "20210903 01:20"
        }
      ]
    }
  ]
