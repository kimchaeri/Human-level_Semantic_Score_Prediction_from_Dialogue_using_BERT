      "utterance": [
        {
          "id": "MDRW2100010066.1.1",
          "form": "안녕하세요. 좋아하는 스포츠있으세요?",
          "original_form": "안녕하세요. 좋아하는 스포츠있으세요?",
          "speaker_id": "1",
          "time": "20210823 13:43"
        },
        {
          "id": "MDRW2100010066.1.2",
          "form": "최근에 배구 재밌게봤어요",
          "original_form": "최근에 배구 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010066.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010066.1.4",
          "form": "저도요. 농구 보는 것도 좋아합니다.",
          "original_form": "저도요. 농구 보는 것도 좋아합니다.",
          "speaker_id": "1",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010066.1.5",
          "form": "올림픽 경기도 챙겨보셨나요?? 배구??",
          "original_form": "올림픽 경기도 챙겨보셨나요?? 배구??",
          "speaker_id": "1",
          "time": "20210823 13:44"
        },
        {
          "id": "MDRW2100010066.1.6",
          "form": "네 예선부터 챙겨봤는데 너무 재밌었어요",
          "original_form": "네 예선부터 챙겨봤는데 너무 재밌었어요",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010066.1.7",
          "form": "배구직접해보고싶다는 생각도 들었구요",
          "original_form": "배구직접해보고싶다는 생각도 들었구요",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010066.1.8",
          "form": "맞아요. 한일전도 진짜 재밌었구요.",
          "original_form": "맞아요. 한일전도 진짜 재밌었구요.",
          "speaker_id": "1",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010066.1.9",
          "form": "직접 하는 스포츠는 뭐 있어요?",
          "original_form": "직접 하는 스포츠는 뭐 있어요?",
          "speaker_id": "1",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100010066.1.10",
          "form": "학생때는 친구들과 농구 잠깐했었는데",
          "original_form": "학생때는 친구들과 농구 잠깐했었는데",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010066.1.11",
          "form": "요즘은 하는게없어요",
          "original_form": "요즘은 하는게없어요",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010066.1.12",
          "form": "하시는 스포츠있나요?",
          "original_form": "하시는 스포츠있나요?",
          "speaker_id": "2",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010066.1.13",
          "form": "저도 딱히 없어요. 운동이라면 필라테스랑 헬스장 가는 정도요",
          "original_form": "저도 딱히 없어요. 운동이라면 필라테스랑 헬스장 가는 정도요",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010066.1.14",
          "form": "다른 사람들이랑 같이 하는 활동은 잘 못하고 있어요",
          "original_form": "다른 사람들이랑 같이 하는 활동은 잘 못하고 있어요",
          "speaker_id": "1",
          "time": "20210823 13:47"
        },
        {
          "id": "MDRW2100010066.1.15",
          "form": "좋아하는 스포츠 스타 있어요?",
          "original_form": "좋아하는 스포츠 스타 있어요?",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010066.1.16",
          "form": "김연경씨 좋아해요",
          "original_form": "김연경씨 좋아해요",
          "speaker_id": "2",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100010066.1.17",
          "form": "팀스포츠가 해보기가 어려운거같아요",
          "original_form": "팀스포츠가 해보기가 어려운거같아요",
          "speaker_id": "2",
          "time": "20210823 13:52"
        },
        {
          "id": "MDRW2100010066.1.18",
          "form": "아 코로나 때문에 더욱 그렇고 이젠 학생이 아니라서 친구들이랑 모여서 뭘 하기도 어렵더라구요",
          "original_form": "아 코로나 때문에 더욱 그렇고 이젠 학생이 아니라서 친구들이랑 모여서 뭘 하기도 어렵더라구요",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010066.1.19",
          "form": "안창림 선수도 아세요?",
          "original_form": "안창림 선수도 아세요?",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010066.1.20",
          "form": "알죠!",
          "original_form": "알죠!",
          "speaker_id": "2",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010066.1.21",
          "form": "배구는 아니고 유도라는 선수인데...",
          "original_form": "배구는 아니고 유도라는 선수인데...",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010066.1.22",
          "form": "유도하시는 귀화하신 분 아니신가요?",
          "original_form": "유도하시는 귀화하신 분 아니신가요?",
          "speaker_id": "2",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100010066.1.23",
          "form": "이번올림픽경기 봤어요",
          "original_form": "이번올림픽경기 봤어요",
          "speaker_id": "2",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010066.1.24",
          "form": "재일교포3세라고 합니다. 일본에 귀화제의 받았는데",
          "original_form": "재일교포3세라고 합니다. 일본에 귀화제의 받았는데",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010066.1.25",
          "form": "거절했다고 하더라구요",
          "original_form": "거절했다고 하더라구요",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010066.1.26",
          "form": "그 분도 좋아하고 김희진 선수도 좋더라구요",
          "original_form": "그 분도 좋아하고 김희진 선수도 좋더라구요",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010066.1.27",
          "form": "두분다 너무 좋죠",
          "original_form": "두분다 너무 좋죠",
          "speaker_id": "2",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010066.1.28",
          "form": "김연경 선수야 말할 것도 없죠",
          "original_form": "김연경 선수야 말할 것도 없죠",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010066.1.29",
          "form": "ㅎㅎ 이번에 브라질 선수가",
          "original_form": "ㅎㅎ 이번에 브라질 선수가",
          "speaker_id": "1",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010066.1.30",
          "form": "도핑했단 기사 보셨나요",
          "original_form": "도핑했단 기사 보셨나요",
          "speaker_id": "1",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010066.1.31",
          "form": "네! 보고 화나더라구요",
          "original_form": "네! 보고 화나더라구요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010066.1.32",
          "form": "도핑좀 안할수없나...그것때문에 국기도 못거는 나라도있는데...",
          "original_form": "도핑좀 안할수없나...그것때문에 국기도 못거는 나라도있는데...",
          "speaker_id": "2",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100010066.1.33",
          "form": "그러니까요. 실격처리했어야 한다고 생각해요",
          "original_form": "그러니까요. 실격처리했어야 한다고 생각해요",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010066.1.34",
          "form": "브라질한테 져서",
          "original_form": "브라질한테 져서",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100010066.1.35",
          "form": "경기 아예 못한 나라도 있고",
          "original_form": "경기 아예 못한 나라도 있고",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010066.1.36",
          "form": "스포츠 정신이 아니죠 정말",
          "original_form": "스포츠 정신이 아니죠 정말",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010066.1.37",
          "form": "우상혁 선수 경기도 보셨나요",
          "original_form": "우상혁 선수 경기도 보셨나요",
          "speaker_id": "1",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010066.1.38",
          "form": "우상혁씨 경기도 재밌게봤어요",
          "original_form": "우상혁씨 경기도 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210823 14:01"
        }
      ]
    }
