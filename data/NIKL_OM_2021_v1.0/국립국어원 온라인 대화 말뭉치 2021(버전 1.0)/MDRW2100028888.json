        },
        {
          "id": "MDRW2100028888.1.2",
          "form": "본업은요?",
          "original_form": "본업은요?",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028888.1.3",
          "form": "본업은 아직 안가졌어요",
          "original_form": "본업은 아직 안가졌어요",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028888.1.4",
          "form": "군문제 해결후에 가질려구요",
          "original_form": "군문제 해결후에 가질려구요",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100028888.1.5",
          "form": "스트레스는 없음요?",
          "original_form": "스트레스는 없음요?",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028888.1.6",
          "form": "딱히 없는거같아요!",
          "original_form": "딱히 없는거같아요!",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028888.1.7",
          "form": "부업안했더라면 잇을법한데",
          "original_form": "부업안했더라면 잇을법한데",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028888.1.8",
          "form": "부업으로 돈은 버니 그래도",
          "original_form": "부업으로 돈은 버니 그래도",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028888.1.9",
          "form": "큰 스트레스는 없는거같아요!",
          "original_form": "큰 스트레스는 없는거같아요!",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028888.1.10",
          "form": "전 이직 준비중",
          "original_form": "전 이직 준비중",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028888.1.11",
          "form": "이직 준비중이시구나",
          "original_form": "이직 준비중이시구나",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028888.1.12",
          "form": "좋게 되실거에요!",
          "original_form": "좋게 되실거에요!",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028888.1.13",
