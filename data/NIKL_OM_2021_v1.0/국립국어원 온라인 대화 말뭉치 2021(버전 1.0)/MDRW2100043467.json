      "utterance": [
        {
          "id": "MDRW2100043467.1.1",
          "form": "안녕하세요 저녁 드셨어요?",
          "original_form": "안녕하세요 저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043467.1.2",
          "form": "안녕하세요!!!!!",
          "original_form": "안녕하세요!!!!!",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043467.1.3",
          "form": "지금 닭꼬치 조금 남은거 먹고 만두 좀 구워먹을까해요~",
          "original_form": "지금 닭꼬치 조금 남은거 먹고 만두 좀 구워먹을까해요~",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043467.1.4",
          "form": "저녁드시나요?",
          "original_form": "저녁드시나요?",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043467.1.5",
          "form": "오 닭꼬치랑 만두ㅠㅜ 너무 맛있겠다ㅠㅠ",
          "original_form": "오 닭꼬치랑 만두ㅠㅜ 너무 맛있겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043467.1.6",
          "form": "저는 벌써 먹었어요! ㅎㅎ 김치찜 먹었어요~~",
          "original_form": "저는 벌써 먹었어요! ㅎㅎ 김치찜 먹었어요~~",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043467.1.7",
          "form": "헉 김치찜 들으니깐 밥땡기네요",
          "original_form": "헉 김치찜 들으니깐 밥땡기네요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043467.1.8",
          "form": "김치찜 너무 맛있잖아여",
          "original_form": "김치찜 너무 맛있잖아여",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043467.1.9",
          "form": "그쵸! 오늘 처음 시킨 곳인데 성공했어요",
          "original_form": "그쵸! 오늘 처음 시킨 곳인데 성공했어요",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043467.1.10",
          "form": "진짜 맛있더라구요",
          "original_form": "진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043467.1.11",
          "form": "이야~ 그럼 뿌듯한데",
          "original_form": "이야~ 그럼 뿌듯한데",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043467.1.12",
          "form": "저는 노선을 바꿔서 탕수육 굽고있어요 ㅎ",
          "original_form": "저는 노선을 바꿔서 탕수육 굽고있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043467.1.13",
          "form": "맞아요 한 입 먹고 성취감 들었어요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 한 입 먹고 성취감 들었어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043467.1.14",
          "form": "꺅 탕수육ㅠㅠ 너무 좋죠ㅠㅜㅠ",
          "original_form": "꺅 탕수육ㅠㅠ 너무 좋죠ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043467.1.15",
          "form": "처음 먹어보는건데 맛있네요 ㅎ",
          "original_form": "처음 먹어보는건데 맛있네요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043467.1.16",
          "form": "씨제이 고메 탕수육이에요 ㅅ",
          "original_form": "씨제이 고메 탕수육이에요 ㅅ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043467.1.17",
          "form": "김치를 부르는 맛이긴하지만 맛있어요",
          "original_form": "김치를 부르는 맛이긴하지만 맛있어요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043467.1.18",
          "form": "오오 밀키트인가요!",
          "original_form": "오오 밀키트인가요!",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043467.1.19",
          "form": "김치하시니까 김치피자탕수육같은 것도 있던데 궁금해요",
          "original_form": "김치하시니까 김치피자탕수육같은 것도 있던데 궁금해요",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043467.1.20",
          "form": "음 아뇨 ㅎ그냥 냉동식품이요",
          "original_form": "음 아뇨 ㅎ그냥 냉동식품이요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043467.1.21",
          "form": "김치피자탕수육이라 상상이안가네요",
          "original_form": "김치피자탕수육이라 상상이안가네요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043467.1.22",
          "form": "그쵸 저 시켜먹을 엄두가 안나요",
          "original_form": "그쵸 저 시켜먹을 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043467.1.23",
          "form": "ㅋㅋㅋㅋㅋ약간 모험같네요",
          "original_form": "ㅋㅋㅋㅋㅋ약간 모험같네요",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043467.1.24",
          "form": "탕수육 안에 김치피자..아닌가",
          "original_form": "탕수육 안에 김치피자..아닌가",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043467.1.25",
          "form": "암튼 상상이 안가네요",
          "original_form": "암튼 상상이 안가네요",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043467.1.26",
          "form": "그쵸 사진을 눈으로 봐도 머리에서 납득이 안가요.. ㅎㅎㅎ",
          "original_form": "그쵸 사진을 눈으로 봐도 머리에서 납득이 안가요.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043467.1.27",
          "form": "(김치냉동탕수육 얘기했어요)",
          "original_form": "(김치냉동탕수육 얘기했어요)",
