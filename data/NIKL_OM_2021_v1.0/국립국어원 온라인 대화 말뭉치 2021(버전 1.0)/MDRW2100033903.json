        {
          "id": "MDRW2100033903.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100033903.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100033903.1.3",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033903.1.4",
          "form": "코로나전에는 했었는데... 터지고 나서는 못했네요..ㅠㅠ",
          "original_form": "코로나전에는 했었는데... 터지고 나서는 못했네요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100033903.1.5",
          "form": "어떤거하셨나요?",
          "original_form": "어떤거하셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033903.1.6",
          "form": "필라테스요! 몸 교정할겸 했었는데 좋았었어요",
          "original_form": "필라테스요! 몸 교정할겸 했었는데 좋았었어요",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033903.1.7",
          "form": "와우 필테좋죠",
          "original_form": "와우 필테좋죠",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100033903.1.8",
          "form": "저도필테하고싶어요",
          "original_form": "저도필테하고싶어요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100033903.1.9",
          "form": "필테가 원래 비싼데...",
          "original_form": "필테가 원래 비싼데...",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033903.1.10",
          "form": "구민운동센터..? 이런곳에서는 싸더라구요",
          "original_form": "구민운동센터..? 이런곳에서는 싸더라구요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033903.1.11",
          "form": "괜찮았어요 되게!",
          "original_form": "괜찮았어요 되게!",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033903.1.12",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033903.1.13",
          "form": "거기서는 얼만가요ㅣ",
          "original_form": "거기서는 얼만가요ㅣ",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033903.1.14",
          "form": "일주일에 2번가는데",
          "original_form": "일주일에 2번가는데",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033903.1.15",
          "form": "한달에 4만 얼마..?",
          "original_form": "한달에 4만 얼마..?",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033903.1.16",
          "form": "수업은 1시간인데 꽉꽉 채워서 해주셨어요!",
          "original_form": "수업은 1시간인데 꽉꽉 채워서 해주셨어요!",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033903.1.17",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100033903.1.18",
          "form": "왤케 저렴한가요?",
          "original_form": "왤케 저렴한가요?",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100033903.1.19",
          "form": "구민센터에서 운영하니까 그런거 아닐까요..?",
          "original_form": "구민센터에서 운영하니까 그런거 아닐까요..?",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100033903.1.20",
          "form": "저렴하다고 뭐 대충하시고 그런건 전혀없었어요",
          "original_form": "저렴하다고 뭐 대충하시고 그런건 전혀없었어요",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100033903.1.21",
          "form": "뭐 사설업체처럼 기계쓰고 그런건 없긴한데 그래도 저는 교정되는게 느껴져서 만족했었어요",
          "original_form": "뭐 사설업체처럼 기계쓰고 그런건 없긴한데 그래도 저는 교정되는게 느껴져서 만족했었어요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100033903.1.22",
          "form": "와우 좋네요",
          "original_form": "와우 좋네요",
          "speaker_id": "2",
