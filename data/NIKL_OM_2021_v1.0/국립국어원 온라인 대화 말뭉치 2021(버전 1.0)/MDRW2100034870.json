      "utterance": [
        {
          "id": "MDRW2100034870.1.1",
          "form": "name2님 가고싶은 여행지 있나요?",
          "original_form": "&name2&님 가고싶은 여행지 있나요?",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034870.1.2",
          "form": "전 제주도요",
          "original_form": "전 제주도요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034870.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034870.1.4",
          "form": "저는.... 경주요. 경주가고싶네요,,,,,",
          "original_form": "저는.... 경주요. 경주가고싶네요,,,,,",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034870.1.5",
          "form": "ㅋㅋㅋ가본 적 있으세요?",
          "original_form": "ㅋㅋㅋ가본 적 있으세요?",
          "speaker_id": "2",
          "time": "20210829 08:38"
        },
        {
          "id": "MDRW2100034870.1.6",
          "form": "아뇨ㅜ 갔던 곳은 부산이 제일 좋았던 것 같아요",
          "original_form": "아뇨ㅜ 갔던 곳은 부산이 제일 좋았던 것 같아요",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100034870.1.7",
          "form": "신기하게 국외보다 국내여행이 더 좋게 기억에 남더라고요",
          "original_form": "신기하게 국외보다 국내여행이 더 좋게 기억에 남더라고요",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100034870.1.8",
          "form": "오 그런가요? 전 국외로 안 나가봤거든요",
          "original_form": "오 그런가요? 전 국외로 안 나가봤거든요",
          "speaker_id": "2",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100034870.1.9",
          "form": "저도 부산 갔을 때 재밌었어요",
          "original_form": "저도 부산 갔을 때 재밌었어요",
          "speaker_id": "2",
          "time": "20210829 09:19"
        },
        {
          "id": "MDRW2100034870.1.10",
          "form": "그쵸! 부산 너무 즐겁더라고요!!!!!!!!",
          "original_form": "그쵸! 부산 너무 즐겁더라고요!!!!!!!!",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100034870.1.11",
          "form": "전 여행내내 비가왔는데도 재밌었던 기억이있네요 ㅎ.ㅎ",
          "original_form": "전 여행내내 비가왔는데도 재밌었던 기억이있네요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100034870.1.12",
          "form": "부산에서 어디어디 가셨어요?",
          "original_form": "부산에서 어디어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100034870.1.13",
          "form": "저 일단... 자갈치 시장이랑 남포동은 다 가봤던 것 같아요!",
          "original_form": "저 일단... 자갈치 시장이랑 남포동은 다 가봤던 것 같아요!",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100034870.1.14",
          "form": "바다는 못보고 왔지만요ㅜㅜㅋㅋ",
          "original_form": "바다는 못보고 왔지만요ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100034870.1.15",
          "form": "아 바다하니까 밤바다 보고 싶어요",
          "original_form": "아 바다하니까 밤바다 보고 싶어요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100034870.1.16",
          "form": "요즘 날씨가 딱 바다 가기 좋은 날씨 같아요",
          "original_form": "요즘 날씨가 딱 바다 가기 좋은 날씨 같아요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100034870.1.17",
          "form": "맞아요 ㅠㅠㅠ 요즘 밤바다 가기에 딱 좋잖아요ㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ 요즘 밤바다 가기에 딱 좋잖아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100034870.1.18",
          "form": "진짜 코로나만 아니였으면 바로 갔을 거 같아요ㅜㅜ",
          "original_form": "진짜 코로나만 아니였으면 바로 갔을 거 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100034870.1.19",
          "form": "그쵸 저도 시간만 있으면...ㅠ",
          "original_form": "그쵸 저도 시간만 있으면...ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100034870.1.20",
