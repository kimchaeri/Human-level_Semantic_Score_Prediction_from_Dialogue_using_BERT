      "utterance": [
        {
          "id": "MDRW2100025915.1.1",
          "form": "에그마요  클럽?",
          "original_form": "에그마요  클럽?",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025915.1.2",
          "form": "이야기해보세욥",
          "original_form": "이야기해보세욥",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025915.1.3",
          "form": "누나야 1개 어머니 1개",
          "original_form": "누나야 1개 어머니 1개",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025915.1.4",
          "form": "비트윈하세요?",
          "original_form": "비트윈하세요?",
          "speaker_id": "2",
          "time": "20210901 17:58"
        },
        {
          "id": "MDRW2100025915.1.5",
          "form": "ㅋㅋ효자시네요",
          "original_form": "ㅋㅋ효자시네요",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025915.1.6",
          "form": "에그마요 클럽 시켰어요",
          "original_form": "에그마요 클럽 시켰어요",
          "speaker_id": "2",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025915.1.7",
          "form": "오늘 서브웨이 플렉스인가요",
          "original_form": "오늘 서브웨이 플렉스인가요",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025915.1.8",
          "form": "여자친구도 좋아하는데",
          "original_form": "여자친구도 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025915.1.9",
          "form": "시켜라고할까요",
          "original_form": "시켜라고할까요",
          "speaker_id": "2",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025915.1.10",
          "form": "폭주 안하게 1개만 먹어야 해요",
          "original_form": "폭주 안하게 1개만 먹어야 해요",
          "speaker_id": "2",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025915.1.11",
          "form": "음식 올때까지 설거지하고 해야겠네요",
          "original_form": "음식 올때까지 설거지하고 해야겠네요",
          "speaker_id": "2",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025915.1.12",
          "form": "오 저는 내일 저녁메뉴로 픽 해야겟어요",
          "original_form": "오 저는 내일 저녁메뉴로 픽 해야겟어요",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025915.1.13",
          "form": "다드셧나유",
          "original_form": "다드셧나유",
          "speaker_id": "1",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025915.1.14",
          "form": "맛있네요",
          "original_form": "맛있네요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100025915.1.15",
          "form": "주무세요",
          "original_form": "주무세요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
