      "utterance": [
        {
          "id": "MDRW2100029884.1.1",
          "form": "이제 건강에대해 토킹합시다 ㅋㅋ",
          "original_form": "이제 건강에대해 토킹합시다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029884.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029884.1.3",
          "form": "하시는 운동 있으세요?",
          "original_form": "하시는 운동 있으세요?",
          "speaker_id": "2",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029884.1.4",
          "form": "검도합니다!",
          "original_form": "검도합니다!",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029884.1.5",
          "form": "오 팔목 힘 좋으시겠네요",
          "original_form": "오 팔목 힘 좋으시겠네요",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029884.1.6",
          "form": "은 못이기죠",
          "original_form": "{censored}은 못이기죠",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029884.1.7",
          "form": "운동하십니까",
          "original_form": "운동하십니까",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029884.1.8",
          "form": "아뇨ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029884.1.9",
          "form": "말로만 해야지 해야지 하다가",
          "original_form": "말로만 해야지 해야지 하다가",
          "speaker_id": "2",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029884.1.10",
          "form": "2년 지났네요?",
          "original_form": "2년 지났네요?",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029884.1.11",
          "form": "건강생각해서 해야합니다",
          "original_form": "건강생각해서 해야합니다",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029884.1.12",
          "form": "운동도 하다보면 재밌습니다",
          "original_form": "운동도 하다보면 재밌습니다",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029884.1.13",
          "form": "오래 하셨어요?",
          "original_form": "오래 하셨어요?",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029884.1.14",
          "form": "제가 스포츠는 좋아하는데",
          "original_form": "제가 스포츠는 좋아하는데",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100029884.1.15",
          "form": "검도 20년은 한거같네요",
          "original_form": "검도 20년은 한거같네요",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029884.1.16",
          "form": "오우 대단하시네요",
          "original_form": "오우 대단하시네요",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029884.1.17",
          "form": "일단 죄송합니다 죄송하구요",
          "original_form": "일단 죄송합니다 죄송하구요",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100029884.1.18",
          "form": "전 어릴 때 수영을 좀 했었네요ㅎ",
          "original_form": "전 어릴 때 수영을 좀 했었네요ㅎ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029884.1.19",
          "form": "전국체전 나가서 털리고 그만뒀지만ㅋㅋ",
          "original_form": "전국체전 나가서 털리고 그만뒀지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029884.1.20",
          "form": "아니에요 뭐가 죄송해요ㅋㅋㅋㅋ",
          "original_form": "아니에요 뭐가 죄송해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100029884.1.21",
          "form": "일단 건강을 위해서라도 운동은 하셔요",
          "original_form": "일단 건강을 위해서라도 운동은 하셔요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100029884.1.22",
          "form": "대단하시네요 20년을",
          "original_form": "대단하시네요 20년을",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100029884.1.23",
          "form": "재미로 하다보니 그리되더군요",
          "original_form": "재미로 하다보니 그리되더군요",
