      "utterance": [
        {
          "id": "MDRW2100027197.1.1",
          "form": "커피 드시나요",
          "original_form": "커피 드시나요",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027197.1.2",
          "form": "전 드립커피 마시는중입니다",
          "original_form": "전 드립커피 마시는중입니다",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.3",
          "form": "저는 아메리카노 마시고 있어요",
          "original_form": "저는 아메리카노 마시고 있어요",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.4",
          "form": "전 기본 커피에",
          "original_form": "전 기본 커피에",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.5",
          "form": "고디바커피 같이내려서",
          "original_form": "고디바커피 같이내려서",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.6",
          "form": "섞어서마셔요",
          "original_form": "섞어서마셔요",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.7",
          "form": "3900원인데 샐러드나 케익. 빵 줘요",
          "original_form": "3900원인데 샐러드나 케익. 빵 줘요",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.8",
          "form": "아무것도 안고르면 1900원이에요",
          "original_form": "아무것도 안고르면 1900원이에요",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.9",
          "form": "카페에 왔엉ᆢ",
          "original_form": "카페에 왔엉ᆢ",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.10",
          "form": "왔어요",
          "original_form": "왔어요",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027197.1.11",
          "form": "고디바는 초콜릿아닌가요",
          "original_form": "고디바는 초콜릿아닌가요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.12",
          "form": "완전 괜찮네요",
          "original_form": "완전 괜찮네요",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.13",
          "form": "집근처 맛집인가요",
          "original_form": "집근처 맛집인가요",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.14",
          "form": "제주도는 커피숍 다비싸더라구요",
          "original_form": "제주도는 커피숍 다비싸더라구요",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.15",
          "form": "체인점인데 제주가 본점이에요",
          "original_form": "체인점인데 제주가 본점이에요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.16",
          "form": "초콜릿 향나는 고급 커피입니다",
          "original_form": "초콜릿 향나는 고급 커피입니다",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.17",
          "form": "제주도에서 제일큰 체인점일거에요",
          "original_form": "제주도에서 제일큰 체인점일거에요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.18",
          "form": "고디바는 고급이지요",
          "original_form": "고디바는 고급이지요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.19",
          "form": "초콜릿 위주로 파는회사는맞아요",
          "original_form": "초콜릿 위주로 파는회사는맞아요",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.20",
          "form": "입이 고급이시군요",
          "original_form": "입이 고급이시군요",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.21",
          "form": "커피에섞어서 타먹음 존맛",
          "original_form": "커피에섞어서 타먹음 존맛",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027197.1.22",
          "form": "식사는 안고급이던데",
          "original_form": "식사는 안고급이던데",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027197.1.23",
          "form": "고급져요",
          "original_form": "고급져요",
          "speaker_id": "2",
          "time": "20210902 10:26"
        }
      ]
