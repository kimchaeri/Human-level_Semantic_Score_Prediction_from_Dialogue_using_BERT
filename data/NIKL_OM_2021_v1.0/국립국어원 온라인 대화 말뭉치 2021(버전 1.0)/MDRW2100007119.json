      "utterance": [
        {
          "id": "MDRW2100007119.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007119.1.2",
          "form": "영화 좋아하는데 자주 보러가진 못하는거 같아요",
          "original_form": "영화 좋아하는데 자주 보러가진 못하는거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007119.1.3",
          "form": "ㅋㅋㅋㅋ영화는 제가 지식이 부족하답니다",
          "original_form": "ㅋㅋㅋㅋ영화는 제가 지식이 부족하답니다",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007119.1.4",
          "form": "먹는거만큼 알지못해요ㅋㅋㅋ",
          "original_form": "먹는거만큼 알지못해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007119.1.5",
          "form": "재밌게 보신 영화 있으세요?",
          "original_form": "재밌게 보신 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007119.1.6",
          "form": "얼마전에 블랙위도우 봤는데 재밌었어요",
          "original_form": "얼마전에 블랙위도우 봤는데 재밌었어요",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007119.1.7",
          "form": "참고로 마블 잘 몰라요..",
          "original_form": "참고로 마블 잘 몰라요..",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007119.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007119.1.9",
          "form": "저도 마블 어벤져스만 봤어요 ㅋㅋㅋ",
          "original_form": "저도 마블 어벤져스만 봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007119.1.10",
          "form": "블랙 위도우 보지도 않았어요..",
          "original_form": "블랙 위도우 보지도 않았어요..",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007119.1.11",
          "form": "재밌게 보신 영화 뭐 있으세요?",
          "original_form": "재밌게 보신 영화 뭐 있으세요?",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007119.1.12",
          "form": "저는 공포 스릴러 이런걸 좋아해서",
          "original_form": "저는 공포 스릴러 이런걸 좋아해서",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007119.1.13",
          "form": "최근엔 극장에서 랑종 보고왔어요!",
          "original_form": "최근엔 극장에서 랑종 보고왔어요!",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007119.1.14",
          "form": "모가디슈랑",
          "original_form": "모가디슈랑",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007119.1.15",
          "form": "오 저는 공포물은 아예 못봐서 ㅠㅠ",
          "original_form": "오 저는 공포물은 아예 못봐서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007119.1.16",
          "form": "모가디슈 넘 보고싶어요 ㅋㅋㅋ",
          "original_form": "모가디슈 넘 보고싶어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007119.1.17",
          "form": "재밌다던데",
          "original_form": "재밌다던데",
          "speaker_id": "2",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007119.1.18",
          "form": "모가디슈 저는 그냥 so so",
          "original_form": "모가디슈 저는 그냥 so so",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007119.1.19",
          "form": "지루하진 않았는데 딱히 와 짱재밌다!! 도 아니었는데",
          "original_form": "지루하진 않았는데 딱히 와 짱재밌다!! 도 아니었는데",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007119.1.20",
          "form": "요즘 호평 많이 받더라구요",
          "original_form": "요즘 호평 많이 받더라구요",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007119.1.21",
          "form": "아 그래요...?",
          "original_form": "아 그래요...?",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007119.1.22",
          "form": "구교환이 매력적이었어요",
          "original_form": "구교환이 매력적이었어요",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007119.1.23",
          "form": "저는 그냥 약간 강철비3 같은 느낌이었는데 ㅋㅋㅋ",
          "original_form": "저는 그냥 약간 강철비3 같은 느낌이었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007119.1.24",
          "form": "많이들 좋아하시더라구요",
          "original_form": "많이들 좋아하시더라구요",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007119.1.25",
          "form": "아프간 일도 있어서 더 현실로 와닿는 뭔가가 있는가 같아요",
          "original_form": "아프간 일도 있어서 더 현실로 와닿는 뭔가가 있는가 같아요",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007119.1.26",
          "form": "강철비도 안봐서 몰라요 ㅠㅠ흑흑",
          "original_form": "강철비도 안봐서 몰라요 ㅠㅠ흑흑",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007119.1.27",
          "form": "신세계는 봤어요 ㅋㅋㅋ",
          "original_form": "신세계는 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007119.1.28",
          "form": "신세계 저도 완전 좋아해요!!",
          "original_form": "신세계 저도 완전 좋아해요!!",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007119.1.29",
          "form": "근데 그렇게 잔인할 줄 모르고 엄마랑 극장가서 봐서..",
          "original_form": "근데 그렇게 잔인할 줄 모르고 엄마랑 극장가서 봐서..",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007119.1.30",
          "form": "좀 난감했어요",
          "original_form": "좀 난감했어요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007119.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 지금은 난감한 장면은 생각안나고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 지금은 난감한 장면은 생각안나고",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007119.1.32",
          "form": "헤이 브라더만 기억나는거 같아요",
          "original_form": "헤이 브라더만 기억나는거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007119.1.33",
          "form": "ㅋㅋㅋㅋㅋ 저는 너무 재밌게봐서 한 4~5번 본 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 너무 재밌게봐서 한 4~5번 본 것 같아요",
