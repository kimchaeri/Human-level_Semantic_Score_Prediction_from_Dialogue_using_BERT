      "utterance": [
        {
          "id": "MDRW2100010639.1.1",
          "form": "견과류가 있길래 흡입하고있어요",
          "original_form": "견과류가 있길래 흡입하고있어요",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010639.1.2",
          "form": "하루견과 ㅋㄲ",
          "original_form": "하루견과 ㅋㄲ",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010639.1.3",
          "form": "하루견과 드시면서 뭐 보시나요?",
          "original_form": "하루견과 드시면서 뭐 보시나요?",
          "speaker_id": "2",
          "time": "20210823 20:31"
        },
        {
          "id": "MDRW2100010639.1.4",
          "form": "요즘 영드에 빠져있어요 ㅎㅎ",
          "original_form": "요즘 영드에 빠져있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010639.1.5",
          "form": "뭐 보시나요?",
          "original_form": "뭐 보시나요?",
          "speaker_id": "2",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010639.1.6",
          "form": "웨이브에서 영드 브라운신부 강추합니다 ㅋㅋ",
          "original_form": "웨이브에서 영드 브라운신부 강추합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:20"
        },
        {
          "id": "MDRW2100010639.1.7",
          "form": "웨이브가 넥플릭스 같은건가요?",
          "original_form": "웨이브가 넥플릭스 같은건가요?",
          "speaker_id": "2",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100010639.1.8",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010639.1.9",
          "form": "핸드폰 요금제 비싼거 썼더니 공짜로ㅋ보게해주더라고요",
          "original_form": "핸드폰 요금제 비싼거 썼더니 공짜로ㅋ보게해주더라고요",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010639.1.10",
          "form": "아!!! 웨이브가 더 재미있는게 많나요? 넥플릭스는 이제 고갈된 느낌이예요",
          "original_form": "아!!! 웨이브가 더 재미있는게 많나요? 넥플릭스는 이제 고갈된 느낌이예요",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010639.1.11",
          "form": "웨이브는 한국 프로가 많아요",
          "original_form": "웨이브는 한국 프로가 많아요",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010639.1.12",
          "form": "티빙과 겹치는것도 제법있어요 미드영드애니 일드쪽은",
          "original_form": "티빙과 겹치는것도 제법있어요 미드영드애니 일드쪽은",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010639.1.13",
          "form": "웨이브에 예능도 많나요? 전 예능 좋아하는데 넥플에는 별로 없어요",
          "original_form": "웨이브에 예능도 많나요? 전 예능 좋아하는데 넥플에는 별로 없어요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010639.1.14",
          "form": "넷플릭스보다는 많은것같아요",
          "original_form": "넷플릭스보다는 많은것같아요",
          "speaker_id": "1",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100010639.1.15",
          "form": "좀더 국내기반 ㅋㄱ",
          "original_form": "좀더 국내기반 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100010639.1.16",
          "form": "단지 티빙과 웨이브는 안겹쳐요",
          "original_form": "단지 티빙과 웨이브는 안겹쳐요",
          "speaker_id": "1",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100010639.1.17",
          "form": "서로 다른방송사걸 서비스로 한달까",
          "original_form": "서로 다른방송사걸 서비스로 한달까",
          "speaker_id": "1",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100010639.1.18",
          "form": "넥플보단 티빙이 낫다고들 하던데 갈아탈까봐요 티빙이든 웨이브든",
          "original_form": "넥플보단 티빙이 낫다고들 하던데 갈아탈까봐요 티빙이든 웨이브든",
          "speaker_id": "2",
          "time": "20210823 21:35"
        },
        {
          "id": "MDRW2100010639.1.19",
          "form": "한국 프로그램 보고싶음 티빙이 나은거같아요",
          "original_form": "한국 프로그램 보고싶음 티빙이 나은거같아요",
          "speaker_id": "1",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100010639.1.20",
          "form": "일단 티비엔 독점서비스하니까요",
          "original_form": "일단 티비엔 독점서비스하니까요",
          "speaker_id": "1",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010639.1.21",
          "form": "티비엔에 재미있는게 많은데요 티빙으로 갈아타야겠군요",
          "original_form": "티비엔에 재미있는게 많은데요 티빙으로 갈아타야겠군요",
          "speaker_id": "2",
          "time": "20210823 22:24"
        },
        {
          "id": "MDRW2100010639.1.22",
          "form": "요샌 아예 그런 앱에서만 서비스하는 예능프로그램도 있더라고요",
          "original_form": "요샌 아예 그런 앱에서만 서비스하는 예능프로그램도 있더라고요",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010639.1.23",
          "form": "전에 신서유기 멤버들 캠핑 프로그램 같은거 본거 같아요 ㅋㄱ",
          "original_form": "전에 신서유기 멤버들 캠핑 프로그램 같은거 본거 같아요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010639.1.24",
          "form": "그 여고추리반이 티빙에서만 했던건데 최근에 티비엔에서 했잖아요 엄청 재미있었어요",
          "original_form": "그 여고추리반이 티빙에서만 했던건데 최근에 티비엔에서 했잖아요 엄청 재미있었어요",
          "speaker_id": "2",
          "time": "20210823 22:27"
        }
      ]
    }
