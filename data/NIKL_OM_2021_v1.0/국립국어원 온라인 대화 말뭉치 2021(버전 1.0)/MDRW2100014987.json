      "utterance": [
        {
          "id": "MDRW2100014987.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014987.1.2",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014987.1.3",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014987.1.4",
          "form": "하루 한잔씩 마셔요",
          "original_form": "하루 한잔씩 마셔요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014987.1.5",
          "form": "저랑 같네요 ㅎㅎㅎ",
          "original_form": "저랑 같네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014987.1.6",
          "form": "네 아메요 ㅎㅎ",
          "original_form": "네 아메요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014987.1.7",
          "form": "카페 어디 제일 자주 가시나요",
          "original_form": "카페 어디 제일 자주 가시나요",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014987.1.8",
          "form": "스벅이나 이디야요",
          "original_form": "스벅이나 이디야요",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014987.1.9",
          "form": "컴포트는 와플이랑 세트 4000원이라 가고요",
          "original_form": "컴포트는 와플이랑 세트 4000원이라 가고요",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014987.1.10",
          "form": "어디 가세요?",
          "original_form": "어디 가세요?",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014987.1.11",
          "form": "오 저도 스벅이랑 이디야 메가커피 이렇게요",
          "original_form": "오 저도 스벅이랑 이디야 메가커피 이렇게요",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014987.1.12",
          "form": "컴포트는 주위에 없네요 ㅠ",
          "original_form": "컴포트는 주위에 없네요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014987.1.13",
          "form": "저도 요새 생겼어요 ㅎㅎ",
          "original_form": "저도 요새 생겼어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014987.1.14",
          "form": "집앞에 ㅎㅎ",
          "original_form": "집앞에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014987.1.15",
          "form": "와플이랑 세트 완전 가성비 좋네요..",
          "original_form": "와플이랑 세트 완전 가성비 좋네요..",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014987.1.16",
          "form": "그렇죠 ㅋㅋ",
          "original_form": "그렇죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014987.1.17",
          "form": "소문나서 사람이 많아 졌어요 ㅡㅡ",
          "original_form": "소문나서 사람이 많아 졌어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014987.1.18",
          "form": "전커피 집에서 아메는 내려먹고",
          "original_form": "전커피 집에서 아메는 내려먹고",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014987.1.19",
          "form": "아 정말요 ㅋㅋㅋ 소문날만 하네요.. 저라도 가겠어요 ㅎㅎ",
          "original_form": "아 정말요 ㅋㅋㅋ 소문날만 하네요.. 저라도 가겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014987.1.20",
          "form": "커피숍은 디저트 많이 먹어요 ㅋㅋ",
          "original_form": "커피숍은 디저트 많이 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014987.1.21",
          "form": "그러게요 ㅎㅎ",
          "original_form": "그러게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014987.1.22",
          "form": "와플이 다른데는4000원이라 비싸요",
          "original_form": "와플이 다른데는4000원이라 비싸요",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014987.1.23",
          "form": "디저트도 같이 드세요?",
          "original_form": "디저트도 같이 드세요?",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014987.1.24",
          "form": "저는 디저트는 투썸 같은 데 가서 먹어요 케익이랑 ㅋㅋ",
          "original_form": "저는 디저트는 투썸 같은 데 가서 먹어요 케익이랑 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014987.1.25",
          "form": "오 투썸 완전 맛나요",
          "original_form": "오 투썸 완전 맛나요",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014987.1.26",
          "form": "딸기케이크 ㅋㅋ",
          "original_form": "딸기케이크 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014987.1.27",
          "form": "봄만 기다려요",
          "original_form": "봄만 기다려요",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014987.1.28",
          "form": "아 그쵸 여름엔 체리로 바뀌더라고요 아쉽 ㅠㅠ",
          "original_form": "아 그쵸 여름엔 체리로 바뀌더라고요 아쉽 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014987.1.29",
          "form": "아 건쫌",
          "original_form": "아 건쫌",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014987.1.30",
          "form": "딸기가 최고입니다 ㅎㅎ",
          "original_form": "딸기가 최고입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014987.1.31",
          "form": "그르니까요.. 맛이 너무 달라요 ㅠ",
          "original_form": "그르니까요.. 맛이 너무 달라요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014987.1.32",
          "form": "저번에 친구가3만원 어치 사줬어요 ㅋㅋ",
          "original_form": "저번에 친구가3만원 어치 사줬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014987.1.33",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014987.1.34",
          "form": "근데 비싸서 ㅡㅡ",
          "original_form": "근데 비싸서 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014987.1.35",
          "form": "몇개 안되더군요",
          "original_form": "몇개 안되더군요",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014987.1.36",
