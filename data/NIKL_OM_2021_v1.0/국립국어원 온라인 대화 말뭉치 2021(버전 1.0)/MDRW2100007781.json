      "utterance": [
        {
          "id": "MDRW2100007781.1.1",
          "form": "저는 회좋아합니다",
          "original_form": "저는 회좋아합니다",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.2",
          "form": "광어회먹고싶네요",
          "original_form": "광어회먹고싶네요",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.3",
          "form": "회 맛있는데",
          "original_form": "회 맛있는데",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.4",
          "form": "광어회 진짜 맛있어요",
          "original_form": "광어회 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.5",
          "form": "아니면 회덮밥ㅋㅋ",
          "original_form": "아니면 회덮밥ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.6",
          "form": "회 먹을 때 소스 어떤거 찍어드세요",
          "original_form": "회 먹을 때 소스 어떤거 찍어드세요",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.7",
          "form": "저는 초장 칙어먹어요",
          "original_form": "저는 초장 칙어먹어요",
          "speaker_id": "1",
          "time": "20210822 10:40"
        },
        {
          "id": "MDRW2100007781.1.8",
          "form": "저는 간장이요!",
          "original_form": "저는 간장이요!",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.9",
          "form": "와사비랑해서요?",
          "original_form": "와사비랑해서요?",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.10",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.11",
          "form": "약간 찡한 맛을 즐기시는구나",
          "original_form": "약간 찡한 맛을 즐기시는구나",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.12",
          "form": "초장은 해산물먹을때먹어요",
          "original_form": "초장은 해산물먹을때먹어요",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.13",
          "form": "네 와사비좋아요",
          "original_form": "네 와사비좋아요",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.14",
          "form": "전복 해삼 이런거 먹을 때요?",
          "original_form": "전복 해삼 이런거 먹을 때요?",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.15",
          "form": "넵맞아요",
          "original_form": "넵맞아요",
          "speaker_id": "2",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007781.1.16",
          "form": "다음주에회한번먹어야겟어요",
          "original_form": "다음주에회한번먹어야겟어요",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007781.1.17",
          "form": "아 말하니까 저도 먹고싶어졌어요ㅎ",
          "original_form": "아 말하니까 저도 먹고싶어졌어요ㅎ",
          "speaker_id": "1",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007781.1.18",
          "form": "저도 먹으러가야겠어요",
          "original_form": "저도 먹으러가야겠어요",
          "speaker_id": "1",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007781.1.19",
          "form": "회좋죠",
          "original_form": "회좋죠",
          "speaker_id": "2",
          "time": "20210822 10:42"
        }
      ]
