      "utterance": [
        {
          "id": "MDRW2100044936.1.1",
          "form": "디저트 좋아하세요?",
          "original_form": "디저트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044936.1.2",
          "form": "네 완전 좋아하죠!!! 디저트 뭐 제일 좋아하세요?",
          "original_form": "네 완전 좋아하죠!!! 디저트 뭐 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044936.1.3",
          "form": "케이크 좋아해요!!!",
          "original_form": "케이크 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044936.1.4",
          "form": "뭐 좋아하시나요",
          "original_form": "뭐 좋아하시나요",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044936.1.5",
          "form": "저는 마카롱이요!ㅋㅋㅋ",
          "original_form": "저는 마카롱이요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044936.1.6",
          "form": "아 마카롱 좋죠ㅠㅠㅠㅠ",
          "original_form": "아 마카롱 좋죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044936.1.7",
          "form": "무슨맛 좋아하세요",
          "original_form": "무슨맛 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044936.1.8",
          "form": "가나슈 들어간 초코를 제일 좋아하고 그 담은 역시 크림브륄레 맛...!",
          "original_form": "가나슈 들어간 초코를 제일 좋아하고 그 담은 역시 크림브륄레 맛...!",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044936.1.9",
          "form": "생각만해도 맛있겠어요",
          "original_form": "생각만해도 맛있겠어요",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044936.1.10",
          "form": "저는 그....",
          "original_form": "저는 그....",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044936.1.11",
          "form": "name1 님도 마카롱 좋아하시나봐요!",
          "original_form": "&name1& 님도 마카롱 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044936.1.12",
          "form": "보통 주황색 마카롱이 무슨 맛이죠??!!",
          "original_form": "보통 주황색 마카롱이 무슨 맛이죠??!!",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044936.1.13",
          "form": "이름이 생각 안나요ㅠㅠ",
          "original_form": "이름이 생각 안나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044936.1.14",
          "form": "아 황치즈 마카롱??",
          "original_form": "아 황치즈 마카롱??",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044936.1.15",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044936.1.16",
          "form": "황치즈!!!!!",
          "original_form": "황치즈!!!!!",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044936.1.17",
          "form": "맛있던데!!!",
          "original_form": "맛있던데!!!",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044936.1.18",
          "form": "맞아요ㅋㅋㅋ 황치즈도 맛있죠...ㅎ",
          "original_form": "맞아요ㅋㅋㅋ 황치즈도 맛있죠...ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044936.1.19",
          "form": "최근에 집 근처에 자주가는 마카롱집이 문을 닫아서 요즘은 못 먹고 있어요",
          "original_form": "최근에 집 근처에 자주가는 마카롱집이 문을 닫아서 요즘은 못 먹고 있어요",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044936.1.20",
          "form": "헉 아쉽네요",
          "original_form": "헉 아쉽네요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
