      "utterance": [
        {
          "id": "MDRW2100003315.1.1",
          "form": "설것이도 끝냈고",
          "original_form": "설것이도 끝냈고",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003315.1.2",
          "form": "이제 드라마볼라고",
          "original_form": "이제 드라마볼라고",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003315.1.3",
          "form": "오우 나도 다끝냈는데 이제 애들 재워야지",
          "original_form": "오우 나도 다끝냈는데 이제 애들 재워야지",
          "speaker_id": "2",
          "time": "20210820 21:52"
        },
        {
          "id": "MDRW2100003315.1.4",
          "form": "오오 일찍 자는구나 바른생활어린이들",
          "original_form": "오오 일찍 자는구나 바른생활어린이들",
          "speaker_id": "1",
          "time": "20210820 21:52"
        },
        {
          "id": "MDRW2100003315.1.5",
          "form": "일찍자야 내가 쉬지ㅋㅋ",
          "original_form": "일찍자야 내가 쉬지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003315.1.6",
          "form": "좋구러~~ ㅎㅎ 나도쉬고싶다 우리애들은 과자묵는다",
          "original_form": "좋구러~~ ㅎㅎ 나도쉬고싶다 우리애들은 과자묵는다",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003315.1.7",
          "form": "이시간에? 안자나?",
          "original_form": "이시간에? 안자나?",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003315.1.8",
          "form": "안자지ㅋㅋ우리 애들은 열한시쯤?",
          "original_form": "안자지ㅋㅋ우리 애들은 열한시쯤?",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003315.1.9",
          "form": "매일 그때 애들이 잔다고?",
          "original_form": "매일 그때 애들이 잔다고?",
          "speaker_id": "2",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003315.1.10",
          "form": "응ㅋ기오늘은 펜트하우스 끝나고 잔다",
          "original_form": "응ㅋ기오늘은 펜트하우스 끝나고 잔다",
          "speaker_id": "1",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003315.1.11",
          "form": "지금 시작했어? 애들도 같이 보나",
          "original_form": "지금 시작했어? 애들도 같이 보나",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003315.1.12",
          "form": "어제 펜트하우스보느라ㅎㅎ",
          "original_form": "어제 펜트하우스보느라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 09:43"
        },
        {
          "id": "MDRW2100003315.1.13",
          "form": "아ㅋ나도 보고 잤는데 애들은 여전히 일찍일어나네",
          "original_form": "아ㅋ나도 보고 잤는데 애들은 여전히 일찍일어나네",
