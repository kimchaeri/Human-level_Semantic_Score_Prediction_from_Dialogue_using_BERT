      "utterance": [
        {
          "id": "MDRW2100013735.1.1",
          "form": "처서도 지났는데 아직 덥네요",
          "original_form": "처서도 지났는데 아직 덥네요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013735.1.2",
          "form": "그러게요~ 비와서 선선해지나 했는데 다시..",
          "original_form": "그러게요~ 비와서 선선해지나 했는데 다시..",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013735.1.3",
          "form": "소나기도 많이 오는데 우산은 잘 챙겨 다니세요?",
          "original_form": "소나기도 많이 오는데 우산은 잘 챙겨 다니세요?",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013735.1.4",
          "form": "네 저는 하늘이 좀 흐리면 꼭 챙겨가요.. ㅎㅎ",
          "original_form": "네 저는 하늘이 좀 흐리면 꼭 챙겨가요.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013735.1.5",
          "form": "장우산이랑 접이식우산이랑 어느쪽 선호하시나요",
          "original_form": "장우산이랑 접이식우산이랑 어느쪽 선호하시나요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013735.1.6",
          "form": "저는 우산은 장우산이요!",
          "original_form": "저는 우산은 장우산이요!",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013735.1.7",
          "form": "튼튼하고 넓은 우산 좋아해요",
          "original_form": "튼튼하고 넓은 우산 좋아해요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013735.1.8",
          "form": "비올 때 신발은 뭐 신는 편이세요?",
          "original_form": "비올 때 신발은 뭐 신는 편이세요?",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013735.1.9",
          "form": "저도요!!! 좀 무겁긴 한데..",
          "original_form": "저도요!!! 좀 무겁긴 한데..",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013735.1.10",
          "form": "저는 샌들 신어요 많이 오면 ㅎㅎ",
          "original_form": "저는 샌들 신어요 많이 오면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013735.1.11",
          "form": "저도요 그냥 젖고 마르는 게 좋아요ㅋㅋ",
          "original_form": "저도요 그냥 젖고 마르는 게 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013735.1.12",
          "form": "그쵸 ㅋㅋㅋ 찝찝한건 넘 싫어서..",
          "original_form": "그쵸 ㅋㅋㅋ 찝찝한건 넘 싫어서..",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013735.1.13",
          "form": "제일 좋아하는 계절은요?",
          "original_form": "제일 좋아하는 계절은요?",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013735.1.14",
          "form": "저..가을?",
          "original_form": "저..가을?",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013735.1.15",
          "form": "봄 좋아하는데",
          "original_form": "봄 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013735.1.16",
          "form": "꽃가루알러지가 있어요ㅠㅠ",
          "original_form": "꽃가루알러지가 있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013735.1.17",
          "form": "아 저도 비염 있어서 봄에 재채기 많이 해요 ㅠㅠㅠ",
          "original_form": "아 저도 비염 있어서 봄에 재채기 많이 해요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013735.1.18",
          "form": "그래도 마스크 쓰고 훨씬 덜하더라고요",
          "original_form": "그래도 마스크 쓰고 훨씬 덜하더라고요",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013735.1.19",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013735.1.20",
          "form": "마스크 쓰고부터 황사에 목 아프고 꽃가루에 콧물 흘리던 거 덜해졌어요",
          "original_form": "마스크 쓰고부터 황사에 목 아프고 꽃가루에 콧물 흘리던 거 덜해졌어요",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013735.1.21",
          "form": "그쵸 신기하더라고요 ㅎㅎ",
          "original_form": "그쵸 신기하더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013735.1.22",
          "form": "계시는 지역에 비 많이 왔나요?",
          "original_form": "계시는 지역에 비 많이 왔나요?",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013735.1.23",
          "form": "폭우는 아니고 오다 안 오다 하네요",
          "original_form": "폭우는 아니고 오다 안 오다 하네요",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013735.1.24",
          "form": "비 피해 없으세요?",
          "original_form": "비 피해 없으세요?",
          "speaker_id": "1",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013735.1.25",
          "form": "네 저희쪽도 크게 피해는 없고 밤에 많이 오더라고요~~",
          "original_form": "네 저희쪽도 크게 피해는 없고 밤에 많이 오더라고요~~",
