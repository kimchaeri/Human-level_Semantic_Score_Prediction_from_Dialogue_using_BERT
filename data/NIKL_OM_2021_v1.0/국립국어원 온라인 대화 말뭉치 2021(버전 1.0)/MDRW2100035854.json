      "utterance": [
        {
          "id": "MDRW2100035854.1.1",
          "form": "안녕하세용! 이번주 내내 비가 온다고하더니 지금은 날이 갰네용ㅎㅎ",
          "original_form": "안녕하세용! 이번주 내내 비가 온다고하더니 지금은 날이 갰네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100035854.1.2",
          "form": "저녁때 되니까 엄청 어두워졌어요ㅠㅠ",
          "original_form": "저녁때 되니까 엄청 어두워졌어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100035854.1.3",
          "form": "낼은 진짜 비 올것같아요",
          "original_form": "낼은 진짜 비 올것같아요",
          "speaker_id": "2",
          "time": "20210830 18:35"
        },
        {
          "id": "MDRW2100035854.1.4",
          "form": "넹넹 오늘은 비온다했는데 안와서 다행이네요ㅎㅎ",
          "original_form": "넹넹 오늘은 비온다했는데 안와서 다행이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100035854.1.5",
          "form": "아침에 우산 챙기세용!",
          "original_form": "아침에 우산 챙기세용!",
          "speaker_id": "1",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100035854.1.6",
          "form": "네!!챙겨야겠어요",
          "original_form": "네!!챙겨야겠어요",
          "speaker_id": "2",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100035854.1.7",
          "form": "빨리 눈이 왔으면 좋겠어요",
          "original_form": "빨리 눈이 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100035854.1.8",
          "form": "저도 눈내리는거 좋아하는데",
          "original_form": "저도 눈내리는거 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.9",
          "form": "눈 좋아하시나봐요~",
          "original_form": "눈 좋아하시나봐요~",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.10",
          "form": "완전 좋아해요~",
          "original_form": "완전 좋아해요~",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.11",
          "form": "날씨도 추운거 좋아해요!",
          "original_form": "날씨도 추운거 좋아해요!",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.12",
          "form": "엇 저두요!ㅋㅋㅋㅋ",
          "original_form": "엇 저두요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.13",
          "form": "겨울을 제일 좋아해요",
          "original_form": "겨울을 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.14",
          "form": "오ㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100035854.1.15",
          "form": "소복소복 눈 밟는 맛이 있죠ㅋㅋ",
          "original_form": "소복소복 눈 밟는 맛이 있죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100035854.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100035854.1.17",
          "form": "가을은 벌레가 어디서 떨어질지 몰라서 너무 싫어요ㅠㅠ",
          "original_form": "가을은 벌레가 어디서 떨어질지 몰라서 너무 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100035854.1.18",
          "form": "아 맞아요ㅠㅠ",
          "original_form": "아 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100035854.1.19",
          "form": "전 근데 가을보다 여름에",
          "original_form": "전 근데 가을보다 여름에",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100035854.1.20",
          "form": "매미 많이 맞았어욬ㅋㅋㅋ길가다...",
          "original_form": "매미 많이 맞았어욬ㅋㅋㅋ길가다...",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100035854.1.21",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100035854.1.22",
          "form": "여름에 뜬금없이 하늘에서 물떨어지는거 매미오줌이라는 얘기듣고",
          "original_form": "여름에 뜬금없이 하늘에서 물떨어지는거 매미오줌이라는 얘기듣고",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100035854.1.23",
          "form": "여름도 싫어졌어요",
          "original_form": "여름도 싫어졌어요",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100035854.1.24",
          "form": "아ㅠㅠㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100035854.1.25",
          "form": "저도 최근에 그 글 봤어요....",
          "original_form": "저도 최근에 그 글 봤어요....",
          "speaker_id": "1",
          "time": "20210830 18:48"
        }
      ]
    }
