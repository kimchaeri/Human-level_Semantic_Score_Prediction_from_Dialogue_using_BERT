      "utterance": [
        {
          "id": "MDRW2100020749.1.1",
          "form": "버스타고 이동중\r\n너무 덥다",
          "original_form": "버스타고 이동중\r\n너무 덥다",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100020749.1.2",
          "form": "거기는 밖에 덥구나",
          "original_form": "거기는 밖에 덥구나",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020749.1.3",
          "form": "엄청 더워서 좀 있으니 땀난다\r\n방에 있을땐 몰랐는데",
          "original_form": "엄청 더워서 좀 있으니 땀난다\r\n방에 있을땐 몰랐는데",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020749.1.4",
          "form": "ㅜㅜ 버스타서 다행이다",
          "original_form": "ㅜㅜ 버스타서 다행이다",
          "speaker_id": "2",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020749.1.5",
          "form": "응 버스 겁나 시원하더라",
          "original_form": "응 버스 겁나 시원하더라",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020749.1.6",
          "form": "오늘 놀이동산 가나",
          "original_form": "오늘 놀이동산 가나",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100020749.1.7",
          "form": "ㅎㅎ아니 비가 이래오면 거의 운행안해서",
          "original_form": "ㅎㅎ아니 비가 이래오면 거의 운행안해서",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100020749.1.8",
          "form": "아 글쿠만ㅜㅜ 아쉽게따",
          "original_form": "아 글쿠만ㅜㅜ 아쉽게따",
          "speaker_id": "2",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100020749.1.9",
          "form": "가도 별로일거라",
          "original_form": "가도 별로일거라",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100020749.1.10",
          "form": "어쩔수없제",
          "original_form": "어쩔수없제",
          "speaker_id": "1",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100020749.1.11",
          "form": "비가 시원하게 온다",
          "original_form": "비가 시원하게 온다",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100020749.1.12",
          "form": "ㅋㅋ 여행가고싶네",
          "original_form": "ㅋㅋ 여행가고싶네",
          "speaker_id": "2",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100020749.1.13",
          "form": "어제 누구랑 대화했는데 강릉이 뚜벅이가기가 좋대",
          "original_form": "어제 누구랑 대화했는데 강릉이 뚜벅이가기가 좋대",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100020749.1.14",
          "form": "강릉이? 오",
          "original_form": "강릉이? 오",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100020749.1.15",
          "form": "거긴 풍경도 좋자나",
          "original_form": "거긴 풍경도 좋자나",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100020749.1.16",
          "form": "ㅇㅇ 바다 진짜 좋아보이던데",
          "original_form": "ㅇㅇ 바다 진짜 좋아보이던데",
          "speaker_id": "2",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100020749.1.17",
          "form": "힐링으로 좋을거같다",
          "original_form": "힐링으로 좋을거같다",
          "speaker_id": "1",
          "time": "20210829 10:57"
        },
        {
          "id": "MDRW2100020749.1.18",
          "form": "ㅇㅇ강릉 티비에서 보면 되게 좋아보이더라",
          "original_form": "ㅇㅇ강릉 티비에서 보면 되게 좋아보이더라",
          "speaker_id": "2",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100020749.1.19",
          "form": "뭔가 깨끗한 느낌있자나ㅎㅎ",
          "original_form": "뭔가 깨끗한 느낌있자나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020749.1.20",
          "form": "뚜벅이에게 좋다니 안가본곳이기도하고 담에 기봐야겠네",
          "original_form": "뚜벅이에게 좋다니 안가본곳이기도하고 담에 기봐야겠네",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020749.1.21",
          "form": "강원도는 한번도 안가봐서 가보고싶네",
          "original_form": "강원도는 한번도 안가봐서 가보고싶네",
          "speaker_id": "2",
          "time": "20210829 11:03"
        },
        {
          "id": "MDRW2100020749.1.22",
          "form": "거리가 머니까 잘 안가지는데\r\n함가보고싶긴하다ㅎ",
          "original_form": "거리가 머니까 잘 안가지는데\r\n함가보고싶긴하다ㅎ",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100020749.1.23",
          "form": "마져 멀어서 도전할 용기가 안생김",
          "original_form": "마져 멀어서 도전할 용기가 안생김",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100020749.1.24",
          "form": "응 그랬는데\r\n한번 가볼까생각드네",
          "original_form": "응 그랬는데\r\n한번 가볼까생각드네",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100020749.1.25",
          "form": "ㅇㅇ 나도! 뚜벅이가 가기 좋다는 소리 듣고",
          "original_form": "ㅇㅇ 나도! 뚜벅이가 가기 좋다는 소리 듣고",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100020749.1.26",
          "form": "나 내일 부산에 병원간김에 드라이브 하고오게",
          "original_form": "나 내일 부산에 병원간김에 드라이브 하고오게",
          "speaker_id": "2",
          "time": "20210905 21:01"
        }
      ]
    }
  ]
