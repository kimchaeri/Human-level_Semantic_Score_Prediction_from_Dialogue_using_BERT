      "utterance": [
        {
          "id": "MDRW2100026604.1.1",
          "form": "요즘 뭐마심?",
          "original_form": "요즘 뭐마심?",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.2",
          "form": "난 주로 탄산 없는 음료?",
          "original_form": "난 주로 탄산 없는 음료?",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.3",
          "form": "약간 비타민 워터같은 종류 좋아함",
          "original_form": "약간 비타민 워터같은 종류 좋아함",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.4",
          "form": "요즘 비타민워터 안팔지않냐",
          "original_form": "요즘 비타민워터 안팔지않냐",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.5",
          "form": "목넘길 때 거부감 없는??",
          "original_form": "목넘길 때 거부감 없는??",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.6",
          "form": "파는걸못봤는데",
          "original_form": "파는걸못봤는데",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.7",
          "form": "내말이... 그래서 아쉬워",
          "original_form": "내말이... 그래서 아쉬워",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.8",
          "form": "이프로부족할때가 딱이네",
          "original_form": "이프로부족할때가 딱이네",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.9",
          "form": "옛날에 마트에도 많이 팔고",
          "original_form": "옛날에 마트에도 많이 팔고",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026604.1.10",
          "form": "1.5리터 사먹음 ㅋㅋㅋㅋ",
          "original_form": "1.5리터 사먹음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.11",
          "form": "편의점에도 있었거든??",
          "original_form": "편의점에도 있었거든??",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅁㅈㅁㅈ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅁㅈㅁㅈ",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.13",
          "form": "요즘 럾으",
          "original_form": "요즘 럾으",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.14",
          "form": "요샌 없어ㅋㅋㅋㅋㅋㅋ",
          "original_form": "요샌 없어ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.15",
          "form": "불편함ㅡㅡ",
          "original_form": "불편함ㅡㅡ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.16",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.17",
          "form": "내 최애 음료 비타민 레몬 C워터",
          "original_form": "내 최애 음료 비타민 레몬 C워터",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.18",
          "form": "너도 한박스 사먹자",
          "original_form": "너도 한박스 사먹자",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.19",
          "form": "다시 내주라....",
          "original_form": "다시 내주라....",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.20",
          "form": "나는 게롤슈타이너 탄산수",
          "original_form": "나는 게롤슈타이너 탄산수",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.22",
          "form": "한박스 샀다",
          "original_form": "한박스 샀다",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.23",
          "form": "그거도 한병씩은 안 팔더라 그래",
          "original_form": "그거도 한병씩은 안 팔더라 그래",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.24",
          "form": "한박스 24개..",
          "original_form": "한박스 24개..",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.25",
          "form": "ㅋㅋㅋㅋㅋ인터넷이그렇지",
          "original_form": "ㅋㅋㅋㅋㅋ인터넷이그렇지",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100026604.1.26",
          "form": "난 잘 마실거란 보장이 없어서 그렇게 못삼",
          "original_form": "난 잘 마실거란 보장이 없어서 그렇게 못삼",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.27",
          "form": "마트에도 있던데??",
          "original_form": "마트에도 있던데??",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.28",
          "form": "이마트ㅇㅇㅇ",
          "original_form": "이마트ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.29",
          "form": "아 있는데 비싸자나...",
          "original_form": "아 있는데 비싸자나...",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.30",
          "form": "너네가 하도 맛있대서 한번 맛이나 볼까했는데",
          "original_form": "너네가 하도 맛있대서 한번 맛이나 볼까했는데",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.31",
          "form": "그리고 들고 집에못와...",
          "original_form": "그리고 들고 집에못와...",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.32",
          "form": "박스떼기로만 팔더라",
          "original_form": "박스떼기로만 팔더라",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.33",
          "form": "이마트 멀기도하고",
          "original_form": "이마트 멀기도하고",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.34",
          "form": "그칰ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그칰ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.35",
          "form": "앗.. 아니던데 6개씩 묶어팔어",
          "original_form": "앗.. 아니던데 6개씩 묶어팔어",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026604.1.36",
          "form": "이마트몰에서도봤는디 비싸",
          "original_form": "이마트몰에서도봤는디 비싸",
          "speaker_id": "1",
