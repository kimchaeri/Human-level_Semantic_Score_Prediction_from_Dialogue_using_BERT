      "utterance": [
        {
          "id": "MDRW2100003962.1.1",
          "form": "게임좋아해요?",
          "original_form": "게임좋아해요?",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003962.1.2",
          "form": "저는 게임 잘 안해서 초딩때한게임 아직도하네요",
          "original_form": "저는 게임 잘 안해서 초딩때한게임 아직도하네요",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003962.1.3",
          "form": "초딩때 한거 어떤거요?",
          "original_form": "초딩때 한거 어떤거요?",
          "speaker_id": "2",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003962.1.4",
          "form": "테일즈런너 아세요?ㅋㅋ",
          "original_form": "테일즈런너 아세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003962.1.5",
          "form": "저 초딩때인기많았는데..추억",
          "original_form": "저 초딩때인기많았는데..추억",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003962.1.6",
          "form": "게임을 잘 안해서 모르겠어요",
          "original_form": "게임을 잘 안해서 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003962.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003962.1.8",
          "form": "모바일겜도안하세요?",
          "original_form": "모바일겜도안하세요?",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003962.1.9",
          "form": "네~예전에는 좀 했었는데 ㅎ",
          "original_form": "네~예전에는 좀 했었는데 ㅎ",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003962.1.10",
          "form": "저도 요즘엔 거의 안해요",
          "original_form": "저도 요즘엔 거의 안해요",
          "speaker_id": "1",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003962.1.11",
          "form": "그냥 안하게 되더라고요",
          "original_form": "그냥 안하게 되더라고요",
          "speaker_id": "1",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003962.1.12",
          "form": "네~ㅎ 재미가 없어졌어요",
          "original_form": "네~ㅎ 재미가 없어졌어요",
          "speaker_id": "2",
          "time": "20210821 14:14"
        },
        {
          "id": "MDRW2100003962.1.13",
          "form": "저는 농장 키우는 게임 좋아했었어요",
          "original_form": "저는 농장 키우는 게임 좋아했었어요",
          "speaker_id": "2",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003962.1.14",
          "form": "오 타이니팜?그런거요?",
          "original_form": "오 타이니팜?그런거요?",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003962.1.15",
          "form": "생각없이하다보면 몇시간 지나있는 그런게임이져",
          "original_form": "생각없이하다보면 몇시간 지나있는 그런게임이져",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003962.1.16",
          "form": "네~그런종류를 참 많이 했었어요 ㅎㅎ",
          "original_form": "네~그런종류를 참 많이 했었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003962.1.17",
          "form": "저도 그런게임 한번 시작하면 중독수준으로 했는데",
          "original_form": "저도 그런게임 한번 시작하면 중독수준으로 했는데",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003962.1.18",
          "form": "또 한번 안하면 영원히 안하게되더라고요",
          "original_form": "또 한번 안하면 영원히 안하게되더라고요",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003962.1.19",
          "form": "맞아요!!!저도요!!",
          "original_form": "맞아요!!!저도요!!",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003962.1.20",
          "form": "ㅋㅋㅋㅋ그래서 되게 극단적이게 되는거같네요",
          "original_form": "ㅋㅋㅋㅋ그래서 되게 극단적이게 되는거같네요",
