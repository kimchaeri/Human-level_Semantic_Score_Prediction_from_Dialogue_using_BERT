      "utterance": [
        {
          "id": "MDRW2100045732.1.1",
          "form": "여자에요 ㅎㅎ",
          "original_form": "여자에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:20"
        },
        {
          "id": "MDRW2100045732.1.2",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "1",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045732.1.3",
          "form": "벌써 9살이에요",
          "original_form": "벌써 9살이에요",
          "speaker_id": "2",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045732.1.4",
          "form": "같이산지 오래되었네요",
          "original_form": "같이산지 오래되었네요",
          "speaker_id": "2",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045732.1.5",
          "form": "오  무슨장난감을 좋아하나요?",
          "original_form": "오  무슨장난감을 좋아하나요?",
          "speaker_id": "1",
          "time": "20210908 06:23"
        },
        {
          "id": "MDRW2100045732.1.6",
          "form": "깃털같은건 안좋아하고 머리끈류 좋아해서 골치에요",
          "original_form": "깃털같은건 안좋아하고 머리끈류 좋아해서 골치에요",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045732.1.7",
          "form": "그래서 수제로 만들어줘요",
          "original_form": "그래서 수제로 만들어줘요",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045732.1.8",
          "form": "name1님은 한번도 반려동물을 키워본적이 없으신가요?",
          "original_form": "&name1&님은 한번도 반려동물을 키워본적이 없으신가요?",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045732.1.9",
          "form": "오 애정듬뿍이시네요 name3는 좋겠엉ㅅ",
          "original_form": "오 애정듬뿍이시네요 &name3&는 좋겠엉ㅅ",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045732.1.10",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045732.1.11",
          "form": "저 초등학생때 카웠었는데",
          "original_form": "저 초등학생때 카웠었는데",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045732.1.12",
          "form": "할머니가 적적해하셔서 두고 왔다가",
          "original_form": "할머니가 적적해하셔서 두고 왔다가",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045732.1.13",
          "form": "걔가 목줄 느슨했는지 사라져버렸대요",
          "original_form": "걔가 목줄 느슨했는지 사라져버렸대요",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045732.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045732.1.15",
          "form": "저도 초딩때 그런경험있어요",
          "original_form": "저도 초딩때 그런경험있어요",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045732.1.16",
          "form": "어렸을때 한번쯤은 그런경험이 있는것같아요",
          "original_form": "어렸을때 한번쯤은 그런경험이 있는것같아요",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045732.1.17",
          "form": "맞아요 흔하죠",
          "original_form": "맞아요 흔하죠",
          "speaker_id": "1",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045732.1.18",
          "form": "부모님들이 자식을 하나의 사람으로 보는게 아니라",
          "original_form": "부모님들이 자식을 하나의 사람으로 보는게 아니라",
          "speaker_id": "1",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045732.1.19",
          "form": "소유물처럼 보기때문에 일어나는 일 같아요",
          "original_form": "소유물처럼 보기때문에 일어나는 일 같아요",
          "speaker_id": "1",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045732.1.20",
          "form": "대화를 잘못올리신것같아요!ㅋㅋ",
          "original_form": "대화를 잘못올리신것같아요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045732.1.21",
          "form": "강아지 두고온거 말한거 맞아요 ㅋㅋ",
          "original_form": "강아지 두고온거 말한거 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045732.1.22",
          "form": "저는 그렇게 생각하거든요",
          "original_form": "저는 그렇게 생각하거든요",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045732.1.23",
          "form": "저는 싫다고 했는데 제 의사랑은 상관없었어요",
          "original_form": "저는 싫다고 했는데 제 의사랑은 상관없었어요",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045732.1.24",
          "form": "아 그런뜻이었군요",
          "original_form": "아 그런뜻이었군요",
          "speaker_id": "2",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045732.1.25",
          "form": "네 저도 그당시에 부모님이 억지로 할머니댁에 데려다놔서.. 결국 없어지고",
          "original_form": "네 저도 그당시에 부모님이 억지로 할머니댁에 데려다놔서.. 결국 없어지고",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045732.1.26",
          "form": "그때기억이 아직도 있네요",
          "original_form": "그때기억이 아직도 있네요",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045732.1.27",
          "form": "저도 그때 그 기분이나 그 표정 강아지 모습 다 기억나요",
          "original_form": "저도 그때 그 기분이나 그 표정 강아지 모습 다 기억나요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045732.1.28",
          "form": "아마도 트라우마가 됐나봐요",
          "original_form": "아마도 트라우마가 됐나봐요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045732.1.29",
          "form": "그때 집에와서 이틀을 내리 울었는데",
          "original_form": "그때 집에와서 이틀을 내리 울었는데",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045732.1.30",
          "form": "미안하단 말씀 없으셨어요 ㅋㅋ",
          "original_form": "미안하단 말씀 없으셨어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045732.1.31",
          "form": "저는 엄마도 같이 우셨는데 어찌됐든 버린거니...",
          "original_form": "저는 엄마도 같이 우셨는데 어찌됐든 버린거니...",
          "speaker_id": "2",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045732.1.32",
          "form": "지금은 인식이 많이 달라져서 다행이에요",
          "original_form": "지금은 인식이 많이 달라져서 다행이에요",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045732.1.33",
          "form": "옛날에는 동물 유기에 대한 아무 개념도 없었는데",
          "original_form": "옛날에는 동물 유기에 대한 아무 개념도 없었는데",
          "speaker_id": "2",
          "time": "20210908 06:34"
        }
      ]
