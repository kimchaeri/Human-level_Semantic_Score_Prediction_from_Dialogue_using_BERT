      "utterance": [
        {
          "id": "MDRW2100039430.1.1",
          "form": "오 나이스",
          "original_form": "오 나이스",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.2",
          "form": "침대",
          "original_form": "침대",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.3",
          "form": "난둘다좋아",
          "original_form": "난둘다좋아",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.4",
          "form": "2층침대 헐..",
          "original_form": "2층침대 헐..",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.5",
          "form": "난 무조건 1층",
          "original_form": "난 무조건 1층",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.6",
          "form": "난 높이잇느겨",
          "original_form": "난 높이잇느겨",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.7",
          "form": "젛아해",
          "original_form": "젛아해",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.8",
          "form": "이지만",
          "original_form": "이지만",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.9",
          "form": "2층 안쓰면 죽어야햔다 그러며",
          "original_form": "2층 안쓰면 죽어야햔다 그러며",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039430.1.10",
          "form": "2층도 쓸수잇긴함",
          "original_form": "2층도 쓸수잇긴함",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.12",
          "form": "저는 그 낮은 천장땜에",
          "original_form": "저는 그 낮은 천장땜에",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.13",
          "form": "답잡하고",
          "original_form": "답잡하고",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.14",
          "form": "울학교는",
          "original_form": "울학교는",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.15",
          "form": "사다리를 잘 목타서",
          "original_form": "사다리를 잘 목타서",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.16",
          "form": "무조건2층이ㅕㅅ지",
          "original_form": "무조건2층이ㅕㅅ지",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.17",
          "form": "글고 막",
          "original_form": "글고 막",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.18",
          "form": "몸이 아프거나",
          "original_form": "몸이 아프거나",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.19",
          "form": "다리다쳣는데",
          "original_form": "다리다쳣는데",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.20",
          "form": "2층써야하면",
          "original_form": "2층써야하면",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.21",
          "form": "1힘들거같아요",
          "original_form": "1힘들거같아요",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.22",
          "form": "개불편할듯",
          "original_form": "개불편할듯",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.23",
          "form": "근대 2층장점",
          "original_form": "근대 2층장점",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039430.1.24",
          "form": "2층 침구 어기족 꺼내고",
          "original_form": "2층 침구 어기족 꺼내고",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.25",
          "form": "침대올라가면 사생활이 어느정도 보장됨",
          "original_form": "침대올라가면 사생활이 어느정도 보장됨",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.26",
          "form": "먼데요",
          "original_form": "먼데요",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.27",
          "form": "오 그건 또",
          "original_form": "오 그건 또",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.28",
          "form": "맞난 말이다",
          "original_form": "맞난 말이다",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.29",
          "form": "1층에잇는사람에게",
          "original_form": "1층에잇는사람에게",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.30",
          "form": "2인실 쓰면 참",
          "original_form": "2인실 쓰면 참",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.31",
          "form": "좋겟네요",
          "original_form": "좋겟네요",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.32",
          "form": "그리고 다같이 2층일때",
          "original_form": "그리고 다같이 2층일때",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.33",
          "form": "잠덜깨서",
          "original_form": "잠덜깨서",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.34",
          "form": "처다보면 웃김",
          "original_form": "처다보면 웃김",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.36",
          "form": "1층도",
          "original_form": "1층도",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.37",
          "form": "그건 웃길듯?",
          "original_form": "그건 웃길듯?",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.38",
          "form": "개웃겨",
          "original_form": "개웃겨",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039430.1.39",
          "form": "그리고 1층사람을",
          "original_form": "그리고 1층사람을",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.40",
          "form": "내려다볼수잇는기회",
          "original_form": "내려다볼수잇는기회",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.41",
          "form": "보장됨",
          "original_form": "보장됨",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.42",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.43",
          "form": "1층망권~",
          "original_form": "1층망권~",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.44",
          "form": "단점 가끔",
          "original_form": "단점 가끔",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.45",
          "form": "폰하다가 떨어뜨리면",
          "original_form": "폰하다가 떨어뜨리면",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039430.1.46",
          "form": "저세상끝까지 감",
          "original_form": "저세상끝까지 감",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039430.1.47",
          "form": "룸메들 화들짝~",
          "original_form": "룸메들 화들짝~",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039430.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039430.1.49",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039430.1.50",
          "form": "야광별 붙일수잇는 장점",
          "original_form": "야광별 붙일수잇는 장점",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039430.1.51",
          "form": "있긴하네요",
          "original_form": "있긴하네요",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039430.1.52",
          "form": "난붙엿지",
          "original_form": "난붙엿지",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039430.1.53",
          "form": "ㅇ야광별붙이면 기분좋다",
          "original_form": "ㅇ야광별붙이면 기분좋다",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039430.1.54",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039430.1.55",
          "form": "야광별 저는 살면서",
          "original_form": "야광별 저는 살면서",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039430.1.56",
          "form": "한번도 붙여본적어ㅃㅅ어요",
          "original_form": "한번도 붙여본적어ㅃㅅ어요",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039430.1.57",
          "form": "살짣 로망이다",
          "original_form": "살짣 로망이다",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039430.1.58",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039430.1.59",
          "form": "꼭붙여봐야만",
          "original_form": "꼭붙여봐야만",
          "speaker_id": "2",
          "time": "20210903 17:39"
        }
      ]
