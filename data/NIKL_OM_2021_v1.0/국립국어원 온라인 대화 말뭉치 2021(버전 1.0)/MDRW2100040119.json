      "utterance": [
        {
          "id": "MDRW2100040119.1.1",
          "form": "손목보호대 좋네요",
          "original_form": "손목보호대 좋네요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100040119.1.2",
          "form": "이쁘지만 아직 쫌더 성능 개선되면 사는게 맞는거같아요 ㅎㅎ",
          "original_form": "이쁘지만 아직 쫌더 성능 개선되면 사는게 맞는거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100040119.1.3",
          "form": "존버!!!!",
          "original_form": "존버!!!!",
          "speaker_id": "1",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100040119.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100040119.1.5",
          "form": "어제 니트랑 바지 샀어요",
          "original_form": "어제 니트랑 바지 샀어요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100040119.1.6",
          "form": "오 가을옷장만하셧군요",
          "original_form": "오 가을옷장만하셧군요",
          "speaker_id": "1",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100040119.1.7",
          "form": "네네 얼른 왔음 합니다",
          "original_form": "네네 얼른 왔음 합니다",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100040119.1.8",
          "form": "가을옷 지르셨나요",
          "original_form": "가을옷 지르셨나요",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100040119.1.9",
          "form": "이제 아침저녁으로는",
          "original_form": "이제 아침저녁으로는",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040119.1.10",
          "form": "쌀쌀해서 잘사셧어욤",
          "original_form": "쌀쌀해서 잘사셧어욤",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040119.1.11",
          "form": "저는 땀쟁이라서",
          "original_form": "저는 땀쟁이라서",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040119.1.12",
          "form": "아직 추석즈음 지를려구여",
          "original_form": "아직 추석즈음 지를려구여",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100040119.1.13",
          "form": "네네 저도요 ㅋㅋ",
          "original_form": "네네 저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100040119.1.14",
          "form": "미리사두고 나중에 입우려고요",
          "original_form": "미리사두고 나중에 입우려고요",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100040119.1.15",
          "form": "좋아하는 브랜드잇으신가욤",
          "original_form": "좋아하는 브랜드잇으신가욤",
          "speaker_id": "1",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100040119.1.16",
          "form": "저는 보세좋아해요 인터넷 쇼핑 ㅋㅋ",
          "original_form": "저는 보세좋아해요 인터넷 쇼핑 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:14"
        },
        {
          "id": "MDRW2100040119.1.17",
          "form": "크 저두",
          "original_form": "크 저두",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100040119.1.18",
          "form": "인터넷쇼핑몰 겁나",
          "original_form": "인터넷쇼핑몰 겁나",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100040119.1.19",
          "form": "애용합니당",
          "original_form": "애용합니당",
          "speaker_id": "1",
          "time": "20210904 11:27"
        },
        {
          "id": "MDRW2100040119.1.20",
          "form": "저도요 좋아하는 쇼핑몰 잇나요",
          "original_form": "저도요 좋아하는 쇼핑몰 잇나요",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100040119.1.21",
          "form": "메이비베이비 슬로우앤드",
          "original_form": "메이비베이비 슬로우앤드",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100040119.1.22",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100040119.1.23",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040119.1.24",
          "form": "가내스라랑",
          "original_form": "가내스라랑",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040119.1.25",
          "form": "가내스라는 첨들어밧어욤 오홍",
          "original_form": "가내스라는 첨들어밧어욤 오홍",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040119.1.26",
          "form": "검색한번해보셔요 맘에드실거에요",
          "original_form": "검색한번해보셔요 맘에드실거에요",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040119.1.27",
          "form": "지그재그에도있구",
          "original_form": "지그재그에도있구",
          "speaker_id": "2",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040119.1.28",
          "form": "지그재그 잇어욧!!!",
          "original_form": "지그재그 잇어욧!!!",
          "speaker_id": "1",
          "time": "20210904 11:36"
        },
        {
          "id": "MDRW2100040119.1.29",
          "form": "네네 분명 좋아하실듯",
          "original_form": "네네 분명 좋아하실듯",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040119.1.30",
          "form": "위드윤 메리어라운드 98도씨",
          "original_form": "위드윤 메리어라운드 98도씨",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040119.1.31",
          "form": "이런데",
          "original_form": "이런데",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040119.1.32",
          "form": "와 저도 거기 다 좋아해요",
          "original_form": "와 저도 거기 다 좋아해요",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040119.1.33",
          "form": "아이쇼핑 한바퀴해요 크크",
          "original_form": "아이쇼핑 한바퀴해요 크크",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100040119.1.34",
          "form": "인수타 하다보면",
          "original_form": "인수타 하다보면",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100040119.1.35",
          "form": "살게너무많아요",
          "original_form": "살게너무많아요",
          "speaker_id": "2",
          "time": "20210904 11:38"
        }
      ]
