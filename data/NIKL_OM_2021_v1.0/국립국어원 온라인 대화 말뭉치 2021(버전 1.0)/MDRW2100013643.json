      "utterance": [
        {
          "id": "MDRW2100013643.1.1",
          "form": "여행은 좋아하세요?",
          "original_form": "여행은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013643.1.2",
          "form": "그렇게 좋아하진 않지만 그래도 일년에 몇번가요",
          "original_form": "그렇게 좋아하진 않지만 그래도 일년에 몇번가요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013643.1.3",
          "form": "그래도 많이 다니시네요 전 좋아해도 일년에 몇번 못가요",
          "original_form": "그래도 많이 다니시네요 전 좋아해도 일년에 몇번 못가요",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013643.1.4",
          "form": "요즘은 전혀못가죠",
          "original_form": "요즘은 전혀못가죠",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013643.1.5",
          "form": "그래서 전 슬퍼요 데 힐링인데... ㅠ",
          "original_form": "그래서 전 슬퍼요 데 힐링인데... ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013643.1.6",
          "form": "맞아요 진짜 어딜못가니 낙이없네요",
          "original_form": "맞아요 진짜 어딜못가니 낙이없네요",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013643.1.7",
          "form": "전 그래도 최근에 영월 다녀왔는데 동굴이 여름이여도 엄청 시원해서 좋더라고요 ㅎ",
          "original_form": "전 그래도 최근에 영월 다녀왔는데 동굴이 여름이여도 엄청 시원해서 좋더라고요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013643.1.8",
          "form": "와 좋으셨겠어요\r\n동굴에 가본적이 없는데ㅋ",
          "original_form": "와 좋으셨겠어요\r\n동굴에 가본적이 없는데ㅋ",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013643.1.9",
          "form": "혹여라도 근처에 동굴 있는데 가면 놀러가보세요 ㅎㅎ 여름은 시원하고 겨울은 따뜻해요 ㅎ",
          "original_form": "혹여라도 근처에 동굴 있는데 가면 놀러가보세요 ㅎㅎ 여름은 시원하고 겨울은 따뜻해요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013643.1.10",
          "form": "그래야겠네요 \r\n여름에 가면 시원해서 좋을거같아요",
          "original_form": "그래야겠네요 \r\n여름에 가면 시원해서 좋을거같아요",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013643.1.11",
          "form": "최근에 다녀온 곳 있으세요?",
          "original_form": "최근에 다녀온 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013643.1.12",
          "form": "멀리간건 여수요",
          "original_form": "멀리간건 여수요",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013643.1.13",
          "form": "비가 많이와서 구경도 많이 못했어요",
          "original_form": "비가 많이와서 구경도 많이 못했어요",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013643.1.14",
          "form": "저도 여수갔을 때 비와서 구경 못했는데 동지네요!",
          "original_form": "저도 여수갔을 때 비와서 구경 못했는데 동지네요!",
          "speaker_id": "1",
          "time": "20210825 16:37"
        }
      ]
