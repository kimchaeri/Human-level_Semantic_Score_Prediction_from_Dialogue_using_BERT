      "utterance": [
        {
          "id": "MDRW2100037194.1.1",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100037194.1.2",
          "form": "맛집 줄은 기다리시나요?",
          "original_form": "맛집 줄은 기다리시나요?",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100037194.1.3",
          "form": "음 네네 ! 근데 30분정도가 한계에요 ㅋㅋㅋ",
          "original_form": "음 네네 ! 근데 30분정도가 한계에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037194.1.4",
          "form": "저도용ㅋㅋㅋ",
          "original_form": "저도용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037194.1.5",
          "form": "줄 길면 다른곳가요ㅋㅋㅋ",
          "original_form": "줄 길면 다른곳가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100037194.1.6",
          "form": "그렇게 기다렸는데",
          "original_form": "그렇게 기다렸는데",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037194.1.7",
          "form": "맛없으면 화나잖아요 !",
          "original_form": "맛없으면 화나잖아요 !",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037194.1.8",
          "form": "맞아요!! 기다렸다구 할인해주는것두 아니고요",
          "original_form": "맞아요!! 기다렸다구 할인해주는것두 아니고요",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100037194.1.9",
          "form": "맞아욬ㅋㅋ 전 그리구 맛집인데",
          "original_form": "맞아욬ㅋㅋ 전 그리구 맛집인데",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037194.1.10",
          "form": "불친절해도 안가요 ㅜ",
          "original_form": "불친절해도 안가요 ㅜ",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037194.1.11",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100037194.1.12",
          "form": "맞죠 !!",
          "original_form": "맞죠 !!",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.13",
          "form": "너무 잘돼서 뻗대는곳 있잖아요!",
          "original_form": "너무 잘돼서 뻗대는곳 있잖아요!",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.14",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.15",
          "form": "오든지말든지 느낌...",
          "original_form": "오든지말든지 느낌...",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.16",
          "form": "한번 갔다가 실망스러워서 맛집 별거없다 싶달까요",
          "original_form": "한번 갔다가 실망스러워서 맛집 별거없다 싶달까요",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.17",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.18",
          "form": "그럴거믄 장사 왜 하시는건지..",
          "original_form": "그럴거믄 장사 왜 하시는건지..",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100037194.1.19",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037194.1.20",
          "form": "사람들이 극찬하는데",
          "original_form": "사람들이 극찬하는데",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037194.1.21",
          "form": "막상 가보면 불친절하고 생각보다 맛도 없고",
          "original_form": "막상 가보면 불친절하고 생각보다 맛도 없고",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100037194.1.22",
          "form": "그런곳이 많아요..",
          "original_form": "그런곳이 많아요..",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100037194.1.23",
          "form": "진짜요 저 그래서 안가게 된달까요",
          "original_form": "진짜요 저 그래서 안가게 된달까요",
          "speaker_id": "2",
