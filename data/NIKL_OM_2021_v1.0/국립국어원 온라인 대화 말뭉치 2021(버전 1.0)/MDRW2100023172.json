      "utterance": [
        {
          "id": "MDRW2100023172.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023172.1.2",
          "form": "혹시 루꼴라피자 좋아하시나요?!",
          "original_form": "혹시 루꼴라피자 좋아하시나요?!",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023172.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023172.1.4",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023172.1.5",
          "form": "저두 루꼴라피자 좋아해요ㅎㅎㅎ",
          "original_form": "저두 루꼴라피자 좋아해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023172.1.6",
          "form": "루꼴라 좋아하시면 주로 양식류를 선호하시나용??",
          "original_form": "루꼴라 좋아하시면 주로 양식류를 선호하시나용??",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023172.1.7",
          "form": "가리는 거 없어용ㅋㅋ",
          "original_form": "가리는 거 없어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023172.1.8",
          "form": "name1은 뭐 좋아하세요?",
          "original_form": "&name1&은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023172.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ가리는거 없으시다니 베스트 오브 베스트시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ가리는거 없으시다니 베스트 오브 베스트시네요",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023172.1.10",
          "form": "저는 일식을 제일 좋아해요!! 그중에서도 초밥을 제일 좋아합니당",
          "original_form": "저는 일식을 제일 좋아해요!! 그중에서도 초밥을 제일 좋아합니당",
          "speaker_id": "1",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023172.1.11",
          "form": "초밥도 맛있죠 ㅋㅋㅋ",
          "original_form": "초밥도 맛있죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023172.1.12",
          "form": "근데 비싸서 자주 못 먹어요ㅠㅠ",
          "original_form": "근데 비싸서 자주 못 먹어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023172.1.13",
          "form": "ㅠㅠㅠㅠㅠ맞아요",
          "original_form": "ㅠㅠㅠㅠㅠ맞아요",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023172.1.14",
          "form": "맛있지만ㅁ 먹을려면 큰맴먹고 먹어야ㅑ하는 친구죠ㅠㅠㅠ",
          "original_form": "맛있지만ㅁ 먹을려면 큰맴먹고 먹어야ㅑ하는 친구죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023172.1.15",
          "form": "무슨 초밥 좋아하세요?",
          "original_form": "무슨 초밥 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023172.1.16",
          "form": "저는 초밥류는 거의 안가리고 다 좋아하는ㄴ데",
          "original_form": "저는 초밥류는 거의 안가리고 다 좋아하는ㄴ데",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023172.1.17",
          "form": "뱃살ㄹ같이 기름진 부위만 조큼 그렇더라구요..",
          "original_form": "뱃살ㄹ같이 기름진 부위만 조큼 그렇더라구요..",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023172.1.18",
          "form": "앗 그럼 연어도 별로신가요?",
          "original_form": "앗 그럼 연어도 별로신가요?",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023172.1.19",
          "form": "앗ㄱ 연어는 많이 못먹어서 그렇지 좋아합니다ㅏ!!",
          "original_form": "앗ㄱ 연어는 많이 못먹어서 그렇지 좋아합니다ㅏ!!",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023172.1.20",
          "form": "광어 뱃살 이런애들보단 덜 기름지더라구용",
          "original_form": "광어 뱃살 이런애들보단 덜 기름지더라구용",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023172.1.21",
          "form": "초밥 얘기하니까 초밥 먹고 싶어요",
          "original_form": "초밥 얘기하니까 초밥 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023172.1.22",
          "form": "앗 저두용",
          "original_form": "앗 저두용",
          "speaker_id": "1",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100023172.1.23",
          "form": "오늘 점심은 초밥 먹을까ㅏ봐요",
          "original_form": "오늘 점심은 초밥 먹을까ㅏ봐요",
          "speaker_id": "1",
          "time": "20210830 10:54"
