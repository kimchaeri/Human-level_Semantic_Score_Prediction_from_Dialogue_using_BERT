      "utterance": [
        {
          "id": "MDRW2100020711.1.1",
          "form": "아 맛없었던 젤리 저도 있었어요",
          "original_form": "아 맛없었던 젤리 저도 있었어요",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020711.1.2",
          "form": "감초맛 젤리 드셔보셨나요",
          "original_form": "감초맛 젤리 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020711.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210828 11:28"
        },
        {
          "id": "MDRW2100020711.1.4",
          "form": "외국애들이 먹는 검은색 젤리..",
          "original_form": "외국애들이 먹는 검은색 젤리..",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.5",
          "form": "그건 무슨맛인ㅇ",
          "original_form": "그건 무슨맛인ㅇ",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.6",
          "form": "아 검정색",
          "original_form": "아 검정색",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.7",
          "form": "먹어본거같다",
          "original_form": "먹어본거같다",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.8",
          "form": "타이어맛이에요",
          "original_form": "타이어맛이에요",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.9",
          "form": "먹고 뱉었던 기억이",
          "original_form": "먹고 뱉었던 기억이",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.10",
          "form": "알거같아요 이거 왜먹나 했었거든요",
          "original_form": "알거같아요 이거 왜먹나 했었거든요",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.11",
          "form": "진짜... 놀랍지 않았나요",
          "original_form": "진짜... 놀랍지 않았나요",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.12",
          "form": "아니 다른건 그렇게 맛있게 만들면서",
          "original_form": "아니 다른건 그렇게 맛있게 만들면서",
          "speaker_id": "1",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020711.1.13",
          "form": "왜 젤리는 그모양으로..",
          "original_form": "왜 젤리는 그모양으로..",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.14",
          "form": "전 그 젤리 살며 한번은 먹어봐야한다고 생각해요",
          "original_form": "전 그 젤리 살며 한번은 먹어봐야한다고 생각해요",
          "speaker_id": "2",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.15",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.16",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.17",
          "form": "진짜 인생의 충격적 경험 중 하나였어요",
          "original_form": "진짜 인생의 충격적 경험 중 하나였어요",
          "speaker_id": "2",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.18",
          "form": "인생의 쓴맛 허무함 ㅋㅋㅋㅋㄱ",
          "original_form": "인생의 쓴맛 허무함 ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.19",
          "form": "아 웃기다",
          "original_form": "아 웃기다",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.20",
          "form": "친구랑 같이 먹고 둘다 식겁해서 뱉었어요ㅋㅋㅋㅠㅠ",
          "original_form": "친구랑 같이 먹고 둘다 식겁해서 뱉었어요ㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.21",
          "form": "그럴만합니다",
          "original_form": "그럴만합니다",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.22",
          "form": "그건 누가먹어도..",
          "original_form": "그건 누가먹어도..",
          "speaker_id": "1",
          "time": "20210828 11:30"
        },
        {
          "id": "MDRW2100020711.1.23",
          "form": "이게 고수 먹었을 때처럼 입안에서 향이 안빠지더라구요",
          "original_form": "이게 고수 먹었을 때처럼 입안에서 향이 안빠지더라구요",
          "speaker_id": "2",
          "time": "20210828 11:30"
        }
      ]
