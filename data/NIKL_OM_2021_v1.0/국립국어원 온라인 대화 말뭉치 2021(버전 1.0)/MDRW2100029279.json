      "utterance": [
        {
          "id": "MDRW2100029279.1.1",
          "form": "그나마 주52시간제 이후론 퇴근 너무 늦으면 적당히 오전 출근시간도 늦추고 있죠ㅜ",
          "original_form": "그나마 주52시간제 이후론 퇴근 너무 늦으면 적당히 오전 출근시간도 늦추고 있죠ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029279.1.2",
          "form": "ㅎ 그다음날 그만큼의 일이 또 생기는 일이죠",
          "original_form": "ㅎ 그다음날 그만큼의 일이 또 생기는 일이죠",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029279.1.3",
          "form": "아 무슨일인가요",
          "original_form": "아 무슨일인가요",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029279.1.4",
          "form": "식견이좁아",
          "original_form": "식견이좁아",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029279.1.5",
          "form": "가늠이 안됩니다",
          "original_form": "가늠이 안됩니다",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029279.1.6",
          "form": "시지프스가 이렇게 살았겠구나 싶음ㅎㅎ",
          "original_form": "시지프스가 이렇게 살았겠구나 싶음ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029279.1.7",
          "form": "시지프스?",
          "original_form": "시지프스?",
          "speaker_id": "1",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029279.1.8",
          "form": "아 언론 쪽서 일해요ㅜ",
          "original_form": "아 언론 쪽서 일해요ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029279.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029279.1.10",
          "form": "대단하십니다",
          "original_form": "대단하십니다",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029279.1.11",
          "form": "아 생각해보니",
          "original_form": "아 생각해보니",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029279.1.12",
          "form": "데드라인!",
          "original_form": "데드라인!",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029279.1.13",
          "form": "언론인 이시네요",
          "original_form": "언론인 이시네요",
          "speaker_id": "1",
          "time": "20210903 01:02"
        },
        {
          "id": "MDRW2100029279.1.14",
          "form": "다음 생에는 절대 이 업엔 종사 안 할 거예요ㅜ",
          "original_form": "다음 생에는 절대 이 업엔 종사 안 할 거예요ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029279.1.15",
          "form": "어쩐지 말씀하시는게 어휘도 다양하게 구사하시고",
          "original_form": "어쩐지 말씀하시는게 어휘도 다양하게 구사하시고",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029279.1.16",
          "form": "힘든가요?",
          "original_form": "힘든가요?",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029279.1.17",
          "form": "마감 스트레스?",
          "original_form": "마감 스트레스?",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029279.1.18",
          "form": "아니ㅋㅋ 제 어휘력은 만화책과 추리소설이 선생이라구욧!",
          "original_form": "아니ㅋㅋ 제 어휘력은 만화책과 추리소설이 선생이라구욧!",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029279.1.19",
          "form": "이 언론은 나에게 똥만 줌ㅜ",
          "original_form": "이 언론은 나에게 똥만 줌ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029279.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029279.1.21",
          "form": "마감도 마감인데..",
          "original_form": "마감도 마감인데..",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029279.1.22",
          "form": "국제행사가 싫고 큰일 터지는 게 너무 싫어요",
          "original_form": "국제행사가 싫고 큰일 터지는 게 너무 싫어요",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029279.1.23",
          "form": "마감은 칼같아야 하는데 변수가 너무 많아서ㅜ",
          "original_form": "마감은 칼같아야 하는데 변수가 너무 많아서ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029279.1.24",
          "form": "올림픽 월드컵 시차부터 따지구 맘편히 못 즐겨요ㅜ",
          "original_form": "올림픽 월드컵 시차부터 따지구 맘편히 못 즐겨요ㅜ",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029279.1.25",
          "form": "아오",
          "original_form": "아오",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029279.1.26",
          "form": "다음 생에는 님처럼 신의 직장 가는게 목표입니다!!",
          "original_form": "다음 생에는 님처럼 신의 직장 가는게 목표입니다!!",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029279.1.27",
          "form": "큰이슈있으면",
          "original_form": "큰이슈있으면",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029279.1.28",
          "form": "신경쓰이겠습니다 ㅠ",
          "original_form": "신경쓰이겠습니다 ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029279.1.29",
          "form": "태아때부터 정진할 거예요!",
          "original_form": "태아때부터 정진할 거예요!",
          "speaker_id": "2",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029279.1.30",
          "form": "지금도 가능하지 않을까요?",
          "original_form": "지금도 가능하지 않을까요?",
          "speaker_id": "1",
          "time": "20210903 01:07"
        }
      ]
    }
  ]
