        {
          "id": "MDRW2100017994.1.2",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017994.1.3",
          "form": "전 요즘 절약에 빠져서 잘 안사요",
          "original_form": "전 요즘 절약에 빠져서 잘 안사요",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017994.1.4",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017994.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017994.1.6",
          "form": "최근에 뭐사셨나요",
          "original_form": "최근에 뭐사셨나요",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017994.1.7",
          "form": "장본 게 다예요",
          "original_form": "장본 게 다예요",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017994.1.8",
          "form": "엄청나네요",
          "original_form": "엄청나네요",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017994.1.9",
          "form": "뭐 사신거있어요?",
          "original_form": "뭐 사신거있어요?",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017994.1.10",
          "form": "음 저도 딱히 없네요",
          "original_form": "음 저도 딱히 없네요",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017994.1.11",
          "form": "요즘은 밖에 나갈 일도 없으니...",
          "original_form": "요즘은 밖에 나갈 일도 없으니...",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017994.1.12",
          "form": "취미용품도 요즘 안 사게돼요",
          "original_form": "취미용품도 요즘 안 사게돼요",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
