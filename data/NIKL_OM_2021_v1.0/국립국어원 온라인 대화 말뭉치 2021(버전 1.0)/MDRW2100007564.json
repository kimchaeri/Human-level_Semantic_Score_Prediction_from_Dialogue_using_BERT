      "utterance": [
        {
          "id": "MDRW2100007564.1.1",
          "form": "최애음식이 뭐야?",
          "original_form": "최애음식이 뭐야?",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007564.1.2",
          "form": "흠 다좋아하는데 오늘은 그릭 복숭아로 할래",
          "original_form": "흠 다좋아하는데 오늘은 그릭 복숭아로 할래",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007564.1.3",
          "form": "먹어봄??",
          "original_form": "먹어봄??",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007564.1.4",
          "form": "그릭복숭아 맛있을거같아",
          "original_form": "그릭복숭아 맛있을거같아",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007564.1.5",
          "form": "설마 그거 그릭모모라고 부르는 사람은 없겠지?",
          "original_form": "설마 그거 그릭모모라고 부르는 사람은 없겠지?",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007564.1.6",
          "form": "모모가 일본어로 복숭아라며",
          "original_form": "모모가 일본어로 복숭아라며",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007564.1.7",
          "form": "근데 그걸 인식한순간 자꾸 그렇게 들리더라",
          "original_form": "근데 그걸 인식한순간 자꾸 그렇게 들리더라",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007564.1.8",
          "form": "맛있더라고 먹어봤어",
          "original_form": "맛있더라고 먹어봤어",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007564.1.9",
          "form": "먹어봤어?",
          "original_form": "먹어봤어?",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007564.1.10",
          "form": "응맛있어",
          "original_form": "응맛있어",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007564.1.11",
          "form": "엄청 특별하진않은데",
          "original_form": "엄청 특별하진않은데",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007564.1.12",
          "form": "예쁘잖아",
          "original_form": "예쁘잖아",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007564.1.13",
          "form": "최애음식이 뭔데?",
          "original_form": "최애음식이 뭔데?",
          "speaker_id": "1",
          "time": "20210822 03:22"
        },
        {
          "id": "MDRW2100007564.1.14",
          "form": "나는 초밥이 맛있는거같아",
          "original_form": "나는 초밥이 맛있는거같아",
          "speaker_id": "2",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007564.1.15",
          "form": "뮤슨초밥??",
          "original_form": "뮤슨초밥??",
          "speaker_id": "1",
          "time": "20210822 03:23"
        },
        {
          "id": "MDRW2100007564.1.16",
          "form": "광어지느러미!",
          "original_form": "광어지느러미!",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007564.1.17",
          "form": "연어도 맛있고",
          "original_form": "연어도 맛있고",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007564.1.18",
          "form": "나오늘연어먹음",
          "original_form": "나오늘연어먹음",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007564.1.19",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007564.1.20",
          "form": "짱맛",
          "original_form": "짱맛",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007564.1.21",
          "form": "알지알지",
          "original_form": "알지알지",
          "speaker_id": "1",
          "time": "20210822 03:27"
        },
        {
          "id": "MDRW2100007564.1.22",
          "form": "알아??",
          "original_form": "알아??",
          "speaker_id": "1",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007564.1.23",
