      "utterance": [
        {
          "id": "MDRW2100011244.1.1",
          "form": "점심은 뭐드세요?",
          "original_form": "점심은 뭐드세요?",
          "speaker_id": "1",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100011244.1.2",
          "form": "저희는 식단표 없어요",
          "original_form": "저희는 식단표 없어요",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100011244.1.3",
          "form": "그냥 가서 주는거 먹는ㅠ",
          "original_form": "그냥 가서 주는거 먹는ㅠ",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100011244.1.4",
          "form": "매일매일이 랜덤박스입니다",
          "original_form": "매일매일이 랜덤박스입니다",
          "speaker_id": "2",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100011244.1.5",
          "form": "차라리 사먹고싶네요",
          "original_form": "차라리 사먹고싶네요",
          "speaker_id": "2",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100011244.1.6",
          "form": "아? 기대될거같은데요 ㅋㅋㅋ",
          "original_form": "아? 기대될거같은데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:59"
        },
        {
          "id": "MDRW2100011244.1.7",
          "form": "ㅖ?? 매일 실망하실걸요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅖ?? 매일 실망하실걸요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011244.1.8",
          "form": "가끔씩 군대밥도 이정돈 아니었는듯 이럽니다",
          "original_form": "가끔씩 군대밥도 이정돈 아니었는듯 이럽니다",
          "speaker_id": "2",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011244.1.9",
          "form": "점심 뭐드세요? 식단표가 있죠??",
          "original_form": "점심 뭐드세요? 식단표가 있죠??",
          "speaker_id": "2",
          "time": "20210824 10:12"
        },
        {
          "id": "MDRW2100011244.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:21"
        },
        {
          "id": "MDRW2100011244.1.11",
          "form": "네 여긴식단표있어요",
          "original_form": "네 여긴식단표있어요",
          "speaker_id": "1",
          "time": "20210824 10:21"
        },
        {
          "id": "MDRW2100011244.1.12",
          "form": "순두부백탕 오징어볶음 야채계란찜 쑥갓나물이네요",
          "original_form": "순두부백탕 오징어볶음 야채계란찜 쑥갓나물이네요",
          "speaker_id": "1",
          "time": "20210824 10:21"
        },
        {
          "id": "MDRW2100011244.1.13",
          "form": "저 순두부가 진짜 손으로 만든거면 맛있는데",
          "original_form": "저 순두부가 진짜 손으로 만든거면 맛있는데",
          "speaker_id": "2",
          "time": "20210824 10:28"
        },
        {
          "id": "MDRW2100011244.1.14",
          "form": "강릉에서 진짜 맛있게 먹었어요",
          "original_form": "강릉에서 진짜 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210824 10:28"
        },
        {
          "id": "MDRW2100011244.1.15",
          "form": "저희는 뭐나오려나",
          "original_form": "저희는 뭐나오려나",
          "speaker_id": "2",
          "time": "20210824 10:28"
        },
        {
          "id": "MDRW2100011244.1.16",
          "form": "밥은 맛있게 나오나요?",
          "original_form": "밥은 맛있게 나오나요?",
          "speaker_id": "2",
          "time": "20210824 10:29"
        },
        {
          "id": "MDRW2100011244.1.17",
          "form": "아,,, 제가 주문넣는데 그냥 마트 순두부입니다만,,,ㅋㅋㅋㅋㅋ",
          "original_form": "아,,, 제가 주문넣는데 그냥 마트 순두부입니다만,,,ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:29"
        },
        {
          "id": "MDRW2100011244.1.18",
          "form": "네 맛있어요 살찌게생겻슴다",
          "original_form": "네 맛있어요 살찌게생겻슴다",
          "speaker_id": "1",
          "time": "20210824 10:29"
        },
        {
          "id": "MDRW2100011244.1.19",
          "form": "오ㅋㅋ 제일 부럽네요",
          "original_form": "오ㅋㅋ 제일 부럽네요",
          "speaker_id": "2",
          "time": "20210824 10:31"
        },
        {
          "id": "MDRW2100011244.1.20",
          "form": "밥이 맛있어야 일할 맛이 나지",
          "original_form": "밥이 맛있어야 일할 맛이 나지",
          "speaker_id": "2",
          "time": "20210824 10:31"
        },
        {
          "id": "MDRW2100011244.1.21",
          "form": "저희는 의욕저하",
          "original_form": "저희는 의욕저하",
          "speaker_id": "2",
          "time": "20210824 10:31"
        },
        {
          "id": "MDRW2100011244.1.22",
          "form": "간식이라도 몰래몰래드세요 ㅋㅋㅋㅋㅋ",
          "original_form": "간식이라도 몰래몰래드세요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011244.1.23",
          "form": "간식 구비되어있나요? 여긴 없습니다ㅋㅋㅋㅋㅋ",
          "original_form": "간식 구비되어있나요? 여긴 없습니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011244.1.24",
          "form": "그래도 음료수는 있어요",
          "original_form": "그래도 음료수는 있어요",
          "speaker_id": "2",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011244.1.25",
          "form": "감사하게도",
          "original_form": "감사하게도",
          "speaker_id": "2",
          "time": "20210824 10:51"
        },
        {
          "id": "MDRW2100011244.1.26",
          "form": "아 제가 아동쪽에서 일해서 사실 제가 먹고싶은간식 애들간식으로 많이사요 ㅋㅋㅋㅋ",
          "original_form": "아 제가 아동쪽에서 일해서 사실 제가 먹고싶은간식 애들간식으로 많이사요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011244.1.27",
          "form": "(비밀입니다)",
          "original_form": "(비밀입니다)",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011244.1.28",
          "form": "오ㅋㅋㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011244.1.29",
          "form": "완전좋네요",
          "original_form": "완전좋네요",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011244.1.30",
          "form": "오예 불고기 나왔습니다 맛점요",
          "original_form": "오예 불고기 나왔습니다 맛점요",
          "speaker_id": "2",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100011244.1.31",
          "form": "저는 닭봉조림먹었어요 메뉴바뀜 ㅎ",
          "original_form": "저는 닭봉조림먹었어요 메뉴바뀜 ㅎ",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011244.1.32",
          "form": "오 간장소스인가요?",
          "original_form": "오 간장소스인가요?",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011244.1.33",
          "form": "넹 완전맛있었어여 한 10개는먹은듯",
          "original_form": "넹 완전맛있었어여 한 10개는먹은듯",
          "speaker_id": "1",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100011244.1.34",
          "form": "ㅋㅋㅋㅋ닭다리보다 닭봉이 맛있더라구요",
          "original_form": "ㅋㅋㅋㅋ닭다리보다 닭봉이 맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011244.1.35",
          "form": "저는 지금 아아에 로아커먹고있어요 스르륵녹아요 졸맛이다",
          "original_form": "저는 지금 아아에 로아커먹고있어요 스르륵녹아요 졸맛이다",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011244.1.36",
          "form": "와 저 로아커 우유맛 진짜 좋아하는데",
          "original_form": "와 저 로아커 우유맛 진짜 좋아하는데",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011244.1.37",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011244.1.38",
          "form": "우유맛이요? 아뇨",
          "original_form": "우유맛이요? 아뇨",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011244.1.39",
          "form": "저는 초코랑 바닐라만 먹어봣어요 우유맛도있구나",
          "original_form": "저는 초코랑 바닐라만 먹어봣어요 우유맛도있구나",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011244.1.40",
          "form": "헐 이걸 모르시다니",
          "original_form": "헐 이걸 모르시다니",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011244.1.41",
          "form": "진짜 맛있어요 물론 기호에 따라 다르겠지만",
          "original_form": "진짜 맛있어요 물론 기호에 따라 다르겠지만",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011244.1.42",
          "form": "꼭 드셔보세요",
          "original_form": "꼭 드셔보세요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011244.1.43",
          "form": "그럼 흰색껍질이겠네요?",
          "original_form": "그럼 흰색껍질이겠네요?",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011244.1.44",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210824 18:26"
        },
        {
          "id": "MDRW2100011244.1.45",
          "form": "고수시군요ㅋㅋㅋㅋ",
          "original_form": "고수시군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:26"
        },
        {
          "id": "MDRW2100011244.1.46",
          "form": "하 바쁘네요 오전엔 한가했는데",
          "original_form": "하 바쁘네요 오전엔 한가했는데",
          "speaker_id": "2",
          "time": "20210824 18:27"
        },
        {
          "id": "MDRW2100011244.1.47",
          "form": "저는 칼퇴했습니다 ㅎㅎㅎ",
          "original_form": "저는 칼퇴했습니다 ㅎㅎㅎ",
          "speaker_id": "1",
