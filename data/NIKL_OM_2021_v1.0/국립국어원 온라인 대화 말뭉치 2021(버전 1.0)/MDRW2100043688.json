      "utterance": [
        {
          "id": "MDRW2100043688.1.1",
          "form": "일하고 계세요?",
          "original_form": "일하고 계세요?",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043688.1.2",
          "form": "지금은 퇴근했어요!",
          "original_form": "지금은 퇴근했어요!",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043688.1.3",
          "form": "오오 일하신지 오래 되셧어요?",
          "original_form": "오오 일하신지 오래 되셧어요?",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043688.1.4",
          "form": "아니요 일년 좀 넘었어요!",
          "original_form": "아니요 일년 좀 넘었어요!",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043688.1.5",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043688.1.6",
          "form": "저는 아직이요!!",
          "original_form": "저는 아직이요!!",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043688.1.7",
          "form": "담주에 첫출근해요!",
          "original_form": "담주에 첫출근해요!",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043688.1.8",
          "form": "오 진짜요?\r\n첫직장이신거에요? 이직하신거에요?",
          "original_form": "오 진짜요?\r\n첫직장이신거에요? 이직하신거에요?",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043688.1.9",
          "form": "처음은 아니고 이직햇어요!!",
          "original_form": "처음은 아니고 이직햇어요!!",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043688.1.10",
          "form": "전직장이 너무 이상햇어서 ㅠㅠ",
          "original_form": "전직장이 너무 이상햇어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043688.1.11",
          "form": "마자요 전직장 이상하면 바로바로  그만두고 새로알아보는것도 좋은거같아요!",
          "original_form": "마자요 전직장 이상하면 바로바로  그만두고 새로알아보는것도 좋은거같아요!",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043688.1.12",
          "form": "거리는 가까워요?",
          "original_form": "거리는 가까워요?",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043688.1.13",
          "form": "40분이용..! 가까운편인가요?? ㅠㅠ",
          "original_form": "40분이용..! 가까운편인가요?? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043688.1.14",
          "form": "네 40분정도면 나쁘지 않은거같아요!",
          "original_form": "네 40분정도면 나쁘지 않은거같아요!",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043688.1.15",
          "form": "무슨직종이세요?",
          "original_form": "무슨직종이세요?",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043688.1.16",
          "form": "저는 간호조무사예요!!!",
          "original_form": "저는 간호조무사예요!!!",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100043688.1.17",
          "form": "근처엔 병원이없네용 ㅠ",
          "original_form": "근처엔 병원이없네용 ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100043688.1.18",
          "form": "요새 더욱더 감사함을 느끼는 직종이시군요ㅠ",
          "original_form": "요새 더욱더 감사함을 느끼는 직종이시군요ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100043688.1.19",
          "form": "근처에 있으면 그나마 편하고 좋을텐데ㅠ",
          "original_form": "근처에 있으면 그나마 편하고 좋을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100043688.1.20",
