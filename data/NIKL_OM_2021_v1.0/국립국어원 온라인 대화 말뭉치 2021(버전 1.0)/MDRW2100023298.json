        },
        {
          "id": "MDRW2100023298.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023298.1.3",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023298.1.4",
          "form": "졸업했어영",
          "original_form": "졸업했어영",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100023298.1.5",
          "form": "학교다닐때 동아리활동하셨었나요",
          "original_form": "학교다닐때 동아리활동하셨었나요",
          "speaker_id": "1",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023298.1.6",
          "form": "넵 대학생 때 했었어요",
          "original_form": "넵 대학생 때 했었어요",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023298.1.7",
          "form": "대학생이세요?",
          "original_form": "대학생이세요?",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023298.1.8",
          "form": "아뇨 졸업한지 오래됐어요 ㅎㅎ..",
          "original_form": "아뇨 졸업한지 오래됐어요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100023298.1.9",
          "form": "어떤동아리하셨어요?",
          "original_form": "어떤동아리하셨어요?",
          "speaker_id": "1",
          "time": "20210830 00:02"
        },
        {
          "id": "MDRW2100023298.1.10",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023298.1.11",
          "form": "저는 광고동아리요 ㅎㅎㅎㅎ",
          "original_form": "저는 광고동아리요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023298.1.12",
          "form": "동아리 활동하셨나요",
          "original_form": "동아리 활동하셨나요",
          "speaker_id": "2",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023298.1.13",
          "form": "네 전 잠깐...독서토론동아리를...ㅎㅎ..",
          "original_form": "네 전 잠깐...독서토론동아리를...ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023298.1.14",
          "form": "광고동아리는 어떤활동을하는거에요?",
          "original_form": "광고동아리는 어떤활동을하는거에요?",
          "speaker_id": "1",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023298.1.15",
          "form": "광고 스터디하고",
          "original_form": "광고 스터디하고",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100023298.1.16",
          "form": "같이 공모전도 나가고 그래영ㅎㅎㅎ",
          "original_form": "같이 공모전도 나가고 그래영ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100023298.1.17",
          "form": "혹시 관련전공이셨나요?",
          "original_form": "혹시 관련전공이셨나요?",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023298.1.18",
          "form": "알찬활동이었네요!",
          "original_form": "알찬활동이었네요!",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023298.1.19",
          "form": "아니요 과는 다른 과였어요!",
          "original_form": "아니요 과는 다른 과였어요!",
          "speaker_id": "2",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100023298.1.20",
          "form": "독서토론동아리는 재미있으셨나요?",
          "original_form": "독서토론동아리는 재미있으셨나요?",
          "speaker_id": "2",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100023298.1.21",
          "form": "..ㅜ아뇨 그래서 아주 잠깐 활동했었어요 아..여긴 내가있을곳이아니구나",
          "original_form": "..ㅜ아뇨 그래서 아주 잠깐 활동했었어요 아..여긴 내가있을곳이아니구나",
          "speaker_id": "1",
