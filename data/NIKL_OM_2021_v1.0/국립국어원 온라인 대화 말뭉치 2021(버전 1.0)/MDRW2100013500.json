      "utterance": [
        {
          "id": "MDRW2100013500.1.1",
          "form": "집안일은 해도해도 끝이 없어",
          "original_form": "집안일은 해도해도 끝이 없어",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013500.1.2",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013500.1.3",
          "form": "어제는 신랑이랑 싸웠어",
          "original_form": "어제는 신랑이랑 싸웠어",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013500.1.4",
          "form": "매우화남",
          "original_form": "매우화남",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013500.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013500.1.6",
          "form": "주말에 일하러간데",
          "original_form": "주말에 일하러간데",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013500.1.7",
          "form": "1박2일로",
          "original_form": "1박2일로",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013500.1.8",
          "form": "너무너무속상해",
          "original_form": "너무너무속상해",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013500.1.9",
          "form": "헐 또?",
          "original_form": "헐 또?",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013500.1.10",
          "form": "그럼 주말에 독박육아네",
          "original_form": "그럼 주말에 독박육아네",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013500.1.11",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013500.1.12",
          "form": "개짱나",
          "original_form": "개짱나",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013500.1.13",
          "form": "하지만난참겠어",
          "original_form": "하지만난참겠어",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013500.1.14",
          "form": "나도 매번 집안일때문에 일주일에 2~3번은 싸우는 듯",
          "original_form": "나도 매번 집안일때문에 일주일에 2~3번은 싸우는 듯",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013500.1.15",
          "form": "우리가훈이 화목 사랑 평화거든",
          "original_form": "우리가훈이 화목 사랑 평화거든",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013500.1.16",
          "form": "가훈도 있나 우린 그런것도 없음",
          "original_form": "가훈도 있나 우린 그런것도 없음",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013500.1.17",
          "form": "응 가훈이없으면 맨날싸울거같아서",
          "original_form": "응 가훈이없으면 맨날싸울거같아서",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013500.1.18",
          "form": "하지만 있어도 맨날싸운단다",
          "original_form": "하지만 있어도 맨날싸운단다",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013500.1.19",
          "form": "퇴근후에는 오빠가 육아 좀 도와줌??",
          "original_form": "퇴근후에는 오빠가 육아 좀 도와줌??",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013500.1.20",
          "form": "흠.....",
          "original_form": "흠.....",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013500.1.21",
          "form": "생각해보니 3개뿐이네?",
          "original_form": "생각해보니 3개뿐이네?",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013500.1.22",
          "form": "좀더 일을 추가시켜야겠다",
          "original_form": "좀더 일을 추가시켜야겠다",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013500.1.23",
          "form": "지금 여기 번개가 엄청침",
          "original_form": "지금 여기 번개가 엄청침",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013500.1.24",
          "form": "우리집에또 물새는건아닌가모르겠네",
          "original_form": "우리집에또 물새는건아닌가모르겠네",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.25",
          "form": "그래 넌 퇴근도 없는데 ㅋㅋ 좀더 일을 시켜",
          "original_form": "그래 넌 퇴근도 없는데 ㅋㅋ 좀더 일을 시켜",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.26",
          "form": "집안일추가될듯",
          "original_form": "집안일추가될듯",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.27",
          "form": "아직 수리 안됐어?",
          "original_form": "아직 수리 안됐어?",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.28",
          "form": "하.....어째서 아파트에 물이새는거지",
          "original_form": "하.....어째서 아파트에 물이새는거지",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.29",
          "form": "신랑이 신경 좀 안쓰나",
          "original_form": "신랑이 신경 좀 안쓰나",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.30",
          "form": "응 여기아파트 입대위들 그지시키들인지",
          "original_form": "응 여기아파트 입대위들 그지시키들인지",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.31",
          "form": "우린 오빠가 그런건 알아서 잘함",
          "original_form": "우린 오빠가 그런건 알아서 잘함",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.32",
          "form": "공짜겁내좋아해서",
          "original_form": "공짜겁내좋아해서",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013500.1.33",
          "form": "헐 작년 아니었나",
          "original_form": "헐 작년 아니었나",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013500.1.34",
          "form": "하자보수때 일단신청해보고 안되면 장기수선충당금쓴데",
          "original_form": "하자보수때 일단신청해보고 안되면 장기수선충당금쓴데",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013500.1.35",
          "form": "망할놈들",
          "original_form": "망할놈들",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013500.1.36",
          "form": "거기도 일처리 엄청 늦게 해주네",
          "original_form": "거기도 일처리 엄청 늦게 해주네",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013500.1.37",
          "form": "하여튼 일겁내못해요",
          "original_form": "하여튼 일겁내못해요",
          "speaker_id": "2",
          "time": "20210825 14:56"
        }
      ]
    }
  ]
