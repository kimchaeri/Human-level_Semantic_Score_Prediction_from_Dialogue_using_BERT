      "utterance": [
        {
          "id": "MDRW2100011070.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011070.1.2",
          "form": "보는 것은 좋아해요.",
          "original_form": "보는 것은 좋아해요.",
          "speaker_id": "2",
          "time": "20210824 01:25"
        },
        {
          "id": "MDRW2100011070.1.3",
          "form": "오 어떤 스포츠 좋아하시나요",
          "original_form": "오 어떤 스포츠 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 01:25"
        },
        {
          "id": "MDRW2100011070.1.4",
          "form": "야구 좋아해요.",
          "original_form": "야구 좋아해요.",
          "speaker_id": "2",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011070.1.5",
          "form": "국내야구 보시나요?",
          "original_form": "국내야구 보시나요?",
          "speaker_id": "1",
          "time": "20210824 01:26"
        },
        {
          "id": "MDRW2100011070.1.6",
          "form": "넵. 메이저리그는 남편이 보면 옆에서 보기도 해요.",
          "original_form": "넵. 메이저리그는 남편이 보면 옆에서 보기도 해요.",
          "speaker_id": "2",
          "time": "20210824 01:32"
        },
        {
          "id": "MDRW2100011070.1.7",
          "form": "어떤구단 응원하시나요ㅎㅎ",
          "original_form": "어떤구단 응원하시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:34"
        },
        {
          "id": "MDRW2100011070.1.8",
          "form": "저는 기아\r\n남편은 두산요.",
          "original_form": "저는 기아\r\n남편은 두산요.",
          "speaker_id": "2",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011070.1.9",
          "form": "직관도 자주 가보셨나요",
          "original_form": "직관도 자주 가보셨나요",
          "speaker_id": "1",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011070.1.10",
          "form": "네.코로나 이전에는 종종 다녔어요.",
          "original_form": "네.코로나 이전에는 종종 다녔어요.",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011070.1.11",
          "form": "참 코로나때문에ㅠㅠ 저도, 직관한번 가봤는데",
          "original_form": "참 코로나때문에ㅠㅠ 저도, 직관한번 가봤는데",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011070.1.12",
          "form": "진짜 재밋더라구요",
          "original_form": "진짜 재밋더라구요",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011070.1.13",
          "form": "매우 신나지요.\r\n응원소리에 먹거리에.ㅎ",
          "original_form": "매우 신나지요.\r\n응원소리에 먹거리에.ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:52"
        },
        {
          "id": "MDRW2100011070.1.14",
          "form": "맥주먹으면서 응원하니 진짜 좋더라구요ㅎㅎ",
          "original_form": "맥주먹으면서 응원하니 진짜 좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:54"
        }
      ]
