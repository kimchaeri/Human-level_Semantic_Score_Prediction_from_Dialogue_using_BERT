      "utterance": [
        {
          "id": "MDRW2100007993.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007993.1.2",
          "form": "코로나 전에 다녀왔던 여행지중 가장 인상깊은 곳이 있으셨나요?",
          "original_form": "코로나 전에 다녀왔던 여행지중 가장 인상깊은 곳이 있으셨나요?",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007993.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007993.1.4",
          "form": "저는 한라산 등산하고 왔었는데 그게 기억에 남네요 ㅠ",
          "original_form": "저는 한라산 등산하고 왔었는데 그게 기억에 남네요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007993.1.5",
          "form": "와 한라산 등산이요?",
          "original_form": "와 한라산 등산이요?",
          "speaker_id": "1",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007993.1.6",
          "form": "네 ㅜㅠ 지금도 제주는 가려면 갈 수 있긴 한데 좀 가기 그렇네요",
          "original_form": "네 ㅜㅠ 지금도 제주는 가려면 갈 수 있긴 한데 좀 가기 그렇네요",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007993.1.7",
          "form": "등산 좋아하는데 산 가면 마스크 많이들 안끼더라구요",
          "original_form": "등산 좋아하는데 산 가면 마스크 많이들 안끼더라구요",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100007993.1.8",
          "form": "아이고...ㅠㅠㅠ 마스크 안끼면 좀 그렇죠",
          "original_form": "아이고...ㅠㅠㅠ 마스크 안끼면 좀 그렇죠",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007993.1.9",
          "form": "아무리 야외라고해도 요새 델타는 옷깃만 스쳐도 퍼진다는데..ㅠㅠ",
          "original_form": "아무리 야외라고해도 요새 델타는 옷깃만 스쳐도 퍼진다는데..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100007993.1.10",
          "form": "그쵸! 너무 무서워요 동네여행이라도 좀 다니고싶네욬ㅋㅋ",
          "original_form": "그쵸! 너무 무서워요 동네여행이라도 좀 다니고싶네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007993.1.11",
          "form": "여행 가고싶으신 곳 있으세요?",
          "original_form": "여행 가고싶으신 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100007993.1.12",
          "form": "저는 해외여행 가고싶어요...ㅠㅠㅠ",
          "original_form": "저는 해외여행 가고싶어요...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007993.1.13",
          "form": "비행기타고 멀리 떠나고싶은데",
          "original_form": "비행기타고 멀리 떠나고싶은데",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007993.1.14",
          "form": "진짜.... 코로나 언제 끝날지...",
          "original_form": "진짜.... 코로나 언제 끝날지...",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100007993.1.15",
          "form": "공항이라도 진짜 가고싶어요",
          "original_form": "공항이라도 진짜 가고싶어요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007993.1.16",
          "form": "해외 어디가고싶으세요?",
          "original_form": "해외 어디가고싶으세요?",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007993.1.17",
          "form": "유럽쪽이요!!",
          "original_form": "유럽쪽이요!!",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007993.1.18",
          "form": "아니면 호주도 가보고싶어요",
          "original_form": "아니면 호주도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007993.1.19",
          "form": "한국이랑 다른 여유로운 느낌 느껴보고 싶고",
          "original_form": "한국이랑 다른 여유로운 느낌 느껴보고 싶고",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100007993.1.20",
          "form": "유적지들도 많이 보고싶고...ㅠㅠ",
          "original_form": "유적지들도 많이 보고싶고...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100007993.1.21",
          "form": "아 ㅜㅜ 맞아요 그 이국적인 느낌",
          "original_form": "아 ㅜㅜ 맞아요 그 이국적인 느낌",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007993.1.22",
          "form": "호주에서 액티비티 해보고싶어요!!",
          "original_form": "호주에서 액티비티 해보고싶어요!!",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007993.1.23",
          "form": "액티비티도 좋죠..!!",
          "original_form": "액티비티도 좋죠..!!",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100007993.1.24",
          "form": "나무님도 가고싶은 해외 여행지가 있으신가요?",
          "original_form": "나무님도 가고싶은 해외 여행지가 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100007993.1.25",
          "form": "저는 캄보디아 앙코르와트 너무 보고싶고",
          "original_form": "저는 캄보디아 앙코르와트 너무 보고싶고",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100007993.1.26",
          "form": "오로라도 보고싶고",
          "original_form": "오로라도 보고싶고",
          "speaker_id": "2",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100007993.1.27",
          "form": "어디든 나가고싶어요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "어디든 나가고싶어요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100007993.1.28",
          "form": "맞아요... 진짜 어디로든...ㅠㅠㅠ",
          "original_form": "맞아요... 진짜 어디로든...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007993.1.29",
          "form": "백신이라도 빨리 맞고싶네요..ㅠㅠ",
          "original_form": "백신이라도 빨리 맞고싶네요..ㅠㅠ",
