      "utterance": [
        {
          "id": "MDRW2100022996.1.1",
          "form": "우도환이랑 같이나옴ㅋㅋㅋ",
          "original_form": "우도환이랑 같이나옴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022996.1.2",
          "form": "우도환이 막 악마에게 예배드리는 사탄 숭배자? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아근데 징그러워 ㅋㅋ",
          "original_form": "우도환이 막 악마에게 예배드리는 사탄 숭배자? ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아근데 징그러워 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022996.1.3",
          "form": "우도환! ㅇㄹ아알아",
          "original_form": "우도환! ㅇㄹ아알아",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022996.1.4",
          "form": "징그러움ㅋㅋㅋㅋ 뱀비늘생김",
          "original_form": "징그러움ㅋㅋㅋㅋ 뱀비늘생김",
          "speaker_id": "1",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022996.1.5",
          "form": "아우 징그러ㅋㅋ 내공포영화는 사탄의인혀으로 끝났다ㅋㅋ",
          "original_form": "아우 징그러ㅋㅋ 내공포영화는 사탄의인혀으로 끝났다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022996.1.6",
          "form": "약간 그게 생각나는 장면도나옴ㅋㅋㅋㅋㅋㅋ",
          "original_form": "약간 그게 생각나는 장면도나옴ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022996.1.7",
          "form": "ㅋㅋㅋㅋ아싫어ㅋㅋ재밌어서 보는거야?!ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아싫어ㅋㅋ재밌어서 보는거야?!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100022996.1.8",
          "form": "ㅋㅋㅋㅋ 중간까지는 흥미로웠지만 후반부는 헐ㅎ 쟤 뭐여 ㅎ 이런느낌으로 봄 ㅋㅋㅋㅋㅋㅋ 고모네집에있었는데 고모가 공포밖에안봄ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 중간까지는 흥미로웠지만 후반부는 헐ㅎ 쟤 뭐여 ㅎ 이런느낌으로 봄 ㅋㅋㅋㅋㅋㅋ 고모네집에있었는데 고모가 공포밖에안봄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022996.1.9",
          "form": "앗 고모님ㅋㅋㅋㅋ공포물 좋아하시나봄",
          "original_form": "앗 고모님ㅋㅋㅋㅋ공포물 좋아하시나봄",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022996.1.10",
          "form": "매니아임 완전",
          "original_form": "매니아임 완전",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100022996.1.11",
          "form": "거기가맨 로맨스 이런거 볼생각은 꿈에도하지 말아야함",
          "original_form": "거기가맨 로맨스 이런거 볼생각은 꿈에도하지 말아야함",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100022996.1.12",
          "form": "난 손예진이랑 조인성 나오는 영화있자나 비오는날 셔츠로 비막고가는겈ㅋ",
          "original_form": "난 손예진이랑 조인성 나오는 영화있자나 비오는날 셔츠로 비막고가는겈ㅋ",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022996.1.13",
          "form": "그거랑 조승우 눈멀어서 손예진 만나는 영화랑 같은건줄 몰랐어",
          "original_form": "그거랑 조승우 눈멀어서 손예진 만나는 영화랑 같은건줄 몰랐어",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022996.1.14",
          "form": "그 뭐냐..클래식? ㅋㅋㅋ",
          "original_form": "그 뭐냐..클래식? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022996.1.15",
          "form": "과거가 조승우아님? ㅋㅋ",
          "original_form": "과거가 조승우아님? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100022996.1.16",
          "form": "아 조승우가 나중아니고?ㅋㅋㅋ",
          "original_form": "아 조승우가 나중아니고?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022996.1.17",
          "form": "아들이 조인성아님? 조승우가 까만 교복입고 이랬던 기억이 있는데",
          "original_form": "아들이 조인성아님? 조승우가 까만 교복입고 이랬던 기억이 있는데",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100022996.1.18",
          "form": "그래? 아ㅋㅋㅋㅋ그럼 둘이 낳은게 조인성인거야?",
          "original_form": "그래? 아ㅋㅋㅋㅋ그럼 둘이 낳은게 조인성인거야?",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022996.1.19",
          "form": "아닐걸 둘이 안되잖아 ㅋㅋㅋ",
          "original_form": "아닐걸 둘이 안되잖아 ㅋㅋㅋ",
