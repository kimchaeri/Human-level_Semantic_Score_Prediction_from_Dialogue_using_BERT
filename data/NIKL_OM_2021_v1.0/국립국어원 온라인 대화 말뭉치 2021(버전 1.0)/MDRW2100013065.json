      "utterance": [
        {
          "id": "MDRW2100013065.1.1",
          "form": "난 곰표맥주는 기대 이하였어",
          "original_form": "난 곰표맥주는 기대 이하였어",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013065.1.2",
          "form": "곰표가 밀가루였는데 요샌 다양해 맥주도있고",
          "original_form": "곰표가 밀가루였는데 요샌 다양해 맥주도있고",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013065.1.3",
          "form": "맞아. 근데 디자인은 참신하긴 해",
          "original_form": "맞아. 근데 디자인은 참신하긴 해",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013065.1.4",
          "form": "유미와 세포들 콜라보도 있고",
          "original_form": "유미와 세포들 콜라보도 있고",
          "speaker_id": "1",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100013065.1.5",
          "form": "맞아 확실히",
          "original_form": "맞아 확실히",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100013065.1.6",
          "form": "기억에 더 잘 남는듯",
          "original_form": "기억에 더 잘 남는듯",
          "speaker_id": "2",
          "time": "20210826 09:07"
        },
        {
          "id": "MDRW2100013065.1.7",
          "form": "응 한정판도 생겨서 더 의미도 있고",
          "original_form": "응 한정판도 생겨서 더 의미도 있고",
          "speaker_id": "1",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100013065.1.8",
          "form": "근데 오히려 실망이 될 수도 있을 듯",
          "original_form": "근데 오히려 실망이 될 수도 있을 듯",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100013065.1.9",
          "form": "그리고 너무 지나친 마케팅도 있다더라고",
          "original_form": "그리고 너무 지나친 마케팅도 있다더라고",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100013065.1.10",
          "form": "아 그렇구나 자기옷중에 자라 띠어리 콜라보도 있자나",
          "original_form": "아 그렇구나 자기옷중에 자라 띠어리 콜라보도 있자나",
          "speaker_id": "1",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100013065.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100013065.1.12",
          "form": "전 띠어리 보유자예요ㅋㅋ",
          "original_form": "전 띠어리 보유자예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:10"
        },
        {
          "id": "MDRW2100013065.1.13",
          "form": "ㅎㅎㅎ그래서 그 옷이 한번사고 그 뒤로 구하기 어려웠나봐",
          "original_form": "ㅎㅎㅎ그래서 그 옷이 한번사고 그 뒤로 구하기 어려웠나봐",
          "speaker_id": "1",
          "time": "20210826 10:11"
        },
        {
          "id": "MDRW2100013065.1.14",
          "form": "마케팅적으로 별로였나",
          "original_form": "마케팅적으로 별로였나",
          "speaker_id": "2",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100013065.1.15",
          "form": "아니 초반에ㅡ내가 잘 산듯",
          "original_form": "아니 초반에ㅡ내가 잘 산듯",
          "speaker_id": "1",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100013065.1.16",
          "form": "마케팅이 너무 먹는 쪽이야",
          "original_form": "마케팅이 너무 먹는 쪽이야",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100013065.1.17",
          "form": "지금도 자라나 에첸엠 콜라보한다고하면 미리 대기했다가 사더라고",
          "original_form": "지금도 자라나 에첸엠 콜라보한다고하면 미리 대기했다가 사더라고",
          "speaker_id": "1",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100013065.1.18",
          "form": "아 다른 곳도 있나",
          "original_form": "아 다른 곳도 있나",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100013065.1.19",
          "form": "많이들 콜라보하지",
          "original_form": "많이들 콜라보하지",
          "speaker_id": "1",
          "time": "20210826 10:17"
        }
      ]
