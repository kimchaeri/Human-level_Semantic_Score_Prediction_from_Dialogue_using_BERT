        {
          "id": "MDRW2100012512.1.2",
          "form": "안녕하세요~^^",
          "original_form": "안녕하세요~^^",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012512.1.3",
          "form": "결혼하셨어요??",
          "original_form": "결혼하셨어요??",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012512.1.4",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012512.1.5",
          "form": "결혼했어요",
          "original_form": "결혼했어요",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012512.1.6",
          "form": "아이둘입니다 ㅎㅎ",
          "original_form": "아이둘입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012512.1.7",
          "form": "저두요~딸 둘ㅎㅎ",
          "original_form": "저두요~딸 둘ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012512.1.8",
          "form": "아이들 몇 살인가요??",
          "original_form": "아이들 몇 살인가요??",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012512.1.9",
          "form": "아 넘 이뿌겠어요",
          "original_form": "아 넘 이뿌겠어요",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012512.1.10",
          "form": "전 ㅋㅋ커요",
          "original_form": "전 ㅋㅋ커요",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012512.1.11",
          "form": "아들 1 딸1",
          "original_form": "아들 1 딸1",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012512.1.12",
          "form": "고3 초6",
          "original_form": "고3 초6",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012512.1.13",
          "form": "이요",
          "original_form": "이요",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012512.1.14",
          "form": "몇살이예요 ?아이가",
          "original_form": "몇살이예요 ?아이가",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100012512.1.15",
          "form": "앗 전 5살 3살이요 ㅎㅎㅎ",
          "original_form": "앗 전 5살 3살이요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012512.1.16",
          "form": "지금도 안 자고 있어요 ㅠㅠㅠ",
          "original_form": "지금도 안 자고 있어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012512.1.17",
          "form": "어머 귀여워라",
          "original_form": "어머 귀여워라",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012512.1.18",
          "form": "저도요 ㅡㅡ",
          "original_form": "저도요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012512.1.19",
          "form": "자래도 안자요 학교 가는데",
          "original_form": "자래도 안자요 학교 가는데",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012512.1.20",
          "form": "참나 저나이에도 둘이 싸워요",
          "original_form": "참나 저나이에도 둘이 싸워요",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012512.1.21",
          "form": "그럼 어린이집은 가나요?",
          "original_form": "그럼 어린이집은 가나요?",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.22",
          "form": "저희는 전면 등교요",
          "original_form": "저희는 전면 등교요",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.23",
          "form": "앗 나이차이가 꽤 나는데도 싸우네요?",
          "original_form": "앗 나이차이가 꽤 나는데도 싸우네요?",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.24",
          "form": "어린이집은 가요~^^",
          "original_form": "어린이집은 가요~^^",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.25",
          "form": "아휴 싸우는데는 나이차이 없네요 ㅋㅌ",
          "original_form": "아휴 싸우는데는 나이차이 없네요 ㅋㅌ",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.26",
          "form": "아다행이네요 3살 5살",
          "original_form": "아다행이네요 3살 5살",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.27",
          "form": "안가면 아이도 엄마도 힘들어요",
          "original_form": "안가면 아이도 엄마도 힘들어요",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012512.1.28",
          "form": "오빠라서 그럴까요ㅎㅎ 누나면 안 싸울것 같은데;;",
          "original_form": "오빠라서 그럴까요ㅎㅎ 누나면 안 싸울것 같은데;;",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.29",
          "form": "맞아여 ㅠㅠ 심지어 9월 1일자로 복직이랍니다....",
          "original_form": "맞아여 ㅠㅠ 심지어 9월 1일자로 복직이랍니다....",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.30",
          "form": "그럴까요?동생 맨날 놀려요",
          "original_form": "그럴까요?동생 맨날 놀려요",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.31",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.32",
          "form": "넘 코앞이네요",
          "original_form": "넘 코앞이네요",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.33",
          "form": "원에 잘 적응 시켜야겠어요",
          "original_form": "원에 잘 적응 시켜야겠어요",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.34",
          "form": "넵..무서워요ㅋㅋ 일하고 육아 다 할 자신이 없어요",
          "original_form": "넵..무서워요ㅋㅋ 일하고 육아 다 할 자신이 없어요",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012512.1.35",
          "form": "걱정 많으시겠어요",
          "original_form": "걱정 많으시겠어요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.36",
          "form": "그래도 다행히 적응은 잘 되어있어서...ㅎㅎ",
          "original_form": "그래도 다행히 적응은 잘 되어있어서...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.37",
          "form": "상상도 안되네요",
          "original_form": "상상도 안되네요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.38",
          "form": "아이들이 안 아프기만 바랄뿐이에요 ㅠ",
          "original_form": "아이들이 안 아프기만 바랄뿐이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.39",
          "form": "진짜 아이가 어려서 제가다 걱정이네요",
          "original_form": "진짜 아이가 어려서 제가다 걱정이네요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.40",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.41",
          "form": "아프면 봐줄 사람이 없어서ㅠㅠ",
          "original_form": "아프면 봐줄 사람이 없어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.42",
          "form": "가을부터 맨날 아파요",
          "original_form": "가을부터 맨날 아파요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.43",
          "form": "아 큰일이네요",
          "original_form": "아 큰일이네요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.44",
          "form": "그럴때가 젤 힘들어요",
          "original_form": "그럴때가 젤 힘들어요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012512.1.45",
          "form": "으어.....ㅠㅠㅠ 안되면 퇴직하신 친정아빠라도...ㅋㅋㅋ",
          "original_form": "으어.....ㅠㅠㅠ 안되면 퇴직하신 친정아빠라도...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012512.1.46",
          "form": "한참 약할나이인데",
          "original_form": "한참 약할나이인데",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012512.1.47",
          "form": "아 네 할아버지도 잘봐주세요",
          "original_form": "아 네 할아버지도 잘봐주세요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012512.1.48",
          "form": "저희 아버지 봐주세요",
          "original_form": "저희 아버지 봐주세요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012512.1.49",
          "form": "네네 아이들도 할아버지 좋아해요ㅋㅋㅋ",
          "original_form": "네네 아이들도 할아버지 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012512.1.50",
          "form": "너무 좋아해서 아빠가 기 빨리셔요ㅋㅋㅋㅋ",
          "original_form": "너무 좋아해서 아빠가 기 빨리셔요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012512.1.51",
