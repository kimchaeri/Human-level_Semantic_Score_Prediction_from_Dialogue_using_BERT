      "utterance": [
        {
          "id": "MDRW2100023719.1.1",
          "form": "요즘 시국에 힘들겠지만 여행계획 있으세요?",
          "original_form": "요즘 시국에 힘들겠지만 여행계획 있으세요?",
          "speaker_id": "1",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100023719.1.2",
          "form": "전 2차 백신 맞으면 짧게 호캉스라도 하려구요",
          "original_form": "전 2차 백신 맞으면 짧게 호캉스라도 하려구요",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023719.1.3",
          "form": "저는 의료인이라서 진작 맞앗어요 ㅎㅎ",
          "original_form": "저는 의료인이라서 진작 맞앗어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100023719.1.4",
          "form": "호캉스 좋져ㅜㅜ",
          "original_form": "호캉스 좋져ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100023719.1.5",
          "form": "우왕~!!!",
          "original_form": "우왕~!!!",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023719.1.6",
          "form": "멋있어요, 근데 힘드시겠네요",
          "original_form": "멋있어요, 근데 힘드시겠네요",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023719.1.7",
          "form": "의료인이지만 여행 나름 야무지게댕겻어요",
          "original_form": "의료인이지만 여행 나름 야무지게댕겻어요",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023719.1.8",
          "form": "마스크열심히쓰고",
          "original_form": "마스크열심히쓰고",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023719.1.9",
          "form": "근무환경이좋아서 힘들지않아요ㅋㅋ",
          "original_form": "근무환경이좋아서 힘들지않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023719.1.10",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100023719.1.11",
          "form": "하긴 병원도 케바켖ㆍ",
          "original_form": "하긴 병원도 케바켖ㆍ",
          "speaker_id": "1",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100023719.1.12",
          "form": "케바케죠",
          "original_form": "케바케죠",
          "speaker_id": "1",
          "time": "20210830 15:58"
        },
        {
          "id": "MDRW2100023719.1.13",
          "form": "ㅋㅋ네 완전좋아요",
          "original_form": "ㅋㅋ네 완전좋아요",
          "speaker_id": "2",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100023719.1.14",
          "form": "화이자 맞으셧나요?",
          "original_form": "화이자 맞으셧나요?",
          "speaker_id": "2",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100023719.1.15",
          "form": "네... 저저번주에 맞았어요",
          "original_form": "네... 저저번주에 맞았어요",
          "speaker_id": "1",
          "time": "20210830 15:59"
        },
        {
          "id": "MDRW2100023719.1.16",
          "form": "아프진 않았어요. 팔이 뻐근하긴했지만요",
          "original_form": "아프진 않았어요. 팔이 뻐근하긴했지만요",
          "speaker_id": "1",
          "time": "20210830 16:00"
        },
        {
          "id": "MDRW2100023719.1.17",
          "form": "화이자는 2차가 아프니 담날까지도 푹 쉬세요ㅠㅠ",
          "original_form": "화이자는 2차가 아프니 담날까지도 푹 쉬세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100023719.1.18",
          "form": "마침 2차가 10월 1일이라 아예 월요일까지 푹 쉬려구요",
          "original_form": "마침 2차가 10월 1일이라 아예 월요일까지 푹 쉬려구요",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100023719.1.19",
          "form": "오오 타이밍 좋네욤",
          "original_form": "오오 타이밍 좋네욤",
          "speaker_id": "2",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100023719.1.20",
          "form": "주변에서 화이자 2차맞고 아프더라구요",
          "original_form": "주변에서 화이자 2차맞고 아프더라구요",
          "speaker_id": "2",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100023719.1.21",
          "form": "네 다들 그렇게 말하더라구요",
          "original_form": "네 다들 그렇게 말하더라구요",
          "speaker_id": "1",
          "time": "20210830 16:08"
        },
        {
          "id": "MDRW2100023719.1.22",
          "form": "일단 마음의 준비는 단단히 했어요",
          "original_form": "일단 마음의 준비는 단단히 했어요",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100023719.1.23",
          "form": "그리고 일단 맞고나면 마음은 편할테니까요",
          "original_form": "그리고 일단 맞고나면 마음은 편할테니까요",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100023719.1.24",
          "form": "ㅎㅎ기억남는여행지잇으신가요",
          "original_form": "ㅎㅎ기억남는여행지잇으신가요",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100023719.1.25",
          "form": "예전에 일본 오사카 혼자 간적 있어요",
          "original_form": "예전에 일본 오사카 혼자 간적 있어요",
          "speaker_id": "1",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100023719.1.26",
          "form": "해외여행 처음으로 혼자 간건데",
          "original_form": "해외여행 처음으로 혼자 간건데",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023719.1.27",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023719.1.28",
          "form": "원래 혼자 잘 노는 타입이기도 하고 ㅎㅎ",
          "original_form": "원래 혼자 잘 노는 타입이기도 하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023719.1.29",
          "form": "오 저는 혼자 암것도안하는 스타일이에요ㅋ",
          "original_form": "오 저는 혼자 암것도안하는 스타일이에요ㅋ",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023719.1.30",
          "form": "혼자선 오로지 집콕",
          "original_form": "혼자선 오로지 집콕",
          "speaker_id": "2",
          "time": "20210830 16:16"
        }
      ]
