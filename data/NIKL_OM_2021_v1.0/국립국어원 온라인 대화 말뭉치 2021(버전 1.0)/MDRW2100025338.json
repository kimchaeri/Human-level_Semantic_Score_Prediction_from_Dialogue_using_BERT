      "utterance": [
        {
          "id": "MDRW2100025338.1.1",
          "form": "안녕하세요 도서를 즐기시나요?",
          "original_form": "안녕하세요 도서를 즐기시나요?",
          "speaker_id": "1",
          "time": "20210901 09:37"
        },
        {
          "id": "MDRW2100025338.1.2",
          "form": "아주 가끔요 한달에 한번?",
          "original_form": "아주 가끔요 한달에 한번?",
          "speaker_id": "2",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100025338.1.3",
          "form": "무슨책을 주로 읽나요",
          "original_form": "무슨책을 주로 읽나요",
          "speaker_id": "1",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025338.1.4",
          "form": "심리학이나 대인관계 그런쪽이요",
          "original_form": "심리학이나 대인관계 그런쪽이요",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025338.1.5",
          "form": "헐 정말 유식하겠군요",
          "original_form": "헐 정말 유식하겠군요",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025338.1.6",
          "form": "저는 웹툰만화책 이런거 좋아해요",
          "original_form": "저는 웹툰만화책 이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025338.1.7",
          "form": "요즘 에세이에 빠지긴했어요",
          "original_form": "요즘 에세이에 빠지긴했어요",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025338.1.8",
          "form": "최근 읽은책 중 제일 재밌었던게 뭐에요?",
          "original_form": "최근 읽은책 중 제일 재밌었던게 뭐에요?",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025338.1.9",
          "form": "아빠는 몰라두돼",
          "original_form": "아빠는 몰라두돼",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025338.1.10",
          "form": "에세이책인데",
          "original_form": "에세이책인데",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025338.1.11",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025338.1.12",
          "form": "오 아니요",
          "original_form": "오 아니요",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025338.1.13",
          "form": "ㅋㅋㅋㅋ저는 그림있는책들이좋더라구요",
          "original_form": "ㅋㅋㅋㅋ저는 그림있는책들이좋더라구요",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025338.1.14",
          "form": "저런책들은 그림보는맛에 보는거 같아요!",
          "original_form": "저런책들은 그림보는맛에 보는거 같아요!",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025338.1.15",
          "form": "아 그림이 있군여",
          "original_form": "아 그림이 있군여",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.16",
          "form": "웹툰도 그래서 좋아하시는구나",
          "original_form": "웹툰도 그래서 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.17",
          "form": "네네~ 무슨책 읽으시나요?",
          "original_form": "네네~ 무슨책 읽으시나요?",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.18",
          "form": "저도 웹툰 좋아하는데",
          "original_form": "저도 웹툰 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.19",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.20",
          "form": "머보세요?",
          "original_form": "머보세요?",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.21",
          "form": "단행본으로는 본적이없군여..",
          "original_form": "단행본으로는 본적이없군여..",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.22",
          "form": "저도 폰으로 봐요 ㅎ",
          "original_form": "저도 폰으로 봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025338.1.23",
          "form": "제목이 좀 그런데 막 이상한 내용 아니에요...",
          "original_form": "제목이 좀 그런데 막 이상한 내용 아니에요...",
          "speaker_id": "2",
          "time": "20210901 10:29"
        },
        {
          "id": "MDRW2100025338.1.24",
          "form": "제목이먼가요?",
          "original_form": "제목이먼가요?",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100025338.1.25",
          "form": "이거요",
          "original_form": "이거요",
          "speaker_id": "2",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100025338.1.26",
          "form": "유우우우부우우우녀키이이일러어어어",
          "original_form": "유우우우부우우우녀키이이일러어어어",
          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100025338.1.27",
          "form": "다음웹툰이요...ㅋㅋㅋㅋ",
          "original_form": "다음웹툰이요...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100025338.1.28",
          "form": "킬러조직이 나오는 만화인데 재밌어요ㅠㅋㅋㅋ",
          "original_form": "킬러조직이 나오는 만화인데 재밌어요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025338.1.29",
          "form": "주인공이 기혼여성이라 제목이 저래요",
          "original_form": "주인공이 기혼여성이라 제목이 저래요",
          "speaker_id": "2",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025338.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ아하",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아하",
          "speaker_id": "1",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025338.1.31",
          "form": "들어본적있는거갘어요",
          "original_form": "들어본적있는거갘어요",
          "speaker_id": "1",
          "time": "20210901 10:32"
        },
        {
          "id": "MDRW2100025338.1.32",
          "form": "웹툰 뭐보세요? 추천해주세요",
          "original_form": "웹툰 뭐보세요? 추천해주세요",
          "speaker_id": "2",
          "time": "20210901 10:33"
        },
        {
          "id": "MDRW2100025338.1.33",
          "form": "저는  로판?이런거 읽어서..",
          "original_form": "저는  로판?이런거 읽어서..",
          "speaker_id": "1",
          "time": "20210901 10:34"
        },
        {
          "id": "MDRW2100025338.1.34",
          "form": "취향적인 그녀 이런 로맨스 좋아해요...ㅎ",
          "original_form": "취향적인 그녀 이런 로맨스 좋아해요...ㅎ",
          "speaker_id": "1",
          "time": "20210901 10:34"
        },
        {
          "id": "MDRW2100025338.1.35",
          "form": "방금 검색해봤는데 그림체 너무 귀엽네요",
          "original_form": "방금 검색해봤는데 그림체 너무 귀엽네요",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025338.1.36",
          "form": "그쵸?저 약간 아기자기 한거 봐요",
          "original_form": "그쵸?저 약간 아기자기 한거 봐요",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025338.1.37",
          "form": "다음웹튠이긴한데",
          "original_form": "다음웹튠이긴한데",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025338.1.38",
          "form": "아니면 나혼자만 레벨업?",
          "original_form": "아니면 나혼자만 레벨업?",
          "speaker_id": "1",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025338.1.39",
          "form": "그거 좀 재미있는거같아요",
          "original_form": "그거 좀 재미있는거같아요",
          "speaker_id": "1",
          "time": "20210901 10:36"
        },
        {
          "id": "MDRW2100025338.1.40",
          "form": "오 액션판타지?인가요",
          "original_form": "오 액션판타지?인가요",
          "speaker_id": "2",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100025338.1.41",
          "form": "현판인거같은데!",
          "original_form": "현판인거같은데!",
          "speaker_id": "1",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025338.1.42",
          "form": "볼만하더라구요",
          "original_form": "볼만하더라구요",
          "speaker_id": "1",
          "time": "20210901 10:38"
        },
        {
          "id": "MDRW2100025338.1.43",
          "form": "오 저 액션 좋아해요 ㅋㅋㅋ",
          "original_form": "오 저 액션 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:39"
        }
      ]
    }
  ]
