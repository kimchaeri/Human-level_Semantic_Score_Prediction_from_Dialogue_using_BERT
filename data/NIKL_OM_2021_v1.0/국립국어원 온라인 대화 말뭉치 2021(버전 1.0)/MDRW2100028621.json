      "utterance": [
        {
          "id": "MDRW2100028621.1.1",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028621.1.2",
          "form": "저 어렸을때",
          "original_form": "저 어렸을때",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028621.1.3",
          "form": "토끼키웠어요",
          "original_form": "토끼키웠어요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028621.1.4",
          "form": "와 이름이 뭐에요?",
          "original_form": "와 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028621.1.5",
          "form": "아니 이름은 없이 그냥 토끼야~하고불렀어요",
          "original_form": "아니 이름은 없이 그냥 토끼야~하고불렀어요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.6",
          "form": "3마리인가",
          "original_form": "3마리인가",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.7",
          "form": "키웠눈데",
          "original_form": "키웠눈데",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.8",
          "form": "세마리나",
          "original_form": "세마리나",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.9",
          "form": "채소주면",
          "original_form": "채소주면",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.10",
          "form": "막받아먹어요",
          "original_form": "막받아먹어요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.11",
          "form": "귀염둥이들",
          "original_form": "귀염둥이들",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.12",
          "form": "나잖아",
          "original_form": "나잖아",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.13",
          "form": "애들이 순해서",
          "original_form": "애들이 순해서",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.14",
          "form": "안아도",
          "original_form": "안아도",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.15",
          "form": "가만히있어줬어요",
          "original_form": "가만히있어줬어요",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.16",
          "form": "어릴적의나는 완전",
          "original_form": "어릴적의나는 완전",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.17",
          "form": "착하네요",
          "original_form": "착하네요",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.18",
          "form": "한주먹거리였을텐대",
          "original_form": "한주먹거리였을텐대",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.19",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.20",
          "form": "토끼가 주로",
          "original_form": "토끼가 주로",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028621.1.21",
          "form": "때리는 역할이죠",
          "original_form": "때리는 역할이죠",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.22",
          "form": "ㅇ아무래도그럴죠...",
          "original_form": "ㅇ아무래도그럴죠...",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.23",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.24",
          "form": "왜지금은",
          "original_form": "왜지금은",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.25",
          "form": "님이 맞는 역할이고",
          "original_form": "님이 맞는 역할이고",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.26",
          "form": "안키우는지",
          "original_form": "안키우는지",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.27",
          "form": "물어봐줄래요?",
          "original_form": "물어봐줄래요?",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.28",
          "form": "아니 이쉑",
          "original_form": "아니 이쉑",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.29",
          "form": "근데 지금은 왜 안키워요^^?",
          "original_form": "근데 지금은 왜 안키워요^^?",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.30",
          "form": "교훈줄게",
          "original_form": "교훈줄게",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.31",
          "form": "그게말이죠...",
          "original_form": "그게말이죠...",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.32",
          "form": "제가토끼를키울적에",
          "original_form": "제가토끼를키울적에",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.33",
          "form": "헌집다오",
          "original_form": "헌집다오",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.34",
          "form": "앞집에서",
          "original_form": "앞집에서",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028621.1.35",
          "form": "강아지를3마리",
          "original_form": "강아지를3마리",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.36",
          "form": "여우 키웟나요",
          "original_form": "여우 키웟나요",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.37",
          "form": "풀어서키웠다 말이에요...",
          "original_form": "풀어서키웠다 말이에요...",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.38",
          "form": "짱이다",
          "original_form": "짱이다",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.39",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.40",
          "form": "그 강아지들이",
          "original_form": "그 강아지들이",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.41",
          "form": "저희집 대문이 열렷을때를",
          "original_form": "저희집 대문이 열렷을때를",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.42",
          "form": "틈타서",
          "original_form": "틈타서",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.43",
          "form": "토끼집을부수고",
          "original_form": "토끼집을부수고",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.44",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.45",
          "form": "토끼를 죽였다는",
          "original_form": "토끼를 죽였다는",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.46",
          "form": "그런일이",
          "original_form": "그런일이",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.47",
          "form": "이.이",
          "original_form": "이.이",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.48",
          "form": "이게.",
          "original_form": "이게.",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.49",
          "form": "있덨다...이말이에요...",
          "original_form": "있덨다...이말이에요...",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.50",
          "form": "뭐노.",
          "original_form": "뭐노.",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.51",
          "form": "이.이게..",
          "original_form": "이.이게..",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028621.1.52",
          "form": "여기서 우리가 얻은교훈은",
          "original_form": "여기서 우리가 얻은교훈은",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.53",
          "form": "문을 잘닫고다니자.",
          "original_form": "문을 잘닫고다니자.",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.54",
          "form": "이거죠...",
          "original_form": "이거죠...",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.55",
          "form": "미친..",
          "original_form": "미친..",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.56",
          "form": "그런 슬픈 사연을가진",
          "original_form": "그런 슬픈 사연을가진",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.57",
          "form": "그런얘기죠...",
          "original_form": "그런얘기죠...",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.58",
          "form": "어롓을때",
          "original_form": "어롓을때",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.59",
          "form": "옆집에서",
          "original_form": "옆집에서",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.60",
          "form": "집채만한 강아지를",
          "original_form": "집채만한 강아지를",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028621.1.61",
          "form": "키웠었어요",
          "original_form": "키웠었어요",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028621.1.62",
          "form": "그 미국강아지인데",
          "original_form": "그 미국강아지인데",
          "speaker_id": "1",
          "time": "20210902 21:11"
        }
      ]
    }
