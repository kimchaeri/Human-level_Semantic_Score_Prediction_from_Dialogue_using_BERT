      "utterance": [
        {
          "id": "MDRW2100021490.1.1",
          "form": "그쵸 코로나 때문에 제약이 너무 많아여",
          "original_form": "그쵸 코로나 때문에 제약이 너무 많아여",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021490.1.2",
          "form": "여행취소도 여러번했어요",
          "original_form": "여행취소도 여러번했어요",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021490.1.3",
          "form": "속상하셨겠어요 ㅠㅠ",
          "original_form": "속상하셨겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021490.1.4",
          "form": "여행이 보통 활력소 아닌가요",
          "original_form": "여행이 보통 활력소 아닌가요",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100021490.1.5",
          "form": "맞아요. 그날을손꼽아 기다리면서 버티는거 있지않아요?",
          "original_form": "맞아요. 그날을손꼽아 기다리면서 버티는거 있지않아요?",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100021490.1.6",
          "form": "맞아요 그게 큰 낙이죠 날 정해놓으면 ㅜㅜ",
          "original_form": "맞아요 그게 큰 낙이죠 날 정해놓으면 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021490.1.7",
          "form": "진짜 코로나 괜찮아지면 해외휴양지 놀러가는게 꿈이에요",
          "original_form": "진짜 코로나 괜찮아지면 해외휴양지 놀러가는게 꿈이에요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021490.1.8",
          "form": "저는 휴양지로 여행 못가봤어요",
          "original_form": "저는 휴양지로 여행 못가봤어요",
          "speaker_id": "2",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021490.1.9",
          "form": "어디로 가고 싶으세요??",
          "original_form": "어디로 가고 싶으세요??",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100021490.1.10",
          "form": "글쎄요 태국이요",
          "original_form": "글쎄요 태국이요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021490.1.11",
          "form": "태국 많이 가더라구요",
          "original_form": "태국 많이 가더라구요",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021490.1.12",
          "form": "이제 여행은 너무 막연해진것같아요",
          "original_form": "이제 여행은 너무 막연해진것같아요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021490.1.13",
          "form": "맞아요 남해도 좋구요",
          "original_form": "맞아요 남해도 좋구요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021490.1.14",
          "form": "가고싶은데 멀어여",
          "original_form": "가고싶은데 멀어여",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021490.1.15",
          "form": "주로 강원도 가요",
          "original_form": "주로 강원도 가요",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021490.1.16",
          "form": "강원도 가본적 없는데 재밌을거같아요!",
          "original_form": "강원도 가본적 없는데 재밌을거같아요!",
          "speaker_id": "1",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021490.1.17",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "2",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021490.1.18",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021490.1.19",
          "form": "주로 어디로 가세요?",
          "original_form": "주로 어디로 가세요?",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021490.1.20",
          "form": "전 여행 자체를 잘 안가서요 집순이인지라 ㅜㅜ 그냥 국내로만요",
          "original_form": "전 여행 자체를 잘 안가서요 집순이인지라 ㅜㅜ 그냥 국내로만요",
          "speaker_id": "1",
          "time": "20210828 20:04"
        }
      ]
