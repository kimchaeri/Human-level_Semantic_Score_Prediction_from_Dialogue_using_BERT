        },
        {
          "id": "MDRW2100005655.1.2",
          "form": "싸이월드 세대가 아니신가요?",
          "original_form": "싸이월드 세대가 아니신가요?",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005655.1.3",
          "form": "싸이월드 세대입니당",
          "original_form": "싸이월드 세대입니당",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005655.1.4",
          "form": "이번에 싸이월드 아이디 확인하셨어요?",
          "original_form": "이번에 싸이월드 아이디 확인하셨어요?",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005655.1.5",
          "form": "저는 아이디가 없어졌더라구요",
          "original_form": "저는 아이디가 없어졌더라구요",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005655.1.6",
          "form": "엇 무조건 복구가 아니었나요",
          "original_form": "엇 무조건 복구가 아니었나요",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005655.1.7",
          "form": "확인 안 해봤는데",
          "original_form": "확인 안 해봤는데",
          "speaker_id": "2",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005655.1.8",
          "form": "제가 탈퇴를 한 건지 기억이 안 나요ㅎㅎ",
          "original_form": "제가 탈퇴를 한 건지 기억이 안 나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005655.1.9",
          "form": "나름 도토리랑 음악 신경 썼던 거 같은데",
          "original_form": "나름 도토리랑 음악 신경 썼던 거 같은데",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005655.1.10",
          "form": "들여다보면 옛날 일기장 보는 느낌일 거 같은데 왠지 보기 무섭기도 해요 ",
          "original_form": "들여다보면 옛날 일기장 보는 느낌일 거 같은데 왠지 보기 무섭기도 해요 ",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005655.1.11",
          "form": "도토리란 말도 엄청 오랜만이네요",
          "original_form": "도토리란 말도 엄청 오랜만이네요",
          "speaker_id": "2",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005655.1.12",
          "form": "맞아요 그때 흑역사가 엄청나죠ㅋㅋㅋㅋ",
          "original_form": "맞아요 그때 흑역사가 엄청나죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:40"
        },
        {
          "id": "MDRW2100005655.1.13",
          "form": "용돈 모아서 도토리 충전했었는데",
          "original_form": "용돈 모아서 도토리 충전했었는데",
          "speaker_id": "1",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005655.1.14",
          "form": "지금은 인스타가 더 재밌는 거 같아요!",
          "original_form": "지금은 인스타가 더 재밌는 거 같아요!",
          "speaker_id": "1",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005655.1.15",
          "form": "도토리 은근 비쌌던 거 같아요",
          "original_form": "도토리 은근 비쌌던 거 같아요",
          "speaker_id": "2",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005655.1.16",
          "form": "지금은 워낙 sns가 잘 돼 있어서ㅋㅋㅋㅋ",
          "original_form": "지금은 워낙 sns가 잘 돼 있어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005655.1.17",
          "form": "맞아요ㅋㅋㅋㅋ 다들 꾸미니까 저도 샀는데 지금 생각하면 좀 비싸네요ㅠㅠ",
          "original_form": "맞아요ㅋㅋㅋㅋ 다들 꾸미니까 저도 샀는데 지금 생각하면 좀 비싸네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005655.1.18",
          "form": "파도타기 였나? 그 친구의 친구 타고 넘어가는 것도 즐거웠죠~",
          "original_form": "파도타기 였나? 그 친구의 친구 타고 넘어가는 것도 즐거웠죠~",
          "speaker_id": "1",
          "time": "20210821 23:43"
        },
        {
          "id": "MDRW2100005655.1.19",
          "form": "파도타기 하고 구경하다가 싸우는 애들도 가끔 있었는데 그것도 추억이네요 ㅎㅎ",
          "original_form": "파도타기 하고 구경하다가 싸우는 애들도 가끔 있었는데 그것도 추억이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005655.1.20",
          "form": "옛날엔 어떻게 그리 부지런했나 싶지만ㅋㅋㅋㅋㅋ 진짜 별의 별 사람들이 다 있었죠ㅋㅋㅋㅋㅋ",
          "original_form": "옛날엔 어떻게 그리 부지런했나 싶지만ㅋㅋㅋㅋㅋ 진짜 별의 별 사람들이 다 있었죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005655.1.21",
          "form": "맞아요 지금은 그렇게 열심히 꾸미라고 해도 힘들 거 같아요 ㅎㅎ",
          "original_form": "맞아요 지금은 그렇게 열심히 꾸미라고 해도 힘들 거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005655.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그 특유의 싸이 감성 맞추겠다고 열심히 글 쓰고 그랬는데 일기도 꼬박 썼어요 저는",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그 특유의 싸이 감성 맞추겠다고 열심히 글 쓰고 그랬는데 일기도 꼬박 썼어요 저는",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005655.1.23",
          "form": "전 사진을 많이 올린 거 같아요",
          "original_form": "전 사진을 많이 올린 거 같아요",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005655.1.24",
          "form": "감성 사진....ㅎ...",
          "original_form": "감성 사진....ㅎ...",
          "speaker_id": "2",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005655.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ악 그시절의 내일이 없던 나...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ악 그시절의 내일이 없던 나...",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005655.1.26",
          "form": "많이 부끄럽네요 정말ㅠ",
          "original_form": "많이 부끄럽네요 정말ㅠ",
