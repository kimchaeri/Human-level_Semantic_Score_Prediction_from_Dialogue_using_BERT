      "utterance": [
        {
          "id": "MDRW2100041686.1.1",
          "form": "대나무칫솔에 이어",
          "original_form": "대나무칫솔에 이어",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041686.1.2",
          "form": "게임하는건없어?",
          "original_form": "게임하는건없어?",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041686.1.3",
          "form": "저스트댄스안해뜨ㅡㅂ은?",
          "original_form": "저스트댄스안해뜨ㅡㅂ은?",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041686.1.4",
          "form": "나 제페토 앱이랑 저스트 댄스",
          "original_form": "나 제페토 앱이랑 저스트 댄스",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041686.1.5",
          "form": "요즘은 잘 안해",
          "original_form": "요즘은 잘 안해",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041686.1.6",
          "form": "name1인 농장 키우기 겜 하지?",
          "original_form": "&name1&인 농장 키우기 겜 하지?",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041686.1.7",
          "form": "아 제페토ㅋㅋㅋㅋ",
          "original_form": "아 제페토ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041686.1.8",
          "form": "그래도열심히네ㅋㅋㅋ",
          "original_form": "그래도열심히네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041686.1.9",
          "form": "응응나는 농사짓지",
          "original_form": "응응나는 농사짓지",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041686.1.10",
          "form": "농사 마니 지었어?",
          "original_form": "농사 마니 지었어?",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041686.1.11",
          "form": "최근 수확한거 모야?",
          "original_form": "최근 수확한거 모야?",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100041686.1.12",
          "form": "최근엔 벼 찹쌀 흑미\r\nㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "최근엔 벼 찹쌀 흑미\r\nㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041686.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041686.1.14",
          "form": "잡곡전문이네",
          "original_form": "잡곡전문이네",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041686.1.15",
          "form": "우와 잡곡 농부구나",
          "original_form": "우와 잡곡 농부구나",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041686.1.16",
          "form": "게임 이름 뭐지? 타이니팜?",
          "original_form": "게임 이름 뭐지? 타이니팜?",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100041686.1.17",
          "form": "타이니팜ㅋㅋㅋ악",
          "original_form": "타이니팜ㅋㅋㅋ악",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041686.1.18",
          "form": "그것은 예전에미쳤던 게임",
          "original_form": "그것은 예전에미쳤던 게임",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041686.1.19",
          "form": "지금은 레알팜!",
          "original_form": "지금은 레알팜!",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041686.1.20",
          "form": "아아 레알팜 이구나",
          "original_form": "아아 레알팜 이구나",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041686.1.21",
          "form": "최근에 받은 농산물 므ㅜ야",
          "original_form": "최근에 받은 농산물 므ㅜ야",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041686.1.22",
          "form": "떡받았는데 엄마가맛없대",
          "original_form": "떡받았는데 엄마가맛없대",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041686.1.23",
          "form": "ㅋㅋㄱㅈㅋㅈㄱ",
          "original_form": "ㅋㅋㄱㅈㅋㅈㄱ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041686.1.24",
          "form": "앜ㅋㅋ 떡도 주니?",
          "original_form": "앜ㅋㅋ 떡도 주니?",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100041686.1.25",
          "form": "어ㅋㅋㄱㅋ랜덤박스신청햇는데 떡이왓어\r\n흑흑\r\n다음엔그냥과일시키려구..",
          "original_form": "어ㅋㅋㄱㅋ랜덤박스신청햇는데 떡이왓어\r\n흑흑\r\n다음엔그냥과일시키려구..",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041686.1.26",
          "form": "와 랜덤박스도 잇니 싱기하다",
          "original_form": "와 랜덤박스도 잇니 싱기하다",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041686.1.27",
          "form": "나도 레알팜 해봐여겠어 오래걸려?",
          "original_form": "나도 레알팜 해봐여겠어 오래걸려?",
          "speaker_id": "2",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100041686.1.28",
          "form": "응 좀 오래걸려ㅋㅋㄱ\r\n근데 한번 딱 시동걸리면뭔지알지",
          "original_form": "응 좀 오래걸려ㅋㅋㄱ\r\n근데 한번 딱 시동걸리면뭔지알지",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041686.1.29",
          "form": "ㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100041686.1.30",
          "form": "한번딱삘받으면",
          "original_form": "한번딱삘받으면",
          "speaker_id": "1",
          "time": "20210906 14:56"
        },
        {
          "id": "MDRW2100041686.1.31",
          "form": "상품다다다받을수있어용",
          "original_form": "상품다다다받을수있어용",
          "speaker_id": "1",
          "time": "20210906 14:56"
        }
      ]
    }
