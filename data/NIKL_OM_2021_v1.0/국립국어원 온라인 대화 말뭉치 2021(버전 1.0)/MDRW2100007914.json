      "utterance": [
        {
          "id": "MDRW2100007914.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210822 11:48"
        },
        {
          "id": "MDRW2100007914.1.2",
          "form": "지금은 무교인데 옛날에는 성당다녔어요",
          "original_form": "지금은 무교인데 옛날에는 성당다녔어요",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007914.1.3",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210822 11:49"
        },
        {
          "id": "MDRW2100007914.1.4",
          "form": "저는 없고 부모님은불교요",
          "original_form": "저는 없고 부모님은불교요",
          "speaker_id": "1",
          "time": "20210822 11:51"
        },
        {
          "id": "MDRW2100007914.1.5",
          "form": "절에 자주 가시나요?",
          "original_form": "절에 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210822 11:53"
        },
        {
          "id": "MDRW2100007914.1.6",
          "form": "아니요 한번도 안가봤는데",
          "original_form": "아니요 한번도 안가봤는데",
          "speaker_id": "1",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007914.1.7",
          "form": "종교에 대해 어떻게 생각하세요?",
          "original_form": "종교에 대해 어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007914.1.8",
          "form": "이번에 코로나때문에 교회이미지 나빠짐",
          "original_form": "이번에 코로나때문에 교회이미지 나빠짐",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007914.1.9",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 12:04"
        },
        {
          "id": "MDRW2100007914.1.10",
          "form": "집 근처에 교회 많나요?",
          "original_form": "집 근처에 교회 많나요?",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007914.1.11",
          "form": "네 ㅠ",
          "original_form": "네 ㅠ",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007914.1.12",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "1",
          "time": "20210822 12:16"
        },
        {
          "id": "MDRW2100007914.1.13",
          "form": "이번 코로나에 그쪽도 집회 같은 거 했나요?",
          "original_form": "이번 코로나에 그쪽도 집회 같은 거 했나요?",
          "speaker_id": "2",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007914.1.14",
          "form": "혹시 모르죠 안에서",
          "original_form": "혹시 모르죠 안에서",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007914.1.15",
          "form": "저희 근처에는 집회 때문에 경찰도 오고 그랬어요",
          "original_form": "저희 근처에는 집회 때문에 경찰도 오고 그랬어요",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007914.1.16",
          "form": "지킬건 지키고 해야되는데",
          "original_form": "지킬건 지키고 해야되는데",
          "speaker_id": "1",
          "time": "20210822 12:19"
        }
