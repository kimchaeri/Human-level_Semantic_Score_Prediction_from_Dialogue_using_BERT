      "utterance": [
        {
          "id": "MDRW2100021548.1.1",
          "form": "안녕하세용 저녁 드셨어요?",
          "original_form": "안녕하세용 저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021548.1.2",
          "form": "넵 점저했어요",
          "original_form": "넵 점저했어요",
          "speaker_id": "2",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021548.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100021548.1.4",
          "form": "저는 방금 먹었어요",
          "original_form": "저는 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100021548.1.5",
          "form": "전 빵치즈자두옥수수 이렇게... 밥이 아닌듯 배부른 애들로 허무하게ㅜㅋㅋ",
          "original_form": "전 빵치즈자두옥수수 이렇게... 밥이 아닌듯 배부른 애들로 허무하게ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100021548.1.6",
          "form": "저랑 비슷하시네요 ㅋㅋㅋㅋ",
          "original_form": "저랑 비슷하시네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100021548.1.7",
          "form": "저도 빵먹었어요",
          "original_form": "저도 빵먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100021548.1.8",
          "form": "ㅋ까망베르치즈",
          "original_form": "ㅋ까망베르치즈",
          "speaker_id": "2",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100021548.1.9",
          "form": "먹었어요 추천합니다",
          "original_form": "먹었어요 추천합니다",
          "speaker_id": "2",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100021548.1.10",
          "form": "까망베르치즈~ 넘 맛있져ㅋㅋㅋ",
          "original_form": "까망베르치즈~ 넘 맛있져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021548.1.11",
          "form": "저는 앙버터 먹었어요",
          "original_form": "저는 앙버터 먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021548.1.12",
          "form": "커피좋아하세요?",
          "original_form": "커피좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100021548.1.13",
          "form": "아 맛난거 드셨네요 아메 좋아요 약과나 마카롱 같이",
          "original_form": "아 맛난거 드셨네요 아메 좋아요 약과나 마카롱 같이",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021548.1.14",
          "form": "앗 달달한거랑 아메리카노랑 궁합 넘 잘맞아용ㅋㅋㅋㅋ",
          "original_form": "앗 달달한거랑 아메리카노랑 궁합 넘 잘맞아용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021548.1.15",
          "form": "저도 예전엔 쓴거 못마셔서 맨날 시럽 넣어먹고 그랬는데ㅋㅋㅋ",
          "original_form": "저도 예전엔 쓴거 못마셔서 맨날 시럽 넣어먹고 그랬는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100021548.1.16",
          "form": "ㅋㅋ나이드니까 쓴맛 너무 좋고요 전 신맛도 좋아해요",
          "original_form": "ㅋㅋ나이드니까 쓴맛 너무 좋고요 전 신맛도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021548.1.17",
          "form": "엇 신맛 좋아하면 젊은거라던데",
          "original_form": "엇 신맛 좋아하면 젊은거라던데",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021548.1.18",
          "form": "신체나이 젊으시겠어용ㅋㅋㅋ",
          "original_form": "신체나이 젊으시겠어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021548.1.19",
          "form": "ㅋㅋ악ㅋㅋ 그런건가요? 저번에 날밤새고 근무할때 커피의 진가를 느꼈어요 아! 이거구나 그전엔 디저트용이었는데요",
          "original_form": "ㅋㅋ악ㅋㅋ 그런건가요? 저번에 날밤새고 근무할때 커피의 진가를 느꼈어요 아! 이거구나 그전엔 디저트용이었는데요",
          "speaker_id": "2",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021548.1.20",
          "form": "오오 저두요ㅋㅋㅋㅋ",
          "original_form": "오오 저두요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021548.1.21",
          "form": "진짜 너무 피곤해서 믹스커피 손도 인대다가",
          "original_form": "진짜 너무 피곤해서 믹스커피 손도 인대다가",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021548.1.22",
          "form": "한포 타먹었는데",
          "original_form": "한포 타먹었는데",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021548.1.23",
          "form": "눈이 뜨이더라구요 ㅣㅋㅋㅋㅋㅋ",
          "original_form": "눈이 뜨이더라구요 ㅣㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100021548.1.24",
          "form": "진짜 현대인의 마약ㅋㅋ",
          "original_form": "진짜 현대인의 마약ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100021548.1.25",
          "form": "카페인은 괜찮으세요? 전 한잔이상은 삼가해요 ㅋㅋ",
          "original_form": "카페인은 괜찮으세요? 전 한잔이상은 삼가해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100021548.1.26",
          "form": "평소엔 두근거려서 안먹고",
          "original_form": "평소엔 두근거려서 안먹고",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100021548.1.27",
          "form": "극강으로 피곤한 날에만 마셔요ㅠㅠ",
          "original_form": "극강으로 피곤한 날에만 마셔요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 20:30"
        }
      ]
