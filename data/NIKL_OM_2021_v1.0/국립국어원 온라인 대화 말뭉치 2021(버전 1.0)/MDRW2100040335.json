      "utterance": [
        {
          "id": "MDRW2100040335.1.1",
          "form": "화제더라구요! 그거 보셨어요?",
          "original_form": "화제더라구요! 그거 보셨어요?",
          "speaker_id": "2",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040335.1.2",
          "form": "지금 보고있어요!!!!!!!",
          "original_form": "지금 보고있어요!!!!!!!",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.3",
          "form": "보면서 님 생각나서",
          "original_form": "보면서 님 생각나서",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.4",
          "form": "달려유ㅏㅆ잖아요!!!!",
          "original_form": "달려유ㅏㅆ잖아요!!!!",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.5",
          "form": "안잔인해요? ㅋㅋㅋㅋㅋ",
          "original_form": "안잔인해요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.6",
          "form": "재밌어보이긴하던데 !! ㅋㅋㅋ",
          "original_form": "재밌어보이긴하던데 !! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.7",
          "form": "초반부터 아...진짜 역겹다",
          "original_form": "초반부터 아...진짜 역겹다",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.8",
          "form": "남적남이",
          "original_form": "남적남이",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.9",
          "form": "아 그럼 힘든데..",
          "original_form": "아 그럼 힘든데..",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.10",
          "form": "저런거구나",
          "original_form": "저런거구나",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.11",
          "form": "했는데 잔인한건 아님!",
          "original_form": "했는데 잔인한건 아님!",
          "speaker_id": "1",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.12",
          "form": "되게 현실적이라던데요!",
          "original_form": "되게 현실적이라던데요!",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040335.1.13",
          "form": "욕설난무하고 그런 스타일인가요? ㅋㅋㅋ 당연할거같지만 ㅋㅋ",
          "original_form": "욕설난무하고 그런 스타일인가요? ㅋㅋㅋ 당연할거같지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040335.1.14",
          "form": "구교환배우가 목소리가 하이톤이고 그래서 무거움을 조금 덜해여ㅋㅋㅋㅋ",
          "original_form": "구교환배우가 목소리가 하이톤이고 그래서 무거움을 조금 덜해여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040335.1.15",
          "form": "욕은 아~~~~즈 리얼합니다",
          "original_form": "욕은 아~~~~즈 리얼합니다",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040335.1.16",
          "form": "1화 시작하자마자 정지했어요 와 쟤는 찐인데?",
          "original_form": "1화 시작하자마자 정지했어요 와 쟤는 찐인데?",
          "speaker_id": "1",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100040335.1.17",
          "form": "이러고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이러고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040335.1.18",
          "form": "사실 군대 노잼이라 안보려다가",
          "original_form": "사실 군대 노잼이라 안보려다가",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040335.1.19",
          "form": "개그맨 윤형빈이 dp출신인데 유튜브에 후기남긴거보고 볼까말까하다가",
          "original_form": "개그맨 윤형빈이 dp출신인데 유튜브에 후기남긴거보고 볼까말까하다가",
          "speaker_id": "1",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100040335.1.20",
          "form": "손석구 나오길래 보고있거든요!!",
          "original_form": "손석구 나오길래 보고있거든요!!",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040335.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ 윤형빈후기가 어떤데요? ㅋㅋㅋ리얼하데요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 윤형빈후기가 어떤데요? ㅋㅋㅋ리얼하데요??",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040335.1.22",
          "form": "아 오늘 시간되면 봐야겠어요 ㅋㅋㅋ궁금해졌어요 ㅋㅋ",
          "original_form": "아 오늘 시간되면 봐야겠어요 ㅋㅋㅋ궁금해졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040335.1.23",
          "form": "고증 진짜 잘된거래요",
          "original_form": "고증 진짜 잘된거래요",
          "speaker_id": "1",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100040335.1.24",
          "form": "자기때 있던일도 비슷하고",
          "original_form": "자기때 있던일도 비슷하고",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100040335.1.25",
          "form": "시간나면 윤형빈영상도 봐보세요!ㅋㅋㅋ",
          "original_form": "시간나면 윤형빈영상도 봐보세요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100040335.1.26",
          "form": "시점도 그렇고 완전최근군대는 아니고 그당시~몇년전 내용일거예요",
          "original_form": "시점도 그렇고 완전최근군대는 아니고 그당시~몇년전 내용일거예요",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100040335.1.27",
          "form": "몇편짜리에요? 이번에 나오는걸로 완결인걸까요?",
          "original_form": "몇편짜리에요? 이번에 나오는걸로 완결인걸까요?",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100040335.1.28",
          "form": "진심 손석구 캐릭터들 다 좋았는데 디피에서도 좋아요!ㅋㅋㅋㅋ꼭보세여!!",
          "original_form": "진심 손석구 캐릭터들 다 좋았는데 디피에서도 좋아요!ㅋㅋㅋㅋ꼭보세여!!",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100040335.1.29",
          "form": "6에피고",
          "original_form": "6에피고",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040335.1.30",
          "form": "시즌2 작가가 작업중이라고만 봤어요 제작확정은 모르겠고",
          "original_form": "시즌2 작가가 작업중이라고만 봤어요 제작확정은 모르겠고",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040335.1.31",
          "form": "저지금 5화보는중이라ㅎㅎㅎ",
          "original_form": "저지금 5화보는중이라ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040335.1.32",
          "form": "앗 벌써요?!! ㅋㅋㅋㅋ",
          "original_form": "앗 벌써요?!! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100040335.1.33",
          "form": "손석구 많이는 안나와요ㅠ 대신 구교환대박웃기니까ㅋㅋㅋ",
