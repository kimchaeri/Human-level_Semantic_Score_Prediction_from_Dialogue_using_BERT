      "utterance": [
        {
          "id": "MDRW2100026178.1.1",
          "form": "우당보다는",
          "original_form": "우당보다는",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026178.1.2",
          "form": "제주학생문화원가세요",
          "original_form": "제주학생문화원가세요",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026178.1.3",
          "form": "도서관 중 가깝습니다",
          "original_form": "도서관 중 가깝습니다",
          "speaker_id": "2",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026178.1.4",
          "form": "오 제인오스틴도 나이가 들면서 새롭게 읽히는 명작이죠",
          "original_form": "오 제인오스틴도 나이가 들면서 새롭게 읽히는 명작이죠",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026178.1.5",
          "form": "영화도 꽤 재미있었던 듯 키이라나이틀리 나온거요",
          "original_form": "영화도 꽤 재미있었던 듯 키이라나이틀리 나온거요",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026178.1.6",
          "form": "마크다아시~~",
          "original_form": "마크다아시~~",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026178.1.7",
          "form": "네. 옷이며 풍경이며 볼게 많아요",
          "original_form": "네. 옷이며 풍경이며 볼게 많아요",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026178.1.8",
          "form": "멋진 다아시ㅎ",
          "original_form": "멋진 다아시ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026178.1.9",
          "form": "책읽은거좋아하시는분이시군여",
          "original_form": "책읽은거좋아하시는분이시군여",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026178.1.10",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026178.1.11",
          "form": "네 독서는 제 낙이에요^^",
          "original_form": "네 독서는 제 낙이에요^^",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.12",
          "form": "김영하 작가님은 어떠신가요",
          "original_form": "김영하 작가님은 어떠신가요",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.13",
          "form": "음 두사람이랑",
          "original_form": "음 두사람이랑",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.14",
          "form": "살인자의추억법 봤는데",
          "original_form": "살인자의추억법 봤는데",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.15",
          "form": "검은꽃은 보다말고",
          "original_form": "검은꽃은 보다말고",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.16",
          "form": "책보다는 작가님이 더 매력있죠",
          "original_form": "책보다는 작가님이 더 매력있죠",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.17",
          "form": "소설은 좀 어두워서 제 취향은 아니에요.",
          "original_form": "소설은 좀 어두워서 제 취향은 아니에요.",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.18",
          "form": "목소리 좋으시구",
          "original_form": "목소리 좋으시구",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100026178.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.20",
          "form": "그 유럽 여행하신그 책",
          "original_form": "그 유럽 여행하신그 책",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.21",
          "form": "에세이는 좋았어요",
          "original_form": "에세이는 좋았어요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.22",
          "form": "네 저도 봤어요",
          "original_form": "네 저도 봤어요",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.23",
          "form": "사진이 한장인가 있는데",
          "original_form": "사진이 한장인가 있는데",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.24",
          "form": "네^^",
          "original_form": "네^^",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.25",
          "form": "참 양귀자 진짜좋아해요",
          "original_form": "참 양귀자 진짜좋아해요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.26",
          "form": "'모순'보다는",
          "original_form": "'모순'보다는",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.27",
          "form": "오 대표작인 어떤게 있을까요",
          "original_form": "오 대표작인 어떤게 있을까요",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.28",
          "form": "'희망'을 더 좋아해요",
          "original_form": "'희망'을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026178.1.29",
          "form": "천년의 사랑도 좋고",
          "original_form": "천년의 사랑도 좋고",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026178.1.30",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026178.1.31",
          "form": "모순도 많은이들의 인생작이에요",
          "original_form": "모순도 많은이들의 인생작이에요",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026178.1.32",
          "form": "저는 희망  강추추",
          "original_form": "저는 희망  강추추",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026178.1.33",
          "form": "직접 글은 안하세요?",
          "original_form": "직접 글은 안하세요?",
          "speaker_id": "1",
          "time": "20210901 20:49"
        }
      ]
