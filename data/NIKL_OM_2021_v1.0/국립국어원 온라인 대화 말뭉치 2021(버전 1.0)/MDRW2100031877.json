      "utterance": [
        {
          "id": "MDRW2100031877.1.1",
          "form": "여행계획 있으신가요??",
          "original_form": "여행계획 있으신가요??",
          "speaker_id": "2",
          "time": "20210905 06:56"
        },
        {
          "id": "MDRW2100031877.1.2",
          "form": "내 제주도 가기로 했습니다",
          "original_form": "내 제주도 가기로 했습니다",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100031877.1.3",
          "form": "배 타고 갈려구요",
          "original_form": "배 타고 갈려구요",
          "speaker_id": "1",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100031877.1.4",
          "form": "제주도 갑시다 ㅋㅋㅋㅋ",
          "original_form": "제주도 갑시다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100031877.1.5",
          "form": "멀미 안합니까??",
          "original_form": "멀미 안합니까??",
          "speaker_id": "2",
          "time": "20210905 06:57"
        },
        {
          "id": "MDRW2100031877.1.6",
          "form": "뭘 하지요",
          "original_form": "뭘 하지요",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100031877.1.7",
          "form": "멀미하지요",
          "original_form": "멀미하지요",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100031877.1.8",
          "form": "여행 갔는데다 일찍 일어나셨나 보네요",
          "original_form": "여행 갔는데다 일찍 일어나셨나 보네요",
          "speaker_id": "1",
          "time": "20210905 06:58"
        },
        {
          "id": "MDRW2100031877.1.9",
          "form": "아들이 6시 반에 일어났습니다",
          "original_form": "아들이 6시 반에 일어났습니다",
          "speaker_id": "1",
          "time": "20210905 06:59"
        },
        {
          "id": "MDRW2100031877.1.10",
          "form": "여행은 즐거워",
          "original_form": "여행은 즐거워",
          "speaker_id": "1",
          "time": "20210905 06:59"
        },
        {
          "id": "MDRW2100031877.1.11",
          "form": "여행은  기대가 됩니다",
          "original_form": "여행은  기대가 됩니다",
          "speaker_id": "1",
          "time": "20210905 06:59"
        },
        {
          "id": "MDRW2100031877.1.12",
          "form": "제주 여행",
          "original_form": "제주 여행",
          "speaker_id": "1",
          "time": "20210905 06:59"
        },
        {
          "id": "MDRW2100031877.1.13",
          "form": "함께 하시겠습니까",
          "original_form": "함께 하시겠습니까",
          "speaker_id": "1",
          "time": "20210905 07:00"
        },
        {
          "id": "MDRW2100031877.1.14",
          "form": "둘째 일 났나요",
          "original_form": "둘째 일 났나요",
          "speaker_id": "1",
          "time": "20210905 07:00"
        },
        {
          "id": "MDRW2100031877.1.15",
          "form": "둘찌는 늘 일어나있요 ㅋㅋㅋㅋ",
          "original_form": "둘찌는 늘 일어나있요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031877.1.16",
          "form": "날씨가 점점 추워지면",
          "original_form": "날씨가 점점 추워지면",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031877.1.17",
          "form": "여행 가서도 일찍 일어나나요",
          "original_form": "여행 가서도 일찍 일어나나요",
          "speaker_id": "1",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031877.1.18",
          "form": "더 일찍 일어나는것 같습니다;;",
          "original_form": "더 일찍 일어나는것 같습니다;;",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031877.1.19",
          "form": "역시 겨울에 아이",
          "original_form": "역시 겨울에 아이",
          "speaker_id": "1",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031877.1.20",
          "form": "휴가갔을때는..5시에 일어났습니다..ㅋㅋㅋㅋ",
          "original_form": "휴가갔을때는..5시에 일어났습니다..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:02"
        },
        {
          "id": "MDRW2100031877.1.21",
          "form": "겨울이 무섭네요;;",
          "original_form": "겨울이 무섭네요;;",
          "speaker_id": "2",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100031877.1.22",
          "form": "아우 무서운 겨울",
          "original_form": "아우 무서운 겨울",
          "speaker_id": "1",
          "time": "20210905 07:03"
        },
        {
          "id": "MDRW2100031877.1.23",
          "form": "제작년 겨울 새벽마다 자전거 태워서 현풍천 돌아 다녔던 기억이ㅜ납니다...",
          "original_form": "제작년 겨울 새벽마다 자전거 태워서 현풍천 돌아 다녔던 기억이ㅜ납니다...",
          "speaker_id": "2",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100031877.1.24",
          "form": "이젠 안그러길 ㅋㅋㅋㅋ",
          "original_form": "이젠 안그러길 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:04"
        },
        {
          "id": "MDRW2100031877.1.25",
          "form": "상전님은 잘 놀고 계십니꺼??ㅋㅋㅋ",
          "original_form": "상전님은 잘 놀고 계십니꺼??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:05"
        },
        {
          "id": "MDRW2100031877.1.26",
          "form": "할머니 품에 안겨 있어요",
          "original_form": "할머니 품에 안겨 있어요",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031877.1.27",
          "form": "일어나자마자 뽀로로 틀어달라고",
          "original_form": "일어나자마자 뽀로로 틀어달라고",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031877.1.28",
          "form": "난리치고",
          "original_form": "난리치고",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031877.1.29",
          "form": "할아버지가 용인 해주셔가지고 틀어줬습니다",
          "original_form": "할아버지가 용인 해주셔가지고 틀어줬습니다",
          "speaker_id": "1",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100031877.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ뽀로로는 못참죠~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ뽀로로는 못참죠~",
          "speaker_id": "2",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100031877.1.31",
          "form": "아침뽀로로는 국룰ㅋㅋㅋㅋ",
          "original_form": "아침뽀로로는 국룰ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100031877.1.32",
          "form": "너무 자주 본다",
          "original_form": "너무 자주 본다",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031877.1.33",
          "form": "뽀로로 와의 여행",
          "original_form": "뽀로로 와의 여행",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031877.1.34",
          "form": "뽀로로와 함께 여행을 떠나요",
          "original_form": "뽀로로와 함께 여행을 떠나요",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031877.1.35",
          "form": "하는 마음으로 또 여행을 떠나요",
          "original_form": "하는 마음으로 또 여행을 떠나요",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031877.1.36",
          "form": "여행떠나요~즐거운 마음으로~~",
          "original_form": "여행떠나요~즐거운 마음으로~~",
          "speaker_id": "2",
          "time": "20210905 07:51"
        }
      ]
