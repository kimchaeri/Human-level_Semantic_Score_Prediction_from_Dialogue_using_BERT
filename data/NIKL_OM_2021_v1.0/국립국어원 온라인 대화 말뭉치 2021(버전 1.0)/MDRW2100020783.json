      "utterance": [
        {
          "id": "MDRW2100020783.1.1",
          "form": "저 세번째 만남에서 사귀기로 했어요!!",
          "original_form": "저 세번째 만남에서 사귀기로 했어요!!",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020783.1.2",
          "form": "보통 이정도인가봐요",
          "original_form": "보통 이정도인가봐요",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020783.1.3",
          "form": "저 네번째 사귀자고 하려고했거든요",
          "original_form": "저 네번째 사귀자고 하려고했거든요",
          "speaker_id": "1",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020783.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020783.1.5",
          "form": "근데 여친이 그때",
          "original_form": "근데 여친이 그때",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.6",
          "form": "왜 사귀자고 안하냐고",
          "original_form": "왜 사귀자고 안하냐고",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.7",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.8",
          "form": "먼저 얘기해버려서",
          "original_form": "먼저 얘기해버려서",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.9",
          "form": "궁금하더라구요",
          "original_form": "궁금하더라구요",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.11",
          "form": "왜 네번째까지 보려고 했어요",
          "original_form": "왜 네번째까지 보려고 했어요",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020783.1.12",
          "form": "음... 이유없이 그냥 한달채우기...",
          "original_form": "음... 이유없이 그냥 한달채우기...",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020783.1.13",
          "form": "아 좀 더 길게 보시려고 ㅋㅋㅋ",
          "original_form": "아 좀 더 길게 보시려고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020783.1.14",
          "form": "한달은 봐야하는거 아닌가 싶어서 ㅋㅋㅋㅋ",
          "original_form": "한달은 봐야하는거 아닌가 싶어서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020783.1.15",
          "form": "전 썸일때랑 남친일때랑 또 다를 거 같아서",
          "original_form": "전 썸일때랑 남친일때랑 또 다를 거 같아서",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020783.1.16",
          "form": "썸이 길어지는 건 별로에요ㅋㅋㅋㅋ",
          "original_form": "썸이 길어지는 건 별로에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020783.1.17",
          "form": "그래서 3번째만남에 사귀자 안하면 저도 제가 하려고 했어요",
          "original_form": "그래서 3번째만남에 사귀자 안하면 저도 제가 하려고 했어요",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020783.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020783.1.19",
          "form": "ㅋㅋㅋㅋㅋ 여친이랑 같네요",
          "original_form": "ㅋㅋㅋㅋㅋ 여친이랑 같네요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020783.1.20",
          "form": "길어져야 일주일인데도요?",
          "original_form": "길어져야 일주일인데도요?",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020783.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020783.1.22",
          "form": "네 일주일 더 썸타야되잖아요",
          "original_form": "네 일주일 더 썸타야되잖아요",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020783.1.23",
          "form": "애타요",
          "original_form": "애타요",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020783.1.24",
          "form": "뭔가 표현을 더 절제해야되는",
          "original_form": "뭔가 표현을 더 절제해야되는",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020783.1.25",
          "form": "ㅋㅋㅋㅋㅋ 진짜 좋았군요?",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 좋았군요?",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020783.1.26",
          "form": "성격이 좀 급하신편?",
          "original_form": "성격이 좀 급하신편?",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020783.1.27",
          "form": "네 성격이 급해요 ㅋㅋㅋㅋ",
          "original_form": "네 성격이 급해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020783.1.28",
          "form": "4번까지 보실 생각이었으면 어차피 맘에 드는 사람이란 소리 아니에요?",
          "original_form": "4번까지 보실 생각이었으면 어차피 맘에 드는 사람이란 소리 아니에요?",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020783.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020783.1.30",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020783.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020783.1.32",
          "form": "팩트라 반박불가",
          "original_form": "팩트라 반박불가",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020783.1.33",
          "form": "근데 제가 3번째 소개팅때",
          "original_form": "근데 제가 3번째 소개팅때",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020783.1.34",
          "form": "세번 정도 만나고 세번째에 그냥 각자 좋은 인연 만나자고 헤어져서",
          "original_form": "세번 정도 만나고 세번째에 그냥 각자 좋은 인연 만나자고 헤어져서",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020783.1.35",
          "form": "좀 트라우마 있었어요 ㅋㅋㅋㅋ",
          "original_form": "좀 트라우마 있었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020783.1.36",
          "form": "이번에도 삼프터쯤에 그만 두기로 하면 어쩌지 하면서",
          "original_form": "이번에도 삼프터쯤에 그만 두기로 하면 어쩌지 하면서",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020783.1.37",
          "form": "계속 만나도 느낌이 잘 안오는?",
          "original_form": "계속 만나도 느낌이 잘 안오는?",
          "speaker_id": "1",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100020783.1.38",
          "form": "그런사람?",
          "original_form": "그런사람?",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020783.1.39",
          "form": "네네 나름 잘 맞는 것 같으면서도 대화가 잘 안 이어지더라구요",
          "original_form": "네네 나름 잘 맞는 것 같으면서도 대화가 잘 안 이어지더라구요",
          "speaker_id": "2",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020783.1.40",
          "form": "뭔가 외모는 취향인데 대화나 생활패턴이 너무 다른",
          "original_form": "뭔가 외모는 취향인데 대화나 생활패턴이 너무 다른",
          "speaker_id": "2",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020783.1.41",
          "form": "그럼 안되요",
          "original_form": "그럼 안되요",
          "speaker_id": "1",
          "time": "20210828 12:49"
        },
        {
          "id": "MDRW2100020783.1.42",
          "form": "지속할수 없는사람",
          "original_form": "지속할수 없는사람",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100020783.1.43",
          "form": "네ㅋㅋㅋ 잘 마무리했져",
          "original_form": "네ㅋㅋㅋ 잘 마무리했져",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020783.1.44",
          "form": "근데 분위기는 쭉 좋긴했는데 그만둔거라 그때처럼 되어버릴까봐 ㅋㅋㅋ",
          "original_form": "근데 분위기는 쭉 좋긴했는데 그만둔거라 그때처럼 되어버릴까봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020783.1.45",
          "form": "암튼 그때 잘 안되어서 이번에 남친 만난거라 넘 좋아요",
          "original_form": "암튼 그때 잘 안되어서 이번에 남친 만난거라 넘 좋아요",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020783.1.46",
          "form": "축하드려요",
          "original_form": "축하드려요",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020783.1.47",
          "form": "결혼까지 골인하세요",
          "original_form": "결혼까지 골인하세요",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020783.1.48",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100020783.1.49",
          "form": "ㅋㅋㅋ한 얼마 정도 만났을 때부터 결혼 생각 했어요?",
          "original_form": "ㅋㅋㅋ한 얼마 정도 만났을 때부터 결혼 생각 했어요?",
          "speaker_id": "2",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020783.1.50",
          "form": "반년정도요",
          "original_form": "반년정도요",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020783.1.51",
          "form": "이사람이면 결혼해서 살아도 되겠다",
          "original_form": "이사람이면 결혼해서 살아도 되겠다",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020783.1.52",
          "form": "결혼생각까지는 좀 신중하게",
          "original_form": "결혼생각까지는 좀 신중하게",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020783.1.53",
          "form": "생각하게되더라구요",
          "original_form": "생각하게되더라구요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020783.1.54",
          "form": "오 그래도 반년이면 빨리 생각하셨네요ㅋㅋㅋ",
          "original_form": "오 그래도 반년이면 빨리 생각하셨네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020783.1.55",
          "form": "빠른건가요?",
          "original_form": "빠른건가요?",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020783.1.56",
          "form": "나이가 있다보니...",
          "original_form": "나이가 있다보니...",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020783.1.57",
          "form": "아 그러시구너 여친분과 나이차가 얼마정도에요",
          "original_form": "아 그러시구너 여친분과 나이차가 얼마정도에요",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020783.1.58",
          "form": "4살이요~",
          "original_form": "4살이요~",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020783.1.59",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020783.1.60",
          "form": "저는 두살 차이나요",
          "original_form": "저는 두살 차이나요",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020783.1.61",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020783.1.62",
          "form": "4살이내가 좋은듯요",
