        },
        {
          "id": "MDRW2100035786.1.2",
          "form": "너는 기념일 챙기는 편이야?",
          "original_form": "너는 기념일 챙기는 편이야?",
          "speaker_id": "1",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.3",
          "form": "주로?",
          "original_form": "주로?",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.4",
          "form": "발렌타인데이도?",
          "original_form": "발렌타인데이도?",
          "speaker_id": "1",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.5",
          "form": "그건 잘 안챙겨",
          "original_form": "그건 잘 안챙겨",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.6",
          "form": "백일은.",
          "original_form": "백일은.",
          "speaker_id": "1",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.7",
          "form": "그건 꼭챙기지",
          "original_form": "그건 꼭챙기지",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.8",
          "form": "나는 200일 300일은 안챙겨",
          "original_form": "나는 200일 300일은 안챙겨",
          "speaker_id": "1",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.9",
          "form": "그건 나도 그래",
          "original_form": "그건 나도 그래",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.10",
          "form": "그냥 1주년 2주년 이런게 좋아",
          "original_form": "그냥 1주년 2주년 이런게 좋아",
          "speaker_id": "1",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100035786.1.11",
          "form": "기념일은 챙기는게 손해는아니야",
          "original_form": "기념일은 챙기는게 손해는아니야",
          "speaker_id": "2",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100035786.1.12",
          "form": "연애의 행복중 하나지",
          "original_form": "연애의 행복중 하나지",
          "speaker_id": "1",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100035786.1.13",
          "form": "맞아 애틋해지기도하고",
          "original_form": "맞아 애틋해지기도하고",
