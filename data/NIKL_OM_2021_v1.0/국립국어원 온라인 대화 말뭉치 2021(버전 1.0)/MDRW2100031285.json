      "utterance": [
        {
          "id": "MDRW2100031285.1.1",
          "form": "투수 8이닝까지 던지다 내려가쏘",
          "original_form": "투수 8이닝까지 던지다 내려가쏘",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.2",
          "form": "축하해",
          "original_form": "축하해",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.3",
          "form": "아깝",
          "original_form": "아깝",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.4",
          "form": "오늘은 대승이네",
          "original_form": "오늘은 대승이네",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.5",
          "form": "체력관리 필요하니까",
          "original_form": "체력관리 필요하니까",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.6",
          "form": "다 던질수있었는데",
          "original_form": "다 던질수있었는데",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.7",
          "form": "투수너무잘한당",
          "original_form": "투수너무잘한당",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ좋겠네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ좋겠네",
          "speaker_id": "2",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031285.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ실시간으로 못봐서 쬐끔아쉽",
          "original_form": "ㅋㅋㅋㅋㅋㅋ실시간으로 못봐서 쬐끔아쉽",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031285.1.10",
          "form": "관중있는 날이라 다행이다",
          "original_form": "관중있는 날이라 다행이다",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031285.1.11",
          "form": "ㅋㅋㅋ관중 있는날 이겨야 좋지",
          "original_form": "ㅋㅋㅋ관중 있는날 이겨야 좋지",
          "speaker_id": "1",
          "time": "20210904 20:04"
        },
        {
          "id": "MDRW2100031285.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ하긴 응원있어야 신나지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하긴 응원있어야 신나지",
          "speaker_id": "2",
          "time": "20210904 20:04"
        },
        {
          "id": "MDRW2100031285.1.13",
          "form": "그것도 그렇고 팬들 슬프게 집가는거 너무 내가다 슬퍼",
          "original_form": "그것도 그렇고 팬들 슬프게 집가는거 너무 내가다 슬퍼",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031285.1.14",
          "form": "( ᷄Θ᷅ )",
          "original_form": "( {emoji:•}᷄Θ{emoji:•}᷅ )",
          "speaker_id": "1",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100031285.1.15",
          "form": "맞아맞아 8이닝도 끝났으면 승리 확정이네 뭐",
          "original_form": "맞아맞아 8이닝도 끝났으면 승리 확정이네 뭐",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100031285.1.16",
          "form": "아냐 야구는 아직몰라ㅋㅋㅋ 그치만 지금도 3루출루해서",
          "original_form": "아냐 야구는 아직몰라ㅋㅋㅋ 그치만 지금도 3루출루해서",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031285.1.17",
          "form": "이길것겉오",
          "original_form": "이길것겉오",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031285.1.18",
          "form": "같오",
          "original_form": "같오",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031285.1.19",
          "form": "기분 좋겠다ㅎㅎ",
          "original_form": "기분 좋겠다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031285.1.20",
          "form": "야구는진짜 한이닝에 엄청많이도 점수따자나! 아까 한이닝에 5점 땃구",
          "original_form": "야구는진짜 한이닝에 엄청많이도 점수따자나! 아까 한이닝에 5점 땃구",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031285.1.21",
          "form": "전에보니까 한 7점도 따더라",
          "original_form": "전에보니까 한 7점도 따더라",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100031285.1.22",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031285.1.23",
          "form": "뭐 방심하면 순식간이더라",
          "original_form": "뭐 방심하면 순식간이더라",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100031285.1.24",
          "form": "마쟈",
          "original_form": "마쟈",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031285.1.25",
          "form": "야구 보는데 만루야 울팀",
          "original_form": "야구 보는데 만루야 울팀",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100031285.1.26",
          "form": "2아웃이긴한데ㅠ",
          "original_form": "2아웃이긴한데ㅠ",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031285.1.27",
          "form": "방금은 몸맞공으루 출루",
          "original_form": "방금은 몸맞공으루 출루",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031285.1.28",
          "form": "3루는 사구로 출루",
          "original_form": "3루는 사구로 출루",
          "speaker_id": "1",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100031285.1.29",
          "form": "2루는 안타!",
          "original_form": "2루는 안타!",
