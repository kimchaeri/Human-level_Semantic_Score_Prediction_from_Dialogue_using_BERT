      "utterance": [
        {
          "id": "MDRW2100040711.1.1",
          "form": "넹 굿나잇!!",
          "original_form": "넹 굿나잇!!",
          "speaker_id": "2",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100040711.1.2",
          "form": "ㅎㅎ 안녕하세요",
          "original_form": "ㅎㅎ 안녕하세요",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100040711.1.3",
          "form": "저어제 필라테스 하고왔오요",
          "original_form": "저어제 필라테스 하고왔오요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100040711.1.4",
          "form": "삭신이 쑤심...",
          "original_form": "삭신이 쑤심...",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100040711.1.5",
          "form": "와 필라테스 열심히",
          "original_form": "와 필라테스 열심히",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100040711.1.6",
          "form": "하셨네여 ㅋㅋㅋ",
          "original_form": "하셨네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100040711.1.7",
          "form": "운동 안하는 저보다 나으신걸요!!! 지금 손가락 운동은 하네여...",
          "original_form": "운동 안하는 저보다 나으신걸요!!! 지금 손가락 운동은 하네여...",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100040711.1.8",
          "form": "안할수가ㅡ없어요 옆에서 계속 봐두셔소",
          "original_form": "안할수가ㅡ없어요 옆에서 계속 봐두셔소",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100040711.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100040711.1.10",
          "form": "ㅋㅋㅋㅋ 아하 역시 현장에 가야되나봅니다",
          "original_form": "ㅋㅋㅋㅋ 아하 역시 현장에 가야되나봅니다",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100040711.1.11",
          "form": "엊그제는 유튜브에서",
          "original_form": "엊그제는 유튜브에서",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100040711.1.12",
          "form": "홈트하나 따라해봤는데",
          "original_form": "홈트하나 따라해봤는데",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100040711.1.13",
          "form": "뛰지않아도 땀이 ㅎㅎㅎ",
          "original_form": "뛰지않아도 땀이 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100040711.1.14",
          "form": "ㅋㅋㅋㅋ 아 홈트가 있었군요",
          "original_form": "ㅋㅋㅋㅋ 아 홈트가 있었군요",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100040711.1.15",
          "form": "생각조차 안했지만...ㅋㅋㅋ",
          "original_form": "생각조차 안했지만...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100040711.1.16",
          "form": "ㅎㅎㅎ다들 하도 뭐라해서",
          "original_form": "ㅎㅎㅎ다들 하도 뭐라해서",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100040711.1.17",
          "form": "근육을 키우려고 시도중입니다",
          "original_form": "근육을 키우려고 시도중입니다",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100040711.1.18",
          "form": "저 진짜 팔 흐물흐물",
          "original_form": "저 진짜 팔 흐물흐물",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100040711.1.19",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100040711.1.20",
          "form": "이제 2주째인걸요",
          "original_form": "이제 2주째인걸요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100040711.1.21",
          "form": "그런데 가을이라 비염이 도져서",
          "original_form": "그런데 가을이라 비염이 도져서",
