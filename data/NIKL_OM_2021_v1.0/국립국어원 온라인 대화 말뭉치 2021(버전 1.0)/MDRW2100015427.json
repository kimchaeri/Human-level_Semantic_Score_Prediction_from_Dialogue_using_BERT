      "utterance": [
        {
          "id": "MDRW2100015427.1.1",
          "form": "왜이리 빨리끝나죠? 잘 오셨어요",
          "original_form": "왜이리 빨리끝나죠? 잘 오셨어요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015427.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015427.1.3",
          "form": "오늘 진짜 비가 너무 와서 재난영화였어료",
          "original_form": "오늘 진짜 비가 너무 와서 재난영화였어료",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015427.1.4",
          "form": "맞아요 아까 보니 진짜 배타고 나와야할뻔했잖아요",
          "original_form": "맞아요 아까 보니 진짜 배타고 나와야할뻔했잖아요",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015427.1.5",
          "form": "운동장 나가다가 이미 시냇물이라서 난감했어요",
          "original_form": "운동장 나가다가 이미 시냇물이라서 난감했어요",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015427.1.6",
          "form": "내일도 비가 올까요",
          "original_form": "내일도 비가 올까요",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015427.1.7",
          "form": "시냇물이라니 아까 뉴스보니 여기저기 잠겼던데",
          "original_form": "시냇물이라니 아까 뉴스보니 여기저기 잠겼던데",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015427.1.8",
          "form": "내일 안올거에요",
          "original_form": "내일 안올거에요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015427.1.9",
          "form": "근데 기상청 바보에요",
          "original_form": "근데 기상청 바보에요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015427.1.10",
          "form": "기상청 슈퍼컴퓨터는 비 예측이 힘들다고 했어요",
          "original_form": "기상청 슈퍼컴퓨터는 비 예측이 힘들다고 했어요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015427.1.11",
          "form": "온도 예측은 비교적 정확하대요",
          "original_form": "온도 예측은 비교적 정확하대요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015427.1.12",
          "form": "오 온도",
          "original_form": "오 온도",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015427.1.13",
          "form": "비는 어렵군요",
          "original_form": "비는 어렵군요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015427.1.14",
          "form": "비구름비구름파워가 없나봐요ㅠ",
          "original_form": "비구름비구름파워가 없나봐요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015427.1.15",
          "form": "구름은 변덕쟁이",
          "original_form": "구름은 변덕쟁이",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015427.1.16",
          "form": "그래도 가을오면 이제 코트 입을 수 있어서 설레요",
          "original_form": "그래도 가을오면 이제 코트 입을 수 있어서 설레요",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015427.1.17",
          "form": "맞아요 저는 자켓 사고싶습니다",
          "original_form": "맞아요 저는 자켓 사고싶습니다",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015427.1.18",
          "form": "베레모도 사고싶어요",
          "original_form": "베레모도 사고싶어요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015427.1.19",
          "form": "실미도 되려나요",
          "original_form": "실미도 되려나요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015427.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ아니요 앙큼걸 됩니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니요 앙큼걸 됩니다",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015427.1.21",
          "form": "베레모 용기를 내서 쓰고 다녀줘요",
          "original_form": "베레모 용기를 내서 쓰고 다녀줘요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015427.1.22",
          "form": "우와 앙큼걸이라니",
          "original_form": "우와 앙큼걸이라니",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015427.1.23",
          "form": "저도 앙큼걸이 될수있으려나요",
          "original_form": "저도 앙큼걸이 될수있으려나요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015427.1.24",
          "form": "네 이미 앙큼걸",
          "original_form": "네 이미 앙큼걸",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015427.1.25",
          "form": "베레모로 완성",
          "original_form": "베레모로 완성",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015427.1.26",
          "form": "이번엔 맘에 드는 가을 옷이 없어서 좀 뛰어넘는 중입니다",
          "original_form": "이번엔 맘에 드는 가을 옷이 없어서 좀 뛰어넘는 중입니다",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015427.1.27",
          "form": "오 저도 그러면 좋겠는데",
          "original_form": "오 저도 그러면 좋겠는데",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015427.1.28",
          "form": "왜케 맘에드는게많죠ㅠㅠ",
          "original_form": "왜케 맘에드는게많죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015427.1.29",
          "form": "앙큼걸이라...",
          "original_form": "앙큼걸이라...",
          "speaker_id": "2",
          "time": "20210825 23:10"
        }
      ]
    }
