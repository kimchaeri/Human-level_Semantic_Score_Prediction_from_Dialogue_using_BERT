      "utterance": [
        {
          "id": "MDRW2100027478.1.1",
          "form": "넹 프렌치토스트에. 에이드요",
          "original_form": "넹 프렌치토스트에. 에이드요",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027478.1.2",
          "form": "유럽식이군요",
          "original_form": "유럽식이군요",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027478.1.3",
          "form": "치즈 넣으니까 맛있더라구요",
          "original_form": "치즈 넣으니까 맛있더라구요",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027478.1.4",
          "form": "버터를 바르면 뭐든 맛있지만",
          "original_form": "버터를 바르면 뭐든 맛있지만",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027478.1.5",
          "form": "오 버터",
          "original_form": "오 버터",
          "speaker_id": "1",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027478.1.6",
          "form": "맛있죠 진짜",
          "original_form": "맛있죠 진짜",
          "speaker_id": "1",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027478.1.7",
          "form": "버터 자체가 지방인걸 알면서 그 향과 풍미는 못 뿌리치죠",
          "original_form": "버터 자체가 지방인걸 알면서 그 향과 풍미는 못 뿌리치죠",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027478.1.8",
          "form": "맞습니다 ㅋㅋ 빵구워 먹고싶다",
          "original_form": "맞습니다 ㅋㅋ 빵구워 먹고싶다",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027478.1.9",
          "form": "이따 간식으로.빵 사알짝구워서 버터바르고",
          "original_form": "이따 간식으로.빵 사알짝구워서 버터바르고",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027478.1.10",
          "form": "마무리는 설탕솔솔 어떠세요?",
          "original_form": "마무리는 설탕솔솔 어떠세요?",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027478.1.11",
          "form": "빵을 사와야겠네요 ㅋㅋㅋ",
          "original_form": "빵을 사와야겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027478.1.12",
          "form": "요즘 꿀토스트 맛있더라규요",
          "original_form": "요즘 꿀토스트 맛있더라규요",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100027478.1.13",
          "form": "집 앞에 빵집을 가야겟어요",
          "original_form": "집 앞에 빵집을 가야겟어요",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027478.1.14",
          "form": "고고.",
          "original_form": "고고.",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027478.1.15",
          "form": "일단 치과 좀 ㅎㅎ",
          "original_form": "일단 치과 좀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027478.1.16",
          "form": "그리고 이마트 가려구요",
          "original_form": "그리고 이마트 가려구요",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027478.1.17",
          "form": "치과 싫어욥 ㅜㅠ",
          "original_form": "치과 싫어욥 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027478.1.18",
          "form": "어우 떼울거 많다 ㅋㅋㅋ 큰일",
          "original_form": "어우 떼울거 많다 ㅋㅋㅋ 큰일",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027478.1.19",
          "form": "일단 장본거나 먹어야지",
          "original_form": "일단 장본거나 먹어야지",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100027478.1.20",
          "form": "마트가면 왜이리 먹고픈게 많은지...",
          "original_form": "마트가면 왜이리 먹고픈게 많은지...",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100027478.1.21",
          "form": "타임세일로 슥 사왔어요",
          "original_form": "타임세일로 슥 사왔어요",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100027478.1.22",
          "form": "벌써 세일해요? 오~득템",
          "original_form": "벌써 세일해요? 오~득템",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100027478.1.23",
          "form": "그 고기 같은거는 몇일 남으면 미리 하더라구요 ㅎㅎ",
          "original_form": "그 고기 같은거는 몇일 남으면 미리 하더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100027478.1.24",
          "form": "전 즉석식품도 좋아해서 보통 7시쯤가요",
          "original_form": "전 즉석식품도 좋아해서 보통 7시쯤가요",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100027478.1.25",
