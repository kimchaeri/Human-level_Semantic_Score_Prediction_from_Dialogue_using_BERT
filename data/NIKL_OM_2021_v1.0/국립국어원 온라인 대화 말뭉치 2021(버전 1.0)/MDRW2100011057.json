      "utterance": [
        {
          "id": "MDRW2100011057.1.1",
          "form": "알지 그 유투버는 정말 재미있어",
          "original_form": "알지 그 유투버는 정말 재미있어",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.2",
          "form": "그러면 지컨은 아니?",
          "original_form": "그러면 지컨은 아니?",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.3",
          "form": "응 알아 둘이 친한가?",
          "original_form": "응 알아 둘이 친한가?",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.4",
          "form": "며칠전에 햄튜브와 지컨이 함께 게임하는 방송을 올렸어",
          "original_form": "며칠전에 햄튜브와 지컨이 함께 게임하는 방송을 올렸어",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.5",
          "form": "흠 최근에 친해진 거 같더라.",
          "original_form": "흠 최근에 친해진 거 같더라.",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.6",
          "form": "그 영상 재미있니?",
          "original_form": "그 영상 재미있니?",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.7",
          "form": "둘 다 반려동물을 키운다는 것에 또 공통점이 있네",
          "original_form": "둘 다 반려동물을 키운다는 것에 또 공통점이 있네",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.8",
          "form": "완전 웃겨",
          "original_form": "완전 웃겨",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.9",
          "form": "하지만 나는 지컨의 영상이 더 재밌더라",
          "original_form": "하지만 나는 지컨의 영상이 더 재밌더라",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.10",
          "form": "어머 그럼 빨리 친해졌겠다",
          "original_form": "어머 그럼 빨리 친해졌겠다",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.11",
          "form": "갈틱폰 지컨영상을 봐보렴.",
          "original_form": "갈틱폰 지컨영상을 봐보렴.",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.12",
          "form": "아 둘이 같은 영상을 올렸니?",
          "original_form": "아 둘이 같은 영상을 올렸니?",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011057.1.13",
          "form": "웅 같은 게임을 했거든",
          "original_form": "웅 같은 게임을 했거든",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011057.1.14",
          "form": "알았어 갈틱폰이라는 게임울 하는 영상이야??",
          "original_form": "알았어 갈틱폰이라는 게임울 하는 영상이야??",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011057.1.15",
          "form": "근데 햄튜브 영상은 조금 편집이 많이 없어",
          "original_form": "근데 햄튜브 영상은 조금 편집이 많이 없어",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011057.1.16",
          "form": "응 맞아.",
          "original_form": "응 맞아.",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011057.1.17",
          "form": "흠 그럼 더 길겠구나",
          "original_form": "흠 그럼 더 길겠구나",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011057.1.18",
          "form": "영상이 길면 집중하기 힘들어",
          "original_form": "영상이 길면 집중하기 힘들어",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011057.1.19",
          "form": "길지는 않지만 뭔가 차분해.",
          "original_form": "길지는 않지만 뭔가 차분해.",
          "speaker_id": "1",
          "time": "20210824 00:14"
        }
      ]
