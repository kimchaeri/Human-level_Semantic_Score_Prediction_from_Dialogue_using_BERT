      "utterance": [
        {
          "id": "MDRW2100019168.1.1",
          "form": "언닌 좀 괜찮아여..??",
          "original_form": "언닌 좀 괜찮아여..??",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019168.1.2",
          "form": "지는 이제 둘찌 델러 갑니다~",
          "original_form": "지는 이제 둘찌 델러 갑니다~",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019168.1.3",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019168.1.4",
          "form": "눈뜨자 마자 빨래돌리네여 ㅋㅋㅋㅋㅋ",
          "original_form": "눈뜨자 마자 빨래돌리네여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 07:49"
        },
        {
          "id": "MDRW2100019168.1.5",
          "form": "어제 아플까싶어가 일찍잤더니~",
          "original_form": "어제 아플까싶어가 일찍잤더니~",
          "speaker_id": "1",
          "time": "20210828 07:49"
        },
        {
          "id": "MDRW2100019168.1.6",
          "form": "할일이ㅠ",
          "original_form": "할일이ㅠ",
          "speaker_id": "1",
          "time": "20210828 07:49"
        },
        {
          "id": "MDRW2100019168.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100019168.1.8",
          "form": "난 문센왔다",
          "original_form": "난 문센왔다",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100019168.1.9",
          "form": "아침부터 아빠와 딸이 싸워서ㅡㅡ",
          "original_form": "아침부터 아빠와 딸이 싸워서ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100019168.1.10",
          "form": "에휴;;;;",
          "original_form": "에휴;;;;",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100019168.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ주말에도 바쁘구마요#",
          "original_form": "ㅋㅋㅋㅋㅋㅋ주말에도 바쁘구마요#",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100019168.1.12",
          "form": "몸은 좀 괜찮아여??",
          "original_form": "몸은 좀 괜찮아여??",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100019168.1.13",
          "form": "엉ㅋ 괜찮다ㅋㅋㅋ",
          "original_form": "엉ㅋ 괜찮다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100019168.1.14",
          "form": "점심 애들주고쉬고있다",
          "original_form": "점심 애들주고쉬고있다",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100019168.1.15",
          "form": "빨래랑해야되는데",
          "original_form": "빨래랑해야되는데",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100019168.1.16",
          "form": "다행이구만요~지는 점심른 라면으로 ㅋㅋㅋㅋ",
          "original_form": "다행이구만요~지는 점심른 라면으로 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:01"
        },
        {
          "id": "MDRW2100019168.1.17",
          "form": "첫찌가 라면 좀 해달라거 ㅋㅋㅋㅋ땡큐~",
          "original_form": "첫찌가 라면 좀 해달라거 ㅋㅋㅋㅋ땡큐~",
          "speaker_id": "1",
          "time": "20210828 12:01"
        },
        {
          "id": "MDRW2100019168.1.18",
          "form": "ㅋㅋ 우리도 볶음밥ㅡㅡ",
          "original_form": "ㅋㅋ 우리도 볶음밥ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019168.1.19",
          "form": "맨날볶음",
          "original_form": "맨날볶음",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019168.1.20",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019168.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ우리도 볶음밥 아님 뭐 미역국 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ우리도 볶음밥 아님 뭐 미역국 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100019168.1.22",
          "form": "첫찌500원주고",
          "original_form": "첫찌500원주고",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100019168.1.23",
          "form": "설거지 시킴요 ㅋㅋㅋㅋ",
          "original_form": "설거지 시킴요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100019168.1.24",
          "form": "오늘 할일 끝 ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 할일 끝 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100019168.1.25",
          "form": "저녁은 시켜묵고 시마이 할래요 ㅋㅋㅋ",
          "original_form": "저녁은 시켜묵고 시마이 할래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100019168.1.26",
          "form": "난 상담ㅋ 영어 오늘간담회하러왔다",
          "original_form": "난 상담ㅋ 영어 오늘간담회하러왔다",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100019168.1.27",
          "form": "첫째둘째",
          "original_form": "첫째둘째",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100019168.1.28",
          "form": "언니 진짜 바쁘다 바빠 ㅋㅋㅋㅋ",
          "original_form": "언니 진짜 바쁘다 바빠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100019168.1.29",
          "form": "담엔 꼭 힐데스로 이사와요 ㅋㅋㅋㅋ",
          "original_form": "담엔 꼭 힐데스로 이사와요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100019168.1.30",
          "form": "그래 힐데스ㅋㅋ",
          "original_form": "그래 힐데스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100019168.1.31",
          "form": "그래야겄다ㅋ",
          "original_form": "그래야겄다ㅋ",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100019168.1.32",
          "form": "관리비도 싸고 좋습니도 ㅋㅋㅋㅋ",
          "original_form": "관리비도 싸고 좋습니도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100019168.1.33",
          "form": "긍정적으로 생각하겠음ㅋㅋ 나진짜 아무리생각해도 윗집 짜증남ㅡㅡ",
          "original_form": "긍정적으로 생각하겠음ㅋㅋ 나진짜 아무리생각해도 윗집 짜증남ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100019168.1.34",
          "form": "생각안해야되는데ㅡㅡ",
          "original_form": "생각안해야되는데ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100019168.1.35",
          "form": "글쵸~생각이 안날수는 없져 ㅋㅋㅋㅋㅋ",
          "original_form": "글쵸~생각이 안날수는 없져 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100019168.1.36",
          "form": "진짜 안 마주쳐야할 인물....",
          "original_form": "진짜 안 마주쳐야할 인물....",
          "speaker_id": "1",
          "time": "20210828 14:57"
