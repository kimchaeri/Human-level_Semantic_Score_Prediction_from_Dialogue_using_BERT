      "utterance": [
        {
          "id": "MDRW2100045475.1.1",
          "form": "내 식도염은 대부분",
          "original_form": "내 식도염은 대부분",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045475.1.2",
          "form": "소처럼 누워있어서",
          "original_form": "소처럼 누워있어서",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.3",
          "form": "걸린걸꺼라",
          "original_form": "걸린걸꺼라",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.5",
          "form": "그래서 어른들이 먹고 눕지 말라는건데...!",
          "original_form": "그래서 어른들이 먹고 눕지 말라는건데...!",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.6",
          "form": "잰장",
          "original_form": "잰장",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.7",
          "form": "우리 할머니는",
          "original_form": "우리 할머니는",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.8",
          "form": "먹고누우면 소된다고밖에",
          "original_form": "먹고누우면 소된다고밖에",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.9",
          "form": "안해줬다고",
          "original_form": "안해줬다고",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.10",
          "form": "그게 되새김질을 하게될거란 뜻이라고",
          "original_form": "그게 되새김질을 하게될거란 뜻이라고",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 할머니 탓 하지 마..ㅡ!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 할머니 탓 하지 마..ㅡ!",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.12",
          "form": "나에게 말을 해줫어야지......",
          "original_form": "나에게 말을 해줫어야지......",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045475.1.13",
          "form": "여튼 난 누울수잇는만큼 누울거야......",
          "original_form": "여튼 난 누울수잇는만큼 누울거야......",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045475.1.14",
          "form": "졸리다",
          "original_form": "졸리다",
          "speaker_id": "2",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045475.1.15",
          "form": "누울수있는만큼 눕지만 당장ㅈ은 누울수없어......",
          "original_form": "누울수있는만큼 눕지만 당장ㅈ은 누울수없어......",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.16",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.17",
          "form": "삼각쿠션응 하나 사봐",
          "original_form": "삼각쿠션응 하나 사봐",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.18",
          "form": "쿠션류는 사면",
          "original_form": "쿠션류는 사면",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.19",
          "form": "멈머가 차지해",
          "original_form": "멈머가 차지해",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.20",
          "form": "그거 푹신해서",
          "original_form": "그거 푹신해서",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.21",
          "form": "비켜주지 않아",
          "original_form": "비켜주지 않아",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.22",
          "form": "납븐자식",
          "original_form": "납븐자식",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.23",
          "form": "기대어앉기 좋더라",
          "original_form": "기대어앉기 좋더라",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045475.1.24",
          "form": "뭔가 소파때문에",
          "original_form": "뭔가 소파때문에",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045475.1.25",
          "form": "더 눕게되는것같아",
          "original_form": "더 눕게되는것같아",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045475.1.26",
          "form": "소파는 사람을 눕히기 너무 좋은 구조야",
          "original_form": "소파는 사람을 눕히기 너무 좋은 구조야",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045475.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 멈머가 차지할 수 없는 구조일걸?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 멈머가 차지할 수 없는 구조일걸?",
          "speaker_id": "1",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045475.1.28",
          "form": "맨바닥에 돗자리만 깔고있어봐",
          "original_form": "맨바닥에 돗자리만 깔고있어봐",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045475.1.29",
          "form": "내가 누웟겠어?",
          "original_form": "내가 누웟겠어?",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045475.1.30",
          "form": "다 소파잘못이야....",
          "original_form": "다 소파잘못이야....",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045475.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 응",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 응",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045475.1.32",
          "form": "당연한걸 묻는구나..",
          "original_form": "당연한걸 묻는구나..",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045475.1.33",
          "form": "아냐 난",
          "original_form": "아냐 난",
          "speaker_id": "2",
          "time": "20210908 02:51"
        }
      ]
    }
  ]
