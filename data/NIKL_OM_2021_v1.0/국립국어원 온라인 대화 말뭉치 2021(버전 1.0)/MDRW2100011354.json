      "utterance": [
        {
          "id": "MDRW2100011354.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011354.1.2",
          "form": "비가 오나요?",
          "original_form": "비가 오나요?",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100011354.1.3",
          "form": "아까까진 내렸는데 지금은 안오네요",
          "original_form": "아까까진 내렸는데 지금은 안오네요",
          "speaker_id": "1",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011354.1.4",
          "form": "name2님 지역은 어떠신가요?",
          "original_form": "&name2&님 지역은 어떠신가요?",
          "speaker_id": "1",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011354.1.5",
          "form": "서울이요~",
          "original_form": "서울이요~",
          "speaker_id": "2",
          "time": "20210824 11:46"
        },
        {
          "id": "MDRW2100011354.1.6",
          "form": "가을이 부쩍 온것같아",
          "original_form": "가을이 부쩍 온것같아",
          "speaker_id": "2",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100011354.1.7",
          "form": "요!",
          "original_form": "요!",
          "speaker_id": "2",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100011354.1.8",
          "form": "맞아요 이제 슬슬 춥다는 느낌이 오고 있다는 느낌이 오죠?",
          "original_form": "맞아요 이제 슬슬 춥다는 느낌이 오고 있다는 느낌이 오죠?",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011354.1.9",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:09"
        },
        {
          "id": "MDRW2100011354.1.10",
          "form": "에어컨도껐어요",
          "original_form": "에어컨도껐어요",
          "speaker_id": "2",
          "time": "20210824 12:09"
        },
        {
          "id": "MDRW2100011354.1.11",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210824 12:10"
        },
        {
          "id": "MDRW2100011354.1.12",
          "form": "가을에 주로 어떻게 입으시나요?",
          "original_form": "가을에 주로 어떻게 입으시나요?",
          "speaker_id": "1",
          "time": "20210824 12:10"
        },
        {
          "id": "MDRW2100011354.1.13",
          "form": "주로 슬랙스",
          "original_form": "주로 슬랙스",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100011354.1.14",
          "form": "입는것같아요",
          "original_form": "입는것같아요",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100011354.1.15",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100011354.1.16",
          "form": "저도 긴팔에 슬랙스 입는 것 같아요",
          "original_form": "저도 긴팔에 슬랙스 입는 것 같아요",
          "speaker_id": "1",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011354.1.17",
          "form": "가을이 날씨가",
          "original_form": "가을이 날씨가",
          "speaker_id": "2",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011354.1.18",
          "form": "제일좋은것같아요",
          "original_form": "제일좋은것같아요",
          "speaker_id": "2",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011354.1.19",
          "form": "맞아요 낙엽밟는 것도 좋고 옷차림도 다양하잖아요",
          "original_form": "맞아요 낙엽밟는 것도 좋고 옷차림도 다양하잖아요",
          "speaker_id": "1",
          "time": "20210824 12:30"
        },
        {
          "id": "MDRW2100011354.1.20",
          "form": "맞아요 그리고",
          "original_form": "맞아요 그리고",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011354.1.21",
          "form": "가을옷이 제일이쁜것같아요",
          "original_form": "가을옷이 제일이쁜것같아요",
          "speaker_id": "2",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011354.1.22",
          "form": "맞아요 적당히 차분하면서도 레이어드 할 부분도 많아서 다양하게 입을 수 있으니까요",
          "original_form": "맞아요 적당히 차분하면서도 레이어드 할 부분도 많아서 다양하게 입을 수 있으니까요",
          "speaker_id": "1",
          "time": "20210824 12:36"
        }
      ]
