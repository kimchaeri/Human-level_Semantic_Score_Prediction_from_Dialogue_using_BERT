      "utterance": [
        {
          "id": "MDRW2100027471.1.1",
          "form": "점심 뭐드셨나요",
          "original_form": "점심 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027471.1.2",
          "form": "저 찜닭이랑 콩나물국이요~",
          "original_form": "저 찜닭이랑 콩나물국이요~",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027471.1.3",
          "form": "뭐드셨어요~?",
          "original_form": "뭐드셨어요~?",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027471.1.4",
          "form": "저는 소고기와 컵라면이용",
          "original_form": "저는 소고기와 컵라면이용",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027471.1.5",
          "form": "후식 안드시나여",
          "original_form": "후식 안드시나여",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027471.1.6",
          "form": "와 소고기에 라면이라니 진짜 맛있겠다 와 대박",
          "original_form": "와 소고기에 라면이라니 진짜 맛있겠다 와 대박",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027471.1.7",
          "form": "저 먹었어요",
          "original_form": "저 먹었어요",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027471.1.8",
          "form": "블랙슈가버블티!!!!",
          "original_form": "블랙슈가버블티!!!!",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027471.1.9",
          "form": "우와 어디파는건가요",
          "original_form": "우와 어디파는건가요",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027471.1.10",
          "form": "음 회사 근처 카페인데 ㅎ 지역 카페에요",
          "original_form": "음 회사 근처 카페인데 ㅎ 지역 카페에요",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100027471.1.11",
          "form": "아그렇군요",
          "original_form": "아그렇군요",
          "speaker_id": "1",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100027471.1.12",
          "form": "퇴근하셨나뇨",
          "original_form": "퇴근하셨나뇨",
          "speaker_id": "1",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100027471.1.13",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100027471.1.14",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100027471.1.15",
          "form": "네네집밥이요",
          "original_form": "네네집밥이요",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100027471.1.16",
          "form": "뭐드셖어요",
          "original_form": "뭐드셖어요",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100027471.1.17",
          "form": "저도 집밥먹고",
          "original_form": "저도 집밥먹고",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100027471.1.18",
          "form": "만두도 먹었어요",
          "original_form": "만두도 먹었어요",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100027471.1.19",
          "form": "우와만두만두",
          "original_form": "우와만두만두",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100027471.1.20",
          "form": "저는 맥주랑 나초 후식으로 먹어요",
          "original_form": "저는 맥주랑 나초 후식으로 먹어요",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100027471.1.21",
          "form": "와~~~!!!!!",
          "original_form": "와~~~!!!!!",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100027471.1.22",
          "form": "나초!!",
          "original_form": "나초!!",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100027471.1.23",
          "form": "청포도 에일인데 맛나네요",
          "original_form": "청포도 에일인데 맛나네요",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.24",
          "form": "한때 나초에 꽂혀서 그거만 먹었던 기억이있네요",
          "original_form": "한때 나초에 꽂혀서 그거만 먹었던 기억이있네요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.25",
          "form": "나쵸도 곰표 나쵸에요",
          "original_form": "나쵸도 곰표 나쵸에요",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.26",
          "form": "추천해요",
          "original_form": "추천해요",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.27",
          "form": "오ㅋㅋㅋ 곰표 나초",
          "original_form": "오ㅋㅋㅋ 곰표 나초",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.28",
          "form": "편의점에서 파나요?",
          "original_form": "편의점에서 파나요?",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.29",
          "form": "청포도 에일이면 맛있겠네요",
          "original_form": "청포도 에일이면 맛있겠네요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.30",
          "form": "네네 폄의점 4개만원",
          "original_form": "네네 폄의점 4개만원",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100027471.1.31",
          "form": "편의점가면 사와야겠어요~",
          "original_form": "편의점가면 사와야겠어요~",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100027471.1.32",
          "form": "또 맛있는거 있나여",
          "original_form": "또 맛있는거 있나여",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100027471.1.33",
          "form": "뚜또과일푸딩요",
          "original_form": "뚜또과일푸딩요",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100027471.1.34",
          "form": "헉 과일푸딩!!!!!!! 맛있겠네요",
          "original_form": "헉 과일푸딩!!!!!!! 맛있겠네요",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100027471.1.35",
          "form": "세상엔 맛있는게 정말 많네요",
          "original_form": "세상엔 맛있는게 정말 많네요",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100027471.1.36",
          "form": "그쵸 편의점 좋어요",
          "original_form": "그쵸 편의점 좋어요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        }
      ]
