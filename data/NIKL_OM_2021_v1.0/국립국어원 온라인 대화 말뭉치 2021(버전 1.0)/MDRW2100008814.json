      "utterance": [
        {
          "id": "MDRW2100008814.1.1",
          "form": "전 오늘 아이스크림 진짜 넘 마니 먹었어요",
          "original_form": "전 오늘 아이스크림 진짜 넘 마니 먹었어요",
          "speaker_id": "2",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100008814.1.2",
          "form": "돼지바 옥동자 블루베리요거트바 녹차초코바 ㅋㅋ",
          "original_form": "돼지바 옥동자 블루베리요거트바 녹차초코바 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100008814.1.3",
          "form": "어머 저도 잘사는거예요 ㅋㅋ",
          "original_form": "어머 저도 잘사는거예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008814.1.4",
          "form": "전 옥동자 돼지바요 ㅋㅋ",
          "original_form": "전 옥동자 돼지바요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100008814.1.5",
          "form": "아 하루 하나는 꼭먹어요 ㅜㅜ",
          "original_form": "아 하루 하나는 꼭먹어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:00"
        },
        {
          "id": "MDRW2100008814.1.6",
          "form": "근데 봉지로사니 아쩔땐 몇개씩 저도 먹어요 ㅜㅜ",
          "original_form": "근데 봉지로사니 아쩔땐 몇개씩 저도 먹어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:00"
        },
        {
          "id": "MDRW2100008814.1.7",
          "form": "400원이라 넘싸요",
          "original_form": "400원이라 넘싸요",
          "speaker_id": "1",
          "time": "20210822 19:00"
        },
        {
          "id": "MDRW2100008814.1.8",
          "form": "이게 싸니까 갈때 한봉지가득사요",
          "original_form": "이게 싸니까 갈때 한봉지가득사요",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008814.1.9",
          "form": "그러니 뭐 ㅋㅋ",
          "original_form": "그러니 뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008814.1.10",
          "form": "많이 먹어요",
          "original_form": "많이 먹어요",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008814.1.11",
          "form": "아이스크림을 네개나 먹었는데 물이라그런가 딱히 막 배부르단 느낌이 없어요 ㅋㅋ",
          "original_form": "아이스크림을 네개나 먹었는데 물이라그런가 딱히 막 배부르단 느낌이 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008814.1.12",
          "form": "억울함 ㅋㅋ",
          "original_form": "억울함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008814.1.13",
          "form": "배 안불러요 그래서 계속 먹어요 ㅋㅋ",
          "original_form": "배 안불러요 그래서 계속 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008814.1.14",
          "form": "먹고 또먹고",
          "original_form": "먹고 또먹고",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008814.1.15",
          "form": "아이스크림은 진짜 그런거같아요",
          "original_form": "아이스크림은 진짜 그런거같아요",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008814.1.16",
          "form": "혹시 빵순이세요 밥순이세요",
          "original_form": "혹시 빵순이세요 밥순이세요",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008814.1.17",
          "form": "네 이게 끊기 힘들어요",
          "original_form": "네 이게 끊기 힘들어요",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008814.1.18",
          "form": "저 젤리도 ㅜㅜ좋아해요",
          "original_form": "저 젤리도 ㅜㅜ좋아해요",
          "speaker_id": "1",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008814.1.19",
          "form": "전 빵순이인데",
          "original_form": "전 빵순이인데",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008814.1.20",
          "form": "오저도요",
          "original_form": "오저도요",
          "speaker_id": "1",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008814.1.21",
          "form": "지금도 쿠키 머핀 먹고 왔어요",
          "original_form": "지금도 쿠키 머핀 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008814.1.22",
          "form": "그래서인지 빵이나 피자 아이스크림 이런건 진짜 잘 먹어요",
          "original_form": "그래서인지 빵이나 피자 아이스크림 이런건 진짜 잘 먹어요",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008814.1.23",
          "form": "앙 넘 맛나여",
          "original_form": "앙 넘 맛나여",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008814.1.24",
          "form": "똑같네요 ㅋㅋ",
          "original_form": "똑같네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008814.1.25",
          "form": "동생은 도대체 그걸 무슨 맛으로 먹는지 모르겠다는데",
          "original_form": "동생은 도대체 그걸 무슨 맛으로 먹는지 모르겠다는데",
          "speaker_id": "2",
          "time": "20210822 19:22"
        },
        {
          "id": "MDRW2100008814.1.26",
          "form": "밀가루 끊어야하는데 큰일이네요",
          "original_form": "밀가루 끊어야하는데 큰일이네요",
          "speaker_id": "2",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008814.1.27",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008814.1.28",
          "form": "못끊지 싶어요 ㅡㅡ",
          "original_form": "못끊지 싶어요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008814.1.29",
          "form": "그거아세요",
          "original_form": "그거아세요",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008814.1.30",
          "form": "합법적 마약",
          "original_form": "합법적 마약",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008814.1.31",
          "form": "흰쌀 밀가루 설탕 ㅋㅋㅋ",
          "original_form": "흰쌀 밀가루 설탕 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008814.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008814.1.33",
          "form": "절대 중독에서 못벗어나죠",
          "original_form": "절대 중독에서 못벗어나죠",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008814.1.34",
          "form": "샌드위치도 같이 먹고 왔어요 ㅋㅋ",
          "original_form": "샌드위치도 같이 먹고 왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008814.1.35",
          "form": "전 오늘 쿠키랑 아이스크림 너무 많이 먹어서 저녁은 스킵하려고요",
          "original_form": "전 오늘 쿠키랑 아이스크림 너무 많이 먹어서 저녁은 스킵하려고요",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008814.1.36",
          "form": "아이스 아메리카노 샀어요",
          "original_form": "아이스 아메리카노 샀어요",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008814.1.37",
          "form": "ㅋㅋ전 막창 먹게요 ㅋㅋ",
          "original_form": "ㅋㅋ전 막창 먹게요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008814.1.38",
          "form": "빽다방에서",
          "original_form": "빽다방에서",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008814.1.39",
          "form": "디저트는 따로죠",
          "original_form": "디저트는 따로죠",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008814.1.40",
          "form": "저도 한잔 했어요",
          "original_form": "저도 한잔 했어요",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008814.1.41",
          "form": "사실 아이스 아메리카노는 자주 사서드세요",
          "original_form": "사실 아이스 아메리카노는 자주 사서드세요",
          "speaker_id": "2",
          "time": "20210822 19:28"
        }
      ]
