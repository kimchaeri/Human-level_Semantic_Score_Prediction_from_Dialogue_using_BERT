      "utterance": [
        {
          "id": "MDRW2100037890.1.1",
          "form": "양치했어요??",
          "original_form": "양치했어요??",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037890.1.2",
          "form": "둘이 자라고 하고 나왔음",
          "original_form": "둘이 자라고 하고 나왔음",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037890.1.3",
          "form": "둘이만 놔두고 나온 지 이틀 됐다",
          "original_form": "둘이만 놔두고 나온 지 이틀 됐다",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037890.1.4",
          "form": "생각보다 잘자 네",
          "original_form": "생각보다 잘자 네",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037890.1.5",
          "form": "언제 또 튀어나올지 모르겠지만",
          "original_form": "언제 또 튀어나올지 모르겠지만",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037890.1.6",
          "form": "투는 오빠랑 둘이 자는 거 굉장히 좋아함",
          "original_form": "투는 오빠랑 둘이 자는 거 굉장히 좋아함",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100037890.1.7",
          "form": "오호 좋구만요~",
          "original_form": "오호 좋구만요~",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.8",
          "form": "둘이 같이 자면 제일 좋죠ㅋㅋ",
          "original_form": "둘이 같이 자면 제일 좋죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.9",
          "form": "어제 아빠가 재우 러 들어와서 슬퍼 단다",
          "original_form": "어제 아빠가 재우 러 들어와서 슬퍼 단다",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.10",
          "form": "무섭지도 않고 일어나면 같이 놀고ㅋㅋ",
          "original_form": "무섭지도 않고 일어나면 같이 놀고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.11",
          "form": "오빠랑 둘이 자려고 했는데",
          "original_form": "오빠랑 둘이 자려고 했는데",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.12",
          "form": "잠들 때만 같이 자고",
          "original_form": "잠들 때만 같이 자고",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.13",
          "form": "아 너무 귀엽다ㅋㅋㅋ",
          "original_form": "아 너무 귀엽다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.14",
          "form": "잠들면 원은 이층침대로 옮긴다",
          "original_form": "잠들면 원은 이층침대로 옮긴다",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.15",
          "form": "아빠가 눈치가 없었네요ㅋㅋㅋ",
          "original_form": "아빠가 눈치가 없었네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.16",
          "form": "그렇지 눈치 없지",
          "original_form": "그렇지 눈치 없지",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.17",
          "form": "이층으로 올릴 수 있어요?",
          "original_form": "이층으로 올릴 수 있어요?",
          "speaker_id": "2",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.18",
          "form": "왜 저러나 몰라ㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜 저러나 몰라ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100037890.1.19",
          "form": "아니 잘 때는 큰 방패밀리 침대에서 자고",
          "original_form": "아니 잘 때는 큰 방패밀리 침대에서 자고",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037890.1.20",
          "form": "잠들면 이층침대 일층으로 옮겨",
          "original_form": "잠들면 이층침대 일층으로 옮겨",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037890.1.21",
          "form": "우리 신랑도 질척 돼요ㅋㅋㅋㅋ",
          "original_form": "우리 신랑도 질척 돼요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037890.1.22",
          "form": "2층에는 신랑이 자고",
          "original_form": "2층에는 신랑이 자고",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037890.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037890.1.24",
          "form": "둘째한테는 그렇게 되는가 봄ㅋㅋㅋ",
          "original_form": "둘째한테는 그렇게 되는가 봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100037890.1.25",
          "form": "턱에 계속 자잘한거 난다",
          "original_form": "턱에 계속 자잘한거 난다",
          "speaker_id": "1",
