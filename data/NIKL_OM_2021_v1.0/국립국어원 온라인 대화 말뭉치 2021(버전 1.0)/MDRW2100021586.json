      "utterance": [
        {
          "id": "MDRW2100021586.1.1",
          "form": "집안 분위기는 어떤편이에요?",
          "original_form": "집안 분위기는 어떤편이에요?",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021586.1.2",
          "form": "화목한편이에요",
          "original_form": "화목한편이에요",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021586.1.3",
          "form": "부럽네요 부모님 사이가 좋으신가봐요",
          "original_form": "부럽네요 부모님 사이가 좋으신가봐요",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021586.1.4",
          "form": "좋은편이라고 생각해요",
          "original_form": "좋은편이라고 생각해요",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021586.1.5",
          "form": "다행이네요 ㅎㅎ",
          "original_form": "다행이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021586.1.6",
          "form": "형제자매 있어요?",
          "original_form": "형제자매 있어요?",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021586.1.7",
          "form": "네 동생2 있어요\r\nname1은요?",
          "original_form": "네 동생2 있어요\r\n&name1&은요?",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021586.1.8",
          "form": "저는 누나 하나 있는데 가끔 사이가 좋기도 하고 나쁘기도 해요 ㅎㅎ",
          "original_form": "저는 누나 하나 있는데 가끔 사이가 좋기도 하고 나쁘기도 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021586.1.9",
          "form": "님은 동생들이랑 잘 지내요?",
          "original_form": "님은 동생들이랑 잘 지내요?",
          "speaker_id": "1",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021586.1.10",
          "form": "저도 사이좋을때도 있고 다툴때도 있고 그래요ㅎㅎ",
          "original_form": "저도 사이좋을때도 있고 다툴때도 있고 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021586.1.11",
          "form": "오 그렇구나 근데 대체로 잘 지내는 편인가봐요?",
          "original_form": "오 그렇구나 근데 대체로 잘 지내는 편인가봐요?",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021586.1.12",
          "form": "저희 가족은 부부끼리 자주 싸워여 ㅜ",
          "original_form": "저희 가족은 부부끼리 자주 싸워여 ㅜ",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021586.1.13",
          "form": "그러시군요ㅜㅜ\r\n대체로 잘 지내는거라 생각하는데 잘 모르겠네요",
          "original_form": "그러시군요ㅜㅜ\r\n대체로 잘 지내는거라 생각하는데 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021586.1.14",
          "form": "남동생과는 성격이 조금 안 맞는 편이라서, 대화를 많이 아끼는편이에요",
          "original_form": "남동생과는 성격이 조금 안 맞는 편이라서, 대화를 많이 아끼는편이에요",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021586.1.15",
          "form": "아~ 남동생은 어떤 성격이에요?",
          "original_form": "아~ 남동생은 어떤 성격이에요?",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021586.1.16",
          "form": "음 저랑 반대되는 성격이랄까요?",
          "original_form": "음 저랑 반대되는 성격이랄까요?",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021586.1.17",
          "form": "ㅠㅠ 그럼 안맞다는 생각이 자주 들어요?",
          "original_form": "ㅠㅠ 그럼 안맞다는 생각이 자주 들어요?",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021586.1.18",
          "form": "네 아무래도 반대되는 성격이다 보니 잘 맞는건 아닌듯 해요",
          "original_form": "네 아무래도 반대되는 성격이다 보니 잘 맞는건 아닌듯 해요",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021586.1.19",
          "form": "아이구 그런 생각이 들땐 어떤 상황이 있었어요?",
          "original_form": "아이구 그런 생각이 들땐 어떤 상황이 있었어요?",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021586.1.20",
          "form": "예를 들면?",
          "original_form": "예를 들면?",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021586.1.21",
          "form": "음 갑자기 떠올리려하니 생각나지 않네요",
          "original_form": "음 갑자기 떠올리려하니 생각나지 않네요",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021586.1.22",
          "form": "ㅋㅋㅋ막 찍먹파 부먹파 이런거 아니에요?",
          "original_form": "ㅋㅋㅋ막 찍먹파 부먹파 이런거 아니에요?",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021586.1.23",
          "form": "그런건 아니에요ㅋㅋㅋ",
          "original_form": "그런건 아니에요ㅋㅋㅋ",
          "speaker_id": "2",
