      "utterance": [
        {
          "id": "MDRW2100018250.1.1",
          "form": "전시 보신거 있으세요?",
          "original_form": "전시 보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018250.1.2",
          "form": "제주도가서 빛의벙커간게 마지막이네요",
          "original_form": "제주도가서 빛의벙커간게 마지막이네요",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018250.1.3",
          "form": "아하 영화 좋아하세요?",
          "original_form": "아하 영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018250.1.4",
          "form": "네~ 코시국전에 자주보러갔어요",
          "original_form": "네~ 코시국전에 자주보러갔어요",
          "speaker_id": "1",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100018250.1.5",
          "form": "최근에 본 영화 어떤거예요?",
          "original_form": "최근에 본 영화 어떤거예요?",
          "speaker_id": "2",
          "time": "20210827 10:55"
        },
        {
          "id": "MDRW2100018250.1.6",
          "form": "영화관에서 최근에 본거는 블랙위도우요!",
          "original_form": "영화관에서 최근에 본거는 블랙위도우요!",
          "speaker_id": "1",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100018250.1.7",
          "form": "오 재밌었나요?",
          "original_form": "오 재밌었나요?",
          "speaker_id": "2",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100018250.1.8",
          "form": "네 전 마블 영화 좋아해서 꼭 영화관가서 봐요",
          "original_form": "네 전 마블 영화 좋아해서 꼭 영화관가서 봐요",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100018250.1.9",
          "form": "저도 맨날 본다고 해놓고 타이밍 놓쳐서 못봤어요ㅠㅠ",
          "original_form": "저도 맨날 본다고 해놓고 타이밍 놓쳐서 못봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100018250.1.10",
          "form": "마블영화 재밌는데!!",
          "original_form": "마블영화 재밌는데!!",
          "speaker_id": "2",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100018250.1.11",
          "form": "코로나덜할때가서 지금은 못갈꺼같아요",
          "original_form": "코로나덜할때가서 지금은 못갈꺼같아요",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100018250.1.12",
          "form": "맞아요 지금은..ㅠㅠ",
          "original_form": "맞아요 지금은..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100018250.1.13",
          "form": "그래도 요샌 vod 빨리나오더라구요",
          "original_form": "그래도 요샌 vod 빨리나오더라구요",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100018250.1.14",
          "form": "그리고 자동차극장 한번도 안가봤는데 가보려구요",
          "original_form": "그리고 자동차극장 한번도 안가봤는데 가보려구요",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100018250.1.15",
          "form": "아하 면허 있으세요?",
          "original_form": "아하 면허 있으세요?",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100018250.1.16",
          "form": "네네 면허있어요~^^ 지역에 자동차극장도 근처에 있는데 코시국아니면 안갔을텐데 가보고싶어졌어요",
          "original_form": "네네 면허있어요~^^ 지역에 자동차극장도 근처에 있는데 코시국아니면 안갔을텐데 가보고싶어졌어요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100018250.1.17",
          "form": "재밌을것같은데 면허가 없어서 못가봤어요ㅠㅠ",
          "original_form": "재밌을것같은데 면허가 없어서 못가봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100018250.1.18",
          "form": "아하 다른분이랑 같이가셔야겠네요~ 최근에 본 영화가 있으신가요?",
          "original_form": "아하 다른분이랑 같이가셔야겠네요~ 최근에 본 영화가 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 00:16"
        }
      ]
