      "utterance": [
        {
          "id": "MDRW2100006068.1.1",
          "form": "추석전에 백신 맞으세요?",
          "original_form": "추석전에 백신 맞으세요?",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100006068.1.2",
          "form": "네 다음주에 맞을 예정이에요",
          "original_form": "네 다음주에 맞을 예정이에요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100006068.1.3",
          "form": "1차요?",
          "original_form": "1차요?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100006068.1.4",
          "form": "네ㅎㅎ 맞으셨어요?",
          "original_form": "네ㅎㅎ 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006068.1.5",
          "form": "네 10일됐어요 ㅎㅎ",
          "original_form": "네 10일됐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006068.1.6",
          "form": "맞고나서 어떠셨어요?",
          "original_form": "맞고나서 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100006068.1.7",
          "form": "1차는 별거 없었어요",
          "original_form": "1차는 별거 없었어요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006068.1.8",
          "form": "팔이 좀 아프고",
          "original_form": "팔이 좀 아프고",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006068.1.9",
          "form": "1번 미열나서 해열제먹고",
          "original_form": "1번 미열나서 해열제먹고",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006068.1.10",
          "form": "그정도?",
          "original_form": "그정도?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006068.1.11",
          "form": "2차가 더 아프다고들 하더라구요",
          "original_form": "2차가 더 아프다고들 하더라구요",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006068.1.12",
          "form": "근데 주변에 2차맞고 멀쩡한 사람도 많더라구요",
          "original_form": "근데 주변에 2차맞고 멀쩡한 사람도 많더라구요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006068.1.13",
          "form": "ㅋㅋ 대부분은 그럴 거에요",
          "original_form": "ㅋㅋ 대부분은 그럴 거에요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006068.1.14",
          "form": "1차 맞기전에 엄청 긴장했거든요",
          "original_form": "1차 맞기전에 엄청 긴장했거든요",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006068.1.15",
          "form": "팔 아픈 건 근육주사라 어쩔 수 없을 것 같고 그냥 약  먹고 쉬어야겠어요ㅋㅋ",
          "original_form": "팔 아픈 건 근육주사라 어쩔 수 없을 것 같고 그냥 약  먹고 쉬어야겠어요ㅋㅋ",
