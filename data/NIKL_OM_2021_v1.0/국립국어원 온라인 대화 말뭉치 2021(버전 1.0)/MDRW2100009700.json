      "utterance": [
        {
          "id": "MDRW2100009700.1.1",
          "form": "이따 집에/오시면 도서어플 알려드릴게요",
          "original_form": "이따 집에/오시면 도서어플 알려드릴게요",
          "speaker_id": "1",
          "time": "20210823 07:52"
        },
        {
          "id": "MDRW2100009700.1.2",
          "form": "무슨 어플",
          "original_form": "무슨 어플",
          "speaker_id": "2",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100009700.1.3",
          "form": "요즘 코로나때문에 도서관에서 책 빌리기 좀 그렇잖아요",
          "original_form": "요즘 코로나때문에 도서관에서 책 빌리기 좀 그렇잖아요",
          "speaker_id": "1",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009700.1.4",
          "form": "그래서 도서관 안간지 오래다",
          "original_form": "그래서 도서관 안간지 오래다",
          "speaker_id": "2",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009700.1.5",
          "form": "근데 책은 읽고 싶으실거 아녜요? 요즘 도서어플깔면 전자책으루 볼 수 있어요",
          "original_form": "근데 책은 읽고 싶으실거 아녜요? 요즘 도서어플깔면 전자책으루 볼 수 있어요",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009700.1.6",
          "form": "잔자책? 요즘 그런것도 나오는구나",
          "original_form": "잔자책? 요즘 그런것도 나오는구나",
          "speaker_id": "2",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009700.1.7",
          "form": "네 이북이라고도 하는데 완전 편해요",
          "original_form": "네 이북이라고도 하는데 완전 편해요",
          "speaker_id": "1",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009700.1.8",
          "form": "어플 깔아야 한다고?",
          "original_form": "어플 깔아야 한다고?",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009700.1.9",
          "form": "제가 깔고 사용법을 알려드릴게요",
          "original_form": "제가 깔고 사용법을 알려드릴게요",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009700.1.10",
          "form": "그래주면 고맙지",
          "original_form": "그래주면 고맙지",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009700.1.11",
          "form": "거기에 굉장히 다양한 자료들이 있으니까 관심분야 검색해서 보실 수 있어요",
          "original_form": "거기에 굉장히 다양한 자료들이 있으니까 관심분야 검색해서 보실 수 있어요",
          "speaker_id": "1",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009700.1.12",
          "form": "돈내는거냐?",
          "original_form": "돈내는거냐?",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009700.1.13",
          "form": "유료도 있구 무료도 있어요",
          "original_form": "유료도 있구 무료도 있어요",
          "speaker_id": "1",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009700.1.14",
          "form": "일단 무료인 거 봐봐야겠다",
          "original_form": "일단 무료인 거 봐봐야겠다",
          "speaker_id": "2",
          "time": "20210823 12:24"
        }
