        },
        {
          "id": "MDRW2100034000.1.2",
          "form": "지금 날씨 좋아요?",
          "original_form": "지금 날씨 좋아요?",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100034000.1.3",
          "form": "흐려요",
          "original_form": "흐려요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100034000.1.4",
          "form": "그래도 시원하지않나요",
          "original_form": "그래도 시원하지않나요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100034000.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034000.1.6",
          "form": "감기조심하세요",
          "original_form": "감기조심하세요",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100034000.1.7",
          "form": "님도 환절기인데 감기조심하세요",
          "original_form": "님도 환절기인데 감기조심하세요",
          "speaker_id": "1",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100034000.1.8",
          "form": "그래야죠 오늘 추워서 깼어요",
          "original_form": "그래야죠 오늘 추워서 깼어요",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034000.1.9",
          "form": "그쪽은 어때요",
          "original_form": "그쪽은 어때요",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034000.1.10",
          "form": "요즘 새벽에 춥네요",
          "original_form": "요즘 새벽에 춥네요",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100034000.1.11",
          "form": "이불 바꿀때가 됐죠",
          "original_form": "이불 바꿀때가 됐죠",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100034000.1.12",
          "form": "좋아하는 계절있나요?",
          "original_form": "좋아하는 계절있나요?",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100034000.1.13",
          "form": "가을이랑 봄이요",
          "original_form": "가을이랑 봄이요",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100034000.1.14",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100034000.1.15",
          "form": "여름이요 이유가뭔가요?",
          "original_form": "여름이요 이유가뭔가요?",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100034000.1.16",
          "form": "날씨가 적당해서 좋아요 볼것도 많고",
          "original_form": "날씨가 적당해서 좋아요 볼것도 많고",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100034000.1.17",
          "form": "name1님은 여름이 왜 좋으신가요",
          "original_form": "&name1&님은 여름이 왜 좋으신가요",
          "speaker_id": "2",
