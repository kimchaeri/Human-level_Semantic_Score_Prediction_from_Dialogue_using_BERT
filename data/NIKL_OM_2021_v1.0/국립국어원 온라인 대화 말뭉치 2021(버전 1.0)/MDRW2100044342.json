      "utterance": [
        {
          "id": "MDRW2100044342.1.1",
          "form": "놀이공원도 좋아해요!",
          "original_form": "놀이공원도 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044342.1.2",
          "form": "저도 좋아해요~ 다만 무서운걸 못탈뿐",
          "original_form": "저도 좋아해요~ 다만 무서운걸 못탈뿐",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044342.1.3",
          "form": "어디 가셨어요?",
          "original_form": "어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044342.1.4",
          "form": "싱가폴 갔을때 놀이동산 갔는데 총쏘는 놀이기구가 너무 재미있었어요",
          "original_form": "싱가폴 갔을때 놀이동산 갔는데 총쏘는 놀이기구가 너무 재미있었어요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044342.1.5",
          "form": "움직이면서 쏘는건가요?",
          "original_form": "움직이면서 쏘는건가요?",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044342.1.6",
          "form": "네~ 놀이기구 자체가 재미있는데 총도 쏘니 재미가 두배였어요",
          "original_form": "네~ 놀이기구 자체가 재미있는데 총도 쏘니 재미가 두배였어요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044342.1.7",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044342.1.8",
          "form": "저는 오사카 유니버셜스튜디오 갔는데",
          "original_form": "저는 오사카 유니버셜스튜디오 갔는데",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044342.1.9",
          "form": "근데 그거 말고는 우리나랑 비슷했고요",
          "original_form": "근데 그거 말고는 우리나랑 비슷했고요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044342.1.10",
          "form": "말 그대로 놀이기구 타는 느낌이라 재밌더라구요",
          "original_form": "말 그대로 놀이기구 타는 느낌이라 재밌더라구요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044342.1.11",
          "form": "뭐가 제일재미있었나요?",
          "original_form": "뭐가 제일재미있었나요?",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044342.1.12",
          "form": "백투더퓨쳐 테마 놀이기구요 ㅋㅋㅋㅋ",
          "original_form": "백투더퓨쳐 테마 놀이기구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044342.1.13",
          "form": "이건 재밌어서 두번 탔어요",
          "original_form": "이건 재밌어서 두번 탔어요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044342.1.14",
          "form": "혹시 가기 되면 그거 먼저 탈게요 ㅎ",
          "original_form": "혹시 가기 되면 그거 먼저 탈게요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044342.1.15",
          "form": "넵 ㅋㅋㅋ 근데 젤 인기많은건 해리포터예요",
          "original_form": "넵 ㅋㅋㅋ 근데 젤 인기많은건 해리포터예요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044342.1.16",
          "form": "그게 놀이기구 이름이예요",
          "original_form": "그게 놀이기구 이름이예요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044342.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044342.1.18",
          "form": "여긴 일찍 안가면 대기 어마무시하니까 젤 먼저 가세요",
          "original_form": "여긴 일찍 안가면 대기 어마무시하니까 젤 먼저 가세요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044342.1.19",
          "form": "네네 영화 테마 놀이기구가 있어요",
          "original_form": "네네 영화 테마 놀이기구가 있어요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044342.1.20",
          "form": "아하!!",
          "original_form": "아하!!",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044342.1.21",
          "form": "싱가폴은 엄청 분비진 않았거든요",
          "original_form": "싱가폴은 엄청 분비진 않았거든요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        }
      ]
