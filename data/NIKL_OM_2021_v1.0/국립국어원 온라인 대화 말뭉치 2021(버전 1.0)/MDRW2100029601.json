      "utterance": [
        {
          "id": "MDRW2100029601.1.1",
          "form": "디즈니 캐릭터 콜라보로",
          "original_form": "디즈니 캐릭터 콜라보로",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.2",
          "form": "폰이 조작쉬워서 좋아요",
          "original_form": "폰이 조작쉬워서 좋아요",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.3",
          "form": "길건너는 겜이에요",
          "original_form": "길건너는 겜이에요",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.4",
          "form": "피씨방도 가시나요",
          "original_form": "피씨방도 가시나요",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.5",
          "form": "폰겜이에요",
          "original_form": "폰겜이에요",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.6",
          "form": "지금은코로나라서 안가요",
          "original_form": "지금은코로나라서 안가요",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.7",
          "form": "카카오게임즈 신작도 반응좋죠",
          "original_form": "카카오게임즈 신작도 반응좋죠",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.8",
          "form": "노트북 장만했어요",
          "original_form": "노트북 장만했어요",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.9",
          "form": "프렌즈샷",
          "original_form": "프렌즈샷",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029601.1.10",
          "form": "골프겜이죠",
          "original_form": "골프겜이죠",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.11",
          "form": "엠게임 열혈강호도요. 중국서 인기많지요",
          "original_form": "엠게임 열혈강호도요. 중국서 인기많지요",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.12",
          "form": "전 애니팡 같은겜이 좋아요",
          "original_form": "전 애니팡 같은겜이 좋아요",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.13",
          "form": "스포츠게임은 안해봤어요",
          "original_form": "스포츠게임은 안해봤어요",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.14",
          "form": "퍼즐같은거..",
          "original_form": "퍼즐같은거..",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.15",
          "form": "달리기  막 연속누르기 힘들고 그래서 안하게 되네요",
          "original_form": "달리기  막 연속누르기 힘들고 그래서 안하게 되네요",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.16",
          "form": "낚시겜은 해봤어요?",
          "original_form": "낚시겜은 해봤어요?",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.17",
          "form": "낚시겜은 아직.. 전 퍼즐이 좋아요",
          "original_form": "낚시겜은 아직.. 전 퍼즐이 좋아요",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029601.1.18",
          "form": "게임이지만 손맛이 죽여줘요",
          "original_form": "게임이지만 손맛이 죽여줘요",
          "speaker_id": "2",
          "time": "20210903 10:51"
        }
