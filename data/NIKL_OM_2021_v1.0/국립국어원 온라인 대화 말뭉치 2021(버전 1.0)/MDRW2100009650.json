      "utterance": [
        {
          "id": "MDRW2100009650.1.1",
          "form": "내년 최저시급은 9100원대래요",
          "original_form": "내년 최저시급은 9100원대래요",
          "speaker_id": "2",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009650.1.2",
          "form": "오 또 오르나요?",
          "original_form": "오 또 오르나요?",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009650.1.3",
          "form": "생각보다 크게 오르네요",
          "original_form": "생각보다 크게 오르네요",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009650.1.4",
          "form": "적게 오를 줄 알았는데",
          "original_form": "적게 오를 줄 알았는데",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009650.1.5",
          "form": "네 저도 놀랐어요",
          "original_form": "네 저도 놀랐어요",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009650.1.6",
          "form": "꽤 오르네요",
          "original_form": "꽤 오르네요",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009650.1.7",
          "form": "아르바이트하세요?",
          "original_form": "아르바이트하세요?",
          "speaker_id": "1",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009650.1.8",
          "form": "안하기는 하는데",
          "original_form": "안하기는 하는데",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009650.1.9",
          "form": "가끔 단순알바 할 때가 있어요",
          "original_form": "가끔 단순알바 할 때가 있어요",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009650.1.10",
          "form": "name1님은 아르바이트 하세요?",
          "original_form": "&name1&님은 아르바이트 하세요?",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009650.1.11",
          "form": "전 알바 요즘은 안하고",
          "original_form": "전 알바 요즘은 안하고",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009650.1.12",
          "form": "내일 과외 첫 날이라 떨고 있어요",
          "original_form": "내일 과외 첫 날이라 떨고 있어요",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009650.1.13",
          "form": "오 과외",
          "original_form": "오 과외",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009650.1.14",
          "form": "저도 과외 해요",
          "original_form": "저도 과외 해요",
          "speaker_id": "2",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009650.1.15",
          "form": "오...!! 혹시 몇학년이신가요?!",
          "original_form": "오...!! 혹시 몇학년이신가요?!",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009650.1.16",
          "form": "저는 초4 초6 중1 고2 고3 해요",
          "original_form": "저는 초4 초6 중1 고2 고3 해요",
          "speaker_id": "2",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009650.1.17",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009650.1.18",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009650.1.19",
          "form": "저는 이번에 초6 중3 하게 되었는데",
          "original_form": "저는 이번에 초6 중3 하게 되었는데",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009650.1.20",
          "form": "혹시 중3 영어 고등선행은",
          "original_form": "혹시 중3 영어 고등선행은",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009650.1.21",
          "form": "어떻게 해야할까요",
          "original_form": "어떻게 해야할까요",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009650.1.22",
          "form": "저는 1년에 한 단어장 3번씩 영어단어장 세번 돌려요",
          "original_form": "저는 1년에 한 단어장 3번씩 영어단어장 세번 돌려요",
          "speaker_id": "2",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009650.1.23",
          "form": "실력에 따라 다른데",
          "original_form": "실력에 따라 다른데",
          "speaker_id": "2",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009650.1.24",
          "form": "아주 잘하는 학생이면 워드마스터 하이퍼 2000 쓰고",
          "original_form": "아주 잘하는 학생이면 워드마스터 하이퍼 2000 쓰고",
          "speaker_id": "2",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009650.1.25",
          "form": "일반적인 학생은 워드마스터/어휘끝/워드마스터ebs파이널로 시작해서",
          "original_form": "일반적인 학생은 워드마스터/어휘끝/워드마스터ebs파이널로 시작해서",
          "speaker_id": "2",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009650.1.26",
          "form": "1년에 한 단어장을 3번 돌리고 3년간 3권을 돌려요",
          "original_form": "1년에 한 단어장을 3번 돌리고 3년간 3권을 돌려요",
          "speaker_id": "2",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009650.1.27",
          "form": "와... 너무 전문적이세요",
          "original_form": "와... 너무 전문적이세요",
          "speaker_id": "1",
          "time": "20210823 02:39"
        },
        {
          "id": "MDRW2100009650.1.28",
          "form": "수업 방식이 고민인데",
          "original_form": "수업 방식이 고민인데",
          "speaker_id": "1",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009650.1.29",
          "form": "모의고사 기출을 하고 있대요",
          "original_form": "모의고사 기출을 하고 있대요",
          "speaker_id": "1",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009650.1.30",
          "form": "어떤게 고민이에요?",
          "original_form": "어떤게 고민이에요?",
          "speaker_id": "2",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009650.1.31",
          "form": "사실 학생을 못 만나봤는데",
          "original_form": "사실 학생을 못 만나봤는데",
          "speaker_id": "1",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009650.1.32",
          "form": "어머니께 진도를 물었더니",
          "original_form": "어머니께 진도를 물었더니",
          "speaker_id": "1",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009650.1.33",
          "form": "2020 3월은 했고 2019 3월 모의고사 하는 중이라고",
          "original_form": "2020 3월은 했고 2019 3월 모의고사 하는 중이라고",
          "speaker_id": "1",
          "time": "20210823 02:40"
        },
        {
          "id": "MDRW2100009650.1.34",
          "form": "학생을 아직 못만났군요",
          "original_form": "학생을 아직 못만났군요",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009650.1.35",
          "form": "설명해주셔서",
          "original_form": "설명해주셔서",
          "speaker_id": "1",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009650.1.36",
          "form": "좀 희안하게 설명해주시네요",
          "original_form": "좀 희안하게 설명해주시네요",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009650.1.37",
          "form": "어떻게 이어나가야 할지 모르겠어요 ㅠㅠ",
