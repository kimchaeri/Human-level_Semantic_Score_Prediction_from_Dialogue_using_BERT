      "utterance": [
        {
          "id": "MDRW2100040640.1.1",
          "form": "근데 너무 많이 몰리면 서버가 감당을 못해서 터지거나",
          "original_form": "근데 너무 많이 몰리면 서버가 감당을 못해서 터지거나",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040640.1.2",
          "form": "요번에 백신 인증 십분에 한번되게 했잖아요",
          "original_form": "요번에 백신 인증 십분에 한번되게 했잖아요",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040640.1.3",
          "form": "그런식으로 해야할듯요",
          "original_form": "그런식으로 해야할듯요",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040640.1.4",
          "form": "터진사이로 들어간 운 좋은 사람이 잡거나 그래요",
          "original_form": "터진사이로 들어간 운 좋은 사람이 잡거나 그래요",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.5",
          "form": "그게 뭔지 모르겠어요 ㅠ",
          "original_form": "그게 뭔지 모르겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.6",
          "form": "역시 운이 모든걸이기네요",
          "original_form": "역시 운이 모든걸이기네요",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.7",
          "form": "저 1차 맞았는데",
          "original_form": "저 1차 맞았는데",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.8",
          "form": "왜 모를까요 ㅠ",
          "original_form": "왜 모를까요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.9",
          "form": "백신예약할때 서버터질까봐",
          "original_form": "백신예약할때 서버터질까봐",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.10",
          "form": "한사람 인증할때",
          "original_form": "한사람 인증할때",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040640.1.11",
          "form": "인증 튕기면 십분뒤에 해야하는?",
          "original_form": "인증 튕기면 십분뒤에 해야하는?",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.12",
          "form": "오!!!",
          "original_form": "오!!!",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.13",
          "form": "그런식으로 사람 조절했어요",
          "original_form": "그런식으로 사람 조절했어요",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.14",
          "form": "괜찮은 방법이네요",
          "original_form": "괜찮은 방법이네요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.15",
          "form": "근데 튕기면 진짜 서럽겠다..",
          "original_form": "근데 튕기면 진짜 서럽겠다..",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.16",
          "form": "ㅠㅠㅠㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅠㅠㅠㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.17",
          "form": "저는 잔여백신으로 잡아서 몰랐어요",
          "original_form": "저는 잔여백신으로 잡아서 몰랐어요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.18",
          "form": "신기한 제도를 만들었네요",
          "original_form": "신기한 제도를 만들었네요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.19",
          "form": "그니까요 ᆢ 근데 본인인증 그런식으로 하니까",
          "original_form": "그니까요 ᆢ 근데 본인인증 그런식으로 하니까",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040640.1.20",
          "form": "서버 진짜 괜찮았어요",
          "original_form": "서버 진짜 괜찮았어요",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040640.1.21",
          "form": "당장 무조건 잡아야하는게 아니고 나중에 해도 괜찮은거라 그렇게 진행하는것도 좋은 방법 같아요!",
          "original_form": "당장 무조건 잡아야하는게 아니고 나중에 해도 괜찮은거라 그렇게 진행하는것도 좋은 방법 같아요!",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040640.1.22",
          "form": "name1님은 영화 좋아하세요?",
          "original_form": "&name1&님은 영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040640.1.23",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040640.1.24",
          "form": "영화 진짜 가끔 봐요",
          "original_form": "영화 진짜 가끔 봐요",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040640.1.25",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040640.1.26",
          "form": "영화도 좋아해요 넷플로는 가장 최근 본 영화는 줄리앤줄리아라는 영화였고",
          "original_form": "영화도 좋아해요 넷플로는 가장 최근 본 영화는 줄리앤줄리아라는 영화였고",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040640.1.27",
          "form": "영화관에사 본거는 루카 봤어요 애니메이션이요",
          "original_form": "영화관에사 본거는 루카 봤어요 애니메이션이요",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040640.1.28",
          "form": "재밌겠어요",
          "original_form": "재밌겠어요",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040640.1.29",
          "form": "저는 영화도 뮤지컬영화만 주로봐요 ㅎ",
          "original_form": "저는 영화도 뮤지컬영화만 주로봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        }
      ]
