      "utterance": [
        {
          "id": "MDRW2100014214.1.1",
          "form": "지금 비 엄청 오지않아요 ㅠㅠ ?",
          "original_form": "지금 비 엄청 오지않아요 ㅠㅠ ?",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014214.1.2",
          "form": "저희는 흐리고 조금씩 와요ㅠㅠ",
          "original_form": "저희는 흐리고 조금씩 와요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014214.1.3",
          "form": "비 많이 오세요?",
          "original_form": "비 많이 오세요?",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014214.1.4",
          "form": "헐 저희 지역은 지금",
          "original_form": "헐 저희 지역은 지금",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014214.1.5",
          "form": "태풍 오는수준으로 와요 ㅠㅠ",
          "original_form": "태풍 오는수준으로 와요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014214.1.6",
          "form": "헐 조심하세요ㅠㅠ날씨가 이상해서 여름끝물에 비기 많이 오네요ㅠㅠ",
          "original_form": "헐 조심하세요ㅠㅠ날씨가 이상해서 여름끝물에 비기 많이 오네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014214.1.7",
          "form": "맞죠 ㅠㅠ 곧 9월인데 장마인 느낌이네요",
          "original_form": "맞죠 ㅠㅠ 곧 9월인데 장마인 느낌이네요",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014214.1.8",
          "form": "또 비 오면 더워지는건 아니겠죠 ??",
          "original_form": "또 비 오면 더워지는건 아니겠죠 ??",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014214.1.9",
          "form": "근데 후덥지근하지 않으세요?왠지 더울느낌ㅜㅠ",
          "original_form": "근데 후덥지근하지 않으세요?왠지 더울느낌ㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014214.1.10",
          "form": "맞아요 ㅠㅠ 지금 좀 덥기도 하고 습한거같아요",
          "original_form": "맞아요 ㅠㅠ 지금 좀 덥기도 하고 습한거같아요",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014214.1.11",
          "form": "이번 여름이 엄청 더웠는데 ㅠㅠ 그만 더웠으면 좋겠어요",
          "original_form": "이번 여름이 엄청 더웠는데 ㅠㅠ 그만 더웠으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014214.1.12",
          "form": "더운것도 그렇고 습고 높은게 너무 싫어요ㅠㅠ",
          "original_form": "더운것도 그렇고 습고 높은게 너무 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014214.1.13",
          "form": "진짜요 ㅠㅠ 습도 높으면 더 덥게 느껴지고",
          "original_form": "진짜요 ㅠㅠ 습도 높으면 더 덥게 느껴지고",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014214.1.14",
          "form": "그 공기가 너무 싫어요",
          "original_form": "그 공기가 너무 싫어요",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014214.1.15",
          "form": "오늘 제습기 물을 두번갈았네요ㅋㅋ",
          "original_form": "오늘 제습기 물을 두번갈았네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014214.1.16",
          "form": "저도 내일 제습기 틀려구요 !",
          "original_form": "저도 내일 제습기 틀려구요 !",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014214.1.17",
          "form": "오늘 비 이렇게 많이 오는줄 모르고",
          "original_form": "오늘 비 이렇게 많이 오는줄 모르고",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014214.1.18",
          "form": "이미 창고에 넣어버렸어요 ㅋㅋ",
          "original_form": "이미 창고에 넣어버렸어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014214.1.19",
          "form": "저는 선풍기를 넣었어요ㅋㅋ 안해도 될거같구ㅋㅋ",
          "original_form": "저는 선풍기를 넣었어요ㅋㅋ 안해도 될거같구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014214.1.20",
          "form": "아 저는 비오면 또 더워질까봐 아직 안넣었어요 ㅎㅎ",
          "original_form": "아 저는 비오면 또 더워질까봐 아직 안넣었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014214.1.21",
          "form": "날씨 쌀쌀해지면 넣으려구요!",
          "original_form": "날씨 쌀쌀해지면 넣으려구요!",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014214.1.22",
          "form": "저도 또 꺼내야겠어요...ㅎㅎ",
          "original_form": "저도 또 꺼내야겠어요...ㅎㅎ",
          "speaker_id": "2",
