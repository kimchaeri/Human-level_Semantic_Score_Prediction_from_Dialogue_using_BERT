      "utterance": [
        {
          "id": "MDRW2100025502.1.1",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025502.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025502.1.3",
          "form": "점심 식사 하셨나요?",
          "original_form": "점심 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025502.1.4",
          "form": "네네 방금 먹었어요 ㅎㅎ 식사하셨어요?",
          "original_form": "네네 방금 먹었어요 ㅎㅎ 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210901 12:21"
        },
        {
          "id": "MDRW2100025502.1.5",
          "form": "저는 아침을 늦게 먹어서\r\n아직이요:)",
          "original_form": "저는 아침을 늦게 먹어서\r\n아직이요:)",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025502.1.6",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 12:22"
        },
        {
          "id": "MDRW2100025502.1.7",
          "form": "학교에서 학식 먹었어요. 오삼불고기에 순두부찌개요 ㅎㅎ",
          "original_form": "학교에서 학식 먹었어요. 오삼불고기에 순두부찌개요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025502.1.8",
          "form": "그럼 아침에 뭐 드셨어요?",
          "original_form": "그럼 아침에 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025502.1.9",
          "form": "오 학식 너무 그립네요 ㅜㅜㅋㅋ",
          "original_form": "오 학식 너무 그립네요 ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025502.1.10",
          "form": "저는 집밥 먹었어요:)",
          "original_form": "저는 집밥 먹었어요:)",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025502.1.11",
          "form": "요샌 학식 가격대가 어떻게 되나요?",
          "original_form": "요샌 학식 가격대가 어떻게 되나요?",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100025502.1.12",
          "form": "집밥이 좋죠!",
          "original_form": "집밥이 좋죠!",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100025502.1.13",
          "form": "여기는 3500원이에요 ㅎㅎ name2님이 다니실때는 얼마였어요?",
          "original_form": "여기는 3500원이에요 ㅎㅎ &name2&님이 다니실때는 얼마였어요?",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100025502.1.14",
          "form": "김치전 1000원\r\n라면 1500원",
          "original_form": "김치전 1000원\r\n라면 1500원",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100025502.1.15",
          "form": "학식 가격은 비슷했던거 같아여 ㅋㅋ",
          "original_form": "학식 가격은 비슷했던거 같아여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100025502.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100025502.1.17",
          "form": "라면 김치전 겁나 싸네요... 여기 라면3천원..",
          "original_form": "라면 김치전 겁나 싸네요... 여기 라면3천원..",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100025502.1.18",
          "form": "띠로리ㅜㅜ 3천원ㄷㄷ",
          "original_form": "띠로리ㅜㅜ 3천원ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100025502.1.19",
          "form": "메뉴에 돈가스도 있었어요?",
          "original_form": "메뉴에 돈가스도 있었어요?",
          "speaker_id": "1",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100025502.1.20",
          "form": "벌써 10년도 더 지나서\r\n기억이 가물가물한데",
          "original_form": "벌써 10년도 더 지나서\r\n기억이 가물가물한데",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100025502.1.21",
          "form": "없었던거 같아요ㅋㅋ",
          "original_form": "없었던거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100025502.1.22",
          "form": "오늘 메뉴는 돈가스인가영?",
