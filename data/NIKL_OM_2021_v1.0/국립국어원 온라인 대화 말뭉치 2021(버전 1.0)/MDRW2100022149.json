        },
        {
          "id": "MDRW2100022149.1.2",
          "form": "스페인",
          "original_form": "스페인",
          "speaker_id": "2",
          "time": "20210829 09:27"
        },
        {
          "id": "MDRW2100022149.1.3",
          "form": "유럽 쪽",
          "original_form": "유럽 쪽",
          "speaker_id": "2",
          "time": "20210829 09:27"
        },
        {
          "id": "MDRW2100022149.1.4",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "2",
          "time": "20210829 09:27"
        },
        {
          "id": "MDRW2100022149.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022149.1.6",
          "form": "저랑잘통하시네요",
          "original_form": "저랑잘통하시네요",
          "speaker_id": "1",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022149.1.7",
          "form": "부모님 모시고는 스위스 가보고싶어요",
          "original_form": "부모님 모시고는 스위스 가보고싶어요",
          "speaker_id": "1",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022149.1.8",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "2",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100022149.1.9",
          "form": "좋음?",
          "original_form": "좋음?",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022149.1.10",
          "form": "대자연 끝판왕",
          "original_form": "대자연 끝판왕",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022149.1.11",
          "form": "매우깨끗",
          "original_form": "매우깨끗",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022149.1.12",
          "form": "가고싶군요",
          "original_form": "가고싶군요",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022149.1.13",
          "form": "스위스",
          "original_form": "스위스",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100022149.1.14",
          "form": "코로나끝나면 댕겨오세여",
          "original_form": "코로나끝나면 댕겨오세여",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022149.1.15",
          "form": "언제끝나요",
          "original_form": "언제끝나요",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100022149.1.16",
          "form": "코로나여행",
          "original_form": "코로나여행",
          "speaker_id": "2",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022149.1.17",
          "form": "ㅋㅋ그르게여",
          "original_form": "ㅋㅋ그르게여",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022149.1.18",
          "form": "같이 공존해야할듯",
          "original_form": "같이 공존해야할듯",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022149.1.19",
          "form": "잠오네유",
          "original_form": "잠오네유",
          "speaker_id": "1",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022149.1.20",
          "form": "어제 여행하셨음?",
          "original_form": "어제 여행하셨음?",
          "speaker_id": "2",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100022149.1.21",
          "form": "어제 여행안갓어욤",
          "original_form": "어제 여행안갓어욤",
          "speaker_id": "1",
          "time": "20210829 10:06"
        },
        {
          "id": "MDRW2100022149.1.22",
          "form": "여행하면  잠와요",
          "original_form": "여행하면  잠와요",
          "speaker_id": "2",
          "time": "20210829 10:07"
        },
        {
          "id": "MDRW2100022149.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:07"
        },
        {
          "id": "MDRW2100022149.1.24",
          "form": "먼소린가유",
          "original_form": "먼소린가유",
          "speaker_id": "1",
          "time": "20210829 10:07"
        },
        {
          "id": "MDRW2100022149.1.25",
          "form": "누가 여행하면 잠자요",
          "original_form": "누가 여행하면 잠자요",
          "speaker_id": "2",
          "time": "20210829 10:08"
        },
        {
          "id": "MDRW2100022149.1.26",
          "form": "꿈나라 여행",
          "original_form": "꿈나라 여행",
          "speaker_id": "2",
          "time": "20210829 10:08"
        },
        {
          "id": "MDRW2100022149.1.27",
          "form": "ㅋㅋ저는",
          "original_form": "ㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210829 10:08"
        },
        {
          "id": "MDRW2100022149.1.28",
          "form": "돌체랑",
          "original_form": "돌체랑",
          "speaker_id": "1",
          "time": "20210829 10:08"
        },
        {
          "id": "MDRW2100022149.1.29",
          "form": "빵먹엇어욤",
          "original_form": "빵먹엇어욤",
          "speaker_id": "1",
          "time": "20210829 10:08"
        },
        {
          "id": "MDRW2100022149.1.30",
          "form": "식사하셧나유",
          "original_form": "식사하셧나유",
          "speaker_id": "1",
          "time": "20210829 10:08"
        },
        {
          "id": "MDRW2100022149.1.31",
          "form": "돌체?",
          "original_form": "돌체?",
          "speaker_id": "2",
          "time": "20210829 10:09"
        },
        {
          "id": "MDRW2100022149.1.32",
          "form": "여행중이세요 지금?",
          "original_form": "여행중이세요 지금?",
          "speaker_id": "2",
          "time": "20210829 10:09"
        },
        {
          "id": "MDRW2100022149.1.33",
          "form": "스벅타벅스에서",
          "original_form": "스벅타벅스에서",
          "speaker_id": "1",
          "time": "20210829 10:09"
        },
        {
          "id": "MDRW2100022149.1.34",
          "form": "사오셧드라구욤",
          "original_form": "사오셧드라구욤",
          "speaker_id": "1",
          "time": "20210829 10:09"
        },
        {
          "id": "MDRW2100022149.1.35",
          "form": "스벅여행가셨구나",
          "original_form": "스벅여행가셨구나",
          "speaker_id": "2",
          "time": "20210829 10:09"
        },
        {
          "id": "MDRW2100022149.1.36",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.37",
          "form": "좋네요 거기",
          "original_form": "좋네요 거기",
          "speaker_id": "2",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.38",
          "form": "홍냐홍냐함니당",
          "original_form": "홍냐홍냐함니당",
          "speaker_id": "1",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.39",
          "form": "어디여행중이신가요",
          "original_form": "어디여행중이신가요",
          "speaker_id": "1",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.40",
          "form": "침대여해인가유",
          "original_form": "침대여해인가유",
          "speaker_id": "1",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.41",
          "form": "그곳은 침대맛집",
          "original_form": "그곳은 침대맛집",
          "speaker_id": "1",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.42",
          "form": "한국가세요",
          "original_form": "한국가세요",
          "speaker_id": "2",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.43",
          "form": "제주도",
          "original_form": "제주도",
          "speaker_id": "2",
          "time": "20210829 10:10"
        },
        {
          "id": "MDRW2100022149.1.44",
          "form": "한라산",
          "original_form": "한라산",
          "speaker_id": "2",
          "time": "20210829 10:11"
        },
        {
          "id": "MDRW2100022149.1.45",
          "form": "제주도 남자친구랑 가고싶어유",
          "original_form": "제주도 남자친구랑 가고싶어유",
          "speaker_id": "1",
          "time": "20210829 10:11"
        }
      ]
