      "utterance": [
        {
          "id": "MDRW2100041272.1.1",
          "form": "얼마전에 은행 갔을때",
          "original_form": "얼마전에 은행 갔을때",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100041272.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.3",
          "form": "여행적금(?) 그런거 있는것 같던데",
          "original_form": "여행적금(?) 그런거 있는것 같던데",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.4",
          "form": "좀 알아봐야겠다",
          "original_form": "좀 알아봐야겠다",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.6",
          "form": "아아아 그것도있던데",
          "original_form": "아아아 그것도있던데",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.7",
          "form": "나도 대충 봐서",
          "original_form": "나도 대충 봐서",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.8",
          "form": "뭐뭐???",
          "original_form": "뭐뭐???",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.9",
          "form": "청년희망적금?",
          "original_form": "청년희망적금?",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.10",
          "form": "오 그건 뭐야?",
          "original_form": "오 그건 뭐야?",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.11",
          "form": "청년내일? 어쩌구 하는 이름이었는데",
          "original_form": "청년내일? 어쩌구 하는 이름이었는데",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.12",
          "form": "근데 청년 들어가는거",
          "original_form": "근데 청년 들어가는거",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.13",
          "form": "은근 매달 소득이 있어야 하는",
          "original_form": "은근 매달 소득이 있어야 하는",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041272.1.14",
          "form": "맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.15",
          "form": "그런것들이 많아서..",
          "original_form": "그런것들이 많아서..",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.16",
          "form": "근로소득있어야 신청가능함",
          "original_form": "근로소득있어야 신청가능함",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.17",
          "form": "쨋든 그",
          "original_form": "쨋든 그",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.18",
          "form": "약간 취직한 26살~ 이정도",
          "original_form": "약간 취직한 26살~ 이정도",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.19",
          "form": "사람들을 위한것 같음 ㅠ",
          "original_form": "사람들을 위한것 같음 ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.20",
          "form": "매달 10만원씩 내가 통장에 돈넣으면",
          "original_form": "매달 10만원씩 내가 통장에 돈넣으면",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.21",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.22",
          "form": "정부가 10만원씩 지원해주는거임",
          "original_form": "정부가 10만원씩 지원해주는거임",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.23",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.24",
          "form": "그래서 한 2~3년뒤에 천몇백정도 만들수있게",
          "original_form": "그래서 한 2~3년뒤에 천몇백정도 만들수있게",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.25",
          "form": "금리도 괜찮고",
          "original_form": "금리도 괜찮고",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041272.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.27",
          "form": "좀 더 알아봐야겠네",
          "original_form": "좀 더 알아봐야겠네",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.28",
          "form": "그게 지역별로 하는거같은데",
          "original_form": "그게 지역별로 하는거같은데",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.29",
          "form": "부산은 작년말에 한번 신청받았고",
          "original_form": "부산은 작년말에 한번 신청받았고",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.30",
          "form": "올해는 아직 모집예정이더라",
          "original_form": "올해는 아직 모집예정이더라",
          "speaker_id": "1",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.31",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.32",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041272.1.33",
          "form": "딱 대기타고 있는다",
          "original_form": "딱 대기타고 있는다",
          "speaker_id": "2",
          "time": "20210905 21:14"
        }
      ]
    }
