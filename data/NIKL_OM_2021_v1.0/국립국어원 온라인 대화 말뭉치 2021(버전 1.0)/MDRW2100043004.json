      "utterance": [
        {
          "id": "MDRW2100043004.1.1",
          "form": "최근에 사신 것 중에 가장 만족스러우셨던게 무엇인가요?",
          "original_form": "최근에 사신 것 중에 가장 만족스러우셨던게 무엇인가요?",
          "speaker_id": "1",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100043004.1.2",
          "form": "저 이번에 토스트기를",
          "original_form": "저 이번에 토스트기를",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100043004.1.3",
          "form": "당근마켓에서 샀는데 ㄹㅇ인생이 윤택해졌어요",
          "original_form": "당근마켓에서 샀는데 ㄹㅇ인생이 윤택해졌어요",
          "speaker_id": "2",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043004.1.4",
          "form": "name1님은 어떠세요",
          "original_form": "&name1&님은 어떠세요",
          "speaker_id": "2",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043004.1.5",
          "form": "와 토스트기",
          "original_form": "와 토스트기",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043004.1.6",
          "form": "저 살려고 간보고있었는데 많이 좋나요?",
          "original_form": "저 살려고 간보고있었는데 많이 좋나요?",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100043004.1.7",
          "form": "저는 뭘샀지 산건 많은데 다 오고있어요 지금",
          "original_form": "저는 뭘샀지 산건 많은데 다 오고있어요 지금",
          "speaker_id": "1",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100043004.1.8",
          "form": "아 최근에 산거중에 젤 비싼건 갤럭시워치4요 ㅋㅋㅋ",
          "original_form": "아 최근에 산거중에 젤 비싼건 갤럭시워치4요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100043004.1.9",
          "form": "ㅋㅋㅋㅋㄱ토스트 필립스껀데 ㄹㅇ괜찮습니다 빵이 바삭하고 편해요",
          "original_form": "ㅋㅋㅋㅋㄱ토스트 필립스껀데 ㄹㅇ괜찮습니다 빵이 바삭하고 편해요",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100043004.1.10",
          "form": "갤럭시워치 4어때요",
          "original_form": "갤럭시워치 4어때요",
          "speaker_id": "2",
          "time": "20210907 13:01"
        },
        {
          "id": "MDRW2100043004.1.11",
          "form": "저도 봤는데 좀탐나던데ㅠ",
          "original_form": "저도 봤는데 좀탐나던데ㅠ",
          "speaker_id": "2",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100043004.1.12",
          "form": "사실 솔직히말하면 비싼 갤핏같아요",
          "original_form": "사실 솔직히말하면 비싼 갤핏같아요",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100043004.1.13",
          "form": "갤핏쓰다가 넘어온건데 갤핏이 너무 잘나와서 그런지ㅋㅋㅋㅋ",
          "original_form": "갤핏쓰다가 넘어온건데 갤핏이 너무 잘나와서 그런지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100043004.1.14",
          "form": "ㅋㅋㅋㅋㄱ아ㅠ단호하고 객관적인후기",
          "original_form": "ㅋㅋㅋㅋㄱ아ㅠ단호하고 객관적인후기",
          "speaker_id": "2",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100043004.1.15",
          "form": "지갑다시 닫았어요",
          "original_form": "지갑다시 닫았어요",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043004.1.16",
          "form": "워치4가 그 인바디?되잖아요",
          "original_form": "워치4가 그 인바디?되잖아요",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043004.1.17",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043004.1.18",
          "form": "아 물론이죠",
          "original_form": "아 물론이죠",
          "speaker_id": "1",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043004.1.19",
          "form": "한번하고 그 워치 화면에서 바로 안보이게 해놨어요",
          "original_form": "한번하고 그 워치 화면에서 바로 안보이게 해놨어요",
          "speaker_id": "1",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043004.1.20",
          "form": "체지방률 들킬까봐 어휴",
          "original_form": "체지방률 들킬까봐 어휴",
          "speaker_id": "1",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100043004.1.21",
          "form": "ㅋㅋㅋㅋㄱㅋㄱㄱ아ㅠ개웃겨 어떻게 나왓길래ㅠ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱㄱ아ㅠ개웃겨 어떻게 나왓길래ㅠ",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100043004.1.22",
          "form": "워치는 잘못이없죠....",
          "original_form": "워치는 잘못이없죠....",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043004.1.23",
          "form": "죄가 있다면 사실대로 측정했다는거 정도....",
          "original_form": "죄가 있다면 사실대로 측정했다는거 정도....",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043004.1.24",
          "form": "눈치껏 낮췄어야지",
          "original_form": "눈치껏 낮췄어야지",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043004.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱㄱ그럼 안되는거잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱㄱ그럼 안되는거잖아요",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043004.1.26",
          "form": "워치4  정확하게 재는군요..",
          "original_form": "워치4  정확하게 재는군요..",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043004.1.27",
          "form": "네네.. 그래도 예쁘니까 봐줬어요",
          "original_form": "네네.. 그래도 예쁘니까 봐줬어요",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043004.1.28",
          "form": "스트랩 바꿨더니 얼마나 귀엽던지....",
          "original_form": "스트랩 바꿨더니 얼마나 귀엽던지....",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043004.1.29",
          "form": "아 저 어제 닥마 샀는데 혹시 신어보셨어요?",
          "original_form": "아 저 어제 닥마 샀는데 혹시 신어보셨어요?",
