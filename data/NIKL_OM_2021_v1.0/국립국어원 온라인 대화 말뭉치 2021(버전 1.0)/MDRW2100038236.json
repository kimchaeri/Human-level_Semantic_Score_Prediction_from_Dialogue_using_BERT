      "utterance": [
        {
          "id": "MDRW2100038236.1.1",
          "form": "응 할껀다함",
          "original_form": "응 할껀다함",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038236.1.2",
          "form": "안한다면서 계속 뭘 할려고 하는 스타일",
          "original_form": "안한다면서 계속 뭘 할려고 하는 스타일",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100038236.1.3",
          "form": "맞아 책임감인가",
          "original_form": "맞아 책임감인가",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038236.1.4",
          "form": "그럴수도 잇지 경영에 대한 ㅋㅋㅋ",
          "original_form": "그럴수도 잇지 경영에 대한 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038236.1.5",
          "form": "욕망의 이서진이다",
          "original_form": "욕망의 이서진이다",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038236.1.6",
          "form": "열심히 잘 할려고 하는게 잇어서 재밌음",
          "original_form": "열심히 잘 할려고 하는게 잇어서 재밌음",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100038236.1.7",
          "form": "사람이 없네",
          "original_form": "사람이 없네",
          "speaker_id": "2",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038236.1.8",
          "form": "저러다가 금방 손님이 온다",
          "original_form": "저러다가 금방 손님이 온다",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038236.1.9",
          "form": "첫영업인데 손님이 많은것도 힘들지",
          "original_form": "첫영업인데 손님이 많은것도 힘들지",
          "speaker_id": "2",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038236.1.10",
          "form": "아까 생수 첫손님 있었음",
          "original_form": "아까 생수 첫손님 있었음",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100038236.1.11",
          "form": "그건 손님이라하기에는",
          "original_form": "그건 손님이라하기에는",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038236.1.12",
          "form": "왜 팁까지 주고 가셨는데",
          "original_form": "왜 팁까지 주고 가셨는데",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038236.1.13",
          "form": "그래 니말이 맞다",
          "original_form": "그래 니말이 맞다",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100038236.1.14",
          "form": "영업욕망이 불타오른데 ㅋㅋ윤사장님",
          "original_form": "영업욕망이 불타오른데 ㅋㅋ윤사장님",
