      "utterance": [
        {
          "id": "MDRW2100028125.1.1",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028125.1.2",
          "form": "잘 자는 고영",
          "original_form": "잘 자는 고영",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100028125.1.3",
          "form": "그치만 빛 쫓아다니면 정말 곤란해",
          "original_form": "그치만 빛 쫓아다니면 정말 곤란해",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028125.1.4",
          "form": "엄청 높은 데 있는 것도",
          "original_form": "엄청 높은 데 있는 것도",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028125.1.5",
          "form": "잡으려고 채터링하고",
          "original_form": "잡으려고 채터링하고",
          "speaker_id": "1",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028125.1.6",
          "form": "아이고애기야",
          "original_form": "아이고애기야",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028125.1.7",
          "form": "그거빛이야",
          "original_form": "그거빛이야",
          "speaker_id": "2",
          "time": "20210902 17:44"
        },
        {
          "id": "MDRW2100028125.1.8",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.9",
          "form": "특히 핸드폰 카메란가 반사된 빛이",
          "original_form": "특히 핸드폰 카메란가 반사된 빛이",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.10",
          "form": "내가 움직이니까 엄청 빨리 움직여서",
          "original_form": "내가 움직이니까 엄청 빨리 움직여서",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.11",
          "form": "아놔ㅠㅠ",
          "original_form": "아놔ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.12",
          "form": "이 시간대에 걍 핸드폰 안 씀",
          "original_form": "이 시간대에 걍 핸드폰 안 씀",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.13",
          "form": "극단적이야",
          "original_form": "극단적이야",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.14",
          "form": "나에겐 컴퓨터가 있다",
          "original_form": "나에겐 컴퓨터가 있다",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100028125.1.15",
          "form": "고영 저번에",
          "original_form": "고영 저번에",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028125.1.16",
          "form": "벌레 잡는다고 거기 매달린 거",
          "original_form": "벌레 잡는다고 거기 매달린 거",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028125.1.17",
          "form": "안 떨어졌니",
          "original_form": "안 떨어졌니",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028125.1.18",
          "form": "웃긴데 걱정됏음",
          "original_form": "웃긴데 걱정됏음",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028125.1.19",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100028125.1.20",
          "form": "그땐 내가 잡아줘서",
          "original_form": "그땐 내가 잡아줘서",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.21",
          "form": "근데 떨어져도 안 다치긴 함",
          "original_form": "근데 떨어져도 안 다치긴 함",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.22",
          "form": "알아서 캣타워로 잘 돌아오던데",
          "original_form": "알아서 캣타워로 잘 돌아오던데",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.23",
          "form": "운동신경 좋은 고영...",
          "original_form": "운동신경 좋은 고영...",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.24",
          "form": "맨날 옷장 위에도 올라가려고 하는데",
          "original_form": "맨날 옷장 위에도 올라가려고 하는데",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.25",
          "form": "막아뒀거든",
          "original_form": "막아뒀거든",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.27",
          "form": "매달렸다가 알아서 떨어짐 걍",
          "original_form": "매달렸다가 알아서 떨어짐 걍",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100028125.1.28",
          "form": "ㅠㅠ귀여워",
          "original_form": "ㅠㅠ귀여워",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.29",
          "form": "옷장 위에... 안 떨어지게 처리만 잘 해주면",
          "original_form": "옷장 위에... 안 떨어지게 처리만 잘 해주면",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.30",
          "form": "높아서... 좋아할 것 같은데",
          "original_form": "높아서... 좋아할 것 같은데",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.31",
          "form": "아직도 어케 해야할지 해답을 찾지 못한",
          "original_form": "아직도 어케 해야할지 해답을 찾지 못한",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.32",
          "form": "캣타워보다 높지",
          "original_form": "캣타워보다 높지",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.33",
          "form": "당근",
          "original_form": "당근",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.34",
          "form": "걍 천장에 닿지",
          "original_form": "걍 천장에 닿지",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100028125.1.35",
          "form": "아니 근데 안 떨어지게 하려면",
          "original_form": "아니 근데 안 떨어지게 하려면",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.36",
          "form": "공사장에서 쓰는 그물같은 거 달아줘야하는 거 아니냐",
          "original_form": "공사장에서 쓰는 그물같은 거 달아줘야하는 거 아니냐",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.37",
          "form": "걍 옷장실격",
          "original_form": "걍 옷장실격",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.38",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.39",
          "form": "아니면 아크릴판으로 막아볼까 싶기도 한데",
          "original_form": "아니면 아크릴판으로 막아볼까 싶기도 한데",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.40",
          "form": "너무 번거로워",
          "original_form": "너무 번거로워",
          "speaker_id": "1",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.41",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.42",
          "form": "아크릴 또 비쌀텐데",
          "original_form": "아크릴 또 비쌀텐데",
          "speaker_id": "2",
          "time": "20210902 17:49"
        },
        {
          "id": "MDRW2100028125.1.43",
