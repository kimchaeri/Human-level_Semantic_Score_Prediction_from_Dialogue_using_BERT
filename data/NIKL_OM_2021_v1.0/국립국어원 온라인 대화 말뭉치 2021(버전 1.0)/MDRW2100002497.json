      "utterance": [
        {
          "id": "MDRW2100002497.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002497.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002497.1.3",
          "form": "책좋아하시나요?",
          "original_form": "책좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002497.1.4",
          "form": "네~ 여담이지만 초~고 다독상 안놓치고 탈정도로",
          "original_form": "네~ 여담이지만 초~고 다독상 안놓치고 탈정도로",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002497.1.5",
          "form": "책 엄청 좋아해요",
          "original_form": "책 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002497.1.6",
          "form": "와우 고등학생때까지 다독상이시라니",
          "original_form": "와우 고등학생때까지 다독상이시라니",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002497.1.7",
          "form": "좋아하시는 장르있나요?",
          "original_form": "좋아하시는 장르있나요?",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002497.1.8",
          "form": "저는 다 읽는 편인데",
          "original_form": "저는 다 읽는 편인데",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002497.1.9",
          "form": "좋아하는 장르는 과학이랑 스릴러쪽이네요!",
          "original_form": "좋아하는 장르는 과학이랑 스릴러쪽이네요!",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002497.1.10",
          "form": "그리고 셜록 홈즈같은 탐정물도 좋아해요",
          "original_form": "그리고 셜록 홈즈같은 탐정물도 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002497.1.11",
          "form": "와 과학이라니.. 대박이네요ㅋㅋㅋ저와는 거리가 먼",
          "original_form": "와 과학이라니.. 대박이네요ㅋㅋㅋ저와는 거리가 먼",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002497.1.12",
          "form": "스릴러도 ㅎㅎㅎ",
          "original_form": "스릴러도 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002497.1.13",
          "form": "저는 책안읽은지 1년정도 된것같네요..!",
          "original_form": "저는 책안읽은지 1년정도 된것같네요..!",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002497.1.14",
          "form": "요즘같이 집에 머무를때",
          "original_form": "요즘같이 집에 머무를때",
          "speaker_id": "2",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002497.1.15",
          "form": "책읽는거 한번 추천드려요!",
          "original_form": "책읽는거 한번 추천드려요!",
          "speaker_id": "2",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002497.1.16",
          "form": "시간 빨리가면서 좋아요!",
          "original_form": "시간 빨리가면서 좋아요!",
          "speaker_id": "2",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002497.1.17",
          "form": "추천하실만한책",
          "original_form": "추천하실만한책",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002497.1.18",
          "form": "있으신가요?!",
          "original_form": "있으신가요?!",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002497.1.19",
          "form": "게이고라는 작가분이 계신데",
          "original_form": "게이고라는 작가분이 계신데",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002497.1.20",
          "form": "풀네임은 기억이 안나구",
          "original_form": "풀네임은 기억이 안나구",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002497.1.21",
          "form": "게이고님 중에 편지라는 작품이 있어요!",
          "original_form": "게이고님 중에 편지라는 작품이 있어요!",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002497.1.22",
          "form": "그거 감동적인데 꼭꼭 보시는거 추천드립니다!",
          "original_form": "그거 감동적인데 꼭꼭 보시는거 추천드립니다!",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002497.1.23",
          "form": "ㅠㅠ 슬픈거군요 슬픈거말고",
          "original_form": "ㅠㅠ 슬픈거군요 슬픈거말고",
          "speaker_id": "1",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002497.1.24",
          "form": "다른건없을까요?",
          "original_form": "다른건없을까요?",
          "speaker_id": "1",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002497.1.25",
          "form": "그러면 흔히 잘 알려져있는",
          "original_form": "그러면 흔히 잘 알려져있는",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002497.1.26",
          "form": "해리포터나 헝거게임, 트와일라잇 시리즈 한번 찾아서 보는것도 좋고",
          "original_form": "해리포터나 헝거게임, 트와일라잇 시리즈 한번 찾아서 보는것도 좋고",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002497.1.27",
          "form": "게이고님의 용의자 x 헌신도 좋아요!",
          "original_form": "게이고님의 용의자 x 헌신도 좋아요!",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002497.1.28",
          "form": "해리포터랑 트와일라잇은 봤네요ㅎㅎ 용의자x도요!",
          "original_form": "해리포터랑 트와일라잇은 봤네요ㅎㅎ 용의자x도요!",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002497.1.29",
          "form": "헝거게임도비슷한 류인가요?",
          "original_form": "헝거게임도비슷한 류인가요?",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002497.1.30",
          "form": "헝거게임이 살짝 메이즈러너?같은 느낌이에요",
          "original_form": "헝거게임이 살짝 메이즈러너?같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002497.1.31",
          "form": "일종의 게임 형식으로 진행되는 그런 스토리인데",
          "original_form": "일종의 게임 형식으로 진행되는 그런 스토리인데",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002497.1.32",
          "form": "볼만하더라고요!",
          "original_form": "볼만하더라고요!",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002497.1.33",
          "form": "오 게임형식스토리의 책이군요 신박하네요",
          "original_form": "오 게임형식스토리의 책이군요 신박하네요",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002497.1.34",
          "form": "제목은 들어본것같아요 유명해서..!",
          "original_form": "제목은 들어본것같아요 유명해서..!",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002497.1.35",
          "form": "영화로도 나와서 유명해요!",
          "original_form": "영화로도 나와서 유명해요!",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002497.1.36",
          "form": "메이즈러너도 그렇고요!",
          "original_form": "메이즈러너도 그렇고요!",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002497.1.37",
          "form": "아 영화로도 나왔군요ㅋㅋㅋ 좋네요",
          "original_form": "아 영화로도 나왔군요ㅋㅋㅋ 좋네요",
          "speaker_id": "1",
          "time": "20210820 13:24"
        }
      ]
