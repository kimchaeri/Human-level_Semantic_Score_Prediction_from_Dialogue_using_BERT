      "utterance": [
        {
          "id": "MDRW2100044753.1.1",
          "form": "팥빵 좋아함?",
          "original_form": "팥빵 좋아함?",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044753.1.2",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044753.1.3",
          "form": "앙버터면 더 좋죠",
          "original_form": "앙버터면 더 좋죠",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044753.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ팥을 좋아함?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ팥을 좋아함?",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044753.1.5",
          "form": "ㅇㅇ그런 편인듯",
          "original_form": "ㅇㅇ그런 편인듯",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044753.1.6",
          "form": "팥죽도 잘 먹어여",
          "original_form": "팥죽도 잘 먹어여",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044753.1.7",
          "form": "팥 싫어함?",
          "original_form": "팥 싫어함?",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044753.1.8",
          "form": "ㅇㅇ별로안조아함 넘달음",
          "original_form": "ㅇㅇ별로안조아함 넘달음",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044753.1.9",
          "form": "앙버터는 먹음 ㅋㅋㅋ",
          "original_form": "앙버터는 먹음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044753.1.10",
          "form": "아하 팥 안달게 하면 씁닼ㅋㅋㅋ",
          "original_form": "아하 팥 안달게 하면 씁닼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044753.1.11",
          "form": "그럼 무슨 빵 좋아하니",
          "original_form": "그럼 무슨 빵 좋아하니",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044753.1.12",
          "form": "오 빵은 일단 다좋아함",
          "original_form": "오 빵은 일단 다좋아함",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044753.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044753.1.14",
          "form": "그냥 팥빵만 잘 안먹는거가텨",
          "original_form": "그냥 팥빵만 잘 안먹는거가텨",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044753.1.15",
          "form": "니는 안먹는빵잇서?",
          "original_form": "니는 안먹는빵잇서?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044753.1.16",
          "form": "ㅋㅋㅋㅋㅋ그겈 알지",
          "original_form": "ㅋㅋㅋㅋㅋ그겈 알지",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044753.1.17",
          "form": "안먹는빵? 어려은디",
          "original_form": "안먹는빵? 어려은디",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044753.1.18",
          "form": "안먹는빵도 잇나",
          "original_form": "안먹는빵도 잇나",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044753.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하긴 다맛잇어.ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ하긴 다맛잇어.ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044753.1.20",
          "form": "비린거 잘뭄?",
          "original_form": "비린거 잘뭄?",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044753.1.21",
          "form": "노...비린거 힘긂",
          "original_form": "노...비린거 힘긂",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044753.1.22",
          "form": "잘 먹니",
          "original_form": "잘 먹니",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044753.1.23",
          "form": "완전 못먹음",
          "original_form": "완전 못먹음",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044753.1.24",
          "form": "비위가약하나봐",
          "original_form": "비위가약하나봐",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044753.1.25",
          "form": "ㅋㅋㅋ비린 것도 문젠데",
          "original_form": "ㅋㅋㅋ비린 것도 문젠데",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044753.1.26",
          "form": "그런 애들은 대체로 식감도 문제",
          "original_form": "그런 애들은 대체로 식감도 문제",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044753.1.27",
          "form": "몰캉쓰?",
          "original_form": "몰캉쓰?",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044753.1.28",
          "form": "다 잘먹고싶더",
          "original_form": "다 잘먹고싶더",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044753.1.29",
          "form": "그럼 맛잇는걸 더 많이먹을거아니여",
          "original_form": "그럼 맛잇는걸 더 많이먹을거아니여",
          "speaker_id": "1",
          "time": "20210907 23:46"
        }
      ]
    }
