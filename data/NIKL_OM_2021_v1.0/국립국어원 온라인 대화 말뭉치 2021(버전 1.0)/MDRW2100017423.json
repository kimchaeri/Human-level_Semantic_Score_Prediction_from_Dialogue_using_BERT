        {
          "id": "MDRW2100017423.1.2",
          "form": "결혼중",
          "original_form": "결혼중",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017423.1.3",
          "form": "7월에 결혼함",
          "original_form": "7월에 결혼함",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017423.1.4",
          "form": "ㅋㄱㅋㅋㄱㄱㄲ",
          "original_form": "ㅋㄱㅋㅋㄱㄱㄲ",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017423.1.5",
          "form": "님은 연애중?",
          "original_form": "님은 연애중?",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017423.1.6",
          "form": "난 내년 결혼예정",
          "original_form": "난 내년 결혼예정",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017423.1.7",
          "form": "오 추카",
          "original_form": "오 추카",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017423.1.8",
          "form": "결혼식을 내년에?",
          "original_form": "결혼식을 내년에?",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017423.1.9",
          "form": "코시국때메 미뤗음",
          "original_form": "코시국때메 미뤗음",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017423.1.10",
          "form": "나도 결혼식은 못함 아직",
          "original_form": "나도 결혼식은 못함 아직",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017423.1.11",
          "form": "결혼식은 내년 \r\n신행은 더 미뤄야겟쥐....",
          "original_form": "결혼식은 내년 \r\n신행은 더 미뤄야겟쥐....",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017423.1.12",
          "form": "나도 결혼식 내년 예정이긴 한데 모르겠음",
          "original_form": "나도 결혼식 내년 예정이긴 한데 모르겠음",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017423.1.13",
          "form": "식에 드는 비용 넘 아까워",
          "original_form": "식에 드는 비용 넘 아까워",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017423.1.14",
          "form": "그돈으로 비지니스타구시파",
          "original_form": "그돈으로 비지니스타구시파",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017423.1.15",
          "form": "많이들어감?",
          "original_form": "많이들어감?",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017423.1.16",
          "form": "내가 드레스 뭐 이런 로망이없어서",
          "original_form": "내가 드레스 뭐 이런 로망이없어서",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017423.1.17",
          "form": "걍 귀차나",
          "original_form": "걍 귀차나",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017423.1.18",
          "form": "드레스도 추가비용 엄청든다며",
          "original_form": "드레스도 추가비용 엄청든다며",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017423.1.19",
          "form": "근데 부모님땜시 해야함",
          "original_form": "근데 부모님땜시 해야함",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017423.1.20",
          "form": "나도 결혼식은 귀찮아",
          "original_form": "나도 결혼식은 귀찮아",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017423.1.21",
          "form": "엄청 귀차너",
          "original_form": "엄청 귀차너",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017423.1.22",
          "form": "촬영도 그렇고",
          "original_form": "촬영도 그렇고",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017423.1.23",
          "form": "마쟈마쟈",
          "original_form": "마쟈마쟈",
