      "utterance": [
        {
          "id": "MDRW2100033886.1.1",
          "form": "뒷내용이 궁금한데 못봐서ㅋㅋ",
          "original_form": "뒷내용이 궁금한데 못봐서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100033886.1.2",
          "form": "그죠 아 뒷내용 항시젤궁금요",
          "original_form": "그죠 아 뒷내용 항시젤궁금요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033886.1.3",
          "form": "저도 다시 봐야하는데",
          "original_form": "저도 다시 봐야하는데",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100033886.1.4",
          "form": "갑자기 아리까리",
          "original_form": "갑자기 아리까리",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100033886.1.5",
          "form": "해서 ㅋㅋ",
          "original_form": "해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100033886.1.6",
          "form": "맨날 그 드라마 볼때마다 범인 변하고 했죠 ㅋㅋ",
          "original_form": "맨날 그 드라마 볼때마다 범인 변하고 했죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100033886.1.7",
          "form": "ㅋㅋ맞아요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.8",
          "form": "남궁민은",
          "original_form": "남궁민은",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.9",
          "form": "아 급 궁금그 나이많은 여자직원",
          "original_form": "아 급 궁금그 나이많은 여자직원",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.10",
          "form": "착한사람 같던데",
          "original_form": "착한사람 같던데",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.11",
          "form": "이였던거 같은데",
          "original_form": "이였던거 같은데",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.13",
          "form": "아빠땜시복수",
          "original_form": "아빠땜시복수",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033886.1.14",
          "form": "오해했죠",
          "original_form": "오해했죠",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033886.1.15",
          "form": "그쵸 어릴때 이호텔에서  죽었다는거",
          "original_form": "그쵸 어릴때 이호텔에서  죽었다는거",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033886.1.16",
          "form": "거기까지 봤었어요",
          "original_form": "거기까지 봤었어요",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033886.1.17",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033886.1.18",
          "form": "범인이 ㅜㅜ",
          "original_form": "범인이 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033886.1.19",
          "form": "불쌍해요",
          "original_form": "불쌍해요",
          "speaker_id": "2",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100033886.1.20",
          "form": "범인아시죠?",
          "original_form": "범인아시죠?",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033886.1.21",
          "form": "아빠범인 ?",
          "original_form": "아빠범인 ?",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033886.1.22",
          "form": "범인이 헉입니다",
          "original_form": "범인이 헉입니다",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033886.1.23",
          "form": "참쏘였어요 현재범인",
          "original_form": "참쏘였어요 현재범인",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033886.1.24",
          "form": "과거범인",
          "original_form": "과거범인",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100033886.1.25",
          "form": "아직 못봐서",
          "original_form": "아직 못봐서",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033886.1.26",
          "form": "몰라요ㅋㅋㅋ",
          "original_form": "몰라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033886.1.27",
          "form": "그이후 못보고 있었어요",
          "original_form": "그이후 못보고 있었어요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033886.1.28",
          "form": "참쏘는 누구예요?!",
          "original_form": "참쏘는 누구예요?!",
          "speaker_id": "1",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100033886.1.29",
          "form": "디테일이 생각 안나네요",
          "original_form": "디테일이 생각 안나네요",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100033886.1.30",
          "form": "아빠는 엄마가 죽였고",
          "original_form": "아빠는 엄마가 죽였고",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100033886.1.31",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.32",
          "form": "이게 정말 미스테리 드라마에요",
          "original_form": "이게 정말 미스테리 드라마에요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.33",
          "form": "맨날 이놈저놈 ㅋㅋ",
          "original_form": "맨날 이놈저놈 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.34",
          "form": "엄마는 아직 한번도 발언한적이 없었는데",
          "original_form": "엄마는 아직 한번도 발언한적이 없었는데",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.35",
          "form": "그러니 기억이 안나네요",
          "original_form": "그러니 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.36",
          "form": "다시봐야죠",
          "original_form": "다시봐야죠",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.37",
          "form": "그 나이많은...",
          "original_form": "그 나이많은...",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.38",
          "form": "엄마가바람나서",
          "original_form": "엄마가바람나서",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.39",
          "form": "얼음 조각하던",
          "original_form": "얼음 조각하던",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.40",
          "form": "아저씨는 조폭같던데 죽었잖아요",
          "original_form": "아저씨는 조폭같던데 죽었잖아요",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.41",
          "form": "의문사",
          "original_form": "의문사",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033886.1.42",
          "form": "뭔살인이 많아요 ㅋㅋ",
          "original_form": "뭔살인이 많아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033886.1.43",
          "form": "초창기 개국드라마라",
          "original_form": "초창기 개국드라마라",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033886.1.44",
          "form": "보면 넘잼나요 ㅋㅋ",
          "original_form": "보면 넘잼나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100033886.1.45",
          "form": "저도 까먹었는데 꼭보고 알려주세요 ㅎㅎ",
          "original_form": "저도 까먹었는데 꼭보고 알려주세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033886.1.46",
          "form": "전 티빙이 없어요",
          "original_form": "전 티빙이 없어요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033886.1.47",
          "form": "tvn은보면 드라마가 다 좋았러요",
          "original_form": "tvn은보면 드라마가 다 좋았러요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100033886.1.48",
          "form": "요새 좀 시들하지 만요",
          "original_form": "요새 좀 시들하지 만요",
