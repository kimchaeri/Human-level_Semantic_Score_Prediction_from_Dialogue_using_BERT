      "utterance": [
        {
          "id": "MDRW2100016484.1.1",
          "form": "ㅎㅎ 역시 일기예보 예상이 빗나갔군",
          "original_form": "ㅎㅎ 역시 일기예보 예상이 빗나갔군",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016484.1.2",
          "form": "비 올 확률이 60프로니까 안올 확률도 40프로로 꽤 높아 ㅋㅋ",
          "original_form": "비 올 확률이 60프로니까 안올 확률도 40프로로 꽤 높아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016484.1.3",
          "form": "ㅋㅋㅋ 맞는 얘기네",
          "original_form": "ㅋㅋㅋ 맞는 얘기네",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016484.1.4",
          "form": "신기하게 해뜨니까 바로 매미울기 시작한다",
          "original_form": "신기하게 해뜨니까 바로 매미울기 시작한다",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016484.1.5",
          "form": "오늘도 산책할 수 있겠네",
          "original_form": "오늘도 산책할 수 있겠네",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100016484.1.6",
          "form": "ㅇㅇ 바람도 시원하고 오늘 산책하기 딱인듯",
          "original_form": "ㅇㅇ 바람도 시원하고 오늘 산책하기 딱인듯",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016484.1.7",
          "form": "오후에 애들이랑 한바퀴 돌고 와 ㅎㅎ",
          "original_form": "오후에 애들이랑 한바퀴 돌고 와 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016484.1.8",
          "form": "습도가 높은데도 오늘은 바람이 불어서 그런지 시원해",
          "original_form": "습도가 높은데도 오늘은 바람이 불어서 그런지 시원해",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016484.1.9",
          "form": "집 안 온도 몇 도야?",
          "original_form": "집 안 온도 몇 도야?",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016484.1.10",
          "form": "지금 26도",
          "original_form": "지금 26도",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016484.1.11",
          "form": "한낮인데도 진짜 시원해지긴 했네",
          "original_form": "한낮인데도 진짜 시원해지긴 했네",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016484.1.12",
          "form": "곧 긴팔 입어야 할 것 같아 ㅎㅎ",
          "original_form": "곧 긴팔 입어야 할 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016484.1.13",
          "form": "ㅎㅎ 난 아직 아니야",
          "original_form": "ㅎㅎ 난 아직 아니야",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016484.1.14",
          "form": "저녁쯤 되면 긴 팔 입어도 덥지 않고 따뜻해 ㅎㅎ",
          "original_form": "저녁쯤 되면 긴 팔 입어도 덥지 않고 따뜻해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100016484.1.15",
          "form": "이제 진짜 가을이구나~~",
          "original_form": "이제 진짜 가을이구나~~",
