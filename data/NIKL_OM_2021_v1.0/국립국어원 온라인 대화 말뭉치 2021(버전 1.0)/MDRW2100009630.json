      "utterance": [
        {
          "id": "MDRW2100009630.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ로판이었어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ로판이었어욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009630.1.2",
          "form": "비엘은 북부대공<으로 홍보하는건 못본거같아요 제 탐라만 그런가?",
          "original_form": "비엘은 북부대공<으로 홍보하는건 못본거같아요 제 탐라만 그런가?",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009630.1.3",
          "form": "로판이면",
          "original_form": "로판이면",
          "speaker_id": "1",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009630.1.4",
          "form": "혹시 남편이 미모를 숨김?",
          "original_form": "혹시 남편이 미모를 숨김?",
          "speaker_id": "1",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009630.1.5",
          "form": "아 씁 맞아요",
          "original_form": "아 씁 맞아요",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009630.1.6",
          "form": "님 로판도 보세요?!?!",
          "original_form": "님 로판도 보세요?!?!",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009630.1.7",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009630.1.8",
          "form": "다 먹어요",
          "original_form": "다 먹어요",
          "speaker_id": "1",
          "time": "20210823 02:02"
        },
        {
          "id": "MDRW2100009630.1.9",
          "form": "로판 추천해주세요",
          "original_form": "로판 추천해주세요",
          "speaker_id": "1",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.10",
          "form": "아놬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아놬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.11",
          "form": "엔딩 후 서브남을 주웠다",
          "original_form": "엔딩 후 서브남을 주웠다",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.12",
          "form": "이거 진짜....",
          "original_form": "이거 진짜....",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.13",
          "form": "아 보죠",
          "original_form": "아 보죠",
          "speaker_id": "1",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.14",
          "form": "네이버인데",
          "original_form": "네이버인데",
          "speaker_id": "1",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.15",
          "form": "웹툰으로 봐야해요",
          "original_form": "웹툰으로 봐야해요",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009630.1.16",
          "form": "그 분 전전작이 투명한",
          "original_form": "그 분 전전작이 투명한",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.17",
          "form": "동거잖아요",
          "original_form": "동거잖아요",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.18",
          "form": "그 소설보다",
          "original_form": "그 소설보다",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.19",
          "form": "이게 소설보다 하 님",
          "original_form": "이게 소설보다 하 님",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.21",
          "form": "감정선을 잘 나타낸 그작품",
          "original_form": "감정선을 잘 나타낸 그작품",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.22",
          "form": "ㅋㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜...맛잘알한테...제가 뭘 추천한거냐고요 나참",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜...맛잘알한테...제가 뭘 추천한거냐고요 나참",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.23",
          "form": "그 송강이랑 한소희ㅡ나오는것도 그 작가님이 그리신 거죠?!",
          "original_form": "그 송강이랑 한소희ㅡ나오는것도 그 작가님이 그리신 거죠?!",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.24",
          "form": "아카켄마 안목 믿어.",
          "original_form": "아카켄마 안목 믿어.",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.25",
          "form": "네 맞아요! 알고있지만",
          "original_form": "네 맞아요! 알고있지만",
          "speaker_id": "2",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009630.1.26",
          "form": "거기 재언이가 참...맛있는데 너무 재수없어요",
          "original_form": "거기 재언이가 참...맛있는데 너무 재수없어요",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009630.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009630.1.28",
          "form": "그 뭐지 마른가지에 바람처럼도 하 씁 매력있더라고요...",
          "original_form": "그 뭐지 마른가지에 바람처럼도 하 씁 매력있더라고요...",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009630.1.29",
          "form": "근데ㅜ저건 소설이 좀 더 재밌는거 같아요",
          "original_form": "근데ㅜ저건 소설이 좀 더 재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009630.1.30",
          "form": "요부 그건가요",
          "original_form": "요부 그건가요",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009630.1.31",
          "form": "후회남주일것같아서 묵혔다볼라고 하기는 하는데",
          "original_form": "후회남주일것같아서 묵혔다볼라고 하기는 하는데",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009630.1.32",
          "form": "이게 웹툰이 소설 뛰어넘는게 ㄹㅇ 쉬운일이 아니랑깨요",
          "original_form": "이게 웹툰이 소설 뛰어넘는게 ㄹㅇ 쉬운일이 아니랑깨요",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009630.1.33",
          "form": "근데 남미숨은 작화로 해냈다.",
          "original_form": "근데 남미숨은 작화로 해냈다.",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009630.1.34",
          "form": "<남편이 미모를 숨김>",
          "original_form": "<남편이 미모를 숨김>",
          "speaker_id": "1",
          "time": "20210823 02:07"
        },
        {
          "id": "MDRW2100009630.1.35",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 02:07"
        },
