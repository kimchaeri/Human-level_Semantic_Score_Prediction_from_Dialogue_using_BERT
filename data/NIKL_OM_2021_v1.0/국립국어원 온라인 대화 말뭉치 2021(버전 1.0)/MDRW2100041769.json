          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041769.1.14",
          "form": "그러니깐은",
          "original_form": "그러니깐은",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041769.1.15",
          "form": "긴장되지 ㅠ",
          "original_form": "긴장되지 ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100041769.1.16",
          "form": "무섭고",
          "original_form": "무섭고",
          "speaker_id": "2",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100041769.1.17",
