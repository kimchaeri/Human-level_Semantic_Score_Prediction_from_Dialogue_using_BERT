      "utterance": [
        {
          "id": "MDRW2100026503.1.1",
          "form": "거긴 비오나요",
          "original_form": "거긴 비오나요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026503.1.2",
          "form": "여긴 지금은 안오네요!",
          "original_form": "여긴 지금은 안오네요!",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026503.1.3",
          "form": "거긴오나요?!",
          "original_form": "거긴오나요?!",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026503.1.4",
          "form": "비가 진짜 계속오네요",
          "original_form": "비가 진짜 계속오네요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026503.1.5",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026503.1.6",
          "form": "아이고 많이오나요?!",
          "original_form": "아이고 많이오나요?!",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026503.1.7",
          "form": "지금은 추적추적이용",
          "original_form": "지금은 추적추적이용",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026503.1.8",
          "form": "ㅋㅋㅋ 우산들고다니기 귀찮",
          "original_form": "ㅋㅋㅋ 우산들고다니기 귀찮",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026503.1.9",
          "form": "내일많이 서늘해질것같아요",
          "original_form": "내일많이 서늘해질것같아요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026503.1.10",
          "form": "글게요 점점 더 가을같아져서용",
          "original_form": "글게요 점점 더 가을같아져서용",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026503.1.11",
          "form": "오늘은 긴팔옷입었어요",
          "original_form": "오늘은 긴팔옷입었어요",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026503.1.12",
          "form": "오 얇은거요?",
          "original_form": "오 얇은거요?",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026503.1.13",
          "form": "네 얇은 남방이요!",
          "original_form": "네 얇은 남방이요!",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026503.1.14",
          "form": "얼른 맨투맨입고싶어요",
          "original_form": "얼른 맨투맨입고싶어요",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026503.1.15",
          "form": "맨투맨의 계절!!",
          "original_form": "맨투맨의 계절!!",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026503.1.16",
          "form": "너무좋아요맨틔맨",
          "original_form": "너무좋아요맨틔맨",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026503.1.17",
          "form": "맨투맨 조아하시나요 ㅋㅋㅋ",
          "original_form": "맨투맨 조아하시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
