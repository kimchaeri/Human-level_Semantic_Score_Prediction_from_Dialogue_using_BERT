      "utterance": [
        {
          "id": "MDRW2100025653.1.1",
          "form": "온라인 커뮤니티 활동 하시나요",
          "original_form": "온라인 커뮤니티 활동 하시나요",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100025653.1.2",
          "form": "네 재밌죠?",
          "original_form": "네 재밌죠?",
          "speaker_id": "2",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100025653.1.3",
          "form": "최신 이슈들도 보구요",
          "original_form": "최신 이슈들도 보구요",
          "speaker_id": "2",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100025653.1.4",
          "form": "커뮤니티 활동하세요?",
          "original_form": "커뮤니티 활동하세요?",
          "speaker_id": "2",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100025653.1.5",
          "form": "네 저는 커뮤니티 활동 해요",
          "original_form": "네 저는 커뮤니티 활동 해요",
          "speaker_id": "1",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100025653.1.6",
          "form": "어떤거 하시나요",
          "original_form": "어떤거 하시나요",
          "speaker_id": "1",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100025653.1.7",
          "form": "다음 카페 인기글 봐요 ㅎㅎ",
          "original_form": "다음 카페 인기글 봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025653.1.8",
          "form": "다양한 정보도 올라오고",
          "original_form": "다양한 정보도 올라오고",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025653.1.9",
          "form": "동물 좋아하는데 동물들 짤도 많이 올라오더라구요",
          "original_form": "동물 좋아하는데 동물들 짤도 많이 올라오더라구요",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025653.1.10",
          "form": "아하 그러시군요\n저는 카톡커뮤니티를 주로 하거든요",
          "original_form": "아하 그러시군요\n저는 카톡커뮤니티를 주로 하거든요",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025653.1.11",
          "form": "다음은 잘 안봤는데 재미있나보군요",
          "original_form": "다음은 잘 안봤는데 재미있나보군요",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025653.1.12",
          "form": "어떤 분야쪽 커뮤니티하시나요",
          "original_form": "어떤 분야쪽 커뮤니티하시나요",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025653.1.13",
          "form": "저는 경제 그런 커뮤니티나\n엑셀 커뮤니티해요",
          "original_form": "저는 경제 그런 커뮤니티나\n엑셀 커뮤니티해요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100025653.1.14",
          "form": "주식커뮤니티방도 있어요",
          "original_form": "주식커뮤니티방도 있어요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100025653.1.15",
          "form": "앗 주식 커뮤니티 넘 좋네요",
          "original_form": "앗 주식 커뮤니티 넘 좋네요",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100025653.1.16",
          "form": "최근에 시작했거든요!",
          "original_form": "최근에 시작했거든요!",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100025653.1.17",
          "form": "커뮤리티방은 그냥 정보정도만 얻어서 주식 커뮤니티는 장점이 그다지 많지 않아요ㅋㅋ",
          "original_form": "커뮤리티방은 그냥 정보정도만 얻어서 주식 커뮤니티는 장점이 그다지 많지 않아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025653.1.18",
          "form": "오 주식하는사람들끼리 한풀이하죠ㅋㅋ",
          "original_form": "오 주식하는사람들끼리 한풀이하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025653.1.19",
          "form": "아하 그래도 공감대 형성이 잘될것같아요",
          "original_form": "아하 그래도 공감대 형성이 잘될것같아요",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025653.1.20",
          "form": "정모도 하시나요?",
          "original_form": "정모도 하시나요?",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025653.1.21",
          "form": "맞아요ㅋㅋ 그런 공감대는 생겨요",
          "original_form": "맞아요ㅋㅋ 그런 공감대는 생겨요",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025653.1.22",
          "form": "코로나라서 정모는 한적이 없어요\n끝나면 할지도 싶네요ㅋㅋ",
          "original_form": "코로나라서 정모는 한적이 없어요\n끝나면 할지도 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025653.1.23",
          "form": "우와 전 정모해본적이 없어서 궁금하네요",
          "original_form": "우와 전 정모해본적이 없어서 궁금하네요",
          "speaker_id": "2",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025653.1.24",
          "form": "전 눈팅족이거든요 ㅋㅋ",
          "original_form": "전 눈팅족이거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:31"
        },
        {
          "id": "MDRW2100025653.1.25",
          "form": "저도 거의 눈팅하는데 시간되면 글적어요ㅋㅋ",
          "original_form": "저도 거의 눈팅하는데 시간되면 글적어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100025653.1.26",
          "form": "계속 보고 있지 않는 한 참여가 어렵죠ㅋㅋ",
          "original_form": "계속 보고 있지 않는 한 참여가 어렵죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100025653.1.27",
          "form": "맞아여 ㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100025653.1.28",
          "form": "엑셀 커뮤니티에선 꿀팁 많이 배울수 있나요?",
          "original_form": "엑셀 커뮤니티에선 꿀팁 많이 배울수 있나요?",
          "speaker_id": "2",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100025653.1.29",
          "form": "아니면 파일 올리면 수정해주시기도해요ㅋㅋ",
          "original_form": "아니면 파일 올리면 수정해주시기도해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025653.1.30",
          "form": "엑셀 커뮤니티가 유용해요",
          "original_form": "엑셀 커뮤니티가 유용해요",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025653.1.31",
          "form": "우와 배울게 많겠네요!",
          "original_form": "우와 배울게 많겠네요!",
          "speaker_id": "2",
          "time": "20210901 15:39"
        }
      ]
    }
  ]
