      "utterance": [
        {
          "id": "MDRW2100019279.1.1",
          "form": "사고싶은 물건 있으세요?",
          "original_form": "사고싶은 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019279.1.2",
          "form": "곧 가을이니 아우터 몇 개 사고싶네욥",
          "original_form": "곧 가을이니 아우터 몇 개 사고싶네욥",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019279.1.3",
          "form": "아우터 어떤거 사고싶으세요?",
          "original_form": "아우터 어떤거 사고싶으세요?",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019279.1.4",
          "form": "코트?자켓?",
          "original_form": "코트?자켓?",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019279.1.5",
          "form": "가죽자켓 살까합니다ㅋㅋ",
          "original_form": "가죽자켓 살까합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019279.1.6",
          "form": "중고거래 해본적 있으세요?",
          "original_form": "중고거래 해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019279.1.7",
          "form": "예전에 모카포트 팔아본 적 있어요ㅋㅋ",
          "original_form": "예전에 모카포트 팔아본 적 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019279.1.8",
          "form": "당근에서요",
          "original_form": "당근에서요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019279.1.9",
          "form": "사본적도 있으세요?",
          "original_form": "사본적도 있으세요?",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019279.1.10",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019279.1.11",
          "form": "컴퓨터 모니터 샀었죠",
          "original_form": "컴퓨터 모니터 샀었죠",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019279.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019279.1.13",
          "form": "컴퓨터 부품이 은근 쏠쏠하게 올라오죠",
          "original_form": "컴퓨터 부품이 은근 쏠쏠하게 올라오죠",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019279.1.14",
          "form": "저는 이사할때",
          "original_form": "저는 이사할때",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019279.1.15",
          "form": "책이랑 음반 책상 이런거 팔아봤어요",
          "original_form": "책이랑 음반 책상 이런거 팔아봤어요",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019279.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019279.1.17",
          "form": "저도 곧 이사하는데",
          "original_form": "저도 곧 이사하는데",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019279.1.18",
          "form": "좌식책상팔려고요",
          "original_form": "좌식책상팔려고요",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019279.1.19",
          "form": "팁좀요",
          "original_form": "팁좀요",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019279.1.20",
          "form": "가격을 높게 올렸다가 하트많이 찍히면 갑자기 할인을 많이하기...?정도",
          "original_form": "가격을 높게 올렸다가 하트많이 찍히면 갑자기 할인을 많이하기...?정도",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019279.1.21",
          "form": "생각나요",
          "original_form": "생각나요",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019279.1.22",
          "form": "오호 그거 괜찮네요",
          "original_form": "오호 그거 괜찮네요",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019279.1.23",
          "form": "책상은 분해해서 파셨나요?",
          "original_form": "책상은 분해해서 파셨나요?",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019279.1.24",
          "form": "저는 못가져다드린다 집앞에서 거래하자고 했더니",
          "original_form": "저는 못가져다드린다 집앞에서 거래하자고 했더니",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019279.1.25",
          "form": "트럭을 몰고와서 가져가셨어요",
          "original_form": "트럭을 몰고와서 가져가셨어요",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019279.1.26",
          "form": "대신 싸게올렸구요",
          "original_form": "대신 싸게올렸구요",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019279.1.27",
          "form": "ㅋㅋㅋ차몰고 오시는 분께 팔아야겠군요",
