        {
          "id": "MDRW2100025252.1.1",
          "form": "근데 늦었는데 안주무시나요 ㅎㅎ",
          "original_form": "근데 늦었는데 안주무시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025252.1.2",
          "form": "저는 야간근무인데",
          "original_form": "저는 야간근무인데",
          "speaker_id": "1",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025252.1.3",
          "form": "안그래도ㅎ 자야 합니다ㅎ",
          "original_form": "안그래도ㅎ 자야 합니다ㅎ",
          "speaker_id": "2",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025252.1.4",
          "form": "오늘 출근이 큰일ㅜ",
          "original_form": "오늘 출근이 큰일ㅜ",
          "speaker_id": "2",
          "time": "20210901 03:02"
        },
        {
          "id": "MDRW2100025252.1.5",
          "form": "야간근무도 피곤하시겠어요",
          "original_form": "야간근무도 피곤하시겠어요",
          "speaker_id": "2",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025252.1.6",
          "form": "저는 끝나고 쉴 수 있잖아요~~",
          "original_form": "저는 끝나고 쉴 수 있잖아요~~",
          "speaker_id": "1",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025252.1.7",
          "form": "그럼 어서 주무시고 내일 이야기 더 나누시죠",
          "original_form": "그럼 어서 주무시고 내일 이야기 더 나누시죠",
          "speaker_id": "1",
          "time": "20210901 03:03"
        },
        {
          "id": "MDRW2100025252.1.8",
          "form": "넹넹 야간근무도 파이팅!",
          "original_form": "넹넹 야간근무도 파이팅!",
          "speaker_id": "2",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025252.1.9",
          "form": "굿나잇이요ㅎㅎ",
          "original_form": "굿나잇이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 03:04"
        },
        {
          "id": "MDRW2100025252.1.10",
          "form": "잘 일어나셨나요",
          "original_form": "잘 일어나셨나요",
          "speaker_id": "1",
          "time": "20210901 10:04"
        },
        {
          "id": "MDRW2100025252.1.11",
          "form": "다시 회사의 노비로ㅎ",
          "original_form": "다시 회사의 노비로ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025252.1.12",
          "form": "지금 점심시간이라 숨통 좀 텄네요",
          "original_form": "지금 점심시간이라 숨통 좀 텄네요",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025252.1.13",
          "form": "야간근무하셨으면 오늘은 오후 출근인가요? 아님 내일 오전?",
          "original_form": "야간근무하셨으면 오늘은 오후 출근인가요? 아님 내일 오전?",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025252.1.14",
          "form": "아 저희 회사 좋은 회사입니다",
          "original_form": "아 저희 회사 좋은 회사입니다",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025252.1.15",
          "form": "4조2교대이기 때문에",
          "original_form": "4조2교대이기 때문에",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025252.1.16",
          "form": "모레 오전입니다",
          "original_form": "모레 오전입니다",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025252.1.17",
          "form": "헉 진짜 좋은 회사 다니시는군요ㄷㄷ",
          "original_form": "헉 진짜 좋은 회사 다니시는군요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100025252.1.18",
          "form": "저는 극악무도한 사업주 아래ㅜ",
          "original_form": "저는 극악무도한 사업주 아래ㅜ",
          "speaker_id": "2",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025252.1.19",
          "form": "ㅋㅋ 아녜요 자꾸 위에서 건드려서ㅠ",
          "original_form": "ㅋㅋ 아녜요 자꾸 위에서 건드려서ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025252.1.20",
          "form": "그건.. 어느 회사나 위가 문제ㅜㅇ",
          "original_form": "그건.. 어느 회사나 위가 문제ㅜㅇ",
