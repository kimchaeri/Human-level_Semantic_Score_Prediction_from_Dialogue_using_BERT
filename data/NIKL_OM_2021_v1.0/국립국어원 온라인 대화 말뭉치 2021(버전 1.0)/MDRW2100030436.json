      "utterance": [
        {
          "id": "MDRW2100030436.1.1",
          "form": "다시보기 구해드리겄습니다",
          "original_form": "다시보기 구해드리겄습니다",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030436.1.2",
          "form": "필요없어욧!",
          "original_form": "필요없어욧!",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030436.1.3",
          "form": "근데 서태지형은 뭐하나요",
          "original_form": "근데 서태지형은 뭐하나요",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030436.1.4",
          "form": "이미 검색 완료ㅎ",
          "original_form": "이미 검색 완료ㅎ",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030436.1.5",
          "form": "돈을 그래많이 벌어놈?",
          "original_form": "돈을 그래많이 벌어놈?",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030436.1.6",
          "form": "오 검색요정 뚜둔",
          "original_form": "오 검색요정 뚜둔",
          "speaker_id": "2",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100030436.1.7",
          "form": "어...잘 먹고 잘 살겠쥬?",
          "original_form": "어...잘 먹고 잘 살겠쥬?",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030436.1.8",
          "form": "그러니까요 활동하나도 안하는데",
          "original_form": "그러니까요 활동하나도 안하는데",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030436.1.9",
          "form": "신기합니다",
          "original_form": "신기합니다",
          "speaker_id": "2",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030436.1.10",
          "form": "좀  아쉽긴 해요",
          "original_form": "좀  아쉽긴 해요",
          "speaker_id": "1",
          "time": "20210904 01:01"
        },
        {
          "id": "MDRW2100030436.1.11",
          "form": "맞아요 아쉽습니다",
          "original_form": "맞아요 아쉽습니다",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030436.1.12",
          "form": "근데 저 되게 라이트한 팬이라",
          "original_form": "근데 저 되게 라이트한 팬이라",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030436.1.13",
          "form": "재능이",
          "original_form": "재능이",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030436.1.14",
          "form": "낭비됨",
          "original_form": "낭비됨",
          "speaker_id": "2",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030436.1.15",
          "form": "신곡 나와도 1, 2년뒤에 알 듯요ㅎㅎ",
          "original_form": "신곡 나와도 1, 2년뒤에 알 듯요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100030436.1.16",
          "form": "ㅎㅎ왜 활동 안하시지",
          "original_form": "ㅎㅎ왜 활동 안하시지",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030436.1.17",
          "form": "노는가 그냥",
          "original_form": "노는가 그냥",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030436.1.18",
          "form": "부럽다 ㅋㅋ",
          "original_form": "부럽다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030436.1.19",
          "form": "ㅋㅋ 다음생애엔",
          "original_form": "ㅋㅋ 다음생애엔",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030436.1.20",
          "form": "서태지 증손주로 태어날 예정입니다",
          "original_form": "서태지 증손주로 태어날 예정입니다",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.21",
          "form": "남자로요?",
          "original_form": "남자로요?",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.22",
          "form": "남자 별롭니다",
          "original_form": "남자 별롭니다",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.23",
          "form": "재산 잘 관리해 물려주셔야 할 텐데ㅎㅎ",
          "original_form": "재산 잘 관리해 물려주셔야 할 텐데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.25",
          "form": "아니ㅋㅋ 왜죠ㅋㅋ",
          "original_form": "아니ㅋㅋ 왜죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.26",
          "form": "여자가 더 좋지않나요?",
          "original_form": "여자가 더 좋지않나요?",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030436.1.27",
          "form": "여자로 살아보고 싶어요 ㅎㅎ",
          "original_form": "여자로 살아보고 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030436.1.28",
          "form": "담생엔 ㅋ",
          "original_form": "담생엔 ㅋ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030436.1.29",
          "form": "여자로 살았으니",
          "original_form": "여자로 살았으니",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030436.1.30",
          "form": "남자로도 살아봐야쥬",
          "original_form": "남자로도 살아봐야쥬",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030436.1.31",
          "form": "저는 균형을 중시하는 사람입니다ㅎㅎ",
          "original_form": "저는 균형을 중시하는 사람입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030436.1.32",
          "form": "그럼 님은 증손녀로",
          "original_form": "그럼 님은 증손녀로",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030436.1.33",
          "form": "아 그럼 잘생긴남자로 태어나세요",
          "original_form": "아 그럼 잘생긴남자로 태어나세요",
          "speaker_id": "2",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030436.1.34",
          "form": "저와 재산다툼 거하게 해봅시당ㅅㅎ",
          "original_form": "저와 재산다툼 거하게 해봅시당ㅅㅎ",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030436.1.35",
          "form": "아 저는 예쁜여자로 태어 나고 싶어요 ㅋㅋ",
          "original_form": "아 저는 예쁜여자로 태어 나고 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100030436.1.36",
          "form": "아 전 그냥 예쁘게태어나서",
          "original_form": "아 전 그냥 예쁘게태어나서",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030436.1.37",
          "form": "결혼하면 되자나요 ㅎㅎ",
          "original_form": "결혼하면 되자나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030436.1.38",
          "form": "다툼은 옳지 않습니다 ㅎㅎ",
          "original_form": "다툼은 옳지 않습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030436.1.39",
          "form": "한번쯤 해보고 싶었는데 아쉽",
          "original_form": "한번쯤 해보고 싶었는데 아쉽",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030436.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030436.1.41",
          "form": "서가의 난 이런거",
          "original_form": "서가의 난 이런거",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030436.1.42",
          "form": "무서워요 ㅋㅋㅋ",
          "original_form": "무서워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030436.1.43",
          "form": "전 평화주의자입니다",
          "original_form": "전 평화주의자입니다",
          "speaker_id": "2",
          "time": "20210904 01:08"
        }
      ]
    }
