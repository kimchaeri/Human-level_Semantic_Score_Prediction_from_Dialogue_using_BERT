      "utterance": [
        {
          "id": "MDRW2100025397.1.1",
          "form": "점심 메뉴는 정했나요??",
          "original_form": "점심 메뉴는 정했나요??",
          "speaker_id": "1",
          "time": "20210901 10:50"
        },
        {
          "id": "MDRW2100025397.1.2",
          "form": "저는 도시락 싸왔습니다!!",
          "original_form": "저는 도시락 싸왔습니다!!",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025397.1.3",
          "form": "점심 정하셨나요?",
          "original_form": "점심 정하셨나요?",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025397.1.4",
          "form": "오 도시락파군요",
          "original_form": "오 도시락파군요",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025397.1.5",
          "form": "전 집에서 해먹습니다",
          "original_form": "전 집에서 해먹습니다",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025397.1.6",
          "form": "지금 감자조림해서 먹을까 생각중이에요",
          "original_form": "지금 감자조림해서 먹을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025397.1.7",
          "form": "감자조림 맛있겠네오!!",
          "original_form": "감자조림 맛있겠네오!!",
          "speaker_id": "2",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100025397.1.8",
          "form": "요리를 잘하시나바요저는 똥손입니당",
          "original_form": "요리를 잘하시나바요저는 똥손입니당",
          "speaker_id": "2",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100025397.1.9",
          "form": "요리는 하는 걸 좋아해요.. 이것저것",
          "original_form": "요리는 하는 걸 좋아해요.. 이것저것",
          "speaker_id": "1",
          "time": "20210901 10:55"
        },
        {
          "id": "MDRW2100025397.1.10",
          "form": "헐진짜그런사람부러워요",
          "original_form": "헐진짜그런사람부러워요",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.11",
          "form": "저는 진짜 못해서 ㅋㅋㅋㅋ",
          "original_form": "저는 진짜 못해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.12",
          "form": "재료 다듬고 치우고 하는건 별로구요..ㅋㅋㅋㅋㅋ",
          "original_form": "재료 다듬고 치우고 하는건 별로구요..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.13",
          "form": "딱 요리만",
          "original_form": "딱 요리만",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.14",
          "form": "저는오히려 치우는걸잘해요",
          "original_form": "저는오히려 치우는걸잘해요",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.16",
          "form": "ㅎㅎㅎ 그거 잘하면 좋은거 아닌가요?ㅎㅎ",
          "original_form": "ㅎㅎㅎ 그거 잘하면 좋은거 아닌가요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.17",
          "form": "도시락은 집에서 싼건가요? 사온건가요?",
          "original_form": "도시락은 집에서 싼건가요? 사온건가요?",
          "speaker_id": "1",
          "time": "20210901 10:56"
        },
        {
          "id": "MDRW2100025397.1.18",
          "form": "네네@냉동도시락그냥 해동해서 들고와요",
          "original_form": "네네@냉동도시락그냥 해동해서 들고와요",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025397.1.19",
          "form": "엄마 반찬몇개랑..",
          "original_form": "엄마 반찬몇개랑..",
          "speaker_id": "2",
          "time": "20210901 10:59"
        },
        {
          "id": "MDRW2100025397.1.20",
          "form": "그렇구나.. 편의점이나 한솥 같은데서 사드시는 분들도 많더라구요",
          "original_form": "그렇구나.. 편의점이나 한솥 같은데서 사드시는 분들도 많더라구요",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025397.1.21",
          "form": "냉동도시락도 파는군요",
          "original_form": "냉동도시락도 파는군요",
          "speaker_id": "1",
          "time": "20210901 11:01"
        },
        {
          "id": "MDRW2100025397.1.22",
          "form": "냉동 볶음밥이예요!",
          "original_form": "냉동 볶음밥이예요!",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025397.1.23",
          "form": "사는게 밥값절약되서...ㅎ",
          "original_form": "사는게 밥값절약되서...ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025397.1.24",
          "form": "아~ 뭔지 알겠어요.. 동생네 갔을때 먹어봤네요..ㅎㅎㅎ",
          "original_form": "아~ 뭔지 알겠어요.. 동생네 갔을때 먹어봤네요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025397.1.25",
          "form": "그거 다양하게 나오죠?",
          "original_form": "그거 다양하게 나오죠?",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025397.1.26",
          "form": "맞아요 여러게 있어요 맛도 갠춘하고",
          "original_form": "맞아요 여러게 있어요 맛도 갠춘하고",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025397.1.27",
          "form": "피시방에서 쓰는거 먹어서요 ㅋㅋ",
          "original_form": "피시방에서 쓰는거 먹어서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025397.1.28",
          "form": "저도 한 4개지 맛정도 먹어봤네요",
          "original_form": "저도 한 4개지 맛정도 먹어봤네요",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025397.1.29",
          "form": "낙지가 젤인기더라구요!!",
          "original_form": "낙지가 젤인기더라구요!!",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025397.1.30",
          "form": "요즘은 없는게 없어요.. 다양한 식품들이 있떠라구요",
          "original_form": "요즘은 없는게 없어요.. 다양한 식품들이 있떠라구요",
