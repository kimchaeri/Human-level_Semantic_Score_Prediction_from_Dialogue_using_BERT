        },
        {
          "id": "MDRW2100012829.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012829.1.3",
          "form": "배달음식 자주 드시나요?",
          "original_form": "배달음식 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012829.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100012829.1.5",
          "form": "저 천생연분이예요",
          "original_form": "저 천생연분이예요",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100012829.1.6",
          "form": "저도요!!!",
          "original_form": "저도요!!!",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100012829.1.7",
          "form": "주로 어떤거 시켜 드시나요?",
          "original_form": "주로 어떤거 시켜 드시나요?",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100012829.1.8",
          "form": "전 초밥이요ㅋㅋㅋ",
          "original_form": "전 초밥이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100012829.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100012829.1.10",
          "form": "저는 치킨, 피자랑",
          "original_form": "저는 치킨, 피자랑",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100012829.1.11",
          "form": "회랑 초밥도 자주 먹어요!",
          "original_form": "회랑 초밥도 자주 먹어요!",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100012829.1.12",
          "form": "무슨 초밥 좋아하시나요?",
          "original_form": "무슨 초밥 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100012829.1.13",
          "form": "전 초밥은",
          "original_form": "전 초밥은",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100012829.1.14",
          "form": "다!",
          "original_form": "다!",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100012829.1.15",
          "form": "좋아해요ㅋㅋㅋ",
          "original_form": "좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100012829.1.16",
          "form": "오!!!",
          "original_form": "오!!!",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100012829.1.17",
          "form": "저는 광어랑 연어 좋아해요ㅎㅎ",
          "original_form": "저는 광어랑 연어 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100012829.1.18",
          "form": "최근에 배달 언제 시키셨나요?",
          "original_form": "최근에 배달 언제 시키셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100012829.1.19",
          "form": "오늘요ㅋㅋㅋㅋㅋ",
          "original_form": "오늘요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100012829.1.20",
          "form": "천생연분짬바 어디안가죠",
          "original_form": "천생연분짬바 어디안가죠",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100012829.1.21",
          "form": "진짜....",
          "original_form": "진짜....",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100012829.1.22",
          "form": "천생연분이신데요???!!!",
          "original_form": "천생연분이신데요???!!!",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100012829.1.23",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100012829.1.24",
          "form": "ㅋㅋㅋ어제저녁은 쫄돈이요",
          "original_form": "ㅋㅋㅋ어제저녁은 쫄돈이요",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100012829.1.25",
          "form": "점심드셨나요?",
