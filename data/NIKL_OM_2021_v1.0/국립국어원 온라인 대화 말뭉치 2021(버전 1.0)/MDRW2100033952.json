      "utterance": [
        {
          "id": "MDRW2100033952.1.1",
          "form": "헐ㅋㅋ과일은 쥬씨나 과일도시락전문점에서 해주는거 알았는데",
          "original_form": "헐ㅋㅋ과일은 쥬씨나 과일도시락전문점에서 해주는거 알았는데",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100033952.1.2",
          "form": "호떡은 처음 들어요",
          "original_form": "호떡은 처음 들어요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100033952.1.3",
          "form": "대박,,,",
          "original_form": "대박,,,",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100033952.1.4",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100033952.1.5",
          "form": "ㅋㅋㅋ 호떡당배달되더라고요",
          "original_form": "ㅋㅋㅋ 호떡당배달되더라고요",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100033952.1.6",
          "form": "저녁은 뭘드시나요?",
          "original_form": "저녁은 뭘드시나요?",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100033952.1.7",
          "form": "아마 저녁은 그냥 집에 있는거 먹을것같아요~",
          "original_form": "아마 저녁은 그냥 집에 있는거 먹을것같아요~",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100033952.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100033952.1.9",
          "form": "음 전",
          "original_form": "음 전",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033952.1.10",
          "form": "초밥생각중이예요 아니면",
          "original_form": "초밥생각중이예요 아니면",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033952.1.11",
          "form": "부대찌개요ㅋㅋㅋ",
          "original_form": "부대찌개요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100033952.1.12",
          "form": "와 초밥이랑 부대찌개!!",
          "original_form": "와 초밥이랑 부대찌개!!",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100033952.1.13",
          "form": "왠지 흐린날에 둘다 어울리는데요?",
          "original_form": "왠지 흐린날에 둘다 어울리는데요?",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033952.1.14",
          "form": "ㅋㅋㄱㅋㅋ역시 메뉴고르는건 어렵네요",
          "original_form": "ㅋㅋㄱㅋㅋ역시 메뉴고르는건 어렵네요",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033952.1.15",
          "form": "ㅋㅋㅋ 맞아요 맛있는게",
          "original_form": "ㅋㅋㅋ 맞아요 맛있는게",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100033952.1.16",
          "form": "너무많아서문제죠",
          "original_form": "너무많아서문제죠",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100033952.1.17",
          "form": "ㅋㅋㅋㅋㅋ진짜 공감돼요\r\n맛집찾는것도 일이에요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 공감돼요\r\n맛집찾는것도 일이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033952.1.18",
          "form": "제일 좋아하는 음식 있으세요?",
          "original_form": "제일 좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100033952.1.19",
          "form": "초밥이요ㅋㅋㅋㅋ기",
          "original_form": "초밥이요ㅋㅋㅋㅋ기",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100033952.1.20",
          "form": "앗ㅋㅋㅋㅋㅋ그렇군요!!\r\n저희집 근처에 상무초밥이라는 초밥프랜차이즈 있는데 맛있더라구요!!",
          "original_form": "앗ㅋㅋㅋㅋㅋ그렇군요!!\r\n저희집 근처에 상무초밥이라는 초밥프랜차이즈 있는데 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100033952.1.21",
          "form": "아 들어본것같아요ㅋㅋㅋㅋ",
          "original_form": "아 들어본것같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100033952.1.22",
          "form": "초밥은 사랑이죠",
          "original_form": "초밥은 사랑이죠",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100033952.1.23",
          "form": "ㅋㄱㅋㅋㅋ좋아하는 초밥집 있으세요?",
          "original_form": "ㅋㄱㅋㅋㅋ좋아하는 초밥집 있으세요?",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100033952.1.24",
          "form": "ㅋㅋㅋ 동네라서요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 동네라서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100033952.1.25",
          "form": "저녁드셨나요ㅣ",
          "original_form": "저녁드셨나요ㅣ",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100033952.1.26",
          "form": "저는 냉장고 털어서 비빔밥 해먹었어요ㅎㅎ",
