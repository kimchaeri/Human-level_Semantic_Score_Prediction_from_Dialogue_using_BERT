      "utterance": [
        {
          "id": "MDRW2100027481.1.1",
          "form": "오히려 저는 신선해서 좋더라구요",
          "original_form": "오히려 저는 신선해서 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 13:02"
        },
        {
          "id": "MDRW2100027481.1.2",
          "form": "아 그런가요? ㅋㅋㅋ 근데 약간",
          "original_form": "아 그런가요? ㅋㅋㅋ 근데 약간",
          "speaker_id": "2",
          "time": "20210902 13:03"
        },
        {
          "id": "MDRW2100027481.1.3",
          "form": "뽑은 사람들이 다 저는 안할거지만 ㅎㅎ 같은 느낌으로 말해서",
          "original_form": "뽑은 사람들이 다 저는 안할거지만 ㅎㅎ 같은 느낌으로 말해서",
          "speaker_id": "2",
          "time": "20210902 13:03"
        },
        {
          "id": "MDRW2100027481.1.4",
          "form": "유저로서 좀 속상하긴 했어요 ㅠㅠ",
          "original_form": "유저로서 좀 속상하긴 했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:03"
        },
        {
          "id": "MDRW2100027481.1.5",
          "form": "설마 똥뿌리기 작전이었던 건가요",
          "original_form": "설마 똥뿌리기 작전이었던 건가요",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027481.1.6",
          "form": "약간 그런느낌같은 ㅋㅋㅋ 너무했어요",
          "original_form": "약간 그런느낌같은 ㅋㅋㅋ 너무했어요",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027481.1.7",
          "form": "그걸 곧이곧대로 내는 넥슨도 참 그러네요",
          "original_form": "그걸 곧이곧대로 내는 넥슨도 참 그러네요",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027481.1.8",
          "form": "맞아요 ㅠㅠ 어쩔 수 없다지만... 어쨌든 그걸 또 인게임 콘텐츠로 내니까 말이죠",
          "original_form": "맞아요 ㅠㅠ 어쩔 수 없다지만... 어쨌든 그걸 또 인게임 콘텐츠로 내니까 말이죠",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100027481.1.9",
          "form": "좀 더 신중했으면 하는데~",
          "original_form": "좀 더 신중했으면 하는데~",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100027481.1.10",
          "form": "이미 넥슨이 반쯤 보낸건지도 모르겠네요",
          "original_form": "이미 넥슨이 반쯤 보낸건지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100027481.1.11",
          "form": "그걸로 매출 많이 나오는걸로 아는데 잘좀 해줬으면 ㅋㅋㅋ",
          "original_form": "그걸로 매출 많이 나오는걸로 아는데 잘좀 해줬으면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027481.1.12",
          "form": "IP 소중하게 여겨야지!!",
          "original_form": "IP 소중하게 여겨야지!!",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027481.1.13",
          "form": "맞아요! 넥슨은 메이플을 아껴달라!",
          "original_form": "맞아요! 넥슨은 메이플을 아껴달라!",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100027481.1.14",
          "form": "그래도 아끼는 게임이니까 말이죠~ㅠㅠ",
          "original_form": "그래도 아끼는 게임이니까 말이죠~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027481.1.15",
          "form": "맞다 모바일 게임은 어떤거 하세요?",
          "original_form": "맞다 모바일 게임은 어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027481.1.16",
          "form": "퍼즐앤드래곤 하고 있어요",
          "original_form": "퍼즐앤드래곤 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027481.1.17",
          "form": "오 유명한건 아는데 ㅋㅋㅋ 재밌나요?",
          "original_form": "오 유명한건 아는데 ㅋㅋㅋ 재밌나요?",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027481.1.18",
          "form": "다른 퍼즐겜들이랑 어떻게 다르지",
          "original_form": "다른 퍼즐겜들이랑 어떻게 다르지",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027481.1.19",
          "form": "일단 가챠가 필요합니다",
          "original_form": "일단 가챠가 필요합니다",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027481.1.20",
          "form": "캐릭터 성능 위주라서요",
          "original_form": "캐릭터 성능 위주라서요",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027481.1.21",
          "form": "퍼즐은 거들뿐입니다",
          "original_form": "퍼즐은 거들뿐입니다",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027481.1.22",
          "form": "아하 ㅋㅋㅋㅋ 신기하네요 그런 곳에도 가챠 있고 ㅠㅠ 퍼즐 어떤 식이에요?",
          "original_form": "아하 ㅋㅋㅋㅋ 신기하네요 그런 곳에도 가챠 있고 ㅠㅠ 퍼즐 어떤 식이에요?",
          "speaker_id": "2",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027481.1.23",
          "form": "한번 잡고 제한시간동안 열심이 움직여서",
          "original_form": "한번 잡고 제한시간동안 열심이 움직여서",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027481.1.24",
          "form": "애니팡과 같은 원리로 터져요",
          "original_form": "애니팡과 같은 원리로 터져요",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027481.1.25",
          "form": "오~ 재밌겠네요 좀 특이해보여서 관심생겨요 ㅋㅋㅋ",
          "original_form": "오~ 재밌겠네요 좀 특이해보여서 관심생겨요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100027481.1.26",
          "form": "저도 퍼즐게임류 좋아해서 ㅎㅎ",
          "original_form": "저도 퍼즐게임류 좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:36"
        }
      ]
