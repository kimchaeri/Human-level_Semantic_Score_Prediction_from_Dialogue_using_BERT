      "utterance": [
        {
          "id": "MDRW2100035812.1.1",
          "form": "너 지금 게임하고 있는 거지",
          "original_form": "너 지금 게임하고 있는 거지",
          "speaker_id": "2",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100035812.1.2",
          "form": "어떻게 알았지",
          "original_form": "어떻게 알았지",
          "speaker_id": "1",
          "time": "20210830 03:18"
        },
        {
          "id": "MDRW2100035812.1.3",
          "form": "나 원래 주말이 시간에 게임 하잖아",
          "original_form": "나 원래 주말이 시간에 게임 하잖아",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100035812.1.4",
          "form": "무슨 게임 하고 있는지 맞춰봐",
          "original_form": "무슨 게임 하고 있는지 맞춰봐",
          "speaker_id": "1",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.5",
          "form": "최근에 스위치 샀다고 자랑을 했지",
          "original_form": "최근에 스위치 샀다고 자랑을 했지",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.6",
          "form": "맞아 스위치 동물의 숲을 하고 있어",
          "original_form": "맞아 스위치 동물의 숲을 하고 있어",
          "speaker_id": "1",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.7",
          "form": "그거 완전 힐링 게임이라던데",
          "original_form": "그거 완전 힐링 게임이라던데",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.8",
          "form": "어느 정도는 맞는 것 같아",
          "original_form": "어느 정도는 맞는 것 같아",
          "speaker_id": "1",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.9",
          "form": "어느 면에서는 힐링이 아닌가 보구나",
          "original_form": "어느 면에서는 힐링이 아닌가 보구나",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.10",
          "form": "빨리 일을 해서 게임 머니를 벌어 드려야 하니까",
          "original_form": "빨리 일을 해서 게임 머니를 벌어 드려야 하니까",
          "speaker_id": "1",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.11",
          "form": "동물에 숲 캐릭터가 정말 귀여워",
          "original_form": "동물에 숲 캐릭터가 정말 귀여워",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100035812.1.12",
          "form": "너구리 말하는 거지",
          "original_form": "너구리 말하는 거지",
          "speaker_id": "1",
          "time": "20210830 03:27"
        },
        {
          "id": "MDRW2100035812.1.13",
          "form": "맞아 엄청 귀엽던데",
          "original_form": "맞아 엄청 귀엽던데",
          "speaker_id": "2",
          "time": "20210830 03:27"
        },
        {
          "id": "MDRW2100035812.1.14",
          "form": "그 애들 말고도 귀여운 캐릭터가 꽤 많이 나와",
          "original_form": "그 애들 말고도 귀여운 캐릭터가 꽤 많이 나와",
          "speaker_id": "1",
          "time": "20210830 03:27"
        },
        {
          "id": "MDRW2100035812.1.15",
          "form": "힐링 게임 맞는 모양이네",
          "original_form": "힐링 게임 맞는 모양이네",
          "speaker_id": "2",
          "time": "20210830 03:27"
        }
