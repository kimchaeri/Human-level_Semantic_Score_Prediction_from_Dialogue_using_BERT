      "utterance": [
        {
          "id": "MDRW2100030489.1.1",
          "form": "어후",
          "original_form": "어후",
          "speaker_id": "2",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030489.1.2",
          "form": "최근에 카카오페이지 스테이지에서도",
          "original_form": "최근에 카카오페이지 스테이지에서도",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030489.1.3",
          "form": "그래서 뭔 얘기중이엇지",
          "original_form": "그래서 뭔 얘기중이엇지",
          "speaker_id": "2",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030489.1.4",
          "form": "아이돌 이야기를 많이 보다보니까",
          "original_form": "아이돌 이야기를 많이 보다보니까",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030489.1.5",
          "form": "등장인물을 떠올리는데 오래걸림",
          "original_form": "등장인물을 떠올리는데 오래걸림",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030489.1.6",
          "form": "아무래도 에피소드가 여기도 비슷비슷해서",
          "original_form": "아무래도 에피소드가 여기도 비슷비슷해서",
          "speaker_id": "1",
          "time": "20210904 02:05"
        },
        {
          "id": "MDRW2100030489.1.7",
          "form": "데못죽도 처음 본 아이돌 장르 소설아 아니라면",
          "original_form": "데못죽도 처음 본 아이돌 장르 소설아 아니라면",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030489.1.8",
          "form": "아마 그냥 스쳐지나갔을 수도 있을 것 같아",
          "original_form": "아마 그냥 스쳐지나갔을 수도 있을 것 같아",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030489.1.9",
          "form": "이벤트에 엮인거 생각하면 아닐걸ㅋㅋㅋㅋ",
          "original_form": "이벤트에 엮인거 생각하면 아닐걸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030489.1.10",
          "form": "이벤트에 약한 사람들",
          "original_form": "이벤트에 약한 사람들",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030489.1.11",
          "form": "그랬을 수도 있고 책인데 이벤트 열어주는게 흔치 않으니까",
          "original_form": "그랬을 수도 있고 책인데 이벤트 열어주는게 흔치 않으니까",
          "speaker_id": "1",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030489.1.12",
          "form": "만약에 이 책이 종이책으로 먼저 접했다면",
          "original_form": "만약에 이 책이 종이책으로 먼저 접했다면",
          "speaker_id": "1",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030489.1.13",
          "form": "이정도 몰입감은 아닐거 같아",
          "original_form": "이정도 몰입감은 아닐거 같아",
          "speaker_id": "1",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030489.1.14",
          "form": "이북이랑 종이책이랑 아무래도 느낌이 호ㅓㄱ실히",
          "original_form": "이북이랑 종이책이랑 아무래도 느낌이 호ㅓㄱ실히",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030489.1.15",
          "form": "다른게 잇어서",
          "original_form": "다른게 잇어서",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030489.1.16",
          "form": "그럴수도 잇을듯해",
          "original_form": "그럴수도 잇을듯해",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030489.1.17",
          "form": "읽히는 속도도 다르고",
          "original_form": "읽히는 속도도 다르고",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030489.1.18",
          "form": "이거는 웹소설로 봐서 좋은 소설인듯",
          "original_form": "이거는 웹소설로 봐서 좋은 소설인듯",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030489.1.19",
          "form": "연재작이라 기다리는 건 좀 지옥이지만",
          "original_form": "연재작이라 기다리는 건 좀 지옥이지만",
          "speaker_id": "2",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030489.1.20",
          "form": "맞아... 최신화 까지 다 봤어?",
          "original_form": "맞아... 최신화 까지 다 봤어?",
          "speaker_id": "1",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030489.1.21",
          "form": "놉...",
          "original_form": "놉...",
          "speaker_id": "2",
          "time": "20210904 02:16"
        },
        {
          "id": "MDRW2100030489.1.22",
          "form": "249화 볼 차례",
          "original_form": "249화 볼 차례",
          "speaker_id": "2",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030489.1.23",
          "form": "13편이 기다리고 잇다",
          "original_form": "13편이 기다리고 잇다",
          "speaker_id": "2",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030489.1.24",
          "form": "최신 10편 남아잇엇는데 늘어낫네",
          "original_form": "최신 10편 남아잇엇는데 늘어낫네",
          "speaker_id": "2",
          "time": "20210904 02:17"
        },
        {
          "id": "MDRW2100030489.1.25",
          "form": "아니.. 그걸 참고 있다니 책은 모름지기 나오면 끝까지 보는게",
          "original_form": "아니.. 그걸 참고 있다니 책은 모름지기 나오면 끝까지 보는게",
          "speaker_id": "1",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100030489.1.26",
          "form": "그러겤ㅋㅋㅋ",
          "original_form": "그러겤ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:35"
        },
        {
          "id": "MDRW2100030489.1.27",
          "form": "어쩌다보니",
          "original_form": "어쩌다보니",
          "speaker_id": "2",
          "time": "20210904 02:35"
        },
        {
          "id": "MDRW2100030489.1.28",
          "form": "다른 봐야할게 지금",
          "original_form": "다른 봐야할게 지금",
          "speaker_id": "2",
          "time": "20210904 02:35"
        },
        {
          "id": "MDRW2100030489.1.29",
          "form": "데못죽은 소장만 해두고잇음ㅋㅋㅋ",
          "original_form": "데못죽은 소장만 해두고잇음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:35"
        }
      ]
