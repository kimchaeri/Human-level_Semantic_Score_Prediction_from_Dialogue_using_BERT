      "utterance": [
        {
          "id": "MDRW2100024495.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210831 11:45"
        },
        {
          "id": "MDRW2100024495.1.2",
          "form": "네~\n온라인 커뮤니티 활동 하시나요?",
          "original_form": "네~\n온라인 커뮤니티 활동 하시나요?",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100024495.1.3",
          "form": "페북도 커뮤니티일까요?",
          "original_form": "페북도 커뮤니티일까요?",
          "speaker_id": "1",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100024495.1.4",
          "form": "페북은 해요",
          "original_form": "페북은 해요",
          "speaker_id": "1",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100024495.1.5",
          "form": "페북 인스타 카페 단톡방 다 해당 하지 않을 까요!!",
          "original_form": "페북 인스타 카페 단톡방 다 해당 하지 않을 까요!!",
          "speaker_id": "2",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024495.1.6",
          "form": "저는 카페랑 단톡방 하고 있어요",
          "original_form": "저는 카페랑 단톡방 하고 있어요",
          "speaker_id": "2",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024495.1.7",
          "form": "아하 네 단톡은 아는사람들이랑 하세요?",
          "original_form": "아하 네 단톡은 아는사람들이랑 하세요?",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024495.1.8",
          "form": "모르는 사람들이랑요?",
          "original_form": "모르는 사람들이랑요?",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024495.1.9",
          "form": "가족 단톡이랑 모르는 사람 단톡 다 있어요",
          "original_form": "가족 단톡이랑 모르는 사람 단톡 다 있어요",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100024495.1.10",
          "form": "단톡사람들이랑 실제로 만나신적 있으세요?",
          "original_form": "단톡사람들이랑 실제로 만나신적 있으세요?",
          "speaker_id": "1",
          "time": "20210831 12:03"
        },
        {
          "id": "MDRW2100024495.1.11",
          "form": "모르는 사람들 있는데는 만나 본 적 없어요.",
          "original_form": "모르는 사람들 있는데는 만나 본 적 없어요.",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100024495.1.12",
          "form": "이벤트 정보 교류 하는거라 친목 도모는 아니라서요.",
          "original_form": "이벤트 정보 교류 하는거라 친목 도모는 아니라서요.",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100024495.1.13",
          "form": "오 좋은 단톡방이네요",
          "original_form": "오 좋은 단톡방이네요",
          "speaker_id": "1",
          "time": "20210831 12:06"
        },
        {
          "id": "MDRW2100024495.1.14",
          "form": "그런 단톡방 엄청 많아요!!",
          "original_form": "그런 단톡방 엄청 많아요!!",
          "speaker_id": "2",
          "time": "20210831 12:06"
        },
        {
          "id": "MDRW2100024495.1.15",
          "form": "헉 그런가요 공개방이에요? 뭘로 검색하면 나올까요",
          "original_form": "헉 그런가요 공개방이에요? 뭘로 검색하면 나올까요",
          "speaker_id": "1",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024495.1.16",
          "form": "이벤트 공유 그런거로 하면 될 거 같아요. 공개방도 있고 잠긴 방도 있을거에요.",
          "original_form": "이벤트 공유 그런거로 하면 될 거 같아요. 공개방도 있고 잠긴 방도 있을거에요.",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024495.1.17",
          "form": "그렇구나 소개해주셔서 감사해요",
          "original_form": "그렇구나 소개해주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100024495.1.18",
          "form": "페북으로 주로 뭐 하세요?",
          "original_form": "페북으로 주로 뭐 하세요?",
          "speaker_id": "2",
          "time": "20210831 12:10"
        }
      ]
