      "utterance": [
        {
          "id": "MDRW2100043623.1.1",
          "form": "얌샘김밥에서 돈가스 먹고싶어",
          "original_form": "얌샘김밥에서 돈가스 먹고싶어",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.2",
          "form": "얌샘!!!!!",
          "original_form": "얌샘!!!!!",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.3",
          "form": "너무좋지",
          "original_form": "너무좋지",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.4",
          "form": "진짜 여기돈가스가 짱이야",
          "original_form": "진짜 여기돈가스가 짱이야",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.5",
          "form": "다 통틀어서?",
          "original_form": "다 통틀어서?",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.6",
          "form": "진심...",
          "original_form": "진심...",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.7",
          "form": "경양식인가?",
          "original_form": "경양식인가?",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.8",
          "form": "엉 경양식",
          "original_form": "엉 경양식",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.9",
          "form": "소스 푹 담군거맞지",
          "original_form": "소스 푹 담군거맞지",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.10",
          "form": "마자용",
          "original_form": "마자용",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.11",
          "form": "나도 그런거좋아햐",
          "original_form": "나도 그런거좋아햐",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.12",
          "form": "진짜??",
          "original_form": "진짜??",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.13",
          "form": "나두 조아하지",
          "original_form": "나두 조아하지",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043623.1.14",
          "form": "너는 일본식 조아하자나",
          "original_form": "너는 일본식 조아하자나",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043623.1.15",
          "form": "아녀..",
          "original_form": "아녀..",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043623.1.16",
          "form": "그것도 맛나긴해",
          "original_form": "그것도 맛나긴해",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043623.1.17",
          "form": "경양이 더좋은걸",
          "original_form": "경양이 더좋은걸",
