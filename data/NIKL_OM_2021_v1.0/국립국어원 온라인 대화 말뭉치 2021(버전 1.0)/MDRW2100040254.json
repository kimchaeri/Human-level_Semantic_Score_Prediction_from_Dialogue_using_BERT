      "utterance": [
        {
          "id": "MDRW2100040254.1.1",
          "form": "아침을 늦게 먹었어요",
          "original_form": "아침을 늦게 먹었어요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040254.1.2",
          "form": "점심 드셨나요",
          "original_form": "점심 드셨나요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040254.1.3",
          "form": "아침 뭐 먹었어요",
          "original_form": "아침 뭐 먹었어요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040254.1.4",
          "form": "저는 아주 많은 양의 커피를 타서 과자 예감이랑 같이 먹다가 고모가 김밥을 사오셔서 그거 좀 먹었어요",
          "original_form": "저는 아주 많은 양의 커피를 타서 과자 예감이랑 같이 먹다가 고모가 김밥을 사오셔서 그거 좀 먹었어요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040254.1.5",
          "form": "중간에 사과도 먹고 꼬북칩 또 먹고 자도 녹차도 먹었어요",
          "original_form": "중간에 사과도 먹고 꼬북칩 또 먹고 자도 녹차도 먹었어요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040254.1.6",
          "form": "그니 저는 전어회를 드디어 먹었습니다",
          "original_form": "그니 저는 전어회를 드디어 먹었습니다",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040254.1.7",
          "form": "아침으로 뭐 드셨나요",
          "original_form": "아침으로 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040254.1.8",
          "form": "주말에 먹는다고 했잖아요",
          "original_form": "주말에 먹는다고 했잖아요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040254.1.9",
          "form": "오늘 드신 거예요 어제 드신 거예요",
          "original_form": "오늘 드신 거예요 어제 드신 거예요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.10",
          "form": "아침에 피부과 갔다가 농수산물 시장 가서",
          "original_form": "아침에 피부과 갔다가 농수산물 시장 가서",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.11",
          "form": "아 완전 부러워요 맛있었나요",
          "original_form": "아 완전 부러워요 맛있었나요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.12",
          "form": "과일 좀 사고 회센터가 서 전어 햇살아서 점심에 먹었습니다",
          "original_form": "과일 좀 사고 회센터가 서 전어 햇살아서 점심에 먹었습니다",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.13",
          "form": "세코 시로 먹었나요 필요 없게 먹었나요",
          "original_form": "세코 시로 먹었나요 필요 없게 먹었나요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.14",
          "form": "세꼬시 뼈 없게",
          "original_form": "세꼬시 뼈 없게",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.15",
          "form": "저는 세코 시로 먹고 싶었는데",
          "original_form": "저는 세코 시로 먹고 싶었는데",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.16",
          "form": "어떤 걸로 드셨나요 반반으로 먹었나요",
          "original_form": "어떤 걸로 드셨나요 반반으로 먹었나요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.17",
          "form": "반반 먹었어요",
          "original_form": "반반 먹었어요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040254.1.18",
          "form": "근데 그건 안중요했어요",
          "original_form": "근데 그건 안중요했어요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.19",
          "form": "맞아요 취향은 다양하지만 존중 해 줘야죠",
          "original_form": "맞아요 취향은 다양하지만 존중 해 줘야죠",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.20",
          "form": "맛있었나요",
          "original_form": "맛있었나요",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.21",
          "form": "마실 건 뭐로 같이 드셨나요 먹었나요",
          "original_form": "마실 건 뭐로 같이 드셨나요 먹었나요",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.22",
          "form": "그것까지  따질  시간에",
          "original_form": "그것까지  따질  시간에",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.23",
          "form": "한 점이라도 더 먹는 게 중요하더라구요",
          "original_form": "한 점이라도 더 먹는 게 중요하더라구요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.24",
          "form": "나도 전어회 먹고 싶다",
          "original_form": "나도 전어회 먹고 싶다",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.25",
          "form": "삼 페인 따 둔 게 있어서 먹었어요",
          "original_form": "삼 페인 따 둔 게 있어서 먹었어요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040254.1.26",
          "form": "저는 김밥 먹었는데",
          "original_form": "저는 김밥 먹었는데",
          "speaker_id": "2",
