        {
          "id": "MDRW2100027432.1.1",
          "form": "전복좋지",
          "original_form": "전복좋지",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.2",
          "form": "전복으로 죽해먹지",
          "original_form": "전복으로 죽해먹지",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.3",
          "form": "버터에 구워도 맛있지",
          "original_form": "버터에 구워도 맛있지",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.4",
          "form": "회로 먹는게 최고야",
          "original_form": "회로 먹는게 최고야",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.5",
          "form": "싱싱하다면 회가 좋아",
          "original_form": "싱싱하다면 회가 좋아",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.6",
          "form": "소라도 전복이랑 비슷해",
          "original_form": "소라도 전복이랑 비슷해",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.7",
          "form": "소라는 데쳐서 먹지만",
          "original_form": "소라는 데쳐서 먹지만",
          "speaker_id": "2",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027432.1.8",
          "form": "전복은 생으로 먹을수있지",
          "original_form": "전복은 생으로 먹을수있지",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027432.1.9",
          "form": "회는 전어회좋아",
          "original_form": "회는 전어회좋아",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027432.1.10",
          "form": "전어회는 지금이 제철이야",
          "original_form": "전어회는 지금이 제철이야",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027432.1.11",
          "form": "념념굳",
          "original_form": "념념굳",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027432.1.12",
          "form": "세꼬시가 맛있지",
          "original_form": "세꼬시가 맛있지",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027432.1.13",
          "form": "손질이 어려워",
          "original_form": "손질이 어려워",
          "speaker_id": "2",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027432.1.14",
          "form": "손질한 보람있지",
          "original_form": "손질한 보람있지",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027432.1.15",
          "form": "구우면 고소하지",
          "original_form": "구우면 고소하지",
          "speaker_id": "2",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027432.1.16",
          "form": "기름기 잘잘흘러",
          "original_form": "기름기 잘잘흘러",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027432.1.17",
          "form": "소스는 맛있어",
          "original_form": "소스는 맛있어",
