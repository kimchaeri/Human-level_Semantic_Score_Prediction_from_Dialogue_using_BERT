        {
          "id": "MDRW2100038251.1.7",
          "form": "얼마였나요",
          "original_form": "얼마였나요",
          "speaker_id": "2",
          "time": "20210902 03:15"
        },
        {
          "id": "MDRW2100038251.1.8",
          "form": "5900원ㅠㅠㅠㅠ",
