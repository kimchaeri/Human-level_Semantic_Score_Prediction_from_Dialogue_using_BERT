      "utterance": [
        {
          "id": "MDRW2100032738.1.1",
          "form": "좋아하는 신곡 있으세요?",
          "original_form": "좋아하는 신곡 있으세요?",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032738.1.2",
          "form": "음악을 잘 안들어서 ㅎㅎ",
          "original_form": "음악을 잘 안들어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032738.1.3",
          "form": "인디계열 좋아하는데 브로콜리너마저 신곡 나왔다고 하더라구요",
          "original_form": "인디계열 좋아하는데 브로콜리너마저 신곡 나왔다고 하더라구요",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032738.1.4",
          "form": "우와아 그렇다면 들어보는게 인지상정이죠",
          "original_form": "우와아 그렇다면 들어보는게 인지상정이죠",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032738.1.5",
          "form": "브로콜리너마저 이름 너무 귀여운데 음악은 또 넘 잘해요 ㅎㅎ",
          "original_form": "브로콜리너마저 이름 너무 귀여운데 음악은 또 넘 잘해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100032738.1.6",
          "form": "전 은퇴한줄 알았네여",
          "original_form": "전 은퇴한줄 알았네여",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032738.1.7",
          "form": "앜ㅋㅋㅋ 너무 안나오시긴 했죠!",
          "original_form": "앜ㅋㅋㅋ 너무 안나오시긴 했죠!",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032738.1.8",
          "form": "그래도 계속 음악하시니 넘 좋네요 ㅎㅎ",
          "original_form": "그래도 계속 음악하시니 넘 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032738.1.9",
          "form": "그럼 공연도 가고 그러신 거예요?",
          "original_form": "그럼 공연도 가고 그러신 거예요?",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032738.1.10",
          "form": "언니네이발관",
          "original_form": "언니네이발관",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032738.1.11",
          "form": "장얼",
          "original_form": "장얼",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032738.1.12",
          "form": "다 좋아했었는데 은퇴하시구 책 내시더라구요",
          "original_form": "다 좋아했었는데 은퇴하시구 책 내시더라구요",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032738.1.13",
          "form": "공연은 홍대 근처 살때 어릴때 많이 봤어요",
          "original_form": "공연은 홍대 근처 살때 어릴때 많이 봤어요",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032738.1.14",
          "form": "우와 홍대에서도 사셨구나 홍대에서 사는 것도 진짜 재밌을거 같아요~",
          "original_form": "우와 홍대에서도 사셨구나 홍대에서 사는 것도 진짜 재밌을거 같아요~",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032738.1.15",
          "form": "버스킹 공연도 많고~",
          "original_form": "버스킹 공연도 많고~",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032738.1.16",
          "form": "네 요즘엔 별로 안할 것 같지만요",
          "original_form": "네 요즘엔 별로 안할 것 같지만요",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032738.1.17",
          "form": "최근엔 새소년 선우정아 검정치마",
          "original_form": "최근엔 새소년 선우정아 검정치마",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032738.1.18",
          "form": "잘 들었어요",
          "original_form": "잘 들었어요",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032738.1.19",
          "form": "그러게요~ 얼른 코로나가 끝나야 될텐데 말이죠ㅠㅠㅠ",
          "original_form": "그러게요~ 얼른 코로나가 끝나야 될텐데 말이죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032738.1.20",
          "form": "노래 듣는 것도 직접 보러 가는 것도 좋아하는데 이제 정말 듣기만 하네요ㅠㅜ",
          "original_form": "노래 듣는 것도 직접 보러 가는 것도 좋아하는데 이제 정말 듣기만 하네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032738.1.21",
          "form": "작년에 이소라 콘서트 티켓 샀는데",
          "original_form": "작년에 이소라 콘서트 티켓 샀는데",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032738.1.22",
          "form": "연기됐다고 하다가 결국 취소 ㅠ",
          "original_form": "연기됐다고 하다가 결국 취소 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032738.1.23",
          "form": "으악 너무 아쉽겠어요ㅠㅠㅠ",
          "original_form": "으악 너무 아쉽겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032738.1.24",
          "form": "저도 이소라님 라이브 듣고 싶어요",
          "original_form": "저도 이소라님 라이브 듣고 싶어요",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032738.1.25",
          "form": "사실 선우정아님 콘서트 가려했은데",
          "original_form": "사실 선우정아님 콘서트 가려했은데",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032738.1.26",
          "form": "안하셔가지구ㅜㅜ",
          "original_form": "안하셔가지구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 23:14"
        }
      ]
    }
