      "utterance": [
        {
          "id": "MDRW2100018885.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018885.1.2",
          "form": "넵 저 실내사이클 자주 타요ㅋㅋㅋ",
          "original_form": "넵 저 실내사이클 자주 타요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018885.1.3",
          "form": "운동하시나요?",
          "original_form": "운동하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018885.1.4",
          "form": "오 실내사이클!!",
          "original_form": "오 실내사이클!!",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018885.1.5",
          "form": "저는 홈트하고 있어요 ㅋㅋ",
          "original_form": "저는 홈트하고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018885.1.6",
          "form": "저도 사이클 질릴 땐 홈트해요ㅋㅋㅋ",
          "original_form": "저도 사이클 질릴 땐 홈트해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018885.1.7",
          "form": "어떤거 하세요?",
          "original_form": "어떤거 하세요?",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018885.1.8",
          "form": "땅끄부부꺼 보면서 하는데\r\n지속이 안되네요ㅠㅋㅋ",
          "original_form": "땅끄부부꺼 보면서 하는데\r\n지속이 안되네요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018885.1.9",
          "form": "앗 땅끄부부 좋죠",
          "original_form": "앗 땅끄부부 좋죠",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018885.1.10",
          "form": "저 칼소폭 찐핵핵매운맛 자주 한답니다",
          "original_form": "저 칼소폭 찐핵핵매운맛 자주 한답니다",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018885.1.11",
          "form": "하얗게 불태우시는군요 ",
          "original_form": "하얗게 불태우시는군요 ",
          "speaker_id": "2",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100018885.1.12",
          "form": "첨엔 한번도 못했는데 이제 세번은 해야 좀 운동했다 싶어요ㅋㅋㅋ",
          "original_form": "첨엔 한번도 못했는데 이제 세번은 해야 좀 운동했다 싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018885.1.13",
          "form": "대단하세요!\r\n몸 완전 탄탄하실거 같아요^^",
          "original_form": "대단하세요!\r\n몸 완전 탄탄하실거 같아요^^",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100018885.1.14",
          "form": "앗... 저는 이제 막 시작한 초보운동러랍니다ㅋㅋㅋ",
          "original_form": "앗... 저는 이제 막 시작한 초보운동러랍니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018885.1.15",
          "form": "살이 급 많이쪄서 빼는 중이에요",
          "original_form": "살이 급 많이쪄서 빼는 중이에요",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100018885.1.16",
          "form": "초보운동러 치고는\r\n극한의 맛도 잘 견디시는데요?ㅋㅋ",
          "original_form": "초보운동러 치고는\r\n극한의 맛도 잘 견디시는데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100018885.1.17",
          "form": "저 딱 찐핵핵만 버틸 수 있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 딱 찐핵핵만 버틸 수 있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018885.1.18",
          "form": "찐핵핵에 특화된 몸인가봐요..",
          "original_form": "찐핵핵에 특화된 몸인가봐요..",
          "speaker_id": "1",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018885.1.19",
          "form": "빅씨스는 해보셨나요?",
          "original_form": "빅씨스는 해보셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018885.1.20",
          "form": "아니요:) 안해봤어요ㅋㅋ\r\n그건 어떠셨어요?",
