      "utterance": [
        {
          "id": "MDRW2100031755.1.1",
          "form": "야식 좋아하시나요?",
          "original_form": "야식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100031755.1.2",
          "form": "꼭 야식이 아니어도 어떤 음식을 좋아하세요?",
          "original_form": "꼭 야식이 아니어도 어떤 음식을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100031755.1.3",
          "form": "야식 진짜 하나 땡기면 꼭 먹어야 해요",
          "original_form": "야식 진짜 하나 땡기면 꼭 먹어야 해요",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100031755.1.4",
          "form": "저는 딱 몸에 안좋은거,,ㅎㅎ",
          "original_form": "저는 딱 몸에 안좋은거,,ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100031755.1.5",
          "form": "떡볶이나 갈비나,, 핏자 치킨,,등등이요 ㅎ",
          "original_form": "떡볶이나 갈비나,, 핏자 치킨,,등등이요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100031755.1.6",
          "form": "아 세상에 지금 크리티컬 당했어요",
          "original_form": "아 세상에 지금 크리티컬 당했어요",
          "speaker_id": "1",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100031755.1.7",
          "form": "떡볶이 좋죠 맨날 먹어도 좋아요",
          "original_form": "떡볶이 좋죠 맨날 먹어도 좋아요",
          "speaker_id": "1",
          "time": "20210905 00:06"
        },
        {
          "id": "MDRW2100031755.1.8",
          "form": "갈비! 좋죠",
          "original_form": "갈비! 좋죠",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031755.1.9",
          "form": "피자 치킨 최근에 안먹어서 진짜 왕떙기네요",
          "original_form": "피자 치킨 최근에 안먹어서 진짜 왕떙기네요",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031755.1.10",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031755.1.11",
          "form": "괜히 죄송하네요..",
          "original_form": "괜히 죄송하네요..",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031755.1.12",
          "form": "저도 먹고싶어요ㅠㅠ",
          "original_form": "저도 먹고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031755.1.13",
          "form": "피자,,",
          "original_form": "피자,,",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031755.1.14",
          "form": "내일 저녁각이네요 ㅋㅋㅎ",
          "original_form": "내일 저녁각이네요 ㅋㅋㅎ",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031755.1.15",
          "form": "아니요 ㅋㅋㅋㅋㅋ 내일 저녁으로 먹을겁니다",
          "original_form": "아니요 ㅋㅋㅋㅋㅋ 내일 저녁으로 먹을겁니다",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100031755.1.16",
          "form": "피자 먹을거에요",
          "original_form": "피자 먹을거에요",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100031755.1.17",
          "form": "ㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031755.1.18",
          "form": "단호하시지 마세요!!",
          "original_form": "단호하시지 마세요!!",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031755.1.19",
          "form": "샐러드 먹어야 건강해져요 ㅠㅠㅠ",
          "original_form": "샐러드 먹어야 건강해져요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031755.1.20",
          "form": "저번주부터 피자 먹겠다고 마음 먹고 있었어요",
          "original_form": "저번주부터 피자 먹겠다고 마음 먹고 있었어요",
          "speaker_id": "1",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031755.1.21",
          "form": "저는 고구마무스도 추가하고 스파게키도 추가해서 먹을래요",
          "original_form": "저는 고구마무스도 추가하고 스파게키도 추가해서 먹을래요",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031755.1.22",
          "form": "샐러드... 그게 뭐죠? 피자에도 채소는 들어가있다구요! 흑흑",
          "original_form": "샐러드... 그게 뭐죠? 피자에도 채소는 들어가있다구요! 흑흑",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031755.1.23",
          "form": "디핑소스도 추가해야지",
          "original_form": "디핑소스도 추가해야지",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031755.1.24",
          "form": "무슨 피자 좋아하세요??",
          "original_form": "무슨 피자 좋아하세요??",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031755.1.25",
          "form": "아 이 배우신분",
          "original_form": "아 이 배우신분",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031755.1.26",
          "form": "고구마무스피자 맛있죠",
          "original_form": "고구마무스피자 맛있죠",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031755.1.27",
          "form": "스파케티 추가는 안하면 서운한걸요",
          "original_form": "스파케티 추가는 안하면 서운한걸요",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100031755.1.28",
          "form": "디핑소스까지.. 아주 훌륭합니다",
          "original_form": "디핑소스까지.. 아주 훌륭합니다",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100031755.1.29",
          "form": "피자 호불호가 따로 없는 편이에요!",
          "original_form": "피자 호불호가 따로 없는 편이에요!",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100031755.1.30",
          "form": "있으면 다 먹어요 ㅋㅋ",
          "original_form": "있으면 다 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100031755.1.31",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031755.1.32",
          "form": "오 다 잘 드시는구나!!",
          "original_form": "오 다 잘 드시는구나!!",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031755.1.33",
          "form": "저는 불고기피자랑 포테이토 좋아해요!!",
          "original_form": "저는 불고기피자랑 포테이토 좋아해요!!",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031755.1.34",
          "form": "음 둘다 좋아요!",
          "original_form": "음 둘다 좋아요!",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031755.1.35",
          "form": "포테이토 피자도 상당히 안먹은지 된것 같아요",
          "original_form": "포테이토 피자도 상당히 안먹은지 된것 같아요",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031755.1.36",
          "form": "내일 무슨피자 먹지",
          "original_form": "내일 무슨피자 먹지",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100031755.1.37",
          "form": "선호하는 브랜드 있으세요??",
          "original_form": "선호하는 브랜드 있으세요??",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031755.1.38",
          "form": "선호한다기보다는 가까워서 피자헛 자주 먹는 편이에요",
          "original_form": "선호한다기보다는 가까워서 피자헛 자주 먹는 편이에요",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031755.1.39",
          "form": "좋아하는 브랜드 있으세요?",
          "original_form": "좋아하는 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031755.1.40",
          "form": "테이크 아웃하면 40프로인가 하지 않나요??",
          "original_form": "테이크 아웃하면 40프로인가 하지 않나요??",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031755.1.41",
          "form": "저희 집 근처에도 피자헛 있어요 !",
          "original_form": "저희 집 근처에도 피자헛 있어요 !",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031755.1.42",
          "form": "저는 피자알볼로가 요즘 맛있더라구요",
          "original_form": "저는 피자알볼로가 요즘 맛있더라구요",
          "speaker_id": "2",
          "time": "20210905 00:19"
        },
        {
          "id": "MDRW2100031755.1.43",
          "form": "맞아요 ㅋㅋㅋ 40프로 혜택을 놓을 수가 없어요",
          "original_form": "맞아요 ㅋㅋㅋ 40프로 혜택을 놓을 수가 없어요",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031755.1.44",
          "form": "피자 알볼로는 어떤 피자가 맛있어요?",
          "original_form": "피자 알볼로는 어떤 피자가 맛있어요?",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031755.1.45",
          "form": "요즘 피자값도 비싸서",
          "original_form": "요즘 피자값도 비싸서",
          "speaker_id": "2",
          "time": "20210905 00:22"
        },
        {
          "id": "MDRW2100031755.1.46",
          "form": "피자 알볼로 어깨피자(피자이름입니다) 가 맛있더라구요",
          "original_form": "피자 알볼로 어깨피자(피자이름입니다) 가 맛있더라구요",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031755.1.47",
          "form": "근데 저도 내일은 피자헛 먹을래요 ㅋㅋㅋ",
          "original_form": "근데 저도 내일은 피자헛 먹을래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031755.1.48",
          "form": "주변에 매장이 있는지 찾아봐야겠어요",
          "original_form": "주변에 매장이 있는지 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        }
