      "utterance": [
        {
          "id": "MDRW2100021794.1.1",
          "form": "어떤 계절 가장 좋아하세요?",
          "original_form": "어떤 계절 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021794.1.2",
          "form": "저는 가을이 좋아요\r\nname1은요?",
          "original_form": "저는 가을이 좋아요\r\n&name1&은요?",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021794.1.3",
          "form": "저도 가을 가장 좋아해요!",
          "original_form": "저도 가을 가장 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021794.1.4",
          "form": "올해는 가을이 좀 길었으면 좋겠어요",
          "original_form": "올해는 가을이 좀 길었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021794.1.5",
          "form": "그러게요 매번 스치듯 안녕 지나가버려요",
          "original_form": "그러게요 매번 스치듯 안녕 지나가버려요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100021794.1.6",
          "form": "맞아요.. 옷도 가을옷 제일 좋아하는데",
          "original_form": "맞아요.. 옷도 가을옷 제일 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021794.1.7",
          "form": "다 못 입네요ㅠㅜ",
          "original_form": "다 못 입네요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021794.1.8",
          "form": "놀러 다니는 것도 가을이 좋아요",
          "original_form": "놀러 다니는 것도 가을이 좋아요",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021794.1.9",
          "form": "날씨도 쾌청하고 날씨도 선선하구요",
          "original_form": "날씨도 쾌청하고 날씨도 선선하구요",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021794.1.10",
          "form": "맞아요 봄도 좋긴한데 미세먼지가 너무 심해요",
          "original_form": "맞아요 봄도 좋긴한데 미세먼지가 너무 심해요",
          "speaker_id": "1",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021794.1.11",
          "form": "가을이 딱 좋은 거 같아요",
          "original_form": "가을이 딱 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021794.1.12",
          "form": "입추, 처서 매직으로 그래도 이제 살것같아요",
          "original_form": "입추, 처서 매직으로 그래도 이제 살것같아요",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100021794.1.13",
          "form": "맞아요 더위가 많이 가셨어요ㅎㅎ",
          "original_form": "맞아요 더위가 많이 가셨어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:53"
        },
        {
          "id": "MDRW2100021794.1.14",
          "form": "아직 매미소리는 들리지만",
          "original_form": "아직 매미소리는 들리지만",
          "speaker_id": "1",
          "time": "20210829 14:53"
        },
        {
          "id": "MDRW2100021794.1.15",
          "form": "그래도 9월까지는 더울것같아요",
          "original_form": "그래도 9월까지는 더울것같아요",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100021794.1.16",
          "form": "요새 날씨가 이상해요",
          "original_form": "요새 날씨가 이상해요",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100021794.1.17",
          "form": "그쵸 그래도 그새 많이 쌀쌀해졌어요",
          "original_form": "그쵸 그래도 그새 많이 쌀쌀해졌어요",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100021794.1.18",
          "form": "아침에 일어날 때 추워요ㅋㅋ",
          "original_form": "아침에 일어날 때 추워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100021794.1.19",
          "form": "가을비가 내리고 나니 새벽엔 추워요",
          "original_form": "가을비가 내리고 나니 새벽엔 추워요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100021794.1.20",
          "form": "얇은 이불 덮어요",
          "original_form": "얇은 이불 덮어요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100021794.1.21",
          "form": "맞아요 저 그저께는 전기장판 잠깐 켰어요",
          "original_form": "맞아요 저 그저께는 전기장판 잠깐 켰어요",
