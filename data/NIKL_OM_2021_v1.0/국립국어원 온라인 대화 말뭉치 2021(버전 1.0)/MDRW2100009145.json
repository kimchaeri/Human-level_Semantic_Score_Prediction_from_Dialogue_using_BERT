      "utterance": [
        {
          "id": "MDRW2100009145.1.1",
          "form": "혹시 종교가 있으세요?",
          "original_form": "혹시 종교가 있으세요?",
          "speaker_id": "1",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009145.1.2",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009145.1.3",
          "form": "예전에 성당 다녔어ㅛ",
          "original_form": "예전에 성당 다녔어ㅛ",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009145.1.4",
          "form": "와 정말요..! 성당이면 천주교이신가요?",
          "original_form": "와 정말요..! 성당이면 천주교이신가요?",
          "speaker_id": "1",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009145.1.5",
          "form": "지금은 잘 안가지만 맞아요 ㅎㅎ",
          "original_form": "지금은 잘 안가지만 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009145.1.6",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009145.1.7",
          "form": "저는 불교에요! 그래서 천주교나 기독교 쪽은 잘 몰라서 궁금했어요ㅎㅎ",
          "original_form": "저는 불교에요! 그래서 천주교나 기독교 쪽은 잘 몰라서 궁금했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009145.1.8",
          "form": "와 불교! 궁금해요",
          "original_form": "와 불교! 궁금해요",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009145.1.9",
          "form": "불교 하면 뭔가 고요하고 평화로운 느낌이에요",
          "original_form": "불교 하면 뭔가 고요하고 평화로운 느낌이에요",
          "speaker_id": "2",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100009145.1.10",
          "form": "맞아요 저 어릴 때는 아무 생각 없었는데",
          "original_form": "맞아요 저 어릴 때는 아무 생각 없었는데",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009145.1.11",
          "form": "요즘은 가족들이랑 절에 가면 그 분위기가 너무 좋더라구요",
          "original_form": "요즘은 가족들이랑 절에 가면 그 분위기가 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009145.1.12",
          "form": "복잡한 생각들이 씻겨나가는 기분이에요",
          "original_form": "복잡한 생각들이 씻겨나가는 기분이에요",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009145.1.13",
          "form": "절 특유의 향도 너무 좋은것같아요",
          "original_form": "절 특유의 향도 너무 좋은것같아요",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009145.1.14",
          "form": "요즘도 자주 가시나봐요?",
          "original_form": "요즘도 자주 가시나봐요?",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009145.1.15",
          "form": "맞아요 저도 그 향 좋아해요",
          "original_form": "맞아요 저도 그 향 좋아해요",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009145.1.16",
          "form": "자주는 아니지만 분기마다 한두번씩은 가요!",
          "original_form": "자주는 아니지만 분기마다 한두번씩은 가요!",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009145.1.17",
          "form": "부모님이 불교라서 자연스럽게 같이 가게 돼요",
          "original_form": "부모님이 불교라서 자연스럽게 같이 가게 돼요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009145.1.18",
          "form": "부모님 종교 따라가는경우가 많죠",
          "original_form": "부모님 종교 따라가는경우가 많죠",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009145.1.19",
          "form": "저도 성당 그렇게 더니기 시작했었내요",
          "original_form": "저도 성당 그렇게 더니기 시작했었내요",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009145.1.20",
          "form": "맞아요 그래도 어릴 때는 따라만 가는거지 무교라고 했는데",
          "original_form": "맞아요 그래도 어릴 때는 따라만 가는거지 무교라고 했는데",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009145.1.21",
          "form": "이걸 스며들었다고 하나요..ㅋㅋㅋ",
          "original_form": "이걸 스며들었다고 하나요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009145.1.22",
          "form": "어 다똑같내요 ㅋㅋㅋㅋㅋ",
          "original_form": "어 다똑같내요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009145.1.23",
          "form": "저도 그랬어요",
          "original_form": "저도 그랬어요",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009145.1.24",
          "form": "종교가..스며들었네요",
          "original_form": "종교가..스며들었네요",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009145.1.25",
          "form": "근데 소속감 느끼기 참좋은 장소같아요",
          "original_form": "근데 소속감 느끼기 참좋은 장소같아요",
          "speaker_id": "2",
          "time": "20210822 22:41"
        },
        {
          "id": "MDRW2100009145.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ스며들었죠..!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ스며들었죠..!",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009145.1.27",
          "form": "맞아요 성당이나 교회는 더 그럴 것 같아요",
          "original_form": "맞아요 성당이나 교회는 더 그럴 것 같아요",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009145.1.28",
          "form": "정기적으로 가고 종교인 분들이랑도 계속 교류하니까요",
          "original_form": "정기적으로 가고 종교인 분들이랑도 계속 교류하니까요",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009145.1.29",
          "form": "맞아요 ㅋㅋㅋ 지금은 잘 안나가서 교류가없지만",
          "original_form": "맞아요 ㅋㅋㅋ 지금은 잘 안나가서 교류가없지만",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009145.1.30",
          "form": "예전에는 같이 여행도가고 그랬었내요",
          "original_form": "예전에는 같이 여행도가고 그랬었내요",
          "speaker_id": "2",
          "time": "20210822 23:27"
        }
      ]
