      "utterance": [
        {
          "id": "MDRW2100034526.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034526.1.2",
          "form": "네 칼국수 먹었어요. name1님은요?",
          "original_form": "네 칼국수 먹었어요. &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034526.1.3",
          "form": "오 칼국수 맛있었겠네요",
          "original_form": "오 칼국수 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034526.1.4",
          "form": "저는 삼치 구워 먹었어요",
          "original_form": "저는 삼치 구워 먹었어요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100034526.1.5",
          "form": "삼치 구이 맛있죠. 저도 저녁엔 조기 구워먹으려구요",
          "original_form": "삼치 구이 맛있죠. 저도 저녁엔 조기 구워먹으려구요",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100034526.1.6",
          "form": "네 이번에 삼치 잡았다고 해서 산지직송으로 받았거든요",
          "original_form": "네 이번에 삼치 잡았다고 해서 산지직송으로 받았거든요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100034526.1.7",
          "form": "신선해서인지 아주 맛있게 먹었어요",
          "original_form": "신선해서인지 아주 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034526.1.8",
          "form": "산지직송이라니 부럽네요",
          "original_form": "산지직송이라니 부럽네요",
          "speaker_id": "2",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100034526.1.9",
          "form": "낚시 하시는 분이 있으신가봐요",
          "original_form": "낚시 하시는 분이 있으신가봐요",
          "speaker_id": "2",
          "time": "20210828 15:00"
        },
        {
          "id": "MDRW2100034526.1.10",
          "form": "아 그건 아니고",
          "original_form": "아 그건 아니고",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100034526.1.11",
          "form": "대청도 쪽에 계시던 분이 소개해주셔서요ㅎㅎ",
          "original_form": "대청도 쪽에 계시던 분이 소개해주셔서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100034526.1.12",
          "form": "요리 잘 하세요?",
          "original_form": "요리 잘 하세요?",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034526.1.13",
          "form": "잘하지는 않아요ㅎㅎ",
          "original_form": "잘하지는 않아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100034526.1.14",
          "form": "저도 유튜브 보고 따라하는 정도 ㅋㅋ",
          "original_form": "저도 유튜브 보고 따라하는 정도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100034526.1.15",
          "form": "오 그래도 따라할 정도면 요리 실력이 어느정도 있는거죠",
          "original_form": "오 그래도 따라할 정도면 요리 실력이 어느정도 있는거죠",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100034526.1.16",
          "form": "칼질은 영 예쁘게는 안 되네요 ㅋㅋ",
          "original_form": "칼질은 영 예쁘게는 안 되네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100034526.1.17",
          "form": "칼질이 힘들죠 은근",
          "original_form": "칼질이 힘들죠 은근",
          "speaker_id": "1",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034526.1.18",
          "form": "무나 당근같은 딱딱한 채소가 진짜 힘들더라구요",
          "original_form": "무나 당근같은 딱딱한 채소가 진짜 힘들더라구요",
          "speaker_id": "2",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034526.1.19",
          "form": "네 그래서 저는 채칼 써요",
          "original_form": "네 그래서 저는 채칼 써요",
          "speaker_id": "1",
          "time": "20210828 15:14"
        }
