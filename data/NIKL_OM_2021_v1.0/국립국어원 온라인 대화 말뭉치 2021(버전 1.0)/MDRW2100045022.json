      "utterance": [
        {
          "id": "MDRW2100045022.1.1",
          "form": "네ㅜㅠㅠ 저 빵이랑 케익 다 사랑해요ㅠㅠ",
          "original_form": "네ㅜㅠㅠ 저 빵이랑 케익 다 사랑해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045022.1.2",
          "form": "초코 케익도 좋아하세요?!",
          "original_form": "초코 케익도 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045022.1.3",
          "form": "네네!!!! 초코 또 엄청 사랑하죠ㅠㅠ",
          "original_form": "네네!!!! 초코 또 엄청 사랑하죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045022.1.4",
          "form": "아 밀가루 너무 좋은거 같아요ㅎㅎ",
          "original_form": "아 밀가루 너무 좋은거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045022.1.5",
          "form": "ㅋㅋㅋ저두요ㅠㅠㅠㅠㅠ초코케익 맛있는 곳 있나요?",
          "original_form": "ㅋㅋㅋ저두요ㅠㅠㅠㅠㅠ초코케익 맛있는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045022.1.6",
          "form": "아 집근처 개인카페에요ㅠㅠ",
          "original_form": "아 집근처 개인카페에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045022.1.7",
          "form": "바게트 같은 빵도 좋아하시나요?",
          "original_form": "바게트 같은 빵도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045022.1.8",
          "form": "네 저완전 좋아해요 ㅋㅋㅋ 건강빵류 ㅋㅋㅋ치아바타",
          "original_form": "네 저완전 좋아해요 ㅋㅋㅋ 건강빵류 ㅋㅋㅋ치아바타",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045022.1.9",
          "form": "아 치아바타 저도 좋아해요!!",
          "original_form": "아 치아바타 저도 좋아해요!!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045022.1.10",
          "form": "올리브 치아바타 좋아하세요?",
          "original_form": "올리브 치아바타 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045022.1.11",
          "form": "허어어엉 ㅠㅠ  네 너무좋아하죠",
          "original_form": "허어어엉 ㅠㅠ  네 너무좋아하죠",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045022.1.12",
          "form": "말하니까 먹구싶어요ㅠㅠ",
          "original_form": "말하니까 먹구싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
