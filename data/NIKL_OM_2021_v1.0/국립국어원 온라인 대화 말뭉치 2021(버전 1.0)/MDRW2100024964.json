          "form": "스토닉?",
          "original_form": "스토닉?",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.49",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.50",
          "form": "사나운거말고",
          "original_form": "사나운거말고",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.51",
          "form": "귀엽게생긴거사고싶은데",
          "original_form": "귀엽게생긴거사고싶은데",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.52",
          "form": "?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024964.1.53",
          "form": "너무사납게생김",
          "original_form": "너무사납게생김",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024964.1.54",
          "form": "저는 둥굴듕굴한거",
          "original_form": "저는 둥굴듕굴한거",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024964.1.55",
          "form": "베뉴가 커여워요?",
          "original_form": "베뉴가 커여워요?",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
