      "utterance": [
        {
          "id": "MDRW2100024964.1.1",
          "form": "남친이 학벌 더 좋아요?",
          "original_form": "남친이 학벌 더 좋아요?",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024964.1.2",
          "form": "학벌이요?",
          "original_form": "학벌이요?",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024964.1.3",
          "form": "대학중퇴함",
          "original_form": "대학중퇴함",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024964.1.4",
          "form": "캐나다로 유학간다니까요 ㅋㅋㅋㅋㅋ1월에",
          "original_form": "캐나다로 유학간다니까요 ㅋㅋㅋㅋㅋ1월에",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024964.1.5",
          "form": "최소 31에 자리잡을듯요",
          "original_form": "최소 31에 자리잡을듯요",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024964.1.6",
          "form": "내나이네",
          "original_form": "내나이네",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.7",
          "form": "근데 저도 직딩생활 빨리하긴했지만",
          "original_form": "근데 저도 직딩생활 빨리하긴했지만",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.8",
          "form": "28까지도 결혼 생각만 했지 실행은 못함",
          "original_form": "28까지도 결혼 생각만 했지 실행은 못함",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.9",
          "form": "후달랴서...",
          "original_form": "후달랴서...",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.10",
          "form": "얘 기다렷다간 제가너무늦을듯요",
          "original_form": "얘 기다렷다간 제가너무늦을듯요",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.12",
          "form": "이래서 인생은 타이밍인건가 싶기도 했지만",
          "original_form": "이래서 인생은 타이밍인건가 싶기도 했지만",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.13",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024964.1.14",
          "form": "인생 ㄹㅇ 타이밍",
          "original_form": "인생 ㄹㅇ 타이밍",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024964.1.15",
          "form": "어렵드라구요 물론 그땐 용기도 없었고",
          "original_form": "어렵드라구요 물론 그땐 용기도 없었고",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024964.1.16",
          "form": "막무가내로 했으면 뭐 달라졌으려나 싶긴 하지만",
          "original_form": "막무가내로 했으면 뭐 달라졌으려나 싶긴 하지만",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024964.1.17",
          "form": "혼자 일해서 먹여살릴 자신은 없어서ㅎ",
          "original_form": "혼자 일해서 먹여살릴 자신은 없어서ㅎ",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024964.1.18",
          "form": "둘다자리잡고 결혼해야죠 ㅋㅋㅋㅋㅋ",
          "original_form": "둘다자리잡고 결혼해야죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024964.1.19",
          "form": "ㅋㅋㅋ엄마가 오래 만나니까 허겁지겁 집은 사셨었는데",
          "original_form": "ㅋㅋㅋ엄마가 오래 만나니까 허겁지겁 집은 사셨었는데",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024964.1.20",
          "form": "운좋게 청약이 됐었어가지고ㅋㅋ",
          "original_form": "운좋게 청약이 됐었어가지고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024964.1.21",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024964.1.22",
          "form": "사실 그럼 여친이 취업만햇어도",
          "original_form": "사실 그럼 여친이 취업만햇어도",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024964.1.23",
          "form": "오는 무슨 이젠 처치곤란이죠",
          "original_form": "오는 무슨 이젠 처치곤란이죠",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024964.1.24",
          "form": "가능성잇엇겟는데요",
          "original_form": "가능성잇엇겟는데요",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024964.1.25",
          "form": "에이 요즘 빚없는사람이 어딧음",
          "original_form": "에이 요즘 빚없는사람이 어딧음",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024964.1.26",
          "form": "취업 했으면 결혼했을걸요?",
          "original_form": "취업 했으면 결혼했을걸요?",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024964.1.27",
          "form": "아마도?",
          "original_form": "아마도?",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024964.1.28",
          "form": "아쉽네",
          "original_form": "아쉽네",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024964.1.29",
          "form": "다시만날생각은없나봐요",
          "original_form": "다시만날생각은없나봐요",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024964.1.30",
          "form": "네 살면서 다신 마주치지 않기를.",
          "original_form": "네 살면서 다신 마주치지 않기를.",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024964.1.31",
          "form": "원래 칼같아요",
          "original_form": "원래 칼같아요",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.32",
          "form": "아니면 아닌거임",
          "original_form": "아니면 아닌거임",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.33",
          "form": "헐ㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.34",
          "form": "매섭다매서워",
          "original_form": "매섭다매서워",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.35",
          "form": "그니까욬ㅋㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.36",
          "form": "최근에 뭐 사신거라도..",
          "original_form": "최근에 뭐 사신거라도..",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.37",
          "form": "워치말고",
          "original_form": "워치말고",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.38",
          "form": "엄,,,,,,",
          "original_form": "엄,,,,,,",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024964.1.39",
          "form": "그럼 차얘기 ㄱㄱ",
          "original_form": "그럼 차얘기 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.40",
          "form": "뭐 알아보십니까?",
          "original_form": "뭐 알아보십니까?",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.41",
          "form": "suv요",
          "original_form": "suv요",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.42",
          "form": "베뉴같은거",
          "original_form": "베뉴같은거",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.43",
          "form": "소형",
          "original_form": "소형",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100024964.1.44",
