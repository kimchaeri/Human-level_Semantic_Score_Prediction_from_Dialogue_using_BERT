        {
          "id": "MDRW2100035334.1.1",
          "form": "11위 어깨춤이 절로 나오는 노래",
          "original_form": "11위 어깨춤이 절로 나오는 노래",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100035334.1.2",
          "form": "트와이스",
          "original_form": "트와이스",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100035334.1.3",
          "form": "어깨춤이라ㅋ 제목한번",
          "original_form": "어깨춤이라ㅋ 제목한번",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100035334.1.4",
          "form": "그러게 난 어깨춤은 안나온다 ㅋㅋ",
          "original_form": "그러게 난 어깨춤은 안나온다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100035334.1.5",
          "form": "전혀 나도 어깨춤 안 나온데ㅋ",
          "original_form": "전혀 나도 어깨춤 안 나온데ㅋ",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100035334.1.6",
          "form": "이게 순위는 가수들인데 얘기는 안무가 위주로 하네",
          "original_form": "이게 순위는 가수들인데 얘기는 안무가 위주로 하네",
          "speaker_id": "2",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100035334.1.7",
          "form": "안무순위 같다",
          "original_form": "안무순위 같다",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100035334.1.8",
          "form": "그러게 틀어야겠다  다 모르는 내용",
          "original_form": "그러게 틀어야겠다  다 모르는 내용",
          "speaker_id": "2",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100035334.1.9",
          "form": "ㅋㅋㅋ 1 위는 봤는데",
          "original_form": "ㅋㅋㅋ 1 위는 봤는데",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035334.1.10",
          "form": "아 그래 누구였는데",
          "original_form": "아 그래 누구였는데",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100035334.1.11",
          "form": "싸이 안무가고 강남스타일",
          "original_form": "싸이 안무가고 강남스타일",
          "speaker_id": "1",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100035334.1.12",
          "form": "아~ 1위 할만하다 인정",
          "original_form": "아~ 1위 할만하다 인정",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100035334.1.13",
          "form": "나도 보면서 인정했다",
          "original_form": "나도 보면서 인정했다",
          "speaker_id": "1",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100035334.1.14",
