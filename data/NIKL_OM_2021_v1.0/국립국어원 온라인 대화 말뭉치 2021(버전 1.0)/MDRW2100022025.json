      "utterance": [
        {
          "id": "MDRW2100022025.1.1",
          "form": "티비 자주 보시나요?",
          "original_form": "티비 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022025.1.2",
          "form": "티비는 안보고",
          "original_form": "티비는 안보고",
          "speaker_id": "2",
          "time": "20210829 07:06"
        },
        {
          "id": "MDRW2100022025.1.3",
          "form": "넷플봐요!",
          "original_form": "넷플봐요!",
          "speaker_id": "2",
          "time": "20210829 07:06"
        },
        {
          "id": "MDRW2100022025.1.4",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210829 09:21"
        },
        {
          "id": "MDRW2100022025.1.5",
          "form": "요즘엔 어떤거 보시나요?",
          "original_form": "요즘엔 어떤거 보시나요?",
          "speaker_id": "1",
          "time": "20210829 09:21"
        },
        {
          "id": "MDRW2100022025.1.6",
          "form": "슬의 열심히보고있어요",
          "original_form": "슬의 열심히보고있어요",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022025.1.7",
          "form": "체인지대이즈랑",
          "original_form": "체인지대이즈랑",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022025.1.8",
          "form": "오 저도 체인지데이즈 재밌게 보고 있어요!",
          "original_form": "오 저도 체인지데이즈 재밌게 보고 있어요!",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022025.1.9",
          "form": "주제가 색달라서 그런지 재밌더라구요",
          "original_form": "주제가 색달라서 그런지 재밌더라구요",
          "speaker_id": "1",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022025.1.10",
          "form": "와! 보는 사람 처음봐서 신나내요 ㅎㅎㅎ",
          "original_form": "와! 보는 사람 처음봐서 신나내요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022025.1.11",
          "form": "저 진짜 재밌게 보고있거든요",
          "original_form": "저 진짜 재밌게 보고있거든요",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022025.1.12",
          "form": "결말이 넘 궁금하고ㅠ",
          "original_form": "결말이 넘 궁금하고ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022025.1.13",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022025.1.14",
          "form": "다들 어떻게 될지 짐작이 안되서 빨리 결말 보고싶어요",
          "original_form": "다들 어떻게 될지 짐작이 안되서 빨리 결말 보고싶어요",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022025.1.15",
          "form": "혹시 환승연애도 보시나요?",
          "original_form": "혹시 환승연애도 보시나요?",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100022025.1.16",
          "form": "티빙애서 하던가요?",
          "original_form": "티빙애서 하던가요?",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022025.1.17",
          "form": "궁금한데 못보고있어요 ㅜㅜ",
          "original_form": "궁금한데 못보고있어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022025.1.18",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022025.1.19",
          "form": "환승연애도 재밌어요!",
          "original_form": "환승연애도 재밌어요!",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100022025.1.20",
          "form": "티빙에서 한 번 보세요!",
          "original_form": "티빙에서 한 번 보세요!",
          "speaker_id": "1",
          "time": "20210901 00:15"
        },
        {
          "id": "MDRW2100022025.1.21",
          "form": "헐 들어봣어요",
          "original_form": "헐 들어봣어요",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100022025.1.22",
          "form": "체인지데이즈 재밌었으면 환승연애도 잘보실거같아요!",
          "original_form": "체인지데이즈 재밌었으면 환승연애도 잘보실거같아요!",
          "speaker_id": "1",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100022025.1.23",
          "form": "최근에 영화 보신거 있으신가요?",
          "original_form": "최근에 영화 보신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100022025.1.24",
          "form": "음~ 최근에.. 크루엘라?",
          "original_form": "음~ 최근에.. 크루엘라?",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100022025.1.25",
          "form": "크루엘라 후기가 좋던데 재밌으셨나요?",
          "original_form": "크루엘라 후기가 좋던데 재밌으셨나요?",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100022025.1.26",
          "form": "옷보는 재미가 쏠쏠했어요",
          "original_form": "옷보는 재미가 쏠쏠했어요",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100022025.1.27",
          "form": "내용은 무난했는데 눈이 즐거워서",
          "original_form": "내용은 무난했는데 눈이 즐거워서",
          "speaker_id": "2",
          "time": "20210901 20:45"
        }
      ]
    }
