      "utterance": [
        {
          "id": "MDRW2100005381.1.1",
          "form": "토요일애는 가고",
          "original_form": "토요일애는 가고",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005381.1.2",
          "form": "일요일애는 쉬고있어요",
          "original_form": "일요일애는 쉬고있어요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005381.1.3",
          "form": "가끔 컨디션이 안조흔 날ㅇㅔ는",
          "original_form": "가끔 컨디션이 안조흔 날ㅇㅔ는",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005381.1.4",
          "form": "산책만 나가고요",
          "original_form": "산책만 나가고요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005381.1.5",
          "form": "헬스장은 집 근처로 가시나요?",
          "original_form": "헬스장은 집 근처로 가시나요?",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005381.1.6",
          "form": "저는 버스타고 다녀요 ㅠㅠ",
          "original_form": "저는 버스타고 다녀요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005381.1.7",
          "form": "친구랑 같이 다니느라 그렇게햤는",
          "original_form": "친구랑 같이 다니느라 그렇게햤는",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005381.1.8",
          "form": "데",
          "original_form": "데",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005381.1.9",
          "form": "이번에 피티끝나면 근처로 옮길것같아요",
          "original_form": "이번에 피티끝나면 근처로 옮길것같아요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005381.1.10",
          "form": "운동 한번 다녀오면 시간이 너무 걸리거든요",
          "original_form": "운동 한번 다녀오면 시간이 너무 걸리거든요",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005381.1.11",
          "form": "집근처가 최고죠",
          "original_form": "집근처가 최고죠",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005381.1.12",
          "form": "근육 풀 때는",
          "original_form": "근육 풀 때는",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005381.1.13",
          "form": "스트레칭만 하나요?",
          "original_form": "스트레칭만 하나요?",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005381.1.14",
          "form": "유튜브보면서 스트레칭 하거나",
          "original_form": "유튜브보면서 스트레칭 하거나",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005381.1.15",
          "form": "폼롤러랑 볼 많이 써요",
          "original_form": "폼롤러랑 볼 많이 써요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005381.1.16",
          "form": "엄청 시원해요",
          "original_form": "엄청 시원해요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005381.1.17",
          "form": "폼롤러 저도 아주 애정하는 물건이에요",
          "original_form": "폼롤러 저도 아주 애정하는 물건이에요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005381.1.18",
          "form": "다리나 엉덩이 근육 풀어주면 엄청 시원하고 좋아요",
          "original_form": "다리나 엉덩이 근육 풀어주면 엄청 시원하고 좋아요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005381.1.19",
          "form": "그래서 긴 거 짧은 거 다 샀어요 ㅋㅋㅋㅋ",
          "original_form": "그래서 긴 거 짧은 거 다 샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005381.1.20",
          "form": "짧은건 사용안해봤는대",
          "original_form": "짧은건 사용안해봤는대",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005381.1.21",
          "form": "긴거에 비해 이점이 있나요?",
          "original_form": "긴거에 비해 이점이 있나요?",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005381.1.22",
          "form": "허벅지 안쪽이나 겨드랑이 풀 때 간편하고 좋아요",
          "original_form": "허벅지 안쪽이나 겨드랑이 풀 때 간편하고 좋아요",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005381.1.23",
          "form": "근데 긴거 있다면 하나로 다 할 수 있긴 하니까요!",
          "original_form": "근데 긴거 있다면 하나로 다 할 수 있긴 하니까요!",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005381.1.24",
          "form": "마사지볼은 어디에 쓰나요?",
          "original_form": "마사지볼은 어디에 쓰나요?",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005381.1.25",
          "form": "아아 또 살필요는 없겠내요 그럼 ㅎㅎ",
          "original_form": "아아 또 살필요는 없겠내요 그럼 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005381.1.26",
          "form": "마사지볼은 부족하다 싶은 부분임ㅅ을땨",
          "original_form": "마사지볼은 부족하다 싶은 부분임ㅅ을땨",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005381.1.27",
          "form": "집중적으로 풀어주고있어요",
          "original_form": "집중적으로 풀어주고있어요",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005381.1.28",
          "form": "발아플때 발바닥 풀어주기도 하고",
          "original_form": "발아플때 발바닥 풀어주기도 하고",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005381.1.29",
          "form": "저는 어깨 자주 풀어줘요",
          "original_form": "저는 어깨 자주 풀어줘요",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005381.1.30",
          "form": "요즘엔 운동기구가 잘나와서",
          "original_form": "요즘엔 운동기구가 잘나와서",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005381.1.31",
          "form": "도구도 다양하네요",
          "original_form": "도구도 다양하네요",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005381.1.32",
          "form": "러닝머신 같은거 집에 들이셨나요?",
          "original_form": "러닝머신 같은거 집에 들이셨나요?",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005381.1.33",
          "form": "러닝머신은 부피가 너무 커서 들이진 못했어요",
          "original_form": "러닝머신은 부피가 너무 커서 들이진 못했어요",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005381.1.34",
          "form": "욕심이 있긴한데 ㅎㅎ 그냥 집에서 뛰어야죠",
          "original_form": "욕심이 있긴한데 ㅎㅎ 그냥 집에서 뛰어야죠",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005381.1.35",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005381.1.36",
          "form": "집에 잡다한 물건이 많아서",
          "original_form": "집에 잡다한 물건이 많아서",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005381.1.37",
          "form": "운동기구는 들일 생각 못하고 있어요",
          "original_form": "운동기구는 들일 생각 못하고 있어요",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005381.1.38",
          "form": "그쵸 러닝머신은 특히 커서 더 부담스러워요",
          "original_form": "그쵸 러닝머신은 특히 커서 더 부담스러워요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005381.1.39",
          "form": "말을 이상하게 했네요 ㅎ. 헬스장가서 뛰어야지",
          "original_form": "말을 이상하게 했네요 ㅎ. 헬스장가서 뛰어야지",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005381.1.40",
          "form": "기구같은 것도 혼자서 하면 사용법 익히기 어렵겠죠?",
          "original_form": "기구같은 것도 혼자서 하면 사용법 익히기 어렵겠죠?",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005381.1.41",
          "form": "처음엔 피티쌤한테 배우셨나요?",
          "original_form": "처음엔 피티쌤한테 배우셨나요?",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005381.1.42",
          "form": "헬스장 가시면 트레이너 분들이 다 알려주실거예요~!!",
          "original_form": "헬스장 가시면 트레이너 분들이 다 알려주실거예요~!!",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005381.1.43",
          "form": "피티는 자세 자세히 봐주거나 운동루틴 확실히 가르쳐주시는 쪽이고",
          "original_form": "피티는 자세 자세히 봐주거나 운동루틴 확실히 가르쳐주시는 쪽이고",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100005381.1.44",
          "form": "기구 사용법은 충분히 익히실 수 있을거예요",
          "original_form": "기구 사용법은 충분히 익히실 수 있을거예요",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005381.1.45",
          "form": "저희 헬스장 같은 경우에는 피티 안해도 자새 봐주시고 그러니까",
          "original_form": "저희 헬스장 같은 경우에는 피티 안해도 자새 봐주시고 그러니까",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005381.1.46",
          "form": "괜찮은 트레이너분 있으시다면 피티안하고도 괜찮으실것같아요",
          "original_form": "괜찮은 트레이너분 있으시다면 피티안하고도 괜찮으실것같아요",
          "speaker_id": "1",
          "time": "20210822 00:05"
        }
      ]
