      "utterance": [
        {
          "id": "MDRW2100043665.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043665.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043665.1.3",
          "form": "에~ 식사하셨어요?",
          "original_form": "에~ 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043665.1.4",
          "form": "아뇨 아직 안먹었어요",
          "original_form": "아뇨 아직 안먹었어요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043665.1.5",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043665.1.6",
          "form": "함박스테이크여",
          "original_form": "함박스테이크여",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043665.1.7",
          "form": "김치찌개랑",
          "original_form": "김치찌개랑",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043665.1.8",
          "form": "시간이 늦었는데 얼릉 드세요",
          "original_form": "시간이 늦었는데 얼릉 드세요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043665.1.9",
          "form": "와 맛있겠네요",
          "original_form": "와 맛있겠네요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043665.1.10",
          "form": "사드신건가요?",
          "original_form": "사드신건가요?",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043665.1.11",
          "form": "집에서 해먹었어요 ㅋㅋ",
          "original_form": "집에서 해먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043665.1.12",
          "form": "와 요리 잘하시나봐요 ㅋㅋㅋ",
          "original_form": "와 요리 잘하시나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043665.1.13",
          "form": "그냥 먹고 살정도? ㅋㅋㅋ",
          "original_form": "그냥 먹고 살정도? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043665.1.14",
          "form": "부러워요 ㅋㅋㅋ",
          "original_form": "부러워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043665.1.15",
          "form": "전 나가서 사먹으려구요",
          "original_form": "전 나가서 사먹으려구요",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043665.1.16",
          "form": "식사는 어떻게 해 드세요?",
          "original_form": "식사는 어떻게 해 드세요?",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043665.1.17",
          "form": "보통 저녁은 사먹거나 시켜먹어요",
          "original_form": "보통 저녁은 사먹거나 시켜먹어요",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043665.1.18",
          "form": "저는 다른 가족이 있기에 주로 밥을 지어먹어요",
          "original_form": "저는 다른 가족이 있기에 주로 밥을 지어먹어요",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043665.1.19",
          "form": "물론 배달음식도 종종 ㅋㅋ",
          "original_form": "물론 배달음식도 종종 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043665.1.20",
          "form": "아 다른 가족이랑 같이 있으면 집에서 해먹기 낫겠어요",
          "original_form": "아 다른 가족이랑 같이 있으면 집에서 해먹기 낫겠어요",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043665.1.21",
          "form": "전 1인분 조절하기도 애매하고",
          "original_form": "전 1인분 조절하기도 애매하고",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043665.1.22",
          "form": "뭐 하나 하면 일주일 내내 그것만 먹어야해서 ㅋㅋㅋ 거의 걍 사먹게되더라구요",
          "original_form": "뭐 하나 하면 일주일 내내 그것만 먹어야해서 ㅋㅋㅋ 거의 걍 사먹게되더라구요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043665.1.23",
          "form": "맞아요 1인분은 재료 사기도 그렇고",
          "original_form": "맞아요 1인분은 재료 사기도 그렇고",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043665.1.24",
          "form": "사실 오늘 김치찌개도 3일째 식탁에 ㅋㅋㅋ",
          "original_form": "사실 오늘 김치찌개도 3일째 식탁에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043665.1.25",
          "form": "ㅋㅋㅋㅋㅋ 그래도 칼칼해서 좋을거같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 그래도 칼칼해서 좋을거같아요",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043665.1.26",
          "form": "다행히 반찬투정없이 잘 먹었네요",
          "original_form": "다행히 반찬투정없이 잘 먹었네요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        }
      ]
    }
