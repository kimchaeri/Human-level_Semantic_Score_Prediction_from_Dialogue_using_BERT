      "utterance": [
        {
          "id": "MDRW2100006907.1.1",
          "form": "요즘 제일 꽂혀 있는 메뉴 뭐예요?",
          "original_form": "요즘 제일 꽂혀 있는 메뉴 뭐예요?",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006907.1.2",
          "form": "움... 꽂힌건 아니고 돼지갈비 먹고싶어요",
          "original_form": "움... 꽂힌건 아니고 돼지갈비 먹고싶어요",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006907.1.3",
          "form": "맛있겠다.. 숯불에 구워서 먹으면 짱인데..",
          "original_form": "맛있겠다.. 숯불에 구워서 먹으면 짱인데..",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006907.1.4",
          "form": "낼은 꼭 먹을거예요",
          "original_form": "낼은 꼭 먹을거예요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006907.1.5",
          "form": "집앞에",
          "original_form": "집앞에",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006907.1.6",
          "form": "무인 고기 집?이 생겨서",
          "original_form": "무인 고기 집?이 생겨서",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006907.1.7",
          "form": "무인고기집??",
          "original_form": "무인고기집??",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006907.1.8",
          "form": "오오 처음들어요",
          "original_form": "오오 처음들어요",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006907.1.9",
          "form": "집에서 구워먹을 수 있는 고기 종류를 팔더라구요",
          "original_form": "집에서 구워먹을 수 있는 고기 종류를 팔더라구요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006907.1.10",
          "form": "낼 사서 집에서 먹어보려구요",
          "original_form": "낼 사서 집에서 먹어보려구요",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006907.1.11",
          "form": "저도 그런거 처음 봤어요ㅋㅋ",
          "original_form": "저도 그런거 처음 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006907.1.12",
          "form": "우와 신기하다",
          "original_form": "우와 신기하다",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006907.1.13",
          "form": "요새 무인가게들이 많아지는것 같아요",
          "original_form": "요새 무인가게들이 많아지는것 같아요",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006907.1.14",
          "form": "맞아요 매대 계산들도 그렇고",
          "original_form": "맞아요 매대 계산들도 그렇고",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006907.1.15",
          "form": "사람이랑 마주치지 않는 게 더 편할때도 있어서..",
          "original_form": "사람이랑 마주치지 않는 게 더 편할때도 있어서..",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006907.1.16",
          "form": "전 오늘 삼계탕 먹었어요",
          "original_form": "전 오늘 삼계탕 먹었어요",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006907.1.17",
          "form": "오앙 맛있던가요?",
          "original_form": "오앙 맛있던가요?",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006907.1.18",
          "form": "전 이번 말복에 먹지도 않고 지나갔는데",
          "original_form": "전 이번 말복에 먹지도 않고 지나갔는데",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006907.1.19",
          "form": "사다 놨던 레토르트가 남아서 처리겸 ㅋㅋ",
          "original_form": "사다 놨던 레토르트가 남아서 처리겸 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006907.1.20",
          "form": "맛은 모.. 평범..",
          "original_form": "맛은 모.. 평범..",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006907.1.21",
          "form": "아항 레토르트였군요",
          "original_form": "아항 레토르트였군요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006907.1.22",
          "form": "오늘 비와서 국물 있는 게 먹고 싶더라구요",
          "original_form": "오늘 비와서 국물 있는 게 먹고 싶더라구요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006907.1.23",
          "form": "비오면 그런게 좋죠",
          "original_form": "비오면 그런게 좋죠",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006907.1.24",
          "form": "오늘 먹은 것 중에 맛있었던 거 있었어요?",
          "original_form": "오늘 먹은 것 중에 맛있었던 거 있었어요?",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006907.1.25",
          "form": "음 초코우유?!?! 아 짜파게티랑 김치 먹은거 맛있었어요",
          "original_form": "음 초코우유?!?! 아 짜파게티랑 김치 먹은거 맛있었어요",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006907.1.26",
          "form": "와 짜파게티 맛있었겠다",
          "original_form": "와 짜파게티 맛있었겠다",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006907.1.27",
          "form": "무인고기 검색해보고 왔어요 ㅋㅋ",
          "original_form": "무인고기 검색해보고 왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006907.1.28",
          "form": "대박이다 자판기 같은 것도 있고 냉장고 진열도 있고",
          "original_form": "대박이다 자판기 같은 것도 있고 냉장고 진열도 있고",
          "speaker_id": "1",
          "time": "20210822 00:22"
        }
      ]
