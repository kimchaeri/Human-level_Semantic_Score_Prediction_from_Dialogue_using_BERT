      "utterance": [
        {
          "id": "MDRW2100026057.1.1",
          "form": "나도 애기 한번 안아보고싶어 갓난애기",
          "original_form": "나도 애기 한번 안아보고싶어 갓난애기",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100026057.1.2",
          "form": "안아본적 없니?",
          "original_form": "안아본적 없니?",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100026057.1.3",
          "form": "너 네살때 막내동생 안았었는데 ㅎ",
          "original_form": "너 네살때 막내동생 안았었는데 ㅎ",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100026057.1.4",
          "form": "동생 안아본거 말곤 없을줄..",
          "original_form": "동생 안아본거 말곤 없을줄..",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100026057.1.5",
          "form": "애키우는데 진짜 중요한건 사랑과 경제적 여유 글구 내생활 인듯",
          "original_form": "애키우는데 진짜 중요한건 사랑과 경제적 여유 글구 내생활 인듯",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100026057.1.6",
          "form": "근데 어려울거 같아",
          "original_form": "근데 어려울거 같아",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100026057.1.7",
          "form": "강아지 앉듯 함되지요",
          "original_form": "강아지 앉듯 함되지요",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100026057.1.8",
          "form": "애기는 귀엽긴한데 좀 무서워 너무 연약해서",
          "original_form": "애기는 귀엽긴한데 좀 무서워 너무 연약해서",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100026057.1.9",
          "form": "ㅋ 맞아 엄마도 첨엔 잘 못해서 니가 불편해했었지\r\n업는것도 잘못했었지",
          "original_form": "ㅋ 맞아 엄마도 첨엔 잘 못해서 니가 불편해했었지\r\n업는것도 잘못했었지",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100026057.1.10",
          "form": "옹알이하면 귀엽나?",
          "original_form": "옹알이하면 귀엽나?",
          "speaker_id": "1",
          "time": "20210903 21:05"
        },
        {
          "id": "MDRW2100026057.1.11",
          "form": "넘 구엽지 ㅎ\r\n뭐라뭐라 옹얀옹알대는데 그것도 무지 신기해",
          "original_form": "넘 구엽지 ㅎ\r\n뭐라뭐라 옹얀옹알대는데 그것도 무지 신기해",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100026057.1.12",
          "form": "갓난애기도 옹알이해?",
          "original_form": "갓난애기도 옹알이해?",
          "speaker_id": "1",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100026057.1.13",
          "form": "당연하지 ㅋ 근데 넘 옛날일이라 기억이 가물가물하다",
          "original_form": "당연하지 ㅋ 근데 넘 옛날일이라 기억이 가물가물하다",
          "speaker_id": "2",
          "time": "20210903 21:07"
        }
      ]
