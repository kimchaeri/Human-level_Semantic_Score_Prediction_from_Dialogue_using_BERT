      "utterance": [
        {
          "id": "MDRW2100026344.1.1",
          "form": "주로 가사일은 누가하나요?",
          "original_form": "주로 가사일은 누가하나요?",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026344.1.2",
          "form": "저는 엄마와 살아서 반반하는 것 같아요~",
          "original_form": "저는 엄마와 살아서 반반하는 것 같아요~",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026344.1.3",
          "form": "음식 설거지 보통 제가 하고 청소 어머니가 하세용",
          "original_form": "음식 설거지 보통 제가 하고 청소 어머니가 하세용",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026344.1.4",
          "form": "아 저는 엄마랑 살땐 엄마가다해주셨는데",
          "original_form": "아 저는 엄마랑 살땐 엄마가다해주셨는데",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026344.1.5",
          "form": "참철이없었던거같아요",
          "original_form": "참철이없었던거같아요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026344.1.6",
          "form": "저는 어머니께서 직장도 다니셔서 더 그랬던 거 같아요~",
          "original_form": "저는 어머니께서 직장도 다니셔서 더 그랬던 거 같아요~",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026344.1.7",
          "form": "저도 보통은 엄마가 해주는 밥 먹다가 늦게 정신차렸네요 ㅎㅎ",
          "original_form": "저도 보통은 엄마가 해주는 밥 먹다가 늦게 정신차렸네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026344.1.8",
          "form": "그래도 착한 딸이신거같아요",
          "original_form": "그래도 착한 딸이신거같아요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026344.1.9",
          "form": "ㅎㅎ 감사합니다",
          "original_form": "ㅎㅎ 감사합니다",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026344.1.10",
          "form": "있을 때 잘하려구요~",
          "original_form": "있을 때 잘하려구요~",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026344.1.11",
          "form": "저는 육아중이라 신랑과 반반하고있어요",
          "original_form": "저는 육아중이라 신랑과 반반하고있어요",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026344.1.12",
          "form": "육아 너무 고생 많으세요",
          "original_form": "육아 너무 고생 많으세요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026344.1.13",
          "form": "밥은제가 설거지는 신랑이",
          "original_form": "밥은제가 설거지는 신랑이",
          "speaker_id": "2",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026344.1.14",
          "form": "육아는 정말 슈퍼맨이 와도 힘들듯해요",
          "original_form": "육아는 정말 슈퍼맨이 와도 힘들듯해요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026344.1.15",
          "form": "반반이 좋죠~ 혼자서는 집안일 너무 힘들어요ㅠㅠ",
          "original_form": "반반이 좋죠~ 혼자서는 집안일 너무 힘들어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026344.1.16",
          "form": "집안일은 해도 티가안나요ㅠ",
          "original_form": "집안일은 해도 티가안나요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026344.1.17",
          "form": "그쵸ㅠㅠ 해도해도 티가 안나니 너무 힘들고",
          "original_form": "그쵸ㅠㅠ 해도해도 티가 안나니 너무 힘들고",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026344.1.18",
          "form": "매일 해야하니 끝이 없어요~",
          "original_form": "매일 해야하니 끝이 없어요~",
          "speaker_id": "1",
          "time": "20210901 21:50"
        }
      ]
