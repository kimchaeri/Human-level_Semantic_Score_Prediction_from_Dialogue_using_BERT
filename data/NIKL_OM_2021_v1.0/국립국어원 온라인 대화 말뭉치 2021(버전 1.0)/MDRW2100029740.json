      "utterance": [
        {
          "id": "MDRW2100029740.1.1",
          "form": "네네! 당연하죠ㅎㅎㅎㅎ",
          "original_form": "네네! 당연하죠ㅎㅎㅎㅎ",
          "speaker_id": "1",
