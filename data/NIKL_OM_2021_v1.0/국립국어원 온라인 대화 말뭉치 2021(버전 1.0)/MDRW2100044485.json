      "utterance": [
        {
          "id": "MDRW2100044485.1.1",
          "form": "아..혹시 다운타우너 버거요?!?!",
          "original_form": "아..혹시 다운타우너 버거요?!?!",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044485.1.2",
          "form": "넹!!!! 줄 2시간은 다른버건데 지금땡기는건",
          "original_form": "넹!!!! 줄 2시간은 다른버건데 지금땡기는건",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044485.1.3",
          "form": "다운타우너에용ㅋㅋㅋㅋ",
          "original_form": "다운타우너에용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044485.1.4",
          "form": "요즘 다운타우너 라인 음식점들 왜케 잘되나요",
          "original_form": "요즘 다운타우너 라인 음식점들 왜케 잘되나요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044485.1.5",
          "form": "그니까요 ㅋㅋ그사람은 뭐 내기만 하면 다 잘되네요ㅠ",
          "original_form": "그니까요 ㅋㅋ그사람은 뭐 내기만 하면 다 잘되네요ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044485.1.6",
          "form": "노티드도넛은 아직도 줄이 ㄷㄷㄷ어마무시하더라구요",
          "original_form": "노티드도넛은 아직도 줄이 ㄷㄷㄷ어마무시하더라구요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044485.1.7",
          "form": "진짜 거긴 줄이 줄을 부르는겻같아요",
          "original_form": "진짜 거긴 줄이 줄을 부르는겻같아요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044485.1.8",
          "form": "먹어보면 그정도는 아닌디...",
          "original_form": "먹어보면 그정도는 아닌디...",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044485.1.9",
          "form": "그니깐요 그렇게까지 줄 설 정도 절대아닌데ㅋㅋㅋ마케팅을 잘한걸까요",
          "original_form": "그니깐요 그렇게까지 줄 설 정도 절대아닌데ㅋㅋㅋ마케팅을 잘한걸까요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044485.1.10",
          "form": "스마일이 귀엽긴하든데 ㅎㅎ",
          "original_form": "스마일이 귀엽긴하든데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044485.1.11",
          "form": "ㅋㅋㅋ맞아요 그 캐릭터가ㅜ인스타 감성이긴해요",
          "original_form": "ㅋㅋㅋ맞아요 그 캐릭터가ㅜ인스타 감성이긴해요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044485.1.12",
          "form": "리틀넥도 가보셨나요?",
          "original_form": "리틀넥도 가보셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044485.1.13",
          "form": "네 ㅋㅋ가봤어요",
          "original_form": "네 ㅋㅋ가봤어요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044485.1.14",
          "form": "저은근 그 셰프 팬인가봐요 ㅋㅋ 다 제입맛에 맞아요 ㅋㅋㅋ",
          "original_form": "저은근 그 셰프 팬인가봐요 ㅋㅋ 다 제입맛에 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044485.1.15",
          "form": "저도 예전에 가봤는데 ㅋㅋㅋ 근데 거기 음식은 그닥 특색은 없지 않나요??",
          "original_form": "저도 예전에 가봤는데 ㅋㅋㅋ 근데 거기 음식은 그닥 특색은 없지 않나요??",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044485.1.16",
          "form": "ㅋㅋㅋ아 맞아요 특색은 없어요 ㅋㅋㅋ 근데그냥 무난하게 먹기좋은 ㅋㅋㅋㅋ?",
          "original_form": "ㅋㅋㅋ아 맞아요 특색은 없어요 ㅋㅋㅋ 근데그냥 무난하게 먹기좋은 ㅋㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044485.1.17",
          "form": "휴 암튼 그렇게 다 잘나가는거 너무 신기해요..",
          "original_form": "휴 암튼 그렇게 다 잘나가는거 너무 신기해요..",
