      "utterance": [
        {
          "id": "MDRW2100000728.1.1",
          "form": "스포츠 경기 직접 가서 본 적 있었나 우리?",
          "original_form": "스포츠 경기 직접 가서 본 적 있었나 우리?",
          "speaker_id": "2",
          "time": "20210819 19:35"
        },
        {
          "id": "MDRW2100000728.1.2",
          "form": "야구만 봤었나?",
          "original_form": "야구만 봤었나?",
          "speaker_id": "2",
          "time": "20210819 19:36"
        },
        {
          "id": "MDRW2100000728.1.3",
          "form": "엥 주제 바꼈네? 우리 야구봤지",
          "original_form": "엥 주제 바꼈네? 우리 야구봤지",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000728.1.4",
          "form": "축구본적 있나?",
          "original_form": "축구본적 있나?",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000728.1.5",
          "form": "응 야구 또 보러가고 싶다ㅠㅠ",
          "original_form": "응 야구 또 보러가고 싶다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:59"
        },
        {
          "id": "MDRW2100000728.1.6",
          "form": "축구는 같이 본 적 없는듯??",
          "original_form": "축구는 같이 본 적 없는듯??",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000728.1.7",
          "form": "난 배구보러가고싶어",
          "original_form": "난 배구보러가고싶어",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000728.1.8",
          "form": "여자배구~~~~",
          "original_form": "여자배구~~~~",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000728.1.9",
          "form": "인삼공사 대전인거 첨알았음",
          "original_form": "인삼공사 대전인거 첨알았음",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000728.1.10",
          "form": "마자 부승관이 인삼공사 팬이라고 한듯",
          "original_form": "마자 부승관이 인삼공사 팬이라고 한듯",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000728.1.11",
          "form": "남배는 삼성화재였나?",
          "original_form": "남배는 삼성화재였나?",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000728.1.12",
          "form": "흥국생명 배구 보고싶었는뎅",
          "original_form": "흥국생명 배구 보고싶었는뎅",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000728.1.13",
          "form": "마자 남배는 삼성화재",
          "original_form": "마자 남배는 삼성화재",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000728.1.14",
          "form": "친구냐고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "친구냐고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000728.1.15",
          "form": "왜나븐말이지..",
          "original_form": "왜나븐말이지..",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000728.1.16",
          "form": "내 친구 승관이..",
          "original_form": "내 친구 승관이..",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000728.1.17",
          "form": "근데 요새 무관중인가",
          "original_form": "근데 요새 무관중인가",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000728.1.18",
          "form": "야구는 무관중인데",
          "original_form": "야구는 무관중인데",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000728.1.19",
          "form": "다 무관중아닐까? 4단계니까",
          "original_form": "다 무관중아닐까? 4단계니까",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000728.1.20",
          "form": "배구는 코로나 이후로 계속 무관중이었음 ㅠㅠ",
          "original_form": "배구는 코로나 이후로 계속 무관중이었음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000728.1.21",
          "form": "넘 아쉽다 진짜",
          "original_form": "넘 아쉽다 진짜",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000728.1.22",
          "form": "배구는 실내라서 더할거같아ㅠㅠ",
          "original_form": "배구는 실내라서 더할거같아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000728.1.23",
          "form": "맞아.. 에어컨도 틀고하면 특히 위험하니깐",
          "original_form": "맞아.. 에어컨도 틀고하면 특히 위험하니깐",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000728.1.24",
          "form": "배구 직관 진짜 가고싶어...",
          "original_form": "배구 직관 진짜 가고싶어...",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000728.1.25",
          "form": "표 얼마일까",
          "original_form": "표 얼마일까",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000728.1.26",
          "form": "글케 안비쌌던거로 기억하는데",
          "original_form": "글케 안비쌌던거로 기억하는데",
          "speaker_id": "2",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000728.1.27",
          "form": "기억조작일수도 있음..",
          "original_form": "기억조작일수도 있음..",
          "speaker_id": "2",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000728.1.28",
          "form": "ㅎㅎ..야구보다 저렴하겠지?",
          "original_form": "ㅎㅎ..야구보다 저렴하겠지?",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100000728.1.29",
          "form": "핸드볼도 재밌었는데..(10년전에봄)",
          "original_form": "핸드볼도 재밌었는데..(10년전에봄)",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100000728.1.30",
          "form": "근데 야구도 대전이 진짜 싼편이어서",
          "original_form": "근데 야구도 대전이 진짜 싼편이어서",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100000728.1.31",
          "form": "오히려 실내니까 더 비쌀수도 있겠다 배구가",
          "original_form": "오히려 실내니까 더 비쌀수도 있겠다 배구가",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100000728.1.32",
          "form": "아진짜?대전 구장이 좀 안좋아서 그런가??",
          "original_form": "아진짜?대전 구장이 좀 안좋아서 그런가??",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100000728.1.33",
          "form": "응 오래돼서 그럴걸?",
          "original_form": "응 오래돼서 그럴걸?",
          "speaker_id": "2",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100000728.1.34",
          "form": "고척은 진짜 잘 보이지도 않는데 비싸",
          "original_form": "고척은 진짜 잘 보이지도 않는데 비싸",
          "speaker_id": "2",
          "time": "20210819 21:43"
        }
      ]
