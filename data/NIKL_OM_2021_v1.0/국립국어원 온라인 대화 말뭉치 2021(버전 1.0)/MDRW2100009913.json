        {
          "id": "MDRW2100009913.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009913.1.2",
          "form": "아뇨 안 키워요",
          "original_form": "아뇨 안 키워요",
          "speaker_id": "2",
          "time": "20210823 11:39"
        },
        {
          "id": "MDRW2100009913.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009913.1.4",
          "form": "키워보고 싶은 동물 있어요?",
          "original_form": "키워보고 싶은 동물 있어요?",
          "speaker_id": "2",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100009913.1.5",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100009913.1.6",
          "form": "종류는 어떤?",
          "original_form": "종류는 어떤?",
          "speaker_id": "2",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100009913.1.7",
          "form": "종은 잘 몰라서 뇨",
          "original_form": "종은 잘 몰라서 뇨",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100009913.1.8",
          "form": "사실 종류 따지는 건 한국 밖에 없대요",
          "original_form": "사실 종류 따지는 건 한국 밖에 없대요",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100009913.1.9",
          "form": "좋은 마인드세요",
          "original_form": "좋은 마인드세요",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100009913.1.10",
          "form": "네 작으면 귀요미",
          "original_form": "네 작으면 귀요미",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100009913.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100009913.1.12",
          "form": "저는 큰 개도 좋더라고요",
          "original_form": "저는 큰 개도 좋더라고요",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100009913.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100009913.1.14",
          "form": "리트리버는 아세요?",
          "original_form": "리트리버는 아세요?",
