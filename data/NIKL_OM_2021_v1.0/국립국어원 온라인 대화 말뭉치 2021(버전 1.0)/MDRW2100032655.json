      "utterance": [
        {
          "id": "MDRW2100032655.1.1",
          "form": "아니 난 종교",
          "original_form": "아니 난 종교",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032655.1.2",
          "form": "없는데?",
          "original_form": "없는데?",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032655.1.3",
          "form": "아하 교리 얘기하고 하길래",
          "original_form": "아하 교리 얘기하고 하길래",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032655.1.4",
          "form": "뭐 새롭게 알게되었나 했지",
          "original_form": "뭐 새롭게 알게되었나 했지",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.5",
          "form": "있을걸내가알기론",
          "original_form": "있을걸내가알기론",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.6",
          "form": "그럼 말도안되잖아",
          "original_form": "그럼 말도안되잖아",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.7",
          "form": "너무",
          "original_form": "너무",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.9",
          "form": "나를 믿기만하면 된다? 그건 사이비지",
          "original_form": "나를 믿기만하면 된다? 그건 사이비지",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.10",
          "form": "아니 사이비도 노력해야한다",
          "original_form": "아니 사이비도 노력해야한다",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.11",
          "form": "했던거 같은데",
          "original_form": "했던거 같은데",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.12",
          "form": "뭔가 종교 가지는 것도 좋다 이런 방향인것 같아서",
          "original_form": "뭔가 종교 가지는 것도 좋다 이런 방향인것 같아서",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032655.1.13",
          "form": "하나 가지게되었나 했어",
          "original_form": "하나 가지게되었나 했어",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032655.1.14",
          "form": "그건 아닌데 너의 논리가 이해가 안돼서",
          "original_form": "그건 아닌데 너의 논리가 이해가 안돼서",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032655.1.15",
          "form": "아 근데 그건 내 이야기야",
          "original_form": "아 근데 그건 내 이야기야",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032655.1.16",
          "form": "종교가 나쁘다가 아니라 나의 성격은 그러하다",
          "original_form": "종교가 나쁘다가 아니라 나의 성격은 그러하다",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032655.1.17",
          "form": "종교로 기댈곳만 생긴다는건 아닌거같아서",
          "original_form": "종교로 기댈곳만 생긴다는건 아닌거같아서",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.18",
          "form": "보통 심리적 안정이 주 이유니까",
          "original_form": "보통 심리적 안정이 주 이유니까",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.19",
          "form": "신부님같은경우 기댈곳만 생긴거보다 더 책임감이 따르고",
          "original_form": "신부님같은경우 기댈곳만 생긴거보다 더 책임감이 따르고",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.20",
          "form": "스님도 그렇고",
          "original_form": "스님도 그렇고",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.21",
          "form": "그치 성직자는 당연히 그렇고",
          "original_form": "그치 성직자는 당연히 그렇고",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.22",
          "form": "그분들은 항상 노력하시는 분들이지",
          "original_form": "그분들은 항상 노력하시는 분들이지",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.23",
          "form": "그 외에 신도들도 본인 종교에 대한 이미지에 책임이 있잖아",
          "original_form": "그 외에 신도들도 본인 종교에 대한 이미지에 책임이 있잖아",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032655.1.24",
          "form": "음...난 고해성사라든가",
          "original_form": "음...난 고해성사라든가",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032655.1.25",
          "form": "분명 그렇게 살라고 하지않나 싶고",
          "original_form": "분명 그렇게 살라고 하지않나 싶고",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032655.1.26",
          "form": "교회에서의 사랑",
          "original_form": "교회에서의 사랑",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032655.1.27",
          "form": "약간 그런 쪽에 부정적인 편이야",
          "original_form": "약간 그런 쪽에 부정적인 편이야",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032655.1.28",
          "form": "그렇게 더열심히 살고 그래야 전도?도 하잖아",
          "original_form": "그렇게 더열심히 살고 그래야 전도?도 하잖아",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032655.1.29",
          "form": "개신교의 경우는 그렇지",
          "original_form": "개신교의 경우는 그렇지",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032655.1.30",
          "form": "그래서 보여주기 식이래도 더 열심히 산다고 일반적으로 진짜 믿는다면",
          "original_form": "그래서 보여주기 식이래도 더 열심히 산다고 일반적으로 진짜 믿는다면",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032655.1.31",
          "form": "모든 종교 아니야?",
          "original_form": "모든 종교 아니야?",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032655.1.32",
          "form": "모범을 보여야",
          "original_form": "모범을 보여야",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032655.1.33",
          "form": "관심이 가지",
          "original_form": "관심이 가지",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032655.1.34",
          "form": "전도?",
          "original_form": "전도?",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032655.1.35",
          "form": "이기적으로 우리만 신믿으니까 잘된다 식으로하면 그 종교는 망할것같은데",
          "original_form": "이기적으로 우리만 신믿으니까 잘된다 식으로하면 그 종교는 망할것같은데",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032655.1.36",
          "form": "살짝 다른게 우리만 신 믿으니까 우리만 잘된다의 느낌...",
          "original_form": "살짝 다른게 우리만 신 믿으니까 우리만 잘된다의 느낌...",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032655.1.37",
          "form": "일단 책임감을 가질 정도로 빠져드는 경우가 그렇게 많지가 않더라고",
          "original_form": "일단 책임감을 가질 정도로 빠져드는 경우가 그렇게 많지가 않더라고",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100032655.1.38",
          "form": "종교 가진 사람 중에 나 우리 종교의 이미지를 위해 열심히 살아요? 찾기 힘들지",
          "original_form": "종교 가진 사람 중에 나 우리 종교의 이미지를 위해 열심히 살아요? 찾기 힘들지",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032655.1.39",
          "form": "현대에는 심리적 안정을 얻기 위한 수단으로 삼는 것이 대다수이고",
          "original_form": "현대에는 심리적 안정을 얻기 위한 수단으로 삼는 것이 대다수이고",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032655.1.40",
          "form": "난 스스로를 봄 더 몰아붙이면서 사는 것에 희열을 느끼기때문에",
          "original_form": "난 스스로를 봄 더 몰아붙이면서 사는 것에 희열을 느끼기때문에",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032655.1.41",
          "form": "조금 더",
          "original_form": "조금 더",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032655.1.42",
          "form": "난 그래서 안 가지려고 한다는 뜻이었어",
          "original_form": "난 그래서 안 가지려고 한다는 뜻이었어",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032655.1.43",
          "form": "그래~",
          "original_form": "그래~",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032655.1.44",
          "form": "음 뭐 그래 ㅋㅋㅋㅋ",
          "original_form": "음 뭐 그래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032655.1.45",
          "form": "근데 넌 왜? 나름 긍정적으로 생각하는 것 같은데",
          "original_form": "근데 넌 왜? 나름 긍정적으로 생각하는 것 같은데",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032655.1.46",
          "form": "안 가지는 이유가 있나?",
          "original_form": "안 가지는 이유가 있나?",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032655.1.47",
          "form": "신이 모두 존재하기 때문에",
          "original_form": "신이 모두 존재하기 때문에",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032655.1.48",
          "form": "유일신이 아니라고 생각해서",
          "original_form": "유일신이 아니라고 생각해서",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032655.1.49",
          "form": "종교는 안믿어",
          "original_form": "종교는 안믿어",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032655.1.50",
          "form": "그리스로마신도?",
          "original_form": "그리스로마신도?",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032655.1.51",
          "form": "문득 궁금해지네",
          "original_form": "문득 궁금해지네",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032655.1.52",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100032655.1.53",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
