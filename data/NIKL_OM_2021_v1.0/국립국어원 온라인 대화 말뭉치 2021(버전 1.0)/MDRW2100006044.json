      "utterance": [
        {
          "id": "MDRW2100006044.1.1",
          "form": "요리하시나요?",
          "original_form": "요리하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100006044.1.2",
          "form": "남편이 장금이라서\r\n저는 맛잇게 먹는 담당이에욧",
          "original_form": "남편이 장금이라서\r\n저는 맛잇게 먹는 담당이에욧",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100006044.1.3",
          "form": "결혼 잘하셨네요 남편분이 요리 전담이신가요?",
          "original_form": "결혼 잘하셨네요 남편분이 요리 전담이신가요?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100006044.1.4",
          "form": "살림꾼이에요 힛",
          "original_form": "살림꾼이에요 힛",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100006044.1.5",
          "form": "제가 똥손이라서ㅋㅋ",
          "original_form": "제가 똥손이라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100006044.1.6",
          "form": "즐겨하시는 메뉴는 뭔가요?",
          "original_form": "즐겨하시는 메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006044.1.7",
          "form": "제가한식좋아해서",
          "original_form": "제가한식좋아해서",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006044.1.8",
          "form": "다 잘해요ㅋㅋ",
          "original_form": "다 잘해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006044.1.9",
          "form": "요리하시나욤",
          "original_form": "요리하시나욤",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100006044.1.10",
          "form": "와이프가 주로 하고 저는 가끔 합니다 ㅎㅎ",
          "original_form": "와이프가 주로 하고 저는 가끔 합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006044.1.11",
          "form": "저녁 뭐드셧나여",
          "original_form": "저녁 뭐드셧나여",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006044.1.12",
          "form": "와이프가 백신맞아서 반찬배달시켜놓은걸로...ㅎㅎ",
          "original_form": "와이프가 백신맞아서 반찬배달시켜놓은걸로...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006044.1.13",
          "form": "ㅎㅎ저는 장금이가 해놓은 김치볶음밥 오뎅탕",
          "original_form": "ㅎㅎ저는 장금이가 해놓은 김치볶음밥 오뎅탕",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006044.1.14",
          "form": "조졋어유",
          "original_form": "조졋어유",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006044.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006044.1.16",
          "form": "맛있으셨겠어요 점심은요??",
          "original_form": "맛있으셨겠어요 점심은요??",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006044.1.17",
          "form": "점심은 직원식당에서 걍 밥먹엇어용",
          "original_form": "점심은 직원식당에서 걍 밥먹엇어용",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100006044.1.18",
          "form": "직원식당은 먹을만하나요ㅡ",
          "original_form": "직원식당은 먹을만하나요ㅡ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100006044.1.19",
          "form": "정말 그냥 쏘쏘에욤",
          "original_form": "정말 그냥 쏘쏘에욤",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100006044.1.20",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100006044.1.21",
          "form": "구냥 영혼없이 먹어여",
          "original_form": "구냥 영혼없이 먹어여",
