        {
          "id": "MDRW2100018315.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018315.1.3",
          "form": "저는 나중에",
          "original_form": "저는 나중에",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018315.1.4",
          "form": "프리랜서로 어피티 이런 데에 기고하고 싶어요",
          "original_form": "프리랜서로 어피티 이런 데에 기고하고 싶어요",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018315.1.5",
          "form": "오오 할수있을것같은데요",
          "original_form": "오오 할수있을것같은데요",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018315.1.6",
          "form": "그런데는 지원을 어떻게할지",
          "original_form": "그런데는 지원을 어떻게할지",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018315.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.8",
          "form": "원고료도 궁금하네요",
          "original_form": "원고료도 궁금하네요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.9",
          "form": "아니면 약간 대학생대상 재테크 컨설팅 이런것도",
          "original_form": "아니면 약간 대학생대상 재테크 컨설팅 이런것도",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.10",
          "form": "진짜궁금해져버렸다",
          "original_form": "진짜궁금해져버렸다",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.11",
          "form": "해보고싶구요",
          "original_form": "해보고싶구요",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.12",
          "form": "하고픈게많은나이",
          "original_form": "하고픈게많은나이",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.14",
          "form": "어피티 같은 데는 종종 직원모집하더라고요",
          "original_form": "어피티 같은 데는 종종 직원모집하더라고요",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.15",
          "form": "이미블로그가 포트폴리오아닐까요",
          "original_form": "이미블로그가 포트폴리오아닐까요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018315.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018315.1.17",
          "form": "그예전에효라클그분은",
          "original_form": "그예전에효라클그분은",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018315.1.18",
          "form": "나가고 다른 뉴스레터하더라고요",
          "original_form": "나가고 다른 뉴스레터하더라고요",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018315.1.19",
          "form": "헐그렇구나 효라클 어쩐지 안보이더니",
          "original_form": "헐그렇구나 효라클 어쩐지 안보이더니",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018315.1.20",
          "form": "블로그하면서 저도 많이 배우는 거 같아요",
          "original_form": "블로그하면서 저도 많이 배우는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018315.1.21",
          "form": "쉽게 설멸하려면 그만큼 많이알아야돼서",
          "original_form": "쉽게 설멸하려면 그만큼 많이알아야돼서",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018315.1.22",
          "form": "name2은 어떤일하고싶으세요?",
          "original_form": "&name2&은 어떤일하고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018315.1.23",
          "form": "효라클이라면 직접 뉴스레터 만들었을수도..",
          "original_form": "효라클이라면 직접 뉴스레터 만들었을수도..",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018315.1.24",
          "form": "뭔가 전문가되면 파이프라인 만들기쉬운것같아요",
          "original_form": "뭔가 전문가되면 파이프라인 만들기쉬운것같아요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018315.1.25",
          "form": "맞아요 정말",
          "original_form": "맞아요 정말",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018315.1.26",
          "form": "돈엄청많이벌듯요",
          "original_form": "돈엄청많이벌듯요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018315.1.27",
          "form": "그래서 저는 pod로라도 책하나 내려고요 재테크관련!",
          "original_form": "그래서 저는 pod로라도 책하나 내려고요 재테크관련!",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.28",
          "form": "전문가까지 아니어도 책 냈다고 하믄 신뢰도 상승이니까요",
          "original_form": "전문가까지 아니어도 책 냈다고 하믄 신뢰도 상승이니까요",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.29",
          "form": "그거 뭐 오카방 하나만으로도",
          "original_form": "그거 뭐 오카방 하나만으로도",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.30",
          "form": "엄청모이고",
          "original_form": "엄청모이고",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.32",
          "form": "리딩방도 하고 그랬자나요 효라클이",
          "original_form": "리딩방도 하고 그랬자나요 효라클이",
          "speaker_id": "1",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.33",
          "form": "저도 살래유",
          "original_form": "저도 살래유",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.34",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.35",
          "form": "이미카페도있고",
          "original_form": "이미카페도있고",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.36",
          "form": "부럽당",
          "original_form": "부럽당",
          "speaker_id": "2",
          "time": "20210826 23:52"
        },
        {
          "id": "MDRW2100018315.1.37",
          "form": "name2은 하고싶은 일 있으세요?",
          "original_form": "&name2&은 하고싶은 일 있으세요?",
          "speaker_id": "1",
          "time": "20210826 23:54"
