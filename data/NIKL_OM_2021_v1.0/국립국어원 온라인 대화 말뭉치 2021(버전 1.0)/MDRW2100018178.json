      "utterance": [
        {
          "id": "MDRW2100018178.1.1",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018178.1.2",
          "form": "아빠가 자기 혈액형 잘못 알고 살았으면소",
          "original_form": "아빠가 자기 혈액형 잘못 알고 살았으면소",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018178.1.3",
          "form": "아내가 바람핀 줄 알고",
          "original_form": "아내가 바람핀 줄 알고",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018178.1.4",
          "form": "살해한거요",
          "original_form": "살해한거요",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018178.1.5",
          "form": "헉 네",
          "original_form": "헉 네",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018178.1.6",
          "form": "진짜 그런가봐요",
          "original_form": "진짜 그런가봐요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018178.1.7",
          "form": "네 저 이런 범죄류 다 좋아해서",
          "original_form": "네 저 이런 범죄류 다 좋아해서",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018178.1.8",
          "form": "들어본 것 같아요",
          "original_form": "들어본 것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018178.1.9",
          "form": "네 그런것 같아요",
          "original_form": "네 그런것 같아요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018178.1.10",
          "form": "근데 갑자기 왜 범죄얘기를;;",
          "original_form": "근데 갑자기 왜 범죄얘기를;;",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018178.1.11",
          "form": "심야괴담회 작가들도 소재가 다 떨어졌나봐요",
          "original_form": "심야괴담회 작가들도 소재가 다 떨어졌나봐요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018178.1.12",
          "form": "네 심괴에 이런 아는 내용 나오면",
          "original_form": "네 심괴에 이런 아는 내용 나오면",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.13",
          "form": "좀 실망스러운 ㅜ",
          "original_form": "좀 실망스러운 ㅜ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.14",
          "form": "저는 처음 보긴 하는데",
          "original_form": "저는 처음 보긴 하는데",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.15",
          "form": "귀신 얘기가 더 재밌긴 하죠",
          "original_form": "귀신 얘기가 더 재밌긴 하죠",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.16",
          "form": "네ㅜㅜ 귀신 보여줘ㅜㅜ",
          "original_form": "네ㅜㅜ 귀신 보여줘ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.17",
          "form": "ㅎㅎ 아쉬우시겠어요",
          "original_form": "ㅎㅎ 아쉬우시겠어요",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.18",
          "form": "네 이런건 표리부동에서나 보여주라구ㅜㅜ",
          "original_form": "네 이런건 표리부동에서나 보여주라구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018178.1.19",
          "form": "근데 저 남편 정말 심각하네요",
          "original_form": "근데 저 남편 정말 심각하네요",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018178.1.20",
          "form": "네 넘 현실 에피소드라",
          "original_form": "네 넘 현실 에피소드라",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018178.1.21",
          "form": "불편하네요.. ㅜ",
          "original_form": "불편하네요.. ㅜ",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018178.1.22",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018178.1.23",
