      "utterance": [
        {
          "id": "MDRW2100043974.1.1",
          "form": "잔치국수 좋아한다고?",
          "original_form": "잔치국수 좋아한다고?",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.2",
          "form": "응 잔치국수 좋아래",
          "original_form": "응 잔치국수 좋아래",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.3",
          "form": "좋아해",
          "original_form": "좋아해",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.4",
          "form": "근데 예전에는 잔치국수 진짜 싫어했어",
          "original_form": "근데 예전에는 잔치국수 진짜 싫어했어",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.5",
          "form": "왜 싫어했어?",
          "original_form": "왜 싫어했어?",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.6",
          "form": "애기 입맛에는 너무 건강한 맛이었나봐",
          "original_form": "애기 입맛에는 너무 건강한 맛이었나봐",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.7",
          "form": "국물도 뜨거운 게",
          "original_form": "국물도 뜨거운 게",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.8",
          "form": "달지도 않고",
          "original_form": "달지도 않고",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.9",
          "form": "젓가락질 하기도 힘들고",
          "original_form": "젓가락질 하기도 힘들고",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043974.1.11",
          "form": "딱 애기스러운 이유로 불호였는데",
          "original_form": "딱 애기스러운 이유로 불호였는데",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043974.1.12",
          "form": "아 충분히 그렇지",
          "original_form": "아 충분히 그렇지",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043974.1.13",
          "form": "머리 크고 나서 진짜 없어서 못 먹어",
          "original_form": "머리 크고 나서 진짜 없어서 못 먹어",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043974.1.14",
          "form": "신기하다 금방 할수있는 요리로",
          "original_form": "신기하다 금방 할수있는 요리로",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043974.1.15",
          "form": "유명하잖아",
          "original_form": "유명하잖아",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043974.1.16",
          "form": "그래서 더 지겨웠나봐 ㅋㅋㅋㅋㅋㅋ맨날 접하니까",
          "original_form": "그래서 더 지겨웠나봐 ㅋㅋㅋㅋㅋㅋ맨날 접하니까",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043974.1.17",
          "form": "우리둘이 여행가면 잔치국수 해먹자",
          "original_form": "우리둘이 여행가면 잔치국수 해먹자",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043974.1.18",
          "form": "저 맛없는 걸 왜 먹지?! 싶었나봐",
          "original_form": "저 맛없는 걸 왜 먹지?! 싶었나봐",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043974.1.19",
          "form": "좋아좋아 지금은 잔치국수 사랑하자",
          "original_form": "좋아좋아 지금은 잔치국수 사랑하자",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043974.1.20",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043974.1.21",
          "form": "잔치국수 고명으로 뭐좋아해",
          "original_form": "잔치국수 고명으로 뭐좋아해",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043974.1.22",
          "form": "계란지단이랑 김은 필수야",
          "original_form": "계란지단이랑 김은 필수야",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.23",
          "form": "당근은 뭐 들어가도 좋고",
          "original_form": "당근은 뭐 들어가도 좋고",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.24",
          "form": "파도 좋고",
          "original_form": "파도 좋고",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.25",
          "form": "ㅋㅋㅋㄱㅋㄱㄱ볶음김치는?",
          "original_form": "ㅋㅋㅋㄱㅋㄱㄱ볶음김치는?",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.26",
          "form": "근데 오이는 싫어...",
          "original_form": "근데 오이는 싫어...",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.27",
          "form": "난 하얗게 먹는 걸 좀 더 좋아해",
          "original_form": "난 하얗게 먹는 걸 좀 더 좋아해",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.28",
          "form": "안 먹는 건 아니지만",
          "original_form": "안 먹는 건 아니지만",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.29",
          "form": "간장양념 넣니",
          "original_form": "간장양념 넣니",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.30",
          "form": "김치 안 들어간 맑은 걸 더 좋아해",
          "original_form": "김치 안 들어간 맑은 걸 더 좋아해",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043974.1.31",
          "form": "간장양념 먹어",
          "original_form": "간장양념 먹어",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043974.1.32",
          "form": "근데 그것보다",
          "original_form": "근데 그것보다",
          "speaker_id": "1",
          "time": "20210907 21:13"
        }
      ]
    }
