      "utterance": [
        {
          "id": "MDRW2100036272.1.1",
          "form": "name1님은 형제관계가 어떻게 되시나요?!",
          "original_form": "&name1&님은 형제관계가 어떻게 되시나요?!",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036272.1.2",
          "form": "저는 동생하나 있어요",
          "original_form": "저는 동생하나 있어요",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036272.1.3",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036272.1.4",
          "form": "저는 좀 특이해요!",
          "original_form": "저는 좀 특이해요!",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036272.1.5",
          "form": "오빠 하나랑 언니 둘이 있습니다 ㅎㅎ",
          "original_form": "오빠 하나랑 언니 둘이 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036272.1.6",
          "form": "그럼 막내시네요??",
          "original_form": "그럼 막내시네요??",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036272.1.7",
          "form": "네 그렇습니다~",
          "original_form": "네 그렇습니다~",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100036272.1.8",
          "form": "그럼 귀여움 많이 받으셨겠어요",
          "original_form": "그럼 귀여움 많이 받으셨겠어요",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036272.1.9",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036272.1.10",
          "form": "님은 동생이랑 사이 좋으신가요?",
          "original_form": "님은 동생이랑 사이 좋으신가요?",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036272.1.11",
          "form": "성별이 달라서 그냥 무시하며 살죠ㅋㅋㅋ",
          "original_form": "성별이 달라서 그냥 무시하며 살죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036272.1.12",
          "form": "성별이 어떻게 다른데요?",
          "original_form": "성별이 어떻게 다른데요?",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036272.1.13",
          "form": "남동생인가요?",
          "original_form": "남동생인가요?",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036272.1.14",
          "form": "남동생있어요ㅋㅋㅋ",
          "original_form": "남동생있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036272.1.15",
          "form": "아 그러면 진짜",
          "original_form": "아 그러면 진짜",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036272.1.16",
          "form": "무시하게 될 거 같아요",
          "original_form": "무시하게 될 거 같아요",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036272.1.17",
          "form": "남동생 말 안들으면 진짜 화날거같아요",
          "original_form": "남동생 말 안들으면 진짜 화날거같아요",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100036272.1.18",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036272.1.19",
          "form": "그냥 무시하거나 싸우죠ㅋㅋㅋㅋ",
          "original_form": "그냥 무시하거나 싸우죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036272.1.20",
          "form": "사실 저희 집에서는",
          "original_form": "사실 저희 집에서는",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036272.1.21",
          "form": "첫째인 오빠가 제일 골치예요 ㅎ",
          "original_form": "첫째인 오빠가 제일 골치예요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036272.1.22",
          "form": "1인분을 못한다고 해야하나...",
          "original_form": "1인분을 못한다고 해야하나...",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036272.1.23",
          "form": "그래요? 약간 부모님한테 편애?를 많이 받으셨나요??",
          "original_form": "그래요? 약간 부모님한테 편애?를 많이 받으셨나요??",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036272.1.24",
          "form": "음 오빠가 몸이 약해서 그러긴 한데",
          "original_form": "음 오빠가 몸이 약해서 그러긴 한데",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036272.1.25",
          "form": "어렸을 때 공부도 잘했는데",
          "original_form": "어렸을 때 공부도 잘했는데",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036272.1.26",
          "form": "너무 남의 덕을 보려고 해요",
          "original_form": "너무 남의 덕을 보려고 해요",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036272.1.27",
          "form": "그래서 대학 입시 공부도 안하고...",
          "original_form": "그래서 대학 입시 공부도 안하고...",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036272.1.28",
          "form": "그러다보니 인생이 전반적으로 꼬였어요ㅠ",
          "original_form": "그러다보니 인생이 전반적으로 꼬였어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036272.1.29",
          "form": "아이고ㅠㅠ 고생이 많으셨겠네요",
          "original_form": "아이고ㅠㅠ 고생이 많으셨겠네요",
          "speaker_id": "1",
          "time": "20210830 23:47"
        }
      ]
    }
