      "utterance": [
        {
          "id": "MDRW2100039870.1.1",
          "form": "응 보고있어 총은 어디서 가져왔는지 모르겠네",
          "original_form": "응 보고있어 총은 어디서 가져왔는지 모르겠네",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039870.1.2",
          "form": "언제가 마지막회야ㅋㅋㅋㅋㅋ",
          "original_form": "언제가 마지막회야ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039870.1.3",
          "form": "나도 몰라 오늘 끝나나",
          "original_form": "나도 몰라 오늘 끝나나",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039870.1.4",
          "form": "다음주가 마지막인가봐",
          "original_form": "다음주가 마지막인가봐",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039870.1.5",
          "form": "오메 머리에 총맞았네",
          "original_form": "오메 머리에 총맞았네",
          "speaker_id": "2",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039870.1.6",
          "form": "너무 어이없어ㅋㅋㅋㅋㅋ",
          "original_form": "너무 어이없어ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039870.1.7",
          "form": "내가 뭘보고 있는거야",
          "original_form": "내가 뭘보고 있는거야",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039870.1.8",
          "form": "이상한 드라마야",
          "original_form": "이상한 드라마야",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039870.1.9",
          "form": "근데 끝까지 볼거잖아ㅋㅋㅋㄱ",
          "original_form": "근데 끝까지 볼거잖아ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039870.1.10",
          "form": "결말은 봐야지....",
          "original_form": "결말은 봐야지....",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039870.1.11",
          "form": "둘이 결혼했어???",
          "original_form": "둘이 결혼했어???",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039870.1.12",
          "form": "어째 웨딩드레스같은거 입고있더라",
          "original_form": "어째 웨딩드레스같은거 입고있더라",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039870.1.13",
          "form": "둘이 약혼했어",
          "original_form": "둘이 약혼했어",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039870.1.14",
          "form": "참 신기한 드라마야ㅋㅋㅋㅋㅋ",
          "original_form": "참 신기한 드라마야ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039870.1.15",
          "form": "요즘에 이거밖에 볼게없어",
          "original_form": "요즘에 이거밖에 볼게없어",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039870.1.16",
          "form": "건물이 터져부려써",
          "original_form": "건물이 터져부려써",
