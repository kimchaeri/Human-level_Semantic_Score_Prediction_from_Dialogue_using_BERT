      "utterance": [
        {
          "id": "MDRW2100041874.1.1",
          "form": "밥도 은근 귀찮아여",
          "original_form": "밥도 은근 귀찮아여",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041874.1.2",
          "form": "전 다행히 형부 야근할때 언니가 밥먹으러 오라고 해서 살아요ㅋㅋ",
          "original_form": "전 다행히 형부 야근할때 언니가 밥먹으러 오라고 해서 살아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041874.1.3",
          "form": "와 좋으시겠다",
          "original_form": "와 좋으시겠다",
          "speaker_id": "2",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100041874.1.4",
          "form": "ㅋㅋㅋ 언니찬스 가끔 쓰시면!!",
          "original_form": "ㅋㅋㅋ 언니찬스 가끔 쓰시면!!",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041874.1.5",
          "form": "네 남이 해준건 다 맛있으니까요ㅋㅋ",
          "original_form": "네 남이 해준건 다 맛있으니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041874.1.6",
          "form": "저희언니는 제가 해줘얔ㅋㅋㅋㅋ",
          "original_form": "저희언니는 제가 해줘얔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041874.1.7",
          "form": "악 그건 힘든데요ㅋㅋㅋ",
          "original_form": "악 그건 힘든데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041874.1.8",
          "form": "요리에 크게 재능이 없는 사람이 있어욬ㅋㅋㅋ",
          "original_form": "요리에 크게 재능이 없는 사람이 있어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041874.1.9",
          "form": "뭐 저희도 다그래여 ㅋㅋㅋ",
          "original_form": "뭐 저희도 다그래여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041874.1.10",
          "form": "ㅋㅋㅋㅋ 좀 심해여",
          "original_form": "ㅋㅋㅋㅋ 좀 심해여",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041874.1.11",
          "form": "연습하다보면 늘겠졐ㅋㅋㅋ",
          "original_form": "연습하다보면 늘겠졐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041874.1.12",
          "form": "안 늘수도 있어여ㅋㅋㅋㅋ",
          "original_form": "안 늘수도 있어여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041874.1.13",
          "form": "ㅋㅋㅋㅋ 악 안돼요",
          "original_form": "ㅋㅋㅋㅋ 악 안돼요",
          "speaker_id": "2",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041874.1.14",
          "form": "저도 얻어먹고 싶어욬ㅋㅋㅋㅋ",
          "original_form": "저도 얻어먹고 싶어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041874.1.15",
          "form": "그럴땐 밀키트로ㅋㅋㅋ",
          "original_form": "그럴땐 밀키트로ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041874.1.16",
          "form": "저 밀키트 많이 안사봤는데 요즘 종류 엄청 많더라고요",
