          "id": "MDRW2100029573.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210903 10:30"
        },
        {
          "id": "MDRW2100029573.1.2",
          "form": "올해 한번도 안갔어요",
          "original_form": "올해 한번도 안갔어요",
          "speaker_id": "2",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029573.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029573.1.4",
          "form": "원래도 잘안가서",
          "original_form": "원래도 잘안가서",
          "speaker_id": "2",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100029573.1.5",
          "form": "전  가고 싶어요",
          "original_form": "전  가고 싶어요",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029573.1.6",
          "form": "요즘은 조심스럽져",
          "original_form": "요즘은 조심스럽져",
