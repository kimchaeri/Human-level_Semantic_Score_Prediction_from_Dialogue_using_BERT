      "utterance": [
        {
          "id": "MDRW2100039487.1.1",
          "form": "결혼은 하셨나요",
          "original_form": "결혼은 하셨나요",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039487.1.2",
          "form": "아니용ㅋㅋ결혼하셨나요?",
          "original_form": "아니용ㅋㅋ결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039487.1.3",
          "form": "네 전 결혼 했어요",
          "original_form": "네 전 결혼 했어요",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039487.1.4",
          "form": "아 아기도 있으신가요?",
          "original_form": "아 아기도 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039487.1.5",
          "form": "저희 애들은 성인 이랍니다",
          "original_form": "저희 애들은 성인 이랍니다",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039487.1.6",
          "form": "와 다 키우셨군요!",
          "original_form": "와 다 키우셨군요!",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039487.1.7",
          "form": "네 홀가분 하답니다",
          "original_form": "네 홀가분 하답니다",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039487.1.8",
          "form": "아주 가끔은 허전 할때도 있긴하죠",
          "original_form": "아주 가끔은 허전 할때도 있긴하죠",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039487.1.9",
          "form": "혼자 외롭진 않으세요",
          "original_form": "혼자 외롭진 않으세요",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039487.1.10",
          "form": "아 남친있을 때도 있고 없을 때도 있고 그러네요ㅎ",
          "original_form": "아 남친있을 때도 있고 없을 때도 있고 그러네요ㅎ",
          "speaker_id": "2",
          "time": "20210904 07:17"
        },
        {
          "id": "MDRW2100039487.1.11",
          "form": "여러사람 만나다가 진짜 사람을 만날거예요",
          "original_form": "여러사람 만나다가 진짜 사람을 만날거예요",
          "speaker_id": "1",
          "time": "20210904 07:57"
        },
        {
          "id": "MDRW2100039487.1.12",
          "form": "그렇군요 좋은 분 만나셨지요?",
          "original_form": "그렇군요 좋은 분 만나셨지요?",
          "speaker_id": "2",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100039487.1.13",
          "form": "글쎄요 좋은사람 ........서로 좋은사람이 되길 맞추며\r\n살아가고 있죠",
          "original_form": "글쎄요 좋은사람 ........서로 좋은사람이 되길 맞추며\r\n살아가고 있죠",
          "speaker_id": "1",
          "time": "20210904 08:58"
        },
        {
          "id": "MDRW2100039487.1.14",
          "form": "지금은 부부끼리만 사시나요?",
          "original_form": "지금은 부부끼리만 사시나요?",
          "speaker_id": "2",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100039487.1.15",
          "form": "아니요 딸은 대학기숙사 아들은 군대요",
          "original_form": "아니요 딸은 대학기숙사 아들은 군대요",
          "speaker_id": "1",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100039487.1.16",
          "form": "아 군대라니 걱정이 많으시겠어요",
          "original_form": "아 군대라니 걱정이 많으시겠어요",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100039487.1.17",
          "form": "코로나로 휴가도 면허도 모두다 금지라서 힘들죠",
          "original_form": "코로나로 휴가도 면허도 모두다 금지라서 힘들죠",
          "speaker_id": "1",
