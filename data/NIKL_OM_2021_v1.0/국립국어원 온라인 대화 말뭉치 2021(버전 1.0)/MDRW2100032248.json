      "utterance": [
        {
          "id": "MDRW2100032248.1.1",
          "form": "홍진경 먹어본 것 같아요, 종갓집이랑",
          "original_form": "홍진경 먹어본 것 같아요, 종갓집이랑",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032248.1.2",
          "form": "종갓집 유명하죠",
          "original_form": "종갓집 유명하죠",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032248.1.3",
          "form": "홍진경은 맛있나요?",
          "original_form": "홍진경은 맛있나요?",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032248.1.4",
          "form": "저는 괜찮았던 것 같아요!",
          "original_form": "저는 괜찮았던 것 같아요!",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032248.1.5",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100032248.1.6",
          "form": "저희는 요즘에 도미솔김치 시켜요",
          "original_form": "저희는 요즘에 도미솔김치 시켜요",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032248.1.7",
          "form": "오, 거기는 처음 들어봐요",
          "original_form": "오, 거기는 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032248.1.8",
          "form": "저도 홈쇼핑보다 처음 알았어요",
          "original_form": "저도 홈쇼핑보다 처음 알았어요",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032248.1.9",
          "form": "김치 브랜드가 진짜 다양한 것 같아요ㅋㅋㅋ",
          "original_form": "김치 브랜드가 진짜 다양한 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032248.1.10",
          "form": "맞아요 도미솔 가성비 괜찮아요",
          "original_form": "맞아요 도미솔 가성비 괜찮아요",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032248.1.11",
          "form": "다음에 한번 드셔보세요",
          "original_form": "다음에 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032248.1.12",
          "form": "오, 네네 다음 쇼핑에 참고할게요",
          "original_form": "오, 네네 다음 쇼핑에 참고할게요",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032248.1.13",
          "form": "네네 공영쇼핑이 제일 싸요ㅎ",
          "original_form": "네네 공영쇼핑이 제일 싸요ㅎ",
          "speaker_id": "1",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032248.1.14",
          "form": "우리 좋아하는 음식 얘기도 했나요??",
          "original_form": "우리 좋아하는 음식 얘기도 했나요??",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100032248.1.15",
          "form": "저도 하도 하다보니 기억이 안나요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 하도 하다보니 기억이 안나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100032248.1.16",
          "form": "다시해도 되죠",
          "original_form": "다시해도 되죠",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032248.1.17",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032248.1.18",
          "form": "저는 김밥천국에 파는 것들 좋아해요 떡볶이 김밥 오므라이스 냉면 등등",
          "original_form": "저는 김밥천국에 파는 것들 좋아해요 떡볶이 김밥 오므라이스 냉면 등등",
          "speaker_id": "2",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032248.1.19",
          "form": "그런게 싸고 맛있죠",
          "original_form": "그런게 싸고 맛있죠",
          "speaker_id": "1",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032248.1.20",
          "form": "저는 샤브샤브 좋아해요",
          "original_form": "저는 샤브샤브 좋아해요",
          "speaker_id": "1",
          "time": "20210905 16:25"
        }
      ]
