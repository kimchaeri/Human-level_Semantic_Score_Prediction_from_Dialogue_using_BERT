      "utterance": [
        {
          "id": "MDRW2100043209.1.1",
          "form": "아 갑자기 족발이.땡기네여ㅠ",
          "original_form": "아 갑자기 족발이.땡기네여ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100043209.1.2",
          "form": "어제 길 가다보니 이제 붕어빵이 나왔어여",
          "original_form": "어제 길 가다보니 이제 붕어빵이 나왔어여",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100043209.1.3",
          "form": "바로 제출해주시니 감사하네요",
          "original_form": "바로 제출해주시니 감사하네요",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100043209.1.4",
          "form": "ㅋㅋㅋ 저 팥붕이 좋아요",
          "original_form": "ㅋㅋㅋ 저 팥붕이 좋아요",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100043209.1.5",
          "form": "ㅋㅋㅋㅋㅋ 요새는 치즈도 있잖아여 아세여?",
          "original_form": "ㅋㅋㅋㅋㅋ 요새는 치즈도 있잖아여 아세여?",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043209.1.6",
          "form": "치즈도 팔아요?? 그건 처음 듣습니다",
          "original_form": "치즈도 팔아요?? 그건 처음 듣습니다",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043209.1.7",
          "form": "치즈,슈크림,팥 3가진데",
          "original_form": "치즈,슈크림,팥 3가진데",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043209.1.8",
          "form": "치즈랑 슈는 두마리 천원,",
          "original_form": "치즈랑 슈는 두마리 천원,",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043209.1.9",
          "form": "팥은3마린데 이것도 작년가격이라ㅠㅠ",
          "original_form": "팥은3마린데 이것도 작년가격이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043209.1.10",
          "form": "와 두마리 천원시대",
          "original_form": "와 두마리 천원시대",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043209.1.11",
          "form": "이러다가 붕어빵도 구워먹을 판이네여",
          "original_form": "이러다가 붕어빵도 구워먹을 판이네여",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043209.1.12",
          "form": "와플팬에 붕어빵 틀 있거든요?",
          "original_form": "와플팬에 붕어빵 틀 있거든요?",
          "speaker_id": "1",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043209.1.13",
          "form": "근데 반죽 자신이 없어서 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 반죽 자신이 없어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043209.1.14",
          "form": "되어있는 반죽도 파니까 그냥 넣으시면 되겠는데",
          "original_form": "되어있는 반죽도 파니까 그냥 넣으시면 되겠는데",
          "speaker_id": "2",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043209.1.15",
          "form": "그런건 좀 양이 많아서",
          "original_form": "그런건 좀 양이 많아서",
          "speaker_id": "2",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043209.1.16",
          "form": "안되겠네여ㅋㅋㅋ",
          "original_form": "안되겠네여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043209.1.17",
          "form": "그니까여 어떤사람은 아줌마한테 사더라구요 ㅎㅎ",
          "original_form": "그니까여 어떤사람은 아줌마한테 사더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043209.1.18",
          "form": "어짜피 그분들도 사서 쓰시는거니 ㅎㅎ",
          "original_form": "어짜피 그분들도 사서 쓰시는거니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043209.1.19",
          "form": "올해는 한번 물어보까싶기도ㅠㅠ",
          "original_form": "올해는 한번 물어보까싶기도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043209.1.20",
          "form": "다양하게 해먹으면 맛있겠어요!!",
          "original_form": "다양하게 해먹으면 맛있겠어요!!",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043209.1.21",
          "form": "지금 이거 쓰는데 세번이나 나쁜말...걸렸어요",
          "original_form": "지금 이거 쓰는데 세번이나 나쁜말...걸렸어요",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043209.1.22",
          "form": "반죽있으면 집에서 모짜좀 넣고 하면 치즈되고 ㅎㅎ",
          "original_form": "반죽있으면 집에서 모짜좀 넣고 하면 치즈되고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043209.1.23",
          "form": "너무 불편..ㅠㅠㅠ 그거",
          "original_form": "너무 불편..ㅠㅠㅠ 그거",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043209.1.24",
          "form": "아니 무슨 심심이는 나쁜말이 왜이리 많을까요ㅠㅠ",
          "original_form": "아니 무슨 심심이는 나쁜말이 왜이리 많을까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043209.1.25",
          "form": "창의력은 있는 놈들이에요",
          "original_form": "창의력은 있는 놈들이에요",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043209.1.26",
          "form": "쏜살치킨",
          "original_form": "쏜살치킨",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043209.1.27",
          "form": "이거어제 안되던데 오늘은 되네",
          "original_form": "이거어제 안되던데 오늘은 되네",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043209.1.28",
          "form": "ㅋㅋㅋㅋ 지금도 다 나쁜말 됐네요 하여튼",
          "original_form": "ㅋㅋㅋㅋ 지금도 다 나쁜말 됐네요 하여튼",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043209.1.29",
          "form": "안되요 ㅎㅎ 저도 ㅋㅋㅋ",
          "original_form": "안되요 ㅎㅎ 저도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043209.1.30",
          "form": "붕어빵 틀이 있으시다니",
          "original_form": "붕어빵 틀이 있으시다니",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043209.1.31",
