      "utterance": [
        {
          "id": "MDRW2100044410.1.1",
          "form": "ㅋㅋㅋㅋㅇㅇ 재밋다하드라",
          "original_form": "ㅋㅋㅋㅋㅇㅇ 재밋다하드라",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044410.1.2",
          "form": "둘다 비주얼이 요정같다 하던뎅",
          "original_form": "둘다 비주얼이 요정같다 하던뎅",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044410.1.3",
          "form": "아 그러니까 얼굴합 미침",
          "original_form": "아 그러니까 얼굴합 미침",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044410.1.4",
          "form": "ㅋㅋㅋㅋ신민아는 왜 안늙어...",
          "original_form": "ㅋㅋㅋㅋ신민아는 왜 안늙어...",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044410.1.5",
          "form": "언니 넘 예쁘드라",
          "original_form": "언니 넘 예쁘드라",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044410.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ아그러니가",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아그러니가",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044410.1.7",
          "form": "몸매도 장난아님",
          "original_form": "몸매도 장난아님",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044410.1.8",
          "form": "ㅇ ㄹㅇ ㅋㅋㅋ다 가지셨어 언니",
          "original_form": "ㅇ ㄹㅇ ㅋㅋㅋ다 가지셨어 언니",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044410.1.9",
          "form": "레깅스핏 장난아니드라",
          "original_form": "레깅스핏 장난아니드라",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044410.1.10",
          "form": "운동장면나왓음",
          "original_form": "운동장면나왓음",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044410.1.11",
          "form": "드라마에서 레깅스입음?",
          "original_form": "드라마에서 레깅스입음?",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044410.1.12",
          "form": "아하 ㅋㅋㅋ어휴...",
          "original_form": "아하 ㅋㅋㅋ어휴...",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044410.1.13",
          "form": "ㅇㅇ 조깅하는장면ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 조깅하는장면ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044410.1.14",
          "form": "김선호 눈빛 장난아니단데",
          "original_form": "김선호 눈빛 장난아니단데",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.15",
          "form": "저번주 엔딩 ㅠㅠ",
          "original_form": "저번주 엔딩 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.16",
          "form": "왜 벌써 꿀떵어져?",
          "original_form": "왜 벌써 꿀떵어져?",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.17",
          "form": "일단 김선호도 첫눈에 반한듯",
          "original_form": "일단 김선호도 첫눈에 반한듯",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.20",
          "form": "요즘 첫눈에 반하는게 대세가",
          "original_form": "요즘 첫눈에 반하는게 대세가",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044410.1.21",
          "form": "하긴 요즘 드라마가 사실적이긴해",
          "original_form": "하긴 요즘 드라마가 사실적이긴해",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044410.1.22",
          "form": "근데 반할만해 인정",
          "original_form": "근데 반할만해 인정",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044410.1.23",
          "form": "예전엔 누가봐더 예쁜데 뮷생것다카고",
          "original_form": "예전엔 누가봐더 예쁜데 뮷생것다카고",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044410.1.24",
          "form": "신민아 봤는데 어캐 안 반해?...",
          "original_form": "신민아 봤는데 어캐 안 반해?...",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044410.1.25",
          "form": "스우파 오늘 어때",
          "original_form": "스우파 오늘 어때",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044410.1.26",
          "form": "스포말곸ㅋㅋㅋㅋ",
          "original_form": "스포말곸ㅋㅋㅋㅋ",
          "speaker_id": "1",
