      "utterance": [
        {
          "id": "MDRW2100002878.1.1",
          "form": "네 오늘은 금요일이라 좀 한가해요",
          "original_form": "네 오늘은 금요일이라 좀 한가해요",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002878.1.2",
          "form": "칼퇴하겠네요",
          "original_form": "칼퇴하겠네요",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002878.1.3",
          "form": "네 금욜은 보통 칼퇴해욬",
          "original_form": "네 금욜은 보통 칼퇴해욬",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002878.1.4",
          "form": "칼퇴하시나요?",
          "original_form": "칼퇴하시나요?",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002878.1.5",
          "form": "아니여",
          "original_form": "아니여",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002878.1.6",
          "form": "불금인데 야근을 ㅜㅜ",
          "original_form": "불금인데 야근을 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002878.1.7",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002878.1.8",
          "form": "그러게요 ㅠ 내일은 쉬시나요??",
          "original_form": "그러게요 ㅠ 내일은 쉬시나요??",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002878.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002878.1.10",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002878.1.11",
          "form": "주말은 휴무라 ㅎ",
          "original_form": "주말은 휴무라 ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002878.1.12",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100002878.1.13",
          "form": "좀전에 했어요   저녁드셨어요?",
          "original_form": "좀전에 했어요   저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100002878.1.14",
          "form": "오늘 쉬는데 출근했어요 갑자기",
          "original_form": "오늘 쉬는데 출근했어요 갑자기",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100002878.1.15",
          "form": "이런... 거기는 오늘 비 많이 안왔어요?",
          "original_form": "이런... 거기는 오늘 비 많이 안왔어요?",
          "speaker_id": "2",
          "time": "20210821 18:43"
