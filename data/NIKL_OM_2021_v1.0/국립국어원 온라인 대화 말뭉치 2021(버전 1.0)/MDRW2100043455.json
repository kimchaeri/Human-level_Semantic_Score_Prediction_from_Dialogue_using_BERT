      "utterance": [
        {
          "id": "MDRW2100043455.1.1",
          "form": "술은 너도 술장봐서 알지만...잘하면 거기 처박힐 것..",
          "original_form": "술은 너도 술장봐서 알지만...잘하면 거기 처박힐 것..",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043455.1.2",
          "form": "울집에도 양주있더라",
          "original_form": "울집에도 양주있더라",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043455.1.3",
          "form": "아빠가 옛날에 선물받은거래",
          "original_form": "아빠가 옛날에 선물받은거래",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043455.1.4",
          "form": "한 번에 마신 술은 아이스 와인 밖에 없어",
          "original_form": "한 번에 마신 술은 아이스 와인 밖에 없어",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043455.1.5",
          "form": "용량이 엄청 적었지",
          "original_form": "용량이 엄청 적었지",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043455.1.6",
          "form": "오늘 과자 사러 갔다가 맥주도 사왔는데ㅋㅋ",
          "original_form": "오늘 과자 사러 갔다가 맥주도 사왔는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043455.1.7",
          "form": "우리친구들 술꾼이야",
          "original_form": "우리친구들 술꾼이야",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043455.1.8",
          "form": "그렇지 않아",
          "original_form": "그렇지 않아",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043455.1.9",
          "form": "간만에 사온거라구",
          "original_form": "간만에 사온거라구",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043455.1.10",
          "form": "한잔이라구!",
          "original_form": "한잔이라구!",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043455.1.11",
          "form": "한잔도 주기적으로 마시면 중독이래써",
          "original_form": "한잔도 주기적으로 마시면 중독이래써",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043455.1.12",
          "form": "오랜만에 마시는거라구ㅋㅋㅋㅋ",
          "original_form": "오랜만에 마시는거라구ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043455.1.13",
          "form": "그치만 사면 네캔에 만원 살 거 아녀",
          "original_form": "그치만 사면 네캔에 만원 살 거 아녀",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043455.1.14",
          "form": "오늘은 두캔만 샀어",
          "original_form": "오늘은 두캔만 샀어",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043455.1.15",
          "form": "뭔 과자 샀어?",
          "original_form": "뭔 과자 샀어?",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043455.1.16",
          "form": "런닝맨에서 피피엘했던 곱창맛 과자ㅋㅋㅋㅋ",
          "original_form": "런닝맨에서 피피엘했던 곱창맛 과자ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043455.1.17",
          "form": "궁금해서 사봄",
          "original_form": "궁금해서 사봄",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043455.1.18",
          "form": "피피엘 효과 좋넼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "피피엘 효과 좋넼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043455.1.19",
          "form": "하지만 유재석이 녹화하면서 계속 먹었다구요!",
          "original_form": "하지만 유재석이 녹화하면서 계속 먹었다구요!",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043455.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043455.1.21",
          "form": "그게 바로 피피엘이라구요",
          "original_form": "그게 바로 피피엘이라구요",
          "speaker_id": "2",
