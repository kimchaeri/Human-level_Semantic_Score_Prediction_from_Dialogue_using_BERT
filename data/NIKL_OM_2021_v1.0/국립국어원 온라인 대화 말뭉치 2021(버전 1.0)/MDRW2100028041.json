      "utterance": [
        {
          "id": "MDRW2100028041.1.1",
          "form": "열은 안나죠?",
          "original_form": "열은 안나죠?",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028041.1.2",
          "form": "열 잴 생각을 안했으니 아마도요ㅎㅎ",
          "original_form": "열 잴 생각을 안했으니 아마도요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100028041.1.3",
          "form": "약발 잘받으시네요",
          "original_form": "약발 잘받으시네요",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.4",
          "form": "제가 원래 좀ㅎㅎ",
          "original_form": "제가 원래 좀ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.5",
          "form": "신진대사가 천천히 진행되는게 좋다고합니다",
          "original_form": "신진대사가 천천히 진행되는게 좋다고합니다",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.6",
          "form": "미용실 염색약도 잘 받죠ㅎ",
          "original_form": "미용실 염색약도 잘 받죠ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.7",
          "form": "신체 건강하십니다",
          "original_form": "신체 건강하십니다",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.8",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.9",
          "form": "흰머리 검은머리로 하시나요?",
          "original_form": "흰머리 검은머리로 하시나요?",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.10",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028041.1.11",
          "form": "색깔로?",
          "original_form": "색깔로?",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028041.1.12",
          "form": "ㅋㅋㅋ미쵸ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ미쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028041.1.13",
          "form": "지금 머리카락은 무슨색인가요,",
          "original_form": "지금 머리카락은 무슨색인가요,",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028041.1.14",
          "form": "자주 안하는데 하면 갈색으로 합니당 핑크브라운 어둡게나",
          "original_form": "자주 안하는데 하면 갈색으로 합니당 핑크브라운 어둡게나",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100028041.1.15",
          "form": "지금은 그냥 검정이요!",
          "original_form": "지금은 그냥 검정이요!",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100028041.1.16",
          "form": "머리카락이 얇아서 자주는 안해요!",
          "original_form": "머리카락이 얇아서 자주는 안해요!",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100028041.1.17",
          "form": "고데기만 해도 상하는 머리라 고데기도 안함!",
          "original_form": "고데기만 해도 상하는 머리라 고데기도 안함!",
          "speaker_id": "1",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028041.1.18",
          "form": "핑크브라운 오",
          "original_form": "핑크브라운 오",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028041.1.19",
          "form": "형 혹시 청순가련형인가요",
          "original_form": "형 혹시 청순가련형인가요",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028041.1.20",
          "form": "청승가증형입쥬?",
          "original_form": "청승가증형입쥬?",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028041.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028041.1.22",
          "form": "님은 염색 종종 하시나요",
          "original_form": "님은 염색 종종 하시나요",
          "speaker_id": "1",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028041.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028041.1.24",
          "form": "앜",
          "original_form": "앜",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028041.1.25",
          "form": "저는 원래 약간",
          "original_form": "저는 원래 약간",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028041.1.26",
          "form": "밤색?",
          "original_form": "밤색?",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028041.1.27",
          "form": "갈색이라",
          "original_form": "갈색이라",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028041.1.28",
          "form": "종종 오해받습니다",
          "original_form": "종종 오해받습니다",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028041.1.29",
          "form": "너무 이쁘다네요",
          "original_form": "너무 이쁘다네요",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028041.1.30",
          "form": "하하하",
          "original_form": "하하하",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028041.1.31",
          "form": "아 머리카락도 많이 갈색이에요? 오오",
          "original_form": "아 머리카락도 많이 갈색이에요? 오오",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028041.1.32",
          "form": "밤색에 가까워요",
          "original_form": "밤색에 가까워요",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028041.1.33",
          "form": "천연 염색 좋겠다!!",
          "original_form": "천연 염색 좋겠다!!",
          "speaker_id": "1",
          "time": "20210902 17:20"
        }
      ]
