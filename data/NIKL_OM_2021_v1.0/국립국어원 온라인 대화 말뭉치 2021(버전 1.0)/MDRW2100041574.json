      "utterance": [
        {
          "id": "MDRW2100041574.1.1",
          "form": "나 스킨로션 다써가서",
          "original_form": "나 스킨로션 다써가서",
          "speaker_id": "1",
          "time": "20210906 11:29"
        },
        {
          "id": "MDRW2100041574.1.2",
          "form": "새로 사려는데 마침 올리브영 세일중이라",
          "original_form": "새로 사려는데 마침 올리브영 세일중이라",
          "speaker_id": "1",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041574.1.3",
          "form": "여기서 살까 생각중",
          "original_form": "여기서 살까 생각중",
          "speaker_id": "1",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041574.1.4",
          "form": "너 보통 어디거 써?",
          "original_form": "너 보통 어디거 써?",
          "speaker_id": "1",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041574.1.5",
          "form": "이니슾..ㅋㅋㅋ",
          "original_form": "이니슾..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:31"
        },
        {
          "id": "MDRW2100041574.1.6",
          "form": "프리메라랑 피지오겔",
          "original_form": "프리메라랑 피지오겔",
          "speaker_id": "2",
          "time": "20210906 11:31"
        },
        {
          "id": "MDRW2100041574.1.7",
          "form": "고민중인데 나도ㅋㅋㅋ",
          "original_form": "고민중인데 나도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:31"
        },
        {
          "id": "MDRW2100041574.1.8",
          "form": "너 성인여드름있고 그러진않아?",
          "original_form": "너 성인여드름있고 그러진않아?",
          "speaker_id": "2",
          "time": "20210906 11:31"
        },
        {
          "id": "MDRW2100041574.1.9",
          "form": "나 종종 올라오긴 해 ㅠ 주로 생리 다가올때",
          "original_form": "나 종종 올라오긴 해 ㅠ 주로 생리 다가올때",
          "speaker_id": "1",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100041574.1.10",
          "form": "그럼 피지오겔 한번 ㄱ?",
          "original_form": "그럼 피지오겔 한번 ㄱ?",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041574.1.11",
          "form": "나도 한번씩올라와서",
          "original_form": "나도 한번씩올라와서",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041574.1.12",
          "form": "써보려고..",
          "original_form": "써보려고..",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041574.1.13",
          "form": "접촉성피부염같은거도있고해서",
          "original_form": "접촉성피부염같은거도있고해서",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041574.1.14",
          "form": "핑크색 한번 질러보실?",
          "original_form": "핑크색 한번 질러보실?",
          "speaker_id": "2",
          "time": "20210906 12:21"
        },
        {
          "id": "MDRW2100041574.1.15",
          "form": "스킨로션?",
          "original_form": "스킨로션?",
          "speaker_id": "1",
          "time": "20210906 12:30"
        },
        {
          "id": "MDRW2100041574.1.16",
          "form": "나 라운드어라운드 사볼까 하고있는데",
          "original_form": "나 라운드어라운드 사볼까 하고있는데",
          "speaker_id": "1",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100041574.1.17",
          "form": "어 거기유명한데아닌가",
          "original_form": "어 거기유명한데아닌가",
          "speaker_id": "2",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100041574.1.18",
          "form": "독도?시리즈",
          "original_form": "독도?시리즈",
          "speaker_id": "2",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100041574.1.19",
          "form": "ㄴㄴ 독도는 라운드랩이고",
          "original_form": "ㄴㄴ 독도는 라운드랩이고",
          "speaker_id": "1",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041574.1.20",
          "form": "라운드어라운드는 녹차라인쪽일걸??",
          "original_form": "라운드어라운드는 녹차라인쪽일걸??",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041574.1.21",
          "form": "스킨로션 라운드랩도 할인하드라",
          "original_form": "스킨로션 라운드랩도 할인하드라",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041574.1.22",
          "form": "그 독도라인 ㅇㅇ",
          "original_form": "그 독도라인 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041574.1.23",
          "form": "오호..고맙군 니가한번써보고나면",
          "original_form": "오호..고맙군 니가한번써보고나면",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100041574.1.24",
          "form": "나도써볼까..",
          "original_form": "나도써볼까..",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100041574.1.25",
          "form": "그거천연재료많이들어간거같아서",
          "original_form": "그거천연재료많이들어간거같아서",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100041574.1.26",
          "form": "조금꺼려지기도하고ㅋㅋㅋ",
          "original_form": "조금꺼려지기도하고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100041574.1.27",
          "form": "그럼 일단 계획대로 라운드어라운드거 써볼게 ㅋㅋㅋ",
          "original_form": "그럼 일단 계획대로 라운드어라운드거 써볼게 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100041574.1.28",
          "form": "여기 같은라인 폼클렌징은 써봤는데",
          "original_form": "여기 같은라인 폼클렌징은 써봤는데",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100041574.1.29",
          "form": "괜찮더라고",
          "original_form": "괜찮더라고",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100041574.1.30",
          "form": "오키 난 피지오겔 써본다",
          "original_form": "오키 난 피지오겔 써본다",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100041574.1.31",
          "form": "너써보고좋으면 라인싹다바꿔봐야지",
          "original_form": "너써보고좋으면 라인싹다바꿔봐야지",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100041574.1.32",
          "form": "ㅋㅋㅋㅋ아 정착좀해야되는데",
          "original_form": "ㅋㅋㅋㅋ아 정착좀해야되는데",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100041574.1.33",
          "form": "ㅋㅋㅋㅋ 근데 나도 매번 걍 필요할때",
          "original_form": "ㅋㅋㅋㅋ 근데 나도 매번 걍 필요할때",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041574.1.34",
          "form": "세일하는것중 괜찮아보이는거로 사",
          "original_form": "세일하는것중 괜찮아보이는거로 사",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041574.1.35",
          "form": "옛날엔 걍 아무거나 발랐는데",
          "original_form": "옛날엔 걍 아무거나 발랐는데",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100041574.1.36",
          "form": "요샌 뭐좋다는거만바르면..",
          "original_form": "요샌 뭐좋다는거만바르면..",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100041574.1.37",
          "form": "피부가 싼맛에 길들여졌나ㅜ",
          "original_form": "피부가 싼맛에 길들여졌나ㅜ",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100041574.1.38",
          "form": "아님 너무 도시에서 살다보니",
          "original_form": "아님 너무 도시에서 살다보니",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100041574.1.39",
          "form": "천연이 독소가 된걸지도...",
          "original_form": "천연이 독소가 된걸지도...",
          "speaker_id": "1",
          "time": "20210906 20:36"
        }
      ]
    }
