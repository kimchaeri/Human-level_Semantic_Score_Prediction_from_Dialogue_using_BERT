        {
          "id": "MDRW2100046053.1.1",
          "form": "건강 위해 하시는 것 있나요? 전 영양제요",
          "original_form": "건강 위해 하시는 것 있나요? 전 영양제요",
          "speaker_id": "2",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046053.1.2",
          "form": "저도 영양제 챙겨 먹어요",
          "original_form": "저도 영양제 챙겨 먹어요",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046053.1.3",
          "form": "저는 귀찮아서 종합비타민..?",
          "original_form": "저는 귀찮아서 종합비타민..?",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046053.1.4",
          "form": "전 거기에 유산균 루테인 먹어요",
          "original_form": "전 거기에 유산균 루테인 먹어요",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046053.1.5",
          "form": "오오 루테인 눈건강에 좋은거죠??",
          "original_form": "오오 루테인 눈건강에 좋은거죠??",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046053.1.6",
          "form": "네 눈 건강에 도움을 주는 ㅎㅎ",
          "original_form": "네 눈 건강에 도움을 주는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046053.1.7",
          "form": "운동도 하세요??",
          "original_form": "운동도 하세요??",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046053.1.8",
          "form": "스트레칭이랑 걷기요",
          "original_form": "스트레칭이랑 걷기요",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046053.1.9",
          "form": "근력운동이 필요해요",
          "original_form": "근력운동이 필요해요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046053.1.10",
          "form": "오오 스트레칭 !!! ㅠㅜ",
          "original_form": "오오 스트레칭 !!! ㅠㅜ",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046053.1.11",
          "form": "저는 너무 뻣뻣해서 잘 못해요",
          "original_form": "저는 너무 뻣뻣해서 잘 못해요",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046053.1.12",
          "form": "저도 뻣뻣해서 쉬운 것만 해요 하면 유연성이 좀 늘긴 해요",
          "original_form": "저도 뻣뻣해서 쉬운 것만 해요 하면 유연성이 좀 늘긴 해요",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046053.1.13",
          "form": "요가도 좋아하세요?",
          "original_form": "요가도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046053.1.14",
          "form": "안 해 봐서 잘 모르겠어요",
          "original_form": "안 해 봐서 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046053.1.15",
          "form": "저는 유투브 보면서 어설프게 따라해요 ㅋㅋ",
          "original_form": "저는 유투브 보면서 어설프게 따라해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046053.1.16",
