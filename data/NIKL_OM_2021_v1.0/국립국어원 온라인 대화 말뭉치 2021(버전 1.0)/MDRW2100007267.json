      "utterance": [
        {
          "id": "MDRW2100007267.1.1",
          "form": "여행 가보셨나요?",
          "original_form": "여행 가보셨나요?",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007267.1.2",
          "form": "최근에 부산대구대전으로 여행갔었어요",
          "original_form": "최근에 부산대구대전으로 여행갔었어요",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007267.1.3",
          "form": "문어인형과 함께 혼여했습니당 ㅎㅎ",
          "original_form": "문어인형과 함께 혼여했습니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007267.1.4",
          "form": "ㅋㅋㅋㅋㅋ 귀엽다",
          "original_form": "ㅋㅋㅋㅋㅋ 귀엽다",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007267.1.5",
          "form": "왜 하필 문어인형이에요?",
          "original_form": "왜 하필 문어인형이에요?",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007267.1.6",
          "form": "그 유명한 뒤집는 문어인형 있잖아요 ㅎㅎ",
          "original_form": "그 유명한 뒤집는 문어인형 있잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007267.1.7",
          "form": "제 문어가 너무 완벽하고 귀여워서 데리고갔어요 ㅋㅋㅋ",
          "original_form": "제 문어가 너무 완벽하고 귀여워서 데리고갔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007267.1.8",
          "form": "세상에 ㅋㅋㅋㅋㅋㅋㅋ 진찌 귀엽네요",
          "original_form": "세상에 ㅋㅋㅋㅋㅋㅋㅋ 진찌 귀엽네요",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007267.1.9",
          "form": "인증샷 찍기 딱 좋은 비주얼",
          "original_form": "인증샷 찍기 딱 좋은 비주얼",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007267.1.10",
          "form": "3군데 가셨으면 길게 가셨겠어요",
          "original_form": "3군데 가셨으면 길게 가셨겠어요",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007267.1.11",
          "form": "맞아요 ㅋㅋ 사실 포인트지점에 문어만 두고 문어사진만 찍었어요",
          "original_form": "맞아요 ㅋㅋ 사실 포인트지점에 문어만 두고 문어사진만 찍었어요",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007267.1.12",
          "form": "사진 보여드릴수없는게 정말 아쉽네요",
          "original_form": "사진 보여드릴수없는게 정말 아쉽네요",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007267.1.13",
          "form": "여행다니면서 많은 문어인형을 봤는데 제 인형이 제일 귀엽더라고요 ㅎㅎ",
          "original_form": "여행다니면서 많은 문어인형을 봤는데 제 인형이 제일 귀엽더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007267.1.14",
          "form": "ㅋㅋㅋㅋ 아 그게 유행인가요? ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 아 그게 유행인가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007267.1.15",
          "form": "어디가 가장 좋으셨어요?",
          "original_form": "어디가 가장 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007267.1.16",
          "form": "부산대구는 혼자였고 대전에서는 아는 지인들이랑 같이 놀았는데 역시 같이 노는게 재밌더라고요",
          "original_form": "부산대구는 혼자였고 대전에서는 아는 지인들이랑 같이 놀았는데 역시 같이 노는게 재밌더라고요",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007267.1.17",
          "form": "순수 여행은 부산이 좋았어요",
          "original_form": "순수 여행은 부산이 좋았어요",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007267.1.18",
          "form": "오 부산은 저도 한번 가봤는데 정말 도로가 구석구석 미로갔더라구요",
          "original_form": "오 부산은 저도 한번 가봤는데 정말 도로가 구석구석 미로갔더라구요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007267.1.19",
          "form": "버스나 택시가 진짜 다이나믹하다는데 ㅋㅋㅋ 한 번 타볼껄 ㅋㅋ",
          "original_form": "버스나 택시가 진짜 다이나믹하다는데 ㅋㅋㅋ 한 번 타볼껄 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007267.1.20",
          "form": "그 버스 유명한 산북도로 타봤는데 진짜 꼬불꼬불 끝도 없이 올라가요",
          "original_form": "그 버스 유명한 산북도로 타봤는데 진짜 꼬불꼬불 끝도 없이 올라가요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007267.1.21",
          "form": "ㅋㅋㅋㅋ 덜덜 다이나믹하다",
          "original_form": "ㅋㅋㅋㅋ 덜덜 다이나믹하다",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007267.1.22",
          "form": "혼자 여행하시면 무섭지 않으세요?",
          "original_form": "혼자 여행하시면 무섭지 않으세요?",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007267.1.23",
          "form": "네 대신 밤에는 안나가고 숙소에만 있었어요",
          "original_form": "네 대신 밤에는 안나가고 숙소에만 있었어요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007267.1.24",
          "form": "근데 은근 숙소에 혼자있으니까 적막하고 아무소리가 없으니 잠깐 좀 무서웠는데 금방 적응되더라고요 ㅎㅎ",
          "original_form": "근데 은근 숙소에 혼자있으니까 적막하고 아무소리가 없으니 잠깐 좀 무서웠는데 금방 적응되더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007267.1.25",
          "form": "오호~~ 저도 언젠가 혼자여행 도전해 봐야겠네요",
          "original_form": "오호~~ 저도 언젠가 혼자여행 도전해 봐야겠네요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007267.1.26",
          "form": "담에 여행갈 계획이 또 있으신가요?",
          "original_form": "담에 여행갈 계획이 또 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007267.1.27",
          "form": "좋았던점은 숙소 혼자쓰니까 옷을 편하게 입고 일정을 막 변경해도 된다는거 그게좋아요",
          "original_form": "좋았던점은 숙소 혼자쓰니까 옷을 편하게 입고 일정을 막 변경해도 된다는거 그게좋아요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007267.1.28",
          "form": "대구도 다시 가보고싶은데 이번에는 강원도쪽 가보고싶어요",
          "original_form": "대구도 다시 가보고싶은데 이번에는 강원도쪽 가보고싶어요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007267.1.29",
          "form": "그렇구나! 자유!",
          "original_form": "그렇구나! 자유!",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007267.1.30",
          "form": "강원도 요즘 핫하죠",
          "original_form": "강원도 요즘 핫하죠",
          "speaker_id": "1",
          "time": "20210822 01:39"
        }
      ]
