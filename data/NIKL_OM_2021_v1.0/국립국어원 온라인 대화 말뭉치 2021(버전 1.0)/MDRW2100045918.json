      "utterance": [
        {
          "id": "MDRW2100045918.1.1",
          "form": "구러게요",
          "original_form": "구러게요",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.2",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.3",
          "form": "너무 재밌어요ㅋㅋㅋ",
          "original_form": "너무 재밌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.4",
          "form": "역시 돈 쓰는 게 최고~",
          "original_form": "역시 돈 쓰는 게 최고~",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.5",
          "form": "네 ㅋㅋㅋ아니",
          "original_form": "네 ㅋㅋㅋ아니",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.6",
          "form": "핫딜이리서",
          "original_form": "핫딜이리서",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.7",
          "form": "제가 만수르였으면",
          "original_form": "제가 만수르였으면",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.8",
          "form": "진짜 다 샀을 거예요",
          "original_form": "진짜 다 샀을 거예요",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.9",
          "form": "제딴엔 좋은마음으로 남한테 추천하면",
          "original_form": "제딴엔 좋은마음으로 남한테 추천하면",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045918.1.10",
          "form": "다들",
          "original_form": "다들",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.11",
          "form": "아 맞아요ㅠㅠㅠ",
          "original_form": "아 맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.12",
          "form": "절이해못하고",
          "original_form": "절이해못하고",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.13",
          "form": "힝ㅠㅠㅠㅠ",
          "original_form": "힝ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.14",
          "form": "그래서 말 안해요ㅜㅜ",
          "original_form": "그래서 말 안해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.15",
          "form": "그래서이젠안추첱해요..",
          "original_form": "그래서이젠안추첱해요..",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.16",
          "form": "마자요...ㅋㅋㅋㅋㅋ",
          "original_form": "마자요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.17",
          "form": "아니 핫딜 좋은데...",
          "original_form": "아니 핫딜 좋은데...",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.18",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.19",
          "form": "ㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.20",
          "form": "이해가안감",
          "original_form": "이해가안감",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.21",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045918.1.22",
          "form": "알뜰살뜰해야",
          "original_form": "알뜰살뜰해야",
