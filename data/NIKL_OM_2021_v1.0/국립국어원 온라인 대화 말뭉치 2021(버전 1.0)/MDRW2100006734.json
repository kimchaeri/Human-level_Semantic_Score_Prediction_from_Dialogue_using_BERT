      "utterance": [
        {
          "id": "MDRW2100006734.1.1",
          "form": "쇼핑도 자주 하시나요?",
          "original_form": "쇼핑도 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210908 08:00"
        },
        {
          "id": "MDRW2100006734.1.2",
          "form": "요즘 인터넷으로 많이 하는 편이에요",
          "original_form": "요즘 인터넷으로 많이 하는 편이에요",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100006734.1.3",
          "form": "최근에 뭐 사셨나요?",
          "original_form": "최근에 뭐 사셨나요?",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006734.1.4",
          "form": "최근에는 쌀 샀어요.",
          "original_form": "최근에는 쌀 샀어요.",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006734.1.5",
          "form": "생태씨님은 뭐 사신 거 있으신가요?",
          "original_form": "생태씨님은 뭐 사신 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006734.1.6",
          "form": "전 무알콜 맥주요ㅋㅋ",
          "original_form": "전 무알콜 맥주요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006734.1.7",
          "form": "햅쌀 나왔나요?",
          "original_form": "햅쌀 나왔나요?",
          "speaker_id": "1",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006734.1.8",
          "form": "아직 안 나온 거 같아요",
          "original_form": "아직 안 나온 거 같아요",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006734.1.9",
          "form": "아 쌀값 좀 내렸나",
          "original_form": "아 쌀값 좀 내렸나",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006734.1.10",
          "form": "쌀 사려고 하니까 다들 추석 지나면 햅쌀 나온다고 그때 사라고 하더라고요",
          "original_form": "쌀 사려고 하니까 다들 추석 지나면 햅쌀 나온다고 그때 사라고 하더라고요",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006734.1.11",
          "form": "그랬으면 좋겠어요...",
          "original_form": "그랬으면 좋겠어요...",
          "speaker_id": "2",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006734.1.12",
          "form": "작년엔가 엄청 비쌌어서..",
          "original_form": "작년엔가 엄청 비쌌어서..",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006734.1.13",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100006734.1.14",
          "form": "작년에도 쌀을 사긴 했는데 가격은 기억이 잘 안 나네요ㅠㅠ",
          "original_form": "작년에도 쌀을 사긴 했는데 가격은 기억이 잘 안 나네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100006734.1.15",
          "form": "쌀도 택배 괜찮나요?",
          "original_form": "쌀도 택배 괜찮나요?",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006734.1.16",
          "form": "네 다행히 안 터지고 왔더라구요.",
          "original_form": "네 다행히 안 터지고 왔더라구요.",
          "speaker_id": "2",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100006734.1.17",
          "form": "코로나라 쌀이 꽤 줄지요",
          "original_form": "코로나라 쌀이 꽤 줄지요",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006734.1.18",
          "form": "넹 거의 집에서만 밥 해 먹다 보니까 생각보다 빨리 빨리 둘더라고요ㅠㅠ",
          "original_form": "넹 거의 집에서만 밥 해 먹다 보니까 생각보다 빨리 빨리 둘더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006734.1.19",
          "form": "식료품도 배달업체 많더라고요",
          "original_form": "식료품도 배달업체 많더라고요",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100006734.1.20",
          "form": "맞아요 진짜 여기저기서 많이 하는 것 같더라구요.",
          "original_form": "맞아요 진짜 여기저기서 많이 하는 것 같더라구요.",
          "speaker_id": "2",
          "time": "20210908 08:12"
        }
      ]
