      "utterance": [
        {
          "id": "MDRW2100007873.1.1",
          "form": "티비에서 차트를 달리는 남자 보고 있음",
          "original_form": "티비에서 차트를 달리는 남자 보고 있음",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007873.1.2",
          "form": "저 미디어에 올린거 더 음 모 같다",
          "original_form": "저 미디어에 올린거 더 음 모 같다",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007873.1.3",
          "form": "저렇게 많은 눈을 만드는거가 더 신기하다",
          "original_form": "저렇게 많은 눈을 만드는거가 더 신기하다",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007873.1.4",
          "form": "ㅋㅋ그러게 음 모 론이 뭐그리 많노",
          "original_form": "ㅋㅋ그러게 음 모 론이 뭐그리 많노",
          "speaker_id": "2",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007873.1.5",
          "form": "내말이 보면서 계속 그 생각했다",
          "original_form": "내말이 보면서 계속 그 생각했다",
          "speaker_id": "1",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007873.1.6",
          "form": "갑자기 유대인이 왜 나오나",
          "original_form": "갑자기 유대인이 왜 나오나",
          "speaker_id": "2",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007873.1.7",
          "form": "원래 음모론은 아무때나 갖다붙임",
          "original_form": "원래 음모론은 아무때나 갖다붙임",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007873.1.8",
          "form": "아 비행기에 무게 조절하기 위해 물탱크 같은것도 있구나",
          "original_form": "아 비행기에 무게 조절하기 위해 물탱크 같은것도 있구나",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007873.1.9",
          "form": "여러가지 음모론 좀 웃기다",
          "original_form": "여러가지 음모론 좀 웃기다",
          "speaker_id": "1",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007873.1.10",
          "form": "시작한지 얼마 안되었는데 벌써 광고 ㅋㅋ",
          "original_form": "시작한지 얼마 안되었는데 벌써 광고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100007873.1.11",
          "form": "내말이 저래 광고가 많노",
          "original_form": "내말이 저래 광고가 많노",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007873.1.12",
          "form": "그러게 채식주의자들 음모란다 ㅋㅋ",
          "original_form": "그러게 채식주의자들 음모란다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007873.1.13",
          "form": "저게 무슨 말인지.  고기는 먹어야지",
          "original_form": "저게 무슨 말인지.  고기는 먹어야지",
          "speaker_id": "1",
          "time": "20210822 11:35"
        },
        {
          "id": "MDRW2100007873.1.14",
          "form": "저렇게 생각하면 음모론이 아닌게 없겠다",
          "original_form": "저렇게 생각하면 음모론이 아닌게 없겠다",
          "speaker_id": "2",
          "time": "20210822 11:35"
        }
