      "utterance": [
        {
          "id": "MDRW2100008708.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008708.1.2",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008708.1.3",
          "form": "저는 예전에 키웠어요~",
          "original_form": "저는 예전에 키웠어요~",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008708.1.4",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008708.1.5",
          "form": "저도요 문조 두마리 키웠구 지금은 구피 키우고있어요",
          "original_form": "저도요 문조 두마리 키웠구 지금은 구피 키우고있어요",
          "speaker_id": "1",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008708.1.6",
          "form": "문조 두마리 분양받았는데 둘다 숫놈이더라구요",
          "original_form": "문조 두마리 분양받았는데 둘다 숫놈이더라구요",
          "speaker_id": "1",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008708.1.7",
          "form": "구피 번식력 좋다고 하던데",
          "original_form": "구피 번식력 좋다고 하던데",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008708.1.8",
          "form": "구피도 어째 숫놈만 받았는데",
          "original_form": "구피도 어째 숫놈만 받았는데",
          "speaker_id": "1",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008708.1.9",
          "form": "딱 한마리가 암컷이더래요",
          "original_form": "딱 한마리가 암컷이더래요",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.11",
          "form": "어찌하여 숫컷만 ㅋㅋㅋ",
          "original_form": "어찌하여 숫컷만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.12",
          "form": "근데 다들 밥만먹고",
          "original_form": "근데 다들 밥만먹고",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.13",
          "form": "평화로워요 어항안이 ㅋㅋ",
          "original_form": "평화로워요 어항안이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.15",
          "form": "지역 카페에 보면 가끔 구피 분양한다고 글이 있어서",
          "original_form": "지역 카페에 보면 가끔 구피 분양한다고 글이 있어서",
          "speaker_id": "2",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008708.1.16",
          "form": "번식력 좋구나 했거든요",
          "original_form": "번식력 좋구나 했거든요",
          "speaker_id": "2",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008708.1.17",
          "form": "아하 구피가 그렇군요 응아만 싸던데 저희집은ㅋㅋㅋㅋ",
          "original_form": "아하 구피가 그렇군요 응아만 싸던데 저희집은ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008708.1.18",
          "form": "어항에 모기잡은게 빠졌는데 잘먹어서",
          "original_form": "어항에 모기잡은게 빠졌는데 잘먹어서",
          "speaker_id": "1",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008708.1.19",
          "form": "놀랐어요",
          "original_form": "놀랐어요",
          "speaker_id": "1",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008708.1.20",
          "form": "아!! 모기를",
          "original_form": "아!! 모기를",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008708.1.21",
          "form": "전 강아지만 키워봤어요",
          "original_form": "전 강아지만 키워봤어요",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008708.1.22",
          "form": "아진짜요",
          "original_form": "아진짜요",
          "speaker_id": "1",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100008708.1.23",
          "form": "강아지 키우셨군요 부럽습니다",
          "original_form": "강아지 키우셨군요 부럽습니다",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008708.1.24",
          "form": "아! 예전엔 금붕어도 키웠네요",
          "original_form": "아! 예전엔 금붕어도 키웠네요",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008708.1.25",
          "form": "강아지ㅜ 저는 구경만해봤네요",
          "original_form": "강아지ㅜ 저는 구경만해봤네요",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008708.1.26",
          "form": "아 뭔가 많이 키워보셨군요",
          "original_form": "아 뭔가 많이 키워보셨군요",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008708.1.27",
          "form": "근데 반려동물로 제일 오랫동안 같이한건 강아지~~",
          "original_form": "근데 반려동물로 제일 오랫동안 같이한건 강아지~~",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008708.1.28",
          "form": "아무래도 오래사는 종이어야 하는거 같아요ㅜ",
          "original_form": "아무래도 오래사는 종이어야 하는거 같아요ㅜ",
          "speaker_id": "1",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008708.1.29",
          "form": "강아지가 애교도 많고 예쁘죠",
          "original_form": "강아지가 애교도 많고 예쁘죠",
          "speaker_id": "1",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008708.1.30",
          "form": "근데 아파트에서 강아지 키울때 외출이 좀 걸리더라구요",
          "original_form": "근데 아파트에서 강아지 키울때 외출이 좀 걸리더라구요",
          "speaker_id": "2",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008708.1.31",
          "form": "아 그래요? 아파트는 강아지 다니면 안되나요?",
          "original_form": "아 그래요? 아파트는 강아지 다니면 안되나요?",
          "speaker_id": "1",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008708.1.32",
          "form": "그런건 아닌데 식구들이 다 외출할때 강아지 혼자 집에 있으면",
          "original_form": "그런건 아닌데 식구들이 다 외출할때 강아지 혼자 집에 있으면",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.33",
          "form": "괜히 걱정되고 신경 쓰여서요",
          "original_form": "괜히 걱정되고 신경 쓰여서요",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.34",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.35",
          "form": "고양이면 모를까",
          "original_form": "고양이면 모를까",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.36",
          "form": "강쥐는",
          "original_form": "강쥐는",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.37",
          "form": "괜히 우울증 생기면 어쩌나 싶고",
          "original_form": "괜히 우울증 생기면 어쩌나 싶고",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.38",
          "form": "외로움을많이탄다더라구요",
          "original_form": "외로움을많이탄다더라구요",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008708.1.39",
          "form": "진짜 주인이 오면 장난아니게 반겨줘요",
          "original_form": "진짜 주인이 오면 장난아니게 반겨줘요",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008708.1.40",
          "form": "혹 강아지가 우울증생겨요?",
          "original_form": "혹 강아지가 우울증생겨요?",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008708.1.41",
          "form": "아유 예쁘기도해라",
          "original_form": "아유 예쁘기도해라",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008708.1.42",
          "form": "꼬리가떨어져라 반가더라구요",
          "original_form": "꼬리가떨어져라 반가더라구요",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008708.1.43",
          "form": "네~ 강아지 우울증도 있데요",
          "original_form": "네~ 강아지 우울증도 있데요",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008708.1.44",
          "form": "사람은 심리상담가는데 그건 어떻게 치료하나요",
          "original_form": "사람은 심리상담가는데 그건 어떻게 치료하나요",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008708.1.45",
          "form": "주인이 같이 있어주는게 좋은데...그게 쉽지 않으니...",
          "original_form": "주인이 같이 있어주는게 좋은데...그게 쉽지 않으니...",
          "speaker_id": "2",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008708.1.46",
          "form": "주인은 니밥을 벌기위해 일해야 한단다...",
          "original_form": "주인은 니밥을 벌기위해 일해야 한단다...",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008708.1.47",
          "form": "맞습니다!!",
          "original_form": "맞습니다!!",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008708.1.48",
          "form": "그래서 계속 못 키우고 다른곳으로 입양 보냈어요",
          "original_form": "그래서 계속 못 키우고 다른곳으로 입양 보냈어요",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008708.1.49",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008708.1.50",
          "form": "입양갔군요ㅜㅜ",
          "original_form": "입양갔군요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008708.1.51",
          "form": "안타까운 인연이에요",
          "original_form": "안타까운 인연이에요",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008708.1.52",
          "form": "그래도 잘 지내고 있다는 소식 간간히 들어서 좋아요^^",
          "original_form": "그래도 잘 지내고 있다는 소식 간간히 들어서 좋아요^^",
