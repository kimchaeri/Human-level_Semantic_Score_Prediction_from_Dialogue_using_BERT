      "utterance": [
        {
          "id": "MDRW2100017921.1.1",
          "form": "클래식 누구 좋아하세요?",
          "original_form": "클래식 누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017921.1.2",
          "form": "전 요새 비킹구르 올라프손이요 ㅋㅋ 완전 하루종일 배경음악이에요",
          "original_form": "전 요새 비킹구르 올라프손이요 ㅋㅋ 완전 하루종일 배경음악이에요",
          "speaker_id": "2",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017921.1.3",
          "form": "첨들어봅니다 ㅜㅜ",
          "original_form": "첨들어봅니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017921.1.4",
          "form": "뭐 주로 연주해요?",
          "original_form": "뭐 주로 연주해요?",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017921.1.5",
          "form": "그라모폰에서 음반 많이 나왔어요 바흐 음악 신박하게 연주해요 ㅋㅋ",
          "original_form": "그라모폰에서 음반 많이 나왔어요 바흐 음악 신박하게 연주해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.6",
          "form": "피아노예요",
          "original_form": "피아노예요",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.7",
          "form": "name1 좋아하는 스타일은 아닐것 같아요",
          "original_form": "&name1& 좋아하는 스타일은 아닐것 같아요",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.8",
          "form": "아 바흐연주자군요",
          "original_form": "아 바흐연주자군요",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.9",
          "form": "쇼팽 연주 들었는데 엉망이더라구요 ㅋㅋ",
          "original_form": "쇼팽 연주 들었는데 엉망이더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.10",
          "form": "바흐는 잘몰라서요",
          "original_form": "바흐는 잘몰라서요",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.11",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017921.1.12",
          "form": "갬성이 없어요 ㅋㅋ 그래도 바흐는 아주 일품이더라구요 드뷔시도 좋구요",
          "original_form": "갬성이 없어요 ㅋㅋ 그래도 바흐는 아주 일품이더라구요 드뷔시도 좋구요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.13",
          "form": "오호 그렇군요",
          "original_form": "오호 그렇군요",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.14",
          "form": "나이어느정도에요?",
          "original_form": "나이어느정도에요?",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.15",
          "form": "name1은 누구 좋아하세요?",
          "original_form": "&name1&은 누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.16",
          "form": "81년생인가 그래요",
          "original_form": "81년생인가 그래요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.17",
          "form": "요즘은 문지영요 ㅋㅋ",
          "original_form": "요즘은 문지영요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.18",
          "form": "83년생인가 ㅋㅋ",
          "original_form": "83년생인가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017921.1.19",
          "form": "아하 문지영씨 넘 좋더라구요",
          "original_form": "아하 문지영씨 넘 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017921.1.20",
          "form": "네 잘쳐요 ㅜㅠ",
          "original_form": "네 잘쳐요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017921.1.21",
          "form": "그분 잘됐으면 좋겠어요",
          "original_form": "그분 잘됐으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017921.1.22",
          "form": "동감합니다",
          "original_form": "동감합니다",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017921.1.23",
          "form": "어떻게 그런 어려운 사정 속에 재능이 빛을 발하는 건지 정말 흥했으면 좋겠어요",
          "original_form": "어떻게 그런 어려운 사정 속에 재능이 빛을 발하는 건지 정말 흥했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017921.1.24",
          "form": "보니까 원래는 잘사는 집 같긴했어요",
          "original_form": "보니까 원래는 잘사는 집 같긴했어요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017921.1.25",
          "form": "그러다 뭔가 사정이안좋아졌나봐요",
          "original_form": "그러다 뭔가 사정이안좋아졌나봐요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017921.1.26",
          "form": "전에 알려주셔서 라벨 들었는데 듣다 깜짝 놀랐잖아요",
          "original_form": "전에 알려주셔서 라벨 들었는데 듣다 깜짝 놀랐잖아요",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017921.1.27",
          "form": "기사검색하다 짜맞춘거요 초기엔 이말저말 있더라구요",
          "original_form": "기사검색하다 짜맞춘거요 초기엔 이말저말 있더라구요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017921.1.28",
          "form": "아아 그러셨군요",
          "original_form": "아아 그러셨군요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017921.1.29",
          "form": "아.. 전 부모님 장애인이고 어렵다는 이야기만 읽었어요",
          "original_form": "아.. 전 부모님 장애인이고 어렵다는 이야기만 읽었어요",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017921.1.30",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017921.1.31",
          "form": "아직 오스트리아서 유학중이죠?",
          "original_form": "아직 오스트리아서 유학중이죠?",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017921.1.32",
          "form": "흠 유학중인진 모르겠어요",
          "original_form": "흠 유학중인진 모르겠어요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017921.1.33",
          "form": "name2 안가셔도되나요",
          "original_form": "&name2& 안가셔도되나요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017921.1.34",
          "form": "클래식도 성차별 너무 심한데 아무튼 잘되셔야해요 그분은",
          "original_form": "클래식도 성차별 너무 심한데 아무튼 잘되셔야해요 그분은",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017921.1.35",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017921.1.36",
          "form": "그렇게 정석으로 잘치는 분들은 특히 더 잘돼야해요",
          "original_form": "그렇게 정석으로 잘치는 분들은 특히 더 잘돼야해요",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017921.1.37",
          "form": "요새 유명한데 저질(?) 연주자들이 너무 많아요",
