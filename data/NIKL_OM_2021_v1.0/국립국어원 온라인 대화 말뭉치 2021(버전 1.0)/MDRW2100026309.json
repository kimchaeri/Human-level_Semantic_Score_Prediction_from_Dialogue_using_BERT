      "utterance": [
        {
          "id": "MDRW2100026309.1.1",
          "form": "집안일은 자주",
          "original_form": "집안일은 자주",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026309.1.2",
          "form": "하시나여 ㅋㅋ",
          "original_form": "하시나여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026309.1.3",
          "form": "이것저것 하고 있어요 ㅋㅋ",
          "original_form": "이것저것 하고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026309.1.4",
          "form": "제일 싫어하는 집안일은 어떤건가요?",
          "original_form": "제일 싫어하는 집안일은 어떤건가요?",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026309.1.5",
          "form": "음쓰버리기요.....",
          "original_form": "음쓰버리기요.....",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026309.1.6",
          "form": "ㅋㅋ다들 비슷하네요",
          "original_form": "ㅋㅋ다들 비슷하네요",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026309.1.7",
          "form": "빨리 버려야지 놔뒀다간..",
          "original_form": "빨리 버려야지 놔뒀다간..",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026309.1.8",
          "form": "진짜 음쓰 색 변하는게...",
          "original_form": "진짜 음쓰 색 변하는게...",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026309.1.9",
          "form": "냄새도 끔찍하죠ㅎㅎ",
          "original_form": "냄새도 끔찍하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026309.1.10",
          "form": "화장실 청소도 참 귀찮은 거 같아요",
          "original_form": "화장실 청소도 참 귀찮은 거 같아요",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026309.1.11",
          "form": "아 거기도 참...",
          "original_form": "아 거기도 참...",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026309.1.12",
          "form": "내일 해야겠어요ㅠ",
          "original_form": "내일 해야겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026309.1.13",
          "form": "락스청소 하고 나면 좋은데 냄새 때문에 자주는 못하겠어요",
          "original_form": "락스청소 하고 나면 좋은데 냄새 때문에 자주는 못하겠어요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026309.1.14",
          "form": "ㅋㅋ내일 하겠습니다",
          "original_form": "ㅋㅋ내일 하겠습니다",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026309.1.15",
          "form": "마스크 끼고 해야지",
          "original_form": "마스크 끼고 해야지",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026309.1.16",
          "form": "저도 생각난김에 조만간 해야겠네요ㅠ",
          "original_form": "저도 생각난김에 조만간 해야겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026309.1.17",
          "form": "ㅋㅋ 내일 출근 안하시나여",
          "original_form": "ㅋㅋ 내일 출근 안하시나여",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026309.1.18",
          "form": "ㅋㅋ 아마 화장실 청소는 주말에나 하겠죠",
          "original_form": "ㅋㅋ 아마 화장실 청소는 주말에나 하겠죠",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026309.1.19",
          "form": "ㅋㅋ아 조만간이었구나",
          "original_form": "ㅋㅋ아 조만간이었구나",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026309.1.20",
          "form": "눈이 피로한가 ㅎㅎ",
          "original_form": "눈이 피로한가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:44"
        }
      ]
