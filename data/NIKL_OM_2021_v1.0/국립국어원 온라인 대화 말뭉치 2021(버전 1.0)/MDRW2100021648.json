      "utterance": [
        {
          "id": "MDRW2100021648.1.1",
          "form": "안녕하세요 벌써 가을 같아요",
          "original_form": "안녕하세요 벌써 가을 같아요",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021648.1.2",
          "form": "안녕하세요~ 날씨가 선선해지니 너무 좋네요",
          "original_form": "안녕하세요~ 날씨가 선선해지니 너무 좋네요",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021648.1.3",
          "form": "가을 좋아하시나요?",
          "original_form": "가을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021648.1.4",
          "form": "가을 날씨는 좋아하는데 바빠서 별로에요 ㅋㅋㅋㅋㅋ",
          "original_form": "가을 날씨는 좋아하는데 바빠서 별로에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021648.1.5",
          "form": "무슨 계절 좋아하세요",
          "original_form": "무슨 계절 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100021648.1.6",
          "form": "전 가을 제일 좋아해요!",
          "original_form": "전 가을 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021648.1.7",
          "form": "니트랑 가디건 입는거 좋아하는데 가을이 딱 그 옷입을 수 있는 날씨라 좋네요",
          "original_form": "니트랑 가디건 입는거 좋아하는데 가을이 딱 그 옷입을 수 있는 날씨라 좋네요",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021648.1.8",
          "form": "니트랑 가디건 예쁘죠",
          "original_form": "니트랑 가디건 예쁘죠",
          "speaker_id": "1",
          "time": "20210829 08:57"
        },
        {
          "id": "MDRW2100021648.1.9",
          "form": "혹시 이번 가을 옷 개시하셨나요?",
          "original_form": "혹시 이번 가을 옷 개시하셨나요?",
          "speaker_id": "1",
          "time": "20210829 08:57"
        },
        {
          "id": "MDRW2100021648.1.10",
          "form": "요즘 운동할때만 외출해서 아직이요..!",
          "original_form": "요즘 운동할때만 외출해서 아직이요..!",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100021648.1.11",
          "form": "근데 긴팔입어도될 것 같아요 오늘 아침에 되게 쌀쌀하더라구요",
          "original_form": "근데 긴팔입어도될 것 같아요 오늘 아침에 되게 쌀쌀하더라구요",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100021648.1.12",
          "form": "맞아요 아침저녁으로 쌀쌀해여",
          "original_form": "맞아요 아침저녁으로 쌀쌀해여",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100021648.1.13",
          "form": "그쵸 다음주부터는 또 비가 온다는데.. 걱정이네요",
          "original_form": "그쵸 다음주부터는 또 비가 온다는데.. 걱정이네요",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100021648.1.14",
          "form": "그러게요 비오면 운동화 다 젖어서 싫은데 말이에요",
          "original_form": "그러게요 비오면 운동화 다 젖어서 싫은데 말이에요",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100021648.1.15",
          "form": "맞아요 축축한 운동화 정말 싫어여..ㅠㅠ",
          "original_form": "맞아요 축축한 운동화 정말 싫어여..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100021648.1.16",
          "form": "양말도 다 젖고ㅠㅠ",
          "original_form": "양말도 다 젖고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100021648.1.17",
          "form": "그쵸 ㅠㅠ 진짜 비오는 날 외출하는거 너무 싫더라구요..",
          "original_form": "그쵸 ㅠㅠ 진짜 비오는 날 외출하는거 너무 싫더라구요..",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100021648.1.18",
          "form": "비는 언제쯤 그칠까요ㅠㅠ",
          "original_form": "비는 언제쯤 그칠까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100021648.1.19",
          "form": "그러게요 ㅠㅠ 그만 왔으면 좋겠네요",
          "original_form": "그러게요 ㅠㅠ 그만 왔으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210829 20:53"
        }
      ]
