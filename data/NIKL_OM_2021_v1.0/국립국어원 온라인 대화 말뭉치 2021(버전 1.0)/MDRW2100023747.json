      "utterance": [
        {
          "id": "MDRW2100023747.1.1",
          "form": "ㅋㅋㅋㅋ 전 고정입니다..",
          "original_form": "ㅋㅋㅋㅋ 전 고정입니다..",
          "speaker_id": "2",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100023747.1.2",
          "form": "샐러드요.. ㅋㅋㅋ",
          "original_form": "샐러드요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100023747.1.3",
          "form": "샐러드 먹으면",
          "original_form": "샐러드 먹으면",
          "speaker_id": "1",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023747.1.4",
          "form": "배 안고파요?????",
          "original_form": "배 안고파요?????",
          "speaker_id": "1",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100023747.1.5",
          "form": "풀을 코끼리 처럼 먹어요... ㅋㅋㅋ \r\n물론 단백질이랑 빵도 먹어요..",
          "original_form": "풀을 코끼리 처럼 먹어요... ㅋㅋㅋ \r\n물론 단백질이랑 빵도 먹어요..",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100023747.1.6",
          "form": "ㅋㅋㅋㅋㅋ아 그래도 그렇게 먹어야죠",
          "original_form": "ㅋㅋㅋㅋㅋ아 그래도 그렇게 먹어야죠",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023747.1.7",
          "form": "안그라면",
          "original_form": "안그라면",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023747.1.8",
          "form": "배고파 죽습니다..",
          "original_form": "배고파 죽습니다..",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100023747.1.9",
          "form": "ㅋㅋㅋ 먹는걸 좋아하시나요?",
          "original_form": "ㅋㅋㅋ 먹는걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100023747.1.10",
          "form": "네 중요하죠",
          "original_form": "네 중요하죠",
          "speaker_id": "1",
          "time": "20210830 16:34"
        },
        {
          "id": "MDRW2100023747.1.11",
          "form": "먹는게 최고 아닌가요..",
          "original_form": "먹는게 최고 아닌가요..",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100023747.1.12",
          "form": "아닌데요? ㅋㅋㅋㅋ",
          "original_form": "아닌데요? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100023747.1.13",
          "form": "뭘 젤 좋아하세유?",
          "original_form": "뭘 젤 좋아하세유?",
          "speaker_id": "2",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100023747.1.14",
          "form": "전 다 좋아해요...",
          "original_form": "전 다 좋아해요...",
          "speaker_id": "1",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100023747.1.15",
          "form": "음식 별로세여?",
          "original_form": "음식 별로세여?",
          "speaker_id": "1",
          "time": "20210830 16:38"
        },
        {
          "id": "MDRW2100023747.1.16",
          "form": "음식이랄까.. 쪼꼬레또는 언청 좋아하죠.. ㅋㅋㅋ",
          "original_form": "음식이랄까.. 쪼꼬레또는 언청 좋아하죠.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023747.1.17",
          "form": "ㅋㅋ ㅋㅋ ㅋㅋ음식은 그냥",
          "original_form": "ㅋㅋ ㅋㅋ ㅋㅋ음식은 그냥",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023747.1.18",
          "form": "살기 위해 먹는건가여?",
          "original_form": "살기 위해 먹는건가여?",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023747.1.19",
          "form": "거의 그런 느낌이에요... \r\n근데.또 과자는 엄청 먹어요\r\n. ㅋㅋㅋ",
          "original_form": "거의 그런 느낌이에요... \r\n근데.또 과자는 엄청 먹어요\r\n. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023747.1.20",
          "form": "ㅋㅋㅋㅋㅋ아 저두여 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ아 저두여 ㅎ",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023747.1.21",
          "form": "과자 좋아합니다",
          "original_form": "과자 좋아합니다",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023747.1.22",
          "form": "홈런볼 꼬북칩 콘초코는 없으면 못살져.. ㅋㅋ",
          "original_form": "홈런볼 꼬북칩 콘초코는 없으면 못살져.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100023747.1.23",
          "form": "어머.. 완전",
          "original_form": "어머.. 완전",
          "speaker_id": "1",
          "time": "20210830 17:15"
        },
        {
          "id": "MDRW2100023747.1.24",
          "form": "맛있을 거 같아요 ㅠㅠㅠ",
          "original_form": "맛있을 거 같아요 ㅠㅠㅠ",
