      "utterance": [
        {
          "id": "MDRW2100022303.1.1",
          "form": "예능 스포츠는 원래 그런 맛이지",
          "original_form": "예능 스포츠는 원래 그런 맛이지",
          "speaker_id": "1",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022303.1.2",
          "form": "두자리 수는 너무 하지 않나욬ㅋㅋㅋㅋ",
          "original_form": "두자리 수는 너무 하지 않나욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022303.1.3",
          "form": "광수가 그만큼 예능적 인재기 때문...",
          "original_form": "광수가 그만큼 예능적 인재기 때문...",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022303.1.4",
          "form": "광수야 돌아왓...",
          "original_form": "광수야 돌아왓...",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022303.1.5",
          "form": "광수 근데 왜 하차해?",
          "original_form": "광수 근데 왜 하차해?",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022303.1.6",
          "form": "바빠서 다리 재활 못 한거 하려구",
          "original_form": "바빠서 다리 재활 못 한거 하려구",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022303.1.7",
          "form": "다쳤구나 ㅇㅁㅇ...",
          "original_form": "다쳤구나 ㅇㅁㅇ...",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022303.1.8",
          "form": "예전에 교통사고 있었음",
          "original_form": "예전에 교통사고 있었음",
          "speaker_id": "1",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022303.1.9",
          "form": "몸이 재산이니 어쩔 수 없지",
          "original_form": "몸이 재산이니 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022303.1.10",
          "form": "좀 쉬어야겠네",
          "original_form": "좀 쉬어야겠네",
          "speaker_id": "2",
          "time": "20210829 12:33"
        },
        {
          "id": "MDRW2100022303.1.11",
          "form": "그치... 좋아지면 돌아왔음 좋겠다",
          "original_form": "그치... 좋아지면 돌아왔음 좋겠다",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022303.1.12",
          "form": "런닝맨이 그때까지 있어야되는 문제가 있지만",
          "original_form": "런닝맨이 그때까지 있어야되는 문제가 있지만",
          "speaker_id": "1",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022303.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 런닝맨도 없어질 것 같애?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 런닝맨도 없어질 것 같애?",
          "speaker_id": "2",
          "time": "20210829 12:34"
        },
        {
          "id": "MDRW2100022303.1.14",
          "form": "지금이야 괜찮지만 예능은 항상 모르는거니깐...",
          "original_form": "지금이야 괜찮지만 예능은 항상 모르는거니깐...",
          "speaker_id": "1",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022303.1.15",
          "form": "???:나도 무도가 영원할줄 알았어",
          "original_form": "???:나도 무도가 영원할줄 알았어",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022303.1.16",
          "form": "그렇넼ㅋㅋㅋ선례가 있었네",
          "original_form": "그렇넼ㅋㅋㅋ선례가 있었네",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022303.1.17",
          "form": "요새 놀뭐에 무도멤버들 나오는데",
          "original_form": "요새 놀뭐에 무도멤버들 나오는데",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022303.1.18",
          "form": "왜 내가 찡해지냐",
          "original_form": "왜 내가 찡해지냐",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
