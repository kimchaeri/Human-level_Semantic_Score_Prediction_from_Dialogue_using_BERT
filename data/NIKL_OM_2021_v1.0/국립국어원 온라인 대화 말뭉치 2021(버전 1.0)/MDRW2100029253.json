      "utterance": [
        {
          "id": "MDRW2100029253.1.1",
          "form": "종교있으세요?!",
          "original_form": "종교있으세요?!",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029253.1.2",
          "form": "아니여 ㅋㅋㅋ 있으신가요?",
          "original_form": "아니여 ㅋㅋㅋ 있으신가요?",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.3",
          "form": "저도없어요",
          "original_form": "저도없어요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.4",
          "form": "교회가보신적있나요?",
          "original_form": "교회가보신적있나요?",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.5",
          "form": "ㅋㅋㅋㅋ 무교이신것도 같네요",
          "original_form": "ㅋㅋㅋㅋ 무교이신것도 같네요",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.6",
          "form": "저는 성당은 있어요",
          "original_form": "저는 성당은 있어요",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.7",
          "form": "헉티가나나요?ㅋㄱㅋㅋㅋ",
          "original_form": "헉티가나나요?ㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.8",
          "form": "교회는 안가봤어요",
          "original_form": "교회는 안가봤어요",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029253.1.9",
          "form": "저는 어릴때 친구따라서 달란트시장",
          "original_form": "저는 어릴때 친구따라서 달란트시장",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029253.1.10",
          "form": "기봤어요",
          "original_form": "기봤어요",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029253.1.11",
          "form": "종교 있으세요 라는 질문부터 무교인임을 증명하는거 같아요 ㅋㅋㅋㅋ",
          "original_form": "종교 있으세요 라는 질문부터 무교인임을 증명하는거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029253.1.12",
          "form": "ㅋㄱㅋㅋㅋㅋ헐그렇게 느껴지시는군요ㅋㅋㄱㅋㄱㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㄱㅋㅋㅋㅋ헐그렇게 느껴지시는군요ㅋㅋㄱㅋㄱㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029253.1.13",
          "form": "조심해아겠다",
          "original_form": "조심해아겠다",
          "speaker_id": "1",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029253.1.14",
          "form": "저도 종교 있으세요 보내려고 했거든요 ㅋㅋㅋㅋ",
          "original_form": "저도 종교 있으세요 보내려고 했거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029253.1.15",
          "form": "ㅋㄱㅋㅋㄱ아 무교 종특..?",
          "original_form": "ㅋㄱㅋㅋㄱ아 무교 종특..?",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029253.1.16",
          "form": "보통 종교는 먼저 밝히시긴 하더라고요 ㅋㅋㅋㅋㅋ",
          "original_form": "보통 종교는 먼저 밝히시긴 하더라고요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029253.1.17",
          "form": "ㅋㄱㅋㅋㅋ아 맞다 아니면 교인이세요? 이러더라고요",
          "original_form": "ㅋㄱㅋㅋㅋ아 맞다 아니면 교인이세요? 이러더라고요",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100029253.1.18",
