      "utterance": [
        {
          "id": "MDRW2100042865.1.1",
          "form": "빗길 조심하세요!",
          "original_form": "빗길 조심하세요!",
          "speaker_id": "2",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100042865.1.2",
          "form": "저 오다가 넘어져서ㅜㅜ 아프더라구욬ㅋㅋ",
          "original_form": "저 오다가 넘어져서ㅜㅜ 아프더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100042865.1.3",
          "form": "헐ㅠㅠ 괜찮으신가용..",
          "original_form": "헐ㅠㅠ 괜찮으신가용..",
          "speaker_id": "1",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042865.1.4",
          "form": "넵 괜찮습니다!",
          "original_form": "넵 괜찮습니다!",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042865.1.5",
          "form": "비는 이래저래 싫어요ㅜㅜ",
          "original_form": "비는 이래저래 싫어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 11:01"
        },
        {
          "id": "MDRW2100042865.1.6",
          "form": "비도오니 점심은 찌개먹어야겠어용",
          "original_form": "비도오니 점심은 찌개먹어야겠어용",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042865.1.7",
          "form": "오 좋죠",
          "original_form": "오 좋죠",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042865.1.8",
          "form": "맛있게드세요!",
          "original_form": "맛있게드세요!",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042865.1.9",
          "form": "저는떡만두국먹었습니당ㅋㅋ",
          "original_form": "저는떡만두국먹었습니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042865.1.10",
          "form": "낮인데도 바람이 싸하네요",
          "original_form": "낮인데도 바람이 싸하네요",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042865.1.11",
          "form": "맞아요ㅜㅜ 비도점점많이오고",
          "original_form": "맞아요ㅜㅜ 비도점점많이오고",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042865.1.12",
          "form": "내일은 쉬는날이라 집에서 흐린날씨 구경해야겠어요 ㅋㅋ",
          "original_form": "내일은 쉬는날이라 집에서 흐린날씨 구경해야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:45"
        },
        {
          "id": "MDRW2100042865.1.13",
          "form": "우와 쉬시는구나 좋으시겠어요ㅜㅜ",
          "original_form": "우와 쉬시는구나 좋으시겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100042865.1.14",
          "form": "오후엔그래도그치나봐요!",
          "original_form": "오후엔그래도그치나봐요!",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100042865.1.15",
          "form": "여긴 지금 흩날려요!",
          "original_form": "여긴 지금 흩날려요!",
          "speaker_id": "1",
          "time": "20210907 16:13"
        },
        {
          "id": "MDRW2100042865.1.16",
          "form": "아 내일오후네요ㅜㅜㅋㅋ",
          "original_form": "아 내일오후네요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100042865.1.17",
          "form": "아하 ㅎㅎ 이제슬슬 국물요리가 땡기네요",
          "original_form": "아하 ㅎㅎ 이제슬슬 국물요리가 땡기네요",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100042865.1.18",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100042865.1.19",
          "form": "저녁으로 해먹을까봐요!",
          "original_form": "저녁으로 해먹을까봐요!",
          "speaker_id": "2",
          "time": "20210907 18:43"
        }
      ]
