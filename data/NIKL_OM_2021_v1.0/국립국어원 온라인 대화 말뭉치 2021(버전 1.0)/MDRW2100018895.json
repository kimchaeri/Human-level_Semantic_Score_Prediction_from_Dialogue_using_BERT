      "utterance": [
        {
          "id": "MDRW2100018895.1.1",
          "form": "요새 사고싶음거 있으신가요",
          "original_form": "요새 사고싶음거 있으신가요",
          "speaker_id": "2",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018895.1.2",
          "form": "저 요새 소소히 식탁 깔개가 갖고 싶어요",
          "original_form": "저 요새 소소히 식탁 깔개가 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018895.1.3",
          "form": "지금 식탁은 그릇을 놓으면 소리가 많이 나더라구요",
          "original_form": "지금 식탁은 그릇을 놓으면 소리가 많이 나더라구요",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018895.1.4",
          "form": "아ㅋㅋㅋ 나무요?",
          "original_form": "아ㅋㅋㅋ 나무요?",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018895.1.5",
          "form": "깔개하나 장만하셔야겟네요 ㅋㅋ",
          "original_form": "깔개하나 장만하셔야겟네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018895.1.6",
          "form": "네네 ㅠㅠ 귀찮아서 미뤘는데 오늘은 사려구요",
          "original_form": "네네 ㅠㅠ 귀찮아서 미뤘는데 오늘은 사려구요",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018895.1.7",
          "form": "name2님은 사고 싶은거 뭐있으세요?",
          "original_form": "&name2&님은 사고 싶은거 뭐있으세요?",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018895.1.8",
          "form": "저 이쁜 목걸이 사구싶어요 ㅠㅋㅋㅋㅋ",
          "original_form": "저 이쁜 목걸이 사구싶어요 ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018895.1.9",
          "form": "걍 오래낄거? 근데 비싸서 못삼 ㅠ",
          "original_form": "걍 오래낄거? 근데 비싸서 못삼 ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018895.1.10",
          "form": "아 맞아요 ㅠㅠ 오래낄거 사면 또 가격이 비싸죠",
          "original_form": "아 맞아요 ㅠㅠ 오래낄거 사면 또 가격이 비싸죠",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018895.1.11",
          "form": "안산선수 로이드 화보 보셨나요?",
          "original_form": "안산선수 로이드 화보 보셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018895.1.12",
          "form": "로이드 별 생각 없었는데 그거 보니까 로이드꺼 하나 사고싶더라구요ㅋㅋㅋ",
          "original_form": "로이드 별 생각 없었는데 그거 보니까 로이드꺼 하나 사고싶더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018895.1.13",
          "form": "짐찾아봤는뎅 이쁘네요 ㅠㅋㅋㅋ",
          "original_form": "짐찾아봤는뎅 이쁘네요 ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018895.1.14",
          "form": "갖고싶은게 넘많아서 문제에요",
          "original_form": "갖고싶은게 넘많아서 문제에요",
          "speaker_id": "2",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100018895.1.15",
          "form": "늘그렇죠 ㅠㅠㅠㅠ",
          "original_form": "늘그렇죠 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018895.1.16",
          "form": "차량용방향제도 사고 싶어요",
          "original_form": "차량용방향제도 사고 싶어요",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018895.1.17",
          "form": "ㅋㅋㅋ 자차있으시구낭 뭐사고싶으세요",
          "original_form": "ㅋㅋㅋ 자차있으시구낭 뭐사고싶으세요",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018895.1.18",
          "form": "저 그냥 꽃향기 별로 안독한거 사고 싶어요ㅋㅋㅋ 디퓨저형으로",
          "original_form": "저 그냥 꽃향기 별로 안독한거 사고 싶어요ㅋㅋㅋ 디퓨저형으로",
          "speaker_id": "1",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018895.1.19",
          "form": "친구께 좋길래 친구한테 추천 받으려구요",
          "original_form": "친구께 좋길래 친구한테 추천 받으려구요",
          "speaker_id": "1",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018895.1.20",
          "form": "name2님은 또 뭐사고 싶으세요?",
          "original_form": "&name2&님은 또 뭐사고 싶으세요?",
          "speaker_id": "1",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018895.1.21",
          "form": "오호! 전 신발이요 ~ 근데 이미 많아서 누르는중 ㅋㅋ",
          "original_form": "오호! 전 신발이요 ~ 근데 이미 많아서 누르는중 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:36"
        },
        {
          "id": "MDRW2100018895.1.22",
          "form": "아하ㅋㅋㅋㅋㅋㅋ저도 신발 바꿔야해요",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋ저도 신발 바꿔야해요",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100018895.1.23",
          "form": "발목 잡아주는 운동화가 갖고싶네요",
          "original_form": "발목 잡아주는 운동화가 갖고싶네요",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100018895.1.24",
          "form": "발목이 좀 흔들거리시나요",
          "original_form": "발목이 좀 흔들거리시나요",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100018895.1.25",
          "form": "예ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠ",
          "original_form": "예ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100018895.1.26",
          "form": "예전엔 슬리퍼신고도 잘다녔는데 요샌 시리네요",
