      "utterance": [
        {
          "id": "MDRW2100022374.1.1",
          "form": "저희집은 세로형 써요",
          "original_form": "저희집은 세로형 써요",
          "speaker_id": "1",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022374.1.2",
          "form": "엇 세로형 김치냉장고요?",
          "original_form": "엇 세로형 김치냉장고요?",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.3",
          "form": "네 일체형 쓰시나봐요",
          "original_form": "네 일체형 쓰시나봐요",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.4",
          "form": "요즘 디자인 그렇게 나와서 정말 예쁘던데.. 일반 냉장고인줄 알았어요",
          "original_form": "요즘 디자인 그렇게 나와서 정말 예쁘던데.. 일반 냉장고인줄 알았어요",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.5",
          "form": "저희집은 그 아주 옛날거에요",
          "original_form": "저희집은 그 아주 옛날거에요",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.6",
          "form": "그냥 냉장고 같긴해요 사실ㅈ그런 김치냉장고가",
          "original_form": "그냥 냉장고 같긴해요 사실ㅈ그런 김치냉장고가",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.7",
          "form": "김치 잘익더라구요",
          "original_form": "김치 잘익더라구요",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.8",
          "form": "세로형은 잘 모르겠어요",
          "original_form": "세로형은 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.9",
          "form": "배추김치는 그 두가지 매력 있더라구요",
          "original_form": "배추김치는 그 두가지 매력 있더라구요",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022374.1.10",
          "form": "어떤매력이요",
          "original_form": "어떤매력이요",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.11",
          "form": "뿌리쪽이랑 잎쪽이라고 해야될까?",
          "original_form": "뿌리쪽이랑 잎쪽이라고 해야될까?",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.12",
          "form": "식감이 달라요",
          "original_form": "식감이 달라요",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.13",
          "form": "배쪽이요?",
          "original_form": "배쪽이요?",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.14",
          "form": "입파리랑",
          "original_form": "입파리랑",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.15",
          "form": "어디쪽 좋아하세요",
          "original_form": "어디쪽 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.16",
          "form": "이파리..",
          "original_form": "이파리..",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.17",
          "form": "저는 그 아삭한 쪽이요",
          "original_form": "저는 그 아삭한 쪽이요",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.18",
          "form": "배쪽?",
          "original_form": "배쪽?",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.19",
          "form": "배쪽 좋아하시는구나",
          "original_form": "배쪽 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.20",
          "form": "저도그래요",
          "original_form": "저도그래요",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.21",
          "form": "근데 대부분은 이파리 좋아하던데 ㅜㅜㅜ",
          "original_form": "근데 대부분은 이파리 좋아하던데 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.22",
          "form": "어릴 땐 이파리좋아했는데",
          "original_form": "어릴 땐 이파리좋아했는데",
          "speaker_id": "1",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022374.1.23",
          "form": "크니까 배쪽이 맛있더라구요",
          "original_form": "크니까 배쪽이 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 13:17"
        }
