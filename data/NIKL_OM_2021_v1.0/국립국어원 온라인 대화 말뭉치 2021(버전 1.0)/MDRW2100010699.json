      "utterance": [
        {
          "id": "MDRW2100010699.1.1",
          "form": "카트라이더2 나오면 같이 해",
          "original_form": "카트라이더2 나오면 같이 해",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010699.1.2",
          "form": "레이싱 게임은 좀",
          "original_form": "레이싱 게임은 좀",
          "speaker_id": "2",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010699.1.3",
          "form": "사이퍼즈 할래?",
          "original_form": "사이퍼즈 할래?",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010699.1.4",
          "form": "아직두 해?",
          "original_form": "아직두 해?",
          "speaker_id": "1",
          "time": "20210823 20:49"
        },
        {
          "id": "MDRW2100010699.1.5",
          "form": "나 PC 켜볼 여력되면 깔아봄",
          "original_form": "나 PC 켜볼 여력되면 깔아봄",
          "speaker_id": "1",
          "time": "20210823 20:49"
        },
        {
          "id": "MDRW2100010699.1.6",
          "form": "ㅇㅇ 근데 고인물 많아져서",
          "original_form": "ㅇㅇ 근데 고인물 많아져서",
          "speaker_id": "2",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010699.1.7",
          "form": "봇전도 ㄱㅊ",
          "original_form": "봇전도 ㄱㅊ",
          "speaker_id": "2",
          "time": "20210823 20:52"
        },
        {
          "id": "MDRW2100010699.1.8",
          "form": "난 그런 겜 잘 못해",
          "original_form": "난 그런 겜 잘 못해",
          "speaker_id": "1",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010699.1.9",
          "form": "그래서 봇전이 있는거야",
          "original_form": "그래서 봇전이 있는거야",
          "speaker_id": "2",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010699.1.10",
          "form": "봇전만 주구장창하는거군",
          "original_form": "봇전만 주구장창하는거군",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010699.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010699.1.12",
          "form": "초보자를 위한 배려라고 해줘",
          "original_form": "초보자를 위한 배려라고 해줘",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010699.1.13",
          "form": "상냥한 게임이구나",
          "original_form": "상냥한 게임이구나",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010699.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010699.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010699.1.16",
          "form": "게임은 상냥하지 않지만 유저가 상냥한 편",
          "original_form": "게임은 상냥하지 않지만 유저가 상냥한 편",
          "speaker_id": "2",
          "time": "20210823 20:59"
        },
        {
          "id": "MDRW2100010699.1.17",
          "form": "뉴비한테 상냥한 고인물 유저들",
          "original_form": "뉴비한테 상냥한 고인물 유저들",
          "speaker_id": "1",
          "time": "20210823 21:00"
        },
        {
          "id": "MDRW2100010699.1.18",
          "form": "제발 와줘 너 없으면 망해",
          "original_form": "제발 와줘 너 없으면 망해",
          "speaker_id": "2",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010699.1.19",
          "form": "그렇게까지?ㅋㅋㅋㅋ",
          "original_form": "그렇게까지?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:10"
        },
        {
          "id": "MDRW2100010699.1.20",
          "form": "고인겜이 다 그렇지",
          "original_form": "고인겜이 다 그렇지",
          "speaker_id": "2",
          "time": "20210823 21:11"
        },
        {
          "id": "MDRW2100010699.1.21",
          "form": "아 요즘 할 신작겜이 없다",
          "original_form": "아 요즘 할 신작겜이 없다",
