      "utterance": [
        {
          "id": "MDRW2100041146.1.1",
          "form": "미용 따로 관리하시나요?",
          "original_form": "미용 따로 관리하시나요?",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100041146.1.2",
          "form": "좀 관리해야겠다싶어서",
          "original_form": "좀 관리해야겠다싶어서",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041146.1.3",
          "form": "피부과 다니고 있어요",
          "original_form": "피부과 다니고 있어요",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041146.1.4",
          "form": "돈 엄청 깨지네요ㅋㅋㅋ",
          "original_form": "돈 엄청 깨지네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041146.1.5",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "1",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100041146.1.6",
          "form": "시술이요??",
          "original_form": "시술이요??",
          "speaker_id": "1",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100041146.1.7",
          "form": "마스크 쓴 뒤로 피부가 너무 뒤집어져서",
          "original_form": "마스크 쓴 뒤로 피부가 너무 뒤집어져서",
          "speaker_id": "2",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100041146.1.8",
          "form": "치료받고 흉터치료도 하고ㅋㅋㅋ",
          "original_form": "치료받고 흉터치료도 하고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100041146.1.9",
          "form": "ㅜㅠ 아... 효과는 확실하죠! ㅋㅋ",
          "original_form": "ㅜㅠ 아... 효과는 확실하죠! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100041146.1.10",
          "form": "넹 ㅠㅠㅋㅋㅋ 그만가고 싶어요 아파옷",
          "original_form": "넹 ㅠㅠㅋㅋㅋ 그만가고 싶어요 아파옷",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100041146.1.11",
          "form": "뭐 하시는 거 있으신가요",
          "original_form": "뭐 하시는 거 있으신가요",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100041146.1.12",
          "form": "저 어제부터 다시 팩하기 시작했어요!",
          "original_form": "저 어제부터 다시 팩하기 시작했어요!",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100041146.1.13",
          "form": "무슨 팩이용 ㅋㅋㅋ",
          "original_form": "무슨 팩이용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:30"
        },
        {
          "id": "MDRW2100041146.1.14",
          "form": "그 1~4천원짜리들 팩이요!",
          "original_form": "그 1~4천원짜리들 팩이요!",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100041146.1.15",
          "form": "너무 많아서 빨리 써버릴려고요",
          "original_form": "너무 많아서 빨리 써버릴려고요",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100041146.1.16",
          "form": "오 많이 쟁여두셨나봐요",
          "original_form": "오 많이 쟁여두셨나봐요",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100041146.1.17",
          "form": "네...ㅜ 청소하다보니 넘 많더라고요",
          "original_form": "네...ㅜ 청소하다보니 넘 많더라고요",
          "speaker_id": "1",
          "time": "20210905 19:40"
        },
        {
          "id": "MDRW2100041146.1.18",
          "form": "전 팩하면 오히려 뭐가 더 나는 편이라 ㅠㅠ 그냥 피부과에 모든 걸 맡겨요",
          "original_form": "전 팩하면 오히려 뭐가 더 나는 편이라 ㅠㅠ 그냥 피부과에 모든 걸 맡겨요",
          "speaker_id": "2",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100041146.1.19",
          "form": "저도 뽀루지때매 접었었는데ㅋㅋ",
          "original_form": "저도 뽀루지때매 접었었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100041146.1.20",
          "form": "둬서 모하나 싶어서 다시 시도하고있어요ㅋㅋ",
          "original_form": "둬서 모하나 싶어서 다시 시도하고있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100041146.1.21",
          "form": "추천해주실 팩 있으신가용",
          "original_form": "추천해주실 팩 있으신가용",
          "speaker_id": "2",
          "time": "20210905 21:23"
        }
      ]
