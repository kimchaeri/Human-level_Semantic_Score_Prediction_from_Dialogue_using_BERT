      "utterance": [
        {
          "id": "MDRW2100014492.1.1",
          "form": "어떤 계절을 좋아하나요?",
          "original_form": "어떤 계절을 좋아하나요?",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014492.1.2",
          "form": "겨울?",
          "original_form": "겨울?",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014492.1.3",
          "form": "가을?",
          "original_form": "가을?",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014492.1.4",
          "form": "봄?",
          "original_form": "봄?",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014492.1.5",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014492.1.6",
          "form": "여름은 싫어요",
          "original_form": "여름은 싫어요",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014492.1.7",
          "form": "ㅋㅋㅋㅋ그냥 여름빼고 다 좋아하네요",
          "original_form": "ㅋㅋㅋㅋ그냥 여름빼고 다 좋아하네요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014492.1.8",
          "form": "아 그런가봅니다 ㅋㅋㅋ",
          "original_form": "아 그런가봅니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014492.1.9",
          "form": "ㅋㅋㅋ이제 슬슬 추워진다",
          "original_form": "ㅋㅋㅋ이제 슬슬 추워진다",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014492.1.10",
          "form": "밖에겁나습혀",
          "original_form": "밖에겁나습혀",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014492.1.11",
          "form": "날좀화창하면좋겟다 ㅠ ㅋㅋㅋㅋ",
          "original_form": "날좀화창하면좋겟다 ㅠ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014492.1.12",
          "form": "지금도 습해? ㅠㅠ 바람은 이제 차갑다 ㅠㅠ",
          "original_form": "지금도 습해? ㅠㅠ 바람은 이제 차갑다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014492.1.13",
          "form": "진짜 이번주내내 비와서 우울해",
          "original_form": "진짜 이번주내내 비와서 우울해",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014492.1.14",
          "form": "밖에이상해 ㅋㅋㅋ 안엔 에어콘끄면 답답하고..",
          "original_form": "밖에이상해 ㅋㅋㅋ 안엔 에어콘끄면 답답하고..",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014492.1.15",
          "form": "문은 열면 안되겠지? 벌레들 빛보고 들어오겠군 ㅠ",
          "original_form": "문은 열면 안되겠지? 벌레들 빛보고 들어오겠군 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014492.1.16",
          "form": "바닥쓰는사이에 한마리들어옴바로죽엿어 무슨희안한벌레야",
          "original_form": "바닥쓰는사이에 한마리들어옴바로죽엿어 무슨희안한벌레야",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014492.1.17",
          "form": "가을이지지금은?",
          "original_form": "가을이지지금은?",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014492.1.18",
          "form": "그렇지? 근데 왠지 한 번 더 더울거같아 ㅋㅋㅋ",
          "original_form": "그렇지? 근데 왠지 한 번 더 더울거같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:31"
        },
        {
          "id": "MDRW2100014492.1.19",
          "form": "아악 ㅋㅋㅋㅋ 시러 샤벽에도 꿉꿉해서 바람별로안차던데",
          "original_form": "아악 ㅋㅋㅋㅋ 시러 샤벽에도 꿉꿉해서 바람별로안차던데",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014492.1.20",
          "form": "ㅋㅋ지금은 바람이 엄청 차!! 내일은 다를거같아",
          "original_form": "ㅋㅋ지금은 바람이 엄청 차!! 내일은 다를거같아",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014492.1.21",
          "form": "아닌디..뻥쟁이",
          "original_form": "아닌디..뻥쟁이",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014492.1.22",
          "form": "엥?! address이랑 또 다른가봐 ㅋㅋㅋ난 추워서 담요 둘둘",
          "original_form": "엥?! &address&이랑 또 다른가봐 ㅋㅋㅋ난 추워서 담요 둘둘",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014492.1.23",
          "form": "ㅋㅋㅋㅋ여기와서살아야겟다!! 산동날씨 수족관",
          "original_form": "ㅋㅋㅋㅋ여기와서살아야겟다!! 산동날씨 수족관",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014492.1.24",
          "form": "어느계절을 좋아하나요?",
