      "utterance": [
        {
          "id": "MDRW2100035873.1.1",
          "form": "하 진짜... 넘 침고이네요,,, 도미노...",
          "original_form": "하 진짜... 넘 침고이네요,,, 도미노...",
          "speaker_id": "2",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100035873.1.2",
          "form": "어차피 돌아가면서 할인하니까 많이 먹어용ㅋㅋㅋ",
          "original_form": "어차피 돌아가면서 할인하니까 많이 먹어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100035873.1.3",
          "form": "만두는 어디꺼 드세용?",
          "original_form": "만두는 어디꺼 드세용?",
          "speaker_id": "1",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100035873.1.4",
          "form": "저 만두... 그 그 홍 어쩌고 하는 곳인데",
          "original_form": "저 만두... 그 그 홍 어쩌고 하는 곳인데",
          "speaker_id": "2",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100035873.1.5",
          "form": "찾아볼게요 아놔...",
          "original_form": "찾아볼게요 아놔...",
          "speaker_id": "2",
          "time": "20210830 10:02"
        },
        {
          "id": "MDRW2100035873.1.6",
          "form": "홍진경 더 만두??",
          "original_form": "홍진경 더 만두??",
          "speaker_id": "1",
          "time": "20210830 10:02"
        },
        {
          "id": "MDRW2100035873.1.7",
          "form": "궁금해요ㅋㅋㅋ 저랑 입맛이 비슷하신것같아섬",
          "original_form": "궁금해요ㅋㅋㅋ 저랑 입맛이 비슷하신것같아섬",
          "speaker_id": "1",
          "time": "20210830 10:02"
        },
        {
          "id": "MDRW2100035873.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ홍룡수제만두요!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ홍룡수제만두요!!!!",
          "speaker_id": "2",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.9",
          "form": "여기 만두 진짜 맛있어요!!!!",
          "original_form": "여기 만두 진짜 맛있어요!!!!",
          "speaker_id": "2",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.10",
          "form": "피가 얇아서!!!!!!",
          "original_form": "피가 얇아서!!!!!!",
          "speaker_id": "2",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.11",
          "form": "와아 이름부터 찐이네요",
          "original_form": "와아 이름부터 찐이네요",
          "speaker_id": "1",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.12",
          "form": "당장 시켜먹어야겠어용",
          "original_form": "당장 시켜먹어야겠어용",
          "speaker_id": "1",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.13",
          "form": "진짜 맛있더라고요!!!! 와...",
          "original_form": "진짜 맛있더라고요!!!! 와...",
          "speaker_id": "2",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.14",
          "form": "저 만두 진짜 좋아하는데 저기는 그 기본만두들이 진짜 맛있더라고요!",
          "original_form": "저 만두 진짜 좋아하는데 저기는 그 기본만두들이 진짜 맛있더라고요!",
          "speaker_id": "2",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.15",
          "form": "아아 맛있겠다....",
          "original_form": "아아 맛있겠다....",
          "speaker_id": "1",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.16",
          "form": "김치랑 고기랑 반반시켜야겠어여",
          "original_form": "김치랑 고기랑 반반시켜야겠어여",
          "speaker_id": "1",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.17",
          "form": "저는 지금 얇은피만두랑",
          "original_form": "저는 지금 얇은피만두랑",
          "speaker_id": "1",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.18",
          "form": "속이...꽉꽉 차있어서 네! 진짜 맛있어요 ㅎ.ㅎ",
          "original_form": "속이...꽉꽉 차있어서 네! 진짜 맛있어요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210830 10:03"
        },
        {
          "id": "MDRW2100035873.1.19",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100035873.1.20",
          "form": "토박이마을이라고 여기 전병이 맛있는데",
          "original_form": "토박이마을이라고 여기 전병이 맛있는데",
          "speaker_id": "1",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100035873.1.21",
          "form": "이 두곳에서 먹어용",
          "original_form": "이 두곳에서 먹어용",
          "speaker_id": "1",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100035873.1.22",
          "form": "헐.......",
          "original_form": "헐.......",
          "speaker_id": "2",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100035873.1.23",
          "form": "전병 너무 좋아하거든요...",
          "original_form": "전병 너무 좋아하거든요...",
          "speaker_id": "2",
          "time": "20210830 10:04"
        },
        {
          "id": "MDRW2100035873.1.24",
          "form": "토박이 마을이랑..얇은피만두...",
          "original_form": "토박이 마을이랑..얇은피만두...",
          "speaker_id": "2",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100035873.1.25",
          "form": "ㅎㅎ 전병 넘 맛있져ㅠㅠ",
          "original_form": "ㅎㅎ 전병 넘 맛있져ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100035873.1.26",
          "form": "김치는 직접 담궈드세용?",
          "original_form": "김치는 직접 담궈드세용?",
          "speaker_id": "1",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100035873.1.27",
          "form": "메밀김치전병ㅠㅠㅠㅠㅠ와 너무 맛있겠어요 네! 김장철에...담궈먹어요!ㅋㅋㅋㅋ",
          "original_form": "메밀김치전병ㅠㅠㅠㅠㅠ와 너무 맛있겠어요 네! 김장철에...담궈먹어요!ㅋㅋㅋㅋ",
