        {
          "id": "MDRW2100007445.1.2",
          "form": "메타버스 알아?",
          "original_form": "메타버스 알아?",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.3",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.4",
          "form": "당연히알지",
          "original_form": "당연히알지",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.5",
          "form": "약간 퍼피레드같은거임",
          "original_form": "약간 퍼피레드같은거임",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.6",
          "form": "안다고!!!!",
          "original_form": "안다고!!!!",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.7",
          "form": "근데 만약에 만약에",
          "original_form": "근데 만약에 만약에",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.8",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.9",
          "form": "퍼피레드에서 수업듣는다고 하면 엄청 웃길듯",
          "original_form": "퍼피레드에서 수업듣는다고 하면 엄청 웃길듯",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.10",
          "form": "퍼피레드같은데서",
          "original_form": "퍼피레드같은데서",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007445.1.11",
          "form": "근데 동영상을 보는거랑 화상으로하는거랑다르잖아",
          "original_form": "근데 동영상을 보는거랑 화상으로하는거랑다르잖아",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007445.1.12",
          "form": "메타버스는 화상이 아니지",
          "original_form": "메타버스는 화상이 아니지",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007445.1.13",
          "form": "가상현실이지",
          "original_form": "가상현실이지",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.14",
          "form": "그니깐",
          "original_form": "그니깐",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.15",
          "form": "근데 아 그럼",
          "original_form": "근데 아 그럼",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.16",
          "form": "수업이 아니라는건가?",
          "original_form": "수업이 아니라는건가?",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.17",
          "form": "좀 예쁘게 꾸미고 싶잖어",
          "original_form": "좀 예쁘게 꾸미고 싶잖어",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.18",
          "form": "수업하는데 뛰어다니고",
          "original_form": "수업하는데 뛰어다니고",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.19",
          "form": "돈이 너무 많이 들어",
          "original_form": "돈이 너무 많이 들어",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007445.1.20",
          "form": "선생님 제 캐릭터가 누워 있는 침대좀 옮길게요",
          "original_form": "선생님 제 캐릭터가 누워 있는 침대좀 옮길게요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007445.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007445.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007445.1.23",
          "form": "강퇴 각",
          "original_form": "강퇴 각",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007445.1.24",
          "form": "강퇴면 퇴학인가",
          "original_form": "강퇴면 퇴학인가",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007445.1.25",
          "form": "막 거기서 꽃비내려",
          "original_form": "막 거기서 꽃비내려",
          "speaker_id": "2",
          "time": "20210822 02:12"
