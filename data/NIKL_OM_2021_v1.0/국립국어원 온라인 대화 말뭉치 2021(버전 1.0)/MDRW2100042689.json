      "utterance": [
        {
          "id": "MDRW2100042689.1.1",
          "form": "저희팬클럽 연장됐어여",
          "original_form": "저희팬클럽 연장됐어여",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.2",
          "form": "원래 4월인가 3월이어서",
          "original_form": "원래 4월인가 3월이어서",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.3",
          "form": "8월까지연장됐다가 올해말까지로 ㅇㅇ",
          "original_form": "8월까지연장됐다가 올해말까지로 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.4",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "2",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.5",
          "form": "우린 아예 기간 얘기도 안해주는뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우린 아예 기간 얘기도 안해주는뎈ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.6",
          "form": "아마 콘서트를 못해서....",
          "original_form": "아마 콘서트를 못해서....",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.7",
          "form": "플디 일하네...",
          "original_form": "플디 일하네...",
          "speaker_id": "2",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.8",
          "form": "그런것같은 너낌....",
          "original_form": "그런것같은 너낌....",
          "speaker_id": "1",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.9",
          "form": "올해 안에",
          "original_form": "올해 안에",
          "speaker_id": "2",
          "time": "20210907 01:29"
        },
        {
          "id": "MDRW2100042689.1.10",
          "form": "콘근데언제할거니...",
          "original_form": "콘근데언제할거니...",
          "speaker_id": "1",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042689.1.11",
          "form": "온라인콘서트라도 해주라",
          "original_form": "온라인콘서트라도 해주라",
          "speaker_id": "2",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042689.1.12",
          "form": "우리 콘",
          "original_form": "우리 콘",
          "speaker_id": "1",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042689.1.13",
          "form": "마지막콘이 19년4월인가",
          "original_form": "마지막콘이 19년4월인가",
          "speaker_id": "1",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042689.1.14",
          "form": "전전생의 일이네....",
          "original_form": "전전생의 일이네....",
          "speaker_id": "2",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042689.1.15",
          "form": "오....민현이합류하고 처음이자 마지막 콘을한것인가....^^",
          "original_form": "오....민현이합류하고 처음이자 마지막 콘을한것인가....^^",
          "speaker_id": "1",
          "time": "20210907 01:30"
        },
        {
          "id": "MDRW2100042689.1.16",
          "form": "그러고 팬미팅하고 온팬하고가 끝이었으니",
          "original_form": "그러고 팬미팅하고 온팬하고가 끝이었으니",
          "speaker_id": "1",
          "time": "20210907 01:31"
        },
        {
          "id": "MDRW2100042689.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 01:31"
        },
        {
          "id": "MDRW2100042689.1.18",
          "form": "완전체로 한 콘서트가",
          "original_form": "완전체로 한 콘서트가",
          "speaker_id": "2",
          "time": "20210907 01:31"
        },
        {
          "id": "MDRW2100042689.1.19",
          "form": "한번밖에 안됐나...?",
          "original_form": "한번밖에 안됐나...?",
          "speaker_id": "2",
          "time": "20210907 01:31"
        },
        {
          "id": "MDRW2100042689.1.20",
          "form": "생각해보니 플디가 되게 일을 안이하게하는구나",
          "original_form": "생각해보니 플디가 되게 일을 안이하게하는구나",
          "speaker_id": "1",
          "time": "20210907 01:31"
        },
        {
          "id": "MDRW2100042689.1.21",
          "form": "남들두번씩은 하는 온콘을 안하다미",
          "original_form": "남들두번씩은 하는 온콘을 안하다미",
          "speaker_id": "1",
          "time": "20210907 01:32"
        },
        {
          "id": "MDRW2100042689.1.22",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042689.1.23",
          "form": "코로나시대 개막후",
          "original_form": "코로나시대 개막후",
          "speaker_id": "1",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042689.1.24",
          "form": "온콘을 한번도 안했을줄은 몰랐네",
          "original_form": "온콘을 한번도 안했을줄은 몰랐네",
          "speaker_id": "2",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042689.1.25",
          "form": "1년8개월동안",
          "original_form": "1년8개월동안",
          "speaker_id": "1",
          "time": "20210907 01:33"
        },
        {
          "id": "MDRW2100042689.1.26",
