      "utterance": [
        {
          "id": "MDRW2100013248.1.1",
          "form": "최근 사신 물건 있으세요?",
          "original_form": "최근 사신 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013248.1.2",
          "form": "전 요즘 머그컵에 관심이 생겨서 ㅋㅋ",
          "original_form": "전 요즘 머그컵에 관심이 생겨서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013248.1.3",
          "form": "생일선물도 다 머그컵으로 받앗네요",
          "original_form": "생일선물도 다 머그컵으로 받앗네요",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100013248.1.4",
          "form": "머그컵 브랜드가 있나요?",
          "original_form": "머그컵 브랜드가 있나요?",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013248.1.5",
          "form": "어제 무슨 이벤트 당첨으로 머그컵 하나 받았어요",
          "original_form": "어제 무슨 이벤트 당첨으로 머그컵 하나 받았어요",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013248.1.6",
          "form": "아뇨아뇨 브랜드라기보다는 그냥 예븐겈ㅋㅋ",
          "original_form": "아뇨아뇨 브랜드라기보다는 그냥 예븐겈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013248.1.7",
          "form": "요즘은 오로라컵에 꽂혀가지고 살펴보는중이에요",
          "original_form": "요즘은 오로라컵에 꽂혀가지고 살펴보는중이에요",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013248.1.8",
          "form": "오로라컵 처음들어봐요",
          "original_form": "오로라컵 처음들어봐요",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013248.1.9",
          "form": "오로라빛나는걸로 코팅한 컵이에요 ㅋㅋ",
          "original_form": "오로라빛나는걸로 코팅한 컵이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013248.1.10",
          "form": "예쁘겠어요 ㅎㅎ",
          "original_form": "예쁘겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013248.1.11",
          "form": "맞아요 이래저래 쓰기 좋아요",
          "original_form": "맞아요 이래저래 쓰기 좋아요",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100013248.1.12",
          "form": "작은것처럼 보이지만 그런 물건이 아기자기한게",
          "original_form": "작은것처럼 보이지만 그런 물건이 아기자기한게",
          "speaker_id": "2",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013248.1.13",
          "form": "일상생활 평화로운 기분유지에 도움되더라구요",
          "original_form": "일상생활 평화로운 기분유지에 도움되더라구요",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013248.1.14",
          "form": "맞아요!!! 소확행이 이런걸까요 ㅎㅎ",
          "original_form": "맞아요!!! 소확행이 이런걸까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013248.1.15",
          "form": "name2은 최근에 뭐사셨어요?",
          "original_form": "&name2&은 최근에 뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013248.1.16",
          "form": "좀 돈나가는거는 액정타블렛이고요",
          "original_form": "좀 돈나가는거는 액정타블렛이고요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100013248.1.17",
          "form": "작은거는 일러스트레이터 책이요 ㅎㅎ",
          "original_form": "작은거는 일러스트레이터 책이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100013248.1.18",
          "form": "와우 그림좋아하시나봐요",
          "original_form": "와우 그림좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100013248.1.19",
          "form": "유튜브하는데요 하다보니 제 썸네일이랑 채널아트랑",
          "original_form": "유튜브하는데요 하다보니 제 썸네일이랑 채널아트랑",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013248.1.20",
          "form": "그려보고싶어지더라구요 좋아하진 않는데 배워보려고해요",
          "original_form": "그려보고싶어지더라구요 좋아하진 않는데 배워보려고해요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013248.1.21",
          "form": "아하 유튜브하면 이것저것 그릴게많죠",
          "original_form": "아하 유튜브하면 이것저것 그릴게많죠",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013248.1.22",
          "form": "유튜브는 어떤 컨셉이에요?",
          "original_form": "유튜브는 어떤 컨셉이에요?",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100013248.1.23",
          "form": "그냥 제 경험 썰풀고 아니면 브이로그 가끔올려요",
          "original_form": "그냥 제 경험 썰풀고 아니면 브이로그 가끔올려요",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013248.1.24",
          "form": "와.. 유튭 편집 잘하시나봐요",
          "original_form": "와.. 유튭 편집 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100013248.1.25",
          "form": "잘하진않구요 말하는거 카메라 하나로찍어서",
          "original_form": "잘하진않구요 말하는거 카메라 하나로찍어서",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100013248.1.26",
          "form": "자막다는거는 별로 안어려워서요",
          "original_form": "자막다는거는 별로 안어려워서요",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100013248.1.27",
          "form": "아 글고 자동자막 지금 나왔어요 프리미어에요",
          "original_form": "아 글고 자동자막 지금 나왔어요 프리미어에요",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100013248.1.28",
          "form": "헐 프리미어쓰면 자막 자동으로 달려요?",
          "original_form": "헐 프리미어쓰면 자막 자동으로 달려요?",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100013248.1.29",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100013248.1.30",
          "form": "네 브류라고 프로그램있긴 했는데",
          "original_form": "네 브류라고 프로그램있긴 했는데",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100013248.1.31",
          "form": "프리미어 안 기능으로 며칠전에 업뎃됐어요",
          "original_form": "프리미어 안 기능으로 며칠전에 업뎃됐어요",
          "speaker_id": "2",
          "time": "20210825 18:34"
        }
      ]
