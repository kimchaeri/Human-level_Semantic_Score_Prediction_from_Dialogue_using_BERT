      "utterance": [
        {
          "id": "MDRW2100004559.1.1",
          "form": "밥먹었니?",
          "original_form": "밥먹었니?",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004559.1.2",
          "form": "응 일반식 먹었어",
          "original_form": "응 일반식 먹었어",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004559.1.3",
          "form": "무슨 일반식 먹었어??",
          "original_form": "무슨 일반식 먹었어??",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004559.1.4",
          "form": "건강에 좋다는 음식 검색해서 먹었어",
          "original_form": "건강에 좋다는 음식 검색해서 먹었어",
          "speaker_id": "2",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004559.1.5",
          "form": "건강에 좋은 음식이 뭐있어?",
          "original_form": "건강에 좋은 음식이 뭐있어?",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004559.1.6",
          "form": "초록채소가 건강에 좋다고 하더라",
          "original_form": "초록채소가 건강에 좋다고 하더라",
          "speaker_id": "2",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004559.1.7",
          "form": "나도 건강음식 찾아봐야겟네",
          "original_form": "나도 건강음식 찾아봐야겟네",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004559.1.8",
          "form": "요즘 건강이 많이 안좋아졌어",
          "original_form": "요즘 건강이 많이 안좋아졌어",
          "speaker_id": "1",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004559.1.9",
          "form": "이 시국이니까 건강 잘 챙겨야지",
          "original_form": "이 시국이니까 건강 잘 챙겨야지",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004559.1.10",
          "form": "건강에 좋은 운동도 찾아봐야겠다",
          "original_form": "건강에 좋은 운동도 찾아봐야겠다",
          "speaker_id": "1",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004559.1.11",
          "form": "건강하면 체력도 좋아지고 미용에 도움도 될거야",
          "original_form": "건강하면 체력도 좋아지고 미용에 도움도 될거야",
          "speaker_id": "2",
          "time": "20210821 17:13"
        },
        {
          "id": "MDRW2100004559.1.12",
          "form": "운동과 건강에 좋은 음식까지 챙겨먹으면 완벽하겠지?",
          "original_form": "운동과 건강에 좋은 음식까지 챙겨먹으면 완벽하겠지?",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004559.1.13",
          "form": "미용도 되고 건강해지겠지",
          "original_form": "미용도 되고 건강해지겠지",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004559.1.14",
          "form": "다이어트가 필요해",
          "original_form": "다이어트가 필요해",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004559.1.15",
          "form": "집에만 있었더니 살도찌고 건강도 안 좋아진거같아",
          "original_form": "집에만 있었더니 살도찌고 건강도 안 좋아진거같아",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004559.1.16",
          "form": "미용 나도 공감해",
          "original_form": "미용 나도 공감해",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004559.1.17",
          "form": "나이드니깐 피부도 엉망이아",
          "original_form": "나이드니깐 피부도 엉망이아",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004559.1.18",
          "form": "언니도 건강관리해야돼",
          "original_form": "언니도 건강관리해야돼",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004559.1.19",
          "form": "지금부터라도 관리해야겠다",
          "original_form": "지금부터라도 관리해야겠다",
          "speaker_id": "1",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004559.1.20",
          "form": "이제 할 나이야",
          "original_form": "이제 할 나이야",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004559.1.21",
          "form": "건강식 잘 챙겨먹자",
          "original_form": "건강식 잘 챙겨먹자",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004559.1.22",
          "form": "응응 운동도 꾸준히할게",
          "original_form": "응응 운동도 꾸준히할게",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004559.1.23",
          "form": "운동하면 살도 빠지고 이뻐질거야",
          "original_form": "운동하면 살도 빠지고 이뻐질거야",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004559.1.24",
          "form": "살빼고 젊어지고십구나",
          "original_form": "살빼고 젊어지고십구나",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004559.1.25",
          "form": "오늘부터 장봐서 건강식 챙겨먹어야겠다",
          "original_form": "오늘부터 장봐서 건강식 챙겨먹어야겠다",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004559.1.26",
          "form": "야채를 많이 먹어야지",
          "original_form": "야채를 많이 먹어야지",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004559.1.27",
          "form": "그래 나도 차려줘",
          "original_form": "그래 나도 차려줘",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004559.1.28",
          "form": "나도 앉아 있기만해서 변비걸렸어",
          "original_form": "나도 앉아 있기만해서 변비걸렸어",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004559.1.29",
          "form": "건강아 안 좋아진거같아",
          "original_form": "건강아 안 좋아진거같아",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004559.1.30",
          "form": "집에 놀러와",
          "original_form": "집에 놀러와",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004559.1.31",
          "form": "그래 갈겡",
          "original_form": "그래 갈겡",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004559.1.32",
          "form": "변비에 좋은  건강차 마시고 맛있는 저녁먹자",
          "original_form": "변비에 좋은  건강차 마시고 맛있는 저녁먹자",
          "speaker_id": "1",
          "time": "20210821 17:18"
        }
      ]
    }
