      "utterance": [
        {
          "id": "MDRW2100038078.1.1",
          "form": "너 혹시 북트롤리 좋은거 아니m",
          "original_form": "너 혹시 북트롤리 좋은거 아니m",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.2",
          "form": "이제 나도 그친구의 행방이 궁금해졌어",
          "original_form": "이제 나도 그친구의 행방이 궁금해졌어",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.3",
          "form": "어휴 말도마.",
          "original_form": "어휴 말도마.",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.4",
          "form": "왜 새로운걸 구하는거야?",
          "original_form": "왜 새로운걸 구하는거야?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.5",
          "form": "요즘 코로나라서 좀 상황이 안좋게됐나봐.",
          "original_form": "요즘 코로나라서 좀 상황이 안좋게됐나봐.",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.6",
          "form": "거기서 규매하지못하게됐어",
          "original_form": "거기서 규매하지못하게됐어",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.7",
          "form": "중국에서 이동을 못한대",
          "original_form": "중국에서 이동을 못한대",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.8",
          "form": "사람뿐만 아니라 물건들도 고생이구나",
          "original_form": "사람뿐만 아니라 물건들도 고생이구나",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.9",
          "form": "난 12일이나 기다렸는데",
          "original_form": "난 12일이나 기다렸는데",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.11",
          "form": "그럼 좀 비싸게 주고 사야하는거야?",
          "original_form": "그럼 좀 비싸게 주고 사야하는거야?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.12",
          "form": "근데 난 12일이나 기다렸어.",
          "original_form": "근데 난 12일이나 기다렸어.",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.13",
          "form": "응 한국제품은 굉장히 비싸",
          "original_form": "응 한국제품은 굉장히 비싸",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.14",
          "form": "최소 2만유ㅓㄴ이 비싸",
          "original_form": "최소 2만유ㅓㄴ이 비싸",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.15",
          "form": "거의 2주를 기다렸구나",
          "original_form": "거의 2주를 기다렸구나",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100038078.1.16",
          "form": "그마저도 맘에드는건 품절이옸어",
          "original_form": "그마저도 맘에드는건 품절이옸어",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.17",
          "form": "그럼 다음 할인을 노려보자",
          "original_form": "그럼 다음 할인을 노려보자",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.18",
          "form": "그리고 재입고알람도하고",
          "original_form": "그리고 재입고알람도하고",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.19",
          "form": "그럼 ㅋㅎ로나가 끝나여해ㅠ",
          "original_form": "그럼 ㅋㅎ로나가 끝나여해ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.20",
          "form": "한국건 비싼데 도전할까?",
          "original_form": "한국건 비싼데 도전할까?",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.21",
          "form": "중국은 상품응 많이 만들면서",
          "original_form": "중국은 상품응 많이 만들면서",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.22",
          "form": "응 니가 못기다리겟다면",
          "original_form": "응 니가 못기다리겟다면",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038078.1.23",
          "form": "출고를 안하는구나",
          "original_form": "출고를 안하는구나",
          "speaker_id": "2",
          "time": "20210901 23:00"
        }
      ]
