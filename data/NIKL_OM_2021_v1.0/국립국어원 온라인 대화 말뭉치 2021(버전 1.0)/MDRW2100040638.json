      "utterance": [
        {
          "id": "MDRW2100040638.1.1",
          "form": "허,,",
          "original_form": "허,,",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040638.1.2",
          "form": "진짜 작정하고 봐야해요",
          "original_form": "진짜 작정하고 봐야해요",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040638.1.3",
          "form": "울 수 밖에 없어,...ㅠㅠ",
          "original_form": "울 수 밖에 없어,...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040638.1.4",
          "form": "그니까요 그 우상궁? 죽는 순간부터",
          "original_form": "그니까요 그 우상궁? 죽는 순간부터",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040638.1.5",
          "form": "눈물 안 멈추지 암ㄹ나요",
          "original_form": "눈물 안 멈추지 암ㄹ나요",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040638.1.6",
          "form": "저 사실 아직도 강한나한테 앙금있어서",
          "original_form": "저 사실 아직도 강한나한테 앙금있어서",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040638.1.7",
          "form": "스타트업 못 봤어요",
          "original_form": "스타트업 못 봤어요",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040638.1.8",
          "form": "앜ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "앜ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040638.1.9",
          "form": "마자요 진짜ㅠ저도 절반부터 울었던 거 같아요",
          "original_form": "마자요 진짜ㅠ저도 절반부터 울었던 거 같아요",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040638.1.10",
          "form": "진짜 펑펑 울었어요",
          "original_form": "진짜 펑펑 울었어요",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040638.1.11",
          "form": "그 때부터 아이유가ㅜ너무 급격하게 불행해지는 것 같아서 ㅠㅠ",
          "original_form": "그 때부터 아이유가ㅜ너무 급격하게 불행해지는 것 같아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040638.1.12",
          "form": "전 이준기랑 강한나 결혼하는 순간부터는",
          "original_form": "전 이준기랑 강한나 결혼하는 순간부터는",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040638.1.13",
          "form": "그냥 화면 란 보고 소리만 들었어요",
          "original_form": "그냥 화면 란 보고 소리만 들었어요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040638.1.14",
          "form": "진짜 배신감오졌죠",
          "original_form": "진짜 배신감오졌죠",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100040638.1.15",
          "form": "갑자기 너무 슬퍼요 어떻게 그렇게 비참하게 만드는지ㅠㅠㅠㅠㅠㅠ",
          "original_form": "갑자기 너무 슬퍼요 어떻게 그렇게 비참하게 만드는지ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:42"
        },
        {
          "id": "MDRW2100040638.1.16",
          "form": "그니까요 앞부분에 ㅋ 생일축하 노래ㅜ버르는 장면..",
          "original_form": "그니까요 앞부분에 ㅋ 생일축하 노래ㅜ버르는 장면..",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100040638.1.17",
          "form": "홍종현이랑 백현 친하게 지냐게 했으면서",
          "original_form": "홍종현이랑 백현 친하게 지냐게 했으면서",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100040638.1.18",
          "form": "작가..... 이럴 수 있냐고요",
          "original_form": "작가..... 이럴 수 있냐고요",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100040638.1.19",
          "form": "진짜ㅠㅠㅠㅠㅠㅠ에바예요 진짜",
          "original_form": "진짜ㅠㅠㅠㅠㅠㅠ에바예요 진짜",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040638.1.20",
          "form": "작가가 선을 넘었어요 진짜 ㅠㅠㅠㅠ",
          "original_form": "작가가 선을 넘었어요 진짜 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040638.1.21",
          "form": "그니까요... 백현 죽는 장면..",
          "original_form": "그니까요... 백현 죽는 장면..",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040638.1.22",
          "form": "진짜 제일 잔인하지 않나요",
          "original_form": "진짜 제일 잔인하지 않나요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100040638.1.23",
          "form": "제일 잔인해요",
          "original_form": "제일 잔인해요",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040638.1.24",
          "form": "몇 번을 돌려봤는지 몰라요,,,,",
          "original_form": "몇 번을 돌려봤는지 몰라요,,,,",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040638.1.25",
          "form": "화살 진짜 내가 막아주고 싶은 심정",
          "original_form": "화살 진짜 내가 막아주고 싶은 심정",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040638.1.26",
          "form": "...ㅠㅠ 그니까요 전 그거랑 서현 쥭을 때가",
          "original_form": "...ㅠㅠ 그니까요 전 그거랑 서현 쥭을 때가",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040638.1.27",
          "form": "제일 가금 찢어졌어요",
          "original_form": "제일 가금 찢어졌어요",
          "speaker_id": "1",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100040638.1.28",
          "form": "아,,,, 진짜 잔인해",
          "original_form": "아,,,, 진짜 잔인해",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100040638.1.29",
          "form": "명장면이라고 꼽히는게 다 슬퍼요",
          "original_form": "명장면이라고 꼽히는게 다 슬퍼요",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040638.1.30",
          "form": "ost 도 너무 좋은데 들으면 눈물나고",
          "original_form": "ost 도 너무 좋은데 들으면 눈물나고",
          "speaker_id": "2",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100040638.1.31",
          "form": "저 그래서 원래는",
          "original_form": "저 그래서 원래는",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040638.1.32",
          "form": "드라마 ost 꼭 다시 ㄷ드는데",
          "original_form": "드라마 ost 꼭 다시 ㄷ드는데",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040638.1.33",
          "form": "보보경심은 죽어도 안들어요",
          "original_form": "보보경심은 죽어도 안들어요",
          "speaker_id": "1",
          "time": "20210905 00:11"
        },
        {
          "id": "MDRW2100040638.1.34",
          "form": "듣지마세요",
          "original_form": "듣지마세요",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040638.1.35",
          "form": "서사가 다 생각나서",
          "original_form": "서사가 다 생각나서",
          "speaker_id": "2",
          "time": "20210905 00:12"
        },
        {
          "id": "MDRW2100040638.1.36",
          "form": "ost 만 들어도 눈물샘 폭발 ㅠㅠ",
          "original_form": "ost 만 들어도 눈물샘 폭발 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100040638.1.37",
          "form": "흑 보보경심 중 어떤 황자 제일 좋아하셨어요!?",
          "original_form": "흑 보보경심 중 어떤 황자 제일 좋아하셨어요!?",
          "speaker_id": "1",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100040638.1.38",
          "form": "전.... 초반엔 강하늘.... 마지먹에는 남주혁..",
          "original_form": "전.... 초반엔 강하늘.... 마지먹에는 남주혁..",
