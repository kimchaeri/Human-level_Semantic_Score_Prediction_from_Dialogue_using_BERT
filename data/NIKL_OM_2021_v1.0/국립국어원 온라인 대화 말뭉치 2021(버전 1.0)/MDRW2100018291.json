      "utterance": [
        {
          "id": "MDRW2100018291.1.1",
          "form": "헉 네 넘 좋아해요!!",
          "original_form": "헉 네 넘 좋아해요!!",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018291.1.2",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018291.1.3",
          "form": "저두요 ㅎㅎ",
          "original_form": "저두요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018291.1.4",
          "form": "게장 못먹는 사람도 많죠 ㅋㅋ",
          "original_form": "게장 못먹는 사람도 많죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.5",
          "form": "네 요즘 간장게장",
          "original_form": "네 요즘 간장게장",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.6",
          "form": "너무 먹고싶어서..ㅎ",
          "original_form": "너무 먹고싶어서..ㅎ",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.7",
          "form": "저도 어제 친구들이랑",
          "original_form": "저도 어제 친구들이랑",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.8",
          "form": "간장게장 맛집 얘기했어요",
          "original_form": "간장게장 맛집 얘기했어요",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.9",
          "form": "오 어딘가요?",
          "original_form": "오 어딘가요?",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.10",
          "form": "그 장안동에",
          "original_form": "그 장안동에",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.11",
          "form": "일미간장게장?",
          "original_form": "일미간장게장?",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.12",
          "form": "그렇대요",
          "original_form": "그렇대요",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018291.1.13",
          "form": "어 저 가봤어요",
          "original_form": "어 저 가봤어요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.14",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.15",
          "form": "어때요??",
          "original_form": "어때요??",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.16",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.17",
          "form": "안 짜고 ㅎㅎ",
          "original_form": "안 짜고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.18",
          "form": "오 안 짜요??",
          "original_form": "오 안 짜요??",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.19",
          "form": "네 많이는 안 짜더라구요",
          "original_form": "네 많이는 안 짜더라구요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.20",
          "form": "얼마정도에요?",
          "original_form": "얼마정도에요?",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.21",
          "form": "저 그때",
          "original_form": "저 그때",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.22",
          "form": "둘이서 75000원?",
          "original_form": "둘이서 75000원?",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.23",
          "form": "헉 비싸네요...ㅠ",
          "original_form": "헉 비싸네요...ㅠ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018291.1.24",
          "form": "비싸긴하죠 ㅎㅎ",
          "original_form": "비싸긴하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.25",
          "form": "재방문 의사 있으세요?",
          "original_form": "재방문 의사 있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.26",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.27",
          "form": "비싸긴 한데",
          "original_form": "비싸긴 한데",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.28",
          "form": "맛은 있어서..",
          "original_form": "맛은 있어서..",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.29",
          "form": "몇년에 한번?",
          "original_form": "몇년에 한번?",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.30",
          "form": "오 저도 가고싶네요 ㅠㅍ",
          "original_form": "오 저도 가고싶네요 ㅠㅍ",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018291.1.31",
          "form": "게장 말하니까 배고프네요 ㅠㅍ",
          "original_form": "게장 말하니까 배고프네요 ㅠㅍ",
          "speaker_id": "2",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018291.1.32",
          "form": "양념게장은요?",
          "original_form": "양념게장은요?",
          "speaker_id": "2",
          "time": "20210826 23:42"
        }
      ]
    }
  ]
