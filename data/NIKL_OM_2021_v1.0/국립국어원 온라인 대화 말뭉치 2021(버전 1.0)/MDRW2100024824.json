      "utterance": [
        {
          "id": "MDRW2100024824.1.1",
          "form": "name2는 싸이월드 했었어?!",
          "original_form": "&name2&는 싸이월드 했었어?!",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024824.1.2",
          "form": "안했다면 패스패스!",
          "original_form": "안했다면 패스패스!",
          "speaker_id": "1",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024824.1.3",
          "form": "안했었오 ㅋㅋㅋㅋ",
          "original_form": "안했었오 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024824.1.4",
          "form": "그래도 얘기해보자",
          "original_form": "그래도 얘기해보자",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024824.1.5",
          "form": "싸이월드 부활되면 좋을것 같아?",
          "original_form": "싸이월드 부활되면 좋을것 같아?",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024824.1.6",
          "form": "아 역시 안했었구나!!!",
          "original_form": "아 역시 안했었구나!!!",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100024824.1.7",
          "form": "나는 이제 블로그를 해서 큰 상관은 없는데",
          "original_form": "나는 이제 블로그를 해서 큰 상관은 없는데",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100024824.1.8",
          "form": "그리고 나는 싸이월드 폐쇄되기 전에",
          "original_form": "그리고 나는 싸이월드 폐쇄되기 전에",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100024824.1.9",
          "form": "다 지워버렸지 뭐야 ",
          "original_form": "다 지워버렸지 뭐야 ",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100024824.1.10",
          "form": "ㅋㅋㅋㅋㅋ왜 지웠어?",
          "original_form": "ㅋㅋㅋㅋㅋ왜 지웠어?",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100024824.1.11",
          "form": "내 흑역사가 가득해서 말이야~~~",
          "original_form": "내 흑역사가 가득해서 말이야~~~",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100024824.1.12",
          "form": "감수성이 예민했나봐,,,",
          "original_form": "감수성이 예민했나봐,,,",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100024824.1.13",
          "form": "음악이랑 스킨도 많이 사놨는데 ㅎㅎㅎ",
          "original_form": "음악이랑 스킨도 많이 사놨는데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100024824.1.14",
          "form": "ㅋㅋㅋㅋ흑역사 궁금하다",
          "original_form": "ㅋㅋㅋㅋ흑역사 궁금하다",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100024824.1.15",
          "form": "싸이 감성 궁금해 ㅋㅋㅋ",
          "original_form": "싸이 감성 궁금해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100024824.1.16",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100024824.1.17",
          "form": "작은 고민도 큰 것처럼 느끼고",
          "original_form": "작은 고민도 큰 것처럼 느끼고",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100024824.1.18",
          "form": "음악은,,, 남들이 잘 모르지만 들으면 짱좋은 걸 깔아놔야하고",
          "original_form": "음악은,,, 남들이 잘 모르지만 들으면 짱좋은 걸 깔아놔야하고",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100024824.1.19",
          "form": "그런게 있었어 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런게 있었어 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100024824.1.20",
          "form": "싸이 부활하면 인스타에서 넘어갈 사람들이 있을까?",
          "original_form": "싸이 부활하면 인스타에서 넘어갈 사람들이 있을까?",
          "speaker_id": "2",
          "time": "20210901 00:00"
        },
        {
          "id": "MDRW2100024824.1.21",
          "form": "어쩌면 또 신세대들이 사용하게 될지도....??",
          "original_form": "어쩌면 또 신세대들이 사용하게 될지도....??",
          "speaker_id": "2",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100024824.1.22",
          "form": "왠지 그럴거같아!!!",
          "original_form": "왠지 그럴거같아!!!",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100024824.1.23",
          "form": "요즘은 블로그 다시 하는 사람들이 많아진 것처럼",
          "original_form": "요즘은 블로그 다시 하는 사람들이 많아진 것처럼",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100024824.1.24",
          "form": "이제 사진 + 텍스트 모두",
          "original_form": "이제 사진 + 텍스트 모두",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100024824.1.25",
          "form": "중요시하는 분위기가 되는 듯?!",
          "original_form": "중요시하는 분위기가 되는 듯?!",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100024824.1.26",
          "form": "ㅎㅎ맞아맞아",
          "original_form": "ㅎㅎ맞아맞아",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100024824.1.27",
          "form": "사진만 있으면 스토리가 없고",
          "original_form": "사진만 있으면 스토리가 없고",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100024824.1.28",
          "form": "글만있음 재미가 없는느낌",
          "original_form": "글만있음 재미가 없는느낌",
          "speaker_id": "2",
          "time": "20210901 15:01"
        },
        {
          "id": "MDRW2100024824.1.29",
          "form": "맞아맞아 !",
          "original_form": "맞아맞아 !",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100024824.1.30",
          "form": "역시 무엇이든 적절한 게 중요한가봐~!",
          "original_form": "역시 무엇이든 적절한 게 중요한가봐~!",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100024824.1.31",
          "form": "싸이월드 부활하면 블로그랑 비슷해지지 않을까 싶어 ㅎㅎ",
          "original_form": "싸이월드 부활하면 블로그랑 비슷해지지 않을까 싶어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100024824.1.32",
          "form": "그렇겠다아",
          "original_form": "그렇겠다아",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100024824.1.33",
          "form": "궁금하네 얼마나 이용할지 사람들이",
          "original_form": "궁금하네 얼마나 이용할지 사람들이",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100024824.1.34",
          "form": "그러게 말이야,,, 광고수익도 있고 그래야",
          "original_form": "그러게 말이야,,, 광고수익도 있고 그래야",
          "speaker_id": "1",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100024824.1.35",
          "form": "사람들이 할거같은데!!!",
          "original_form": "사람들이 할거같은데!!!",
          "speaker_id": "1",
          "time": "20210901 16:07"
        }
      ]
