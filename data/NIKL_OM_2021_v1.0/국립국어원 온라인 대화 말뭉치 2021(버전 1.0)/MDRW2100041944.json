      "utterance": [
        {
          "id": "MDRW2100041944.1.1",
          "form": "요새 즐겨 들으시는 노래나 영화 있으신가요?",
          "original_form": "요새 즐겨 들으시는 노래나 영화 있으신가요?",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041944.1.2",
          "form": "밥잘사주는예쁜누나랑\r\n봄밤 둘다 봐야지 봐야지한게\r\n몇 년이 지났네요 ㅋㅋ",
          "original_form": "밥잘사주는예쁜누나랑\r\n봄밤 둘다 봐야지 봐야지한게\r\n몇 년이 지났네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041944.1.3",
          "form": "아 저도 그거 두개는 안봣어요 ㅋㅋㅋㅋ",
          "original_form": "아 저도 그거 두개는 안봣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041944.1.4",
          "form": "근데 영화",
          "original_form": "근데 영화",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041944.1.5",
          "form": "김고은이랑 정해인 나온거 잇거든요 그건 봣어요",
          "original_form": "김고은이랑 정해인 나온거 잇거든요 그건 봣어요",
          "speaker_id": "2",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041944.1.6",
          "form": "유열의 음악앨범?",
          "original_form": "유열의 음악앨범?",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041944.1.7",
          "form": "아아 맞아요!!!",
          "original_form": "아아 맞아요!!!",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041944.1.8",
          "form": "제목이 기억안났었는데~~~~",
          "original_form": "제목이 기억안났었는데~~~~",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041944.1.9",
          "form": "그거에여 ㅎㅎ",
          "original_form": "그거에여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041944.1.10",
          "form": "저는 그시절 학생은 아니지만",
          "original_form": "저는 그시절 학생은 아니지만",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041944.1.11",
          "form": "추억에 잠기더라구요",
          "original_form": "추억에 잠기더라구요",
          "speaker_id": "2",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100041944.1.12",
          "form": "오 저도 봐야겠어영:)",
          "original_form": "오 저도 봐야겠어영:)",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041944.1.13",
          "form": "추천해요!!",
          "original_form": "추천해요!!",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041944.1.14",
          "form": "전 그런 달달한 영화 좋더라구요",
          "original_form": "전 그런 달달한 영화 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041944.1.15",
          "form": "정해인님 작품활동한거 찾아봤는데\r\n관심있어했던것들 많은데\r\n어쩜 이리 하나도 안본건지ㅋ",
          "original_form": "정해인님 작품활동한거 찾아봤는데\r\n관심있어했던것들 많은데\r\n어쩜 이리 하나도 안본건지ㅋ",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041944.1.16",
          "form": "저랑 비슷하시네요!!ㅎㅎㅎ",
          "original_form": "저랑 비슷하시네요!!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041944.1.17",
          "form": "근데 유열의 음악앨범에서도 약간 싸움도 하고",
          "original_form": "근데 유열의 음악앨범에서도 약간 싸움도 하고",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041944.1.18",
          "form": "그런 학생이엇는데 얼굴이 너무 곱상해서 좀 안어울리긴햇어요 ㅎㅎㅎ",
          "original_form": "그런 학생이엇는데 얼굴이 너무 곱상해서 좀 안어울리긴햇어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041944.1.19",
          "form": "봄밤이랑 정말 재밋다고 하더라두요",
          "original_form": "봄밤이랑 정말 재밋다고 하더라두요",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041944.1.20",
          "form": "봄밤\r\n한지민님 너무 좋아해서\r\n꼭 보려했었는데ㅋㅋㅋㅋ",
          "original_form": "봄밤{emoji:♡}\r\n한지민님 너무 좋아해서\r\n꼭 보려했었는데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041944.1.21",
          "form": "부자님이랑 대화하면서\r\n볼 것들 많이 늘어나서 좋네요",
          "original_form": "부자님이랑 대화하면서\r\n볼 것들 많이 늘어나서 좋네요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041944.1.22",
          "form": "가을밤이 더 풍성해지겠어여",
          "original_form": "가을밤이 더 풍성해지겠어여",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041944.1.23",
          "form": "오 그렇게 말씀해주시니 넘 감사해용>",
          "original_form": "오 그렇게 말씀해주시니 넘 감사해용{emoji:♡}>",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041944.1.24",
