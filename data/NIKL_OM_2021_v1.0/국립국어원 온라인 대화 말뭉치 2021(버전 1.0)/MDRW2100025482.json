        {
          "id": "MDRW2100025482.1.1",
          "form": "영화좋아하시나요",
          "original_form": "영화좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025482.1.2",
          "form": "영화 짱 좋아해요",
          "original_form": "영화 짱 좋아해요",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025482.1.3",
          "form": "최근 본 영화관람 뭐 있으세요",
          "original_form": "최근 본 영화관람 뭐 있으세요",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025482.1.4",
          "form": "저는 씽크홀을 잼게봤어요",
          "original_form": "저는 씽크홀을 잼게봤어요",
          "speaker_id": "1",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025482.1.5",
          "form": "오 그거 인기 많던데요",
          "original_form": "오 그거 인기 많던데요",
          "speaker_id": "2",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025482.1.6",
          "form": "남 배우들이 웃기더라구요",
          "original_form": "남 배우들이 웃기더라구요",
          "speaker_id": "2",
          "time": "20210901 11:59"
        },
        {
          "id": "MDRW2100025482.1.7",
          "form": "저는 주말에 공포영화보러가요",
          "original_form": "저는 주말에 공포영화보러가요",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025482.1.8",
          "form": "헐 공포영화...",
          "original_form": "헐 공포영화...",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025482.1.9",
          "form": "저는 좋아하는데 잘못봐요 ㅠㅠ",
          "original_form": "저는 좋아하는데 잘못봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025482.1.10",
          "form": "귀 막고 보기 국룰입니다",
          "original_form": "귀 막고 보기 국룰입니다",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025482.1.11",
          "form": "뮤지컬도 잼던데 너무 비싸요",
          "original_form": "뮤지컬도 잼던데 너무 비싸요",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025482.1.12",
          "form": "헐 뮤지컬 저도 좋아해요!!",
          "original_form": "헐 뮤지컬 저도 좋아해요!!",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025482.1.13",
          "form": "근데 너무 비싸ㅅ",
          "original_form": "근데 너무 비싸ㅅ",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025482.1.14",
          "form": "서",
          "original_form": "서",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025482.1.15",
          "form": "연극을 보는편이에요",
          "original_form": "연극을 보는편이에요",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025482.1.16",
          "form": "오 연극도 엄청 옛날에 한번 봤는데 잼더라구요",
          "original_form": "오 연극도 엄청 옛날에 한번 봤는데 잼더라구요",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025482.1.17",
          "form": "콘서트도 자주 갔었는데 요즘 못가니 아쉽네요",
          "original_form": "콘서트도 자주 갔었는데 요즘 못가니 아쉽네요",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025482.1.18",
          "form": "멎어요 ㅠㅠ 누구 좋아하세여?",
          "original_form": "멎어요 ㅠㅠ 누구 좋아하세여?",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025482.1.19",
          "form": "저는 세븐틴 콘서트 가봤어요",
          "original_form": "저는 세븐틴 콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025482.1.20",
          "form": "세븐틴 노래 넘 신나요",
          "original_form": "세븐틴 노래 넘 신나요",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025482.1.21",
          "form": "저는 지오디 좋아했는데 요즘은 중국가수 좋아해요 ㅋㅋ",
          "original_form": "저는 지오디 좋아했는데 요즘은 중국가수 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025482.1.22",
          "form": "지오디!!",
          "original_form": "지오디!!",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025482.1.23",
          "form": "지오디에 김태우 바께몰라오..",
          "original_form": "지오디에 김태우 바께몰라오..",
          "speaker_id": "1",
