      "utterance": [
        {
          "id": "MDRW2100009889.1.1",
          "form": "점심 뭐드실거에요?",
          "original_form": "점심 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009889.1.2",
          "form": "점심은 햄버거네요 ㅋㅋㅋ",
          "original_form": "점심은 햄버거네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009889.1.3",
          "form": "회사 옆에 햄버거집이라ㅠ",
          "original_form": "회사 옆에 햄버거집이라ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009889.1.4",
          "form": "비도오고 선택의 여지가 없네여 ㅋㅋ",
          "original_form": "비도오고 선택의 여지가 없네여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009889.1.5",
          "form": "와~간편하니 맛나겠어요~!! 어디 햄버거 인가요?",
          "original_form": "와~간편하니 맛나겠어요~!! 어디 햄버거 인가요?",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009889.1.6",
          "form": "햄버거도 좋죠~ㅎㅎ",
          "original_form": "햄버거도 좋죠~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:43"
        },
        {
          "id": "MDRW2100009889.1.7",
          "form": "점심 메뉴 고르셨어요,??",
          "original_form": "점심 메뉴 고르셨어요,??",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009889.1.8",
          "form": "저는 치즈돈까스 먹을 것 같아요",
          "original_form": "저는 치즈돈까스 먹을 것 같아요",
          "speaker_id": "1",
          "time": "20210823 11:46"
        },
        {
          "id": "MDRW2100009889.1.9",
          "form": "맛있겠어요^^ 저녁은 치돈으로 해야겠네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛있겠어요^^ 저녁은 치돈으로 해야겠네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009889.1.10",
          "form": "오~!저녁에는 치돈~! 좋네요~!ㅎㅎ",
          "original_form": "오~!저녁에는 치돈~! 좋네요~!ㅎㅎ",
