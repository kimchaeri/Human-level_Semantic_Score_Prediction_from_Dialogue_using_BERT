      "utterance": [
        {
          "id": "MDRW2100029876.1.1",
          "form": "네ㅎㅎ 총 4명이서 살고 있어요",
          "original_form": "네ㅎㅎ 총 4명이서 살고 있어요",
          "speaker_id": "2",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029876.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029876.1.3",
          "form": "저도 4인가족이에요ㅎㅎ",
          "original_form": "저도 4인가족이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029876.1.4",
          "form": "4인 가족이 딱 좋은거 같아요",
          "original_form": "4인 가족이 딱 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029876.1.5",
          "form": "음식점에 가서 앉기도 편하고",
          "original_form": "음식점에 가서 앉기도 편하고",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100029876.1.6",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029876.1.7",
          "form": "외식 종종하시나요?",
          "original_form": "외식 종종하시나요?",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100029876.1.8",
          "form": "예전엔 꽤 했는데",
          "original_form": "예전엔 꽤 했는데",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029876.1.9",
          "form": "코로나 이후로 픽업만 하고 있어요",
          "original_form": "코로나 이후로 픽업만 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100029876.1.10",
          "form": "아무래도 그렇겠죠ㅠㅜ",
          "original_form": "아무래도 그렇겠죠ㅠㅜ",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029876.1.11",
          "form": "어떤 음식을 주로 드시나요?",
          "original_form": "어떤 음식을 주로 드시나요?",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029876.1.12",
          "form": "딱 정해진건 없고",
          "original_form": "딱 정해진건 없고",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029876.1.13",
          "form": "밀키트로 나오는거 자주 먹어요",
          "original_form": "밀키트로 나오는거 자주 먹어요",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029876.1.14",
          "form": "밀키트좋죠 나베, 파스타, 스테이크 종류만 먹어봤는데 어떤걸 드셔보셨나요",
          "original_form": "밀키트좋죠 나베, 파스타, 스테이크 종류만 먹어봤는데 어떤걸 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029876.1.15",
          "form": "감바스 먹어봤는데 맛있어요",
          "original_form": "감바스 먹어봤는데 맛있어요",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029876.1.16",
          "form": "감바스도 맛있죠!!",
          "original_form": "감바스도 맛있죠!!",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029876.1.17",
          "form": "밥이랑 같이 먹어도 어울리더라구요",
          "original_form": "밥이랑 같이 먹어도 어울리더라구요",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029876.1.18",
          "form": "파스타도 익혀서 넣으면 맛있을텐데 밀키트 특성상 소스량이 부족할거 같네요ㅠ",
          "original_form": "파스타도 익혀서 넣으면 맛있을텐데 밀키트 특성상 소스량이 부족할거 같네요ㅠ",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029876.1.19",
          "form": "파스타 소량을 넣어주긴 해요",
          "original_form": "파스타 소량을 넣어주긴 해요",
          "speaker_id": "2",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029876.1.20",
          "form": "근데 극소량입니다ㅋㅋ",
          "original_form": "근데 극소량입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029876.1.21",
          "form": "ㅋㅋㅋㅋ없느니보다 못한",
          "original_form": "ㅋㅋㅋㅋ없느니보다 못한",
          "speaker_id": "1",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029876.1.22",
          "form": "애피타이저로 드시면 돼요ㅋㅋ",
          "original_form": "애피타이저로 드시면 돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029876.1.23",
          "form": "ㅋㅋㅋㅋ그것보단 파스타를 따로먹을게요",
          "original_form": "ㅋㅋㅋㅋ그것보단 파스타를 따로먹을게요",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029876.1.24",
          "form": "현명하신 선택입니다ㅋㅋ",
          "original_form": "현명하신 선택입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029876.1.25",
          "form": "코로나이전에 외출도 가족끼리 잘 나가셨나요?",
          "original_form": "코로나이전에 외출도 가족끼리 잘 나가셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029876.1.26",
          "form": "네 나가면 꼭 가족끼리 나갔어요",
          "original_form": "네 나가면 꼭 가족끼리 나갔어요",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029876.1.27",
          "form": "가장 기억에 남은 장소는 어디인가요?",
          "original_form": "가장 기억에 남은 장소는 어디인가요?",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029876.1.28",
          "form": "동해 자주 갔던 기억이 있네요",
          "original_form": "동해 자주 갔던 기억이 있네요",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029876.1.29",
          "form": "동해좋죠ㅎㅎ",
          "original_form": "동해좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029876.1.30",
          "form": "동해가 바다 보기에는 제일 좋은거 같아요",
          "original_form": "동해가 바다 보기에는 제일 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210903 16:40"
        }
      ]
