      "utterance": [
        {
          "id": "MDRW2100040271.1.1",
          "form": "시 라는 와인이요",
          "original_form": "시 라는 와인이요",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.2",
          "form": "그래서 모스카토 나 모스카토 다스티를 자주 사 먹어요",
          "original_form": "그래서 모스카토 나 모스카토 다스티를 자주 사 먹어요",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.3",
          "form": "시라요ㅎㅎ",
          "original_form": "시라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.4",
          "form": "싫어 맛있나요",
          "original_form": "싫어 맛있나요",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.5",
          "form": "쉬라",
          "original_form": "쉬라",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.6",
          "form": "인식을 잘 못 하네요 바보 같은 음성 인식",
          "original_form": "인식을 잘 못 하네요 바보 같은 음성 인식",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.7",
          "form": "디아블로  나",
          "original_form": "디아블로  나",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040271.1.8",
          "form": "저는 빌라엠 좋아합니다",
          "original_form": "저는 빌라엠 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.9",
          "form": "크리사도 시라ㅋㅋ",
          "original_form": "크리사도 시라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.10",
          "form": "비싸서 잘 못 사 먹지요",
          "original_form": "비싸서 잘 못 사 먹지요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.11",
          "form": "스윗 와인은 가끔 마시구",
          "original_form": "스윗 와인은 가끔 마시구",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.12",
          "form": "너무 드라이한 건 또 싫고",
          "original_form": "너무 드라이한 건 또 싫고",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.13",
          "form": "전에 CU에서 만원 이상 와인을 사면 1,000원 할인 쿠폰 주는 행사를 했었는데요",
          "original_form": "전에 CU에서 만원 이상 와인을 사면 1,000원 할인 쿠폰 주는 행사를 했었는데요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.14",
          "form": "중간 맛이 좋아요",
          "original_form": "중간 맛이 좋아요",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.15",
          "form": "좋은 행사 네요",
          "original_form": "좋은 행사 네요",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.16",
          "form": "9900원짜리 샀는데 일 하시는 분이 만원으로 처리해 주셔서 쿠폰 받아서 쓴 적이 있어요",
          "original_form": "9900원짜리 샀는데 일 하시는 분이 만원으로 처리해 주셔서 쿠폰 받아서 쓴 적이 있어요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.17",
          "form": "개이득 이었죠 맛있게 먹었답니다",
          "original_form": "개이득 이었죠 맛있게 먹었답니다",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.18",
          "form": "저는 샹그리아도 좋아해요",
          "original_form": "저는 샹그리아도 좋아해요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.19",
          "form": "훌륭한 네요",
          "original_form": "훌륭한 네요",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.20",
          "form": "샹그리아 너무 좋아요",
          "original_form": "샹그리아 너무 좋아요",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.21",
          "form": "와인에는 어떤 한조가 어울리나요",
          "original_form": "와인에는 어떤 한조가 어울리나요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040271.1.22",
          "form": "와인에는 치즈라고 얘기는 하는데",
          "original_form": "와인에는 치즈라고 얘기는 하는데",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040271.1.23",
          "form": "저는 늦게 해서 별로 에요",
          "original_form": "저는 늦게 해서 별로 에요",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040271.1.24",
          "form": "느끼해서",
          "original_form": "느끼해서",
          "speaker_id": "2",
          "time": "20210904 15:21"
        }
      ]
