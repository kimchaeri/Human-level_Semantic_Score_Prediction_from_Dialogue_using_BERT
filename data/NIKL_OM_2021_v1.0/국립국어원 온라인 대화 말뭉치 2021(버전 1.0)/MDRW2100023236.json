      "utterance": [
        {
          "id": "MDRW2100023236.1.1",
          "form": "다른 거 사서 괜찮아욬ㅋㅋㅋ",
          "original_form": "다른 거 사서 괜찮아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023236.1.2",
          "form": "ㅎㅎㅎ네넹",
          "original_form": "ㅎㅎㅎ네넹",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100023236.1.3",
          "form": "커뮤니티 하는거 있우세요?",
          "original_form": "커뮤니티 하는거 있우세요?",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100023236.1.4",
          "form": "가끔 더쿠 들어가요",
          "original_form": "가끔 더쿠 들어가요",
          "speaker_id": "1",
          "time": "20210829 23:49"
        },
        {
          "id": "MDRW2100023236.1.5",
          "form": "아 자주 들어본것같아요!",
          "original_form": "아 자주 들어본것같아요!",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023236.1.6",
          "form": "심심할 때 보고 있음 시간이 금방 가더라고요ㅋㅋ",
          "original_form": "심심할 때 보고 있음 시간이 금방 가더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100023236.1.7",
          "form": "시간보내기 쏠쏠하죠 ㅎㅎ",
          "original_form": "시간보내기 쏠쏠하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023236.1.8",
          "form": "하시는 거 있으세요?",
          "original_form": "하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023236.1.9",
          "form": "저는 다음 인기글 구경하거나",
          "original_form": "저는 다음 인기글 구경하거나",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023236.1.10",
          "form": "학교커뮤니티에서 정보얻는정도요!",
          "original_form": "학교커뮤니티에서 정보얻는정도요!",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023236.1.11",
          "form": "저는 네이버 위주로 써서 다음 인기글은 생각도 못 해봤어요",
          "original_form": "저는 네이버 위주로 써서 다음 인기글은 생각도 못 해봤어요",
          "speaker_id": "1",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023236.1.12",
          "form": "보통 무슨 내용 올라오나요?",
          "original_form": "보통 무슨 내용 올라오나요?",
          "speaker_id": "1",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023236.1.13",
          "form": "다양하게 올라와요!",
          "original_form": "다양하게 올라와요!",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023236.1.14",
          "form": "그때그때 이슈나",
          "original_form": "그때그때 이슈나",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023236.1.15",
          "form": "연예인들 얘기도 올라오고",
          "original_form": "연예인들 얘기도 올라오고",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023236.1.16",
          "form": "유머글",
          "original_form": "유머글",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023236.1.17",
          "form": "제목으로 낚는 낚시글 ㅋㅋ",
          "original_form": "제목으로 낚는 낚시글 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023236.1.18",
          "form": "레시피도 올라와요!",
          "original_form": "레시피도 올라와요!",
          "speaker_id": "2",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100023236.1.19",
          "form": "오 다양하니까 재밌겠어요",
          "original_form": "오 다양하니까 재밌겠어요",
          "speaker_id": "1",
          "time": "20210830 00:11"
        },
        {
          "id": "MDRW2100023236.1.20",
          "form": "어떤 주제를 젤 좋아하세요?",
          "original_form": "어떤 주제를 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 00:11"
        },
        {
          "id": "MDRW2100023236.1.21",
          "form": "유머글이 제일 재밌는거같아요 ㅋㅋ",
          "original_form": "유머글이 제일 재밌는거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:11"
        },
        {
          "id": "MDRW2100023236.1.22",
          "form": "너무 우문현답이네욬ㅋㅋㅋㅋ",
          "original_form": "너무 우문현답이네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:13"
        }
      ]
