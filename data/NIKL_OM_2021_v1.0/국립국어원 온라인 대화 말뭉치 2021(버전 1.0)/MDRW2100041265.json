      "utterance": [
        {
          "id": "MDRW2100041265.1.1",
          "form": "상당히 바람이 많이 부네요",
          "original_form": "상당히 바람이 많이 부네요",
          "speaker_id": "1",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041265.1.2",
          "form": "그러게요 이제 진짜 가을 같아요ㅋㅋㅋ",
          "original_form": "그러게요 이제 진짜 가을 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100041265.1.3",
          "form": "그쵸 쌀쌀해요",
          "original_form": "그쵸 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041265.1.4",
          "form": "이럴때 감기조심하세요",
          "original_form": "이럴때 감기조심하세요",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041265.1.5",
          "form": "이제 낙엽이 물들어가겠네여",
          "original_form": "이제 낙엽이 물들어가겠네여",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041265.1.6",
          "form": "그러니까요 이제 트렌치코트를 꺼낼 수 있지 않을까 생각해요ㅋㅋㅋㅋ",
          "original_form": "그러니까요 이제 트렌치코트를 꺼낼 수 있지 않을까 생각해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041265.1.7",
          "form": "와우ㅋㅋㅋ트렌치코트",
          "original_form": "와우ㅋㅋㅋ트렌치코트",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041265.1.8",
          "form": "살찌고나서 다신 입지못했다죠",
          "original_form": "살찌고나서 다신 입지못했다죠",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041265.1.9",
          "form": "옷장에 모셔두긴 했지만",
          "original_form": "옷장에 모셔두긴 했지만",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041265.1.10",
          "form": "코트류는 살쪄도 티 안나는 류 아닌가요??",
          "original_form": "코트류는 살쪄도 티 안나는 류 아닌가요??",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041265.1.11",
          "form": "딱 맞게 구입하신건가요?",
          "original_form": "딱 맞게 구입하신건가요?",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041265.1.12",
          "form": "어..음.. 왕창 쪘거든요",
          "original_form": "어..음.. 왕창 쪘거든요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041265.1.13",
          "form": "시중 브랜드에선 살수없더라고요ㅎ",
          "original_form": "시중 브랜드에선 살수없더라고요ㅎ",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041265.1.14",
          "form": "요즘 옷들 왜이렇게 작던지",
          "original_form": "요즘 옷들 왜이렇게 작던지",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041265.1.15",
          "form": "맞아요 특히 여름옷들은 거의 반려견 옷이죠ㅋㅋㅋㅋ",
          "original_form": "맞아요 특히 여름옷들은 거의 반려견 옷이죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041265.1.16",
          "form": "다 크롭에 쫙 달라붙잖아요",
          "original_form": "다 크롭에 쫙 달라붙잖아요",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041265.1.17",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041265.1.18",
          "form": "아동복",
          "original_form": "아동복",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041265.1.19",
          "form": "인줄알았어요",
          "original_form": "인줄알았어요",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041265.1.20",
          "form": "그냥 한뼘 이던데",
          "original_form": "그냥 한뼘 이던데",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041265.1.21",
          "form": "그러니까요 근데 여름옷들은 그런게 베스트셀러니까 진짜 저런 옷을 입고 다니는 사람이 많나 궁금해지죠",
          "original_form": "그러니까요 근데 여름옷들은 그런게 베스트셀러니까 진짜 저런 옷을 입고 다니는 사람이 많나 궁금해지죠",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100041265.1.22",
          "form": "근데 은근 길거리에서 입고다니시는분들 많이 봤어여",
          "original_form": "근데 은근 길거리에서 입고다니시는분들 많이 봤어여",
          "speaker_id": "1",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100041265.1.23",
          "form": "저는 약간 외곽에 살아서 거의 못봤는데 그런 옷 입는 분들이 많군요",
          "original_form": "저는 약간 외곽에 살아서 거의 못봤는데 그런 옷 입는 분들이 많군요",
          "speaker_id": "2",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100041265.1.24",
          "form": "네네 ㅎ 저도 수도권지역 아니라서 ",
          "original_form": "네네 ㅎ 저도 수도권지역 아니라서 ",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041265.1.25",
          "form": "젊은분들이",
          "original_form": "젊은분들이",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041265.1.26",
          "form": "입는거같아요",
          "original_form": "입는거같아요",
          "speaker_id": "1",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100041265.1.27",
          "form": "근데 사실 전 몸매되도 그런 옷 못 입을 것 같아요ㅋㅋㅋ",
          "original_form": "근데 사실 전 몸매되도 그런 옷 못 입을 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100041265.1.28",
          "form": "ㅋㅋㅋ저도요 유교우먼이라서ㅋ못입을거같아요",
          "original_form": "ㅋㅋㅋ저도요 유교우먼이라서ㅋ못입을거같아요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041265.1.29",
          "form": "저도 유교걸이라ㅋㅋㅋㅋㅋ",
          "original_form": "저도 유교걸이라ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
