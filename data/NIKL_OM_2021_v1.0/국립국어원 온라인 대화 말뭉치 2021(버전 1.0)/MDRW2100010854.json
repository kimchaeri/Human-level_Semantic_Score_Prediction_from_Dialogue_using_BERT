      "utterance": [
        {
          "id": "MDRW2100010854.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010854.1.2",
          "form": "아뇨 ㅋㅋㅋ 저는 가족이랑 살고잇어서..",
          "original_form": "아뇨 ㅋㅋㅋ 저는 가족이랑 살고잇어서..",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010854.1.3",
          "form": "님은 키우새요.",
          "original_form": "님은 키우새요.",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010854.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010854.1.5",
          "form": "저도 아니요. 어렸을 때는 새를 키웠었어요",
          "original_form": "저도 아니요. 어렸을 때는 새를 키웠었어요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010854.1.6",
          "form": "헐 어떤 새요??",
          "original_form": "헐 어떤 새요??",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010854.1.7",
          "form": "앵무세? 카나리아?",
          "original_form": "앵무세? 카나리아?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010854.1.8",
          "form": "문조라고 들어보셨나요?",
          "original_form": "문조라고 들어보셨나요?",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010854.1.9",
          "form": "넹 들어는 봣어요!!",
          "original_form": "넹 들어는 봣어요!!",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010854.1.10",
          "form": "색이 예쁜가요?",
          "original_form": "색이 예쁜가요?",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010854.1.11",
          "form": "깃털은 뮤슨 색이에요?",
          "original_form": "깃털은 뮤슨 색이에요?",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010854.1.12",
          "form": "제가 키운 건 백문조라고 흰색이에요",
          "original_form": "제가 키운 건 백문조라고 흰색이에요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010854.1.13",
          "form": "부리랑 눈 주위는 다홍색이고요",
          "original_form": "부리랑 눈 주위는 다홍색이고요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010854.1.14",
          "form": "아고 예쁜 아가였겠어요",
          "original_form": "아고 예쁜 아가였겠어요",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010854.1.15",
          "form": "사이즈는 참새만 해요",
          "original_form": "사이즈는 참새만 해요",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010854.1.16",
          "form": "이제는 직접 키우기는 부담스럽고 그냥 모니터로만 좋아하려구요",
          "original_form": "이제는 직접 키우기는 부담스럽고 그냥 모니터로만 좋아하려구요",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010854.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 키울려면 손가는게 이만저만이 아니죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요 키울려면 손가는게 이만저만이 아니죠",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010854.1.18",
          "form": "비용도 비용이고 헤어질 때 슬픈 게 제일 큰 문제죠 ㅠ",
          "original_form": "비용도 비용이고 헤어질 때 슬픈 게 제일 큰 문제죠 ㅠ",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010854.1.19",
          "form": "ㅜㅜㅜ 저도 헤어질게 무서워서 반려동물 못키우겠어요",
          "original_form": "ㅜㅜㅜ 저도 헤어질게 무서워서 반려동물 못키우겠어요",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100010854.1.20",
          "form": "맘아파서ㅜ어떻게 보내요 ㅜㅜ",
          "original_form": "맘아파서ㅜ어떻게 보내요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:39"
        }
