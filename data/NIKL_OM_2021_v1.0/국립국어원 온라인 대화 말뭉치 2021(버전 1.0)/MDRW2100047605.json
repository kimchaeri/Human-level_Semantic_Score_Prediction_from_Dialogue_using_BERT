      "utterance": [
        {
          "id": "MDRW2100047605.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100047605.1.2",
          "form": "네 ㅎㅎ 돈가스랑 된장 끓여서요",
          "original_form": "네 ㅎㅎ 돈가스랑 된장 끓여서요",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100047605.1.3",
          "form": "오 맛있으셨겠어요 !",
          "original_form": "오 맛있으셨겠어요 !",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100047605.1.4",
          "form": "돈까스 직접 하셨나요?",
          "original_form": "돈까스 직접 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100047605.1.5",
          "form": "네ㅎㅎ직접 했어욤",
          "original_form": "네ㅎㅎ직접 했어욤",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100047605.1.6",
          "form": "오ㅜㅜ 어떤 고기로 돈까스하세요?",
          "original_form": "오ㅜㅜ 어떤 고기로 돈까스하세요?",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100047605.1.7",
          "form": "등심 썼어요~",
          "original_form": "등심 썼어요~",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047605.1.8",
          "form": "그러시구나 저희집은 목살써서 하는데 기름지고 맛있더라구용",
          "original_form": "그러시구나 저희집은 목살써서 하는데 기름지고 맛있더라구용",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100047605.1.9",
          "form": "돈까스 얘기하니 먹고싶네요~",
          "original_form": "돈까스 얘기하니 먹고싶네요~",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100047605.1.10",
          "form": "오 맛있을것 같아요 ㅎㅎ",
          "original_form": "오 맛있을것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100047605.1.11",
          "form": "내일은 어떤 메뉴 드실 계획인가요?",
          "original_form": "내일은 어떤 메뉴 드실 계획인가요?",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047605.1.12",
          "form": "내일은 저녁은 부대찌게요 ㅋㅋ 낮에는 혼자 먹으니 대충 ..? 먹으면 되서 저녁만 걱정하면 되거든요 ㅎㅎ",
          "original_form": "내일은 저녁은 부대찌게요 ㅋㅋ 낮에는 혼자 먹으니 대충 ..? 먹으면 되서 저녁만 걱정하면 되거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:56"
        },
        {
          "id": "MDRW2100047605.1.13",
          "form": "아하 부대찌개 맛있죠 햄이랑 김치 가득 넣고 하면 맛있죠",
          "original_form": "아하 부대찌개 맛있죠 햄이랑 김치 가득 넣고 하면 맛있죠",
          "speaker_id": "2",
          "time": "20210827 00:00"
        },
        {
          "id": "MDRW2100047605.1.14",
          "form": "맞아요 ㅋㅋㅋ 맛있어용 떡국떡도 좀 넣고 ㅎㅎ 콩나물도 넣고",
          "original_form": "맞아요 ㅋㅋㅋ 맛있어용 떡국떡도 좀 넣고 ㅎㅎ 콩나물도 넣고",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100047605.1.15",
          "form": "오 그쵸ㅋㅋ 라면사리도 넣으시나요",
