      "utterance": [
        {
          "id": "MDRW2100020779.1.1",
          "form": "노래 좋아하세요?",
          "original_form": "노래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020779.1.2",
          "form": "요즘 날씨에 듣기좋은 노래 뭐 있을까요?",
          "original_form": "요즘 날씨에 듣기좋은 노래 뭐 있을까요?",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020779.1.3",
          "form": "뭔가 밝은 노래들이 좋을 듯해요",
          "original_form": "뭔가 밝은 노래들이 좋을 듯해요",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020779.1.4",
          "form": "오 그렇네요 추천곡 혹시 있으세요ㅎㅎ?",
          "original_form": "오 그렇네요 추천곡 혹시 있으세요ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020779.1.5",
          "form": "저도 그냥 음원사이트에서 그때그때 끌리는 것 들어서ㅎㅎ",
          "original_form": "저도 그냥 음원사이트에서 그때그때 끌리는 것 들어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020779.1.6",
          "form": "ㅋㅋ 유재석씨 같은 탑백 귀를 가지신 모양이에요",
          "original_form": "ㅋㅋ 유재석씨 같은 탑백 귀를 가지신 모양이에요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020779.1.7",
          "form": "아아 그건 아니고",
          "original_form": "아아 그건 아니고",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020779.1.8",
          "form": "이미 탑백 된 노래를 들어요ㅋㅋ",
          "original_form": "이미 탑백 된 노래를 들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020779.1.9",
          "form": "ㅋㅋ 그럼 음원강자 아이유 노래도 들으세요?ㅋㅋ",
          "original_form": "ㅋㅋ 그럼 음원강자 아이유 노래도 들으세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020779.1.10",
          "form": "네네 아이유 노래도 좋아해요",
          "original_form": "네네 아이유 노래도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020779.1.11",
          "form": "최근에 악동뮤지션에 콜라보한 낙하 많이 들었어요",
          "original_form": "최근에 악동뮤지션에 콜라보한 낙하 많이 들었어요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020779.1.12",
          "form": "오 최근에 콜라보했어요? 들어봐야겠어요ㅋㅋㅋ!!!",
          "original_form": "오 최근에 콜라보했어요? 들어봐야겠어요ㅋㅋㅋ!!!",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100020779.1.13",
          "form": "아이유 직전에 낸 앨범에 어푸! 그 노래가 또 악뮤 노래라면서요",
          "original_form": "아이유 직전에 낸 앨범에 어푸! 그 노래가 또 악뮤 노래라면서요",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020779.1.14",
          "form": "전 그것도 참 좋더라구요",
          "original_form": "전 그것도 참 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020779.1.15",
          "form": "아이유 노래는 맘이 몽글몽글해져요",
          "original_form": "아이유 노래는 맘이 몽글몽글해져요",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020779.1.16",
          "form": "맞아요 저는 아이유 음색도 엄청 좋아해요",
          "original_form": "맞아요 저는 아이유 음색도 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020779.1.17",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020779.1.18",
          "form": "노래는 보통 어떤 음원사이트에서 들으세요?",
          "original_form": "노래는 보통 어떤 음원사이트에서 들으세요?",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020779.1.19",
          "form": "저는 플로 이용해요!",
          "original_form": "저는 플로 이용해요!",
          "speaker_id": "1",
          "time": "20210828 13:42"
        }
