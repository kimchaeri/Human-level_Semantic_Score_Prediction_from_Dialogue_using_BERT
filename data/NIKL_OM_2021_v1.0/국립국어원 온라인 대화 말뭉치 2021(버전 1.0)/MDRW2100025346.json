      "utterance": [
        {
          "id": "MDRW2100025346.1.1",
          "form": "메타버스 갑니다",
          "original_form": "메타버스 갑니다",
          "speaker_id": "1",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.2",
          "form": "메타버스 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "메타버스 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.3",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.4",
          "form": "모름",
          "original_form": "모름",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.5",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.6",
          "form": "요즘 시대가 바뀌어썽요",
          "original_form": "요즘 시대가 바뀌어썽요",
          "speaker_id": "1",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.7",
          "form": "설명부탁요",
          "original_form": "설명부탁요",
          "speaker_id": "2",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025346.1.8",
          "form": "이게 대충 말하자면 우리가 하던 게임이 현실이 되는건데",
          "original_form": "이게 대충 말하자면 우리가 하던 게임이 현실이 되는건데",
          "speaker_id": "1",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100025346.1.9",
          "form": "가상 현실에 내가 들어가는거죠",
          "original_form": "가상 현실에 내가 들어가는거죠",
          "speaker_id": "1",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100025346.1.10",
          "form": "아 이거 그거네 드라마",
          "original_form": "아 이거 그거네 드라마",
          "speaker_id": "2",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100025346.1.11",
          "form": "알함브라 봤어요?",
          "original_form": "알함브라 봤어요?",
          "speaker_id": "2",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100025346.1.12",
          "form": "그거 진짜재밌게봣는데",
          "original_form": "그거 진짜재밌게봣는데",
          "speaker_id": "2",
          "time": "20210901 09:55"
        },
        {
          "id": "MDRW2100025346.1.13",
          "form": "미국 초글링들 80프로가 로블록스라는 가상 플랫폼을 이용해요",
          "original_form": "미국 초글링들 80프로가 로블록스라는 가상 플랫폼을 이용해요",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.14",
          "form": "모름 저 드라마 영화 즐기진 않아요",
          "original_form": "모름 저 드라마 영화 즐기진 않아요",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.15",
          "form": "보면보는데",
          "original_form": "보면보는데",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.16",
          "form": "뭐 하는걸 좋아함",
          "original_form": "뭐 하는걸 좋아함",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.17",
          "form": "아 저는 셋다좋아함",
          "original_form": "아 저는 셋다좋아함",
          "speaker_id": "2",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.18",
          "form": "초딩들이 똑똑하네 역시 미국",
          "original_form": "초딩들이 똑똑하네 역시 미국",
          "speaker_id": "2",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.19",
          "form": "암튼 싸이월드 하듯이",
          "original_form": "암튼 싸이월드 하듯이",
          "speaker_id": "1",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025346.1.20",
          "form": "얘네는 로블록스를함",
          "original_form": "얘네는 로블록스를함",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.21",
          "form": "가상 공간에서 서로 캐릭터가 되어서 떠들고 뭐 하고 하는건데",
          "original_form": "가상 공간에서 서로 캐릭터가 되어서 떠들고 뭐 하고 하는건데",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.22",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.23",
          "form": "앞으로는 이런 시스템이 더더욱 많아질거에요",
          "original_form": "앞으로는 이런 시스템이 더더욱 많아질거에요",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.24",
          "form": "심즈같은건가요",
          "original_form": "심즈같은건가요",
          "speaker_id": "2",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.25",
          "form": "최근에도 그 가상 모델 보셨나요",
          "original_form": "최근에도 그 가상 모델 보셨나요",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.26",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025346.1.27",
          "form": "심즈 가상현실판",
          "original_form": "심즈 가상현실판",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.28",
          "form": "시간되면 알함브라 한번보세요 졸잼임",
          "original_form": "시간되면 알함브라 한번보세요 졸잼임",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.29",
          "form": "근데 재밌겠다",
          "original_form": "근데 재밌겠다",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.30",
          "form": "제목부터 비호감인데요",
          "original_form": "제목부터 비호감인데요",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.31",
          "form": "스토리좀",
          "original_form": "스토리좀",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.32",
          "form": "알함브라 궁전의 비밀인가",
          "original_form": "알함브라 궁전의 비밀인가",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.33",
          "form": "현빈이 남주고 박신혜가 여주인데",
          "original_form": "현빈이 남주고 박신혜가 여주인데",
          "speaker_id": "2",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025346.1.34",
          "form": "현빈이 자기 ㅂㄹ친구랑 같이 게임회사 대표로 있다가",
          "original_form": "현빈이 자기 ㅂㄹ친구랑 같이 게임회사 대표로 있다가",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.35",
          "form": ";; 인도거인줄",
          "original_form": ";; 인도거인줄",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.36",
          "form": "어? 칼들고 설치고 그건가",
          "original_form": "어? 칼들고 설치고 그건가",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.37",
          "form": "갑자기 현빈이 게임안으로 들어가게됨",
          "original_form": "갑자기 현빈이 게임안으로 들어가게됨",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.38",
          "form": "레벨나오고",
          "original_form": "레벨나오고",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.39",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.41",
          "form": "어... 대충 그런 느낌",
          "original_form": "어... 대충 그런 느낌",
          "speaker_id": "1",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100025346.1.42",
          "form": "생각난김에 오늘부터 다시 정주행해야겠어요",
          "original_form": "생각난김에 오늘부터 다시 정주행해야겠어요",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025346.1.43",
          "form": "근데 그거는 포켓몬고처럼 증강현실이고",
          "original_form": "근데 그거는 포켓몬고처럼 증강현실이고",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025346.1.44",
          "form": "로블록스 이거 마인크래프트네요",
          "original_form": "로블록스 이거 마인크래프트네요",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025346.1.45",
          "form": "드라마를 지나가면서 흘깃 봐서 모르것네",
          "original_form": "드라마를 지나가면서 흘깃 봐서 모르것네",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025346.1.46",
          "form": "네네 그런 느낌이에요",
          "original_form": "네네 그런 느낌이에요",
          "speaker_id": "1",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025346.1.47",
          "form": "재밌네요 지식이 많으시네",
          "original_form": "재밌네요 지식이 많으시네",
          "speaker_id": "2",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025346.1.48",
          "form": "화상 회의도 그런걸로 하고 그러더라구요",
          "original_form": "화상 회의도 그런걸로 하고 그러더라구요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025346.1.49",
          "form": "넓고 얕은 지식들",
          "original_form": "넓고 얕은 지식들",
          "speaker_id": "1",
          "time": "20210901 10:01"
        }
      ]
    }
