        {
          "id": "MDRW2100028046.1.1",
          "form": "군대갓다오심 ?",
          "original_form": "군대갓다오심 ?",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100028046.1.2",
          "form": "네 군대다녀오심?",
          "original_form": "네 군대다녀오심?",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100028046.1.3",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "1",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100028046.1.4",
          "form": "육군이요?",
          "original_form": "육군이요?",
          "speaker_id": "1",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100028046.1.5",
          "form": "네 ㅋㅋ탱크타고 다녔습니다",
          "original_form": "네 ㅋㅋ탱크타고 다녔습니다",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028046.1.6",
          "form": "군대 면제신가요?",
          "original_form": "군대 면제신가요?",
          "speaker_id": "2",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028046.1.7",
          "form": "네 면제요",
          "original_form": "네 면제요",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028046.1.8",
          "form": "탱크 ? 탱크조종병 ?",
          "original_form": "탱크 ? 탱크조종병 ?",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100028046.1.9",
          "form": "탱크 지휘했습니다",
          "original_form": "탱크 지휘했습니다",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028046.1.10",
          "form": "4분의4박자로 지휘하니까 잘나가더라구호",
          "original_form": "4분의4박자로 지휘하니까 잘나가더라구호",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028046.1.11",
          "form": "딴따라란",
          "original_form": "딴따라란",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028046.1.12",
          "form": "농담이구요 포반장했습니다",
          "original_form": "농담이구요 포반장했습니다",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100028046.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100028046.1.14",
          "form": "포 쏘는 사람이요?",
          "original_form": "포 쏘는 사람이요?",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028046.1.15",
          "form": "네 포도 쏘구요",
          "original_form": "네 포도 쏘구요",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028046.1.16",
          "form": "오 재밌어요? 대박...",
          "original_form": "오 재밌어요? 대박...",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028046.1.17",
          "form": "멋잇다",
          "original_form": "멋잇다",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028046.1.18",
          "form": "뽕하고 날라갑니다",
          "original_form": "뽕하고 날라갑니다",
          "speaker_id": "2",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028046.1.19",
