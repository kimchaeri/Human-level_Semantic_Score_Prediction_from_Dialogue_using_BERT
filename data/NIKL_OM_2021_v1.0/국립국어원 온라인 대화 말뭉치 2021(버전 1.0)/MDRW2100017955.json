      "utterance": [
        {
          "id": "MDRW2100017955.1.1",
          "form": "여행도 좋아하시나요?",
          "original_form": "여행도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100017955.1.2",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017955.1.3",
          "form": "네네 코로나땜시 못가지만 여행은 언제나 설레죠~!",
          "original_form": "네네 코로나땜시 못가지만 여행은 언제나 설레죠~!",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017955.1.4",
          "form": "여행 진짜 코로나땜에...",
          "original_form": "여행 진짜 코로나땜에...",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017955.1.5",
          "form": "해외여행 가고싶어요",
          "original_form": "해외여행 가고싶어요",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017955.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017955.1.7",
          "form": "어딜 젤 가고싶나요",
          "original_form": "어딜 젤 가고싶나요",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017955.1.8",
          "form": "전 하와이",
          "original_form": "전 하와이",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017955.1.9",
          "form": "전 몰디브",
          "original_form": "전 몰디브",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017955.1.10",
          "form": "둘다 휴양지네여ㅋ",
          "original_form": "둘다 휴양지네여ㅋ",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017955.1.11",
          "form": "유럽도 가보고 싶었는데",
          "original_form": "유럽도 가보고 싶었는데",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.12",
          "form": "이번 파리 올림픽을 계기로",
          "original_form": "이번 파리 올림픽을 계기로",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.13",
          "form": "이번생은 망한건가 싶기도 하고",
          "original_form": "이번생은 망한건가 싶기도 하고",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.14",
          "form": "함 가보시는 거 어떨까요",
          "original_form": "함 가보시는 거 어떨까요",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.15",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.17",
          "form": "백신도 못맞았는데ㅋㅋㅋ",
          "original_form": "백신도 못맞았는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017955.1.18",
          "form": "괌은 갈수있어여",
          "original_form": "괌은 갈수있어여",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.19",
          "form": "백신 맞으면",
          "original_form": "백신 맞으면",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.20",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.21",
          "form": "쓰읍...괌..?",
          "original_form": "쓰읍...괌..?",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.22",
          "form": "저도 아직 못맞아거",
          "original_form": "저도 아직 못맞아거",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.23",
          "form": "걍...걍...계획만 하고 있네요",
          "original_form": "걍...걍...계획만 하고 있네요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.25",
          "form": "하와이에서 서핑하는 그날까지",
          "original_form": "하와이에서 서핑하는 그날까지",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017955.1.26",
          "form": "괌은 쇼핑말곤",
          "original_form": "괌은 쇼핑말곤",
          "speaker_id": "1",
          "time": "20210826 22:19"
        }
      ]
    }
  ]
