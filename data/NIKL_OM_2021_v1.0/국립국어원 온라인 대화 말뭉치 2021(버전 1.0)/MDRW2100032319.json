      "utterance": [
        {
          "id": "MDRW2100032319.1.1",
          "form": "그럼 즁식은 냐ㅐ부음식이야?",
          "original_form": "그럼 즁식은 냐ㅐ부음식이야?",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032319.1.2",
          "form": "응 조리사 쌤들이 만들잖아",
          "original_form": "응 조리사 쌤들이 만들잖아",
          "speaker_id": "1",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032319.1.3",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032319.1.4",
          "form": "내가 석식도우미였거든",
          "original_form": "내가 석식도우미였거든",
          "speaker_id": "1",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032319.1.5",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032319.1.6",
          "form": "급식 퍼주는 도우미",
          "original_form": "급식 퍼주는 도우미",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.7",
          "form": "우와 봉사활동?",
          "original_form": "우와 봉사활동?",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.8",
          "form": "엉 봉사시간도 받고",
          "original_form": "엉 봉사시간도 받고",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.9",
          "form": "급식비도 면제였어",
          "original_form": "급식비도 면제였어",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.11",
          "form": "우리는 급식비 안내는데",
          "original_form": "우리는 급식비 안내는데",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.12",
          "form": "헐 진짜?!?!",
          "original_form": "헐 진짜?!?!",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.13",
          "form": "무료 급식?!",
          "original_form": "무료 급식?!",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.14",
          "form": "응 아마?",
          "original_form": "응 아마?",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032319.1.15",
          "form": "그럼 학생증",
          "original_form": "그럼 학생증",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.16",
          "form": "필요없나?",
          "original_form": "필요없나?",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.17",
          "form": "아니 그건 있어",
          "original_form": "아니 그건 있어",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.18",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.19",
          "form": "급식먹을때",
          "original_form": "급식먹을때",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.20",
          "form": "찍어야하거든",
          "original_form": "찍어야하거든",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.21",
          "form": "엥??왜??",
          "original_form": "엥??왜??",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.22",
          "form": "두번 먹거나",
          "original_form": "두번 먹거나",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.23",
          "form": "신청안한애들 먹을까봐",
          "original_form": "신청안한애들 먹을까봐",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032319.1.24",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210905 15:27"
        }
      ]
