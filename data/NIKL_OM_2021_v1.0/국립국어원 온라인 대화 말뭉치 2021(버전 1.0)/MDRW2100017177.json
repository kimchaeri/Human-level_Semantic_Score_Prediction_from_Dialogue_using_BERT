        },
        {
          "id": "MDRW2100017177.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100017177.1.3",
          "form": "요며칠 바빠서 못봤네요ㅜㅜ",
          "original_form": "요며칠 바빠서 못봤네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100017177.1.4",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100017177.1.5",
          "form": "ㅋㅋㅋ 꾸준히 하려고하는데 생각보다 어렵네요",
          "original_form": "ㅋㅋㅋ 꾸준히 하려고하는데 생각보다 어렵네요",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100017177.1.6",
          "form": "혹시 무슨일 하세요?",
          "original_form": "혹시 무슨일 하세요?",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100017177.1.7",
          "form": "사무직이요",
          "original_form": "사무직이요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100017177.1.8",
          "form": "오 혹시 요즘 재택하시나요?",
          "original_form": "오 혹시 요즘 재택하시나요?",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100017177.1.9",
          "form": "아직오",
          "original_form": "아직오",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100017177.1.10",
          "form": "사람이 문제죠",
          "original_form": "사람이 문제죠",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100017177.1.11",
          "form": "사람이요?",
          "original_form": "사람이요?",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100017177.1.12",
          "form": "상사가ㅜ힘들어요",
          "original_form": "상사가ㅜ힘들어요",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100017177.1.13",
          "form": "아하ㅠ 고생하시네요",
          "original_form": "아하ㅠ 고생하시네요",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100017177.1.14",
          "form": "같이 일하는 사람이 제일 중요한거같아요",
          "original_form": "같이 일하는 사람이 제일 중요한거같아요",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100017177.1.15",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100017177.1.16",
          "form": "힘내세요!",
          "original_form": "힘내세요!",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100017177.1.17",
          "form": "요즘 코시국이라 퇴사도 마냥 답이 아니더라구요",
          "original_form": "요즘 코시국이라 퇴사도 마냥 답이 아니더라구요",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100017177.1.18",
          "form": "심사숙거 할라두요",
          "original_form": "심사숙거 할라두요",
