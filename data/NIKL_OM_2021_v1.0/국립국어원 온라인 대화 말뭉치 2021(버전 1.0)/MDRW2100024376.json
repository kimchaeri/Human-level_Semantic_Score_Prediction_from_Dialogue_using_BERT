      "utterance": [
        {
          "id": "MDRW2100024376.1.1",
          "form": "머리 두개 예쁘게낫다고 팔고",
          "original_form": "머리 두개 예쁘게낫다고 팔고",
          "speaker_id": "2",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.2",
          "form": "그러더라고요 ㅋㅋ",
          "original_form": "그러더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.3",
          "form": "히익",
          "original_form": "히익",
          "speaker_id": "1",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.4",
          "form": "아녀 ㅋㅋ고양이 오드아이여",
          "original_form": "아녀 ㅋㅋ고양이 오드아이여",
          "speaker_id": "2",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.6",
          "form": "아니 좀 어떻게 생각해보면",
          "original_form": "아니 좀 어떻게 생각해보면",
          "speaker_id": "1",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.7",
          "form": "기괴한거같아요ㅠ",
          "original_form": "기괴한거같아요ㅠ",
          "speaker_id": "1",
          "time": "20210831 03:01"
        },
        {
          "id": "MDRW2100024376.1.8",
          "form": "인간이 제일 잔인해",
          "original_form": "인간이 제일 잔인해",
          "speaker_id": "1",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.9",
          "form": "맞아요.. 생각해보니",
          "original_form": "맞아요.. 생각해보니",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.10",
          "form": "그런곳은 안가는게 맞겟내요..",
          "original_form": "그런곳은 안가는게 맞겟내요..",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.11",
          "form": "그래서 정말 도움이필요한애들이아닌이상..",
          "original_form": "그래서 정말 도움이필요한애들이아닌이상..",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.13",
          "form": "동물원도 좀 그렇죠",
          "original_form": "동물원도 좀 그렇죠",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.14",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.15",
          "form": "동물원 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "동물원 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.16",
          "form": "구조하듯 데려오는건 좋긴한데 말이죠",
          "original_form": "구조하듯 데려오는건 좋긴한데 말이죠",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.17",
          "form": "초원에서 뛰어놀아야 할 애들이ㅠㅠㅠ",
          "original_form": "초원에서 뛰어놀아야 할 애들이ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.18",
          "form": "그 아쿠아리움에 사는",
          "original_form": "그 아쿠아리움에 사는",
          "speaker_id": "1",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.19",
          "form": "네네..",
          "original_form": "네네..",
          "speaker_id": "2",
          "time": "20210831 03:02"
        },
        {
          "id": "MDRW2100024376.1.20",
          "form": "돌고래들 정신병 엄청 걸린다면서요",
          "original_form": "돌고래들 정신병 엄청 걸린다면서요",
          "speaker_id": "1",
          "time": "20210831 03:03"
        },
        {
          "id": "MDRW2100024376.1.21",
          "form": "돌고래는 지능도 높아가지구..ㅠㅠ",
          "original_form": "돌고래는 지능도 높아가지구..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 03:03"
        },
        {
          "id": "MDRW2100024376.1.22",
          "form": "맞아요 ㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 03:03"
