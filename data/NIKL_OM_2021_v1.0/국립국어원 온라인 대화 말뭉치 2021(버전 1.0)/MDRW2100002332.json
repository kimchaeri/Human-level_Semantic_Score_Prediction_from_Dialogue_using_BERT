      "utterance": [
        {
          "id": "MDRW2100002332.1.1",
          "form": "저 요번 휴가는 안갔어요",
          "original_form": "저 요번 휴가는 안갔어요",
          "speaker_id": "2",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100002332.1.2",
          "form": "저도 요 휴가가 뭔가요 후유",
          "original_form": "저도 요 휴가가 뭔가요 후유",
          "speaker_id": "1",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002332.1.3",
          "form": "그냥 집에 있다 잠깐식 맛집 투어 갔어요",
          "original_form": "그냥 집에 있다 잠깐식 맛집 투어 갔어요",
          "speaker_id": "2",
          "time": "20210820 09:44"
        },
        {
          "id": "MDRW2100002332.1.4",
          "form": "지방으로요",
          "original_form": "지방으로요",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002332.1.5",
          "form": "사람이 없는데로 갔어요",
          "original_form": "사람이 없는데로 갔어요",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002332.1.6",
          "form": "맛집 투어 좋죠~~",
          "original_form": "맛집 투어 좋죠~~",
          "speaker_id": "1",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002332.1.7",
          "form": "맛있는곳 추천좀해주셔요~~~ ㅎㅎ",
          "original_form": "맛있는곳 추천좀해주셔요~~~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002332.1.8",
          "form": "근데 저번에 첨으로 골목 거기 갔는데",
          "original_form": "근데 저번에 첨으로 골목 거기 갔는데",
          "speaker_id": "2",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002332.1.9",
          "form": "못먹었어요 ㅜㅜ오히려 옆집 맛집 발견했어요",
          "original_form": "못먹었어요 ㅜㅜ오히려 옆집 맛집 발견했어요",
          "speaker_id": "2",
          "time": "20210820 09:48"
        },
        {
          "id": "MDRW2100002332.1.10",
          "form": "저 그냥 대는데로 가서",
          "original_form": "저 그냥 대는데로 가서",
          "speaker_id": "2",
          "time": "20210820 09:49"
        },
        {
          "id": "MDRW2100002332.1.11",
          "form": "골목그 방영옆에 베트남 음식점 넘 맛났어요",
          "original_form": "골목그 방영옆에 베트남 음식점 넘 맛났어요",
          "speaker_id": "2",
          "time": "20210820 09:49"
        },
        {
          "id": "MDRW2100002332.1.12",
          "form": "오앗 베트남 음식  저희집에 킬러 잇어서",
          "original_form": "오앗 베트남 음식  저희집에 킬러 잇어서",
          "speaker_id": "1",
          "time": "20210820 10:02"
        },
        {
          "id": "MDRW2100002332.1.13",
          "form": "엄청 싸고 다양해서 좋았어요",
          "original_form": "엄청 싸고 다양해서 좋았어요",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002332.1.14",
          "form": "의외의 발견이였어요 ㅎㅎ어디든 떠나니 찾게되네요",
          "original_form": "의외의 발견이였어요 ㅎㅎ어디든 떠나니 찾게되네요",
          "speaker_id": "2",
          "time": "20210820 10:03"
        },
        {
          "id": "MDRW2100002332.1.15",
          "form": "광천에 놀러갔을때 게장집도 성다리 부러지게 나오던데 이름이 항시 생각안나요",
          "original_form": "광천에 놀러갔을때 게장집도 성다리 부러지게 나오던데 이름이 항시 생각안나요",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002332.1.16",
          "form": "발걸음 닿는대로 가서요",
          "original_form": "발걸음 닿는대로 가서요",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002332.1.17",
          "form": "맞아요 발길 닿는데효 가면  꼭 기억이 안나더랴구요 ㅎ",
          "original_form": "맞아요 발길 닿는데효 가면  꼭 기억이 안나더랴구요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002332.1.18",
          "form": "맨날 계획없이 다녀요 ㅋㅋ",
          "original_form": "맨날 계획없이 다녀요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002332.1.19",
          "form": "저도 20대  여행가다 지나간 작은 마을에서 먹은 닭계장집\n아직도 생각나요",
          "original_form": "저도 20대  여행가다 지나간 작은 마을에서 먹은 닭계장집\n아직도 생각나요",
          "speaker_id": "1",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002332.1.20",
          "form": "그러니까요 그런것들이 뇌리에 박혀서 생각나요",
          "original_form": "그러니까요 그런것들이 뇌리에 박혀서 생각나요",
          "speaker_id": "2",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002332.1.21",
          "form": "근데 위치 기억은 왜 왜  안날까요 ㅎㅎㅎ",
          "original_form": "근데 위치 기억은 왜 왜  안날까요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:14"
        },
        {
          "id": "MDRW2100002332.1.22",
          "form": "여행엔 역시 맛난 음식이 함께해야해요",
          "original_form": "여행엔 역시 맛난 음식이 함께해야해요",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002332.1.23",
          "form": "진짜 간판도 없는 곳이였어요 ㅎㅎ",
          "original_form": "진짜 간판도 없는 곳이였어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002332.1.24",
          "form": "절대 안나요 ㅋㅋ",
          "original_form": "절대 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002332.1.25",
          "form": "맞아요 꼭 없어요",
          "original_form": "맞아요 꼭 없어요",
          "speaker_id": "2",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002332.1.26",
          "form": "노부부가 하셨는데 아직도 하실련지 그립긴 하네요",
          "original_form": "노부부가 하셨는데 아직도 하실련지 그립긴 하네요",
          "speaker_id": "1",
          "time": "20210820 10:15"
        },
        {
          "id": "MDRW2100002332.1.27",
          "form": "아 예전 우연히 들렀던 시장한켠에 그날그날 안주를 내오던 막걸리집도 간판이 없어요",
          "original_form": "아 예전 우연히 들렀던 시장한켠에 그날그날 안주를 내오던 막걸리집도 간판이 없어요",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002332.1.28",
          "form": "맞아요 노부부 ㅎㅎ",
          "original_form": "맞아요 노부부 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002332.1.29",
          "form": "공식인가봐요",
          "original_form": "공식인가봐요",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002332.1.30",
          "form": "왠지 돌아 가셨을것도 같고요 ㅜㅜ",
          "original_form": "왠지 돌아 가셨을것도 같고요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 10:16"
        },
        {
          "id": "MDRW2100002332.1.31",
          "form": "그릇도 제각각 제멋대로  갖춘것도 없는데\n맛은 끝내주는 ㅎㅎ",
          "original_form": "그릇도 제각각 제멋대로  갖춘것도 없는데\n맛은 끝내주는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:17"
        },
        {
          "id": "MDRW2100002332.1.32",
          "form": "헉 맞아요 전부 달랐어요 플라스틱에 스텐에 사기그릇에",
          "original_form": "헉 맞아요 전부 달랐어요 플라스틱에 스텐에 사기그릇에",
          "speaker_id": "2",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002332.1.33",
          "form": "아 손맛이 있었죠",
          "original_form": "아 손맛이 있었죠",
          "speaker_id": "2",
          "time": "20210820 10:18"
        },
        {
          "id": "MDRW2100002332.1.34",
          "form": "요번에 제주도 가려했는데 ㅜㅜ못갔어요 코로나 때문에",
          "original_form": "요번에 제주도 가려했는데 ㅜㅜ못갔어요 코로나 때문에",
          "speaker_id": "2",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002332.1.35",
          "form": "맘대로 갈날 오면 좋겠어요",
          "original_form": "맘대로 갈날 오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210820 10:19"
        },
        {
          "id": "MDRW2100002332.1.36",
          "form": "어멋 다똑같은  ㅋㅋㅋ",
          "original_form": "어멋 다똑같은  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 10:20"
        },
        {
          "id": "MDRW2100002332.1.37",
          "form": "빨리 코로나 끝나고 여행 가길 빌어보아요 ㅎㅎ",
          "original_form": "빨리 코로나 끝나고 여행 가길 빌어보아요 ㅎㅎ",
