      "utterance": [
        {
          "id": "MDRW2100008762.1.1",
          "form": "밥 먹음?",
          "original_form": "밥 먹음?",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008762.1.2",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008762.1.3",
          "form": "ㄴㄴㄴ아직",
          "original_form": "ㄴㄴㄴ아직",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008762.1.4",
          "form": "과자를너무많이먹음",
          "original_form": "과자를너무많이먹음",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.5",
          "form": "뮤ㅓ야 지금 6시야???",
          "original_form": "뮤ㅓ야 지금 6시야???",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.6",
          "form": "아니 저기요",
          "original_form": "아니 저기요",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.7",
          "form": "저녁먹을시간임",
          "original_form": "저녁먹을시간임",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.8",
          "form": "밥 먹어야지",
          "original_form": "밥 먹어야지",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ넌뭐먹을거냐",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넌뭐먹을거냐",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.10",
          "form": "나 밥먹고 가만히 있음",
          "original_form": "나 밥먹고 가만히 있음",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008762.1.11",
          "form": "점심먹은거아니었음?",
          "original_form": "점심먹은거아니었음?",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.12",
          "form": "지금 6시인데?",
          "original_form": "지금 6시인데?",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.13",
          "form": "초밥하고피자먹었다며",
          "original_form": "초밥하고피자먹었다며",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.14",
          "form": "그거 한참 전 아니야?",
          "original_form": "그거 한참 전 아니야?",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.15",
          "form": "ㅇㅇㅇ그니까",
          "original_form": "ㅇㅇㅇ그니까",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.16",
          "form": "12시인가 2시인가 그때 즈음에 먹음",
          "original_form": "12시인가 2시인가 그때 즈음에 먹음",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.17",
          "form": "저녁언제먹었냐",
          "original_form": "저녁언제먹었냐",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.18",
          "form": "그리고 심부름 갔다와서",
          "original_form": "그리고 심부름 갔다와서",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.19",
          "form": "빠르네",
          "original_form": "빠르네",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.20",
          "form": "방금",
          "original_form": "방금",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008762.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008762.1.22",
          "form": "밥먹음?",
          "original_form": "밥먹음?",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008762.1.23",
          "form": "아니면뭐다른거?",
          "original_form": "아니면뭐다른거?",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008762.1.24",
          "form": "심부름 갔다오자마자 먹으라 그러셔서 먹음",
          "original_form": "심부름 갔다오자마자 먹으라 그러셔서 먹음",
