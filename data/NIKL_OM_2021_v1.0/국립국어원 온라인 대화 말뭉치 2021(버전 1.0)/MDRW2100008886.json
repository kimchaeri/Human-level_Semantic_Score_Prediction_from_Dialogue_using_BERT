      "utterance": [
        {
          "id": "MDRW2100008886.1.1",
          "form": "일과직업으로 자연히 넘어왔네욧ㅋㅋ",
          "original_form": "일과직업으로 자연히 넘어왔네욧ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008886.1.2",
          "form": "앗 넹 ㅋㅋㅋ 일할때 새로 알게된 모습이 있나요? ㅎㅎ",
          "original_form": "앗 넹 ㅋㅋㅋ 일할때 새로 알게된 모습이 있나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008886.1.3",
          "form": "새로..알게된 모습?..",
          "original_form": "새로..알게된 모습?..",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008886.1.4",
          "form": "게으른 나에서 너무 게으른 나의 모습 같은거요?",
          "original_form": "게으른 나에서 너무 게으른 나의 모습 같은거요?",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008886.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008886.1.6",
          "form": "저는 포커페이스가 잘 안돼요... ㅠㅠ",
          "original_form": "저는 포커페이스가 잘 안돼요... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008886.1.7",
          "form": "아.. 그건 경력이 필요할거같아요 아무래도..ㅜ",
          "original_form": "아.. 그건 경력이 필요할거같아요 아무래도..ㅜ",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008886.1.8",
          "form": "쉽지않죠",
          "original_form": "쉽지않죠",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008886.1.9",
          "form": "경력이 쌓여도 그런거 같아요 ㅋㅋㅋ 감출수가 없는 ㅋㅋㅋ",
          "original_form": "경력이 쌓여도 그런거 같아요 ㅋㅋㅋ 감출수가 없는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008886.1.10",
          "form": "그래서 마스크가 한편으론 좋은듯한 ㅋㅋ",
          "original_form": "그래서 마스크가 한편으론 좋은듯한 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008886.1.11",
          "form": "맞아요 전 이제 마스크가편해요",
          "original_form": "맞아요 전 이제 마스크가편해요",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008886.1.12",
          "form": "화장도안하고",
          "original_form": "화장도안하고",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008886.1.13",
          "form": "지하철에서 목운동할때 콧구멍 안보여서 편해요",
          "original_form": "지하철에서 목운동할때 콧구멍 안보여서 편해요",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008886.1.14",
          "form": "ㅎㅎㅎ 맞아요",
          "original_form": "ㅎㅎㅎ 맞아요",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008886.1.15",
          "form": "일하면서 가장 보람찬? 뿌듯한 기억 있으세요?",
          "original_form": "일하면서 가장 보람찬? 뿌듯한 기억 있으세요?",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008886.1.16",
          "form": "어 아무래도 재능이 잘쓰일 때 좋죠",
          "original_form": "어 아무래도 재능이 잘쓰일 때 좋죠",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008886.1.17",
          "form": "사람은 밥만먹는 동물이 아니니까요",
          "original_form": "사람은 밥만먹는 동물이 아니니까요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008886.1.18",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008886.1.19",
          "form": "그쵸 인정받을 때..",
          "original_form": "그쵸 인정받을 때..",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008886.1.20",
          "form": "저는 뭔가 피드백이 오면 더 좋은거 같아요",
          "original_form": "저는 뭔가 피드백이 오면 더 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008886.1.21",
          "form": "그래서 저도 동료에게 좋은 부분이 있으면 꼭 말로 해줘요",
          "original_form": "그래서 저도 동료에게 좋은 부분이 있으면 꼭 말로 해줘요",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008886.1.22",
          "form": "아하 피드백도 성장에 중요하죠",
          "original_form": "아하 피드백도 성장에 중요하죠",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008886.1.23",
          "form": "정말 좋은동료시네요",
          "original_form": "정말 좋은동료시네요",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008886.1.24",
          "form": "그런사람들이 주위에 있으면 좋은영향력을 받는거같아요",
          "original_form": "그런사람들이 주위에 있으면 좋은영향력을 받는거같아요",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008886.1.25",
          "form": "ㅎㅎ 부끄럽네요 ㅋㅋㅋ 전 칭찬받는거 좋아하는 사람이라 더 그런거 같아요 ㅋㅋㅋ",
          "original_form": "ㅎㅎ 부끄럽네요 ㅋㅋㅋ 전 칭찬받는거 좋아하는 사람이라 더 그런거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008886.1.26",
          "form": "근데 반대로 부정적인 피드백으로 자극받는 사람도 있어서 신기한거 같아요~ 그걸 밑거름으로 삼는",
          "original_form": "근데 반대로 부정적인 피드백으로 자극받는 사람도 있어서 신기한거 같아요~ 그걸 밑거름으로 삼는",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008886.1.27",
          "form": "한편으론 부럽기도 해요!",
          "original_form": "한편으론 부럽기도 해요!",
          "speaker_id": "2",
          "time": "20210822 19:36"
        },
        {
          "id": "MDRW2100008886.1.28",
          "form": "사람이 다 다르듯이 같은 피드백도 각자 다르게 받아들이는거 같아요",
          "original_form": "사람이 다 다르듯이 같은 피드백도 각자 다르게 받아들이는거 같아요",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008886.1.29",
          "form": "뭐든 자양분으로삼아서 나아가면",
          "original_form": "뭐든 자양분으로삼아서 나아가면",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008886.1.30",
          "form": "좋은결과를 만들어낼거같아요",
          "original_form": "좋은결과를 만들어낼거같아요",
          "speaker_id": "1",
          "time": "20210822 19:38"
        },
        {
          "id": "MDRW2100008886.1.31",
          "form": "맞아요 ㅎㅎ 저는 의기소침해지는 스타일이라 ㅋㅋㅋ 그런 분들 보면 저런 사람도 있구나 하면서 꼭  나쁜 의도로 그런 말을 하는 건 아니구나 생각하게 되더라고요",
          "original_form": "맞아요 ㅎㅎ 저는 의기소침해지는 스타일이라 ㅋㅋㅋ 그런 분들 보면 저런 사람도 있구나 하면서 꼭  나쁜 의도로 그런 말을 하는 건 아니구나 생각하게 되더라고요",
          "speaker_id": "2",
