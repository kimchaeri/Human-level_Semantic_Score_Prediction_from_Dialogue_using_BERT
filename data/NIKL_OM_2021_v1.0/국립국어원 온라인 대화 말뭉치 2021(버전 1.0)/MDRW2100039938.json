      "utterance": [
        {
          "id": "MDRW2100039938.1.1",
          "form": "제주도 가고 싶어요ㅠ",
          "original_form": "제주도 가고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039938.1.2",
          "form": "코로나 끝나면 가고싶은곳이 어디안가요 ?",
          "original_form": "코로나 끝나면 가고싶은곳이 어디안가요 ?",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039938.1.3",
          "form": "해외 나가보고픈데",
          "original_form": "해외 나가보고픈데",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039938.1.4",
          "form": "일단 쿠바 가보고 싶어요",
          "original_form": "일단 쿠바 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039938.1.5",
          "form": "쿠바 생소한 곳이네요",
          "original_form": "쿠바 생소한 곳이네요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039938.1.6",
          "form": "많이 알려진 곳은 아니죠",
          "original_form": "많이 알려진 곳은 아니죠",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039938.1.7",
          "form": "무엇보다 공산국가이기도 하구요",
          "original_form": "무엇보다 공산국가이기도 하구요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039938.1.8",
          "form": "물론 관광은 된답니다",
          "original_form": "물론 관광은 된답니다",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039938.1.9",
          "form": "공산국가 좀 무섭네요",
          "original_form": "공산국가 좀 무섭네요",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039938.1.10",
          "form": "치안이 좀 괜찮을까요 ?",
          "original_form": "치안이 좀 괜찮을까요 ?",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039938.1.11",
          "form": "관광객이 많아서 괜찮대요",
          "original_form": "관광객이 많아서 괜찮대요",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039938.1.12",
          "form": "우리나라에서도 많이 가나요 ?",
          "original_form": "우리나라에서도 많이 가나요 ?",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039938.1.13",
          "form": "가는 사람들은 가는거 같아요",
          "original_form": "가는 사람들은 가는거 같아요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039938.1.14",
          "form": "저도 추천받은거라서요",
          "original_form": "저도 추천받은거라서요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039938.1.15",
          "form": "아름다운 경치 구경을 많이 할수가 있는곳인가요 ?",
          "original_form": "아름다운 경치 구경을 많이 할수가 있는곳인가요 ?",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039938.1.16",
          "form": "네 바다도 이쁘고 도시 분위기도 신기하대요",
          "original_form": "네 바다도 이쁘고 도시 분위기도 신기하대요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039938.1.17",
          "form": "말로만 들어도 꼭 가보고 싶어지네요",
          "original_form": "말로만 들어도 꼭 가보고 싶어지네요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039938.1.18",
          "form": "ㅎㅎ 한번 검색해보세요",
          "original_form": "ㅎㅎ 한번 검색해보세요",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100039938.1.19",
          "form": "검색하고 알아봐야겠네요",
          "original_form": "검색하고 알아봐야겠네요",
          "speaker_id": "1",
          "time": "20210904 07:53"
        }
      ]
