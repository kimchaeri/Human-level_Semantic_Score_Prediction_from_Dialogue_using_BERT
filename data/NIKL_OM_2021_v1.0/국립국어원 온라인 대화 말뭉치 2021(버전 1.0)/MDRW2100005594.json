      "utterance": [
        {
          "id": "MDRW2100005594.1.1",
          "form": "언니 자요?ㅋㅋ",
          "original_form": "언니 자요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005594.1.2",
          "form": "애들자요?",
          "original_form": "애들자요?",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005594.1.3",
          "form": "애들잔다ㅋ",
          "original_form": "애들잔다ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005594.1.4",
          "form": "너거는 자나?",
          "original_form": "너거는 자나?",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005594.1.5",
          "form": "방금 육퇴했어요.ㅋㅋ",
          "original_form": "방금 육퇴했어요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005594.1.6",
          "form": "집안일도 마무리했어요?ㅋ",
          "original_form": "집안일도 마무리했어요?ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005594.1.7",
          "form": "설거지ㅡㅡ",
          "original_form": "설거지ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005594.1.8",
          "form": "이제 정리하고 자야재ㅋㅋ",
          "original_form": "이제 정리하고 자야재ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005594.1.9",
          "form": "니는 집안일다했나?",
          "original_form": "니는 집안일다했나?",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005594.1.10",
          "form": "빨래만 개면될것같아요.ㅋㅋ",
          "original_form": "빨래만 개면될것같아요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005594.1.11",
          "form": "어두워서 빨래는 쉽지않을것같아요.",
          "original_form": "어두워서 빨래는 쉽지않을것같아요.",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005594.1.12",
          "form": "난도 애들어지린거 치워야되는데 귀찬흐ㅡ",
          "original_form": "난도 애들어지린거 치워야되는데 귀찬흐ㅡ",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005594.1.13",
          "form": "울신랑도 방에있고",
          "original_form": "울신랑도 방에있고",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005594.1.14",
          "form": "치우고 이제 슬슬 자야재ㅋㅋ",
          "original_form": "치우고 이제 슬슬 자야재ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005594.1.15",
          "form": "ㅋㅋㅋㅋ오늘은 일찍자네요 언니ㅋ",
          "original_form": "ㅋㅋㅋㅋ오늘은 일찍자네요 언니ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005594.1.16",
          "form": "어제 너무늦게잤는데 일찍일어남",
          "original_form": "어제 너무늦게잤는데 일찍일어남",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005594.1.17",
          "form": "애들 한자시험친다고ㅋ",
          "original_form": "애들 한자시험친다고ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:25"
        },
        {
          "id": "MDRW2100005594.1.18",
          "form": "와 ㅋㅋㅋㅋ대단해요ㅋㄲㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋ대단해요ㅋㄲㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005594.1.19",
          "form": "한자공부했어요? 시험공부?",
          "original_form": "한자공부했어요? 시험공부?",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005594.1.20",
          "form": "한자 센터다니잖아ㅋ",
          "original_form": "한자 센터다니잖아ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.21",
          "form": "오늘자격증시험",
          "original_form": "오늘자격증시험",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.23",
          "form": "급수시험인가?",
          "original_form": "급수시험인가?",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.24",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.25",
          "form": "둘다쳤어요?ㅋㅋ",
          "original_form": "둘다쳤어요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.26",
          "form": "어 ㅋㅋ",
          "original_form": "어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.27",
          "form": "푸하하",
          "original_form": "푸하하",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.28",
          "form": "결과는 언제 나와요?ㄱ",
          "original_form": "결과는 언제 나와요?ㄱ",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.29",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005594.1.30",
          "form": "모르겠다 한달?",
          "original_form": "모르겠다 한달?",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005594.1.31",
          "form": "애들이 더바쁨ㅋ",
          "original_form": "애들이 더바쁨ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005594.1.32",
          "form": "그렇네요. 애들 멋지다.ㅋㅋㅋ",
          "original_form": "그렇네요. 애들 멋지다.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:28"
        }
      ]
