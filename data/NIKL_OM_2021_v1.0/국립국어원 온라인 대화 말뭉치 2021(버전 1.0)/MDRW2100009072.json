        {
          "id": "MDRW2100009072.1.2",
          "form": "스티커ㅋㅋ",
          "original_form": "스티커ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009072.1.3",
          "form": "전 노래",
          "original_form": "전 노래",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009072.1.4",
          "form": "일기장에서 사는건 포도알이었나요?",
          "original_form": "일기장에서 사는건 포도알이었나요?",
          "speaker_id": "2",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009072.1.5",
          "form": "포도알ㅋ",
          "original_form": "포도알ㅋ",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100009072.1.6",
          "form": "그건 가물가물하네요",
          "original_form": "그건 가물가물하네요",
          "speaker_id": "1",
          "time": "20210822 21:06"
        },
        {
          "id": "MDRW2100009072.1.7",
          "form": "전 일기 엄청 열심히 썼거든요 보면 부끄러울듯요 ㅋㅋㅋ",
          "original_form": "전 일기 엄청 열심히 썼거든요 보면 부끄러울듯요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:07"
        },
        {
          "id": "MDRW2100009072.1.8",
          "form": "ㅎㅎ 싸이월드가 옛생각 많이하게하네요",
          "original_form": "ㅎㅎ 싸이월드가 옛생각 많이하게하네요",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009072.1.9",
          "form": "파도타기도 열심히 했었어요 ㅋㅋㅋ",
          "original_form": "파도타기도 열심히 했었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009072.1.10",
          "form": "악 파도타기",
          "original_form": "악 파도타기",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009072.1.11",
          "form": "잊고있었어요",
          "original_form": "잊고있었어요",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009072.1.12",
          "form": "줄줄이",
          "original_form": "줄줄이",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009072.1.13",
          "form": "1촌맺기 ㅋ",
          "original_form": "1촌맺기 ㅋ",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009072.1.14",
          "form": "넘 웃겨요",
          "original_form": "넘 웃겨요",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009072.1.15",
          "form": "미니미 꾸미는것도 재미있었는데요",
          "original_form": "미니미 꾸미는것도 재미있었는데요",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009072.1.16",
          "form": "그땐 죽기살기로",
          "original_form": "그땐 죽기살기로",
          "speaker_id": "1",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009072.1.17",
          "form": "저 퇴근하면 싸이 앞에 앉아서 싸이질 했었잖아요",
          "original_form": "저 퇴근하면 싸이 앞에 앉아서 싸이질 했었잖아요",
