      "utterance": [
        {
          "id": "MDRW2100018363.1.1",
          "form": "패션 좋아하시나요?",
          "original_form": "패션 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 06:36"
        },
        {
          "id": "MDRW2100018363.1.2",
          "form": "네 근데 돈이 없어요",
          "original_form": "네 근데 돈이 없어요",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018363.1.3",
          "form": "옷값이 너무 비싸요..",
          "original_form": "옷값이 너무 비싸요..",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018363.1.4",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018363.1.5",
          "form": "살벌해졌어요..",
          "original_form": "살벌해졌어요..",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018363.1.6",
          "form": "요즘 어떤게 유행이죠?",
          "original_form": "요즘 어떤게 유행이죠?",
          "speaker_id": "1",
          "time": "20210827 08:34"
        },
        {
          "id": "MDRW2100018363.1.7",
          "form": "가을은 역시 코트같아요",
          "original_form": "가을은 역시 코트같아요",
          "speaker_id": "2",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018363.1.8",
          "form": "올해도 코트가 유행인가요 ㅎㅎ?",
          "original_form": "올해도 코트가 유행인가요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018363.1.9",
          "form": "설마 프렌치코트..?",
          "original_form": "설마 프렌치코트..?",
          "speaker_id": "1",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018363.1.10",
          "form": "아마..?",
          "original_form": "아마..?",
          "speaker_id": "2",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018363.1.11",
          "form": "기본템이 되어버렸어요",
          "original_form": "기본템이 되어버렸어요",
          "speaker_id": "2",
          "time": "20210827 08:54"
        },
        {
          "id": "MDRW2100018363.1.12",
          "form": "ㅋㅋㅋ 그 베이지 코트는 아니겠죠",
          "original_form": "ㅋㅋㅋ 그 베이지 코트는 아니겠죠",
          "speaker_id": "1",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100018363.1.13",
          "form": "그..거 맞습니다ㅋㅋㅋ",
          "original_form": "그..거 맞습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018363.1.14",
          "form": "요맘때 모두와 커플룩이 되어버리는 그..!",
          "original_form": "요맘때 모두와 커플룩이 되어버리는 그..!",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018363.1.15",
          "form": "유행은 매번 도네요 ㅎㅎ",
          "original_form": "유행은 매번 도네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018363.1.16",
          "form": "예전엔 바바리맨들이 입는다고 기피하던 때가 있었는데..",
          "original_form": "예전엔 바바리맨들이 입는다고 기피하던 때가 있었는데..",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018363.1.17",
          "form": "맞아요ㅋㅋ 어느새 가을을 상징하게 되었어요",
          "original_form": "맞아요ㅋㅋ 어느새 가을을 상징하게 되었어요",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018363.1.18",
          "form": "어떤 색 사셨어요?",
          "original_form": "어떤 색 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018363.1.19",
          "form": "베이지랑 카키요ㅋㅋㅋ",
          "original_form": "베이지랑 카키요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018363.1.20",
          "form": "두개씩이나 사셨어요??!",
          "original_form": "두개씩이나 사셨어요??!",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018363.1.21",
          "form": "같은 디자인이에요?",
          "original_form": "같은 디자인이에요?",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018363.1.22",
          "form": "다른 디자인인데 사고 보니 두 개네요ㅋㅋㅋ",
          "original_form": "다른 디자인인데 사고 보니 두 개네요ㅋㅋㅋ",
