          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100033927.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100033927.1.6",
          "form": "난 코로나 때문에 신청 안 했어",
          "original_form": "난 코로나 때문에 신청 안 했어",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100033927.1.7",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100033927.1.8",
          "form": "활동을 잘 못 한대",
          "original_form": "활동을 잘 못 한대",
          "speaker_id": "2",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100033927.1.9",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100033927.1.10",
          "form": "어떤 동아리 해?",
          "original_form": "어떤 동아리 해?",
