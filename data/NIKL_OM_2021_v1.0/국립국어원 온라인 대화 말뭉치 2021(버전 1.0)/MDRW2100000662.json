      "utterance": [
        {
          "id": "MDRW2100000662.1.1",
          "form": "날씨가 좀 춥네",
          "original_form": "날씨가 좀 춥네",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000662.1.2",
          "form": "글게 이제 가을인듯",
          "original_form": "글게 이제 가을인듯",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000662.1.3",
          "form": "계절의위대함 ㅎ",
          "original_form": "계절의위대함 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000662.1.4",
          "form": "ㅋㅋ 긴옷 입어야겠다",
          "original_form": "ㅋㅋ 긴옷 입어야겠다",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000662.1.5",
          "form": "아직도 낮은 덥다",
          "original_form": "아직도 낮은 덥다",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000662.1.6",
          "form": "그렇긴 함 ㅎ",
          "original_form": "그렇긴 함 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000662.1.7",
          "form": "잠잘땐 약간 뚜거운 이불이 필요함 ㅎ",
          "original_form": "잠잘땐 약간 뚜거운 이불이 필요함 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000662.1.8",
          "form": "옹 좀 있으면 극세사 이불 꺼내야할듯",
          "original_form": "옹 좀 있으면 극세사 이불 꺼내야할듯",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000662.1.9",
          "form": "극세사는 겨울되야지 오바다",
          "original_form": "극세사는 겨울되야지 오바다",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000662.1.10",
          "form": "ㅋㅋㅋ 오바 아님 다음주만 돼도 필요할듯",
          "original_form": "ㅋㅋㅋ 오바 아님 다음주만 돼도 필요할듯",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000662.1.11",
          "form": "그러게 요즘 봄 가을이 짧으니 ㅋ",
          "original_form": "그러게 요즘 봄 가을이 짧으니 ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000662.1.12",
          "form": "그릏다 ㅎ ㅠ 가을 넘 짧아",
          "original_form": "그릏다 ㅎ ㅠ 가을 넘 짧아",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000662.1.13",
          "form": "아쉽아쉽 ㅠㅠ",
          "original_form": "아쉽아쉽 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000662.1.14",
          "form": "미세먼지 요새도 안좋은가?",
          "original_form": "미세먼지 요새도 안좋은가?",
