      "utterance": [
        {
          "id": "MDRW2100005534.1.1",
          "form": "엉 빵집옆에",
          "original_form": "엉 빵집옆에",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005534.1.2",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005534.1.3",
          "form": "요즘 거기",
          "original_form": "요즘 거기",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005534.1.4",
          "form": "후기 별로더라",
          "original_form": "후기 별로더라",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005534.1.5",
          "form": "까페에 평이 안좋제??",
          "original_form": "까페에 평이 안좋제??",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005534.1.6",
          "form": "안그래도 신랑한테",
          "original_form": "안그래도 신랑한테",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.7",
          "form": "고기맛이 별로라고",
          "original_form": "고기맛이 별로라고",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.8",
          "form": "거기 요새 평안좋다고 가지마라 했다",
          "original_form": "거기 요새 평안좋다고 가지마라 했다",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.9",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.10",
          "form": "신랑도 오늘 삼겹살 먹더니",
          "original_form": "신랑도 오늘 삼겹살 먹더니",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.11",
          "form": "고깃집 바꿔야겠다고 하대",
          "original_form": "고깃집 바꿔야겠다고 하대",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.12",
          "form": "응 저녁맛있게 먹었었어야했는데",
          "original_form": "응 저녁맛있게 먹었었어야했는데",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.13",
          "form": "좀 그랬겠네",
          "original_form": "좀 그랬겠네",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.14",
          "form": "엉 이제 다른데 가야겠음",
          "original_form": "엉 이제 다른데 가야겠음",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.15",
          "form": "응 맞아 거기 괜찮았는데 아쉽네",
          "original_form": "응 맞아 거기 괜찮았는데 아쉽네",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.16",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.17",
          "form": "삼겹살 어디서 사노??",
          "original_form": "삼겹살 어디서 사노??",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005534.1.18",
          "form": "address이나 요즘엔 식자재",
          "original_form": "&address&이나 요즘엔 식자재",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005534.1.19",
          "form": "식자재마트에서 장 많이본다.",
          "original_form": "식자재마트에서 장 많이본다.",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005534.1.20",
          "form": "아 식자재??",
          "original_form": "아 식자재??",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005534.1.21",
          "form": "거기 아가방 뒤에??",
          "original_form": "거기 아가방 뒤에??",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005534.1.22",
          "form": "아님 address??",
          "original_form": "아님 &address&??",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005534.1.23",
          "form": "응응 저기 address. 놀이방있는곳.",
          "original_form": "응응 저기 &address&. 놀이방있는곳.",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005534.1.24",
          "form": "거기 고기 괜찮나??",
          "original_form": "거기 고기 괜찮나??",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005534.1.25",
          "form": "응 나는 괜찮더라ㅋㅋ",
          "original_form": "응 나는 괜찮더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005534.1.26",
          "form": "아빠가 얼마전에 거기서 소꼬리 사서 곰탕 끓여줬는데 고기 맛있더라고",
          "original_form": "아빠가 얼마전에 거기서 소꼬리 사서 곰탕 끓여줬는데 고기 맛있더라고",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005534.1.27",
          "form": "고기사러 함 가야겠네",
          "original_form": "고기사러 함 가야겠네",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005534.1.28",
          "form": "응 맞아. 저렴하고 맛도 괜찮은듯",
          "original_form": "응 맞아. 저렴하고 맛도 괜찮은듯",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005534.1.29",
          "form": "address은 좀 비싸다던데 괜찮나??",
          "original_form": "&address&은 좀 비싸다던데 괜찮나??",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005534.1.30",
          "form": "응 비싸도 맛있어",
          "original_form": "응 비싸도 맛있어",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005534.1.31",
          "form": "아하 비싸도 맛있군 ㅎㅎ",
          "original_form": "아하 비싸도 맛있군 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:59"
        }
      ]
    }
