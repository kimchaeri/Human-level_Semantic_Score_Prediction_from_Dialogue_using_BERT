      "utterance": [
        {
          "id": "MDRW2100012372.1.1",
          "form": "곰표 이런거는 뭔가 옛날 갬성을 느낄수 있어 인기 많은듯요~",
          "original_form": "곰표 이런거는 뭔가 옛날 갬성을 느낄수 있어 인기 많은듯요~",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012372.1.2",
          "form": "곰표 먹어봤나?",
          "original_form": "곰표 먹어봤나?",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012372.1.3",
          "form": "맛은 그냥ㅈ그렇다던데",
          "original_form": "맛은 그냥ㅈ그렇다던데",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012372.1.4",
          "form": "지는 술을 안먹잖아여 ㅋㅋㅋㅋ",
          "original_form": "지는 술을 안먹잖아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012372.1.5",
          "form": "그런건 진짜 다 마케팅을 잘해서~",
          "original_form": "그런건 진짜 다 마케팅을 잘해서~",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012372.1.6",
          "form": "한번먹고는 안먹을듯 ㅋㅋㅋ",
          "original_form": "한번먹고는 안먹을듯 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012372.1.7",
          "form": "나도 술을 안 먹으니 모르겠네",
          "original_form": "나도 술을 안 먹으니 모르겠네",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012372.1.8",
          "form": "두꺼비는 뭐지",
          "original_form": "두꺼비는 뭐지",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012372.1.9",
          "form": "진로 독감이 뭔지 모름",
          "original_form": "진로 독감이 뭔지 모름",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012372.1.10",
          "form": "도 까비",
          "original_form": "도 까비",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012372.1.11",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012372.1.12",
          "form": "두꺼비",
          "original_form": "두꺼비",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012372.1.13",
          "form": "ㅋㅋㅋㅋ진로 그 두꺼비 소주",
          "original_form": "ㅋㅋㅋㅋ진로 그 두꺼비 소주",
          "speaker_id": "1",
          "time": "20210825 07:56"
        },
        {
          "id": "MDRW2100012372.1.14",
          "form": "요즘 그거 두꺼비 뭐 만들어가 많이 팔잖아요~",
          "original_form": "요즘 그거 두꺼비 뭐 만들어가 많이 팔잖아요~",
          "speaker_id": "1",
          "time": "20210825 07:56"
        },
        {
          "id": "MDRW2100012372.1.15",
          "form": "사은품 때문에 많이들 사는듯요~",
          "original_form": "사은품 때문에 많이들 사는듯요~",
          "speaker_id": "1",
          "time": "20210825 07:57"
        },
        {
          "id": "MDRW2100012372.1.16",
          "form": "사은품ㅋㅋ 두꺼비소주ㅋㅋ",
          "original_form": "사은품ㅋㅋ 두꺼비소주ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:03"
        },
        {
          "id": "MDRW2100012372.1.17",
          "form": "보니깐 좀 귀여워서 혹하던데~",
          "original_form": "보니깐 좀 귀여워서 혹하던데~",
          "speaker_id": "1",
          "time": "20210825 09:06"
        },
        {
          "id": "MDRW2100012372.1.18",
          "form": "술을 안먹으니 사면 술을 버려야함 ㅋㅋㅋㅋㅋ",
          "original_form": "술을 안먹으니 사면 술을 버려야함 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:06"
        },
        {
          "id": "MDRW2100012372.1.19",
          "form": "근데 그런 콜라보는 술값이 더 올라가지 않나여??",
          "original_form": "근데 그런 콜라보는 술값이 더 올라가지 않나여??",
          "speaker_id": "1",
          "time": "20210825 09:06"
        },
        {
          "id": "MDRW2100012372.1.20",
          "form": "그렇겧지?",
          "original_form": "그렇겧지?",
          "speaker_id": "2",
          "time": "20210825 09:09"
        },
        {
          "id": "MDRW2100012372.1.21",
          "form": "우리는 술안마시니ㅡㅡ",
          "original_form": "우리는 술안마시니ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 09:10"
        },
        {
          "id": "MDRW2100012372.1.22",
          "form": "울신랑줘야되나",
          "original_form": "울신랑줘야되나",
          "speaker_id": "2",
          "time": "20210825 09:10"
        },
        {
          "id": "MDRW2100012372.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ사은품때문에 술을 끊을수가 없겠군여 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ사은품때문에 술을 끊을수가 없겠군여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:10"
        },
        {
          "id": "MDRW2100012372.1.24",
          "form": "곰표궁금하긴하다",
          "original_form": "곰표궁금하긴하다",
          "speaker_id": "2",
          "time": "20210825 09:17"
        },
        {
          "id": "MDRW2100012372.1.25",
          "form": "우린 먹을수 없는 맛일꺼예요 ㅋㅋㅋㅋㅋㅇ",
          "original_form": "우린 먹을수 없는 맛일꺼예요 ㅋㅋㅋㅋㅋㅇ",
          "speaker_id": "1",
          "time": "20210825 09:20"
        },
        {
          "id": "MDRW2100012372.1.26",
          "form": "이슬톡톡이 젱 맞아여 ㅋㅋㅋㅋ",
          "original_form": "이슬톡톡이 젱 맞아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:20"
        },
        {
          "id": "MDRW2100012372.1.27",
          "form": "이슬톡톡도 못먹음ㅋㅋ",
          "original_form": "이슬톡톡도 못먹음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012372.1.28",
          "form": "난 술끊음",
          "original_form": "난 술끊음",
          "speaker_id": "2",
          "time": "20210825 09:46"
        },
        {
          "id": "MDRW2100012372.1.29",
          "form": "진짜요??난 그것도 취하긴해서 잘 안먹지만 ㅋㅋㅋㅋ",
          "original_form": "진짜요??난 그것도 취하긴해서 잘 안먹지만 ㅋㅋㅋㅋ",
