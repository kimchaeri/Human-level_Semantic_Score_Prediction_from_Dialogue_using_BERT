      "utterance": [
        {
          "id": "MDRW2100000364.1.1",
          "form": "반가워요 어디여행을 자주다니세요??",
          "original_form": "반가워요 어디여행을 자주다니세요??",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000364.1.2",
          "form": "저는 부산을 자주다녀요",
          "original_form": "저는 부산을 자주다녀요",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000364.1.3",
          "form": "바다가 없는곳에 살고있어서요",
          "original_form": "바다가 없는곳에 살고있어서요",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000364.1.4",
          "form": "부산 가끔 가봤는데 볼거리가 많나봐요",
          "original_form": "부산 가끔 가봤는데 볼거리가 많나봐요",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000364.1.5",
          "form": "네 부산에 가면 기장 해녀촌이 유명해요",
          "original_form": "네 부산에 가면 기장 해녀촌이 유명해요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000364.1.6",
          "form": "해녀촌이면 어떤걸 하는곳인가요??",
          "original_form": "해녀촌이면 어떤걸 하는곳인가요??",
          "speaker_id": "1",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000364.1.7",
          "form": "주로 해산물을 많이먹어요",
          "original_form": "주로 해산물을 많이먹어요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000364.1.8",
          "form": "멍게,해삼,낙지,소라등등 해산물과 전복죽을 먹으러가요",
          "original_form": "멍게,해삼,낙지,소라등등 해산물과 전복죽을 먹으러가요",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000364.1.9",
          "form": "맛있을거 같아요 가격은 비싸지 않나요??",
          "original_form": "맛있을거 같아요 가격은 비싸지 않나요??",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000364.1.10",
          "form": "3~5만원이면 충분히 배부르게 먹을수 있어요",
          "original_form": "3~5만원이면 충분히 배부르게 먹을수 있어요",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000364.1.11",
          "form": "바다가 바로  앞이라 보는재미도 있구요",
          "original_form": "바다가 바로  앞이라 보는재미도 있구요",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000364.1.12",
          "form": "그렇군요 한번쯤 가봐야겠네요",
          "original_form": "그렇군요 한번쯤 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000364.1.13",
          "form": "부산 또 다른곳은 추천 해 주실곳 있으신가요~?",
          "original_form": "부산 또 다른곳은 추천 해 주실곳 있으신가요~?",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000364.1.14",
          "form": "이하정 간장게장은 꼭드셔보세요",
          "original_form": "이하정 간장게장은 꼭드셔보세요",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000364.1.15",
          "form": "정말 인생 간장게장이에요",
          "original_form": "정말 인생 간장게장이에요",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000364.1.16",
          "form": "얼마나 맛있길래 극찬일까요~??",
          "original_form": "얼마나 맛있길래 극찬일까요~??",
          "speaker_id": "1",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000364.1.17",
          "form": "진짜 입에서 살살녹아요",
          "original_form": "진짜 입에서 살살녹아요",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000364.1.18",
          "form": "검색해보니 정말 맛있어 보이네요 부산가면 먹어보러 갈게요",
          "original_form": "검색해보니 정말 맛있어 보이네요 부산가면 먹어보러 갈게요",
          "speaker_id": "1",
          "time": "20210819 16:07"
        }
