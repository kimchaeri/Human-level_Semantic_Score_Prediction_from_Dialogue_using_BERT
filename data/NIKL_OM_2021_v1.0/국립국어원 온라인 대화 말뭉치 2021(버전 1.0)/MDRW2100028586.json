      "utterance": [
        {
          "id": "MDRW2100028586.1.1",
          "form": "맞어ㅜㅜㅜㅜ그거 진짜 개고생이야ㅜㅜㅜㅜㅜㅜ....뼈삭고",
          "original_form": "맞어ㅜㅜㅜㅜ그거 진짜 개고생이야ㅜㅜㅜㅜㅜㅜ....뼈삭고",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028586.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ춤추다가 관절 다 나가고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ춤추다가 관절 다 나가고",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028586.1.3",
          "form": "진짜 아무나 못해",
          "original_form": "진짜 아무나 못해",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028586.1.4",
          "form": "어깨무릎안좋고",
          "original_form": "어깨무릎안좋고",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028586.1.5",
          "form": "다이어트 빡세게하고",
          "original_form": "다이어트 빡세게하고",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028586.1.6",
          "form": "감정은 늘 혹사당하곸ㅋㅋㅋ",
          "original_form": "감정은 늘 혹사당하곸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.7",
          "form": "ㄹㅇ 여돌은",
          "original_form": "ㄹㅇ 여돌은",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.8",
          "form": "평범한 생활도 못하고",
          "original_form": "평범한 생활도 못하고",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.9",
          "form": "다이어트 너무 심하게하니",
          "original_form": "다이어트 너무 심하게하니",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.10",
          "form": "몸안좋을대로 안좋고",
          "original_form": "몸안좋을대로 안좋고",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.11",
          "form": "조금만살쪄도 난리나고",
          "original_form": "조금만살쪄도 난리나고",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.12",
          "form": "맞어 그리고 여돌은 성희롱도ㅜㅜㅜㅜ",
          "original_form": "맞어 그리고 여돌은 성희롱도ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028586.1.13",
          "form": "진짜심해",
          "original_form": "진짜심해",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028586.1.14",
          "form": "아.........\r\nㄹㅇ",
          "original_form": "아.........\r\nㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028586.1.15",
          "form": "지금은 더이상 주목받지못하지만",
          "original_form": "지금은 더이상 주목받지못하지만",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028586.1.16",
          "form": "에이프릴 진솔?",
          "original_form": "에이프릴 진솔?",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028586.1.17",
          "form": "아 알지",
          "original_form": "아 알지",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028586.1.18",
          "form": "걔 미자인데 성희롱 심하다고",
          "original_form": "걔 미자인데 성희롱 심하다고",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.19",
          "form": "걔 한마디 했잖아",
          "original_form": "걔 한마디 했잖아",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.20",
          "form": "하지말라고 표출하니까",
          "original_form": "하지말라고 표출하니까",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.21",
          "form": "그건 좀 속시원",
          "original_form": "그건 좀 속시원",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.22",
          "form": "건방지다고",
          "original_form": "건방지다고",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.23",
          "form": "안뜨고싶다고",
          "original_form": "안뜨고싶다고",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.24",
          "form": "ㅡㅡ........ 진짜 미친건가싶고",
          "original_form": "ㅡㅡ........ 진짜 미친건가싶고",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.25",
          "form": "남돌들은 그나마 여자팬들한테 우쭈쭈 많이 받는데",
          "original_form": "남돌들은 그나마 여자팬들한테 우쭈쭈 많이 받는데",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.26",
          "form": "남자팬들은 진짜 이상한 애들 많아",
          "original_form": "남자팬들은 진짜 이상한 애들 많아",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.27",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.28",
          "form": "쁘걸네도 이상해",
          "original_form": "쁘걸네도 이상해",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028586.1.29",
          "form": "아 알지알지",
          "original_form": "아 알지알지",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028586.1.30",
          "form": "아저씨들 쁘걸한테 난리치는거",
          "original_form": "아저씨들 쁘걸한테 난리치는거",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028586.1.31",
          "form": "우리덕에떴다며",
          "original_form": "우리덕에떴다며",
          "speaker_id": "1",
          "time": "20210902 20:59"
        }
      ]
    }
