      "utterance": [
        {
          "id": "MDRW2100025011.1.1",
          "form": "에엥..........!!!",
          "original_form": "에엥..........!!!",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100025011.1.2",
          "form": "특정한 항이나있는걸로ㅏ 사면",
          "original_form": "특정한 항이나있는걸로ㅏ 사면",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100025011.1.3",
          "form": "오리지날두 짜요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 하림이거이거..",
          "original_form": "오리지날두 짜요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 하림이거이거..",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.4",
          "form": "금방질려서 오리지날마사먹었는데",
          "original_form": "금방질려서 오리지날마사먹었는데",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.5",
          "form": "짭짤해요 ㅋㄱㅋㅋㅋ",
          "original_form": "짭짤해요 ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.6",
          "form": "다이어트용 맞는거겠죠...?",
          "original_form": "다이어트용 맞는거겠죠...?",
          "speaker_id": "1",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.7",
          "form": "해동만 잘해서먹음 엄청부드럽긴 한데",
          "original_form": "해동만 잘해서먹음 엄청부드럽긴 한데",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.8",
          "form": "ㅋㄱㅋㅋㅋ그거만 먹으면 맞을거에요...ㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋ그거만 먹으면 맞을거에요...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.9",
          "form": "하림 소시지도 괜찮더라구요",
          "original_form": "하림 소시지도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.10",
          "form": "부드럽고 안퍽퍽하가",
          "original_form": "부드럽고 안퍽퍽하가",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100025011.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.12",
          "form": "소세지 사놓은 거 있는데",
          "original_form": "소세지 사놓은 거 있는데",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.13",
          "form": "근데 닭가.슴살 소세지는",
          "original_form": "근데 닭가.슴살 소세지는",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.14",
          "form": "프랑크아니면 못먹겠더라고요 식감이 너무...ㅠㅠㅠㅠ",
          "original_form": "프랑크아니면 못먹겠더라고요 식감이 너무...ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.15",
          "form": "아 글고보니 하림이닭 한달에 두세번",
          "original_form": "아 글고보니 하림이닭 한달에 두세번",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.16",
          "form": "오리지날 990원 할인해요",
          "original_form": "오리지날 990원 할인해요",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.17",
          "form": "그때사드세용ㅋㄱㅋㅋㅋ",
          "original_form": "그때사드세용ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.18",
          "form": "거기서파는 닭가.슴살 핫도그는 정말 속세맛",
          "original_form": "거기서파는 닭가.슴살 핫도그는 정말 속세맛",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100025011.1.19",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.20",
          "form": "그리고 닭가.슴살 슬라이스햄도 일반햄맛",
          "original_form": "그리고 닭가.슴살 슬라이스햄도 일반햄맛",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.21",
          "form": "핫도그 그거",
          "original_form": "핫도그 그거",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.22",
          "form": "와 햄도",
          "original_form": "와 햄도",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.23",
          "form": "먹어봤거든요?!?!?!",
          "original_form": "먹어봤거든요?!?!?!",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.24",
          "form": "아니 그건진짜 그냥일반핫도그예요",
          "original_form": "아니 그건진짜 그냥일반핫도그예요",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.25",
          "form": "미쳤어",
          "original_form": "미쳤어",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.26",
          "form": "핫도그 진짜 맛있죠 ㅋㅋㄱㅋㅋ",
          "original_form": "핫도그 진짜 맛있죠 ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.27",
          "form": "근데 그럴만한게",
          "original_form": "근데 그럴만한게",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.28",
          "form": "다이어트용 아닌게 분명한...",
          "original_form": "다이어트용 아닌게 분명한...",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.29",
          "form": "양이작아서 다이어트인가..",
          "original_form": "양이작아서 다이어트인가..",
          "speaker_id": "2",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.30",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.31",
          "form": "쪼.끄.만.한.데",
          "original_form": "쪼.끄.만.한.데",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100025011.1.32",
          "form": "아니 저기서 대체 뭐가 나쁜말이지 암튼",
          "original_form": "아니 저기서 대체 뭐가 나쁜말이지 암튼",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100025011.1.33",
          "form": "완전 그건 핑거푸드 아닌가요??",
          "original_form": "완전 그건 핑거푸드 아닌가요??",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100025011.1.34",
          "form": "한입거리죠 한입거리...",
          "original_form": "한입거리죠 한입거리...",
          "speaker_id": "2",
          "time": "20210831 22:11"
        }
      ]
