      "utterance": [
        {
          "id": "MDRW2100036308.1.1",
          "form": "요새당근올린거없어용?",
          "original_form": "요새당근올린거없어용?",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100036308.1.2",
          "form": "네 요즘은 없어요",
          "original_form": "네 요즘은 없어요",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036308.1.3",
          "form": "안팔려요",
          "original_form": "안팔려요",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100036308.1.4",
          "form": "맞아요 얼마나후려쳐야팔릴까요..",
          "original_form": "맞아요 얼마나후려쳐야팔릴까요..",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036308.1.5",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036308.1.6",
          "form": "공짜나눔하면 득달같이 연락오겟지",
          "original_form": "공짜나눔하면 득달같이 연락오겟지",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100036308.1.7",
          "form": "1분안에연락옵니다.ㅇㅈㄹ",
          "original_form": "1분안에연락옵니다.ㅇㅈㄹ",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100036308.1.8",
          "form": "맞아요 슬포",
          "original_form": "맞아요 슬포",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036308.1.9",
          "form": "그치만끊을수없는당근쓰",
          "original_form": "그치만끊을수없는당근쓰",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036308.1.10",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036308.1.11",
          "form": "버릴바에야 파능게낫죠",
          "original_form": "버릴바에야 파능게낫죠",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036308.1.12",
          "form": "그렇죠 맞는말이옵니다",
          "original_form": "그렇죠 맞는말이옵니다",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036308.1.13",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036308.1.14",
          "form": "사질말아야하능데 ㅋㅋ",
          "original_form": "사질말아야하능데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036308.1.15",
          "form": "ㅋㅋㅋㅋㅋ아 그거부터가문제구나ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 그거부터가문제구나ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036308.1.16",
          "form": "알수업ㄱ는 소비의 세계",
          "original_form": "알수업ㄱ는 소비의 세계",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036308.1.17",
          "form": "ㅋㅋㅋㅋㅋ 끊을수없어요.",
          "original_form": "ㅋㅋㅋㅋㅋ 끊을수없어요.",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
