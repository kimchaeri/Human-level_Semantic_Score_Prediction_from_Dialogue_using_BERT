      "utterance": [
        {
          "id": "MDRW2100023147.1.1",
          "form": "오 종교 있으세요?",
          "original_form": "오 종교 있으세요?",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100023147.1.2",
          "form": "저는 무교입니다ㅎ",
          "original_form": "저는 무교입니다ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100023147.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100023147.1.4",
          "form": "저는 일단 카톨릭입니다",
          "original_form": "저는 일단 카톨릭입니다",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100023147.1.5",
          "form": "일단은 ㅋㅋ",
          "original_form": "일단은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100023147.1.6",
          "form": "아니 왜 일단은이죠ㅎㅎ",
          "original_form": "아니 왜 일단은이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100023147.1.7",
          "form": "냉담자인가용?",
          "original_form": "냉담자인가용?",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100023147.1.8",
          "form": "오 무교이신분이",
          "original_form": "오 무교이신분이",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100023147.1.9",
          "form": "전문어를 구사하셨어!",
          "original_form": "전문어를 구사하셨어!",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100023147.1.10",
          "form": "네 냉담자요ㅜ",
          "original_form": "네 냉담자요ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100023147.1.11",
          "form": "ㅎㅎ 저도 세례명은 있거든요ㅎ",
          "original_form": "ㅎㅎ 저도 세례명은 있거든요ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100023147.1.12",
          "form": "그럼 유아세례받으신 건가요",
          "original_form": "그럼 유아세례받으신 건가요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100023147.1.13",
          "form": "넵넵 저는 영성체까지만했어요",
          "original_form": "넵넵 저는 영성체까지만했어요",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100023147.1.14",
          "form": "그럼 같이 냉담자인가요?ㅋㅋ",
          "original_form": "그럼 같이 냉담자인가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100023147.1.15",
          "form": "ㅎㅎ 그렇죠",
          "original_form": "ㅎㅎ 그렇죠",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100023147.1.16",
          "form": "독실한 친구하나가 절 그리 부름ㅎ",
          "original_form": "독실한 친구하나가 절 그리 부름ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100023147.1.17",
          "form": "영성체 시험 어렵지 않나용",
          "original_form": "영성체 시험 어렵지 않나용",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100023147.1.18",
          "form": "저는 어릴때? 했어서 근데 ㅋㅋ 사도신경 못외워서ㅜㅜ스티커못받았던기억이나요ㅜ",
          "original_form": "저는 어릴때? 했어서 근데 ㅋㅋ 사도신경 못외워서ㅜㅜ스티커못받았던기억이나요ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100023147.1.19",
          "form": "아ㅋㄱㄴ 그 밀가루가...먹고 싶었죠",
          "original_form": "아ㅋㄱㄴ 그 밀가루가...먹고 싶었죠",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100023147.1.20",
          "form": "근데 어릴때 다같이 해서 그런지",
          "original_form": "근데 어릴때 다같이 해서 그런지",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100023147.1.21",
          "form": "어찌어찌 통과시켜주더라고요",
          "original_form": "어찌어찌 통과시켜주더라고요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100023147.1.22",
          "form": "아니ㅋㄱㄴ 혈연 지연 이런 연은 없어져야 합니다!",
          "original_form": "아니ㅋㄱㄴ 혈연 지연 이런 연은 없어져야 합니다!",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100023147.1.23",
          "form": "근데 사도신경이 중간에 있었는데 ㅋ그거빼고 다외웠는데",
          "original_form": "근데 사도신경이 중간에 있었는데 ㅋ그거빼고 다외웠는데",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100023147.1.24",
          "form": "나름 서러웠던 기억은 나요",
          "original_form": "나름 서러웠던 기억은 나요",
          "speaker_id": "2",
          "time": "20210901 23:10"
        }
      ]
    }
  ]
