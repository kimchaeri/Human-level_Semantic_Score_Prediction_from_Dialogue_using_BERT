      "utterance": [
        {
          "id": "MDRW2100038662.1.1",
          "form": "아직 저녁 식사 전이신가요?",
          "original_form": "아직 저녁 식사 전이신가요?",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100038662.1.2",
          "form": "네에 이제 퇴근해요ㅠㅠ",
          "original_form": "네에 이제 퇴근해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038662.1.3",
          "form": "너무 수고하셨어요ㅠㅠ 맛있는 거 챙겨드세요!",
          "original_form": "너무 수고하셨어요ㅠㅠ 맛있는 거 챙겨드세요!",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100038662.1.4",
          "form": "그래야죵!!!",
          "original_form": "그래야죵!!!",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038662.1.5",
          "form": "식사 주로 직접 해 드세요?",
          "original_form": "식사 주로 직접 해 드세요?",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038662.1.6",
          "form": "할머니랑 같이 사는데 할머니가 거의 해주세요ㅎㅎ",
          "original_form": "할머니랑 같이 사는데 할머니가 거의 해주세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100038662.1.7",
          "form": "혼자 먹을 때만 가끔 해먹어요",
          "original_form": "혼자 먹을 때만 가끔 해먹어요",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100038662.1.8",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038662.1.9",
          "form": "요즘 배달도 넘 잘해주잖아요",
          "original_form": "요즘 배달도 넘 잘해주잖아요",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038662.1.10",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100038662.1.11",
          "form": "배달 음식 자주 드시나요?",
          "original_form": "배달 음식 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038662.1.12",
          "form": "일주일에 두세번은 시켜먹는것 같아요!",
          "original_form": "일주일에 두세번은 시켜먹는것 같아요!",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038662.1.13",
          "form": "주러 뭐 시켜드세요?",
          "original_form": "주러 뭐 시켜드세요?",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038662.1.14",
          "form": "흠 치킨 피자 기본이고",
          "original_form": "흠 치킨 피자 기본이고",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100038662.1.15",
          "form": "야채곱창 많이 먹어요!!",
          "original_form": "야채곱창 많이 먹어요!!",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100038662.1.16",
          "form": "곱창 너무 맛있죠!!",
          "original_form": "곱창 너무 맛있죠!!",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100038662.1.17",
          "form": "치즈 추가해서 먹는 거 좋아해요ㅎㅎ",
          "original_form": "치즈 추가해서 먹는 거 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100038662.1.18",
          "form": "완전",
          "original_form": "완전",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038662.1.19",
          "form": "그리고 계란찜도 먹어야해요",
          "original_form": "그리고 계란찜도 먹어야해요",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100038662.1.20",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100038662.1.21",
          "form": "보들보들 계란찜 너무 맛있어요",
          "original_form": "보들보들 계란찜 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100038662.1.22",
          "form": "ㅋㅋㅋ그런곳에서 파는 계란찜은 뭔가 달라요",
