      "utterance": [
        {
          "id": "MDRW2100035934.1.1",
          "form": "긍까유",
          "original_form": "긍까유",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.2",
          "form": "울엄마 파김치두 맛잇는거같아 ㅋㅋㅋ",
          "original_form": "울엄마 파김치두 맛잇는거같아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.3",
          "form": "그리고 치사하게 31일까지가 뭐야!!",
          "original_form": "그리고 치사하게 31일까지가 뭐야!!",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.4",
          "form": "경주가 맛잇다해서 엄마가 뿌듯해함",
          "original_form": "경주가 맛잇다해서 엄마가 뿌듯해함",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.5",
          "form": "그랴? 내일이면 땡이넹 ㅋㅋ",
          "original_form": "그랴? 내일이면 땡이넹 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.6",
          "form": "이번주 내내도 아니고!!",
          "original_form": "이번주 내내도 아니고!!",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.7",
          "form": "ㅋㅋㅋ경주 입맛 까다롭나요?",
          "original_form": "ㅋㅋㅋ경주 입맛 까다롭나요?",
          "speaker_id": "1",
          "time": "20210830 11:58"
        },
        {
          "id": "MDRW2100035934.1.8",
          "form": "어떠하다 저떠하다 ㅋㅋㅋ말해",
          "original_form": "어떠하다 저떠하다 ㅋㅋㅋ말해",
          "speaker_id": "2",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035934.1.9",
          "form": "아빠한테도 인정받으면 좋아햐 ㅋㅋㅋㅋ",
          "original_form": "아빠한테도 인정받으면 좋아햐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035934.1.10",
          "form": "낼만지나면 9월이당",
          "original_form": "낼만지나면 9월이당",
          "speaker_id": "2",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035934.1.11",
          "form": "ㅋㅋㅋㅋ울엄마 내가 말하면 하여간 못하는게 말은 제일 많다고",
          "original_form": "ㅋㅋㅋㅋ울엄마 내가 말하면 하여간 못하는게 말은 제일 많다고",
          "speaker_id": "1",
          "time": "20210830 11:59"
        },
        {
          "id": "MDRW2100035934.1.12",
          "form": "ㅋㅋㅋ와우 시간 너무 빨라 벌써 12시네",
          "original_form": "ㅋㅋㅋ와우 시간 너무 빨라 벌써 12시네",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100035934.1.13",
          "form": "ㅋㅋㅋㅋ원래 그렇쥬",
          "original_form": "ㅋㅋㅋㅋ원래 그렇쥬",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100035934.1.14",
          "form": "그러게 아까 11시엿는디",
          "original_form": "그러게 아까 11시엿는디",
          "speaker_id": "2",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100035934.1.15",
          "form": "ㅋㅋㅋ올때 메로나 말고 올때 짜파게티로 바꿔야겠다",
          "original_form": "ㅋㅋㅋ올때 메로나 말고 올때 짜파게티로 바꿔야겠다",
          "speaker_id": "1",
          "time": "20210830 12:00"
        },
        {
          "id": "MDRW2100035934.1.16",
          "form": "ㅋㅋㅋㅋ메로나 추억의 아스크림이다",
          "original_form": "ㅋㅋㅋㅋ메로나 추억의 아스크림이다",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100035934.1.17",
          "form": "투썸오더로 주문넣음?",
          "original_form": "투썸오더로 주문넣음?",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100035934.1.18",
          "form": "ㅋㅋ아닐걸? 엄마 그냥 가서 주문한다고 기프티콘만 줬어!",
          "original_form": "ㅋㅋ아닐걸? 엄마 그냥 가서 주문한다고 기프티콘만 줬어!",
          "speaker_id": "1",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100035934.1.19",
          "form": "난 전에 오더주문넣고 찾아오기만함 !",
          "original_form": "난 전에 오더주문넣고 찾아오기만함 !",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100035934.1.20",
          "form": "그니깐 하트도하나생겻더라궁",
          "original_form": "그니깐 하트도하나생겻더라궁",
          "speaker_id": "2",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100035934.1.21",
          "form": "ㅋㅋㅋ오 이런! 하트 날렸구먼!!",
          "original_form": "ㅋㅋㅋ오 이런! 하트 날렸구먼!!",
          "speaker_id": "1",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100035934.1.22",
          "form": "담엔 오더로 해야겠어",
          "original_form": "담엔 오더로 해야겠어",
