      "utterance": [
        {
          "id": "MDRW2100033843.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033843.1.2",
          "form": "저 치킨먹었어요ㅋㅋㅋ",
          "original_form": "저 치킨먹었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033843.1.3",
          "form": "벌써 드셨어요?",
          "original_form": "벌써 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033843.1.4",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100033843.1.5",
          "form": "배가 고파서요",
          "original_form": "배가 고파서요",
          "speaker_id": "2",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100033843.1.6",
          "form": "이 시간에 치킨 배달되나요?",
          "original_form": "이 시간에 치킨 배달되나요?",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100033843.1.7",
          "form": "저희는 5ㅅㅣ넘어야 배달되더라구여ㅠ",
          "original_form": "저희는 5ㅅㅣ넘어야 배달되더라구여ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100033843.1.8",
          "form": "어제 밤에 먹다가남은거요ㅋㅋㅋ",
          "original_form": "어제 밤에 먹다가남은거요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100033843.1.9",
          "form": "처갓집치킨이랍니다",
          "original_form": "처갓집치킨이랍니다",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100033843.1.10",
          "form": "아ㅋㅋㅋ 치킨은\r\n담날 먹는게 또 꿀맛이죵",
          "original_form": "아ㅋㅋㅋ 치킨은\r\n담날 먹는게 또 꿀맛이죵{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100033843.1.11",
          "form": "양념드셨어요?",
          "original_form": "양념드셨어요?",
          "speaker_id": "1",
          "time": "20210825 12:27"
        },
        {
          "id": "MDRW2100033843.1.12",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100033843.1.13",
          "form": "슈프림양념치킨이요!",
          "original_form": "슈프림양념치킨이요!",
          "speaker_id": "2",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100033843.1.14",
          "form": "아 처갓집은 슈프림이 대세죠ㅋㅋ",
          "original_form": "아 처갓집은 슈프림이 대세죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:39"
        },
        {
          "id": "MDRW2100033843.1.15",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033843.1.16",
          "form": "시그니쳐죠!",
          "original_form": "시그니쳐죠!",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033843.1.17",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100033843.1.18",
          "form": "사랑해요",
          "original_form": "사랑해요{emoji:♡}{emoji:♡}{emoji:♡}{emoji:♡}{emoji:♡}{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100033843.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100033843.1.20",
          "form": "어떤브랜드 좋아하세요?",
          "original_form": "어떤브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 12:58"
        },
        {
          "id": "MDRW2100033843.1.21",
          "form": "떡볶이요?ㅋㅋ",
          "original_form": "떡볶이요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033843.1.22",
          "form": "오빠네 떡볶이 아세요?ㅋㅋ",
          "original_form": "오빠네 떡볶이 아세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:04"
        },
        {
          "id": "MDRW2100033843.1.23",
          "form": "그게뭐죠?!!!",
          "original_form": "그게뭐죠?!!!",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100033843.1.24",
          "form": "옛날 떡볶인데",
          "original_form": "옛날 떡볶인데",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100033843.1.25",
          "form": "초등학교때 먹던 맛이라\r\n종종  먹음 맛있어요ㅋㅋ",
          "original_form": "초등학교때 먹던 맛이라\r\n종종  먹음 맛있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100033843.1.26",
          "form": "달쫀은 아세요?",
          "original_form": "달쫀은 아세요?",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100033843.1.27",
          "form": "달쫀은 뭐죠?!!!",
          "original_form": "달쫀은 뭐죠?!!!",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100033843.1.28",
          "form": "다 체인점인가요?",
          "original_form": "다 체인점인가요?",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100033843.1.29",
          "form": "네 체인인데....ㅋㅋ",
          "original_form": "네 체인인데....ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033843.1.30",
          "form": "제가 사는 쪽에만 있는 걸까요? ㅠㅋㅋ",
          "original_form": "제가 사는 쪽에만 있는 걸까요? ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100033843.1.31",
          "form": "ㅠㅠ처음듣네용",
          "original_form": "ㅠㅠ처음듣네용",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033843.1.32",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033843.1.33",
          "form": "그러게요:)\r\n두군데 다 맛있는데\r\n함께할수 없어 아쉽네요",
          "original_form": "그러게요:)\r\n두군데 다 맛있는데\r\n함께할수 없어 아쉽네요",
