        {
          "id": "MDRW2100028169.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028169.1.2",
          "form": "방금 먹었어요",
          "original_form": "방금 먹었어요",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028169.1.3",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028169.1.4",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100028169.1.5",
          "form": "너무 늦지않게 드셔요!",
          "original_form": "너무 늦지않게 드셔요!",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028169.1.6",
          "form": "뭐 드실지 정해두셨어요?",
          "original_form": "뭐 드실지 정해두셨어요?",
          "speaker_id": "2",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100028169.1.7",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210902 18:16"
        },
        {
          "id": "MDRW2100028169.1.8",
          "form": "치킨은 짱이죠",
          "original_form": "치킨은 짱이죠",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028169.1.9",
          "form": "완전 훌륭한 선택이네요!",
          "original_form": "완전 훌륭한 선택이네요!",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028169.1.10",
          "form": "네 ㅋ",
          "original_form": "네 ㅋ",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028169.1.11",
          "form": "저는 두부 스테이크 먹었어요",
          "original_form": "저는 두부 스테이크 먹었어요",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028169.1.12",
          "form": "스테이크보단 동그랑땡 같은 음식이기는 한데 맛있더라구요",
          "original_form": "스테이크보단 동그랑땡 같은 음식이기는 한데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028169.1.13",
          "form": "맛나겠네녀",
          "original_form": "맛나겠네녀",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028169.1.14",
          "form": "ㅎㅎ 맛있어요",
          "original_form": "ㅎㅎ 맛있어요",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100028169.1.15",
          "form": "이제는 식사 하셨나요??",
          "original_form": "이제는 식사 하셨나요??",
          "speaker_id": "2",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028169.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028169.1.17",
          "form": "어떤 치킨 드셨어요?",
          "original_form": "어떤 치킨 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 20:59"
