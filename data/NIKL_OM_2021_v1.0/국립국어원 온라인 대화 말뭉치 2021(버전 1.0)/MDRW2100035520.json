      "utterance": [
        {
          "id": "MDRW2100035520.1.1",
          "form": "야채는 어떤거 싫어하세요?",
          "original_form": "야채는 어떤거 싫어하세요?",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100035520.1.2",
          "form": "그린빈이요!",
          "original_form": "그린빈이요!",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035520.1.3",
          "form": "저도 콩류는 싹다 싫어하는데 !",
          "original_form": "저도 콩류는 싹다 싫어하는데 !",
          "speaker_id": "2",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100035520.1.4",
          "form": "이유가 있나요?",
          "original_form": "이유가 있나요?",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100035520.1.5",
          "form": "냉동야채에 그린빈 있어서 처음 먹었는데 말로 표현 못 할 맛이였어요",
          "original_form": "냉동야채에 그린빈 있어서 처음 먹었는데 말로 표현 못 할 맛이였어요",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100035520.1.6",
          "form": "아 ㅋㄱㅋㅋ 그 맛 알죠 ...",
          "original_form": "아 ㅋㄱㅋㅋ 그 맛 알죠 ...",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100035520.1.7",
          "form": "전 냉동야채로 나오는건 싹 다 싫어해요",
          "original_form": "전 냉동야채로 나오는건 싹 다 싫어해요",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100035520.1.8",
          "form": "당근 콩 .. 브로콜리 ...",
          "original_form": "당근 콩 .. 브로콜리 ...",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100035520.1.9",
          "form": "저도요..  냉동야채 맛이 좀 떨어지긴하더라구요",
          "original_form": "저도요..  냉동야채 맛이 좀 떨어지긴하더라구요",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035520.1.10",
          "form": "아 브로콜리도 싫네요",
          "original_form": "아 브로콜리도 싫네요",
          "speaker_id": "1",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100035520.1.11",
          "form": "브로콜리는 심지어",
          "original_form": "브로콜리는 심지어",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035520.1.12",
          "form": "더럽데요 그 머리?가 ..",
          "original_form": "더럽데요 그 머리?가 ..",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035520.1.13",
          "form": "으으.. 진짜요?",
          "original_form": "으으.. 진짜요?",
          "speaker_id": "1",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100035520.1.14",
          "form": "너무 충격이네요..",
          "original_form": "너무 충격이네요..",
          "speaker_id": "1",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100035520.1.15",
          "form": "네 벌레가.. 숨어있다구 ...",
          "original_form": "네 벌레가.. 숨어있다구 ...",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100035520.1.16",
          "form": "생으론 절대 드시지마셔요 ㅋㅋ",
          "original_form": "생으론 절대 드시지마셔요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100035520.1.17",
          "form": "와.. 저 안씻고 먹었는데",
          "original_form": "와.. 저 안씻고 먹었는데",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100035520.1.18",
          "form": "이런..",
          "original_form": "이런..",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100035520.1.19",
          "form": "헐 안돼요 ,,,",
          "original_form": "헐 안돼요 ,,,",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100035520.1.20",
          "form": "꼭 씻어서 드세요 .. !",
          "original_form": "꼭 씻어서 드세요 .. !",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035520.1.21",
          "form": "저도 예전엔 안씻고 먹었는데 하하 ...",
          "original_form": "저도 예전엔 안씻고 먹었는데 하하 ...",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035520.1.22",
          "form": "와..지금부터라도 씻어먹어야겠어요",
          "original_form": "와..지금부터라도 씻어먹어야겠어요",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100035520.1.23",
          "form": "name2님 아님 평생 안씻어먹었었을거에요",
          "original_form": "&name2&님 아님 평생 안씻어먹었었을거에요",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100035520.1.24",
