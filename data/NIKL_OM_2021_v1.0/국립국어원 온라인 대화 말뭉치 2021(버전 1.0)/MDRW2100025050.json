      "utterance": [
        {
          "id": "MDRW2100025050.1.1",
          "form": "저희도 차타고 20분ㅇ어서",
          "original_form": "저희도 차타고 20분ㅇ어서",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.2",
          "form": "역시 사람이 잘 먹고 살려면",
          "original_form": "역시 사람이 잘 먹고 살려면",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.3",
          "form": "가끔가는데",
          "original_form": "가끔가는데",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.4",
          "form": "대도시에 있어야 하는 거 같아요",
          "original_form": "대도시에 있어야 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.5",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.6",
          "form": "스타필드 코스트코 이마트 다 몰려있어요",
          "original_form": "스타필드 코스트코 이마트 다 몰려있어요",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.7",
          "form": "저희는 스타필드밖에 없어요ㅠㅠㅠ",
          "original_form": "저희는 스타필드밖에 없어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.8",
          "form": "이것도 다 최근에 생겼더라구요",
          "original_form": "이것도 다 최근에 생겼더라구요",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025050.1.9",
          "form": "코스트코 가려면 용인으로 가능..",
          "original_form": "코스트코 가려면 용인으로 가능..",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.10",
          "form": "이전엔 진짜 산바로옆에 조용한 동네였는데...",
          "original_form": "이전엔 진짜 산바로옆에 조용한 동네였는데...",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.11",
          "form": "맥도날드도 버거킹도없던...",
          "original_form": "맥도날드도 버거킹도없던...",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.12",
          "form": "띠로리...",
          "original_form": "띠로리...",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.13",
          "form": "나가드시면",
          "original_form": "나가드시면",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.14",
          "form": "요샌 맛짇도많이생기고",
          "original_form": "요샌 맛짇도많이생기고",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.15",
          "form": "주로 뭐 제일 자주 드세요?",
          "original_form": "주로 뭐 제일 자주 드세요?",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025050.1.16",
          "form": "저는 안가리고먹는편인데",
          "original_form": "저는 안가리고먹는편인데",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025050.1.17",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025050.1.18",
          "form": "요샌 샤브샤브같은게 제일 좋더라구요",
          "original_form": "요샌 샤브샤브같은게 제일 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025050.1.19",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025050.1.20",
          "form": "훠궈같은것도 좋아하고",
          "original_form": "훠궈같은것도 좋아하고",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025050.1.21",
          "form": "순한맛에 빠지셨나요",
          "original_form": "순한맛에 빠지셨나요",
          "speaker_id": "1",
          "time": "20210831 22:46"
