      "utterance": [
        {
          "id": "MDRW2100023005.1.1",
          "form": "안녕하세요 name2",
          "original_form": "안녕하세요 &name2&",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100023005.1.2",
          "form": "날씨가 좋죠?",
          "original_form": "날씨가 좋죠?",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023005.1.3",
          "form": "네 ㅎ 더위도 가시는거같고 좋네여",
          "original_form": "네 ㅎ 더위도 가시는거같고 좋네여",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023005.1.4",
          "form": "사계절중 어떤계절이 좋아요?",
          "original_form": "사계절중 어떤계절이 좋아요?",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023005.1.5",
          "form": "여름이요",
          "original_form": "여름이요",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023005.1.6",
          "form": "오감이 만족되는 계절 아닙니까?",
          "original_form": "오감이 만족되는 계절 아닙니까?",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023005.1.7",
          "form": "하하 ㅎㅎ 전 봄이 ㅎ\r\n꽃이 피는 설렘이라던지 좋더라구요",
          "original_form": "하하 ㅎㅎ 전 봄이 ㅎ\r\n꽃이 피는 설렘이라던지 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023005.1.8",
          "form": "아~ 꽃이요 로멘티스트셨네",
          "original_form": "아~ 꽃이요 로멘티스트셨네",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023005.1.9",
          "form": "저 집에서 꽃키우는데",
          "original_form": "저 집에서 꽃키우는데",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023005.1.10",
          "form": "방울토마톸ㅋㅋㅋㅋ 꽃이 폈어요 아마 한두달쯤이면 열매를 수확할수있을것같네요",
          "original_form": "방울토마톸ㅋㅋㅋㅋ 꽃이 폈어요 아마 한두달쯤이면 열매를 수확할수있을것같네요",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023005.1.11",
          "form": "우오",
          "original_form": "우오",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023005.1.12",
          "form": "꽃을 좋아하는데 키워놓은 꽃은 다 죽어버렸습니다ㅜ",
          "original_form": "꽃을 좋아하는데 키워놓은 꽃은 다 죽어버렸습니다ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023005.1.13",
          "form": "플라워슬레이어 십니까",
          "original_form": "플라워슬레이어 십니까",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023005.1.14",
          "form": "물잘주고 햇빛잘쬐여 주니까 알아서 잘크던데",
          "original_form": "물잘주고 햇빛잘쬐여 주니까 알아서 잘크던데",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023005.1.15",
          "form": "너무 물을 많이 준게 아닐까??? 생각합니다",
          "original_form": "너무 물을 많이 준게 아닐까??? 생각합니다",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023005.1.16",
          "form": "난이도가 높은 꽃을 키우신거 아니예요?",
          "original_form": "난이도가 높은 꽃을 키우신거 아니예요?",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023005.1.17",
          "form": "초보자한테 추천할만한 식물같은걸 키우시면 잘 자랄거요?",
          "original_form": "초보자한테 추천할만한 식물같은걸 키우시면 잘 자랄거요?",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023005.1.18",
          "form": "걸요",
          "original_form": "걸요",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023005.1.19",
          "form": "선물 받은건데 ㅜㅜ 글쎄요ㅜ",
          "original_form": "선물 받은건데 ㅜㅜ 글쎄요ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023005.1.20",
          "form": "꽃집에서 가장 기르기 쉬운 꽃 사왔다고 했는데ㅜ친구가",
          "original_form": "꽃집에서 가장 기르기 쉬운 꽃 사왔다고 했는데ㅜ친구가",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023005.1.21",
          "form": "꽃을 선물한것은 사심이 있다는게지요",
          "original_form": "꽃을 선물한것은 사심이 있다는게지요",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023005.1.22",
          "form": "앗 ㅎㅎ 동성친구요",
          "original_form": "앗 ㅎㅎ 동성친구요",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023005.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023005.1.24",
          "form": "여름이라 그런지 \r\n겨울에 샀던 식물하나는 잘 살아있습니다!",
          "original_form": "여름이라 그런지 \r\n겨울에 샀던 식물하나는 잘 살아있습니다!",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023005.1.25",
          "form": "다행이지요",
          "original_form": "다행이지요",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023005.1.26",
          "form": "가을장마도 끝난거같아 좋습니다",
          "original_form": "가을장마도 끝난거같아 좋습니다",
          "speaker_id": "1",
          "time": "20210829 20:34"
        }
      ]
    }
