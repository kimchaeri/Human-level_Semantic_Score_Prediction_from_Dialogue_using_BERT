      "utterance": [
        {
          "id": "MDRW2100034467.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034467.1.2",
          "form": "점심을 먹었습니당",
          "original_form": "점심을 먹었습니당",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100034467.1.3",
          "form": "name1님 점심드셨나요",
          "original_form": "&name1&님 점심드셨나요",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100034467.1.4",
          "form": "네 점심 먹었네요~",
          "original_form": "네 점심 먹었네요~",
          "speaker_id": "1",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100034467.1.5",
          "form": "점심 뭐드셨나요?",
          "original_form": "점심 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100034467.1.6",
          "form": "저는 치즈스틱과 핫도그를 먹었어요",
          "original_form": "저는 치즈스틱과 핫도그를 먹었어요",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100034467.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100034467.1.8",
          "form": "맛있었을 것 같아여 ..",
          "original_form": "맛있었을 것 같아여 ..",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100034467.1.9",
          "form": "저는 연어포케 먹었어요!!",
          "original_form": "저는 연어포케 먹었어요!!",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100034467.1.10",
          "form": "오 포케는 처음들어보는 음식이네요!",
          "original_form": "오 포케는 처음들어보는 음식이네요!",
          "speaker_id": "2",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100034467.1.11",
          "form": "맛있으셨나요",
          "original_form": "맛있으셨나요",
          "speaker_id": "2",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100034467.1.12",
          "form": "네네 연어덮밥같은건데 맛있었어용ㅎㅎ",
          "original_form": "네네 연어덮밥같은건데 맛있었어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100034467.1.13",
          "form": "담에 한번 먹어봐야겠네요ㅋㅋㅋ",
          "original_form": "담에 한번 먹어봐야겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100034467.1.14",
          "form": "오훈데 커피는 드셨나요",
          "original_form": "오훈데 커피는 드셨나요",
          "speaker_id": "2",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100034467.1.15",
          "form": "커피말고 밀크티 마셨어요ㅎㅎ",
          "original_form": "커피말고 밀크티 마셨어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100034467.1.16",
          "form": "커피 드셨나요?",
          "original_form": "커피 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100034467.1.17",
          "form": "네ㅋㅋ 전 아아 먹었어요",
          "original_form": "네ㅋㅋ 전 아아 먹었어요",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100034467.1.18",
          "form": "밀크티 저도 좋아합니당",
          "original_form": "밀크티 저도 좋아합니당",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100034467.1.19",
          "form": "밀크티 맛있죠 저도 커피 먹고싶은데 커피만 마심 잠이 안와서 못먹어요 ㅠㅠ",
          "original_form": "밀크티 맛있죠 저도 커피 먹고싶은데 커피만 마심 잠이 안와서 못먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100034467.1.20",
          "form": "저런..ㅠㅠ 제 주위에도 카페인에 약한 분들이 종종 계시더라구요",
          "original_form": "저런..ㅠㅠ 제 주위에도 카페인에 약한 분들이 종종 계시더라구요",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100034467.1.21",
          "form": "카페에선 주로 뭘 드시나여",
          "original_form": "카페에선 주로 뭘 드시나여",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100034467.1.22",
          "form": "저는 주로 케이크 먹어용 ..ㅋㅋ 디저트위주로 먹습니다",
          "original_form": "저는 주로 케이크 먹어용 ..ㅋㅋ 디저트위주로 먹습니다",
