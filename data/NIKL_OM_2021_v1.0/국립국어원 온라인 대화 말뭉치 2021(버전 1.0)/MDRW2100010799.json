      "utterance": [
        {
          "id": "MDRW2100010799.1.1",
          "form": "넷플릭스 좋아하세나요",
          "original_form": "넷플릭스 좋아하세나요",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010799.1.2",
          "form": "네 가끔 봐요",
          "original_form": "네 가끔 봐요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010799.1.3",
          "form": "넷플릭스 구독하시나요",
          "original_form": "넷플릭스 구독하시나요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010799.1.4",
          "form": "저는 구독한지 한 3년됐어요",
          "original_form": "저는 구독한지 한 3년됐어요",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010799.1.5",
          "form": "미드 한드중에 뭘 더 많이보세요?",
          "original_form": "미드 한드중에 뭘 더 많이보세요?",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010799.1.6",
          "form": "저는 골고루 보는 편인데 해외거를 좀 더 자주본거같아요",
          "original_form": "저는 골고루 보는 편인데 해외거를 좀 더 자주본거같아요",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010799.1.7",
          "form": "아무래도 국내컨텐츠는 넷플아니어도 볼 수 있으니까요",
          "original_form": "아무래도 국내컨텐츠는 넷플아니어도 볼 수 있으니까요",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010799.1.8",
          "form": "맞아요 저도 그런것 같아요",
          "original_form": "맞아요 저도 그런것 같아요",
          "speaker_id": "1",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010799.1.9",
          "form": "최애 드라마가 뭐세요?",
          "original_form": "최애 드라마가 뭐세요?",
          "speaker_id": "1",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010799.1.10",
          "form": "저 빨간머리앤 잘봤어요",
          "original_form": "저 빨간머리앤 잘봤어요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010799.1.11",
          "form": "아 빨강머리앤 너무 좋죠 시즌1에서는 앤이 말이 너무 많아서 좀 진입이 힘들엇는데",
          "original_form": "아 빨강머리앤 너무 좋죠 시즌1에서는 앤이 말이 너무 많아서 좀 진입이 힘들엇는데",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010799.1.12",
          "form": "너무 재밋더라구요",
          "original_form": "너무 재밋더라구요",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010799.1.13",
          "form": "우울할 때 볼 넷플을 추천하신다면 뭐가잇을ㅋ가요",
          "original_form": "우울할 때 볼 넷플을 추천하신다면 뭐가잇을ㅋ가요",
          "speaker_id": "1",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010799.1.14",
          "form": "나를 차버린 스파이..?",
          "original_form": "나를 차버린 스파이..?",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010799.1.15",
          "form": "보셨납요",
          "original_form": "보셨납요",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010799.1.16",
          "form": "아 완전 봣죠 밀라쿠니스도 좋아하고 그 옆 언니도 좋아하거든요",
          "original_form": "아 완전 봣죠 밀라쿠니스도 좋아하고 그 옆 언니도 좋아하거든요",
          "speaker_id": "1",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010799.1.17",
          "form": "드라마는 아니고 영화에요",
          "original_form": "드라마는 아니고 영화에요",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010799.1.18",
          "form": "그 옆 언니 너무 매력적이더라구요",
          "original_form": "그 옆 언니 너무 매력적이더라구요",
          "speaker_id": "1",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010799.1.19",
          "form": "맞아요 너무 좋아요",
          "original_form": "맞아요 너무 좋아요",
          "speaker_id": "2",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100010799.1.20",
          "form": "킬링타임으로 딱이었어요",
          "original_form": "킬링타임으로 딱이었어요",
          "speaker_id": "2",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100010799.1.21",
          "form": "좋은 추천 감사함미더..",
          "original_form": "좋은 추천 감사함미더..",
          "speaker_id": "1",
          "time": "20210823 22:06"
        }
      ]
