        {
          "id": "MDRW2100020482.1.1",
          "form": "근데 넌 베라에서 머가 젤조아?",
          "original_form": "근데 넌 베라에서 머가 젤조아?",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.2",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.3",
          "form": "역시엄외!",
          "original_form": "역시엄외!",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.4",
          "form": "엄외가 짱이군",
          "original_form": "엄외가 짱이군",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.5",
          "form": "언닌?",
          "original_form": "언닌?",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.6",
          "form": "나는 아봉!",
          "original_form": "나는 아봉!",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.7",
          "form": "아몬드봉봉",
          "original_form": "아몬드봉봉",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.8",
          "form": "우린진짜",
          "original_form": "우린진짜",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.9",
          "form": "초코처돌이다 그치",
          "original_form": "초코처돌이다 그치",
          "speaker_id": "1",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.10",
          "form": "그런거같애",
          "original_form": "그런거같애",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020482.1.11",
          "form": "근데 너무 맛잇는걸..",
          "original_form": "근데 너무 맛잇는걸..",
          "speaker_id": "2",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020482.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020482.1.13",
          "form": "넘맛나",
          "original_form": "넘맛나",
          "speaker_id": "1",
          "time": "20210828 08:16"
        },
        {
          "id": "MDRW2100020482.1.14",
          "form": "초코는 어쩔수가없어",
          "original_form": "초코는 어쩔수가없어",
          "speaker_id": "2",
          "time": "20210828 08:16"
        },
        {
