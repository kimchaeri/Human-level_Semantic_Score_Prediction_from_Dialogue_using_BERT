      "utterance": [
        {
          "id": "MDRW2100045426.1.1",
          "form": "언니 유튜브같은건 많이 안 봐?",
          "original_form": "언니 유튜브같은건 많이 안 봐?",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045426.1.2",
          "form": "나는 너가 자빱티비 채널 콘텐츠를 꼭 봐줬으면좋겠어",
          "original_form": "나는 너가 자빱티비 채널 콘텐츠를 꼭 봐줬으면좋겠어",
          "speaker_id": "2",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045426.1.3",
          "form": "아 그거 챠언니도 재밌다던데",
          "original_form": "아 그거 챠언니도 재밌다던데",
          "speaker_id": "1",
          "time": "20210908 02:27"
        },
        {
          "id": "MDRW2100045426.1.4",
          "form": "시리즈가 있는거야??",
          "original_form": "시리즈가 있는거야??",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045426.1.5",
          "form": "응응 처음엔 입문하기 어렵게느껴질수도있는데",
          "original_form": "응응 처음엔 입문하기 어렵게느껴질수도있는데",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045426.1.6",
          "form": "한번 빠지면 진짜 너무재밌어서",
          "original_form": "한번 빠지면 진짜 너무재밌어서",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045426.1.7",
          "form": "시리즈도 다양하거든",
          "original_form": "시리즈도 다양하거든",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045426.1.8",
          "form": "너도 분명 재밌게볼듯!!",
          "original_form": "너도 분명 재밌게볼듯!!",
          "speaker_id": "2",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045426.1.9",
          "form": "오!! 그럼 봐야하는 순서같은거 있어?",
          "original_form": "오!! 그럼 봐야하는 순서같은거 있어?",
          "speaker_id": "1",
          "time": "20210908 02:28"
        },
        {
          "id": "MDRW2100045426.1.10",
          "form": "정리되어있는게있는데",
          "original_form": "정리되어있는게있는데",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045426.1.11",
          "form": "내 추천은",
          "original_form": "내 추천은",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045426.1.12",
          "form": "서리서막 혹은 자매꽃을 먼저보는걸 추천해",
          "original_form": "서리서막 혹은 자매꽃을 먼저보는걸 추천해",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045426.1.13",
          "form": "너는 자극적인 내용을 좋아할거같아서 ㅋㅋ",
          "original_form": "너는 자극적인 내용을 좋아할거같아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:29"
        },
        {
          "id": "MDRW2100045426.1.14",
          "form": "스테디시리즈는 우정리노트 시리즈야!",
          "original_form": "스테디시리즈는 우정리노트 시리즈야!",
          "speaker_id": "2",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045426.1.15",
          "form": "오!! 사전지식 하나도 없이 봐도 괜찮은거야?",
          "original_form": "오!! 사전지식 하나도 없이 봐도 괜찮은거야?",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045426.1.16",
          "form": "약간 이런 유튜브 자컨은 처음이라 두근거린당 ㅎㅎ",
          "original_form": "약간 이런 유튜브 자컨은 처음이라 두근거린당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 02:30"
        },
        {
          "id": "MDRW2100045426.1.17",
          "form": "두개는 사전지식없이 봐도 돼!!",
          "original_form": "두개는 사전지식없이 봐도 돼!!",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045426.1.18",
          "form": "우정리노트도 사전지식없어도돼!",
          "original_form": "우정리노트도 사전지식없어도돼!",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045426.1.19",
          "form": "오 좋아!! 한국어니까 작업하면서 틀어놓기에도 편하겠다",
          "original_form": "오 좋아!! 한국어니까 작업하면서 틀어놓기에도 편하겠다",
          "speaker_id": "1",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045426.1.20",
          "form": "사실 우정리가 첫번째로 보기 딱 좋은데 너한텐 초반이 조금 평화롭게 느껴질수있어",
          "original_form": "사실 우정리가 첫번째로 보기 딱 좋은데 너한텐 초반이 조금 평화롭게 느껴질수있어",
          "speaker_id": "2",
          "time": "20210908 02:31"
        },
        {
          "id": "MDRW2100045426.1.21",
          "form": "나는 애니메이션 같은건 일본어라 작업하면서 보기힘들었거든",
          "original_form": "나는 애니메이션 같은건 일본어라 작업하면서 보기힘들었거든",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045426.1.22",
          "form": "맞아맞아 작업영상으로 최적이야",
          "original_form": "맞아맞아 작업영상으로 최적이야",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045426.1.23",
          "form": "나도야",
          "original_form": "나도야",
          "speaker_id": "2",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045426.1.24",
          "form": "아ㅋㅋㅋㅋ 그치 나는 아무래도 극적인 전개를 좋아하니까",
          "original_form": "아ㅋㅋㅋㅋ 그치 나는 아무래도 극적인 전개를 좋아하니까",
          "speaker_id": "1",
          "time": "20210908 02:32"
        },
        {
          "id": "MDRW2100045426.1.25",
          "form": "자매꽃이 진짜 초반부터재밌어..",
          "original_form": "자매꽃이 진짜 초반부터재밌어..",
          "speaker_id": "2",
          "time": "20210908 02:32"
        }
      ]
    }
