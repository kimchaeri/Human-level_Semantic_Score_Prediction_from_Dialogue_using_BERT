      "utterance": [
        {
          "id": "MDRW2100037284.1.1",
          "form": "그걸 남한태 보여주고 살아요? 대단하네요 ㅡㅡ;;;",
          "original_form": "그걸 남한태 보여주고 살아요? 대단하네요 ㅡㅡ;;;",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037284.1.2",
          "form": "네... 인스타에 올라오더라구요...",
          "original_form": "네... 인스타에 올라오더라구요...",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037284.1.3",
          "form": "자식이 뭘 보고배울지 걱정되네요",
          "original_form": "자식이 뭘 보고배울지 걱정되네요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037284.1.4",
          "form": "그리고 또 다른 친구는",
          "original_form": "그리고 또 다른 친구는",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037284.1.5",
          "form": "자꾸 수술복 입은 남편 사진을 올려요",
          "original_form": "자꾸 수술복 입은 남편 사진을 올려요",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 걔 계정인지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 걔 계정인지",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.7",
          "form": "남편 계정인지",
          "original_form": "남편 계정인지",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.8",
          "form": "아 저 아는 언니도",
          "original_form": "아 저 아는 언니도",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.9",
          "form": "의사남친은 아닌데",
          "original_form": "의사남친은 아닌데",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.10",
          "form": "암튼 만나면 남친이야기만 하더라구요",
          "original_form": "암튼 만나면 남친이야기만 하더라구요",
          "speaker_id": "1",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.11",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.12",
          "form": "아니근데 그런 분들 너무 신기하지 않아요?",
          "original_form": "아니근데 그런 분들 너무 신기하지 않아요?",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.13",
          "form": "뉴스만 봐도",
          "original_form": "뉴스만 봐도",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.14",
          "form": "여혐 범죄가 얼마나 많고",
          "original_form": "여혐 범죄가 얼마나 많고",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037284.1.15",
          "form": "박사방... 성매매.. 이런거 보면",
          "original_form": "박사방... 성매매.. 이런거 보면",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037284.1.16",
          "form": "자기 남친 남편은 다를 줄 아나",
          "original_form": "자기 남친 남편은 다를 줄 아나",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037284.1.17",
          "form": "그니까요 어떤 아줌마 중에",
          "original_form": "그니까요 어떤 아줌마 중에",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037284.1.18",
          "form": "온라인으로 아는 아줌만데요",
          "original_form": "온라인으로 아는 아줌만데요",
          "speaker_id": "1",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037284.1.19",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037284.1.20",
          "form": "남편한테 자아의탁 하나요?",
          "original_form": "남편한테 자아의탁 하나요?",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037284.1.21",
          "form": "가끔 삼십분만 친구공개글",
          "original_form": "가끔 삼십분만 친구공개글",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.22",
          "form": "이러면서 우리남편 서울대교수",
          "original_form": "이러면서 우리남편 서울대교수",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.23",
          "form": "이러는 아줌마있었어요",
          "original_form": "이러는 아줌마있었어요",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 저걸 어디다 올려요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 저걸 어디다 올려요?",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.25",
          "form": "아니 근데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 근데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.26",
          "form": "제가 대학원생이라 교수님들 잘 아는데",
          "original_form": "제가 대학원생이라 교수님들 잘 아는데",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.27",
          "form": "교수들 진짜 더럽게 놀아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "교수들 진짜 더럽게 놀아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.28",
          "form": "모 sns사이트에요 ㅋㅋㅋ",
          "original_form": "모 sns사이트에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.29",
          "form": "어휴 진짜 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어휴 진짜 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037284.1.30",
          "form": "네 뭐 그리 믿진 않습니다",
          "original_form": "네 뭐 그리 믿진 않습니다",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037284.1.31",
          "form": "저희 학과는 어떤 남자 교수가 불륜 저질러서",
          "original_form": "저희 학과는 어떤 남자 교수가 불륜 저질러서",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037284.1.32",
          "form": "사모님이 애 데리고 와서",
          "original_form": "사모님이 애 데리고 와서",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037284.1.33",
          "form": "오피스 문 쾅쾅쾅 두들기면서 소리 질러가지고",
          "original_form": "오피스 문 쾅쾅쾅 두들기면서 소리 질러가지고",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037284.1.34",
          "form": "그 연구실 대학원생들이 달래느라 힘들었대요",
          "original_form": "그 연구실 대학원생들이 달래느라 힘들었대요",
          "speaker_id": "2",
          "time": "20210831 22:16"
        }
      ]
