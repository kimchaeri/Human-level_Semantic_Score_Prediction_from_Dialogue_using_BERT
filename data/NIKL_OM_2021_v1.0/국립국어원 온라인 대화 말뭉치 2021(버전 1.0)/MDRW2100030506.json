      "utterance": [
        {
          "id": "MDRW2100030506.1.1",
          "form": "바나나숨숨집",
          "original_form": "바나나숨숨집",
          "speaker_id": "2",
          "time": "20210904 02:20"
        },
        {
          "id": "MDRW2100030506.1.2",
          "form": "짱귀엽겠다",
          "original_form": "짱귀엽겠다",
          "speaker_id": "2",
          "time": "20210904 02:20"
        },
        {
          "id": "MDRW2100030506.1.3",
          "form": "귀여워...",
          "original_form": "귀여워...",
          "speaker_id": "1",
          "time": "20210904 02:20"
        },
        {
          "id": "MDRW2100030506.1.4",
          "form": "숨숨집 귀여운 거 너무 많아서",
          "original_form": "숨숨집 귀여운 거 너무 많아서",
          "speaker_id": "1",
          "time": "20210904 02:20"
        },
        {
          "id": "MDRW2100030506.1.5",
          "form": "갖고 싶은데...",
          "original_form": "갖고 싶은데...",
          "speaker_id": "1",
          "time": "20210904 02:20"
        },
        {
          "id": "MDRW2100030506.1.6",
          "form": "안 쓸 게 뻔함",
          "original_form": "안 쓸 게 뻔함",
          "speaker_id": "1",
          "time": "20210904 02:20"
        },
        {
          "id": "MDRW2100030506.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:21"
        },
        {
          "id": "MDRW2100030506.1.8",
          "form": "난... 타르트 방석...? 너무 탐남",
          "original_form": "난... 타르트 방석...? 너무 탐남",
          "speaker_id": "2",
          "time": "20210904 02:21"
        },
        {
          "id": "MDRW2100030506.1.9",
          "form": "님네 집 고영에게 사주고 싶음",
          "original_form": "님네 집 고영에게 사주고 싶음",
          "speaker_id": "2",
          "time": "20210904 02:21"
        },
        {
          "id": "MDRW2100030506.1.10",
          "form": "탐나...",
          "original_form": "탐나...",
          "speaker_id": "1",
          "time": "20210904 02:21"
        },
        {
          "id": "MDRW2100030506.1.11",
          "form": "안 쓰겠지...",
          "original_form": "안 쓰겠지...",
          "speaker_id": "1",
          "time": "20210904 02:21"
        },
        {
          "id": "MDRW2100030506.1.12",
          "form": "ㅎ...",
          "original_form": "ㅎ...",
          "speaker_id": "2",
          "time": "20210904 02:21"
        },
        {
          "id": "MDRW2100030506.1.13",
          "form": "쿠션이나 숨숨집 진짜",
          "original_form": "쿠션이나 숨숨집 진짜",
          "speaker_id": "1",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100030506.1.14",
          "form": "너무 안 쓸 것 같아서",
          "original_form": "너무 안 쓸 것 같아서",
          "speaker_id": "1",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100030506.1.15",
          "form": "맨날 중고로나 찾아봄",
          "original_form": "맨날 중고로나 찾아봄",
          "speaker_id": "1",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100030506.1.16",
          "form": "근데 진짜 안 쓸 것 같음",
          "original_form": "근데 진짜 안 쓸 것 같음",
          "speaker_id": "2",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100030506.1.17",
          "form": "거기 있느니 사람 무릎에 와서 누울 듯",
          "original_form": "거기 있느니 사람 무릎에 와서 누울 듯",
          "speaker_id": "2",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100030506.1.18",
          "form": "근데 의외로 집에 있는 베개 하나는 좋아한단 말이지",
          "original_form": "근데 의외로 집에 있는 베개 하나는 좋아한단 말이지",
          "speaker_id": "1",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030506.1.19",
          "form": "근데 그거 하나만 좋아함",
          "original_form": "근데 그거 하나만 좋아함",
          "speaker_id": "1",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030506.1.20",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030506.1.21",
          "form": "어느 부분이 좋은 건지 모르겠음",
          "original_form": "어느 부분이 좋은 건지 모르겠음",
          "speaker_id": "1",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030506.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030506.1.23",
          "form": "취향이 까다로우신 고여이",
          "original_form": "취향이 까다로우신 고여이",
          "speaker_id": "2",
          "time": "20210904 02:23"
        },
        {
          "id": "MDRW2100030506.1.24",
          "form": "좋아한다는 베개",
          "original_form": "좋아한다는 베개",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030506.1.25",
          "form": "고여이 모양 베개인가요",
          "original_form": "고여이 모양 베개인가요",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030506.1.26",
          "form": "아뇨 그거 말고",
          "original_form": "아뇨 그거 말고",
          "speaker_id": "1",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030506.1.27",
          "form": "약간 평평하고 시원한 재질 베개 있는데",
          "original_form": "약간 평평하고 시원한 재질 베개 있는데",
          "speaker_id": "1",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030506.1.28",
          "form": "그것만 위에 올라감",
          "original_form": "그것만 위에 올라감",
          "speaker_id": "1",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030506.1.29",
          "form": "취향을 알 수 없군",
          "original_form": "취향을 알 수 없군",
          "speaker_id": "2",
          "time": "20210904 02:25"
        },
        {
          "id": "MDRW2100030506.1.30",
          "form": "그냥 시원해서 그런가 하고 추측만 할뿐",
          "original_form": "그냥 시원해서 그런가 하고 추측만 할뿐",
          "speaker_id": "1",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100030506.1.31",
          "form": "올 여름이 덥긴 했지",
          "original_form": "올 여름이 덥긴 했지",
          "speaker_id": "2",
          "time": "20210904 02:27"
        }
      ]
    }
