      "utterance": [
        {
          "id": "MDRW2100028034.1.1",
          "form": "아 아뇨 죄송",
          "original_form": "아 아뇨 죄송",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028034.1.2",
          "form": "님이 좋아한다는 건 들었음",
          "original_form": "님이 좋아한다는 건 들었음",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028034.1.3",
          "form": "웃겨 보세요",
          "original_form": "웃겨 보세요",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028034.1.4",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028034.1.5",
          "form": "아이키도 나오더라",
          "original_form": "아이키도 나오더라",
          "speaker_id": "2",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100028034.1.6",
          "form": "아이키 아시나요",
          "original_form": "아이키 아시나요",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.7",
          "form": "난 몰랐는데",
          "original_form": "난 몰랐는데",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.8",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.9",
          "form": "인스타 팔로우도 했는데요",
          "original_form": "인스타 팔로우도 했는데요",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.10",
          "form": "옛저녁에",
          "original_form": "옛저녁에",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.11",
          "form": "웃기다 근데 왜 스우파 안 보는데",
          "original_form": "웃기다 근데 왜 스우파 안 보는데",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.12",
          "form": "웃기네 크게 관심은 없어서",
          "original_form": "웃기네 크게 관심은 없어서",
          "speaker_id": "2",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100028034.1.13",
          "form": "아이키 잘생겻어",
          "original_form": "아이키 잘생겻어",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.14",
          "form": "근데 솔직히 본방은 악편 때문에 좀 속터져서",
          "original_form": "근데 솔직히 본방은 악편 때문에 좀 속터져서",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.15",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.16",
          "form": "악편을 할 거면 좀 성의있게라도 하면 안되냐?",
          "original_form": "악편을 할 거면 좀 성의있게라도 하면 안되냐?",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.17",
          "form": "프듀급인가요",
          "original_form": "프듀급인가요",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.18",
          "form": "같은 리액션 여기서 쓰고 저기서 쓰고 또 쓰고 또",
          "original_form": "같은 리액션 여기서 쓰고 저기서 쓰고 또 쓰고 또",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.19",
          "form": "성의없어",
          "original_form": "성의없어",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.20",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100028034.1.21",
          "form": "근데 출연진이 하도...",
          "original_form": "근데 출연진이 하도...",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028034.1.22",
          "form": "약간 짬 있는 사람들이라",
          "original_form": "약간 짬 있는 사람들이라",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028034.1.23",
          "form": "어이없기만 하고 딱히 기분 나쁘진 않은,",
          "original_form": "어이없기만 하고 딱히 기분 나쁘진 않은,",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028034.1.24",
          "form": "그냥 어이없기만",
          "original_form": "그냥 어이없기만",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028034.1.25",
          "form": "제발 편집에 성의를",
          "original_form": "제발 편집에 성의를",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100028034.1.26",
          "form": "어떤 사람이 댓글에 불량식품맛 나는 거 보니까 엠넷 방송이네 써뒀던데",
          "original_form": "어떤 사람이 댓글에 불량식품맛 나는 거 보니까 엠넷 방송이네 써뒀던데",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028034.1.27",
          "form": "딱 그 짝",
          "original_form": "딱 그 짝",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028034.1.28",
          "form": "아이 미2쳣나",
          "original_form": "아이 미2쳣나",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028034.1.29",
          "form": "불량식품맛",
          "original_form": "불량식품맛",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028034.1.30",
          "form": "그래도 재밌어요",
          "original_form": "그래도 재밌어요",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100028034.1.31",
          "form": "약간 리얼 힙합",
          "original_form": "약간 리얼 힙합",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.32",
          "form": "근데 난 언프리티 랩스타도 솔직히 재밌게 봐서",
          "original_form": "근데 난 언프리티 랩스타도 솔직히 재밌게 봐서",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.33",
          "form": "이러나 저러나 여자만 무더기로 나오면 재밌어하는",
          "original_form": "이러나 저러나 여자만 무더기로 나오면 재밌어하는",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.34",
          "form": "그냥 여자를 좋아하는 거 아닌가요?",
          "original_form": "그냥 여자를 좋아하는 거 아닌가요?",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.35",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "2",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.36",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.37",
          "form": "아니라고는 할 수 없는",
          "original_form": "아니라고는 할 수 없는",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100028034.1.38",
          "form": "그런 편이지",
          "original_form": "그런 편이지",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028034.1.39",
          "form": "근데 연습생들 데려다가 하는 건",
          "original_form": "근데 연습생들 데려다가 하는 건",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028034.1.40",
          "form": "킹받아",
          "original_form": "킹받아",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028034.1.41",
          "form": "그건 좀",
          "original_form": "그건 좀",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028034.1.42",
          "form": "언프리티도 경력 있고 짬 있는 래퍼들 데려다가 해서",
          "original_form": "언프리티도 경력 있고 짬 있는 래퍼들 데려다가 해서",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028034.1.43",
          "form": "재밌었던 거고",
          "original_form": "재밌었던 거고",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100028034.1.44",
          "form": "연생 데려다 아이돌학교같은 거 만들고 악편하기",
          "original_form": "연생 데려다 아이돌학교같은 거 만들고 악편하기",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100028034.1.45",
          "form": "최악",
          "original_form": "최악",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
