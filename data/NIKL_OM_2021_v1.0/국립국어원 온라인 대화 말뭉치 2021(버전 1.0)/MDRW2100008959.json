      "utterance": [
        {
          "id": "MDRW2100008959.1.1",
          "form": "저녁식사는 하셨어요?",
          "original_form": "저녁식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:58"
        },
        {
          "id": "MDRW2100008959.1.2",
          "form": "넵! 먹었습니다~",
          "original_form": "넵! 먹었습니다~",
          "speaker_id": "2",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008959.1.3",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:46"
        },
        {
          "id": "MDRW2100008959.1.4",
          "form": "저는 라면이여...ㅋㅋㅋ",
          "original_form": "저는 라면이여...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100008959.1.5",
          "form": "진라면 매운맛 먹었습니다~~",
          "original_form": "진라면 매운맛 먹었습니다~~",
          "speaker_id": "1",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100008959.1.6",
          "form": "배달음식 많이 시키세요??",
          "original_form": "배달음식 많이 시키세요??",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100008959.1.7",
          "form": "자주라고는 볼수 없는데 오늘 시켰네욬ㅋㅋㅋㅋ",
          "original_form": "자주라고는 볼수 없는데 오늘 시켰네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100008959.1.8",
          "form": "오늘 라떼랑 크로플 핫도그 주문해서 먹었어용",
          "original_form": "오늘 라떼랑 크로플 핫도그 주문해서 먹었어용",
          "speaker_id": "1",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100008959.1.9",
          "form": "요즘 진짜 어디든 배달이 되서 솔직히 좀 신기하긴해요ㅋㅋㅋ",
          "original_form": "요즘 진짜 어디든 배달이 되서 솔직히 좀 신기하긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100008959.1.10",
          "form": "전 밀면 시켰죠ㅎㅎㅎ",
          "original_form": "전 밀면 시켰죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100008959.1.11",
          "form": "오호! ㅋㅋㅋ 저녁으로 드시는거예요??",
          "original_form": "오호! ㅋㅋㅋ 저녁으로 드시는거예요??",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100008959.1.12",
          "form": "네ㅋㅋ\r\n비도 오고 찝찝하니ㅋㅋㅋ",
          "original_form": "네ㅋㅋ\r\n비도 오고 찝찝하니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100008959.1.13",
          "form": "시원한게 땡기네요ㅎㅎ",
          "original_form": "시원한게 땡기네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100008959.1.14",
          "form": "크! 좋쥬~ 저는 파전에 맥주 딱 마시구 싶네요",
          "original_form": "크! 좋쥬~ 저는 파전에 맥주 딱 마시구 싶네요",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100008959.1.15",
          "form": "튀김종류 약간 기름진게 먹고싶어요 ㅎㅎ",
          "original_form": "튀김종류 약간 기름진게 먹고싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100008959.1.16",
          "form": "비오는 날 파전은 국룰이죠ㅋㅋㅋㅋㅋ",
          "original_form": "비오는 날 파전은 국룰이죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100008959.1.17",
          "form": "전 맥주 보단 막걸리로ㅋㅋ",
          "original_form": "전 맥주 보단 막걸리로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100008959.1.18",
          "form": "저는 막걸리 먹으면 두통이 진짜 어마무시해욬ㅋㅋㅋ",
          "original_form": "저는 막걸리 먹으면 두통이 진짜 어마무시해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100008959.1.19",
          "form": "막걸리 잘드세여??",
          "original_form": "막걸리 잘드세여??",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100008959.1.20",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100008959.1.21",
          "form": "사이다와 함께라면 잘 먹어요!",
          "original_form": "사이다와 함께라면 잘 먹어요!",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100008959.1.22",
          "form": "막걸리와 사이다...?",
          "original_form": "막걸리와 사이다...?",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100008959.1.23",
          "form": "어떻게 먹는거에요?",
          "original_form": "어떻게 먹는거에요?",
