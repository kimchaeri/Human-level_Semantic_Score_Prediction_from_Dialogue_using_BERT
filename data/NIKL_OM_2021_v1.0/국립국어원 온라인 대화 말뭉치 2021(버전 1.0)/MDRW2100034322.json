      "utterance": [
        {
          "id": "MDRW2100034322.1.1",
          "form": "비가 애매하게오네요",
          "original_form": "비가 애매하게오네요",
          "speaker_id": "1",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100034322.1.2",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "2",
          "time": "20210901 10:10"
        },
        {
          "id": "MDRW2100034322.1.3",
          "form": "아침에는 엄청 왔어요",
          "original_form": "아침에는 엄청 왔어요",
          "speaker_id": "2",
          "time": "20210901 10:10"
        },
        {
          "id": "MDRW2100034322.1.4",
          "form": "비 그치면 이제 금방 추워질 거 같지않나요",
          "original_form": "비 그치면 이제 금방 추워질 거 같지않나요",
          "speaker_id": "1",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100034322.1.5",
          "form": "네 지금도 습기 때뭉에",
          "original_form": "네 지금도 습기 때뭉에",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100034322.1.6",
          "form": "덥죠 시원해요",
          "original_form": "덥죠 시원해요",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100034322.1.7",
          "form": "진짜 습해서 ㅋㅋㅋ 마스크 쓰니까 장난아니에요",
          "original_form": "진짜 습해서 ㅋㅋㅋ 마스크 쓰니까 장난아니에요",
          "speaker_id": "1",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100034322.1.8",
          "form": "정망 최악이죠",
          "original_form": "정망 최악이죠",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100034322.1.9",
          "form": "머스크에 습기에.ㅡ",
          "original_form": "머스크에 습기에.ㅡ",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100034322.1.10",
          "form": "여름 어떻게 버텼나싶어요",
          "original_form": "여름 어떻게 버텼나싶어요",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100034322.1.11",
          "form": "근데 또 겨울 오면 더위가 그리우려나욬ㅋㅋ",
          "original_form": "근데 또 겨울 오면 더위가 그리우려나욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100034322.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ살짝인데 그게 전 1분만가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ살짝인데 그게 전 1분만가요",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100034322.1.13",
          "form": "여름이 너뮤 싫어서 ㅠㅠ",
          "original_form": "여름이 너뮤 싫어서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100034322.1.14",
          "form": "1분ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "1분ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100034322.1.15",
          "form": "저도 겨울이 더 좋아요",
          "original_form": "저도 겨울이 더 좋아요",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100034322.1.16",
          "form": "그쵸ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100034322.1.17",
          "form": "여름은 진짜 ㅠ",
          "original_form": "여름은 진짜 ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100034322.1.18",
          "form": "더운걸 참을수가 없어요 ㅋㅋㅋ",
          "original_form": "더운걸 참을수가 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100034322.1.19",
          "form": "저두요 ㅠ",
          "original_form": "저두요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100034322.1.20",
          "form": "여름 진짜",
          "original_form": "여름 진짜",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100034322.1.21",
          "form": "내년엔 죽부인살까봐요",
          "original_form": "내년엔 죽부인살까봐요",
