      "utterance": [
        {
          "id": "MDRW2100030034.1.1",
          "form": "오늘  저녁은요  감자 솥밥입니다",
          "original_form": "오늘  저녁은요  감자 솥밥입니다",
          "speaker_id": "2",
          "time": "20210903 19:17"
        },
        {
          "id": "MDRW2100030034.1.2",
          "form": "감자만 들었나요?",
          "original_form": "감자만 들었나요?",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100030034.1.3",
          "form": "내 감자 흰쌀",
          "original_form": "내 감자 흰쌀",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100030034.1.4",
          "form": "어...허니문기간이 끝나가나요",
          "original_form": "어...허니문기간이 끝나가나요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100030034.1.5",
          "form": "근데!",
          "original_form": "근데!",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100030034.1.6",
          "form": "허니문이 뭔가요",
          "original_form": "허니문이 뭔가요",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100030034.1.7",
          "form": "감자밥 맛있죠ㅎ 밥솥에 감자 얹어서 감자밥해도 맛있는데",
          "original_form": "감자밥 맛있죠ㅎ 밥솥에 감자 얹어서 감자밥해도 맛있는데",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100030034.1.8",
          "form": "아 맞습니다 그겁니다",
          "original_form": "아 맞습니다 그겁니다",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100030034.1.9",
          "form": "전기밥솥 아니면 어떨지 궁금ㅎㅎ",
          "original_form": "전기밥솥 아니면 어떨지 궁금ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100030034.1.10",
          "form": "맛 차이가 큰가요ㅎㅎㅎ",
          "original_form": "맛 차이가 큰가요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100030034.1.11",
          "form": "진짜 진짜 소 밥 진짜 짱짱",
          "original_form": "진짜 진짜 소 밥 진짜 짱짱",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100030034.1.12",
          "form": "솥 하나 사시죠",
          "original_form": "솥 하나 사시죠",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100030034.1.13",
          "form": "헉 맛있겠다ㅜㅇㅜ",
          "original_form": "헉 맛있겠다ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100030034.1.14",
          "form": "하지만 우린 그거 사도",
          "original_form": "하지만 우린 그거 사도",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100030034.1.15",
          "form": "연어솥밥은 못할거라ㅜ",
          "original_form": "연어솥밥은 못할거라ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100030034.1.16",
          "form": "감자밥만을 위해 사기엔ㅜㅇ",
          "original_form": "감자밥만을 위해 사기엔ㅜㅇ",
          "speaker_id": "1",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100030034.1.17",
          "form": "뭔가 아쉽네요",
          "original_form": "뭔가 아쉽네요",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100030034.1.18",
          "form": "솥밥 시리즈 또 뭐가 있나요!!",
          "original_form": "솥밥 시리즈 또 뭐가 있나요!!",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
