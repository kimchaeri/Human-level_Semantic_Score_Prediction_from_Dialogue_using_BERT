      "utterance": [
        {
          "id": "MDRW2100009328.1.1",
          "form": "음 저는 딱 이거다 하는건없어요",
          "original_form": "음 저는 딱 이거다 하는건없어요",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009328.1.2",
          "form": "어떤 편이세요?",
          "original_form": "어떤 편이세요?",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009328.1.3",
          "form": "아 그러시구나 저는 케이팝도 가끔 듣고 락도 가끔 듣고 그래요!",
          "original_form": "아 그러시구나 저는 케이팝도 가끔 듣고 락도 가끔 듣고 그래요!",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009328.1.4",
          "form": "그럼 구독하는 ott 서비스 있으세요?",
          "original_form": "그럼 구독하는 ott 서비스 있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009328.1.5",
          "form": "음 멜론같은거 말하시는거죠?",
          "original_form": "음 멜론같은거 말하시는거죠?",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009328.1.6",
          "form": "전 뭔가 영상도 같이보는게 좋더라고요",
          "original_form": "전 뭔가 영상도 같이보는게 좋더라고요",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009328.1.7",
          "form": "뮤직비디오파? ㅋㅋ",
          "original_form": "뮤직비디오파? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009328.1.8",
          "form": "아 그 넷플이나 왓챠 이런거요 ㅋㅋㅋ",
          "original_form": "아 그 넷플이나 왓챠 이런거요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009328.1.9",
          "form": "아 저도 뮤비 좋아해요!",
          "original_form": "아 저도 뮤비 좋아해요!",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009328.1.10",
          "form": "저 아직도 fx 4 walls 뮤비를 잊지못해요",
          "original_form": "저 아직도 fx 4 walls 뮤비를 잊지못해요",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009328.1.11",
          "form": "아하 저는 핸드폰요금제",
          "original_form": "아하 저는 핸드폰요금제",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009328.1.12",
          "form": "비싼거쓰면서 웨이브가 무료였어요 얼마전까지",
          "original_form": "비싼거쓰면서 웨이브가 무료였어요 얼마전까지",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009328.1.13",
          "form": "아 웨이브! 저 한번도 안써봤어요",
          "original_form": "아 웨이브! 저 한번도 안써봤어요",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009328.1.14",
          "form": "재밌는거 많나요",
          "original_form": "재밌는거 많나요",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009328.1.15",
          "form": "티빙이랑 영드 미드 일드 이런카테고리는 겹치는것도 좀 있었공ㆍ",
          "original_form": "티빙이랑 영드 미드 일드 이런카테고리는 겹치는것도 좀 있었공ㆍ",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009328.1.16",
          "form": "고요",
          "original_form": "고요",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009328.1.17",
          "form": "고독한 미식가 ㅜ",
          "original_form": "고독한 미식가 ㅜ",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009328.1.18",
          "form": "일드의 정수인듯 ㅋㅋ",
          "original_form": "일드의 정수인듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009328.1.19",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009328.1.20",
          "form": "하라가 헷타",
          "original_form": "하라가 헷타",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009328.1.21",
          "form": "그거 한문장 외웠어요",
          "original_form": "그거 한문장 외웠어요",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009328.1.22",
          "form": "그냥 암생각없이 보기좋은거같앙ㆍ",
          "original_form": "그냥 암생각없이 보기좋은거같앙ㆍ",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009328.1.23",
          "form": "크게 스트레스 받는거없이",
          "original_form": "크게 스트레스 받는거없이",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009328.1.24",
          "form": "약간 그런 저세상의 잔잔함? ㅋㅋ",
          "original_form": "약간 그런 저세상의 잔잔함? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009328.1.25",
          "form": "아 맞아요 그냥 먹고.. 부러워지고 ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 그냥 먹고.. 부러워지고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009328.1.26",
          "form": "일드 그런거 많은거같아요",
          "original_form": "일드 그런거 많은거같아요",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009328.1.27",
          "form": "그런거나 수사물도 좋아요 ㅋㅋ",
          "original_form": "그런거나 수사물도 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009328.1.28",
          "form": "왤케 범죄가 많아! 같은 느낌이지만 ㅋㅋㅋ",
          "original_form": "왤케 범죄가 많아! 같은 느낌이지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009328.1.29",
          "form": "아 맞아욬ㅋㅋ 맨날 사람 죽고",
          "original_form": "아 맞아욬ㅋㅋ 맨날 사람 죽고",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009328.1.30",
          "form": "코난도 보면 범죄를 몰고다니잖아요 ㅋㅋㅋ",
          "original_form": "코난도 보면 범죄를 몰고다니잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009328.1.31",
          "form": "아니 내 주변엔 살인사건이 하나도없는데",
          "original_form": "아니 내 주변엔 살인사건이 하나도없는데",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009328.1.32",
          "form": "시골 경찰이 맨날 살인사건 출동하는거 좀웃겨요",
          "original_form": "시골 경찰이 맨날 살인사건 출동하는거 좀웃겨요",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009328.1.33",
          "form": "그런거 보다보면 되게",
          "original_form": "그런거 보다보면 되게",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009328.1.34",
          "form": "범죄에 경각심 들지않나요",
          "original_form": "범죄에 경각심 들지않나요",
          "speaker_id": "1",
          "time": "20210822 23:22"
        }
      ]
