      "utterance": [
        {
          "id": "MDRW2100017691.1.1",
          "form": "요즘 자주 접속하는 곳 있어?",
          "original_form": "요즘 자주 접속하는 곳 있어?",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017691.1.2",
          "form": "인스티즈 자주 가",
          "original_form": "인스티즈 자주 가",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017691.1.3",
          "form": "거기서 뭐해?",
          "original_form": "거기서 뭐해?",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017691.1.4",
          "form": "주로 인기글 보고 있어",
          "original_form": "주로 인기글 보고 있어",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017691.1.5",
          "form": "난 더쿠라는 곳에서 뷰티 카테고리 본다",
          "original_form": "난 더쿠라는 곳에서 뷰티 카테고리 본다",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017691.1.6",
          "form": "뷰티에 관심 많나보네",
          "original_form": "뷰티에 관심 많나보네",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017691.1.7",
          "form": "그건 아니고 정보글이나 관심 있는 글만 봐",
          "original_form": "그건 아니고 정보글이나 관심 있는 글만 봐",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017691.1.8",
          "form": "응,그렇구나",
          "original_form": "응,그렇구나",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017691.1.9",
          "form": "관심 있는 글 뭐보는데?",
          "original_form": "관심 있는 글 뭐보는데?",
          "speaker_id": "2",
          "time": "20210826 20:55"
        },
        {
          "id": "MDRW2100017691.1.10",
          "form": "주로 핫딜이나 이벤트 보는데 도움이 많이 되고 있어",
          "original_form": "주로 핫딜이나 이벤트 보는데 도움이 많이 되고 있어",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017691.1.11",
          "form": "여성이 많이 이용하는 사이트라 정보력이 좋아",
          "original_form": "여성이 많이 이용하는 사이트라 정보력이 좋아",
          "speaker_id": "1",
          "time": "20210826 20:57"
        },
        {
          "id": "MDRW2100017691.1.12",
          "form": "나는 이슈글을 많이봐",
          "original_form": "나는 이슈글을 많이봐",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017691.1.13",
          "form": "아까 얘기 했잖아ㅋㅋ",
          "original_form": "아까 얘기 했잖아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017691.1.14",
          "form": "다른 커뮤니티 이용하는 데 없어?",
          "original_form": "다른 커뮤니티 이용하는 데 없어?",
          "speaker_id": "2",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017691.1.15",
          "form": "덕질 사이트 주로 보기만 하지만",
          "original_form": "덕질 사이트 주로 보기만 하지만",
          "speaker_id": "1",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017691.1.16",
          "form": "난 네이트판도 가끔 들어가",
          "original_form": "난 네이트판도 가끔 들어가",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017691.1.17",
          "form": "아 거기 글 인스타에서 가끔 봐",
          "original_form": "아 거기 글 인스타에서 가끔 봐",
          "speaker_id": "1",
          "time": "20210826 21:10"
        }
