          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022445.1.39",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022445.1.40",
          "form": "연어도 그렇게 반응이 좋진않더라구요",
          "original_form": "연어도 그렇게 반응이 좋진않더라구요",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022445.1.41",
          "form": "진짜 신기해요",
          "original_form": "진짜 신기해요",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022445.1.42",
