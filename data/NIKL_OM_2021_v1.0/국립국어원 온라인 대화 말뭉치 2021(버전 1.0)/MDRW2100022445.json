      "utterance": [
        {
          "id": "MDRW2100022445.1.1",
          "form": "여름같이 더울때도 이불속을 좋아하나요?",
          "original_form": "여름같이 더울때도 이불속을 좋아하나요?",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022445.1.2",
          "form": "갑자기 궁금해지네요",
          "original_form": "갑자기 궁금해지네요",
          "speaker_id": "1",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022445.1.3",
          "form": "네 그럴 때는 잠깐 들어갔다가 나와요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 그럴 때는 잠깐 들어갔다가 나와요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022445.1.4",
          "form": "아니면 머리만 내놓고 있거나",
          "original_form": "아니면 머리만 내놓고 있거나",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022445.1.5",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022445.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022445.1.7",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022445.1.8",
          "form": "그래도 좋아하는군요ㅋㅋㅋㅋ",
          "original_form": "그래도 좋아하는군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022445.1.9",
          "form": "겨울엔 너무 따뜻하고 아늑할것같어요",
          "original_form": "겨울엔 너무 따뜻하고 아늑할것같어요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022445.1.10",
          "form": "그 온기가 진짜 너무 안정적",
          "original_form": "그 온기가 진짜 너무 안정적",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022445.1.11",
          "form": "네 ㅋㅋㅋㅋ 어떤 분은 이불 속에 빨래망 거치대 같은거 넣으시더라고요",
          "original_form": "네 ㅋㅋㅋㅋ 어떤 분은 이불 속에 빨래망 거치대 같은거 넣으시더라고요",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022445.1.12",
          "form": "그럼 그 공간에 고양이가 들어가서 쉰대요 ㅋㅋㅋ",
          "original_form": "그럼 그 공간에 고양이가 들어가서 쉰대요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022445.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022445.1.14",
          "form": "고양이집 금방 완성이네요",
          "original_form": "고양이집 금방 완성이네요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022445.1.15",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022445.1.16",
          "form": "고양이란 생물이란 왜 이렇게 귀여울까요",
          "original_form": "고양이란 생물이란 왜 이렇게 귀여울까요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022445.1.17",
          "form": "ㅋㅋㅋㅋㅋ그러니까요 진짜 확 그냥이에요",
          "original_form": "ㅋㅋㅋㅋㅋ그러니까요 진짜 확 그냥이에요",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022445.1.18",
          "form": "우리집 고양이가 뭐를 가장 좋아할까요",
          "original_form": "우리집 고양이가 뭐를 가장 좋아할까요",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022445.1.19",
          "form": "맞춰보세요",
          "original_form": "맞춰보세요",
