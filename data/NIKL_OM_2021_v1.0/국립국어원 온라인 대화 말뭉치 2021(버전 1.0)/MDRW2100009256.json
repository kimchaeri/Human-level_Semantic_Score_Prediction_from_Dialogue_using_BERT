      "utterance": [
        {
          "id": "MDRW2100009256.1.1",
          "form": "오늘도 하루가 시작되었네요~\r\n아침식사 하셨나요?",
          "original_form": "오늘도 하루가 시작되었네요~\r\n아침식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 08:15"
        },
        {
          "id": "MDRW2100009256.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210823 08:16"
        },
        {
          "id": "MDRW2100009256.1.3",
          "form": "애들만 줬어요",
          "original_form": "애들만 줬어요",
          "speaker_id": "2",
          "time": "20210823 08:16"
        },
        {
          "id": "MDRW2100009256.1.4",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210823 08:16"
        },
        {
          "id": "MDRW2100009256.1.5",
          "form": "학교 이제 다 보냈네요 ㅎㅎ",
          "original_form": "학교 이제 다 보냈네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 08:16"
        },
        {
          "id": "MDRW2100009256.1.6",
          "form": "항상 아침을 뭘해줄지 고민돼요",
          "original_form": "항상 아침을 뭘해줄지 고민돼요",
          "speaker_id": "2",
          "time": "20210823 08:20"
        },
        {
          "id": "MDRW2100009256.1.7",
          "form": "애들 학교 정상 등교하죠?",
          "original_form": "애들 학교 정상 등교하죠?",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009256.1.8",
          "form": "저는 애들만 챙겨주고 출근했어요ㅎㅎ",
          "original_form": "저는 애들만 챙겨주고 출근했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009256.1.9",
          "form": "아이들끼리 둘은 집에 있구요",
          "original_form": "아이들끼리 둘은 집에 있구요",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100009256.1.10",
          "form": "아이들이 의젓하네요",
          "original_form": "아이들이 의젓하네요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009256.1.11",
          "form": "항상 걱정 되시겠어요 애들끼리 있어서요",
          "original_form": "항상 걱정 되시겠어요 애들끼리 있어서요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009256.1.12",
          "form": "네 그래서 중간중간에 계속 전화 걸어봐요ㅜ",
          "original_form": "네 그래서 중간중간에 계속 전화 걸어봐요ㅜ",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009256.1.13",
          "form": "그래도 목요일부터는 등교하니까요ㅎㅎ",
          "original_form": "그래도 목요일부터는 등교하니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100009256.1.14",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009256.1.15",
          "form": "차라리 이럴땐 등교가나아요",
          "original_form": "차라리 이럴땐 등교가나아요",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009256.1.16",
          "form": "가면 밥도 주고",
          "original_form": "가면 밥도 주고",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009256.1.17",
          "form": "애들만 있는것보다 안심되는거 같아요",
          "original_form": "애들만 있는것보다 안심되는거 같아요",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009256.1.18",
          "form": "코로나라 위험하지만",
          "original_form": "코로나라 위험하지만",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009256.1.19",
          "form": "학교가 맘이 놓일것 같아요",
          "original_form": "학교가 맘이 놓일것 같아요",
          "speaker_id": "2",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100009256.1.20",
          "form": "저희 애들은 지방이라 전면 등교 저번주부터해요",
          "original_form": "저희 애들은 지방이라 전면 등교 저번주부터해요",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100009256.1.21",
          "form": "아! 저희도 전면등교인데 아마 수업일수 때문에 개학이 늦은거 같애요",
          "original_form": "아! 저희도 전면등교인데 아마 수업일수 때문에 개학이 늦은거 같애요",
          "speaker_id": "1",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100009256.1.22",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100009256.1.23",
          "form": "참나 애들 끼리 집에 있는데 빨리 가게 해주지",
          "original_form": "참나 애들 끼리 집에 있는데 빨리 가게 해주지",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.24",
          "form": "4학년인데 밥솥에 밥만 떠서 먹으라고하는데도",
          "original_form": "4학년인데 밥솥에 밥만 떠서 먹으라고하는데도",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.26",
          "form": "손데일까봐 걱정되더라구요ㅜ",
          "original_form": "손데일까봐 걱정되더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.27",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.28",
          "form": "할줄아는게 아무것도 없어서",
          "original_form": "할줄아는게 아무것도 없어서",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.29",
          "form": "저도 6학년 딸 못시키겠어요",
          "original_form": "저도 6학년 딸 못시키겠어요",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.30",
          "form": "저희도요 ㅜㅜ",
          "original_form": "저희도요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.31",
          "form": "숟가락만 놀줄 알아요",
          "original_form": "숟가락만 놀줄 알아요",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100009256.1.32",
          "form": "엄마맘이 시킬수가 없어요",
          "original_form": "엄마맘이 시킬수가 없어요",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100009256.1.33",
          "form": "ㅠㅡㅠ저흰 남자애들이니깐 더해요.. ㅜㅜ",
          "original_form": "ㅠㅡㅠ저흰 남자애들이니깐 더해요.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100009256.1.34",
          "form": "혹 다칠까 힘들까",
          "original_form": "혹 다칠까 힘들까",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100009256.1.35",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100009256.1.36",
          "form": "뭐 시켜도 제대로 못하고. .",
          "original_form": "뭐 시켜도 제대로 못하고. .",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
