      "utterance": [
        {
          "id": "MDRW2100034133.1.1",
          "form": "게임 하는거 좋아하세요?",
          "original_form": "게임 하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100034133.1.2",
          "form": "좋아해요ㅎㅎ",
          "original_form": "좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034133.1.3",
          "form": "어떤거 즐겨하세요!",
          "original_form": "어떤거 즐겨하세요!",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034133.1.4",
          "form": "쿠키런 오븐 즐겨해요",
          "original_form": "쿠키런 오븐 즐겨해요",
          "speaker_id": "2",
          "time": "20210827 17:19"
        },
        {
          "id": "MDRW2100034133.1.5",
          "form": "오 쿠키런 오랜만이네요 ㅎㅎ",
          "original_form": "오 쿠키런 오랜만이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100034133.1.6",
          "form": "한때 즐겨했는데 !",
          "original_form": "한때 즐겨했는데 !",
          "speaker_id": "1",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100034133.1.7",
          "form": "추억의 게임이죠ㅋㅋ",
          "original_form": "추억의 게임이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100034133.1.8",
          "form": "요즘도 사람 많나요?",
          "original_form": "요즘도 사람 많나요?",
          "speaker_id": "1",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100034133.1.9",
          "form": "거의 고인물이지만 많이 해요ㅋㅋㅋㅋ",
          "original_form": "거의 고인물이지만 많이 해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100034133.1.10",
          "form": "아 ㅋㅋ 그러면 예전부터 계속 하신거에요??",
          "original_form": "아 ㅋㅋ 그러면 예전부터 계속 하신거에요??",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100034133.1.11",
          "form": "저는 올 초부터 시작했어요ㅋㅋ",
          "original_form": "저는 올 초부터 시작했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100034133.1.12",
          "form": "오오 막 오래는 안하셨네요 !",
          "original_form": "오오 막 오래는 안하셨네요 !",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100034133.1.13",
          "form": "막 아이템빨 심한가요??",
          "original_form": "막 아이템빨 심한가요??",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100034133.1.14",
          "form": "쿠키, 보물, 펫, 실력의 4박자라 템빨 심합니당ㅋㅋㅋ",
          "original_form": "쿠키, 보물, 펫, 실력의 4박자라 템빨 심합니당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100034133.1.15",
          "form": "아 완전 실력 제외하고 다 템빨이네욬ㅋㅋㅋ",
          "original_form": "아 완전 실력 제외하고 다 템빨이네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100034133.1.16",
          "form": "현질도 하시나요?",
          "original_form": "현질도 하시나요?",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100034133.1.17",
          "form": "원스토어 포인트에 얼마씩 더해서 해요ㅋㅋㅋ",
          "original_form": "원스토어 포인트에 얼마씩 더해서 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:31"
        }
