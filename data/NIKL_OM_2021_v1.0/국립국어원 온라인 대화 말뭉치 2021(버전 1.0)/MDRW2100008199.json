        },
        {
          "id": "MDRW2100008199.1.2",
          "form": "공연 좋아하나요",
          "original_form": "공연 좋아하나요",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.3",
          "form": "이건희컬렉션",
          "original_form": "이건희컬렉션",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.4",
          "form": "무슨 전시회",
          "original_form": "무슨 전시회",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.5",
          "form": "예약어려워",
          "original_form": "예약어려워",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.6",
          "form": "아하 예약하기 어렵지",
          "original_form": "아하 예약하기 어렵지",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.7",
          "form": "나는 공연보러 가고 싶다",
          "original_form": "나는 공연보러 가고 싶다",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.8",
          "form": "코로나라서 공연보러가기 무서워",
          "original_form": "코로나라서 공연보러가기 무서워",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.9",
          "form": "무슨공연",
          "original_form": "무슨공연",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008199.1.10",
          "form": "콘서트요 피아노",
          "original_form": "콘서트요 피아노",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.11",
          "form": "실제로 콘서트장에서 듣는 피아노 짱 좋아요",
          "original_form": "실제로 콘서트장에서 듣는 피아노 짱 좋아요",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.12",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.13",
          "form": "이루마",
          "original_form": "이루마",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.14",
          "form": "조성진",
          "original_form": "조성진",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.15",
          "form": "이루마 콘서트 가봤어요",
          "original_form": "이루마 콘서트 가봤어요",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.16",
          "form": "조성진도 가보고 싶네요",
          "original_form": "조성진도 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.17",
          "form": "좋겠군요",
          "original_form": "좋겠군요",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008199.1.18",
          "form": "여수에 홀로그램 전시관 있다는데 엄청 예쁘대요",
          "original_form": "여수에 홀로그램 전시관 있다는데 엄청 예쁘대요",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008199.1.19",
          "form": "나중에 한 번 가보세요",
