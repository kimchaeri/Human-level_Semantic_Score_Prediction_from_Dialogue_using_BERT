      "utterance": [
        {
          "id": "MDRW2100012302.1.1",
          "form": "안녕하세요 뮤지컬좋아하시나요?",
          "original_form": "안녕하세요 뮤지컬좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100012302.1.2",
          "form": "자주 안보지만 좋아해요",
          "original_form": "자주 안보지만 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012302.1.3",
          "form": "제 친구들이 많이 보더라구요ㅋㅋㅋ",
          "original_form": "제 친구들이 많이 보더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:04"
        },
        {
          "id": "MDRW2100012302.1.4",
          "form": "어떤 극 보셨어요?",
          "original_form": "어떤 극 보셨어요?",
          "speaker_id": "1",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012302.1.5",
          "form": "예전이라 잘은 기억 안나요",
          "original_form": "예전이라 잘은 기억 안나요",
          "speaker_id": "2",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012302.1.6",
          "form": "친구따라 간거라서",
          "original_form": "친구따라 간거라서",
          "speaker_id": "2",
          "time": "20210824 21:06"
        },
        {
          "id": "MDRW2100012302.1.7",
          "form": "근데 되게 재밌긴했어요!",
          "original_form": "근데 되게 재밌긴했어요!",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100012302.1.8",
          "form": "중간 쉬는시간이 금방 오더라구요",
          "original_form": "중간 쉬는시간이 금방 오더라구요",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100012302.1.9",
          "form": "그럼 최근에 보신 영화는  뭐예요?",
          "original_form": "그럼 최근에 보신 영화는  뭐예요?",
          "speaker_id": "1",
          "time": "20210824 21:08"
        },
        {
          "id": "MDRW2100012302.1.10",
          "form": "좀 됐는데 크루엘라 봤어요",
          "original_form": "좀 됐는데 크루엘라 봤어요",
          "speaker_id": "2",
          "time": "20210824 21:09"
        },
        {
          "id": "MDRW2100012302.1.11",
          "form": "혹시 보셨나요?",
          "original_form": "혹시 보셨나요?",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100012302.1.12",
          "form": "저는 보려다가 못보고 그뒤에 블랙위도우 봤어요",
          "original_form": "저는 보려다가 못보고 그뒤에 블랙위도우 봤어요",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012302.1.13",
          "form": "그렇게 배우분이 매력적이셨다던데ㅠ",
          "original_form": "그렇게 배우분이 매력적이셨다던데ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012302.1.14",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012302.1.15",
          "form": "어 저도 블랙위도우 보려고했는데",
          "original_form": "어 저도 블랙위도우 보려고했는데",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012302.1.16",
          "form": "네 연기 진짜 잘했어요",
          "original_form": "네 연기 진짜 잘했어요",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012302.1.17",
          "form": "디즈니원작 안보고 봐도 재밌더라구요",
          "original_form": "디즈니원작 안보고 봐도 재밌더라구요",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012302.1.18",
          "form": "블랙위도우 저도 보고싶은데 시간이 잘 안나네요",
          "original_form": "블랙위도우 저도 보고싶은데 시간이 잘 안나네요",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012302.1.19",
          "form": "영화 안그래도 시간 잘안나는데 영화표값도 은근슬쩍 올라서 은근 부담스러워요",
          "original_form": "영화 안그래도 시간 잘안나는데 영화표값도 은근슬쩍 올라서 은근 부담스러워요",
          "speaker_id": "1",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012302.1.20",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012302.1.21",
          "form": "그 차이가 꽤 크더라구요",
          "original_form": "그 차이가 꽤 크더라구요",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012302.1.22",
          "form": "이젠 최소 9천원 이래서",
          "original_form": "이젠 최소 9천원 이래서",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012302.1.23",
          "form": "이래저래 할인안받으면 만원 훌쩍 넘어가기도하구요",
          "original_form": "이래저래 할인안받으면 만원 훌쩍 넘어가기도하구요",
          "speaker_id": "1",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012302.1.24",
          "form": "맞아요 진짜 부담스럽긴해요",
          "original_form": "맞아요 진짜 부담스럽긴해요",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012302.1.25",
          "form": "저도 몰랐는데 가격 오른뒤로",
          "original_form": "저도 몰랐는데 가격 오른뒤로",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012302.1.26",
          "form": "잘 안가게 된 것 같아요",
          "original_form": "잘 안가게 된 것 같아요",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012302.1.27",
          "form": "그죠 코로나라 피하고있다가 다시보니까 갑작스레 부담스러워서참...ㅠ",
          "original_form": "그죠 코로나라 피하고있다가 다시보니까 갑작스레 부담스러워서참...ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:16"
        }
      ]
