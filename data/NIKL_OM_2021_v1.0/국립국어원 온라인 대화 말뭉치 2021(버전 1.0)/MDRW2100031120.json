      "utterance": [
        {
          "id": "MDRW2100031120.1.1",
          "form": "저희는",
          "original_form": "저희는",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031120.1.2",
          "form": "제 회사도 편애 좀심해요",
          "original_form": "제 회사도 편애 좀심해요",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031120.1.3",
          "form": "대리가 초대졸이라 4년제랑 차별대우받아서",
          "original_form": "대리가 초대졸이라 4년제랑 차별대우받아서",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031120.1.4",
          "form": "그만둔다고ㅜㅜ",
          "original_form": "그만둔다고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031120.1.5",
          "form": "근데 고민중인거 같아요",
          "original_form": "근데 고민중인거 같아요",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031120.1.6",
          "form": "사람들땜에",
          "original_form": "사람들땜에",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031120.1.7",
          "form": "아 직원들 관둔다그럼 속상해요",
          "original_form": "아 직원들 관둔다그럼 속상해요",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.8",
          "form": "회사는 많은데",
          "original_form": "회사는 많은데",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.9",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.10",
          "form": "지금다니는곳보다 과연 다 좋을까",
          "original_form": "지금다니는곳보다 과연 다 좋을까",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.11",
          "form": "하는 의문점이 들죠",
          "original_form": "하는 의문점이 들죠",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.12",
          "form": "아 진짜요!!!",
          "original_form": "아 진짜요!!!",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.13",
          "form": "잘못 이직하면",
          "original_form": "잘못 이직하면",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.14",
          "form": "더 구렁텅이로 들어가는수가 있어요",
          "original_form": "더 구렁텅이로 들어가는수가 있어요",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031120.1.15",
          "form": "나락으로...ㅜㅠ",
          "original_form": "나락으로...ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031120.1.16",
          "form": "전 이 회사가 마지막일거같아서",
          "original_form": "전 이 회사가 마지막일거같아서",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031120.1.17",
          "form": "회사가 제발 잘되기를,,,",
          "original_form": "회사가 제발 잘되기를,,,",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031120.1.18",
          "form": "이제 만족하고 다니시는건가여?ㅋㅋ",
          "original_form": "이제 만족하고 다니시는건가여?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031120.1.19",
          "form": "ㅋㅋ더는 이직이... ㅋㅋ내일채움공제 못받고 다니다가 작년에 재직자로ㅋㅋ 5년남았어요ㅋㅋ",
          "original_form": "ㅋㅋ더는 이직이... ㅋㅋ내일채움공제 못받고 다니다가 작년에 재직자로ㅋㅋ 5년남았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031120.1.20",
          "form": "과연 오년을...",
          "original_form": "과연 오년을...",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031120.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031120.1.22",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031120.1.23",
          "form": "하셨구나 ㅋㅋㅋ",
          "original_form": "하셨구나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031120.1.24",
          "form": "저희 직장에",
          "original_form": "저희 직장에",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031120.1.25",
          "form": "꼬꼬마친구들 첫직장 돈받는거? 그거땜에 못그만두고 다니는친구 셋 있습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "꼬꼬마친구들 첫직장 돈받는거? 그거땜에 못그만두고 다니는친구 셋 있습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100031120.1.26",
          "form": "ㅋㅋ전 꼬꼬마가 아니라 완전 막찬데 재직 4년차에 가입했어요ㅋㅋ",
          "original_form": "ㅋㅋ전 꼬꼬마가 아니라 완전 막찬데 재직 4년차에 가입했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031120.1.27",
          "form": "울회사 신입들도 5명정도ㅋㅋ",
          "original_form": "울회사 신입들도 5명정도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031120.1.28",
          "form": "ㅋㅋㅋㅋㅋ늦게하셨네요",
          "original_form": "ㅋㅋㅋㅋㅋ늦게하셨네요",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031120.1.29",
          "form": "그친구들 족쇄 제대로 찼군여 ㅋㅋㅋㅋ",
          "original_form": "그친구들 족쇄 제대로 찼군여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031120.1.30",
          "form": "네 회사입장에서 완전 땡큐죠!",
          "original_form": "네 회사입장에서 완전 땡큐죠!",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100031120.1.31",
          "form": "그런걸 만들어서 족쇄를 채우다니",
          "original_form": "그런걸 만들어서 족쇄를 채우다니",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031120.1.32",
          "form": "나이들수록 확실히 신입들 너무 예쁘지않나요ㅜ",
          "original_form": "나이들수록 확실히 신입들 너무 예쁘지않나요ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031120.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 귀엽죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 귀엽죠",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031120.1.34",
          "form": "어린애들",
          "original_form": "어린애들",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031120.1.35",
          "form": "선배님 하면서 뭐 물어보는거",
          "original_form": "선배님 하면서 뭐 물어보는거",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100031120.1.36",
          "form": "착하고 뭐든 배우려고하고",
          "original_form": "착하고 뭐든 배우려고하고",
          "speaker_id": "1",
          "time": "20210904 17:17"
        }
      ]
    }
