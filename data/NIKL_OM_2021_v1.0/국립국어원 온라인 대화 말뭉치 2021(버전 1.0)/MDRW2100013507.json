      "utterance": [
        {
          "id": "MDRW2100013507.1.1",
          "form": "최근에 가장 좋았던 쇼핑이 있을까요",
          "original_form": "최근에 가장 좋았던 쇼핑이 있을까요",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013507.1.2",
          "form": "타미죠",
          "original_form": "타미죠",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013507.1.3",
          "form": "싸게 살수 있는게 좋았어요",
          "original_form": "싸게 살수 있는게 좋았어요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013507.1.4",
          "form": "타미힐리거",
          "original_form": "타미힐리거",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013507.1.5",
          "form": "오 저는 길가다가 본게 다인데",
          "original_form": "오 저는 길가다가 본게 다인데",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013507.1.6",
          "form": "싸게구매가가능하군요",
          "original_form": "싸게구매가가능하군요",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013507.1.7",
          "form": "별로 안비싸요",
          "original_form": "별로 안비싸요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013507.1.8",
          "form": "오 부자신가봐요",
          "original_form": "오 부자신가봐요",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013507.1.9",
          "form": "7만원이면 티하나 사요",
          "original_form": "7만원이면 티하나 사요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013507.1.10",
          "form": "부자 아니라 평범한거죠",
          "original_form": "부자 아니라 평범한거죠",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013507.1.11",
          "form": "상대적 박탈감이 듭니다",
          "original_form": "상대적 박탈감이 듭니다",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013507.1.12",
          "form": "선물하기도 좋아요",
          "original_form": "선물하기도 좋아요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013507.1.13",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013507.1.14",
          "form": "방금 찾아보니까",
          "original_form": "방금 찾아보니까",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013507.1.15",
          "form": "양말이 너무 예쁘네여",
          "original_form": "양말이 너무 예쁘네여",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013507.1.16",
          "form": "저는 양말 그런데서 안사요",
          "original_form": "저는 양말 그런데서 안사요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013507.1.17",
          "form": "그리고 스웨터도 예쁘고요",
          "original_form": "그리고 스웨터도 예쁘고요",
          "speaker_id": "1",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013507.1.18",
          "form": "빨리 검색하시네요",
          "original_form": "빨리 검색하시네요",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013507.1.19",
          "form": "안쓰는 폰으로 검색해봤어요",
          "original_form": "안쓰는 폰으로 검색해봤어요",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.20",
          "form": "스웨터 좋아하세요?",
          "original_form": "스웨터 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.22",
          "form": "무슨색?",
          "original_form": "무슨색?",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.23",
          "form": "노란색이요",
          "original_form": "노란색이요",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.24",
          "form": "옥수수색?",
          "original_form": "옥수수색?",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.25",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013507.1.26",
