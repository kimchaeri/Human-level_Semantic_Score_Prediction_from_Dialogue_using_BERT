      "utterance": [
        {
          "id": "MDRW2100022531.1.1",
          "form": "네네 진짜 작은 휴지로 구성되어잇어요",
          "original_form": "네네 진짜 작은 휴지로 구성되어잇어요",
          "speaker_id": "2",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022531.1.2",
          "form": "카카오프렌즈 그려진거에요?ㅋㅋ",
          "original_form": "카카오프렌즈 그려진거에요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.3",
          "form": "아마 그것도 있지않을까요 ㅋㅋㅋㅋ",
          "original_form": "아마 그것도 있지않을까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.4",
          "form": "지나가다가 본 것 같아요",
          "original_form": "지나가다가 본 것 같아요",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.5",
          "form": "일반 작은물티슈에 반정도크기?",
          "original_form": "일반 작은물티슈에 반정도크기?",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.6",
          "form": "근데 저 얼마전에 해외꺼 휴대용티슈 사봤는데",
          "original_form": "근데 저 얼마전에 해외꺼 휴대용티슈 사봤는데",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.7",
          "form": "진짜 너무 좋은 거 있죠",
          "original_form": "진짜 너무 좋은 거 있죠",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.8",
          "form": "아그래요? 브랜드가뭐에요?",
          "original_form": "아그래요? 브랜드가뭐에요?",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.9",
          "form": "제품이름이 템포였어요",
          "original_form": "제품이름이 템포였어요",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022531.1.10",
          "form": "어디나라건지 기억이 잘 안나는데 독일인가 그랬던 것 같아요",
          "original_form": "어디나라건지 기억이 잘 안나는데 독일인가 그랬던 것 같아요",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.11",
          "form": "오오 거기서 휴지도나오ㅡㄴ군요",
          "original_form": "오오 거기서 휴지도나오ㅡㄴ군요",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.12",
          "form": "엄청 품질이 좋아서",
          "original_form": "엄청 품질이 좋아서",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.13",
          "form": "음료 흘리면 한 번에 싹 닦여요 한장이...",
          "original_form": "음료 흘리면 한 번에 싹 닦여요 한장이...",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.14",
          "form": "참고할게요! 담에 사봐야겠어요",
          "original_form": "참고할게요! 담에 사봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.15",
          "form": "오 흡수력 뛰어나나보다",
          "original_form": "오 흡수력 뛰어나나보다",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.16",
          "form": "근데 좀 가격이 비싸서 아까워요 ㅜㅜ",
          "original_form": "근데 좀 가격이 비싸서 아까워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.17",
          "form": "저 그거 쿠팡인가 거기서 산건데...",
          "original_form": "저 그거 쿠팡인가 거기서 산건데...",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100022531.1.18",
          "form": "그럼 일단 한번은 사보는걸로 ㅋㅋㅋㅋ",
          "original_form": "그럼 일단 한번은 사보는걸로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022531.1.19",
          "form": "시간 지나서 보니 올리브영에서 더 싸게 팔더라구요 ㅠㅠ!!",
          "original_form": "시간 지나서 보니 올리브영에서 더 싸게 팔더라구요 ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022531.1.20",
          "form": "오 올영가면 구매각이네요",
          "original_form": "오 올영가면 구매각이네요",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022531.1.21",
          "form": "되게 흡수력도 좋고 부드러워서 이제 그거밖에 못쓸 것 같아요",
          "original_form": "되게 흡수력도 좋고 부드러워서 이제 그거밖에 못쓸 것 같아요",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022531.1.22",
          "form": "구매 추천 꾹 드립니다 ㅎㅎ",
          "original_form": "구매 추천 꾹 드립니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:29"
        }
