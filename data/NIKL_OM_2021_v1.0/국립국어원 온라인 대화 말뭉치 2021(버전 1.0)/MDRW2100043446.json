      "utterance": [
        {
          "id": "MDRW2100043446.1.1",
          "form": "오 안에 들어가셨구나!",
          "original_form": "오 안에 들어가셨구나!",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043446.1.2",
          "form": "나도...들어갔었나? 기억이 안나네요..",
          "original_form": "나도...들어갔었나? 기억이 안나네요..",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043446.1.3",
          "form": "안들어갔던거같아요 전",
          "original_form": "안들어갔던거같아요 전",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043446.1.4",
          "form": "사실 들어가는 거 자체는 별 거 없는데",
          "original_form": "사실 들어가는 거 자체는 별 거 없는데",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043446.1.5",
          "form": "보이는 풍경이 한국이랑 완전 다르니까 그게 좋더라구요",
          "original_form": "보이는 풍경이 한국이랑 완전 다르니까 그게 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043446.1.6",
          "form": "저는 낮에 첨봣는데 그냥 에펠탑 보는 순간 그 중압감이 와",
          "original_form": "저는 낮에 첨봣는데 그냥 에펠탑 보는 순간 그 중압감이 와",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043446.1.7",
          "form": "근데 밤에 불켜지니까 세상에..",
          "original_form": "근데 밤에 불켜지니까 세상에..",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043446.1.8",
          "form": "저 버킷리스트 생겼잖아요 그때",
          "original_form": "저 버킷리스트 생겼잖아요 그때",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043446.1.9",
          "form": "그쵸 낮에는 아 그냥 철제 건축물이구나.. 크다..",
          "original_form": "그쵸 낮에는 아 그냥 철제 건축물이구나.. 크다..",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043446.1.10",
          "form": "이랬는데 밤에ㅠㅜㅜㅜㅠ",
          "original_form": "이랬는데 밤에ㅠㅜㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043446.1.11",
          "form": "순식간에 감성에 젖어요 진짜ㅜㅠ",
          "original_form": "순식간에 감성에 젖어요 진짜ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043446.1.12",
          "form": "나중에 사랑하는 사람이 생기몀",
          "original_form": "나중에 사랑하는 사람이 생기몀",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043446.1.13",
          "form": "같이 오고싶다 하는 로망..",
          "original_form": "같이 오고싶다 하는 로망..",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043446.1.14",
          "form": "어후ㅠㅠㅜㅠㅜㅠㅠ",
          "original_form": "어후ㅠㅠㅜㅠㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043446.1.15",
          "form": "에펠탑에서 청혼하는 사람들 꽤 있잖아요",
          "original_form": "에펠탑에서 청혼하는 사람들 꽤 있잖아요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043446.1.16",
          "form": "로맨틱하네요ㅠㅠ",
          "original_form": "로맨틱하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043446.1.17",
          "form": "그니가요!!",
          "original_form": "그니가요!!",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043446.1.18",
          "form": "체코는 혼자 여행했는데 거기도 참 좋앗어요 ㅋㅋㅋ",
          "original_form": "체코는 혼자 여행했는데 거기도 참 좋앗어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043446.1.19",
          "form": "음식도 맥주도 ..맥주 그리워요",
          "original_form": "음식도 맥주도 ..맥주 그리워요",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043446.1.20",
          "form": "와 체코는 진짜 책에서만 봐서 너무 궁금해요",
          "original_form": "와 체코는 진짜 책에서만 봐서 너무 궁금해요",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043446.1.21",
          "form": "맥주ㅠㅜ 너무 맛있었을 거 같아요ㅜㅠ",
          "original_form": "맥주ㅠㅜ 너무 맛있었을 거 같아요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043446.1.22",
          "form": "코젤이 체코 맥주잖아요?",
          "original_form": "코젤이 체코 맥주잖아요?",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043446.1.23",
          "form": "와....찐은 다릅니다",
          "original_form": "와....찐은 다릅니다",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043446.1.24",
          "form": "와으아와ㅠㅜㅜㅠㅠ",
          "original_form": "와으아와ㅠㅜㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043446.1.25",
          "form": "저 술 안 좋아하는데 맥주만 진짜 좋아하거든요",
          "original_form": "저 술 안 좋아하는데 맥주만 진짜 좋아하거든요",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043446.1.26",
          "form": "어머니도 맥주 좋아하시는데 체코랑 독일 같이 가야겠어요ㅜㅜㅠ",
          "original_form": "어머니도 맥주 좋아하시는데 체코랑 독일 같이 가야겠어요ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043446.1.27",
          "form": "ㅋㅋㅋㅋ무조건 가셔야합니다 그렇다면",
          "original_form": "ㅋㅋㅋㅋ무조건 가셔야합니다 그렇다면",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043446.1.28",
          "form": "물가도 싸고",
          "original_form": "물가도 싸고",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043446.1.29",
          "form": "정말 저 아무데나 들어갓던 곳에서 마신 코젤흑생맥주맛을",
          "original_form": "정말 저 아무데나 들어갓던 곳에서 마신 코젤흑생맥주맛을",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043446.1.30",
          "form": "그 감동을 아직도 잊지 못해요",
          "original_form": "그 감동을 아직도 잊지 못해요",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043446.1.31",
          "form": "와 흑맥주..! 저 버킷리스트에 적어요 지금..",
          "original_form": "와 흑맥주..! 저 버킷리스트에 적어요 지금..",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043446.1.32",
          "form": "물가도 싸다니 넘 좋네요ㅠㅠㅠㅠ",
          "original_form": "물가도 싸다니 넘 좋네요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043446.1.33",
          "form": "그니까요 최곱니다 사람들도 친절해요",
          "original_form": "그니까요 최곱니다 사람들도 친절해요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043446.1.34",
          "form": "사람들도 친절하구",
          "original_form": "사람들도 친절하구",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043446.1.35",
          "form": "독일은 안가봣는데 옥토버페스트 ㅋㅋㅋㅋ죽기전에는 가봐여할텐데요",
          "original_form": "독일은 안가봣는데 옥토버페스트 ㅋㅋㅋㅋ죽기전에는 가봐여할텐데요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        }
      ]
    }
