      "utterance": [
        {
          "id": "MDRW2100047489.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100047489.1.2",
          "form": "아뇨 아직 집 가는 중이여서요",
          "original_form": "아뇨 아직 집 가는 중이여서요",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100047489.1.3",
          "form": "저녁은 어떤 거 드실 예정인가요?",
          "original_form": "저녁은 어떤 거 드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100047489.1.4",
          "form": "김치찌개 먹을 거예요",
          "original_form": "김치찌개 먹을 거예요",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100047489.1.5",
          "form": "늦게 드시네요! 원래 이 시간에 드시나요?",
          "original_form": "늦게 드시네요! 원래 이 시간에 드시나요?",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100047489.1.6",
          "form": "집에 늦게 도착해가지구",
          "original_form": "집에 늦게 도착해가지구",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047489.1.7",
          "form": "보통은 6시에 먹어요",
          "original_form": "보통은 6시에 먹어요",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047489.1.8",
          "form": "바쁘셨나봐요ㅜㅜ 직접 요리하신 건가요?",
          "original_form": "바쁘셨나봐요ㅜㅜ 직접 요리하신 건가요?",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100047489.1.9",
          "form": "할머니가 만들어줬아요ㅋㅋㅋㅋ",
          "original_form": "할머니가 만들어줬아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047489.1.10",
          "form": "된장찌개 직접 요리해 본 적은 있네요",
          "original_form": "된장찌개 직접 요리해 본 적은 있네요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100047489.1.11",
          "form": "저도 할머니랑 같이 살아서 할머니가 자주 요리해주세요ㅎㅎ 된장찌개랑 김치찌개 중에 어떤 걸 더 좋아하시나요?",
          "original_form": "저도 할머니랑 같이 살아서 할머니가 자주 요리해주세요ㅎㅎ 된장찌개랑 김치찌개 중에 어떤 걸 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100047489.1.12",
          "form": "저는 된장이 더 좋아요!",
          "original_form": "저는 된장이 더 좋아요!",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047489.1.13",
          "form": "분식에서 나오는 미소된장국도 좋아하구요",
          "original_form": "분식에서 나오는 미소된장국도 좋아하구요",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047489.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100047489.1.15",
          "form": "저는 김치찌개요! 된장도 좋아해요~",
          "original_form": "저는 김치찌개요! 된장도 좋아해요~",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047489.1.16",
          "form": "분식은 자주 드시나요?",
          "original_form": "분식은 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100047489.1.17",
          "form": "가끔 떡볶이 땡길 때가 있어요",
          "original_form": "가끔 떡볶이 땡길 때가 있어요",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100047489.1.18",
          "form": "볶음밥이나 김밥 먹고 싶을 띠ㅢ 분식점 가요",
          "original_form": "볶음밥이나 김밥 먹고 싶을 띠ㅢ 분식점 가요",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100047489.1.19",
          "form": "맛있죠! 어떤 김밥 제일 좋아하시나요? 저는 요즘 참치가 맛있더라고요!",
          "original_form": "맛있죠! 어떤 김밥 제일 좋아하시나요? 저는 요즘 참치가 맛있더라고요!",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047489.1.20",
          "form": "저도 참치 제일 좋아하는데",
          "original_form": "저도 참치 제일 좋아하는데",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047489.1.21",
          "form": "일반으로 먹을 때도 있어요",
          "original_form": "일반으로 먹을 때도 있어요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100047489.1.22",
          "form": "근데 참치엔 마요가 들어가야돼요",
          "original_form": "근데 참치엔 마요가 들어가야돼요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100047489.1.23",
          "form": "안그럼 뻑뻑해서",
          "original_form": "안그럼 뻑뻑해서",
          "speaker_id": "2",
          "time": "20210826 20:29"
        }
      ]
    }
