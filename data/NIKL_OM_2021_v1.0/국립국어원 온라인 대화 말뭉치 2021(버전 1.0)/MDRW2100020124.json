      "utterance": [
        {
          "id": "MDRW2100020124.1.1",
          "form": "그니까 재미도 없고",
          "original_form": "그니까 재미도 없고",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020124.1.2",
          "form": "감동도 없고",
          "original_form": "감동도 없고",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020124.1.3",
          "form": "좀 너무 나가는 거 같아",
          "original_form": "좀 너무 나가는 거 같아",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020124.1.4",
          "form": "그니까 원래 12회가 끝인데 연기되서 14회로 바뀐거야",
          "original_form": "그니까 원래 12회가 끝인데 연기되서 14회로 바뀐거야",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020124.1.5",
          "form": "아 연장한 거야?",
          "original_form": "아 연장한 거야?",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020124.1.6",
          "form": "응 찾아보니까 그렇던데?",
          "original_form": "응 찾아보니까 그렇던데?",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020124.1.7",
          "form": "아직도 시청률이 엄청 잘 나오나 보다",
          "original_form": "아직도 시청률이 엄청 잘 나오나 보다",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020124.1.8",
          "form": "그건 모르겠는데;;",
          "original_form": "그건 모르겠는데;;",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020124.1.9",
          "form": "그러니까 연장할 정도인 건 모르겠는데",
          "original_form": "그러니까 연장할 정도인 건 모르겠는데",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020124.1.10",
          "form": "올림픽 때문 아닌가?",
          "original_form": "올림픽 때문 아닌가?",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020124.1.11",
          "form": "근데 결방 때문에 밀린 게 아니라 회차 자체가 늘어난 거잖아",
          "original_form": "근데 결방 때문에 밀린 게 아니라 회차 자체가 늘어난 거잖아",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020124.1.12",
          "form": "둘 다 그래 연장도 맞고 연장도 맞음",
          "original_form": "둘 다 그래 연장도 맞고 연장도 맞음",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020124.1.13",
          "form": "그치 연기도 맞고 연장도 맞지",
          "original_form": "그치 연기도 맞고 연장도 맞지",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020124.1.14",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 23:55"
        },
        {
          "id": "MDRW2100020124.1.15",
