      "utterance": [
        {
          "id": "MDRW2100021650.1.1",
          "form": "재택근무 해봤어요?",
          "original_form": "재택근무 해봤어요?",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021650.1.2",
          "form": "네 해보긴 했는데 엄청 짧았어요ㅋㅋㅋ",
          "original_form": "네 해보긴 했는데 엄청 짧았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021650.1.3",
          "form": "대면이 무척 필요한 직종이라",
          "original_form": "대면이 무척 필요한 직종이라",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021650.1.4",
          "form": "재택 하세요?",
          "original_form": "재택 하세요?",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021650.1.5",
          "form": "지금은 안하고 있는데 재택근무 해보긴 했엉",
          "original_form": "지금은 안하고 있는데 재택근무 해보긴 했엉",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021650.1.6",
          "form": "님은 어떠셨어요?",
          "original_form": "님은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021650.1.7",
          "form": "전 심심하더라구요ㅋㅋ",
          "original_form": "전 심심하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021650.1.8",
          "form": "사무실에선 동료들과 소담소담 얘기하는 재미도 있는데",
          "original_form": "사무실에선 동료들과 소담소담 얘기하는 재미도 있는데",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021650.1.9",
          "form": "재택하면 너무 외로워용..",
          "original_form": "재택하면 너무 외로워용..",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021650.1.10",
          "form": "앗 맞아요 ㅋㅋㅋ",
          "original_form": "앗 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021650.1.11",
          "form": "뭔가 메신저도 안되니까 혼자 덩그러니 있는 기분이였어요",
          "original_form": "뭔가 메신저도 안되니까 혼자 덩그러니 있는 기분이였어요",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021650.1.12",
          "form": "그쵸 다들 연락도 두절되고ㅋㅋㅋㅋ",
          "original_form": "그쵸 다들 연락도 두절되고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021650.1.13",
          "form": "그러다가 갑자기 카톡으로 업무 지시 오고...",
          "original_form": "그러다가 갑자기 카톡으로 업무 지시 오고...",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021650.1.14",
          "form": "퇴근시간 지났는데도 뒤늦게 다시 하라 그러구...",
          "original_form": "퇴근시간 지났는데도 뒤늦게 다시 하라 그러구...",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021650.1.15",
          "form": "그니까요 재택 너무 피곤해요.. ㅋㅋㅋ",
          "original_form": "그니까요 재택 너무 피곤해요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021650.1.16",
          "form": "ㅋㅋㅋㅋ그래도 눈치 안보고 편하게 방구끼고 간식 먹는 건 좋더라구요?^^",
          "original_form": "ㅋㅋㅋㅋ그래도 눈치 안보고 편하게 방구끼고 간식 먹는 건 좋더라구요?^^",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021650.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021650.1.18",
          "form": "맞아요 그런건 확실히 좋았죠..ㅋㅋㅋㅋ",
          "original_form": "맞아요 그런건 확실히 좋았죠..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021650.1.19",
          "form": "그리고 가끔 할 일 없으면 옆에 넷플 틀어놓기도 하고 ㅋㅋ",
          "original_form": "그리고 가끔 할 일 없으면 옆에 넷플 틀어놓기도 하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021650.1.20",
          "form": "ㅋㅋㅋㅋㅋ자유로우셨네여",
          "original_form": "ㅋㅋㅋㅋㅋ자유로우셨네여",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021650.1.21",
          "form": "전 사실 수업해야해서 긴장감은 내내 유지하고 있었어요ㅋㅋㅋㅋ",
          "original_form": "전 사실 수업해야해서 긴장감은 내내 유지하고 있었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021650.1.22",
          "form": "허걱 진짜 긴장하셨겠어요",
          "original_form": "허걱 진짜 긴장하셨겠어요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021650.1.23",
          "form": "마이크 off인 줄 알고 실수라도 하면 ㄷㄷ",
          "original_form": "마이크 off인 줄 알고 실수라도 하면 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021650.1.24",
