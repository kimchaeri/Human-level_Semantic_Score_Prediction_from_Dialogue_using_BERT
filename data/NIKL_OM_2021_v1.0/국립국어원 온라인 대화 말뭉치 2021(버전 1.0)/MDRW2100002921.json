      "utterance": [
        {
          "id": "MDRW2100002921.1.1",
          "form": "저 예전에 미국 뉴욕에 메트로폴리탄 간 적 있는데",
          "original_form": "저 예전에 미국 뉴욕에 메트로폴리탄 간 적 있는데",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002921.1.2",
          "form": "어 그러시구나",
          "original_form": "어 그러시구나",
          "speaker_id": "1",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002921.1.3",
          "form": "엄청 크더라구요 전시관이",
          "original_form": "엄청 크더라구요 전시관이",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002921.1.4",
          "form": "오 저도 가보고 싶네요",
          "original_form": "오 저도 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002921.1.5",
          "form": "전부 꼼꼼히 보려면 며칠이 걸린다는 얘기가 있던데",
          "original_form": "전부 꼼꼼히 보려면 며칠이 걸린다는 얘기가 있던데",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002921.1.6",
          "form": "우와 진짜 큰가봐요",
          "original_form": "우와 진짜 큰가봐요",
          "speaker_id": "1",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002921.1.7",
          "form": "특히 기억 남는 작품 있나요?",
          "original_form": "특히 기억 남는 작품 있나요?",
          "speaker_id": "1",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002921.1.8",
          "form": "반고흐 별이 빛나는 밤에요",
          "original_form": "반고흐 별이 빛나는 밤에요",
          "speaker_id": "2",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002921.1.9",
          "form": "그 작품 앞에 사람 엄청 많더라구요",
          "original_form": "그 작품 앞에 사람 엄청 많더라구요",
          "speaker_id": "2",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002921.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002921.1.11",
          "form": "그 작품 저도 좋아요",
          "original_form": "그 작품 저도 좋아요",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002921.1.12",
          "form": "저 한국에 반고흐 전시 때 봤던 거 같은데",
          "original_form": "저 한국에 반고흐 전시 때 봤던 거 같은데",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002921.1.13",
          "form": "공연 관람 보러 가고싶네요",
          "original_form": "공연 관람 보러 가고싶네요",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002921.1.14",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002921.1.15",
          "form": "코로나 빨리 끝났으면 ㅠㅠ",
          "original_form": "코로나 빨리 끝났으면 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002921.1.16",
          "form": "코로나 안정되면 꼭 가야겠어요!",
          "original_form": "코로나 안정되면 꼭 가야겠어요!",
          "speaker_id": "1",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002921.1.17",
          "form": "저도요 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 16:45"
        }
      ]
