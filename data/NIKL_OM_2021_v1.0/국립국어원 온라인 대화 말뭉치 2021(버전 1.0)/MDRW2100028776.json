      "utterance": [
        {
          "id": "MDRW2100028776.1.1",
          "form": "언니 유튜브 봐?",
          "original_form": "언니 유튜브 봐?",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028776.1.2",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.3",
          "form": "유튜브 봐 언니?",
          "original_form": "유튜브 봐 언니?",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.4",
          "form": "주로",
          "original_form": "주로",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.5",
          "form": "뭘 봐?",
          "original_form": "뭘 봐?",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.6",
          "form": "주로 뮤비나 힐링영상 봐",
          "original_form": "주로 뮤비나 힐링영상 봐",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.7",
          "form": "그렇구나 !!",
          "original_form": "그렇구나 !!",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.8",
          "form": "코코넛 껍질 깎는 영상같은 거",
          "original_form": "코코넛 껍질 깎는 영상같은 거",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.9",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.10",
          "form": "보다보면 은근 재밋어.",
          "original_form": "보다보면 은근 재밋어.",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.11",
          "form": "빠져들어",
          "original_form": "빠져들어",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028776.1.12",
          "form": "너는어떤데?",
          "original_form": "너는어떤데?",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.13",
          "form": "나두 그렇게 생각해",
          "original_form": "나두 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.14",
          "form": "나는 게임영상이나",
          "original_form": "나는 게임영상이나",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.15",
          "form": "재밌는 하이라이트 영상이나",
          "original_form": "재밌는 하이라이트 영상이나",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.16",
          "form": "웃긴거나",
          "original_form": "웃긴거나",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.17",
          "form": "아무튼 웃긴걸봐 ㅎㅎ",
          "original_form": "아무튼 웃긴걸봐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.18",
          "form": "그렇구나 ㅎㅎ",
          "original_form": "그렇구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.19",
          "form": "게임을좋아하나보네",
          "original_form": "게임을좋아하나보네",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.20",
          "form": "게 임 조 아",
          "original_form": "게 임 조 아",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.21",
          "form": "나 두 조 아",
          "original_form": "나 두 조 아",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028776.1.22",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028776.1.23",
          "form": "주로 무슨게임해?",
          "original_form": "주로 무슨게임해?",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028776.1.24",
          "form": "좋은 노래 없나",
          "original_form": "좋은 노래 없나",
          "speaker_id": "1",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028776.1.25",
