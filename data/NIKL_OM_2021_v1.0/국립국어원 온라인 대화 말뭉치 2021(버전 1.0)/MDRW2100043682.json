      "utterance": [
        {
          "id": "MDRW2100043682.1.1",
          "form": "피부를 위해 세수하러 가야겠지...",
          "original_form": "피부를 위해 세수하러 가야겠지...",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043682.1.2",
          "form": "씻고 오십쇼",
          "original_form": "씻고 오십쇼",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043682.1.3",
          "form": "세수한번 좋지?",
          "original_form": "세수한번 좋지?",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043682.1.4",
          "form": "세수한번 잘 씻기더라",
          "original_form": "세수한번 잘 씻기더라",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043682.1.5",
          "form": "자극적이지 않은데 클렌징이 잘돼",
          "original_form": "자극적이지 않은데 클렌징이 잘돼",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043682.1.6",
          "form": "그래 그니까 얼른 씻고 오쇼",
          "original_form": "그래 그니까 얼른 씻고 오쇼",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043682.1.7",
          "form": "네 피부건강을 위해",
          "original_form": "네 피부건강을 위해",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043682.1.8",
          "form": "세수하고",
          "original_form": "세수하고",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043682.1.9",
          "form": "팩 붙이고 오겠습니다!",
          "original_form": "팩 붙이고 오겠습니다!",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043682.1.10",
          "form": "넹 다녀오세요",
          "original_form": "넹 다녀오세요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043682.1.11",
          "form": "피부건강을 위해 팩 붙이고 왔슴돠",
          "original_form": "피부건강을 위해 팩 붙이고 왔슴돠",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043682.1.12",
          "form": "훌륭하십니다",
          "original_form": "훌륭하십니다",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043682.1.13",
          "form": "피부가 고마워할거에요",
          "original_form": "피부가 고마워할거에요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043682.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043682.1.15",
          "form": "비타민은 요즘 안드십니까",
          "original_form": "비타민은 요즘 안드십니까",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043682.1.16",
          "form": "네 안먹었네요",
          "original_form": "네 안먹었네요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043682.1.17",
          "form": "지금 하나 먹을까",
          "original_form": "지금 하나 먹을까",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043682.1.18",
          "form": "네 드십시오",
          "original_form": "네 드십시오",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043682.1.19",
          "form": "영양제 챙기기",
          "original_form": "영양제 챙기기",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043682.1.20",
          "form": "ㅋㅋ종합비타민 있던가?",
          "original_form": "ㅋㅋ종합비타민 있던가?",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043682.1.21",
          "form": "걍 비타민 씨만 먹나?",
          "original_form": "걍 비타민 씨만 먹나?",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043682.1.22",
          "form": "비타민씨도 있고 루테인도 있습니다",
          "original_form": "비타민씨도 있고 루테인도 있습니다",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043682.1.23",
          "form": "건강을 위해 챙겨드십쇼",
          "original_form": "건강을 위해 챙겨드십쇼",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043682.1.24",
          "form": "잘 먹었어요",
          "original_form": "잘 먹었어요",
          "speaker_id": "2",
          "time": "20210907 20:21"
        }
      ]
