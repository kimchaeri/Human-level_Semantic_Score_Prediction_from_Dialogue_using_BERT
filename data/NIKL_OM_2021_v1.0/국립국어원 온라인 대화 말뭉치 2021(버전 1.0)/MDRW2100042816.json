      "utterance": [
        {
          "id": "MDRW2100042816.1.1",
          "form": "서울가고싶어용",
          "original_form": "서울가고싶어용",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042816.1.2",
          "form": "저는 목요일에가려고요 ㅋㅋㅋ  호캉스지만",
          "original_form": "저는 목요일에가려고요 ㅋㅋㅋ  호캉스지만",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042816.1.3",
          "form": "헐 잼겠어요 ㅠㅠ",
          "original_form": "헐 잼겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 10:41"
        },
        {
          "id": "MDRW2100042816.1.4",
          "form": "오랜만에 서울가는거라 신나네요 ㅋㅋㅋ",
          "original_form": "오랜만에 서울가는거라 신나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:48"
        },
        {
          "id": "MDRW2100042816.1.5",
          "form": "부러워요 ㅠㅠ",
          "original_form": "부러워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042816.1.6",
          "form": "어다안가시나요",
          "original_form": "어다안가시나요",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042816.1.7",
          "form": "ㅣ저도 가고싶네용",
          "original_form": "ㅣ저도 가고싶네용",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100042816.1.8",
          "form": "ㅋㅋㅋ왜안가셔요",
          "original_form": "ㅋㅋㅋ왜안가셔요",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100042816.1.9",
          "form": "코로나땜시..",
          "original_form": "코로나땜시..",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100042816.1.10",
          "form": "앗 호캉스정도는..",
          "original_form": "앗 호캉스정도는..",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042816.1.11",
          "form": "그러니까요 ㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:04"
        },
        {
          "id": "MDRW2100042816.1.12",
          "form": "ㅋㅋㅋ저는 이번주가 기대됩니더",
          "original_form": "ㅋㅋㅋ저는 이번주가 기대됩니더",
          "speaker_id": "1",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100042816.1.13",
          "form": "재미잇겠네요",
          "original_form": "재미잇겠네요",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100042816.1.14",
          "form": "가서 머하나요??",
          "original_form": "가서 머하나요??",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100042816.1.15",
          "form": "헬스가고..수영하고..밥먹고..",
          "original_form": "헬스가고..수영하고..밥먹고..",
