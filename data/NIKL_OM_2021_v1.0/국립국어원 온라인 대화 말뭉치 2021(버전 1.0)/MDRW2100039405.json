      "utterance": [
        {
          "id": "MDRW2100039405.1.1",
          "form": "게임 조아~ 전 즐겜러지만.",
          "original_form": "게임 조아~ 전 즐겜러지만.",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039405.1.2",
          "form": "할나 빡겜컨텐츠같더라고요 ㅇㄴ",
          "original_form": "할나 빡겜컨텐츠같더라고요 ㅇㄴ",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039405.1.3",
          "form": "할나가 뭐예요? 게임 이름?",
          "original_form": "할나가 뭐예요? 게임 이름?",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039405.1.4",
          "form": "애매한 취향이라 먼가,. 스팀게임들 취향맞는거 찾기 좀 힘든듯",
          "original_form": "애매한 취향이라 먼가,. 스팀게임들 취향맞는거 찾기 좀 힘든듯",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039405.1.5",
          "form": "전 스타듀밸리가 제일 맘에 들어요",
          "original_form": "전 스타듀밸리가 제일 맘에 들어요",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.6",
          "form": "그리고 비연시",
          "original_form": "그리고 비연시",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.7",
          "form": "ㅇㅇ 들어봤을걸 할로우나이트 게임이름",
          "original_form": "ㅇㅇ 들어봤을걸 할로우나이트 게임이름",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.8",
          "form": "비연시 하다 이게뭐임하고 접음",
          "original_form": "비연시 하다 이게뭐임하고 접음",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.9",
          "form": "할로윈 나이트가 아니었구나",
          "original_form": "할로윈 나이트가 아니었구나",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.10",
          "form": "원래 그런 병맛으로 하는 게임인걸",
          "original_form": "원래 그런 병맛으로 하는 게임인걸",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.11",
          "form": "스듀는 제가...잘 모르겠어요 시작도 안해봐서",
          "original_form": "스듀는 제가...잘 모르겠어요 시작도 안해봐서",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.12",
          "form": "난 병맛이 게임 주면 그다지...",
          "original_form": "난 병맛이 게임 주면 그다지...",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.13",
          "form": "센미한테 당장 영업해달라고 해",
          "original_form": "센미한테 당장 영업해달라고 해",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039405.1.14",
          "form": "전 재밋는거 조아해서요",
          "original_form": "전 재밋는거 조아해서요",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.15",
          "form": "로보토미 재밌더라",
          "original_form": "로보토미 재밌더라",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.16",
          "form": "컨트롤 어차피 개못하구",
          "original_form": "컨트롤 어차피 개못하구",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.17",
          "form": "겁도 많구",
          "original_form": "겁도 많구",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.18",
          "form": "스토리 진지해봤자 뽕차는 게 아니라 지루해해",
          "original_form": "스토리 진지해봤자 뽕차는 게 아니라 지루해해",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.19",
          "form": "시간이 없엉. 게임컨텐츠 고르는것중에 시간소모도 저는 꽤 들어가요 아무래도 지금 사정이 사정이라",
          "original_form": "시간이 없엉. 게임컨텐츠 고르는것중에 시간소모도 저는 꽤 들어가요 아무래도 지금 사정이 사정이라",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.20",
          "form": "전 컨은 하다보면 늘긴하는데... 시간붓기가 힘드니까",
          "original_form": "전 컨은 하다보면 늘긴하는데... 시간붓기가 힘드니까",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039405.1.21",
          "form": "스토리 진지한거 전 조아해",
          "original_form": "스토리 진지한거 전 조아해",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039405.1.22",
          "form": "탐정뎐 재밌어요 백합미연시야",
          "original_form": "탐정뎐 재밌어요 백합미연시야",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039405.1.23",
          "form": "전에 말해서 집어둿는데 까먹었다",
          "original_form": "전에 말해서 집어둿는데 까먹었다",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039405.1.24",
          "form": "전 힐링게임이 좋아요",
          "original_form": "전 힐링게임이 좋아요",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039405.1.25",
          "form": "투더문도 하려고했는데 공략없나 찾아봐야함 전 걍 혼자깨는건 지루해요",
          "original_form": "투더문도 하려고했는데 공략없나 찾아봐야함 전 걍 혼자깨는건 지루해요",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039405.1.26",
          "form": "힐링겜도 갠찬죠 굶지마도 나름 힐링겜인듯...?",
          "original_form": "힐링겜도 갠찬죠 굶지마도 나름 힐링겜인듯...?",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039405.1.27",
          "form": "굶지마가요...? 힐링겜 기준이 왜그래",
          "original_form": "굶지마가요...? 힐링겜 기준이 왜그래",
          "speaker_id": "1",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039405.1.28",
          "form": "힐링은 좋긴한데 너무 늘어지면 좀 재미 끌기가 힘들어서... 원래 제가 액션겜 위주로 했어서 그럴지도",
          "original_form": "힐링은 좋긴한데 너무 늘어지면 좀 재미 끌기가 힘들어서... 원래 제가 액션겜 위주로 했어서 그럴지도",
          "speaker_id": "2",
          "time": "20210903 17:23"
        },
        {
          "id": "MDRW2100039405.1.29",
          "form": "아 저 액션겜 하면 제 캐가 어딨는지 몰라서 죽어요",
          "original_form": "아 저 액션겜 하면 제 캐가 어딨는지 몰라서 죽어요",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039405.1.30",
          "form": "얼마나 귀여워요 스듀랑 비슷하잔아",
          "original_form": "얼마나 귀여워요 스듀랑 비슷하잔아",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039405.1.31",
          "form": "오 나 진짜 잘한다 했는데 상대캐더라고",
          "original_form": "오 나 진짜 잘한다 했는데 상대캐더라고",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039405.1.32",
          "form": "저도 그래요 근데 그건 처음엔 다 어쩔수없는듯",
          "original_form": "저도 그래요 근데 그건 처음엔 다 어쩔수없는듯",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039405.1.33",
          "form": "전 그 과정에서 재미를 느끼지 못하고 탈주했죠",
          "original_form": "전 그 과정에서 재미를 느끼지 못하고 탈주했죠",
          "speaker_id": "1",
          "time": "20210903 17:25"
        }
      ]
    }
