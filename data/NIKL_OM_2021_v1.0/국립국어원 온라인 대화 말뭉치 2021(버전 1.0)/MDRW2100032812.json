      "utterance": [
        {
          "id": "MDRW2100032812.1.1",
          "form": "군인 병장 월급 100만원쯤 주는거 어떠세여",
          "original_form": "군인 병장 월급 100만원쯤 주는거 어떠세여",
          "speaker_id": "1",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100032812.1.2",
          "form": "전 좋죠~",
          "original_form": "전 좋죠~",
          "speaker_id": "2",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100032812.1.3",
          "form": "세금만 이상하게 안쓰면 다 줄 수 있을거 같아요",
          "original_form": "세금만 이상하게 안쓰면 다 줄 수 있을거 같아요",
          "speaker_id": "2",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100032812.1.4",
          "form": "아 근데 엄청 금수저여도 괜찮을까요",
          "original_form": "아 근데 엄청 금수저여도 괜찮을까요",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100032812.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100032812.1.6",
          "form": "오 그러네요? 그래도 고생하는데 똑같이 줘도 될거 같아요",
          "original_form": "오 그러네요? 그래도 고생하는데 똑같이 줘도 될거 같아요",
          "speaker_id": "2",
          "time": "20210906 00:11"
        },
        {
          "id": "MDRW2100032812.1.7",
          "form": "대신 금수저면 세금 많이 내게 하기?",
          "original_form": "대신 금수저면 세금 많이 내게 하기?",
          "speaker_id": "2",
          "time": "20210906 00:12"
        },
        {
          "id": "MDRW2100032812.1.8",
          "form": "군대가면 많이들 고생하겠죠",
          "original_form": "군대가면 많이들 고생하겠죠",
          "speaker_id": "1",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032812.1.9",
          "form": "그러게요ㅠㅠ 다들 넘 고생하니 안쓰러워요",
          "original_form": "그러게요ㅠㅠ 다들 넘 고생하니 안쓰러워요",
          "speaker_id": "2",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032812.1.10",
          "form": "제 동생도 의경가서 힘들었는데 100만원 주라해 ㅠㅠ",
          "original_form": "제 동생도 의경가서 힘들었는데 100만원 주라해 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032812.1.11",
          "form": "에고ㅠㅠ 동생분도 고생하셧네요",
          "original_form": "에고ㅠㅠ 동생분도 고생하셧네요",
          "speaker_id": "2",
          "time": "20210906 00:13"
        },
        {
          "id": "MDRW2100032812.1.12",
          "form": "저희 오빠도 의경이었는데~",
          "original_form": "저희 오빠도 의경이었는데~",
          "speaker_id": "2",
          "time": "20210906 00:14"
        },
        {
          "id": "MDRW2100032812.1.13",
          "form": "촛불시위때 가서 정말 개고생",
          "original_form": "촛불시위때 가서 정말 개고생",
          "speaker_id": "1",
          "time": "20210906 00:14"
        },
        {
          "id": "MDRW2100032812.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:14"
        },
        {
          "id": "MDRW2100032812.1.15",
          "form": "키가 커서 맨날 잘 맞더라고요 죽창으로ㅠㅜ",
          "original_form": "키가 커서 맨날 잘 맞더라고요 죽창으로ㅠㅜ",
          "speaker_id": "2",
          "time": "20210906 00:14"
        },
        {
          "id": "MDRW2100032812.1.16",
          "form": "그래서 제가 그럴땐 구냥 수그리라고!! 했었네요 ㅋㅋㅋ 그게 잘 안되나 봐요",
          "original_form": "그래서 제가 그럴땐 구냥 수그리라고!! 했었네요 ㅋㅋㅋ 그게 잘 안되나 봐요",
          "speaker_id": "2",
          "time": "20210906 00:14"
        },
        {
          "id": "MDRW2100032812.1.17",
          "form": "어 남자분이신가요",
          "original_form": "어 남자분이신가요",
