      "utterance": [
        {
          "id": "MDRW2100030445.1.1",
          "form": "저도 굴 먹고 노로바이러스 걸려본 후로 굴 너무 무서워요..",
          "original_form": "저도 굴 먹고 노로바이러스 걸려본 후로 굴 너무 무서워요..",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100030445.1.2",
          "form": "그러니까요 ㅋㅋㅋ 노로죠",
          "original_form": "그러니까요 ㅋㅋㅋ 노로죠",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030445.1.3",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030445.1.4",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030445.1.5",
          "form": "힘드셨겠다",
          "original_form": "힘드셨겠다",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030445.1.6",
          "form": "안 걸려보셨어요?",
          "original_form": "안 걸려보셨어요?",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030445.1.7",
          "form": "네 안걸려봤어요 증상이 어때요?",
          "original_form": "네 안걸려봤어요 증상이 어때요?",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030445.1.8",
          "form": "장염이랑 비슷해요",
          "original_form": "장염이랑 비슷해요",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030445.1.9",
          "form": "알러지 있는 음식 없으세여?",
          "original_form": "알러지 있는 음식 없으세여?",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030445.1.10",
          "form": "어이쿠야 날 굴 먹으면 안되겠어요 ㅠㅠ",
          "original_form": "어이쿠야 날 굴 먹으면 안되겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030445.1.11",
          "form": "알러지는 없고 먹기 싫은 음식은 있어요",
          "original_form": "알러지는 없고 먹기 싫은 음식은 있어요",
          "speaker_id": "2",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030445.1.12",
          "form": "어떤 건가요?",
          "original_form": "어떤 건가요?",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030445.1.13",
          "form": "선지같은거나 내장류요",
          "original_form": "선지같은거나 내장류요",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030445.1.14",
          "form": "ㅋㅋ 그런거 좀 싫어요",
          "original_form": "ㅋㅋ 그런거 좀 싫어요",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030445.1.15",
          "form": "앗 저도 선지 싫어요",
          "original_form": "앗 저도 선지 싫어요",
          "speaker_id": "1",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030445.1.16",
          "form": "순대도 내장은 간만 조금 먹어욧",
          "original_form": "순대도 내장은 간만 조금 먹어욧",
          "speaker_id": "1",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030445.1.17",
          "form": "저두요!! name1님은 알러지 있으세요?",
          "original_form": "저두요!! &name1&님은 알러지 있으세요?",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030445.1.18",
          "form": "저도 다행히 그런 건 없어요",
          "original_form": "저도 다행히 그런 건 없어요",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030445.1.19",
          "form": "먹기 싫은 음식만 존재할뿐..어 아니다",
          "original_form": "먹기 싫은 음식만 존재할뿐..어 아니다",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030445.1.20",
          "form": "조개류 알러지가 있는 거 같아요 그러고보니..",
          "original_form": "조개류 알러지가 있는 거 같아요 그러고보니..",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030445.1.21",
          "form": "아 육수도 알러지 있으세요?",
          "original_form": "아 육수도 알러지 있으세요?",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030445.1.22",
          "form": "굴먹고 노로바이러스 걸리기 전에도 다같이 조개구이먹으면 늘 저만 탈났어요",
          "original_form": "굴먹고 노로바이러스 걸리기 전에도 다같이 조개구이먹으면 늘 저만 탈났어요",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030445.1.23",
          "form": "웬만하면 드시지 말아야겠네요",
          "original_form": "웬만하면 드시지 말아야겠네요",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030445.1.24",
          "form": "네네 솔직히",
          "original_form": "네네 솔직히",
