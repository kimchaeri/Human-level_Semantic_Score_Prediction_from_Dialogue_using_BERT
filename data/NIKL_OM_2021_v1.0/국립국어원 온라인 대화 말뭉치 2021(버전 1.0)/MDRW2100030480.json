      "utterance": [
        {
          "id": "MDRW2100030480.1.1",
          "form": "자네",
          "original_form": "자네",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030480.1.2",
          "form": "밥도 안 먹고 자네...",
          "original_form": "밥도 안 먹고 자네...",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030480.1.3",
          "form": "이궁",
          "original_form": "이궁",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030480.1.4",
          "form": "차려뒀으니까 배고프면 먹으러 가겠지",
          "original_form": "차려뒀으니까 배고프면 먹으러 가겠지",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030480.1.5",
          "form": "귀여워잇",
          "original_form": "귀여워잇",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030480.1.6",
          "form": "애기 천사같아...",
          "original_form": "애기 천사같아...",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030480.1.7",
          "form": "맞아 귀여워...",
          "original_form": "맞아 귀여워...",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030480.1.8",
          "form": "사간 장난감 좋아해줘서 기쁘다",
          "original_form": "사간 장난감 좋아해줘서 기쁘다",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030480.1.9",
          "form": "맞아 좋아해... 달린 게 작아서 그런가",
          "original_form": "맞아 좋아해... 달린 게 작아서 그런가",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030480.1.10",
          "form": "줄이 좀 길어서 대충 감아놨어",
          "original_form": "줄이 좀 길어서 대충 감아놨어",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030480.1.11",
          "form": "쪼끄만 걸로 골라오길 잘햇군",
          "original_form": "쪼끄만 걸로 골라오길 잘햇군",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030480.1.12",
          "form": "캣그라스에는 다시 도전 안하시나요",
          "original_form": "캣그라스에는 다시 도전 안하시나요",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030480.1.13",
          "form": "캣그라스...",
          "original_form": "캣그라스...",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030480.1.14",
          "form": "화분이랑 흙 사와서 다시 도전할까 생각 중인데",
          "original_form": "화분이랑 흙 사와서 다시 도전할까 생각 중인데",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030480.1.15",
          "form": "사러 가기 귀찮아",
          "original_form": "사러 가기 귀찮아",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030480.1.16",
          "form": "이열",
          "original_form": "이열",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030480.1.17",
          "form": "이열.",
          "original_form": "이열.",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100030480.1.18",
          "form": "나도 식물 쪽은 잘 몰라서",
          "original_form": "나도 식물 쪽은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.19",
          "form": "조언을 못해주겠네",
          "original_form": "조언을 못해주겠네",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.20",
          "form": "파이팅!",
          "original_form": "파이팅!",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.21",
          "form": "주변에 드루이드 있었으면 좋겠다",
          "original_form": "주변에 드루이드 있었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.22",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.23",
          "form": "키워달라고 하고 돈만 내고 싶다",
          "original_form": "키워달라고 하고 돈만 내고 싶다",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.24",
          "form": "식물성장 커미션",
          "original_form": "식물성장 커미션",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030480.1.25",
          "form": "귀리가 그렇게 잘 큰다는데",
          "original_form": "귀리가 그렇게 잘 큰다는데",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030480.1.26",
          "form": "다 거짓말이야",
          "original_form": "다 거짓말이야",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030480.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030480.1.28",
          "form": "나도 선인장 죽여봐서 할 말 x",
          "original_form": "나도 선인장 죽여봐서 할 말 x",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030480.1.29",
          "form": "씨 불릴 때 안 잠기게 했어야 했나",
          "original_form": "씨 불릴 때 안 잠기게 했어야 했나",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.30",
          "form": "너무 어렵다 식물은...",
          "original_form": "너무 어렵다 식물은...",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.31",
          "form": "설명서 같은 거 없었어?",
          "original_form": "설명서 같은 거 없었어?",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.32",
          "form": "거기엔 씨 불리라는 말이 없어서",
          "original_form": "거기엔 씨 불리라는 말이 없어서",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.33",
          "form": "그냥 흙에다 심고 물 주세용~ 했지",
          "original_form": "그냥 흙에다 심고 물 주세용~ 했지",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.34",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.35",
          "form": "근데 그렇게 하면 백퍼 죽을 것 같아서",
          "original_form": "근데 그렇게 하면 백퍼 죽을 것 같아서",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.36",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.37",
          "form": "님들만 아나요...",
          "original_form": "님들만 아나요...",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.38",
          "form": "그렇게 안 해도 죽는 거였음",
          "original_form": "그렇게 안 해도 죽는 거였음",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030480.1.39",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
