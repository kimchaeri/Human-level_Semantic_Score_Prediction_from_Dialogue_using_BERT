      "utterance": [
        {
          "id": "MDRW2100032833.1.1",
          "form": "강아지는 잘지냉?",
          "original_form": "강아지는 잘지냉?",
          "speaker_id": "1",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100032833.1.2",
          "form": "잘지내",
          "original_form": "잘지내",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032833.1.3",
          "form": "넌강아지 안키우고싶어?",
          "original_form": "넌강아지 안키우고싶어?",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032833.1.4",
          "form": "음 책임지는거 잘할 자신이 없어ㅠ",
          "original_form": "음 책임지는거 잘할 자신이 없어ㅠ",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032833.1.5",
          "form": "강아지 뭐좋아해??",
          "original_form": "강아지 뭐좋아해??",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032833.1.6",
          "form": "장난감 가지고 놀고 산책하는거 좋아해",
          "original_form": "장난감 가지고 놀고 산책하는거 좋아해",
          "speaker_id": "2",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100032833.1.7",
          "form": "예전에 반려동물 키워본 적 없어?",
          "original_form": "예전에 반려동물 키워본 적 없어?",
          "speaker_id": "2",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100032833.1.8",
          "form": "물고기 키워봤다 그리고 없는거 같은데ㅋㅋㅋ",
          "original_form": "물고기 키워봤다 그리고 없는거 같은데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100032833.1.9",
          "form": "물고기 내가전에 말했을걸",
          "original_form": "물고기 내가전에 말했을걸",
          "speaker_id": "1",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100032833.1.10",
          "form": "기생충때뮤에 다 사망했다구",
          "original_form": "기생충때뮤에 다 사망했다구",
          "speaker_id": "1",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100032833.1.11",
          "form": "으악ㅋㅋㅋㅋㅋㅋ",
          "original_form": "으악ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100032833.1.12",
          "form": "물고기 작은거?",
          "original_form": "물고기 작은거?",
          "speaker_id": "2",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100032833.1.13",
          "form": "종류별로 키웠었어ㅋㅋㅋ",
          "original_form": "종류별로 키웠었어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100032833.1.14",
          "form": "작은것도 있고",
          "original_form": "작은것도 있고",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100032833.1.15",
          "form": "초딩때 같은데",
          "original_form": "초딩때 같은데",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100032833.1.16",
          "form": "잘기억안나",
          "original_form": "잘기억안나",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100032833.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 13:54"
        },
        {
          "id": "MDRW2100032833.1.18",
          "form": "물고기 죽으면 어떻게했오",
          "original_form": "물고기 죽으면 어떻게했오",
          "speaker_id": "2",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100032833.1.19",
          "form": "어 진짜 좀 징그러워가지고 나충격받고",
          "original_form": "어 진짜 좀 징그러워가지고 나충격받고",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100032833.1.20",
          "form": "엄마가 처리했는데",
          "original_form": "엄마가 처리했는데",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100032833.1.21",
          "form": "그 처리할때 옆에서 보고있었거든?",
          "original_form": "그 처리할때 옆에서 보고있었거든?",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100032833.1.22",
          "form": "우리집은 변기에 버렸던듯ㅋㅋㅋㅋㅋ",
          "original_form": "우리집은 변기에 버렸던듯ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.23",
          "form": "거기서 기생충 한마리 툭 나옴..",
          "original_form": "거기서 기생충 한마리 툭 나옴..",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.24",
          "form": "진짜 충곀 아니 그게 애들이다",
          "original_form": "진짜 충곀 아니 그게 애들이다",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.26",
          "form": "마른채로 죽은거야",
          "original_form": "마른채로 죽은거야",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.27",
          "form": "으으으으으",
          "original_form": "으으으으으",
          "speaker_id": "2",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.28",
          "form": "그게다 엄마말로는 기생충짓 같대",
          "original_form": "그게다 엄마말로는 기생충짓 같대",
          "speaker_id": "1",
          "time": "20210906 13:58"
        },
        {
          "id": "MDRW2100032833.1.29",
          "form": "기생충이 커?",
          "original_form": "기생충이 커?",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100032833.1.30",
          "form": "난 진짜 하루에한마리씩 자꾸 죽어서",
          "original_form": "난 진짜 하루에한마리씩 자꾸 죽어서",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100032833.1.31",
          "form": "아니 실같았을걸",
          "original_form": "아니 실같았을걸",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100032833.1.32",
          "form": "물바까주고 그랬는데 계속죽어서 어쩔수없다고 엄마가 그러구",
          "original_form": "물바까주고 그랬는데 계속죽어서 어쩔수없다고 엄마가 그러구",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100032833.1.33",
          "form": "징그러워...",
          "original_form": "징그러워...",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100032833.1.34",
          "form": "그뒤로 안키워 물고기도",
          "original_form": "그뒤로 안키워 물고기도",
          "speaker_id": "1",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100032833.1.35",
          "form": "맞아,,ㅋㅋㅋ",
          "original_form": "맞아,,ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100032833.1.36",
          "form": "건강검진이 중요해 반려동물은",
          "original_form": "건강검진이 중요해 반려동물은",
          "speaker_id": "1",
          "time": "20210906 14:00"
        }
      ]
