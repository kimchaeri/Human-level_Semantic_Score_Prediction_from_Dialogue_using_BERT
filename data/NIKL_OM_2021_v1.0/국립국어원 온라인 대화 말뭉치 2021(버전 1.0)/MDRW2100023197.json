      "utterance": [
        {
          "id": "MDRW2100023197.1.1",
          "form": "교육철학 나누는건 전 좋은데",
          "original_form": "교육철학 나누는건 전 좋은데",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023197.1.2",
          "form": "가끔 피곤하기도 하더라구요",
          "original_form": "가끔 피곤하기도 하더라구요",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023197.1.3",
          "form": "아 마자여 근데 전 아직 막 뚜렷한 철학이 없어서 그런가",
          "original_form": "아 마자여 근데 전 아직 막 뚜렷한 철학이 없어서 그런가",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023197.1.4",
          "form": "그냥 이런 저런 얘기 들으니 좋더라구여 ㅋㅋㅋ",
          "original_form": "그냥 이런 저런 얘기 들으니 좋더라구여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.5",
          "form": "직장에서 교육철학 강요 좀 안했음 좋겠어요 ㅠ",
          "original_form": "직장에서 교육철학 강요 좀 안했음 좋겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.6",
          "form": "심지어 선생님들끼리 서로 싸워요 *^^* 기싸움..",
          "original_form": "심지어 선생님들끼리 서로 싸워요 *^^* 기싸움..",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.7",
          "form": "물론 과하면 피곤..ㅋㅋㅋ",
          "original_form": "물론 과하면 피곤..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.8",
          "form": "그리고 자꾸 적용을 강요하고..",
          "original_form": "그리고 자꾸 적용을 강요하고..",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.10",
          "form": "교육철학 안따라가면 교육에 관심 없는 선생님으로 매도하고 ㅠㅠ",
          "original_form": "교육철학 안따라가면 교육에 관심 없는 선생님으로 매도하고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.11",
          "form": "되게 으쌰으쌰하는 동네신가봐요",
          "original_form": "되게 으쌰으쌰하는 동네신가봐요",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.12",
          "form": "엄청 열정적이세요 학교가",
          "original_form": "엄청 열정적이세요 학교가",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023197.1.13",
          "form": "저도 한 열정하는데 저하고 방향은 좀 다르더라구요",
          "original_form": "저도 한 열정하는데 저하고 방향은 좀 다르더라구요",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023197.1.14",
          "form": "ㅋㅋㅋㅋㅋ그니까 말만 들어도 엄청 열정남 같아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그니까 말만 들어도 엄청 열정남 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023197.1.15",
          "form": "ㅠㅠ그런게 느껴져요 ?",
          "original_form": "ㅠㅠ그런게 느껴져요 ?",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023197.1.16",
          "form": "네네 열심히 사는거 같아여",
          "original_form": "네네 열심히 사는거 같아여",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023197.1.17",
          "form": "감사해요 기분이 좋네요 ㅎㅎㅎ",
          "original_form": "감사해요 기분이 좋네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023197.1.18",
          "form": "저도 혁신학교에 잇어서 좀 으쌰으쌰하는데",
          "original_form": "저도 혁신학교에 잇어서 좀 으쌰으쌰하는데",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.19",
          "form": "엇~ 저도 혁신학교...",
          "original_form": "엇~ 저도 혁신학교...",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.20",
          "form": "앜ㅋㅋㅋㅋㅋ반갑네옄ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ반갑네옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.21",
          "form": "그 지역이 서울은 아니시랬죠..?",
          "original_form": "그 지역이 서울은 아니시랬죠..?",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.23",
          "form": "저는 부산!!",
          "original_form": "저는 부산!!",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.24",
          "form": "휴 뭔가 다행이다~ZZZ",
          "original_form": "휴 뭔가 다행이다~ZZZ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023197.1.26",
          "form": "갑자기 같은 학교고 그러면 진짜 민망할 것 같아요",
          "original_form": "갑자기 같은 학교고 그러면 진짜 민망할 것 같아요",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.28",
          "form": "어우 생각도 하기싫다",
          "original_form": "어우 생각도 하기싫다",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.30",
          "form": "하지만 저희 학교에 젊은이가 별로 없기에",
          "original_form": "하지만 저희 학교에 젊은이가 별로 없기에",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아아",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.32",
          "form": "거기 학교에 남교사 몇분 계세용",
          "original_form": "거기 학교에 남교사 몇분 계세용",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023197.1.33",
          "form": "8명쯤 되는 거 같아요",
          "original_form": "8명쯤 되는 거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023197.1.34",
          "form": "어 좀 계시네요",
          "original_form": "어 좀 계시네요",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023197.1.35",
          "form": "기간제쌤들까지 합쳐서",
          "original_form": "기간제쌤들까지 합쳐서",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
