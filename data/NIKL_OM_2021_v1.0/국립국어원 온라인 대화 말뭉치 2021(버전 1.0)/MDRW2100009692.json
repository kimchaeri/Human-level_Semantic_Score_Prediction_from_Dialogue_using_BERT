      "utterance": [
        {
          "id": "MDRW2100009692.1.1",
          "form": "최근에 마이크를 하나샀어",
          "original_form": "최근에 마이크를 하나샀어",
          "speaker_id": "1",
          "time": "20210823 04:22"
        },
        {
          "id": "MDRW2100009692.1.2",
          "form": "어떤 마이크요?",
          "original_form": "어떤 마이크요?",
          "speaker_id": "2",
          "time": "20210823 04:22"
        },
        {
          "id": "MDRW2100009692.1.3",
          "form": "콘덴서 마이크인데 수음을 잘해",
          "original_form": "콘덴서 마이크인데 수음을 잘해",
          "speaker_id": "1",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.4",
          "form": "노래부르세요?",
          "original_form": "노래부르세요?",
          "speaker_id": "2",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.5",
          "form": "그럼 최고의 가수지",
          "original_form": "그럼 최고의 가수지",
          "speaker_id": "1",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.7",
          "form": "사람을 폭력적으로 만드는건 내가 일등일걸",
          "original_form": "사람을 폭력적으로 만드는건 내가 일등일걸",
          "speaker_id": "1",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.8",
          "form": "어... 음치?",
          "original_form": "어... 음치?",
          "speaker_id": "2",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.9",
          "form": "세간에서는 나를 그렇게 부르지",
          "original_form": "세간에서는 나를 그렇게 부르지",
          "speaker_id": "1",
          "time": "20210823 04:23"
        },
        {
          "id": "MDRW2100009692.1.10",
          "form": "당당하시네요",
          "original_form": "당당하시네요",
          "speaker_id": "2",
          "time": "20210823 04:24"
        },
        {
          "id": "MDRW2100009692.1.11",
          "form": "그럼 당당해야지",
          "original_form": "그럼 당당해야지",
          "speaker_id": "1",
          "time": "20210823 04:24"
        },
        {
          "id": "MDRW2100009692.1.12",
          "form": "그래도 마이크 세일할때 직구로 산거거든",
          "original_form": "그래도 마이크 세일할때 직구로 산거거든",
          "speaker_id": "1",
          "time": "20210823 04:24"
        },
        {
          "id": "MDRW2100009692.1.13",
          "form": "아마존에서 사셨어요?",
          "original_form": "아마존에서 사셨어요?",
          "speaker_id": "2",
          "time": "20210823 04:24"
        },
        {
          "id": "MDRW2100009692.1.14",
          "form": "일마존이긴 한데",
          "original_form": "일마존이긴 한데",
          "speaker_id": "1",
          "time": "20210823 04:24"
        },
        {
          "id": "MDRW2100009692.1.15",
          "form": "대충 19만원?",
          "original_form": "대충 19만원?",
          "speaker_id": "1",
          "time": "20210823 04:24"
        },
        {
          "id": "MDRW2100009692.1.16",
          "form": "그럼 통관비용들지 않아요?",
          "original_form": "그럼 통관비용들지 않아요?",
          "speaker_id": "2",
          "time": "20210823 04:25"
        },
        {
          "id": "MDRW2100009692.1.17",
          "form": "10만원 이상은 세금이 붙지",
          "original_form": "10만원 이상은 세금이 붙지",
          "speaker_id": "1",
          "time": "20210823 04:25"
        }
