      "utterance": [
        {
          "id": "MDRW2100036792.1.1",
          "form": "점심드셔야죠",
          "original_form": "점심드셔야죠",
          "speaker_id": "2",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100036792.1.2",
          "form": "아침을 늦게먹어서 배가안고파요",
          "original_form": "아침을 늦게먹어서 배가안고파요",
          "speaker_id": "1",
          "time": "20210831 12:16"
        },
        {
          "id": "MDRW2100036792.1.3",
          "form": "전 1인 보쌈 배달시켜 먹었어요. 배부르네요",
          "original_form": "전 1인 보쌈 배달시켜 먹었어요. 배부르네요",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100036792.1.4",
          "form": "오 보쌈 맛있겠어요 가격은 어느정도였어요?",
          "original_form": "오 보쌈 맛있겠어요 가격은 어느정도였어요?",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100036792.1.5",
          "form": "17000원이요.. 며칠 점심값 몰아 먹었어요 ㅎ",
          "original_form": "17000원이요.. 며칠 점심값 몰아 먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036792.1.6",
          "form": "1인보쌈인데 꽤 나가네요 ㅜㅜ",
          "original_form": "1인보쌈인데 꽤 나가네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036792.1.7",
          "form": "네..구성 좋고 맛있어서 날도 꾸리하고 \n맛있게 먹었으니 아깝지는 않어요 ~",
          "original_form": "네..구성 좋고 맛있어서 날도 꾸리하고 \n맛있게 먹었으니 아깝지는 않어요 ~",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036792.1.8",
          "form": "네 맛있게먹으면 된거죠!!!",
          "original_form": "네 맛있게먹으면 된거죠!!!",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036792.1.9",
          "form": "아침에 뭐 드셨어요?",
          "original_form": "아침에 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036792.1.10",
          "form": "아침엔 복숭아 조금 먹었어요",
          "original_form": "아침엔 복숭아 조금 먹었어요",
          "speaker_id": "1",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036792.1.11",
          "form": "커피나 음료는 어떤거 좋아하나요?",
          "original_form": "커피나 음료는 어떤거 좋아하나요?",
          "speaker_id": "2",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100036792.1.12",
          "form": "아이스 아메리카노가 막먹기좋던데요",
          "original_form": "아이스 아메리카노가 막먹기좋던데요",
          "speaker_id": "1",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100036792.1.13",
          "form": "더치커피도좋아요",
          "original_form": "더치커피도좋아요",
          "speaker_id": "1",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100036792.1.14",
          "form": "저는 달달구리 하게 마시는거 좋아요",
          "original_form": "저는 달달구리 하게 마시는거 좋아요",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036792.1.15",
          "form": "라떼나 스무디 같은것도 좋고 전 얼죽아 파에요",
          "original_form": "라떼나 스무디 같은것도 좋고 전 얼죽아 파에요",
