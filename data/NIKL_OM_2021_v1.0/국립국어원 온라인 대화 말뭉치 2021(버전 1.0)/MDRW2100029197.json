      "utterance": [
        {
          "id": "MDRW2100029197.1.1",
          "form": "지긍 말씀하신 그런",
          "original_form": "지긍 말씀하신 그런",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029197.1.2",
          "form": "부모님 뵙고 결혼으로 진행하려는 과정에서",
          "original_form": "부모님 뵙고 결혼으로 진행하려는 과정에서",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029197.1.3",
          "form": "헤어지게되는 그런상황",
          "original_form": "헤어지게되는 그런상황",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029197.1.4",
          "form": "음... 그리고 생각해보면",
          "original_form": "음... 그리고 생각해보면",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029197.1.5",
          "form": "상견례 하고도 많이 쫑나던데",
          "original_form": "상견례 하고도 많이 쫑나던데",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029197.1.6",
          "form": "이 사람이다 라는 \n그런 확신이 없었던거 같아요",
          "original_form": "이 사람이다 라는 \n그런 확신이 없었던거 같아요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029197.1.7",
          "form": "결혼하고 싶은 시기에 만나고 있던\n사람이 그 친구였고 그러다보니\n그 친구랑 그런 이야기를 하게 되고",
          "original_form": "결혼하고 싶은 시기에 만나고 있던\n사람이 그 친구였고 그러다보니\n그 친구랑 그런 이야기를 하게 되고",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029197.1.8",
          "form": "근데보통 그렇게 결혼하지 않나요",
          "original_form": "근데보통 그렇게 결혼하지 않나요",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029197.1.9",
          "form": "그렇게 진행이 되고 그랬던거 같아요",
          "original_form": "그렇게 진행이 되고 그랬던거 같아요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029197.1.10",
          "form": "친구가 똑같은 얘기를 했었는데",
          "original_form": "친구가 똑같은 얘기를 했었는데",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029197.1.11",
          "form": "그 친구랑 만나는 중에",
          "original_form": "그 친구랑 만나는 중에",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029197.1.12",
          "form": "지금 남편을 만났었거든요?",
          "original_form": "지금 남편을 만났었거든요?",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029197.1.13",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029197.1.14",
          "form": "그 한번의 만남이\n모든걸 바꿔놓게된거죠 ㅎㅎ",
          "original_form": "그 한번의 만남이\n모든걸 바꿔놓게된거죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029197.1.15",
          "form": "남편이랑 그 친구도 아는 사이에요",
          "original_form": "남편이랑 그 친구도 아는 사이에요",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029197.1.16",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029197.1.17",
          "form": "남편이랑 저는 대학동기고\n그 친구는 후배고 ㅋㅋㅋ",
          "original_form": "남편이랑 저는 대학동기고\n그 친구는 후배고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029197.1.18",
          "form": "이래서 함부로 측근을 소개해주면 안되는데",
          "original_form": "이래서 함부로 측근을 소개해주면 안되는데",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029197.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029197.1.20",
          "form": "ㅋㅋ위험하네요 지짜 ㅋㅋ",
          "original_form": "ㅋㅋ위험하네요 지짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029197.1.21",
          "form": "친구소개도",
          "original_form": "친구소개도",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029197.1.22",
          "form": "신중해야함",
          "original_form": "신중해야함",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029197.1.23",
          "form": "ㅋㅋㅋ다위험해요",
          "original_form": "ㅋㅋㅋ다위험해요",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029197.1.24",
          "form": "남녀사이는ㅋㅋ",
          "original_form": "남녀사이는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029197.1.25",
          "form": "서로 눈맞는걸 우리가 어떻게 할수없으니",
          "original_form": "서로 눈맞는걸 우리가 어떻게 할수없으니",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029197.1.26",
          "form": "남편이랑은 어떻게 만났는지\n이야기 했던가요? ㅋㅋㅋ",
          "original_form": "남편이랑은 어떻게 만났는지\n이야기 했던가요? ㅋㅋㅋ",
          "speaker_id": "1",
