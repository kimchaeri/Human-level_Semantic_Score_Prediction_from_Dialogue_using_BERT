      "utterance": [
        {
          "id": "MDRW2100044853.1.1",
          "form": "소주야",
          "original_form": "소주야",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.2",
          "form": "당연히 소주를 먹어야죠",
          "original_form": "당연히 소주를 먹어야죠",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.3",
          "form": "거예요",
          "original_form": "거예요",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.4",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.5",
          "form": "고민 중이에요 독도 소주 새로 나왔다는데",
          "original_form": "고민 중이에요 독도 소주 새로 나왔다는데",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.6",
          "form": "그건",
          "original_form": "그건",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.7",
          "form": "새로 나왔는데 도수가 상당히 낮아서 부드럽 대요",
          "original_form": "새로 나왔는데 도수가 상당히 낮아서 부드럽 대요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044853.1.8",
          "form": "어디서 파나요",
          "original_form": "어디서 파나요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.9",
          "form": "편의점에서",
          "original_form": "편의점에서",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.10",
          "form": "나 편의점",
          "original_form": "나 편의점",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.11",
          "form": "네 편의점 이요 어딘지는 잘 모르겠어요",
          "original_form": "네 편의점 이요 어딘지는 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.12",
          "form": "그렇구나 그쵸 회에는 소주 자",
          "original_form": "그렇구나 그쵸 회에는 소주 자",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.13",
          "form": "그런데 화이트 와인도 맛있어요 약간 스파크 있는 걸로",
          "original_form": "그런데 화이트 와인도 맛있어요 약간 스파크 있는 걸로",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.14",
          "form": "응 빨리 주말이 왔으면 좋겠네요",
          "original_form": "응 빨리 주말이 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.15",
          "form": "네 이제 화요일에 끝났는데 벌써 주말이 그립네요",
          "original_form": "네 이제 화요일에 끝났는데 벌써 주말이 그립네요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044853.1.16",
          "form": "저도 그러네요",
          "original_form": "저도 그러네요",
