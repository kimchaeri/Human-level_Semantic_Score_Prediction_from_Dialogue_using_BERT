      "utterance": [
        {
          "id": "MDRW2100010216.1.1",
          "form": "재택근무 하는거 어떻게 생각해,",
          "original_form": "재택근무 하는거 어떻게 생각해,",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010216.1.2",
          "form": "좋지 내 꿈이야",
          "original_form": "좋지 내 꿈이야",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010216.1.3",
          "form": "요즘 코로나 땜에 재택근무 많던데",
          "original_form": "요즘 코로나 땜에 재택근무 많던데",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010216.1.4",
          "form": "너무 좋은 거 같아",
          "original_form": "너무 좋은 거 같아",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010216.1.5",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010216.1.6",
          "form": "코로나 끝나도 계속 했으면 좋겠어",
          "original_form": "코로나 끝나도 계속 했으면 좋겠어",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010216.1.7",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010216.1.8",
          "form": "하지만 괜찮아지면 출근하라하겠지",
          "original_form": "하지만 괜찮아지면 출근하라하겠지",
          "speaker_id": "2",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010216.1.9",
          "form": "아 짜증나겠다",
          "original_form": "아 짜증나겠다",
          "speaker_id": "1",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010216.1.10",
          "form": "뭐 어쩔 수 없지",
          "original_form": "뭐 어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010216.1.11",
          "form": "맨날 재택근무하면 좋겠다",
          "original_form": "맨날 재택근무하면 좋겠다",
          "speaker_id": "1",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010216.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:48"
        },
        {
          "id": "MDRW2100010216.1.13",
          "form": "재택근무 해보고 싶다",
          "original_form": "재택근무 해보고 싶다",
          "speaker_id": "1",
          "time": "20210823 15:49"
        },
        {
          "id": "MDRW2100010216.1.14",
          "form": "그러게 일이 제대로 되려나",
          "original_form": "그러게 일이 제대로 되려나",
