      "utterance": [
        {
          "id": "MDRW2100015569.1.1",
          "form": "안녕하세요\r\n커피 좋아하시나요?",
          "original_form": "안녕하세요\r\n커피 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015569.1.2",
          "form": "네ㅎㅎ 저는 라떼 좋아해요",
          "original_form": "네ㅎㅎ 저는 라떼 좋아해요",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015569.1.3",
          "form": "커피 자주 드세용?",
          "original_form": "커피 자주 드세용?",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015569.1.4",
          "form": "네 하루한잔은 마셔요",
          "original_form": "네 하루한잔은 마셔요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.5",
          "form": "라떼는 커피가 진해야 맛나더라구요",
          "original_form": "라떼는 커피가 진해야 맛나더라구요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.6",
          "form": "저도 하루에 한잔은 마시는것같아요ㅎㅎ",
          "original_form": "저도 하루에 한잔은 마시는것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.8",
          "form": "아이스 ? 핫?? 뭐를 즐기시나요",
          "original_form": "아이스 ? 핫?? 뭐를 즐기시나요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.9",
          "form": "그리고 우유도 진한걸로 해야 맛있어요",
          "original_form": "그리고 우유도 진한걸로 해야 맛있어요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.10",
          "form": "전 얼죽아에요 ㅋㅋ",
          "original_form": "전 얼죽아에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015569.1.11",
          "form": "맞아요 매일우유가 맛났어요",
          "original_form": "맞아요 매일우유가 맛났어요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015569.1.12",
          "form": "어느 커피숍 라떼가 맛있어요?",
          "original_form": "어느 커피숍 라떼가 맛있어요?",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015569.1.13",
          "form": "음 스타벅스랑 폴바셋이요",
          "original_form": "음 스타벅스랑 폴바셋이요",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015569.1.14",
          "form": "그리고 맥도날드꺼도 맛있어요 ㅎㅎ",
          "original_form": "그리고 맥도날드꺼도 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015569.1.15",
          "form": "카페는 주로 어디로 가세요?",
          "original_form": "카페는 주로 어디로 가세요?",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015569.1.16",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015569.1.17",
          "form": "폴바셋도 맛나죠 ㅎ",
          "original_form": "폴바셋도 맛나죠 ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015569.1.18",
          "form": "맥도날드 가성비 짱이에요",
          "original_form": "맥도날드 가성비 짱이에요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015569.1.19",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015569.1.20",
          "form": "커피 얘기하니깐 마시고싶네요ㅜ",
          "original_form": "커피 얘기하니깐 마시고싶네요ㅜ",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015569.1.21",
          "form": "맥도날드 커피 한동안 자주마셨어요 여름 아아로..ㅎ",
          "original_form": "맥도날드 커피 한동안 자주마셨어요 여름 아아로..ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015569.1.22",
          "form": "네 비가오니",
          "original_form": "네 비가오니",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015569.1.23",
          "form": "따뜻한 카푸치노 생각나네요",
          "original_form": "따뜻한 카푸치노 생각나네요",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015569.1.24",
          "form": "크 카푸치노도 맛있죠ㅎㅎㅎ",
          "original_form": "크 카푸치노도 맛있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015569.1.25",
          "form": "전 커피값이 너무 많이나가서",
          "original_form": "전 커피값이 너무 많이나가서",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015569.1.26",
          "form": "커피머신을 샀어용ㅎㅎ",
          "original_form": "커피머신을 샀어용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015569.1.27",
          "form": "저두 샀어요 ㅎㅎ",
          "original_form": "저두 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015569.1.28",
