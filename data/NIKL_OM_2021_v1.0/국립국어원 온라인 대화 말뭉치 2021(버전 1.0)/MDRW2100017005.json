      "utterance": [
        {
          "id": "MDRW2100017005.1.1",
          "form": "전시회 자주 보러 가세요?",
          "original_form": "전시회 자주 보러 가세요?",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100017005.1.2",
          "form": "전시회는 못 가봤어요 ㅠㅠ 좋아하세요?",
          "original_form": "전시회는 못 가봤어요 ㅠㅠ 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017005.1.3",
          "form": "네네 좋아해요! 근데 요즘엔 가기 힘드네요",
          "original_form": "네네 좋아해요! 근데 요즘엔 가기 힘드네요",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017005.1.4",
          "form": "그러겠어요 ㅠㅠ",
          "original_form": "그러겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017005.1.5",
          "form": "무슨 전시회 위주로 다니셨어요?",
          "original_form": "무슨 전시회 위주로 다니셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100017005.1.6",
          "form": "저는 사진전 좋아해요",
          "original_form": "저는 사진전 좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017005.1.7",
          "form": "최근에 요시고사진전 핫하덩데",
          "original_form": "최근에 요시고사진전 핫하덩데",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017005.1.8",
          "form": "대기가 3시간이어서 못봣어요 ㅠ",
          "original_form": "대기가 3시간이어서 못봣어요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017005.1.9",
          "form": "헉 엄청나네요",
          "original_form": "헉 엄청나네요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017005.1.10",
          "form": "인기가 진짜 많나봐요",
          "original_form": "인기가 진짜 많나봐요",
          "speaker_id": "1",
          "time": "20210826 19:55"
        },
        {
          "id": "MDRW2100017005.1.11",
          "form": "네네 ㅠㅠ 서울에서하는건데 간김에 볼랬더니 못봐서 아쉽네요 ㅠ",
          "original_form": "네네 ㅠㅠ 서울에서하는건데 간김에 볼랬더니 못봐서 아쉽네요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017005.1.12",
          "form": "ㅠㅠ 정말 아쉬우셨겠어요 시간내서 오셨는데",
          "original_form": "ㅠㅠ 정말 아쉬우셨겠어요 시간내서 오셨는데",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017005.1.13",
          "form": "그래도 다행인건 다른볼일때문에 간거라 ㅠㅠ 그래도 많이아쉽네용",
          "original_form": "그래도 다행인건 다른볼일때문에 간거라 ㅠㅠ 그래도 많이아쉽네용",
          "speaker_id": "2",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017005.1.14",
          "form": "그나마 다행이네요 ㅠㅠ 다음엔 원하는 전시회 꼭 성공하시길!!",
          "original_form": "그나마 다행이네요 ㅠㅠ 다음엔 원하는 전시회 꼭 성공하시길!!",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017005.1.15",
          "form": "네네 연극은 보신적 있어요?",
          "original_form": "네네 연극은 보신적 있어요?",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017005.1.16",
          "form": "예전에 고등학생때 봤는데 제목도 기억이 안나여 너무 옛날이라 ㅋㅋㅋ",
          "original_form": "예전에 고등학생때 봤는데 제목도 기억이 안나여 너무 옛날이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017005.1.17",
          "form": "ㅋㅋㅋㅋㅋ그렇군요 저는 연극은 본적없어요",
          "original_form": "ㅋㅋㅋㅋㅋ그렇군요 저는 연극은 본적없어요",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
