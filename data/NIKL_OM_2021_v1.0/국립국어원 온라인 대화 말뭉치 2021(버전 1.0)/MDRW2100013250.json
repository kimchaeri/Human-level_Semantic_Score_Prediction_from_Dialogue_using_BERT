      "utterance": [
        {
          "id": "MDRW2100013250.1.1",
          "form": "무슨커피가 좋아??",
          "original_form": "무슨커피가 좋아??",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.2",
          "form": "음 나는",
          "original_form": "음 나는",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.3",
          "form": "바닐라라떼",
          "original_form": "바닐라라떼",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.4",
          "form": "그거맛잇어?",
          "original_form": "그거맛잇어?",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.5",
          "form": "엉 조아",
          "original_form": "엉 조아",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.6",
          "form": "완전 달달한 커피맛",
          "original_form": "완전 달달한 커피맛",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.7",
          "form": "나두 나중에먹어봐야지",
          "original_form": "나두 나중에먹어봐야지",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.8",
          "form": "후회안할 맛이야",
          "original_form": "후회안할 맛이야",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.9",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.10",
          "form": "나는 그",
          "original_form": "나는 그",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.11",
          "form": "초코라떼",
          "original_form": "초코라떼",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.12",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013250.1.13",
          "form": "커피는 아직별로구망",
          "original_form": "커피는 아직별로구망",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013250.1.14",
          "form": "마잣",
          "original_form": "마잣",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013250.1.15",
          "form": "커피는 뭐가 맛난지 모르겟어",
          "original_form": "커피는 뭐가 맛난지 모르겟어",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013250.1.16",
          "form": "일단 카푸치노나",
          "original_form": "일단 카푸치노나",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013250.1.17",
          "form": "오오그래야겟다",
          "original_form": "오오그래야겟다",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013250.1.18",
          "form": "라떼 같은걸 먹어",
          "original_form": "라떼 같은걸 먹어",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013250.1.19",
          "form": "알써",
          "original_form": "알써",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
