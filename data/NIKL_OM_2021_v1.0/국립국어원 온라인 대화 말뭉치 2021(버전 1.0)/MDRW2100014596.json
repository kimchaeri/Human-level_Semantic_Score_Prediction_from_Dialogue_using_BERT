      "utterance": [
        {
          "id": "MDRW2100014596.1.1",
          "form": "안녕하세여ㅎㅎ",
          "original_form": "안녕하세여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014596.1.2",
          "form": "좋아하는 영화있으신가요?",
          "original_form": "좋아하는 영화있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014596.1.3",
          "form": "영화보다는 예능자주봅니다",
          "original_form": "영화보다는 예능자주봅니다",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014596.1.4",
          "form": "영화 추천해주실 수 있으세요?",
          "original_form": "영화 추천해주실 수 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014596.1.5",
          "form": "예능뭐보세요?",
          "original_form": "예능뭐보세요?",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014596.1.6",
          "form": "요즘은 런닝맨 일박이일봐요",
          "original_form": "요즘은 런닝맨 일박이일봐요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014596.1.7",
          "form": "오오ㅎ 저도 런닝맨좋아해요ㅎ",
          "original_form": "오오ㅎ 저도 런닝맨좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100014596.1.8",
          "form": "누굴 제일 응원하세요?",
          "original_form": "누굴 제일 응원하세요?",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100014596.1.9",
          "form": "전 유재석이요ㅎㅎ",
          "original_form": "전 유재석이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100014596.1.10",
          "form": "재밌어서ㅎㅎ",
          "original_form": "재밌어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100014596.1.11",
          "form": "누구좋아하세요?",
          "original_form": "누구좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100014596.1.12",
          "form": "저도!!! ㅋㅋ 전 사실 오래된 무한재석교 신자예묘",
          "original_form": "저도!!! ㅋㅋ 전 사실 오래된 무한재석교 신자예묘",
          "speaker_id": "2",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100014596.1.13",
          "form": "암요ㅎㅎ",
          "original_form": "암요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100014596.1.14",
          "form": "유재석고 재석고ㅎㅎ",
          "original_form": "유재석고 재석고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100014596.1.15",
          "form": "이광수도좋아했었는데ㅠ아쉽네요ㅠㅠ",
          "original_form": "이광수도좋아했었는데ㅠ아쉽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100014596.1.16",
          "form": "광수 ㅜㅠ 마지막편 아직 못봤어요",
          "original_form": "광수 ㅜㅠ 마지막편 아직 못봤어요",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100014596.1.17",
          "form": "ㅎㅎ 저두요ㅎㅎ",
          "original_form": "ㅎㅎ 저두요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100014596.1.18",
          "form": "피곤해서 맨날 앞에만보고 뒤에는패쓰요ㅎㅎ",
          "original_form": "피곤해서 맨날 앞에만보고 뒤에는패쓰요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100014596.1.19",
          "form": "피곤하면 왜 티비보면서 자는걸까요?ㅎㄹ",
          "original_form": "피곤하면 왜 티비보면서 자는걸까요?ㅎㄹ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100014596.1.20",
          "form": "ㅋㅋ꿀잠자요ㅋㅋ",
          "original_form": "ㅋㅋ꿀잠자요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100014596.1.21",
          "form": "그리고 밤에 잠이 안오고...ㅜ",
          "original_form": "그리고 밤에 잠이 안오고...ㅜ",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100014596.1.22",
          "form": "영상틀어놓고자는편이세요?ㅎㅎㅎ",
          "original_form": "영상틀어놓고자는편이세요?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100014596.1.23",
          "form": "왜요?ㅎㅎ",
          "original_form": "왜요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100014596.1.24",
          "form": "낮에 자면 밤에 잠이 안오고 오만 컨텐츠가 다 재밌고요",
          "original_form": "낮에 자면 밤에 잠이 안오고 오만 컨텐츠가 다 재밌고요",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100014596.1.25",
          "form": "빙고요ㅎㅎㅎ",
          "original_form": "빙고요ㅎㅎㅎ",
