      "utterance": [
        {
          "id": "MDRW2100035612.1.1",
          "form": "그래서 어떻게 되셨어요??",
          "original_form": "그래서 어떻게 되셨어요??",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035612.1.2",
          "form": "칼국수 집에 직접가서 드신거죠?",
          "original_form": "칼국수 집에 직접가서 드신거죠?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035612.1.3",
          "form": "네네 ㅜㅜ",
          "original_form": "네네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.4",
          "form": "주인한테 오라고해서 이게 뭐냐고",
          "original_form": "주인한테 오라고해서 이게 뭐냐고",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.5",
          "form": "나왔다고 하니까 손으로 갖고가서는",
          "original_form": "나왔다고 하니까 손으로 갖고가서는",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.6",
          "form": "주먹으로 휴지를 쥐는거에뇨",
          "original_form": "주먹으로 휴지를 쥐는거에뇨",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.7",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.8",
          "form": "사진안찍고 부른건데",
          "original_form": "사진안찍고 부른건데",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.9",
          "form": "경황이 없어서 사진찍는걸 깜빡했어요",
          "original_form": "경황이 없어서 사진찍는걸 깜빡했어요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.10",
          "form": "주인이 사과도안하고 그냥 가시라고 하길래",
          "original_form": "주인이 사과도안하고 그냥 가시라고 하길래",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100035612.1.11",
          "form": "사진찍어야 신고할거잖아뇨",
          "original_form": "사진찍어야 신고할거잖아뇨",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035612.1.12",
          "form": "증거자료가 될진 모르겠어요 그래도 신고는 해보려구요",
          "original_form": "증거자료가 될진 모르겠어요 그래도 신고는 해보려구요",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035612.1.13",
          "form": "개업한 집이었는데 황당했어요 ㅠㅠ",
          "original_form": "개업한 집이었는데 황당했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035612.1.14",
          "form": "헐...... 와 진짜 양심없다...",
          "original_form": "헐...... 와 진짜 양심없다...",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.15",
          "form": "와 개업한집인데 그모양...",
          "original_form": "와 개업한집인데 그모양...",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.16",
          "form": "해운대31cm해물칼국수입니다",
          "original_form": "해운대31cm해물칼국수입니다",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.17",
          "form": "당근같은데도 올리면 비슷한 경험있는분들 댓글달고 하더라구요!!",
          "original_form": "당근같은데도 올리면 비슷한 경험있는분들 댓글달고 하더라구요!!",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.18",
          "form": "기억할게여...",
          "original_form": "기억할게여...",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.19",
          "form": "전국에 체인점많어요",
          "original_form": "전국에 체인점많어요",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.20",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035612.1.21",
          "form": "저희동네에는 진짜 맛집으로 유명한 곳 있는데 인기가 많아서 몇년만에 근처에 2호점 냈거든요",
          "original_form": "저희동네에는 진짜 맛집으로 유명한 곳 있는데 인기가 많아서 몇년만에 근처에 2호점 냈거든요",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035612.1.22",
          "form": "근데 음식에서 담배꽁초나왔대요",
          "original_form": "근데 음식에서 담배꽁초나왔대요",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035612.1.23",
          "form": "그리구 들어가는 입구 사이로 보면 주방장들이 밖에서 담배피고 그런다고;;",
          "original_form": "그리구 들어가는 입구 사이로 보면 주방장들이 밖에서 담배피고 그런다고;;",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035612.1.24",
          "form": "근데 저도 유명세에 비해서 청결하단느낌 받은적은 없거든요",
          "original_form": "근데 저도 유명세에 비해서 청결하단느낌 받은적은 없거든요",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035612.1.25",
          "form": "그래서 다들 알만하다고 댓글 달고 그러더라구요",
          "original_form": "그래서 다들 알만하다고 댓글 달고 그러더라구요",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035612.1.26",
          "form": "어으 진짜 별 일이네요..",
          "original_form": "어으 진짜 별 일이네요..",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035612.1.27",
          "form": "헐 어떻게 하면 음식에 담배꽁초가 들어갈까요",
          "original_form": "헐 어떻게 하면 음식에 담배꽁초가 들어갈까요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035612.1.28",
          "form": "진짜 너무 황당하네요",
          "original_form": "진짜 너무 황당하네요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035612.1.29",
          "form": "저도 이렇게 뭐가 나온건 첨이라 너무 기분이더러워요",
          "original_form": "저도 이렇게 뭐가 나온건 첨이라 너무 기분이더러워요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035612.1.30",
          "form": "그르게요.. 진짜ㅡㅡ.. 토닥토닥..",
          "original_form": "그르게요.. 진짜ㅡㅡ.. 토닥토닥..",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035612.1.31",
          "form": "ㅠㅠ 감사합니다",
          "original_form": "ㅠㅠ 감사합니다",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035612.1.32",
          "form": "저도 요즘 비오고 그래서 칼국수 먹고싶던 참인데...",
          "original_form": "저도 요즘 비오고 그래서 칼국수 먹고싶던 참인데...",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035612.1.33",
          "form": "걍 안먹어야겠어요..",
          "original_form": "걍 안먹어야겠어요..",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035612.1.34",
          "form": "저희동네에는 없는 체인점같긴하지만",
          "original_form": "저희동네에는 없는 체인점같긴하지만",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035612.1.35",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100035612.1.36",
          "form": "음식점들이 하는게",
          "original_form": "음식점들이 하는게",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035612.1.37",
          "form": "그러면서 또 별로라하면 악플이라며 고소한다하구요",
          "original_form": "그러면서 또 별로라하면 악플이라며 고소한다하구요",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100035612.1.38",
          "form": "ㅜㅜ 그러긴하더라구요..",
          "original_form": "ㅜㅜ 그러긴하더라구요..",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035612.1.39",
          "form": "만약 당근같은곳에 올리면 가게명 다올리지말고 추측할수있을 정도로만 적으셔야할거에요ㅠ",
          "original_form": "만약 당근같은곳에 올리면 가게명 다올리지말고 추측할수있을 정도로만 적으셔야할거에요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:34"
        }
      ]
    }
