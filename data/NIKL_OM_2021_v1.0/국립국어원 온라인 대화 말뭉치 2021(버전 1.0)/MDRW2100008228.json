      "utterance": [
        {
          "id": "MDRW2100008228.1.1",
          "form": "여름도 이제 다 가네요",
          "original_form": "여름도 이제 다 가네요",
          "speaker_id": "2",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008228.1.2",
          "form": "네 선선한 날씨같아요",
          "original_form": "네 선선한 날씨같아요",
          "speaker_id": "1",
          "time": "20210822 14:19"
        },
        {
          "id": "MDRW2100008228.1.3",
          "form": "장마 영향으로 비가 많이 오는데",
          "original_form": "장마 영향으로 비가 많이 오는데",
          "speaker_id": "2",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008228.1.4",
          "form": "선선해져서 좋아요",
          "original_form": "선선해져서 좋아요",
          "speaker_id": "2",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008228.1.5",
          "form": "맞아요 저녁에 산책도 하고 좋아요",
          "original_form": "맞아요 저녁에 산책도 하고 좋아요",
          "speaker_id": "1",
          "time": "20210822 14:20"
        },
        {
          "id": "MDRW2100008228.1.6",
          "form": "어느 계절을 제일 좋아하세요?",
          "original_form": "어느 계절을 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008228.1.7",
          "form": "저는 가을을 제일 좋아해요",
          "original_form": "저는 가을을 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008228.1.8",
          "form": "오 이제 제일 좋아하는 가을이 오겠네요",
          "original_form": "오 이제 제일 좋아하는 가을이 오겠네요",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008228.1.9",
          "form": "내일인가가 처서이던데",
          "original_form": "내일인가가 처서이던데",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008228.1.10",
          "form": "진짜 가을이에요!!!",
          "original_form": "진짜 가을이에요!!!",
          "speaker_id": "2",
          "time": "20210822 14:21"
        },
        {
          "id": "MDRW2100008228.1.11",
          "form": "맞아요 금방 지나가기전에 최대한 가을을 잘보내고 싶어요ㅎㅎ",
          "original_form": "맞아요 금방 지나가기전에 최대한 가을을 잘보내고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008228.1.12",
          "form": "가을은 넘 빨리가요ㅠ",
          "original_form": "가을은 넘 빨리가요ㅠ",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008228.1.13",
          "form": "제일 걷기 좋고 떠나기 좋은 날씨인데 말이에요",
          "original_form": "제일 걷기 좋고 떠나기 좋은 날씨인데 말이에요",
          "speaker_id": "2",
          "time": "20210822 14:23"
        },
        {
          "id": "MDRW2100008228.1.14",
          "form": "맞아요 봄엔 벚꽃도 못봐서 이번 가을은 산책도 하고 소풍도 다니고 싶어요ㅎㅎ",
          "original_form": "맞아요 봄엔 벚꽃도 못봐서 이번 가을은 산책도 하고 소풍도 다니고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008228.1.15",
          "form": "맞아요 이제 긴팔 입어도 될 것 같아요",
          "original_form": "맞아요 이제 긴팔 입어도 될 것 같아요",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008228.1.16",
          "form": "여름옷도 집어넣고 예쁜 가을 옷 입고 싶어요",
          "original_form": "여름옷도 집어넣고 예쁜 가을 옷 입고 싶어요",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008228.1.17",
          "form": "맞아요 트렌치도 미리 사두고 보고만 있어요ㅎㅎ",
          "original_form": "맞아요 트렌치도 미리 사두고 보고만 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008228.1.18",
          "form": "트렌치 좋아요ㅠ",
