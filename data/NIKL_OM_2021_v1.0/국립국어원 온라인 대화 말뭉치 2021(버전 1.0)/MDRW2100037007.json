      "utterance": [
        {
          "id": "MDRW2100037007.1.1",
          "form": "음식중에 제일 좋아하시는건 어떤거에요?",
          "original_form": "음식중에 제일 좋아하시는건 어떤거에요?",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037007.1.2",
          "form": "초밥이랑 갈비찜 젤좋아해요 ㅎㅎ",
          "original_form": "초밥이랑 갈비찜 젤좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037007.1.3",
          "form": "오웅 둘 다 너무 맛있죠",
          "original_form": "오웅 둘 다 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100037007.1.4",
          "form": "진짜 조합도 최고예요",
          "original_form": "진짜 조합도 최고예요",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100037007.1.5",
          "form": "네 돈많으면 매일 먹을텐데 ㅠㅠ",
          "original_form": "네 돈많으면 매일 먹을텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037007.1.6",
          "form": "진짜요ㅠ",
          "original_form": "진짜요ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037007.1.7",
          "form": "매일먹어도 질릴수가 없는것들이에요",
          "original_form": "매일먹어도 질릴수가 없는것들이에요",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037007.1.8",
          "form": "그러니까요 ㅎㅎㅎ 얘기하니 먹고싶네요",
          "original_form": "그러니까요 ㅎㅎㅎ 얘기하니 먹고싶네요",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100037007.1.9",
          "form": "저두..ㅋㅋㅋ",
          "original_form": "저두..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100037007.1.10",
          "form": "갈비 사실 없어서 못먹는거잖아욤",
          "original_form": "갈비 사실 없어서 못먹는거잖아욤",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100037007.1.11",
          "form": "그죠 맞습니다",
          "original_form": "그죠 맞습니다",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037007.1.12",
          "form": "비싸서 잘 안해줘요",
          "original_form": "비싸서 잘 안해줘요",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100037007.1.13",
          "form": "맞아여ㅠ",
          "original_form": "맞아여ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100037007.1.14",
          "form": "고기값이 또 치솟잖아요ㅜ",
          "original_form": "고기값이 또 치솟잖아요ㅜ",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100037007.1.15",
          "form": "아 그래요 고객 값이 왜 치솟는 거예요?",
          "original_form": "아 그래요 고객 값이 왜 치솟는 거예요?",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100037007.1.16",
          "form": "날씨에 따라 돼지병걸렸거나.. 이럴때 오른다고 들었어요ㅠ",
          "original_form": "날씨에 따라 돼지병걸렸거나.. 이럴때 오른다고 들었어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100037007.1.17",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037007.1.18",
          "form": "큰일이네요...",
          "original_form": "큰일이네요...",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037007.1.19",
          "form": "고기키울때 환경오염많이된대요",
          "original_form": "고기키울때 환경오염많이된대요",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037007.1.20",
          "form": "헉 그래요? 몰랐네요..",
          "original_form": "헉 그래요? 몰랐네요..",
          "speaker_id": "2",
          "time": "20210831 17:44"
        }
      ]
