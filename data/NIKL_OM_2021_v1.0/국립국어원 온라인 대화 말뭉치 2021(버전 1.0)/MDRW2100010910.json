      "utterance": [
        {
          "id": "MDRW2100010910.1.1",
          "form": "나혼자산다 보시나요?",
          "original_form": "나혼자산다 보시나요?",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010910.1.2",
          "form": "가끔요",
          "original_form": "가끔요",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010910.1.3",
          "form": "오상욱 나오네요",
          "original_form": "오상욱 나오네요",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010910.1.4",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010910.1.5",
          "form": "재방인거같지만요",
          "original_form": "재방인거같지만요",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010910.1.6",
          "form": "켜야겠...",
          "original_form": "켜야겠...",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010910.1.7",
          "form": "진짜 기네요 오상욱 ㅋ",
          "original_form": "진짜 기네요 오상욱 ㅋ",
          "speaker_id": "1",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010910.1.8",
          "form": "티빙에도 벳플에도 없군요 티비를켜야...",
          "original_form": "티빙에도 벳플에도 없군요 티비를켜야...",
          "speaker_id": "2",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010910.1.9",
          "form": "엥비씨드라마 에서 나오고있어요",
          "original_form": "엥비씨드라마 에서 나오고있어요",
          "speaker_id": "1",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100010910.1.10",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210823 22:56"
        },
        {
          "id": "MDRW2100010910.1.11",
          "form": "시작한지 얼마 안됐군요",
          "original_form": "시작한지 얼마 안됐군요",
          "speaker_id": "2",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100010910.1.12",
          "form": "저렇게 긴데, 불편하겠다는",
          "original_form": "저렇게 긴데, 불편하겠다는",
          "speaker_id": "2",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100010910.1.13",
          "form": "네 저도 막 틀었는데 자고 일어나더라구요",
          "original_form": "네 저도 막 틀었는데 자고 일어나더라구요",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100010910.1.14",
          "form": "집이 신기하네요 구조가",
          "original_form": "집이 신기하네요 구조가",
          "speaker_id": "2",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010910.1.15",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010910.1.16",
          "form": "무슨 사무실같아보여요",
          "original_form": "무슨 사무실같아보여요",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010910.1.17",
          "form": "집이 아니라",
          "original_form": "집이 아니라",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010910.1.18",
          "form": "어멋",
          "original_form": "어멋",
          "speaker_id": "2",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100010910.1.19",
          "form": "운동부 사무실이라는 글자가 보이네요 ㅎㅎ",
          "original_form": "운동부 사무실이라는 글자가 보이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010910.1.20",
          "form": "아 감독실 있는 거보니",
          "original_form": "아 감독실 있는 거보니",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010910.1.21",
          "form": "집구조가 아니네요 ㅎ",
          "original_form": "집구조가 아니네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010910.1.22",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010910.1.23",
          "form": "진짜 귀엽게 잘생겼네요",
          "original_form": "진짜 귀엽게 잘생겼네요",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010910.1.24",
          "form": "넘 귀엽네요",
          "original_form": "넘 귀엽네요",
          "speaker_id": "2",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010910.1.25",
          "form": "24살....",
          "original_form": "24살....",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010910.1.26",
          "form": "와 저런 케이스도 있군요",
          "original_form": "와 저런 케이스도 있군요",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010910.1.27",
          "form": "첨봤어요",
          "original_form": "첨봤어요",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010910.1.28",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100010910.1.29",
          "form": "근데 집안좀 치워주고싶네요",
          "original_form": "근데 집안좀 치워주고싶네요",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010910.1.30",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010910.1.31",
          "form": "먼가 너저분한게 정리가 안되어보이네요 ㅎ",
          "original_form": "먼가 너저분한게 정리가 안되어보이네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:03"
        }
      ]
    }
  ]
