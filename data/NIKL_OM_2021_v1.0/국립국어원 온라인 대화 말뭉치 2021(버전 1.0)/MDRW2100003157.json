        {
          "id": "MDRW2100003157.1.1",
          "form": "요즘엔 코로나로 사람 만나기 힘들겠어요",
          "original_form": "요즘엔 코로나로 사람 만나기 힘들겠어요",
          "speaker_id": "1",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003157.1.2",
          "form": "맞아요 결혼 하셨나요?",
          "original_form": "맞아요 결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100003157.1.3",
          "form": "네 저는 기혼입니다. 결혼하셨나요?",
          "original_form": "네 저는 기혼입니다. 결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003157.1.4",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003157.1.5",
          "form": "저는 결혼 안했어요",
          "original_form": "저는 결혼 안했어요",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003157.1.6",
          "form": "결혼은 어떤가요?",
          "original_form": "결혼은 어떤가요?",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003157.1.7",
          "form": "선택인 것 같아요ㅎㅎ",
          "original_form": "선택인 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003157.1.8",
          "form": "정말 다른 삶이 시작되어서ㅎㅎ",
          "original_form": "정말 다른 삶이 시작되어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003157.1.9",
          "form": "이상형을 여쭤도 될까요?",
          "original_form": "이상형을 여쭤도 될까요?",
          "speaker_id": "1",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003157.1.10",
          "form": "전 순하고 잘생긴 사람이 이상형이에요 ㅎ",
          "original_form": "전 순하고 잘생긴 사람이 이상형이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003157.1.11",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003157.1.12",
          "form": "이젠 이상형이 뭔지 모르겠지만ㅎㅎ",
          "original_form": "이젠 이상형이 뭔지 모르겠지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003157.1.13",
          "form": "역시 생각이 비슷한 사람?ㅎㅎ",
          "original_form": "역시 생각이 비슷한 사람?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003157.1.14",
          "form": "생각이 잘 맞고 비슷해야 좋은 거 같아요",
          "original_form": "생각이 잘 맞고 비슷해야 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003157.1.15",
          "form": "특히 결혼은요",
          "original_form": "특히 결혼은요",
          "speaker_id": "2",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003157.1.16",
          "form": "네 특히",
          "original_form": "네 특히",
          "speaker_id": "1",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003157.1.17",
          "form": "가족과의 만남인지라",
          "original_form": "가족과의 만남인지라",
          "speaker_id": "1",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003157.1.18",
          "form": "정말 서로 이해가 많이 되어야지요ㅠ",
          "original_form": "정말 서로 이해가 많이 되어야지요ㅠ",
          "speaker_id": "1",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003157.1.19",
          "form": "맞아요 전 그래서 결혼에 대해 생각이 멀어져요",
          "original_form": "맞아요 전 그래서 결혼에 대해 생각이 멀어져요",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003157.1.20",
          "form": "결혼은 어려운 거 같네요",
          "original_form": "결혼은 어려운 거 같네요",
          "speaker_id": "2",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003157.1.21",
          "form": "결혼을 해도 그 문제는 계속 따라다녀요",
          "original_form": "결혼을 해도 그 문제는 계속 따라다녀요",
