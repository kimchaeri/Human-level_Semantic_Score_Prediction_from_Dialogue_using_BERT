      "utterance": [
        {
          "id": "MDRW2100014765.1.1",
          "form": "오늘 저녁은 뭐 드셨어요??",
          "original_form": "오늘 저녁은 뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014765.1.2",
          "form": "저는 도가니탕 먹었어요",
          "original_form": "저는 도가니탕 먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014765.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014765.1.4",
          "form": "전 라면먹었습니당",
          "original_form": "전 라면먹었습니당",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014765.1.5",
          "form": "무슨라면 먹었게요?",
          "original_form": "무슨라면 먹었게요?",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014765.1.6",
          "form": "제 최애라면이 진라면 매운맛이거등요",
          "original_form": "제 최애라면이 진라면 매운맛이거등요",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014765.1.7",
          "form": "이걸 드셨으리라 생각해봅니당 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이걸 드셨으리라 생각해봅니당 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014765.1.8",
          "form": "아쉽게도",
          "original_form": "아쉽게도",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014765.1.9",
          "form": "열라면입니다",
          "original_form": "열라면입니다",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014765.1.10",
          "form": "한글자 틀리셨네용 ㅋㅋㅋㅋ",
          "original_form": "한글자 틀리셨네용 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014765.1.11",
          "form": "앜",
          "original_form": "앜",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014765.1.12",
          "form": "열라면.. 매운 음식을 잘 드시는 군요",
          "original_form": "열라면.. 매운 음식을 잘 드시는 군요",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014765.1.13",
          "form": "적당히 잘먹는 스탈이에요!",
          "original_form": "적당히 잘먹는 스탈이에요!",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014765.1.14",
          "form": "근데 거의 맵찌에 가까운..?",
          "original_form": "근데 거의 맵찌에 가까운..?",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014765.1.15",
          "form": "신전떡볶이 어떤 맛드시나요",
          "original_form": "신전떡볶이 어떤 맛드시나요",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014765.1.16",
          "form": "저는 중간맛 먹는데 ㅎㅎ",
          "original_form": "저는 중간맛 먹는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014765.1.17",
          "form": "신전은 안먹은지 너무 오래됐는데",
          "original_form": "신전은 안먹은지 너무 오래됐는데",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014765.1.18",
          "form": "기억을 더듬어생각해보면",
          "original_form": "기억을 더듬어생각해보면",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014765.1.19",
          "form": "중간맛 먹었던것 같아요 ㅎㅎ",
          "original_form": "중간맛 먹었던것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014765.1.20",
          "form": "헉 떡볶이 드신다면 어디서 드시나요?",
          "original_form": "헉 떡볶이 드신다면 어디서 드시나요?",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014765.1.21",
          "form": "저는 신전을 시켜먹어서 아무생각 없이 신전으로 말씀드렸네요 ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "저는 신전을 시켜먹어서 아무생각 없이 신전으로 말씀드렸네요 ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014765.1.22",
          "form": "회사다니면서는 엽떡을 많이 먹었어요!!",
          "original_form": "회사다니면서는 엽떡을 많이 먹었어요!!",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014765.1.23",
          "form": "저도 옛날에 신전 많이 먹었어요 ㅎㅎ",
          "original_form": "저도 옛날에 신전 많이 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014765.1.24",
          "form": "와 엽떡 신전 다 잘드시는 분 오랜만에 뵈어요",
          "original_form": "와 엽떡 신전 다 잘드시는 분 오랜만에 뵈어요",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014765.1.25",
          "form": "다들 한 쪽만 좋아하고 다른 건 싫어하더라고요 ㅋㅋㅋ",
          "original_form": "다들 한 쪽만 좋아하고 다른 건 싫어하더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014765.1.26",
          "form": "사실 떡볶이는 거를 타선이 없져 ㅎㅎ",
          "original_form": "사실 떡볶이는 거를 타선이 없져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014765.1.27",
          "form": "이야 먹박사이시군요",
          "original_form": "이야 먹박사이시군요",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014765.1.28",
          "form": "혹시 맛집 이런거에 관심 많으세뇨?",
          "original_form": "혹시 맛집 이런거에 관심 많으세뇨?",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014765.1.29",
          "form": "쩝쩝박사입니다 ㅎ",
          "original_form": "쩝쩝박사입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014765.1.30",
          "form": "맛집은 찾아서 가는 스타일은 아니고",
          "original_form": "맛집은 찾아서 가는 스타일은 아니고",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014765.1.31",
          "form": "제출이 될지 모르고 제출했어요ㅠㅠ",
          "original_form": "제출이 될지 모르고 제출했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:04"
        }
      ]
