      "utterance": [
        {
          "id": "MDRW2100025531.1.1",
          "form": "요즘 세상 돌아가는 걸 보면",
          "original_form": "요즘 세상 돌아가는 걸 보면",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025531.1.2",
          "form": "안그래도 결혼 욕구가 0에 수렴하는데",
          "original_form": "안그래도 결혼 욕구가 0에 수렴하는데",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025531.1.3",
          "form": "이제는 마이너스예요!",
          "original_form": "이제는 마이너스예요!",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025531.1.4",
          "form": "저도요.",
          "original_form": "저도요.",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100025531.1.5",
          "form": "매일 매일 새로운 사건이 터지죠",
          "original_form": "매일 매일 새로운 사건이 터지죠",
          "speaker_id": "1",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100025531.1.6",
          "form": "어떻게 발전이 없지...",
          "original_form": "어떻게 발전이 없지...",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025531.1.7",
          "form": "그러면서 여자친구는 사귀고 싶어하고....",
          "original_form": "그러면서 여자친구는 사귀고 싶어하고....",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025531.1.8",
          "form": "여자를 좋아..ㅎ 하니까 품평질하는건데",
          "original_form": "여자를 좋아..ㅎ 하니까 품평질하는건데",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025531.1.9",
          "form": "걍 노답이죠",
          "original_form": "걍 노답이죠",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025531.1.10",
          "form": "진짜요 그게 좋아한다는 개념이",
          "original_form": "진짜요 그게 좋아한다는 개념이",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025531.1.11",
          "form": "마라탕 좋아하듯이,,",
          "original_form": "마라탕 좋아하듯이,,",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025531.1.12",
          "form": "나는 중국당면 넣은게 좋아! << 이런 느낌으로다가 좋아하는거네요",
          "original_form": "나는 중국당면 넣은게 좋아! << 이런 느낌으로다가 좋아하는거네요",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025531.1.13",
          "form": "그정도면 차라리 귀엽겠네요",
          "original_form": "그정도면 차라리 귀엽겠네요",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025531.1.14",
          "form": "그건 한 난 머리 긴게 좋아 정도잖아요",
          "original_form": "그건 한 난 머리 긴게 좋아 정도잖아요",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025531.1.15",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025531.1.16",
          "form": "상품 스펙 비교하듯이",
          "original_form": "상품 스펙 비교하듯이",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025531.1.17",
          "form": "비교하고",
          "original_form": "비교하고",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025531.1.18",
          "form": "그러면서 연애는 하고싶어하고~~~",
          "original_form": "그러면서 연애는 하고싶어하고~~~",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025531.1.19",
          "form": "뭐 따져보고 싶은건 이해해요",
          "original_form": "뭐 따져보고 싶은건 이해해요",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025531.1.20",
          "form": "애초에 소개팅, 맞선 그런거 다 급..  맞춰서 주선하는거고",
          "original_form": "애초에 소개팅, 맞선 그런거 다 급..  맞춰서 주선하는거고",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025531.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025531.1.22",
          "form": "근데 그렇게 만났으면 존중을 해주던가",
          "original_form": "근데 그렇게 만났으면 존중을 해주던가",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025531.1.23",
          "form": "자기보다 잘난 여자 만나면 잘났다고 후려치고",
          "original_form": "자기보다 잘난 여자 만나면 잘났다고 후려치고",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.24",
          "form": "아니면 무시하고...!!!",
          "original_form": "아니면 무시하고...!!!",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.25",
          "form": "아니면 한d급이 a급한테 질척대고",
          "original_form": "아니면 한d급이 a급한테 질척대고",
          "speaker_id": "1",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.26",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.27",
          "form": "자기 수준을 모른다잖아요...",
          "original_form": "자기 수준을 모른다잖아요...",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.28",
          "form": "저 예전에 그 짤 본 적 있는데",
          "original_form": "저 예전에 그 짤 본 적 있는데",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.29",
          "form": "남자들으뉴",
          "original_form": "남자들으뉴",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.30",
          "form": "그러니까요.",
          "original_form": "그러니까요.",
          "speaker_id": "1",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.31",
          "form": "남자들은 여자들한테 들이대면",
          "original_form": "남자들은 여자들한테 들이대면",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.32",
          "form": "'나 정도면 괜찮겠지' 하면서",
          "original_form": "'나 정도면 괜찮겠지' 하면서",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025531.1.33",
          "form": "상위권 여자들한테 들이댄다는거요!",
          "original_form": "상위권 여자들한테 들이댄다는거요!",
          "speaker_id": "2",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025531.1.34",
          "form": "그래도 a급은 누가봐도 쩌니까 차라리 괜찮은데",
          "original_form": "그래도 a급은 누가봐도 쩌니까 차라리 괜찮은데",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025531.1.35",
          "form": "배나온",
          "original_form": "배나온",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025531.1.36",
          "form": "나이많은 아저씨가",
          "original_form": "나이많은 아저씨가",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025531.1.37",
          "form": "20대 초반 여성에게 할머니라고 한다든가...~~~~~",
          "original_form": "20대 초반 여성에게 할머니라고 한다든가...~~~~~",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025531.1.38",
          "form": "b급정도 되는 분들한테 d급들이 '너정도면 (내가)만날만하지' 를 시전하잖아요",
          "original_form": "b급정도 되는 분들한테 d급들이 '너정도면 (내가)만날만하지' 를 시전하잖아요",
