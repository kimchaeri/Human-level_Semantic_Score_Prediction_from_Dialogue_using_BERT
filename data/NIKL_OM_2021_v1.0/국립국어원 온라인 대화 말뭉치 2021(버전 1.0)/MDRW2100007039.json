      "utterance": [
        {
          "id": "MDRW2100007039.1.1",
          "form": "최근에 여행가신 곳 어디세요?",
          "original_form": "최근에 여행가신 곳 어디세요?",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100007039.1.2",
          "form": "요샌 여행을 잘못가요",
          "original_form": "요샌 여행을 잘못가요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007039.1.3",
          "form": "근처가 계곡이나 산이 많아서 근처 잠깐가요 ㅎㅎ",
          "original_form": "근처가 계곡이나 산이 많아서 근처 잠깐가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007039.1.4",
          "form": "어디 가셨어요?",
          "original_form": "어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007039.1.5",
          "form": "그쵸 저는 2020년 1월에 마지막으로 스페인 포르투갈 갔다왔어요",
          "original_form": "그쵸 저는 2020년 1월에 마지막으로 스페인 포르투갈 갔다왔어요",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007039.1.6",
          "form": "오 다행히 잘 다녀오셨네요",
          "original_form": "오 다행히 잘 다녀오셨네요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007039.1.7",
          "form": "네 12월에 출발했는데 올때되니 공항에서 난리더라구요ㅠ",
          "original_form": "네 12월에 출발했는데 올때되니 공항에서 난리더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007039.1.8",
          "form": "그렇죠 완전 놀라셨죠",
          "original_form": "그렇죠 완전 놀라셨죠",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007039.1.9",
          "form": "그때 생각나네요 ㅜㅜ",
          "original_form": "그때 생각나네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007039.1.10",
          "form": "그러더니 이렇게 해외 여행도 못가고 발이 묶였네요",
          "original_form": "그러더니 이렇게 해외 여행도 못가고 발이 묶였네요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007039.1.11",
          "form": "전 스페인 포르투칼 가신분 첨봐요",
          "original_form": "전 스페인 포르투칼 가신분 첨봐요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007039.1.12",
          "form": "선택한 이유가 특별히 있으세요?",
          "original_form": "선택한 이유가 특별히 있으세요?",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007039.1.13",
          "form": "아 진짜요?ㅋㅋ 예전에 유럽 일주했었는데 동선짜다가 그쪽만빼먹어서 아쉬워서요",
          "original_form": "아 진짜요?ㅋㅋ 예전에 유럽 일주했었는데 동선짜다가 그쪽만빼먹어서 아쉬워서요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007039.1.14",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007039.1.15",
          "form": "전 비행기를 오래 못타서 못가요",
          "original_form": "전 비행기를 오래 못타서 못가요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007039.1.16",
          "form": "대만 까지가 마지노선이라",
          "original_form": "대만 까지가 마지노선이라",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007039.1.17",
          "form": "대만이나 일본 제주도 갔어요",
          "original_form": "대만이나 일본 제주도 갔어요",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007039.1.18",
          "form": "아렸을때 태국 가보고요",
          "original_form": "아렸을때 태국 가보고요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007039.1.19",
          "form": "와 전 아직 태국 대만은 못가봤어요 자유여행으로 가신건가요?",
          "original_form": "와 전 아직 태국 대만은 못가봤어요 자유여행으로 가신건가요?",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007039.1.20",
          "form": "태국 패키지",
          "original_form": "태국 패키지",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007039.1.21",
          "form": "대만 자유요",
          "original_form": "대만 자유요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007039.1.22",
          "form": "영어를 못하지만 영어를 쓰시니 대충 잘 지내고 왔어요 ㅎㅎ",
          "original_form": "영어를 못하지만 영어를 쓰시니 대충 잘 지내고 왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007039.1.23",
          "form": "대만 펑리수도 드셔보셨겠네요",
          "original_form": "대만 펑리수도 드셔보셨겠네요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007039.1.24",
          "form": "네 ㅋㅋ사와서 선물도 하고요",
          "original_form": "네 ㅋㅋ사와서 선물도 하고요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007039.1.25",
          "form": "전형적인 코스로 다샀네요 ㅋㅋ",
          "original_form": "전형적인 코스로 다샀네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007039.1.26",
          "form": "좀 가격이 있는게 맛나요",
          "original_form": "좀 가격이 있는게 맛나요",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007039.1.27",
          "form": "남들 다가는 데가 역시최고인거같아요 괜히 모험하기보다는ㅋㅋ",
          "original_form": "남들 다가는 데가 역시최고인거같아요 괜히 모험하기보다는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:36"
        }
      ]
