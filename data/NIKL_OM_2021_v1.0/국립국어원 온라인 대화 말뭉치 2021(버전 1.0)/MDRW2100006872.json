      "utterance": [
        {
          "id": "MDRW2100006872.1.1",
          "form": "기름기때믈에 느끼해요",
          "original_form": "기름기때믈에 느끼해요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006872.1.2",
          "form": "그렇긴한데 한번씩 먹고싶지않나요",
          "original_form": "그렇긴한데 한번씩 먹고싶지않나요",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006872.1.3",
          "form": "구이는 기름져서  안먹어요",
          "original_form": "구이는 기름져서  안먹어요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006872.1.4",
          "form": "양곱창 전골은 먹나요?",
          "original_form": "양곱창 전골은 먹나요?",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100006872.1.5",
          "form": "전골은 매콤하면요",
          "original_form": "전골은 매콤하면요",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006872.1.6",
          "form": "마지막 국물에 볶음밥 넣으면 그리 맛있어요",
          "original_form": "마지막 국물에 볶음밥 넣으면 그리 맛있어요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006872.1.7",
          "form": "기름지니까 그렇게먹음 좋겠어요",
          "original_form": "기름지니까 그렇게먹음 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006872.1.8",
          "form": "남포동에 양곱창 전골이 있어요",
          "original_form": "남포동에 양곱창 전골이 있어요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006872.1.9",
          "form": "많지않나요 곱창골목",
          "original_form": "많지않나요 곱창골목",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006872.1.10",
          "form": "알곱창이랑 양곱창이랑 막창 대창 다 있지요",
          "original_form": "알곱창이랑 양곱창이랑 막창 대창 다 있지요",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100006872.1.11",
          "form": "남포막창 남녀노소 많죠",
          "original_form": "남포막창 남녀노소 많죠",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006872.1.12",
          "form": "네 술한잔하기 좋아요 노천이라서",
          "original_form": "네 술한잔하기 좋아요 노천이라서",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006872.1.13",
          "form": "저녁에 가볼까요",
          "original_form": "저녁에 가볼까요",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100006872.1.14",
          "form": "빵집도 요새 유명한 곳 많죠",
          "original_form": "빵집도 요새 유명한 곳 많죠",
