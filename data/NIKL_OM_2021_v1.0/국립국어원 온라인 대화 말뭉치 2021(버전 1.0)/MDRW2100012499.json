      "utterance": [
        {
          "id": "MDRW2100012499.1.1",
          "form": "저 노가다 해봣어요ㅎ",
          "original_form": "저 노가다 해봣어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012499.1.2",
          "form": "그때 당시 여자친구 생일선물산다고 해봤던 기억이...",
          "original_form": "그때 당시 여자친구 생일선물산다고 해봤던 기억이...",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012499.1.3",
          "form": "대학때라 열정적이었나봅니다.... ㅎㅎㅎㅎ",
          "original_form": "대학때라 열정적이었나봅니다.... ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012499.1.4",
          "form": "오ㅎ 여친생일선물땜에 노가다까지",
          "original_form": "오ㅎ 여친생일선물땜에 노가다까지",
          "speaker_id": "1",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012499.1.5",
          "form": "알바는 왜하섰어요?ㅎ 이유가?",
          "original_form": "알바는 왜하섰어요?ㅎ 이유가?",
          "speaker_id": "2",
          "time": "20210824 22:35"
        },
        {
          "id": "MDRW2100012499.1.6",
          "form": "그러게요ㅎㅎ 지금와서 생각하면 왜 했는지ㅎㅎㅎ",
          "original_form": "그러게요ㅎㅎ 지금와서 생각하면 왜 했는지ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100012499.1.7",
          "form": "근데 아마 그때 당시에는 그렇게라도 기쁘게 해주고 싶었나봐요ㅎ",
          "original_form": "근데 아마 그때 당시에는 그렇게라도 기쁘게 해주고 싶었나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100012499.1.8",
          "form": "멋진 남친이였네요ㅎ",
          "original_form": "멋진 남친이였네요ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:36"
        },
        {
          "id": "MDRW2100012499.1.9",
          "form": "전그냥 경험으로?ㅎ",
          "original_form": "전그냥 경험으로?ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100012499.1.10",
          "form": "친구랑같이해서 재밌었어요 힘들긴 엄청힘들어요",
          "original_form": "친구랑같이해서 재밌었어요 힘들긴 엄청힘들어요",
          "speaker_id": "1",
          "time": "20210824 22:37"
        },
        {
          "id": "MDRW2100012499.1.11",
          "form": "다른알바는 안해보셨어요?",
          "original_form": "다른알바는 안해보셨어요?",
          "speaker_id": "1",
          "time": "20210824 22:39"
        },
        {
          "id": "MDRW2100012499.1.12",
          "form": "아ㅎㅎ 경험으로 하신거면.. 혹시 금수저?ㅎㅎㅎ",
          "original_form": "아ㅎㅎ 경험으로 하신거면.. 혹시 금수저?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012499.1.13",
          "form": "다른 알바는... 아맞다ㅎㅎ",
          "original_form": "다른 알바는... 아맞다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012499.1.14",
          "form": "아버지가 다니는 회사 쪽에 알바자리나서 한번해ㅛ네요ㅎㅎ",
          "original_form": "아버지가 다니는 회사 쪽에 알바자리나서 한번해ㅛ네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100012499.1.15",
          "form": "전기사 보조업무였는데...",
          "original_form": "전기사 보조업무였는데...",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100012499.1.16",
          "form": "그 알바하고 나서 절대로 알바는 안해야 겟다고 생각했어요ㅎ",
          "original_form": "그 알바하고 나서 절대로 알바는 안해야 겟다고 생각했어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100012499.1.17",
          "form": "ㅎㅎ흙수저요ㅎㅎ",
          "original_form": "ㅎㅎ흙수저요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100012499.1.18",
          "form": "부모님이 카페알바 이런거하면 이상한걸줄아셨어요 그당시ㅎ",
          "original_form": "부모님이 카페알바 이런거하면 이상한걸줄아셨어요 그당시ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012499.1.19",
          "form": "다방생각하셨는지ㅋ",
          "original_form": "다방생각하셨는지ㅋ",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012499.1.20",
          "form": "절대안된다고해서 알아본게 음식 서빙알바",
          "original_form": "절대안된다고해서 알아본게 음식 서빙알바",
          "speaker_id": "1",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012499.1.21",
          "form": "ㅎㅎㅎㅎ다방ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ다방ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012499.1.22",
          "form": "저도 그랬던것 같애요ㅎ 그당시는 아빠가 알바 절때 하지말라고 했었네요ㅎ",
          "original_form": "저도 그랬던것 같애요ㅎ 그당시는 아빠가 알바 절때 하지말라고 했었네요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012499.1.23",
          "form": "어릴때 집집마다 전단지붙이는 아르바이트하다가 걸려서ㅎㅎ",
          "original_form": "어릴때 집집마다 전단지붙이는 아르바이트하다가 걸려서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012499.1.24",
          "form": "ㅎㅎ 부모님은 자식걱정하시니까",
          "original_form": "ㅎㅎ 부모님은 자식걱정하시니까",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012499.1.25",
          "form": "아무래도 그런것같애요ㅎ",
          "original_form": "아무래도 그런것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012499.1.26",
          "form": "저도 제 딸이 하고있으면 속상할듯요ㅎ",
          "original_form": "저도 제 딸이 하고있으면 속상할듯요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012499.1.27",
          "form": "직업이 귀천이 없다지만 내자식이 고생하는건 싫은게 부모마음이니깐요 ㅡㅎ",
          "original_form": "직업이 귀천이 없다지만 내자식이 고생하는건 싫은게 부모마음이니깐요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012499.1.28",
          "form": "마자요ㅎ 그래서 공모전업무는 끝내신거예요?",
          "original_form": "마자요ㅎ 그래서 공모전업무는 끝내신거예요?",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012499.1.29",
          "form": "업무에서 해방?",
          "original_form": "업무에서 해방?",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012499.1.30",
          "form": "결제만 남았는데ㅎㅎ 상사에게 토스했어요ㅎㅎ",
          "original_form": "결제만 남았는데ㅎㅎ 상사에게 토스했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012499.1.31",
          "form": "배구선수처럼 토스잘해요ㅎ",
          "original_form": "배구선수처럼 토스잘해요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012499.1.32",
          "form": "좋네요ㅎㅎ",
          "original_form": "좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012499.1.33",
          "form": "이제퇴근하신건 아니죠?",
          "original_form": "이제퇴근하신건 아니죠?",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012499.1.34",
          "form": "에이 설마요ㅎ 회사가 아무리 안좋아도ㅎㅎ",
          "original_form": "에이 설마요ㅎ 회사가 아무리 안좋아도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012499.1.35",
          "form": "ㅎㅎ 야근하셨다길래",
          "original_form": "ㅎㅎ 야근하셨다길래",
          "speaker_id": "1",
