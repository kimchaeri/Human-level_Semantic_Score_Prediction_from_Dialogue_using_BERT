        },
        {
          "id": "MDRW2100011745.1.2",
          "form": "제가 한 때 잘했던 거",
          "original_form": "제가 한 때 잘했던 거",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011745.1.3",
          "form": "이젠 소비가 줄었지",
          "original_form": "이젠 소비가 줄었지",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011745.1.4",
          "form": "나는 이번에 쿠폰뿌리길래 벨트삼",
          "original_form": "나는 이번에 쿠폰뿌리길래 벨트삼",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011745.1.5",
          "form": "벨트 분명히 있었는데 못찾아서ㅋㅋ",
          "original_form": "벨트 분명히 있었는데 못찾아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:24"
        },
        {
          "id": "MDRW2100011745.1.6",
          "form": "오 벨트?",
          "original_form": "오 벨트?",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011745.1.7",
          "form": "바지는 고무줄 바지가 진리 아닌가요",
          "original_form": "바지는 고무줄 바지가 진리 아닌가요",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100011745.1.8",
          "form": "벨트는 그래도 하나는 있으면 좋으니까",
          "original_form": "벨트는 그래도 하나는 있으면 좋으니까",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011745.1.9",
          "form": "고무줄바지는 멋이않나",
          "original_form": "고무줄바지는 멋이않나",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011745.1.10",
          "form": "하!",
          "original_form": "하!",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100011745.1.11",
          "form": "우리가 언제부터 멋을 챙겼다규 ㅋㅋㅋㅋㅋ",
          "original_form": "우리가 언제부터 멋을 챙겼다규 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011745.1.12",
          "form": "와낰ㅋㅋㅋㅋ",
          "original_form": "와낰ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:26"
        },
        {
          "id": "MDRW2100011745.1.13",
          "form": "가끔 멋쟁이고싶을때가 있다...",
          "original_form": "가끔 멋쟁이고싶을때가 있다...{emoji:☆}",
          "speaker_id": "1",
          "time": "20210824 17:58"
        },
        {
          "id": "MDRW2100011745.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 최근 제가 구매한 물건은 건강검진비입니다 하하",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 최근 제가 구매한 물건은 건강검진비입니다 하하",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011745.1.15",
          "form": "ㅋㅋㅋㅋ이야 그건 써야지",
          "original_form": "ㅋㅋㅋㅋ이야 그건 써야지",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011745.1.16",
          "form": "바로 오늘이죠",
          "original_form": "바로 오늘이죠",
          "speaker_id": "2",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011745.1.17",
          "form": "내일이나 낼모래 결과나온다니 두고봐야지",
          "original_form": "내일이나 낼모래 결과나온다니 두고봐야지",
          "speaker_id": "2",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011745.1.18",
          "form": "피검사말고 다른검사도 했어?",
          "original_form": "피검사말고 다른검사도 했어?",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011745.1.19",
          "form": "뭐 씨티도 찍고 소변도 하고~",
          "original_form": "뭐 씨티도 찍고 소변도 하고~",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100011745.1.20",
          "form": "근데 소변에서 단백뇨가 나왔대",
          "original_form": "근데 소변에서 단백뇨가 나왔대",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100011745.1.21",
          "form": "어 나도 나왔던듯",
          "original_form": "어 나도 나왔던듯",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011745.1.22",
          "form": "근데 심한거아니고 공복이라 그럴수있댔나",
          "original_form": "근데 심한거아니고 공복이라 그럴수있댔나",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011745.1.23",
          "form": "기억이 가물가물",
          "original_form": "기억이 가물가물",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011745.1.24",
          "form": "그건 당뇨랑 상관없다더라",
          "original_form": "그건 당뇨랑 상관없다더라",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011745.1.25",
          "form": "드디어 거의 1년을 미루던 노트북 거치대를 샀다는소식",
          "original_form": "드디어 거의 1년을 미루던 노트북 거치대를 샀다는소식",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100011745.1.26",
          "form": "방금 배송왔다",
          "original_form": "방금 배송왔다",
