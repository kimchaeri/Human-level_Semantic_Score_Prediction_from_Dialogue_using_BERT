      "utterance": [
        {
          "id": "MDRW2100023866.1.1",
          "form": "저녁은 뭐드세요?",
          "original_form": "저녁은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023866.1.2",
          "form": "이미 먹었어요 ㅎㅎ",
          "original_form": "이미 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100023866.1.3",
          "form": "전 참치회덮밥 만들어 먹었는데 님은요?",
          "original_form": "전 참치회덮밥 만들어 먹었는데 님은요?",
          "speaker_id": "1",
          "time": "20210830 19:26"
        },
        {
          "id": "MDRW2100023866.1.4",
          "form": "아 이미 드셨군요 ㅋㅋ 저는 애들이랑 짜장밥 해먹으려구요 ㅋ",
          "original_form": "아 이미 드셨군요 ㅋㅋ 저는 애들이랑 짜장밥 해먹으려구요 ㅋ",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023866.1.5",
          "form": "어머 짜장밥~",
          "original_form": "어머 짜장밥~",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023866.1.6",
          "form": "짜장도 만들 줄 아세요?",
          "original_form": "짜장도 만들 줄 아세요?",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023866.1.7",
          "form": "네 짜장도 카레처럼 만들면 간단해요ㅋㅋ",
          "original_form": "네 짜장도 카레처럼 만들면 간단해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023866.1.8",
          "form": "카레가루처럼 짜장가루가 따로 있어요?",
          "original_form": "카레가루처럼 짜장가루가 따로 있어요?",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023866.1.9",
          "form": "네네 짜장가루가 따로있어요ㅋㅋ",
          "original_form": "네네 짜장가루가 따로있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100023866.1.10",
          "form": "방법은 동일하구요ㅋ",
          "original_form": "방법은 동일하구요ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100023866.1.11",
          "form": "어머 그렇구나 처음 알았어요",
          "original_form": "어머 그렇구나 처음 알았어요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100023866.1.12",
          "form": "짜장밥은 맛있게 드셨어요?",
          "original_form": "짜장밥은 맛있게 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100023866.1.13",
          "form": "네 맛있게먹었어요",
          "original_form": "네 맛있게먹었어요",
          "speaker_id": "2",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100023866.1.14",
          "form": "아침은 뭐드세요?",
          "original_form": "아침은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100023866.1.15",
          "form": "저 아침 건너 뛰고 지금 점심 먹어요!!",
          "original_form": "저 아침 건너 뛰고 지금 점심 먹어요!!",
          "speaker_id": "1",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100023866.1.16",
          "form": "바게트빵에 연어랑 삶은계란, 채소  얹고 소스 샤악 뿌렸어여",
          "original_form": "바게트빵에 연어랑 삶은계란, 채소  얹고 소스 샤악 뿌렸어여",
          "speaker_id": "1",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100023866.1.17",
          "form": "ㅋㅋㅋ와....대박...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛있겠어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ와....대박...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛있겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100023866.1.18",
          "form": "저도 아침은 결국 건너뛰었네요 ㅋㅋ",
          "original_form": "저도 아침은 결국 건너뛰었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:23"
        },
        {
          "id": "MDRW2100023866.1.19",
          "form": "저는 잡채밥먹어요ㅋ",
          "original_form": "저는 잡채밥먹어요ㅋ",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100023866.1.20",
          "form": "헤헤 진짜 맛있었어여",
          "original_form": "헤헤 진짜 맛있었어여",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100023866.1.21",
          "form": "갑자기 회의 호출 당해서 허겁지겁 먹었지만 ㅜ",
          "original_form": "갑자기 회의 호출 당해서 허겁지겁 먹었지만 ㅜ",
