      "utterance": [
        {
          "id": "MDRW2100044849.1.1",
          "form": "알바해보셨어요?",
          "original_form": "알바해보셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044849.1.2",
          "form": "넹 ㅋㅋㅋ 과외요",
          "original_form": "넹 ㅋㅋㅋ 과외요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044849.1.3",
          "form": "꿀과외",
          "original_form": "꿀과외",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044849.1.4",
          "form": "저 전교꼴찌학생을 70등까지 만들어줬더니",
          "original_form": "저 전교꼴찌학생을 70등까지 만들어줬더니",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044849.1.5",
          "form": "ㅋㅋㅋㅋ ㅠㅠ 과외는 시급이 안올라여",
          "original_form": "ㅋㅋㅋㅋ ㅠㅠ 과외는 시급이 안올라여",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044849.1.6",
          "form": "과외급등했습니다ㅋㅋ",
          "original_form": "과외급등했습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044849.1.7",
          "form": "마자여 그리고 동네마다 책정된금액이있다는!",
          "original_form": "마자여 그리고 동네마다 책정된금액이있다는!",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044849.1.8",
          "form": "급등햇어요??",
          "original_form": "급등햇어요??",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044849.1.9",
          "form": "얼마나요?",
          "original_form": "얼마나요?",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044849.1.10",
          "form": "300등이요ㅋㅋㅋ",
          "original_form": "300등이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.11",
          "form": "전혀 공부하는법을 모르길래",
          "original_form": "전혀 공부하는법을 모르길래",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.12",
          "form": "중2인데",
          "original_form": "중2인데",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.13",
          "form": "교과서 외워서 같이 시험친듷",
          "original_form": "교과서 외워서 같이 시험친듷",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.14",
          "form": "와 ㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.15",
          "form": "물만주시다가 음료수도주시더라구요",
          "original_form": "물만주시다가 음료수도주시더라구요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.16",
          "form": "ㅋㅋㅋ얼마나올려주셧어요",
          "original_form": "ㅋㅋㅋ얼마나올려주셧어요",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044849.1.17",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044849.1.18",
          "form": "돈은 십만원더올려주셨던기억이ㅋㅋ",
          "original_form": "돈은 십만원더올려주셨던기억이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044849.1.19",
          "form": "근데 똑똑한애들은 과외못하겠더라구요ㅋ",
          "original_form": "근데 똑똑한애들은 과외못하겠더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044849.1.20",
          "form": "오히려제가 공부해야되자나요ㅋㅋㅋ",
          "original_form": "오히려제가 공부해야되자나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044849.1.21",
          "form": "ㅋㅋㅋㅋ 마자여 어중간이 젤 나아요",
          "original_form": "ㅋㅋㅋㅋ 마자여 어중간이 젤 나아요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044849.1.22",
          "form": "훠...올려주실만 ㅋㅋㅋ",
          "original_form": "훠...올려주실만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044849.1.23",
          "form": "알주 몇시간 하셧어요",
          "original_form": "알주 몇시간 하셧어요",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044849.1.24",
          "form": "일주일에",
          "original_form": "일주일에",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044849.1.25",
          "form": "아 걔는 전과목보느라 세번했었고",
          "original_form": "아 걔는 전과목보느라 세번했었고",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044849.1.26",
          "form": "다른애는 수학만해서 1시간반씩 일주일에두번만요ㅋ",
          "original_form": "다른애는 수학만해서 1시간반씩 일주일에두번만요ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044849.1.27",
          "form": "전과목 과외 ㅋㅋㅋ",
          "original_form": "전과목 과외 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044849.1.28",
          "form": "진짜 같이 교과서 외웠다니까여ㅋㅋㅋㅋ",
          "original_form": "진짜 같이 교과서 외웠다니까여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044849.1.29",
          "form": "미술 그림까지ㅋㅋㅋ",
          "original_form": "미술 그림까지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044849.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 세번가지고 되나여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 세번가지고 되나여",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044849.1.31",
          "form": "얼마받으셧어요?",
          "original_form": "얼마받으셧어요?",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044849.1.32",
          "form": "50 ㅋㅋㅋ",
          "original_form": "50 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044849.1.33",
          "form": "근데 그당시 알 알죠? 맨날 알 없다고 저한테 수신자부담전화하고...",
          "original_form": "근데 그당시 알 알죠? 맨날 알 없다고 저한테 수신자부담전화하고...",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044849.1.34",
          "form": "그래도 그때가 그립네요",
          "original_form": "그래도 그때가 그립네요",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044849.1.35",
          "form": "무슨과외하셨어요?",
          "original_form": "무슨과외하셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044849.1.36",
          "form": "왘 ㅋㅋㅋ 그당시면 많이받으셧네요",
          "original_form": "왘 ㅋㅋㅋ 그당시면 많이받으셧네요",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044849.1.37",
          "form": "전 수학이요 ㅋㅋㅋ",
          "original_form": "전 수학이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044849.1.38",
          "form": "전과목이었으니ㅋㅋㅋ",
          "original_form": "전과목이었으니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044849.1.39",
          "form": "수학은 30정도였죠?",
          "original_form": "수학은 30정도였죠?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044849.1.40",
          "form": "저희동네는 그랬음여ㅋㅋ",
          "original_form": "저희동네는 그랬음여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044849.1.41",
          "form": "ㅋㅋㅋㅋ보통 30~40?",
          "original_form": "ㅋㅋㅋㅋ보통 30~40?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044849.1.42",
          "form": "10년넘게 똑같은듯요",
          "original_form": "10년넘게 똑같은듯요",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044849.1.43",
          "form": "맞네요 진짜ㅜㅜ",
          "original_form": "맞네요 진짜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100044849.1.44",
          "form": "다른알바보다 편하긴한데 잘 안오르는게아쉽",
          "original_form": "다른알바보다 편하긴한데 잘 안오르는게아쉽",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044849.1.45",
          "form": "그죠...왜케안오를까요",
          "original_form": "그죠...왜케안오를까요",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100044849.1.46",
          "form": "지금도하시는건아니져?ㅋㅋ",
          "original_form": "지금도하시는건아니져?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100044849.1.47",
          "form": "다시할라고욬ㅋㅋㅋ",
          "original_form": "다시할라고욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100044849.1.48",
          "form": "그럼 더 올려받읍시다ㅋㄱ",
          "original_form": "그럼 더 올려받읍시다ㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100044849.1.49",
          "form": "요즘은 수학의정석 이런건 안할꺼자나여ㅋㅋㅋㅋㅋㅋ (부끄)",
          "original_form": "요즘은 수학의정석 이런건 안할꺼자나여ㅋㅋㅋㅋㅋㅋ (부끄)",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100044849.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런거같아요...거의? ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런거같아요...거의? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100044849.1.51",
          "form": "하려는사람이넘쳐서",
          "original_form": "하려는사람이넘쳐서",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100044849.1.52",
          "form": "안오르나봅니닼ㅋㅋㅋ",
          "original_form": "안오르나봅니닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100044849.1.53",
          "form": "구직활동하는사람들도 스트레스엄청날듯여ㅜ 과외는 소개소개로 마니하게되더라구요ㅋ",
          "original_form": "구직활동하는사람들도 스트레스엄청날듯여ㅜ 과외는 소개소개로 마니하게되더라구요ㅋ",
          "speaker_id": "2",
