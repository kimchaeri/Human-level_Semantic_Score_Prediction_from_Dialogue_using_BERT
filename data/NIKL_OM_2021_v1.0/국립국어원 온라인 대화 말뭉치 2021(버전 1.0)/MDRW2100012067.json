      "utterance": [
        {
          "id": "MDRW2100012067.1.1",
          "form": "재택 근무 해보셨나요?",
          "original_form": "재택 근무 해보셨나요?",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012067.1.2",
          "form": "네 코로나 심할때",
          "original_form": "네 코로나 심할때",
          "speaker_id": "2",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100012067.1.3",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "2",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100012067.1.4",
          "form": "저는 코로나때문은 말고 회사다닐때 2개월 정도 해봤어요",
          "original_form": "저는 코로나때문은 말고 회사다닐때 2개월 정도 해봤어요",
          "speaker_id": "1",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012067.1.5",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012067.1.6",
          "form": "잘되시던가요?",
          "original_form": "잘되시던가요?",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012067.1.7",
          "form": "재택 근무 맞으시던가요?",
          "original_form": "재택 근무 맞으시던가요?",
          "speaker_id": "1",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012067.1.8",
          "form": "집중이 안되서 힘들더라고요",
          "original_form": "집중이 안되서 힘들더라고요",
          "speaker_id": "1",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012067.1.9",
          "form": "저는 일이 안되더라구요",
          "original_form": "저는 일이 안되더라구요",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012067.1.10",
          "form": "상사 없는건 최고ㅎㅎ",
          "original_form": "상사 없는건 최고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100012067.1.11",
          "form": "밥도 머먹을지 고민해야되고 ㅎ",
          "original_form": "밥도 머먹을지 고민해야되고 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100012067.1.12",
          "form": "그냥 구내식당에서 주는거 먹는게 젤인데 ㅎ",
          "original_form": "그냥 구내식당에서 주는거 먹는게 젤인데 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100012067.1.13",
          "form": "맞아요 상사안보이는거로는 최고죠",
          "original_form": "맞아요 상사안보이는거로는 최고죠",
          "speaker_id": "2",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012067.1.14",
          "form": "밥차리는게 귀찮긴하죠ㅋㅋ",
          "original_form": "밥차리는게 귀찮긴하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012067.1.15",
          "form": "네 귀찮아서 굶다가",
          "original_form": "네 귀찮아서 굶다가",
          "speaker_id": "2",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012067.1.16",
          "form": "오후늦게 배고파서",
          "original_form": "오후늦게 배고파서",
          "speaker_id": "2",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012067.1.17",
          "form": "저녁폭식한적도 있어요 ㅎ",
          "original_form": "저녁폭식한적도 있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100012067.1.18",
          "form": "잘 안챙겨 먹기도 하더라고요",
          "original_form": "잘 안챙겨 먹기도 하더라고요",
          "speaker_id": "1",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012067.1.19",
          "form": "집중 안될때는 어떻게 하셨나요?",
          "original_form": "집중 안될때는 어떻게 하셨나요?",
          "speaker_id": "1",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012067.1.20",
          "form": "청소했어요 ㅎ",
          "original_form": "청소했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:57"
        },
        {
          "id": "MDRW2100012067.1.21",
          "form": "괜히 안하던 청소를 ㅋㅋ",
          "original_form": "괜히 안하던 청소를 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012067.1.22",
          "form": "청소 좋죠",
          "original_form": "청소 좋죠",
          "speaker_id": "1",
          "time": "20210824 18:58"
        },
        {
          "id": "MDRW2100012067.1.23",
          "form": "저는 산책했어요",
          "original_form": "저는 산책했어요",
          "speaker_id": "1",
          "time": "20210824 18:58"
        }
      ]
