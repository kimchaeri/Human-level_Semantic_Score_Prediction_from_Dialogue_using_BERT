      "utterance": [
        {
          "id": "MDRW2100040636.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040636.1.2",
          "form": "name2라는 닉넴 맘에 드네요",
          "original_form": "&name2&라는 닉넴 맘에 드네요",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040636.1.3",
          "form": "안녕하세요ㅋㅋㅋ 같이 열심히 주워봐요!!",
          "original_form": "안녕하세요ㅋㅋㅋ 같이 열심히 주워봐요!!",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040636.1.4",
          "form": "넹넹 ㅋㅋ 아르바이트 많이 하셨나요",
          "original_form": "넹넹 ㅋㅋ 아르바이트 많이 하셨나요",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040636.1.5",
          "form": "넵 단기알바 이것저것 해봤어요ㅋㅋㅋㅋ",
          "original_form": "넵 단기알바 이것저것 해봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040636.1.6",
          "form": "그러시군요 저두요",
          "original_form": "그러시군요 저두요",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040636.1.7",
          "form": "객실정리 알바도 해보고",
          "original_form": "객실정리 알바도 해보고",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040636.1.8",
          "form": "오 어떤 알바가 제일 기억에 남으셨나요?",
          "original_form": "오 어떤 알바가 제일 기억에 남으셨나요?",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040636.1.9",
          "form": "브래지어 포장알바요 ㅋㅋㅋ",
          "original_form": "브래지어 포장알바요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040636.1.10",
          "form": "브래지어를 재포장해서 마트 행사상품으로 보내요",
          "original_form": "브래지어를 재포장해서 마트 행사상품으로 보내요",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040636.1.11",
          "form": "와 은근 노동강도가 쎌 것 같아욬ㅋㅋㅋㅋ",
          "original_form": "와 은근 노동강도가 쎌 것 같아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040636.1.12",
          "form": "이런거 보통 가면 포장해야할게 산더미처럼 쌓여있지 않나요?",
          "original_form": "이런거 보통 가면 포장해야할게 산더미처럼 쌓여있지 않나요?",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040636.1.13",
          "form": "공장 바닥에 퍼질러앉아서",
          "original_form": "공장 바닥에 퍼질러앉아서",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040636.1.14",
          "form": "브래지어를 비닐봉지에 열개씩 차곡차곡 넣어요",
          "original_form": "브래지어를 비닐봉지에 열개씩 차곡차곡 넣어요",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040636.1.15",
          "form": "그렇더라구요 아시네요!",
          "original_form": "그렇더라구요 아시네요!",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040636.1.16",
          "form": "ㅋㅋㅋㅋㅋ이런 알바들이 대체로 그렇더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ이런 알바들이 대체로 그렇더라구요",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040636.1.17",
          "form": "예식장? 케이터링 서비스 조리 알바도 해봤는데",
          "original_form": "예식장? 케이터링 서비스 조리 알바도 해봤는데",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040636.1.18",
          "form": "음식이 정말 산더미처럼 쌓여있어서 산더미 같은 쟁반으로 옮기고",
          "original_form": "음식이 정말 산더미처럼 쌓여있어서 산더미 같은 쟁반으로 옮기고",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040636.1.19",
          "form": "노동집약적이었죠....",
          "original_form": "노동집약적이었죠....",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040636.1.20",
          "form": "돈은 얼마나 주나요?",
          "original_form": "돈은 얼마나 주나요?",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040636.1.21",
          "form": "그거 알바 해보고나면 그런 상품들이 달리보이지 않던가요?",
          "original_form": "그거 알바 해보고나면 그런 상품들이 달리보이지 않던가요?",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040636.1.22",
          "form": "음 오래되서 까먹었어요ㅋㅋㅋㅋ",
          "original_form": "음 오래되서 까먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040636.1.23",
          "form": "대충 최저시급에 식대정도 맞춰주지 않았을까 싶어요ㅋㅋㅋㅋ",
          "original_form": "대충 최저시급에 식대정도 맞춰주지 않았을까 싶어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040636.1.24",
          "form": "저도 브래지어 알바 최저시급 받았어요",
          "original_form": "저도 브래지어 알바 최저시급 받았어요",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040636.1.25",
          "form": "확실히 예식장 조리알바를 하고 나니까",
          "original_form": "확실히 예식장 조리알바를 하고 나니까",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040636.1.26",
          "form": "예식장 음식들 이미지가 조금 달라보이긴 하더라구요 ㅋㅋㅋㅋ",
          "original_form": "예식장 음식들 이미지가 조금 달라보이긴 하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:43"
        }
      ]
    }
