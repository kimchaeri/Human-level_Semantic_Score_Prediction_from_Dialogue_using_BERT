      "utterance": [
        {
          "id": "MDRW2100000970.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000970.1.2",
          "form": "아직 안하고 있는데",
          "original_form": "아직 안하고 있는데",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000970.1.3",
          "form": "지금 운동할까 고민이에요",
          "original_form": "지금 운동할까 고민이에요",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000970.1.4",
          "form": "운동 하고 계세요?",
          "original_form": "운동 하고 계세요?",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000970.1.5",
          "form": "전 요즘 유투브 영상틀고",
          "original_form": "전 요즘 유투브 영상틀고",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000970.1.6",
          "form": "홈트해요!",
          "original_form": "홈트해요!",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000970.1.7",
          "form": "추천추천",
          "original_form": "추천추천",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000970.1.8",
          "form": "오 홈트도 좋죠",
          "original_form": "오 홈트도 좋죠",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000970.1.9",
          "form": "저도 의지박약인데",
          "original_form": "저도 의지박약인데",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000970.1.10",
          "form": "금요일하고 주말마다는 조금씩 해요ㅋㅋㅋ",
          "original_form": "금요일하고 주말마다는 조금씩 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000970.1.11",
          "form": "ㅋㅋㅋㅋ저도 살이 넘많이쪄서 하구있는데",
          "original_form": "ㅋㅋㅋㅋ저도 살이 넘많이쪄서 하구있는데",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000970.1.12",
          "form": "요즘 땀흘리믄 시원함을 깨달아버렷어요",
          "original_form": "요즘 땀흘리믄 시원함을 깨달아버렷어요",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000970.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도 하면서 땀 흘리고 운동하니가 죽을 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도 하면서 땀 흘리고 운동하니가 죽을 거 같아요",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000970.1.14",
          "form": "그래도 운동하고 씻으면 완전 뿌듯!!!",
          "original_form": "그래도 운동하고 씻으면 완전 뿌듯!!!",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000970.1.15",
          "form": "맞아여 ㅋㅋ 운동하고 안먹으면 다행 ..",
          "original_form": "맞아여 ㅋㅋ 운동하고 안먹으면 다행 ..",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000970.1.16",
          "form": "배고파서 먹으면 무용지물 ...흑",
          "original_form": "배고파서 먹으면 무용지물 ...흑",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000970.1.17",
          "form": "ㅋㅋㅋㅋ ㅋ 근데 운동하면 배고파져요....",
          "original_form": "ㅋㅋㅋㅋ ㅋ 근데 운동하면 배고파져요....",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000970.1.18",
          "form": "인간의 몸이란....",
          "original_form": "인간의 몸이란....",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000970.1.19",
          "form": "맞아요 진짜 ㅜㅋㅋㅋ 이러다 여름다지나가면 열심히 빈둥빈둥댈지두 ..",
          "original_form": "맞아요 진짜 ㅜㅋㅋㅋ 이러다 여름다지나가면 열심히 빈둥빈둥댈지두 ..",
          "speaker_id": "2",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000970.1.20",
          "form": "여름에라도 열심히 해놔야",
          "original_form": "여름에라도 열심히 해놔야",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000970.1.21",
          "form": "겨울에 그나마 든든,,,",
          "original_form": "겨울에 그나마 든든,,,",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000970.1.22",
          "form": "요새 살이 많이쪄서 하하",
          "original_form": "요새 살이 많이쪄서 하하",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000970.1.23",
          "form": "확실히 요즘 밖을 안나가니.. 살이 다들찌나봐요 ㅠㅠ",
          "original_form": "확실히 요즘 밖을 안나가니.. 살이 다들찌나봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000970.1.24",
          "form": "그러니까요..... 운동을 무조건 꼭 해야하는데 의지가 휴",
          "original_form": "그러니까요..... 운동을 무조건 꼭 해야하는데 의지가 휴",
