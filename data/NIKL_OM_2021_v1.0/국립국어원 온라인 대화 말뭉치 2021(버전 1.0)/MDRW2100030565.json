        {
          "id": "MDRW2100030565.1.1",
          "form": "전 하고 있어요",
          "original_form": "전 하고 있어요",
          "speaker_id": "2",
          "time": "20210904 08:51"
        },
        {
          "id": "MDRW2100030565.1.2",
          "form": "오 전 안해요",
          "original_form": "오 전 안해요",
          "speaker_id": "1",
          "time": "20210904 09:00"
        },
        {
          "id": "MDRW2100030565.1.3",
          "form": "네 저도요",
          "original_form": "네 저도요",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030565.1.4",
          "form": "하고 계시담서요",
          "original_form": "하고 계시담서요",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100030565.1.5",
          "form": "하고있는줄 알았는데 아니나봐요",
          "original_form": "하고있는줄 알았는데 아니나봐요",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030565.1.6",
          "form": "그런건뭐에여",
          "original_form": "그런건뭐에여",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100030565.1.7",
          "form": "그러게요 이상형은 있어요",
          "original_form": "그러게요 이상형은 있어요",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100030565.1.8",
          "form": "이상형이?",
          "original_form": "이상형이?",
          "speaker_id": "1",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030565.1.9",
          "form": "음 하얀사람이요",
          "original_form": "음 하얀사람이요",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030565.1.10",
          "form": "아하 저는 처음본 여자요",
          "original_form": "아하 저는 처음본 여자요",
          "speaker_id": "1",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100030565.1.11",
          "form": "호기심이 많으시군요",
          "original_form": "호기심이 많으시군요",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030565.1.12",
          "form": "정복욕이 강해요",
          "original_form": "정복욕이 강해요",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030565.1.13",
          "form": "해로운 욕심이에요",
          "original_form": "해로운 욕심이에요",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030565.1.14",
