        },
        {
          "id": "MDRW2100007014.1.2",
          "form": "해외는 가야지",
          "original_form": "해외는 가야지",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100007014.1.3",
          "form": "뉴욕?",
          "original_form": "뉴욕?",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.4",
          "form": "그런데 말고 휴양지가야지",
          "original_form": "그런데 말고 휴양지가야지",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.5",
          "form": "휴양지? 별론데 난",
          "original_form": "휴양지? 별론데 난",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.6",
          "form": "괌 어때",
          "original_form": "괌 어때",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.7",
          "form": "시드니도 생각남",
          "original_form": "시드니도 생각남",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.8",
          "form": "쉬러가야지",
          "original_form": "쉬러가야지",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.9",
          "form": "시드니 괜찮네",
          "original_form": "시드니 괜찮네",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.10",
          "form": "휴양지갈바에 한국에서 잇지",
          "original_form": "휴양지갈바에 한국에서 잇지",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.11",
          "form": "한번 가봤으니 계획도 더 잘짤 수 있겠군",
          "original_form": "한번 가봤으니 계획도 더 잘짤 수 있겠군",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100007014.1.12",
          "form": "근데 휴양지 안가봐서그런거임 나 가본적없음",
          "original_form": "근데 휴양지 안가봐서그런거임 나 가본적없음",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007014.1.13",
          "form": "그냥 할게 없고 쉴수 있는곳이 휴양지",
          "original_form": "그냥 할게 없고 쉴수 있는곳이 휴양지",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007014.1.14",
          "form": "휴양지 가면 좋을수도?",
          "original_form": "휴양지 가면 좋을수도?",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007014.1.15",
          "form": "그래 좋다",
          "original_form": "그래 좋다",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100007014.1.16",
          "form": "시드니 다시 가고싶다",
          "original_form": "시드니 다시 가고싶다",
