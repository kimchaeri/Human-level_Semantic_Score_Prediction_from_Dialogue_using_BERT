        },
        {
          "id": "MDRW2100046654.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211103 22:56"
        },
        {
          "id": "MDRW2100046654.1.3",
          "form": "옷 입을 때 가장 중요하게 생각하는 부분이 어떤 거예요?",
          "original_form": "옷 입을 때 가장 중요하게 생각하는 부분이 어떤 거예요?",
          "speaker_id": "2",
          "time": "20211103 22:57"
        },
        {
          "id": "MDRW2100046654.1.4",
          "form": "디자인",
          "original_form": "디자인",
          "speaker_id": "1",
          "time": "20211104 01:34"
        },
        {
          "id": "MDRW2100046654.1.5",
          "form": "디자인 보시는구나 ㅎㅎ",
          "original_form": "디자인 보시는구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046654.1.6",
          "form": "출근은 하셨나요?",
          "original_form": "출근은 하셨나요?",
          "speaker_id": "2",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046654.1.7",
          "form": "네 지금 했엉ㅅ",
          "original_form": "네 지금 했엉ㅅ",
          "speaker_id": "1",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046654.1.8",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "1",
          "time": "20211104 09:16"
        },
        {
          "id": "MDRW2100046654.1.9",
          "form": "집이랑 회사 가까우신가요? ㅋㅋ",
          "original_form": "집이랑 회사 가까우신가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046654.1.10",
          "form": "저는 지금 출근중입니다",
          "original_form": "저는 지금 출근중입니다",
          "speaker_id": "2",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046654.1.11",
          "form": "네 지하철 타고 이십분 걸령ㅅ",
          "original_form": "네 지하철 타고 이십분 걸령ㅅ",
          "speaker_id": "1",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046654.1.12",
          "form": "걸려요",
          "original_form": "걸려요",
          "speaker_id": "1",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046654.1.13",
          "form": "죄송해요 자꾸 오타나네요",
          "original_form": "죄송해요 자꾸 오타나네요",
          "speaker_id": "1",
          "time": "20211104 09:17"
        },
        {
          "id": "MDRW2100046654.1.14",
          "form": "괜찮습니다 ㅎㅎ",
          "original_form": "괜찮습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046654.1.15",
          "form": "헉 회사 엄청 가까우시네요",
          "original_form": "헉 회사 엄청 가까우시네요",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046654.1.16",
          "form": "역시 학교랑 회사는 가까운게 최고죠",
          "original_form": "역시 학교랑 회사는 가까운게 최고죠",
          "speaker_id": "2",
          "time": "20211104 09:20"
        },
        {
          "id": "MDRW2100046654.1.17",
          "form": "네 가까운데로 이직했어요",
          "original_form": "네 가까운데로 이직했어요",
          "speaker_id": "1",
          "time": "20211104 09:21"
        },
        {
          "id": "MDRW2100046654.1.18",
          "form": "너무 편해요 ㅎㅎ",
          "original_form": "너무 편해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 09:21"
        },
        {
          "id": "MDRW2100046654.1.19",
          "form": "출퇴근 시간 짧은게 큰 행복이에요 ㅋㅋㅋ",
          "original_form": "출퇴근 시간 짧은게 큰 행복이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 09:23"
        },
        {
          "id": "MDRW2100046654.1.20",
          "form": "늦게 일어나도 되고 일찍 퇴근할 수 있고 ㅋㅋㅋ 넘 좋아요!!!",
          "original_form": "늦게 일어나도 되고 일찍 퇴근할 수 있고 ㅋㅋㅋ 넘 좋아요!!!",
          "speaker_id": "2",
          "time": "20211104 09:23"
        },
        {
          "id": "MDRW2100046654.1.21",
          "form": "맞아요  좋아요",
          "original_form": "맞아요  좋아요",
          "speaker_id": "1",
          "time": "20211104 09:28"
        },
        {
          "id": "MDRW2100046654.1.22",
          "form": "그치만 회사는 가기 싫네요 ㅋㅋㅋ",
          "original_form": "그치만 회사는 가기 싫네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 09:30"
        },
        {
          "id": "MDRW2100046654.1.23",
          "form": "어떤 일 하세요?",
          "original_form": "어떤 일 하세요?",
