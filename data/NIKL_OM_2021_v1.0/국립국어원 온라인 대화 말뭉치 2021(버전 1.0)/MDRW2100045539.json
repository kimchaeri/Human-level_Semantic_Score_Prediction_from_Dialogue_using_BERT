      "utterance": [
        {
          "id": "MDRW2100045539.1.1",
          "form": "성형해보셨어요?",
          "original_form": "성형해보셨어요?",
          "speaker_id": "1",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100045539.1.2",
          "form": "아뇨!! 해보셨나요?",
          "original_form": "아뇨!! 해보셨나요?",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100045539.1.3",
          "form": "저도 안해봤는데",
          "original_form": "저도 안해봤는데",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100045539.1.4",
          "form": "마스크덕분에 많이들하더라고요",
          "original_form": "마스크덕분에 많이들하더라고요",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045539.1.5",
          "form": "오 마스크 많이 쓰는 게 또 영향이 있군요",
          "original_form": "오 마스크 많이 쓰는 게 또 영향이 있군요",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045539.1.6",
          "form": "하긴 덜 보이니까..",
          "original_form": "하긴 덜 보이니까..",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100045539.1.7",
          "form": "전 외커풀인데 ㅋㅋ 그냥 살아요ㅋㅋ",
          "original_form": "전 외커풀인데 ㅋㅋ 그냥 살아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045539.1.8",
          "form": "아 저는 있는데 나이들수록 얇아지는거 아시나요? ㅠㅠ",
          "original_form": "아 저는 있는데 나이들수록 얇아지는거 아시나요? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100045539.1.9",
          "form": "사람들이 왜 수술하는지 알겠어요",
          "original_form": "사람들이 왜 수술하는지 알겠어요",
          "speaker_id": "2",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100045539.1.10",
          "form": "아!! 맞아요",
          "original_form": "아!! 맞아요",
          "speaker_id": "1",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045539.1.11",
          "form": "다시 찝더라고요",
          "original_form": "다시 찝더라고요",
          "speaker_id": "1",
          "time": "20210908 03:24"
        },
        {
          "id": "MDRW2100045539.1.12",
          "form": "그쵸 찝은 사람도 나중에 다시 찝던데 전 욕심때문인줄 알았더니 아니었어요 ㅋㅋ",
          "original_form": "그쵸 찝은 사람도 나중에 다시 찝던데 전 욕심때문인줄 알았더니 아니었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:25"
        },
        {
          "id": "MDRW2100045539.1.13",
          "form": "그러니까요 없던 사람도 만드는데요!",
          "original_form": "그러니까요 없던 사람도 만드는데요!",
          "speaker_id": "1",
          "time": "20210908 03:28"
        },
        {
          "id": "MDRW2100045539.1.14",
          "form": "전 속눈썩 펌하려고요 시술이랑",
          "original_form": "전 속눈썩 펌하려고요 시술이랑",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045539.1.15",
          "form": "나 속눈썹 펌 많이 하더라고요..",
          "original_form": "나 속눈썹 펌 많이 하더라고요..",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045539.1.16",
          "form": "편하다던데",
          "original_form": "편하다던데",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045539.1.17",
          "form": "네 전 늘 찝거든요 뷰러로",
          "original_form": "네 전 늘 찝거든요 뷰러로",
          "speaker_id": "1",
          "time": "20210908 03:30"
        },
        {
          "id": "MDRW2100045539.1.18",
          "form": "아하 저는 속눈썹이 잘 찔러서.. 저도 뷰러 해야되는데 귀찮아서 ㅋㅋㅋ",
          "original_form": "아하 저는 속눈썹이 잘 찔러서.. 저도 뷰러 해야되는데 귀찮아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045539.1.19",
          "form": "연장도 요즘되게 자연스러워요!",
          "original_form": "연장도 요즘되게 자연스러워요!",
          "speaker_id": "1",
          "time": "20210908 03:34"
        }
      ]
