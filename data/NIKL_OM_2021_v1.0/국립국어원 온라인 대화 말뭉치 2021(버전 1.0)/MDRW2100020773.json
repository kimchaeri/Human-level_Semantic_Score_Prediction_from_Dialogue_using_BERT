      "utterance": [
        {
          "id": "MDRW2100020773.1.1",
          "form": "스포츠 경기 관람 좋아하시나요?",
          "original_form": "스포츠 경기 관람 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020773.1.2",
          "form": "올림픽이랑 월드컵만요 ㅋㅋ",
          "original_form": "올림픽이랑 월드컵만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020773.1.3",
          "form": "다음주? 카타르 월드컵 예선도 보시나요?",
          "original_form": "다음주? 카타르 월드컵 예선도 보시나요?",
          "speaker_id": "2",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020773.1.4",
          "form": "??!",
          "original_form": "??!",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020773.1.5",
          "form": "그게 벌써 다음주에요?",
          "original_form": "그게 벌써 다음주에요?",
          "speaker_id": "1",
          "time": "20210828 12:42"
        },
        {
          "id": "MDRW2100020773.1.6",
          "form": "그래서 보던 드라마가 결방한다고 들었어요..",
          "original_form": "그래서 보던 드라마가 결방한다고 들었어요..",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020773.1.7",
          "form": "헐랭 대박 알려주셔서 감사해요 ㅎㅎ",
          "original_form": "헐랭 대박 알려주셔서 감사해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020773.1.8",
          "form": "올림픽 끝나고 좀 심심했는데 볼거리 생겨서 좋네요",
          "original_form": "올림픽 끝나고 좀 심심했는데 볼거리 생겨서 좋네요",
          "speaker_id": "1",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020773.1.9",
          "form": "카더라가 아니길 바랄게요ㅎㅎ",
          "original_form": "카더라가 아니길 바랄게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020773.1.10",
          "form": "ㅋㅋㅋㅋ결방이요?",
          "original_form": "ㅋㅋㅋㅋ결방이요?",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020773.1.11",
          "form": "네네ㅋㅋㅋㅋ 아마 결방 맞을 거예요ㅎㅎ",
          "original_form": "네네ㅋㅋㅋㅋ 아마 결방 맞을 거예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020773.1.12",
          "form": "ㅠㅠㅠ올림픽은 얼마나 해요?",
          "original_form": "ㅠㅠㅠ올림픽은 얼마나 해요?",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020773.1.13",
          "form": "아니 올림픽 아니라 월드컵이요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니 올림픽 아니라 월드컵이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020773.1.14",
          "form": "제가 월드컵에 무지해서 모르겠어요...예선이니 짧지 않을까요?",
          "original_form": "제가 월드컵에 무지해서 모르겠어요...예선이니 짧지 않을까요?",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100020773.1.15",
          "form": "예선 통과하면 또 월드컵 본선도 하니 짧진 않을 것 같아요",
          "original_form": "예선 통과하면 또 월드컵 본선도 하니 짧진 않을 것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100020773.1.16",
          "form": "이번엔 좀 다르려나요ㅋㅋㅋㅋㅋ",
          "original_form": "이번엔 좀 다르려나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100020773.1.17",
          "form": "2002 월드컵을 못 보는 게 한이에요ㅠ",
