        {
          "id": "MDRW2100019933.1.1",
          "form": "좋아하는 음식있으신가요",
          "original_form": "좋아하는 음식있으신가요",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019933.1.2",
          "form": "좋아하는 음식있으신가요",
          "original_form": "좋아하는 음식있으신가요",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019933.1.3",
          "form": "저는 분식좋아해요",
          "original_form": "저는 분식좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.4",
          "form": "김밥 떡볶이 쫄면 등등",
          "original_form": "김밥 떡볶이 쫄면 등등",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.5",
          "form": "분식 뭐 좋아하세요",
          "original_form": "분식 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.6",
          "form": "골고루 좋아하시는구나",
          "original_form": "골고루 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.7",
          "form": "무슨음식좋아하세요??",
          "original_form": "무슨음식좋아하세요??",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.8",
          "form": "저도 분식좋아해요",
          "original_form": "저도 분식좋아해요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.9",
          "form": "김밥좋아합니다ㅎ",
          "original_form": "김밥좋아합니다ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.10",
          "form": "김밥은 어떤 김밥 좋아하시나요",
          "original_form": "김밥은 어떤 김밥 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019933.1.11",
          "form": "저 참치김밥 좋아해요",
          "original_form": "저 참치김밥 좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.12",
          "form": "점심에도 참김이랑 라면 먹었어요 ㅎㅎ",
          "original_form": "점심에도 참김이랑 라면 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.13",
          "form": "참치김밥 너무 맛있어요",
          "original_form": "참치김밥 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.14",
          "form": "라면은 어떤라면 드셨어요",
          "original_form": "라면은 어떤라면 드셨어요",
          "speaker_id": "1",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.15",
          "form": "신라면인 것 같아요!",
          "original_form": "신라면인 것 같아요!",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.16",
          "form": "김가네에서 먹엇거든요 ㅎㅎ",
          "original_form": "김가네에서 먹엇거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.17",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210827 22:55"
        },
        {
          "id": "MDRW2100019933.1.18",
          "form": "앗 그러셨그나 점심은 파바 샐러드 먹었어오ㅎㅎ",
          "original_form": "앗 그러셨그나 점심은 파바 샐러드 먹었어오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019933.1.19",
          "form": "파바가 은근 샐러드 맛집이더라구요",
          "original_form": "파바가 은근 샐러드 맛집이더라구요",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019933.1.20",
          "form": "오 맞어요 파바 샐러드 은근 맛잇어요",
          "original_form": "오 맞어요 파바 샐러드 은근 맛잇어요",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019933.1.21",
          "form": "하지만 배가 차지않는다 ㅋㅋㅋㅋ",
          "original_form": "하지만 배가 차지않는다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019933.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019933.1.23",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019933.1.24",
