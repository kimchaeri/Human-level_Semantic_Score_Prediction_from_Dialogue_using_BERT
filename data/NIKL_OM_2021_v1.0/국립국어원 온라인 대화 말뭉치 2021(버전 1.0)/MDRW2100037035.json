      "utterance": [
        {
          "id": "MDRW2100037035.1.1",
          "form": "저녁 어떤거 드실지 고르셨어요?",
          "original_form": "저녁 어떤거 드실지 고르셨어요?",
          "speaker_id": "1",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100037035.1.2",
          "form": "아뇨 아직이요 name1님은 뭐드실거에요?",
          "original_form": "아뇨 아직이요 &name1&님은 뭐드실거에요?",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100037035.1.3",
          "form": "점심에 먹은 전이 남아서 그거 부쳐 먹으려고해요!",
          "original_form": "점심에 먹은 전이 남아서 그거 부쳐 먹으려고해요!",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037035.1.4",
          "form": "아하 드시는게 무슨 전이에요?",
          "original_form": "아하 드시는게 무슨 전이에요?",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037035.1.5",
          "form": "김치전이요!",
          "original_form": "김치전이요!",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100037035.1.6",
          "form": "이번엔 된장국도 곁들여서용~",
          "original_form": "이번엔 된장국도 곁들여서용~",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100037035.1.7",
          "form": "집에 있는 김치로 만드신거에요? 요리잘하시나봐요",
          "original_form": "집에 있는 김치로 만드신거에요? 요리잘하시나봐요",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100037035.1.8",
          "form": "엄마가 만들어 주셨어욥ㅋㄲㅋ",
          "original_form": "엄마가 만들어 주셨어욥ㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100037035.1.9",
          "form": "저는 볶음밥 겨우 합니닷..ㅋㅋ",
          "original_form": "저는 볶음밥 겨우 합니닷..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100037035.1.10",
          "form": "아하 김치전을 다 만드시는줄 알았어요 ㄷㄷ",
          "original_form": "아하 김치전을 다 만드시는줄 알았어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100037035.1.11",
          "form": "ㅋㅋ저두 해보고자했지만..! 섞는게 최대였어욥",
          "original_form": "ㅋㅋ저두 해보고자했지만..! 섞는게 최대였어욥",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100037035.1.12",
          "form": "배민에서 배달 자주 하세요?",
          "original_form": "배민에서 배달 자주 하세요?",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100037035.1.13",
          "form": "요즘은 일주일에 한번만 하려고해요!",
          "original_form": "요즘은 일주일에 한번만 하려고해요!",
          "speaker_id": "1",
          "time": "20210831 18:25"
        },
        {
          "id": "MDRW2100037035.1.14",
          "form": "저도 자꾸 보게되어서 어플 지웠어요",
          "original_form": "저도 자꾸 보게되어서 어플 지웠어요",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037035.1.15",
          "form": "우왕 대단하세요ㅋㅋㅈ",
          "original_form": "우왕 대단하세요ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100037035.1.16",
          "form": "전에 지웠다가 다시 깔았슴닷..",
          "original_form": "전에 지웠다가 다시 깔았슴닷..",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100037035.1.17",
          "form": "방심하믄 안되겠어요!",
          "original_form": "방심하믄 안되겠어요!",
          "speaker_id": "1",
