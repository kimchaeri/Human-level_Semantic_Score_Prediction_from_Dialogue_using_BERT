      "utterance": [
        {
          "id": "MDRW2100002480.1.1",
          "form": "줄넘기?",
          "original_form": "줄넘기?",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002480.1.2",
          "form": "웅 줄넘기 짧아져서 줄넘기 사야 돼",
          "original_form": "웅 줄넘기 짧아져서 줄넘기 사야 돼",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002480.1.3",
          "form": "어떤 걸로 살까",
          "original_form": "어떤 걸로 살까",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002480.1.4",
          "form": "그냥 가볍고 줄 잘 꼬이지 않는거",
          "original_form": "그냥 가볍고 줄 잘 꼬이지 않는거",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002480.1.5",
          "form": "아무리 좋은 것도 오래 쓰다보면 줄 꼬이던데ㅠ",
          "original_form": "아무리 좋은 것도 오래 쓰다보면 줄 꼬이던데ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002480.1.6",
          "form": "그냥 적당한 걸로 사요",
          "original_form": "그냥 적당한 걸로 사요",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002480.1.7",
          "form": "아디다스 같은 메이커로 사야하나",
          "original_form": "아디다스 같은 메이커로 사야하나",
          "speaker_id": "2",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002480.1.8",
          "form": "노노 넘 고급 말고 그냥 적당한거",
          "original_form": "노노 넘 고급 말고 그냥 적당한거",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002480.1.9",
          "form": "ㅎㅎㅎ 그럼 애들꺼랑 비슷한 걸로 살게",
          "original_form": "ㅎㅎㅎ 그럼 애들꺼랑 비슷한 걸로 살게",
          "speaker_id": "2",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002480.1.10",
          "form": "그랴요 그 정도면 충분해.",
          "original_form": "그랴요 그 정도면 충분해.",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002480.1.11",
          "form": "길이만 잘 확인해줘",
          "original_form": "길이만 잘 확인해줘",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002480.1.12",
          "form": "알았응",
          "original_form": "알았응",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002480.1.13",
          "form": "길이 충분한 걸로 알아볼게",
          "original_form": "길이 충분한 걸로 알아볼게",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002480.1.14",
          "form": "스마일캐시로 사면 좀 싸자나",
          "original_form": "스마일캐시로 사면 좀 싸자나",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002480.1.15",
          "form": "웅 ㅎㅎ 내가 알아서 알아볼게",
          "original_form": "웅 ㅎㅎ 내가 알아서 알아볼게",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002480.1.16",
          "form": "오늘 옷한벌 사야겠다. 티셔츠",
          "original_form": "오늘 옷한벌 사야겠다. 티셔츠",
