      "utterance": [
        {
          "id": "MDRW2100008892.1.1",
          "form": "혹시 아르바이트나 직업이 있나요?",
          "original_form": "혹시 아르바이트나 직업이 있나요?",
          "speaker_id": "2",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008892.1.2",
          "form": "아뇨! 학생이라서요 ㅠㅠ",
          "original_form": "아뇨! 학생이라서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008892.1.3",
          "form": "님은용",
          "original_form": "님은용",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008892.1.4",
          "form": "아직 학생이시네요!",
          "original_form": "아직 학생이시네요!",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008892.1.5",
          "form": "저는 직장인이요 ㅋㅋ",
          "original_form": "저는 직장인이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008892.1.6",
          "form": "아하 무슨일해요??",
          "original_form": "아하 무슨일해요??",
          "speaker_id": "1",
          "time": "20210822 19:37"
        },
        {
          "id": "MDRW2100008892.1.7",
          "form": "아 저는 해외영업쪽이요!!",
          "original_form": "아 저는 해외영업쪽이요!!",
          "speaker_id": "2",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100008892.1.8",
          "form": "오!! 대단하시네용",
          "original_form": "오!! 대단하시네용",
          "speaker_id": "1",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008892.1.9",
          "form": "아니에요 ㅎㅎ",
          "original_form": "아니에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008892.1.10",
          "form": "혹시 뭐 하고 싶은 직업이나 있어요?",
          "original_form": "혹시 뭐 하고 싶은 직업이나 있어요?",
          "speaker_id": "2",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100008892.1.11",
          "form": "음 그냥 전 아무 직장이나 들어가고싶어요 ㅋㅋ",
          "original_form": "음 그냥 전 아무 직장이나 들어가고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:05"
        },
        {
          "id": "MDRW2100008892.1.12",
          "form": "학생이면 고등학생이에요??",
          "original_form": "학생이면 고등학생이에요??",
          "speaker_id": "2",
          "time": "20210822 20:05"
        },
        {
          "id": "MDRW2100008892.1.13",
          "form": "아녀 중3이여 ㅋㅋ",
          "original_form": "아녀 중3이여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:07"
        },
        {
          "id": "MDRW2100008892.1.14",
          "form": "와.....",
          "original_form": "와.....",
          "speaker_id": "2",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008892.1.15",
          "form": "아직 직업가질 때까지는 많이 남았으니",
          "original_form": "아직 직업가질 때까지는 많이 남았으니",
          "speaker_id": "2",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008892.1.16",
          "form": "걱정 안 해도 될 것 같아요",
          "original_form": "걱정 안 해도 될 것 같아요",
          "speaker_id": "2",
          "time": "20210822 20:08"
        },
        {
          "id": "MDRW2100008892.1.17",
          "form": "ㅎㅎ 감사합니당!!",
          "original_form": "ㅎㅎ 감사합니당!!",
          "speaker_id": "1",
          "time": "20210822 20:09"
        },
        {
          "id": "MDRW2100008892.1.18",
          "form": "어렸을 때 꿈꿨던 직업은 없어요?",
          "original_form": "어렸을 때 꿈꿨던 직업은 없어요?",
          "speaker_id": "2",
          "time": "20210822 20:10"
        },
        {
          "id": "MDRW2100008892.1.19",
          "form": "초딩때 요리사 하고싶었는데 ㅋㅋ 제가 요리를 못하더라구요",
          "original_form": "초딩때 요리사 하고싶었는데 ㅋㅋ 제가 요리를 못하더라구요",
          "speaker_id": "1",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008892.1.20",
          "form": "저도 어렸을 땐 요리사가 꿈이었어요",
          "original_form": "저도 어렸을 땐 요리사가 꿈이었어요",
          "speaker_id": "2",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100008892.1.21",
          "form": "요리를 해본 적도 없었는데 요리사가 되고 싶다고 했었더라구요 ㅋㅋ",
          "original_form": "요리를 해본 적도 없었는데 요리사가 되고 싶다고 했었더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100008892.1.22",
          "form": "ㅋㅋㅋㅋㅋ 요리사가 먼가 멋져보이고 제일 인기많앗던거 같아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 요리사가 먼가 멋져보이고 제일 인기많앗던거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
