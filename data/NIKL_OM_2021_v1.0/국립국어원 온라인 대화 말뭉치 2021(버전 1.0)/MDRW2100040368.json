      "utterance": [
        {
          "id": "MDRW2100040368.1.1",
          "form": "아마 고흥 기사식당치면 바로 검색될꺼에요ㅎㅎ",
          "original_form": "아마 고흥 기사식당치면 바로 검색될꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100040368.1.2",
          "form": "삼겹살백반이라고 쳐야나오네요ㅎㅎ",
          "original_form": "삼겹살백반이라고 쳐야나오네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100040368.1.3",
          "form": "동방기사식당인가요!",
          "original_form": "동방기사식당인가요!",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100040368.1.4",
          "form": "8000원짜리 스케일이 어마무시하네요ㄷㄷ",
          "original_form": "8000원짜리 스케일이 어마무시하네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100040368.1.5",
          "form": "국도 맛있어요ㅋ",
          "original_form": "국도 맛있어요ㅋ",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100040368.1.6",
          "form": "먹는데는 진심이라ㅋㅋㅋㅋ",
          "original_form": "먹는데는 진심이라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100040368.1.7",
          "form": "점심에 삼겹 먹었는데 이거보니 허기지네요ㅎㅎ",
          "original_form": "점심에 삼겹 먹었는데 이거보니 허기지네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100040368.1.8",
          "form": "여기가면 고기도 추가해 먹어야겠어요ㅎ",
          "original_form": "여기가면 고기도 추가해 먹어야겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040368.1.9",
          "form": "아 맞다 초벌안해줬어요ㅋㅋ",
          "original_form": "아 맞다 초벌안해줬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040368.1.10",
          "form": "제가 안구웠더니ㅋㅋㅋ",
          "original_form": "제가 안구웠더니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040368.1.11",
          "form": "먹느라 먹기전 과정이 없어졌나봐요ㅋㅋㅋ",
          "original_form": "먹느라 먹기전 과정이 없어졌나봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040368.1.12",
          "form": "아ㅋㅋ 저도 제가 안 구워도 되는 친구랑 가야겠네요ㅋㅋ",
          "original_form": "아ㅋㅋ 저도 제가 안 구워도 되는 친구랑 가야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040368.1.13",
          "form": "고기가 맛있었어요ㅠㅠ",
          "original_form": "고기가 맛있었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040368.1.14",
          "form": "고기가 흔히 먹는 삼겹과 좀 다르네요",
          "original_form": "고기가 흔히 먹는 삼겹과 좀 다르네요",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100040368.1.15",
          "form": "얇고 후추 간돼 있어서",
          "original_form": "얇고 후추 간돼 있어서",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100040368.1.16",
          "form": "밥도둑 삘이 납니다!!",
          "original_form": "밥도둑 삘이 납니다!!",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100040368.1.17",
          "form": "네! 맞아요! 매의 눈이십니다!ㅋㅋㅋㅋ",
          "original_form": "네! 맞아요! 매의 눈이십니다!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100040368.1.18",
          "form": "제가 위장끈은 하버드죠ㅎㅎㅎ",
          "original_form": "제가 위장끈은 하버드죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100040368.1.19",
          "form": "어머나 저도 위장끈이 엄청 긴데ㅋㅋㅋ",
          "original_form": "어머나 저도 위장끈이 엄청 긴데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100040368.1.20",
          "form": "잘하면 동기가 될뻔했네요ㅋㅋㅋㅋ",
          "original_form": "잘하면 동기가 될뻔했네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100040368.1.21",
          "form": "ㅎㅎ 앗 동문 아니었나요!",
          "original_form": "ㅎㅎ 앗 동문 아니었나요!",
          "speaker_id": "1",
          "time": "20210904 18:54"
        },
        {
          "id": "MDRW2100040368.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ역시 배우신분ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ역시 배우신분ㅋㅋㅋ",
