      "utterance": [
        {
          "id": "MDRW2100032460.1.1",
          "form": "양파계란카레 만들기 쉬워!",
          "original_form": "양파계란카레 만들기 쉬워!",
          "speaker_id": "2",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100032460.1.2",
          "form": "카레 좋아해?",
          "original_form": "카레 좋아해?",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100032460.1.3",
          "form": "조리 간편해서 자주 먹어",
          "original_form": "조리 간편해서 자주 먹어",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032460.1.4",
          "form": "아비꼬 맛있는데",
          "original_form": "아비꼬 맛있는데",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032460.1.5",
          "form": "아비꼬 맛있지",
          "original_form": "아비꼬 맛있지",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032460.1.6",
          "form": "매운 맛도 잘 먹어?",
          "original_form": "매운 맛도 잘 먹어?",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032460.1.7",
          "form": "매운 맛은 안먹어 봤어",
          "original_form": "매운 맛은 안먹어 봤어",
          "speaker_id": "2",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100032460.1.8",
          "form": "매운 맛은 너무 매워 순한 맛이 좋아 ㅋㅋㅋ",
          "original_form": "매운 맛은 너무 매워 순한 맛이 좋아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032460.1.9",
          "form": "돈까스랑 카레 잘 어울려",
          "original_form": "돈까스랑 카레 잘 어울려",
          "speaker_id": "2",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032460.1.10",
          "form": "아비꼬가 아이디어가 좋아",
          "original_form": "아비꼬가 아이디어가 좋아",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032460.1.11",
          "form": "맞아 인기 많았지",
          "original_form": "맞아 인기 많았지",
          "speaker_id": "2",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032460.1.12",
          "form": "요즘에도 많나?",
          "original_form": "요즘에도 많나?",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032460.1.13",
          "form": "모르겠어 코로나 터지고는 안가서",
          "original_form": "모르겠어 코로나 터지고는 안가서",
          "speaker_id": "2",
          "time": "20210905 18:34"
