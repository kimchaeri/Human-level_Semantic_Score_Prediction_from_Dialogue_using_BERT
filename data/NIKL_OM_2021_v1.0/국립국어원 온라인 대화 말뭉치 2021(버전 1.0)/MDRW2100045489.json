      "utterance": [
        {
          "id": "MDRW2100045489.1.1",
          "form": "난",
          "original_form": "난",
          "speaker_id": "2",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045489.1.2",
          "form": "차디찬 맨바닥엔",
          "original_form": "차디찬 맨바닥엔",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045489.1.3",
          "form": "한여름 외엔 눕지 안아",
          "original_form": "한여름 외엔 눕지 안아",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045489.1.4",
          "form": "이미 침대와 소파에 길들여졌어....",
          "original_form": "이미 침대와 소파에 길들여졌어....",
          "speaker_id": "2",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045489.1.5",
          "form": "ㅋㅋㅋㅋㅋ 글킨 해",
          "original_form": "ㅋㅋㅋㅋㅋ 글킨 해",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045489.1.6",
          "form": "나도 쿠션감 없으면 못누워",
          "original_form": "나도 쿠션감 없으면 못누워",
          "speaker_id": "1",
          "time": "20210908 02:52"
        },
        {
          "id": "MDRW2100045489.1.7",
          "form": "ㅇㅇ그리고 이게",
          "original_form": "ㅇㅇ그리고 이게",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045489.1.8",
          "form": "내가 나이를 먹어서 그런가",
          "original_form": "내가 나이를 먹어서 그런가",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045489.1.9",
          "form": "바닥에 누워있으면",
          "original_form": "바닥에 누워있으면",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045489.1.10",
          "form": "괜히 오한드는 기분이야",
          "original_form": "괜히 오한드는 기분이야",
          "speaker_id": "2",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100045489.1.11",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠ 뼈가 시리니 ...?",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠ 뼈가 시리니 ...?",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045489.1.12",
          "form": "아니 그건 아닌데",
          "original_form": "아니 그건 아닌데",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045489.1.13",
          "form": "근육이 굳어",
          "original_form": "근육이 굳어",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045489.1.14",
          "form": "목이 돌아가는 느낌이야....",
          "original_form": "목이 돌아가는 느낌이야....",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.15",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.16",
          "form": "어른들의 말을 이해해가는군....",
          "original_form": "어른들의 말을 이해해가는군....",
          "speaker_id": "1",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ요새",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ요새",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.18",
          "form": "날씨만 저기압이면",
          "original_form": "날씨만 저기압이면",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.19",
          "form": "그렇게 머리가 아파서",
          "original_form": "그렇게 머리가 아파서",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.20",
          "form": "아 이게",
          "original_form": "아 이게",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.21",
          "form": "무릎시리면 비오는",
          "original_form": "무릎시리면 비오는",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.22",
          "form": "그건가보다",
          "original_form": "그건가보다",
          "speaker_id": "2",
          "time": "20210908 02:55"
        },
        {
          "id": "MDRW2100045489.1.23",
          "form": "하고있음...",
          "original_form": "하고있음...",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045489.1.24",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045489.1.25",
          "form": "벌써 그러면 어떡해....",
          "original_form": "벌써 그러면 어떡해....",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045489.1.26",
          "form": "아냐 편두통은",
          "original_form": "아냐 편두통은",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045489.1.27",
          "form": "옛날부터 심했어",
          "original_form": "옛날부터 심했어",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045489.1.28",
          "form": "이제 좀",
          "original_form": "이제 좀",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045489.1.29",
          "form": "섬세하게 분류할 뿐이지....",
          "original_form": "섬세하게 분류할 뿐이지....",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045489.1.30",
          "form": "그리고 요새 진통제를 안먹어서",
          "original_form": "그리고 요새 진통제를 안먹어서",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100045489.1.31",
          "form": "ㅇㅎ.... 진통제 왜 안먹어?",
          "original_form": "ㅇㅎ.... 진통제 왜 안먹어?",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045489.1.32",
          "form": "이번주 금요일에",
          "original_form": "이번주 금요일에",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045489.1.33",
          "form": "백신 맞거든?",
          "original_form": "백신 맞거든?",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045489.1.34",
          "form": "근데 그전에 진통제 먹으면",
          "original_form": "근데 그전에 진통제 먹으면",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045489.1.35",
          "form": "반감될수도 있대서",
          "original_form": "반감될수도 있대서",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045489.1.36",
          "form": "걍 머리아프면 벽에다 찧고있어",
          "original_form": "걍 머리아프면 벽에다 찧고있어",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045489.1.37",
          "form": "밷 코로나 밷 코로나!!",
          "original_form": "밷 코로나 밷 코로나!!",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045489.1.38",
          "form": "아 백신때문에 ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "아 백신때문에 ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045489.1.39",
          "form": "ㅇㅇ....으쩔수업지",
          "original_form": "ㅇㅇ....으쩔수업지",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045489.1.40",
          "form": "세번 맞는짓은 하고싶지 않으므로...",
          "original_form": "세번 맞는짓은 하고싶지 않으므로...",
          "speaker_id": "2",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045489.1.41",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045489.1.42",
          "form": "그래..",
          "original_form": "그래..",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100045489.1.43",
          "form": "컨디션 관리 잘해...",
          "original_form": "컨디션 관리 잘해...",
          "speaker_id": "1",
