      "utterance": [
        {
          "id": "MDRW2100041881.1.1",
          "form": "맞아요 편해서 좋아요",
          "original_form": "맞아요 편해서 좋아요",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041881.1.2",
          "form": "그 밀푀유 샀다가 놀랬잖아요ㅋㅋㅋㅋ",
          "original_form": "그 밀푀유 샀다가 놀랬잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041881.1.3",
          "form": "편하지 않으세여?",
          "original_form": "편하지 않으세여?",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041881.1.4",
          "form": "당연히 되어있는줄 알고ㅋㅋㅋㅋ 근데 생각해보니 해놓으면 상하는데",
          "original_form": "당연히 되어있는줄 알고ㅋㅋㅋㅋ 근데 생각해보니 해놓으면 상하는데",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041881.1.5",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041881.1.6",
          "form": "그런 생각까지 안하고 샀어욬ㅋㅋㅋㅋ",
          "original_form": "그런 생각까지 안하고 샀어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041881.1.7",
          "form": "그건 번거롭긴하죠",
          "original_form": "그건 번거롭긴하죠",
          "speaker_id": "2",
          "time": "20210906 17:02"
        },
        {
          "id": "MDRW2100041881.1.8",
          "form": "그냥 하기는 했는데",
          "original_form": "그냥 하기는 했는데",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041881.1.9",
          "form": "귀찮았어욬ㅋㅋㅋ",
          "original_form": "귀찮았어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041881.1.10",
          "form": "그냥 하는게 빠를판ㅋㅋㅋㅋ",
          "original_form": "그냥 하는게 빠를판ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041881.1.11",
          "form": "전 전에 곱창전골 삿는데 야채를 안씻고햇어여",
          "original_form": "전 전에 곱창전골 삿는데 야채를 안씻고햇어여",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041881.1.12",
          "form": "다 씻어주는줄ㅋㅋㅋ",
          "original_form": "다 씻어주는줄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041881.1.13",
          "form": "ㅋㅋㅋㅋㅋ 저도 곱창전골 사고",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 곱창전골 사고",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100041881.1.14",
          "form": "다 씻어서 나오는거 아닌가 했는데",
          "original_form": "다 씻어서 나오는거 아닌가 했는데",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041881.1.15",
          "form": "아니더라고욬ㅋㅋㅋ",
          "original_form": "아니더라고욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041881.1.16",
          "form": "식구들한테 안씻은건 비밀로 했어여ㅋ",
          "original_form": "식구들한테 안씻은건 비밀로 했어여ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041881.1.17",
          "form": "아직까지 기술의 발전 멀었다고 생각합니다 ㅋㅋㅋ",
          "original_form": "아직까지 기술의 발전 멀었다고 생각합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041881.1.18",
          "form": "ㅋㅋㅋㅋㅋ 괜찮아요 그럴수도 있죠",
          "original_form": "ㅋㅋㅋㅋㅋ 괜찮아요 그럴수도 있죠",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100041881.1.19",
          "form": "안씻은것도 먹고 해야 면역력이...ㅋㅋㅋ",
          "original_form": "안씻은것도 먹고 해야 면역력이...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100041881.1.20",
