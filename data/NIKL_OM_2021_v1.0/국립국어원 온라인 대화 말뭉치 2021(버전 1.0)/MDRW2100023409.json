      "utterance": [
        {
          "id": "MDRW2100023409.1.1",
          "form": "제 딸은 겁이많나봐요ㅎ",
          "original_form": "제 딸은 겁이많나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023409.1.2",
          "form": "머리짜르는데 엄청 울어요ㅎ",
          "original_form": "머리짜르는데 엄청 울어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023409.1.3",
          "form": "많이 울어요?",
          "original_form": "많이 울어요?",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023409.1.4",
          "form": "아 5세 오빠더 울어요  ㅎㅎ",
          "original_form": "아 5세 오빠더 울어요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023409.1.5",
          "form": "아빠닮아서ㅎ 내성적이고ㅎㅎ",
          "original_form": "아빠닮아서ㅎ 내성적이고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:41"
        },
        {
          "id": "MDRW2100023409.1.6",
          "form": "애들은 100에 100은 싫어해여 ㅎㅎ",
          "original_form": "애들은 100에 100은 싫어해여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.7",
          "form": "ㅎㅎㅎ 다 우나봐요ㅎ",
          "original_form": "ㅎㅎㅎ 다 우나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.8",
          "form": "글쿤요ㅎ",
          "original_form": "글쿤요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.9",
          "form": "근데 너무 우니깐 쨘해서 ㅠ",
          "original_form": "근데 너무 우니깐 쨘해서 ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.10",
          "form": "애기가 우는걸 못보겟어요ㅠ",
          "original_form": "애기가 우는걸 못보겟어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.11",
          "form": "아근데 여아들은 5살 이상되면 펌 할땐 안울어요 ㅎㅎ",
          "original_form": "아근데 여아들은 5살 이상되면 펌 할땐 안울어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.12",
          "form": "뻔히 하고싶지않아서 때쓰는건줄 알면서도ㅎ",
          "original_form": "뻔히 하고싶지않아서 때쓰는건줄 알면서도ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100023409.1.13",
          "form": "아이 울면 속상하죠  ㅎㅎ 맴찢",
          "original_form": "아이 울면 속상하죠  ㅎㅎ 맴찢",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023409.1.14",
          "form": "오 그렇군요ㅎ 5살이상되야지 펌할수있다고 하더라구요ㅎ",
          "original_form": "오 그렇군요ㅎ 5살이상되야지 펌할수있다고 하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023409.1.15",
          "form": "엄마는 안달애주거든요 그게 당연한건데",
          "original_form": "엄마는 안달애주거든요 그게 당연한건데",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023409.1.16",
          "form": "36개월 이상이면 되요~",
          "original_form": "36개월 이상이면 되요~",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023409.1.17",
          "form": "전 너무 슬퍼서 몰래 안아주고 달래주고",
          "original_form": "전 너무 슬퍼서 몰래 안아주고 달래주고",
          "speaker_id": "2",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100023409.1.18",
          "form": "훈육이 가장어려워요ㅠ",
          "original_form": "훈육이 가장어려워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023409.1.19",
          "form": "음 맞아요 ㅎㅎ",
          "original_form": "음 맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023409.1.20",
          "form": "펌은 지금 해도되군요ㅎ",
          "original_form": "펌은 지금 해도되군요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023409.1.21",
          "form": "네네 ㅎㅎ  오빠어린이는 36개월때 한번 했어요 ㅎㅎㅎ",
          "original_form": "네네 ㅎㅎ  오빠어린이는 36개월때 한번 했어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023409.1.22",
          "form": "아마 제 딸은 제가 만만할꺼에요ㅎㅎ",
          "original_form": "아마 제 딸은 제가 만만할꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100023409.1.23",
          "form": "우와ㅎ 귀여웠겟네요ㅎ",
          "original_form": "우와ㅎ 귀여웠겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.24",
          "form": "음  잘한다고 만만하진 않아요",
          "original_form": "음  잘한다고 만만하진 않아요",
          "speaker_id": "1",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.25",
          "form": "이게 아이들이요",
          "original_form": "이게 아이들이요",
          "speaker_id": "1",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.26",
          "form": "서열을 알아요",
          "original_form": "서열을 알아요",
          "speaker_id": "1",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.27",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.28",
          "form": "서열ㅎ",
          "original_form": "서열ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.29",
          "form": "아빠가 밑이죠ㅎㅎㅎㅎㅎ",
          "original_form": "아빠가 밑이죠ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.30",
          "form": "음 그래서 만만히 볼수 잇죠",
          "original_form": "음 그래서 만만히 볼수 잇죠",
          "speaker_id": "1",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.31",
          "form": "맨날 아빠! 아빠! 이거해봐요ㅎ",
          "original_form": "맨날 아빠! 아빠! 이거해봐요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:45"
        },
        {
          "id": "MDRW2100023409.1.32",
          "form": "뽀뽀도 안해주고ㅎ 삐치고ㅎㅎ",
          "original_form": "뽀뽀도 안해주고ㅎ 삐치고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:46"
        },
        {
          "id": "MDRW2100023409.1.33",
          "form": "음 그리고 편해서 그러기도 하구요",
          "original_form": "음 그리고 편해서 그러기도 하구요",
          "speaker_id": "1",
          "time": "20210830 01:46"
        },
        {
          "id": "MDRW2100023409.1.34",
          "form": "그래도 그모습이 얼마나 이쁜지ㅎㅎ 너무 행복해요ㅎ",
          "original_form": "그래도 그모습이 얼마나 이쁜지ㅎㅎ 너무 행복해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:46"
        },
        {
          "id": "MDRW2100023409.1.35",
          "form": "편한가봐요ㅎ아마 엄마보다 절 더편하게생각할꺼 같애요ㅎ",
          "original_form": "편한가봐요ㅎ아마 엄마보다 절 더편하게생각할꺼 같애요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:46"
        }
      ]
