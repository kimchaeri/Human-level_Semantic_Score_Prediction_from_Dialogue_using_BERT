      "utterance": [
        {
          "id": "MDRW2100034076.1.1",
          "form": "안녕하세요 ~ 오늘 날씨 좋지않나요",
          "original_form": "안녕하세요 ~ 오늘 날씨 좋지않나요",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100034076.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100034076.1.3",
          "form": "시원해서 좋네요!",
          "original_form": "시원해서 좋네요!",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100034076.1.4",
          "form": "비는 안 오세요?",
          "original_form": "비는 안 오세요?",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100034076.1.5",
          "form": "네 저희 지역은 비는 안오네요 !",
          "original_form": "네 저희 지역은 비는 안오네요 !",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100034076.1.6",
          "form": "근데 소나기가 올거같긴해요 ㅎㅎ 비 안오시나요?",
          "original_form": "근데 소나기가 올거같긴해요 ㅎㅎ 비 안오시나요?",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100034076.1.7",
          "form": "방금 조금 왔는데 다시 그쳤어요. 원래 비 소식이 있었거든요!",
          "original_form": "방금 조금 왔는데 다시 그쳤어요. 원래 비 소식이 있었거든요!",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100034076.1.8",
          "form": "저희쪽도 3시에 비가 온다해서 ㅎㅎ 올거같아요 !",
          "original_form": "저희쪽도 3시에 비가 온다해서 ㅎㅎ 올거같아요 !",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034076.1.9",
          "form": "내일도 비 많이 온대요 !",
          "original_form": "내일도 비 많이 온대요 !",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100034076.1.10",
          "form": "그렇군요! 집에만 있어야겠어요... 내일 외출할 일 있으세요?",
          "original_form": "그렇군요! 집에만 있어야겠어요... 내일 외출할 일 있으세요?",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100034076.1.11",
          "form": "저는 원래 내일 병원에 가려했는데 비가 와서 안가려구요 ㅎㅎ",
          "original_form": "저는 원래 내일 병원에 가려했는데 비가 와서 안가려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100034076.1.12",
          "form": "지금은 외출하셨어요?",
          "original_form": "지금은 외출하셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100034076.1.13",
          "form": "아뇨 집이에요!",
          "original_form": "아뇨 집이에요!",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100034076.1.14",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100034076.1.15",
          "form": "저도 집이에요 ! 근데 곧 나가야하는데",
          "original_form": "저도 집이에요 ! 근데 곧 나가야하는데",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100034076.1.16",
          "form": "우산을 챙겨야겠죠 ?",
          "original_form": "우산을 챙겨야겠죠 ?",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100034076.1.17",
          "form": "네네 꼭 챙겨가세요!",
          "original_form": "네네 꼭 챙겨가세요!",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100034076.1.18",
          "form": "3단짜리 챙겨가야겠네요 !",
          "original_form": "3단짜리 챙겨가야겠네요 !",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100034076.1.19",
          "form": "우산 챙겼으니 비가 와야될텐데 ㅋㅋ",
          "original_form": "우산 챙겼으니 비가 와야될텐데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100034076.1.20",
          "form": "앗ㅋㅋㅋㅋ 옷도 따뜻하게 입고 나가세요! 비오고 나서 많이 쌀쌀해졌어요",
          "original_form": "앗ㅋㅋㅋㅋ 옷도 따뜻하게 입고 나가세요! 비오고 나서 많이 쌀쌀해졌어요",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100034076.1.21",
          "form": "그래야겠어요! 얇은 가디건 하나 챙기려구요 ㅎㅎ",
          "original_form": "그래야겠어요! 얇은 가디건 하나 챙기려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        }
      ]
