      "utterance": [
        {
          "id": "MDRW2100033869.1.1",
          "form": "name2님의\r\n최애떡볶이는 어딘가여ㅋㅋ",
          "original_form": "&name2&님의\r\n최애떡볶이는 어딘가여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033869.1.2",
          "form": "전ㅋㅋㅋ",
          "original_form": "전ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100033869.1.3",
          "form": "요즘 떡군이네 파스타면추가해서 계속먹어요ㅋㅋㅋㅋ",
          "original_form": "요즘 떡군이네 파스타면추가해서 계속먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033869.1.4",
          "form": "빨간떡볶이는 신전이랑",
          "original_form": "빨간떡볶이는 신전이랑",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033869.1.5",
          "form": "옆집떡볶이좋아한답니다",
          "original_form": "옆집떡볶이좋아한답니다",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033869.1.6",
          "form": "즉석떡볶이도 좋아해요",
          "original_form": "즉석떡볶이도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033869.1.7",
          "form": "떡군이네는 몰라용ㅋㅋ",
          "original_form": "떡군이네는 몰라용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100033869.1.8",
          "form": "ㅋㅋㅋ 맛있답니다",
          "original_form": "ㅋㅋㅋ 맛있답니다",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033869.1.9",
          "form": "삼겹살vs목살은 어떠세요?",
          "original_form": "삼겹살vs목살은 어떠세요?",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100033869.1.10",
          "form": "저는 목살 안좋아해요ㅋㅋ",
          "original_form": "저는 목살 안좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100033869.1.11",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033869.1.12",
          "form": "삼겹파랍니다",
          "original_form": "삼겹파랍니다",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100033869.1.13",
          "form": "삼겹이가 더 맛있죠ㅋㅋㅋㅋㅋㅈ",
          "original_form": "삼겹이가 더 맛있죠ㅋㅋㅋㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033869.1.14",
          "form": "살코기와 비계의\r\n아름다운 조화 ㅋㅋㅈ",
          "original_form": "살코기와 비계의\r\n아름다운 조화 ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033869.1.15",
          "form": "흑돼지는 어떠세요?",
          "original_form": "흑돼지는 어떠세요?",
          "speaker_id": "2",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033869.1.16",
          "form": "흑돼지는....ㅋㅋㅋㅋㅋ",
          "original_form": "흑돼지는....ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033869.1.17",
          "form": "삼겹이 만큼은 아니지만 어쨋든 호!",
          "original_form": "삼겹이 만큼은 아니지만 어쨋든 호!",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033869.1.18",
          "form": "불호는 아닙니다ㅋㅋ",
          "original_form": "불호는 아닙니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033869.1.19",
          "form": "와우 흑돼지",
          "original_form": "와우 흑돼지",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100033869.1.20",
          "form": "존맛인데ㅠㅠ",
          "original_form": "존맛인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100033869.1.21",
          "form": "제주도 가고싶네요",
          "original_form": "제주도 가고싶네요",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100033869.1.22",
          "form": "코시국이라\r\n가기가 좀 그렇죠? ㅠ",
          "original_form": "코시국이라\r\n가기가 좀 그렇죠? ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100033869.1.23",
          "form": "그쵸ㅠ아무래도",
          "original_form": "그쵸ㅠ아무래도",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100033869.1.24",
          "form": "짜장vs카레는",
          "original_form": "짜장vs카레는",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100033869.1.25",
          "form": "어떠세요?",
          "original_form": "어떠세요?",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100033869.1.26",
          "form": "짜장",
          "original_form": "짜장",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033869.1.27",
          "form": "카레를 못먹습니다ㅋㅋ",
          "original_form": "카레를 못먹습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033869.1.28",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100033869.1.29",
          "form": "둘다사실별로지만",
          "original_form": "둘다사실별로지만",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100033869.1.30",
          "form": "둘중고르자면 짜장이요",
          "original_form": "둘중고르자면 짜장이요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        }
      ]
