      "utterance": [
        {
          "id": "MDRW2100023295.1.1",
          "form": "아르바이트 경험 있나요?",
          "original_form": "아르바이트 경험 있나요?",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023295.1.2",
          "form": "네 ㅎ.ㅎ 키즈카페 알바를 주로 했었어요",
          "original_form": "네 ㅎ.ㅎ 키즈카페 알바를 주로 했었어요",
          "speaker_id": "2",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023295.1.3",
          "form": "어떠셨나요",
          "original_form": "어떠셨나요",
          "speaker_id": "1",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023295.1.4",
          "form": "생각보다 엄청 재밌었어요ㅋㅋㅋ",
          "original_form": "생각보다 엄청 재밌었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023295.1.5",
          "form": "어떤점이요?",
          "original_form": "어떤점이요?",
          "speaker_id": "1",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023295.1.6",
          "form": "아이들이랑 생각보다 잘 맞았거든요... 재밌었던 기억이 있네요ㅋㅋ",
          "original_form": "아이들이랑 생각보다 잘 맞았거든요... 재밌었던 기억이 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023295.1.7",
          "form": "다른 알바도 해보셨나요",
          "original_form": "다른 알바도 해보셨나요",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023295.1.8",
          "form": "네!애견카페 알바도 했었답니다ㅋㅋㅋㅋ",
          "original_form": "네!애견카페 알바도 했었답니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023295.1.9",
          "form": "키즈카페랑 비슷한가요??",
          "original_form": "키즈카페랑 비슷한가요??",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023295.1.10",
          "form": "네 거의비슷해요 케어대상이 강아지로 바뀌었다는점이요!",
          "original_form": "네 거의비슷해요 케어대상이 강아지로 바뀌었다는점이요!",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023295.1.11",
          "form": "재밌으셨겠어요",
          "original_form": "재밌으셨겠어요",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023295.1.12",
          "form": "네 나름 엄청 신기했던 경험이었네요 ㅋㅋㅋㅋ",
          "original_form": "네 나름 엄청 신기했던 경험이었네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023295.1.13",
          "form": "어떤일 하세요?",
          "original_form": "어떤일 하세요?",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023295.1.14",
          "form": "꽃집에서 일하고 있어요",
          "original_form": "꽃집에서 일하고 있어요",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023295.1.15",
          "form": "와 꽃집 안 힘드신가요?",
          "original_form": "와 꽃집 안 힘드신가요?",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
