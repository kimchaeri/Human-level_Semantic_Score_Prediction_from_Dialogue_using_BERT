      "utterance": [
        {
          "id": "MDRW2100032178.1.1",
          "form": "멀가장좋아하시나용",
          "original_form": "멀가장좋아하시나용",
          "speaker_id": "1",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100032178.1.2",
          "form": "카페가고싶아요..",
          "original_form": "카페가고싶아요..",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032178.1.3",
          "form": "저는 매운 거 좋아해요",
          "original_form": "저는 매운 거 좋아해요",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032178.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032178.1.5",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032178.1.6",
          "form": "맵찔이입니당",
          "original_form": "맵찔이입니당",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032178.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032178.1.8",
          "form": "아ㅋㅋㅋ저도 잘 먹지는 못해요",
          "original_form": "아ㅋㅋㅋ저도 잘 먹지는 못해요",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032178.1.9",
          "form": "카페 음료 어떤 거 좋아하세요?",
          "original_form": "카페 음료 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032178.1.10",
          "form": "저는에이류만먹어서",
          "original_form": "저는에이류만먹어서",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032178.1.11",
          "form": "어디든 다잘먹어요",
          "original_form": "어디든 다잘먹어요",
          "speaker_id": "1",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032178.1.12",
          "form": "아 에이드 좋아하시는구나",
          "original_form": "아 에이드 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032178.1.13",
          "form": "저는 요즘 바닐라 라떼가 좋더라구요",
          "original_form": "저는 요즘 바닐라 라떼가 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032178.1.14",
          "form": "오그거 달달하고 맛잇죵",
          "original_form": "오그거 달달하고 맛잇죵",
          "speaker_id": "1",
          "time": "20210905 14:01"
        },
        {
          "id": "MDRW2100032178.1.15",
          "form": "그쵸 전엔 아메리카노만 마셨는데 요즘은 바닐라 라떼도 맛있더라구요",
          "original_form": "그쵸 전엔 아메리카노만 마셨는데 요즘은 바닐라 라떼도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032178.1.16",
          "form": "전안가려요!!",
          "original_form": "전안가려요!!",
          "speaker_id": "1",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100032178.1.17",
          "form": "아 정말요? 에이드류 자주 드신다고 해서 카페인 못 드시는 줄 알았어요",
          "original_form": "아 정말요? 에이드류 자주 드신다고 해서 카페인 못 드시는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210905 17:53"
        },
        {
          "id": "MDRW2100032178.1.18",
          "form": "아니에요!!",
          "original_form": "아니에요!!",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032178.1.19",
          "form": "저 카페일해서",
          "original_form": "저 카페일해서",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032178.1.20",
          "form": "다잘먹어요 ㅋㅋ",
          "original_form": "다잘먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100032178.1.21",
          "form": "와 카페 일 하시는구나 그럼 커피 직접 내리시는 거예요?",
          "original_form": "와 카페 일 하시는구나 그럼 커피 직접 내리시는 거예요?",
          "speaker_id": "2",
          "time": "20210906 07:40"
        },
        {
          "id": "MDRW2100032178.1.22",
          "form": "넹넹!!",
          "original_form": "넹넹!!",
          "speaker_id": "1",
          "time": "20210906 07:40"
        },
        {
          "id": "MDRW2100032178.1.23",
          "form": "근데 기계가 해주는데",
          "original_form": "근데 기계가 해주는데",
          "speaker_id": "1",
          "time": "20210906 07:40"
        },
        {
          "id": "MDRW2100032178.1.24",
          "form": "저는 핸드메이드로 한번 내려보고싶어요 ㅠㅠㅠ",
          "original_form": "저는 핸드메이드로 한번 내려보고싶어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 07:40"
        },
        {
          "id": "MDRW2100032178.1.25",
          "form": "한번 먹어본적있는데",
          "original_form": "한번 먹어본적있는데",
          "speaker_id": "1",
          "time": "20210906 07:41"
        },
        {
          "id": "MDRW2100032178.1.26",
          "form": "풍미가 대박이더라구요!!",
          "original_form": "풍미가 대박이더라구요!!",
          "speaker_id": "1",
          "time": "20210906 07:41"
        },
        {
          "id": "MDRW2100032178.1.27",
          "form": "와 너무 멋있어요 저는 기계로 내리는 사람들이 되게 전문적이어 보이던데",
          "original_form": "와 너무 멋있어요 저는 기계로 내리는 사람들이 되게 전문적이어 보이던데",
          "speaker_id": "2",
          "time": "20210906 07:42"
        },
        {
          "id": "MDRW2100032178.1.28",
          "form": "오 핸드드립같은 건가요? 커피를 잘 몰라서",
          "original_form": "오 핸드드립같은 건가요? 커피를 잘 몰라서",
          "speaker_id": "2",
          "time": "20210906 07:42"
        },
        {
          "id": "MDRW2100032178.1.29",
          "form": "네네 핸드 메이드 같은거예요",
          "original_form": "네네 핸드 메이드 같은거예요",
          "speaker_id": "1",
          "time": "20210906 07:42"
        },
        {
          "id": "MDRW2100032178.1.30",
          "form": "기계보다 더 섬세해요",
          "original_form": "기계보다 더 섬세해요",
          "speaker_id": "1",
          "time": "20210906 07:42"
        },
        {
          "id": "MDRW2100032178.1.31",
          "form": "물조절도 해야하고",
          "original_form": "물조절도 해야하고",
          "speaker_id": "1",
          "time": "20210906 07:42"
        },
        {
          "id": "MDRW2100032178.1.32",
          "form": "가는것도직접해야하고",
          "original_form": "가는것도직접해야하고",
          "speaker_id": "1",
          "time": "20210906 07:42"
        },
        {
          "id": "MDRW2100032178.1.33",
          "form": "약간 힘든거하고 보상받는 그런느낌이랄까..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "약간 힘든거하고 보상받는 그런느낌이랄까..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 07:43"
        }
      ]
