      "utterance": [
        {
          "id": "MDRW2100013323.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013323.1.2",
          "form": "안녕하세요 학생이세요?",
          "original_form": "안녕하세요 학생이세요?",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013323.1.3",
          "form": "아뇨 저는 졸업했답니다. 지금은 취준생이예요",
          "original_form": "아뇨 저는 졸업했답니다. 지금은 취준생이예요",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013323.1.4",
          "form": "님은 학생이세용?",
          "original_form": "님은 학생이세용?",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013323.1.5",
          "form": "아뇨 학생아니에요",
          "original_form": "아뇨 학생아니에요",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013323.1.6",
          "form": "그렇군요 저는 취준하느라 시험을 준비중인데 오늘은 비가 와서 그런가 공부가 잘 안되네요ㅠㅠ",
          "original_form": "그렇군요 저는 취준하느라 시험을 준비중인데 오늘은 비가 와서 그런가 공부가 잘 안되네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013323.1.7",
          "form": "네 ㅠㅠ 무슨시험이세요?",
          "original_form": "네 ㅠㅠ 무슨시험이세요?",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013323.1.8",
          "form": "공시요!ㅎㅎ",
          "original_form": "공시요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013323.1.9",
          "form": "name2은 직장인이신가여?",
          "original_form": "&name2&은 직장인이신가여?",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013323.1.10",
          "form": "아뇨 유튜버에요 ㅎㅎ",
          "original_form": "아뇨 유튜버에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013323.1.11",
          "form": "우와~멋져요ㅎㅎ",
          "original_form": "우와~멋져요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013323.1.12",
          "form": "ㅎㅎ 감사합니다",
          "original_form": "ㅎㅎ 감사합니다",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013323.1.13",
          "form": "앞으로도 멋진 유튜버가 되시길!!:)",
          "original_form": "앞으로도 멋진 유튜버가 되시길!!:)",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013323.1.14",
          "form": "감사합니다 ㅎㅎ 시험 언제세요?",
          "original_form": "감사합니다 ㅎㅎ 시험 언제세요?",
          "speaker_id": "2",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013323.1.15",
          "form": "시험은 10월이용!!",
          "original_form": "시험은 10월이용!!",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100013323.1.16",
          "form": "혹시7급시험이에요?",
          "original_form": "혹시7급시험이에요?",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013323.1.17",
          "form": "넵ㅠㅠ맞아여",
          "original_form": "넵ㅠㅠ맞아여",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013323.1.18",
          "form": "이번에 피셋으로바뀌었죠? 시험 어땠어요?",
          "original_form": "이번에 피셋으로바뀌었죠? 시험 어땠어요?",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013323.1.19",
          "form": "피셋은 어렵더라구요ㅠㅠ 그래서 지방직7급을 노리고있어요",
          "original_form": "피셋은 어렵더라구요ㅠㅠ 그래서 지방직7급을 노리고있어요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013323.1.20",
          "form": "지방직은 피셋안하는군요",
          "original_form": "지방직은 피셋안하는군요",
