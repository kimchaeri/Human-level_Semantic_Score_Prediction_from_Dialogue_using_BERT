      "utterance": [
        {
          "id": "MDRW2100028656.1.1",
          "form": "도를..믿으십니까",
          "original_form": "도를..믿으십니까",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028656.1.2",
          "form": "도는 안믿고 저는 불교 좋아해요",
          "original_form": "도는 안믿고 저는 불교 좋아해요",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028656.1.3",
          "form": "그러고보니 도를 하면 사이비고 신천지한테 당한 기억난 떠오르는",
          "original_form": "그러고보니 도를 하면 사이비고 신천지한테 당한 기억난 떠오르는",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028656.1.4",
          "form": "조용히하세요",
          "original_form": "조용히하세요",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028656.1.5",
          "form": "가끔 의정부 갈 때 무서우니까",
          "original_form": "가끔 의정부 갈 때 무서우니까",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.6",
          "form": "저도 당했거든요!?",
          "original_form": "저도 당했거든요!?",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.7",
          "form": "난 그 일로 내 친구도 못믿게 되었어",
          "original_form": "난 그 일로 내 친구도 못믿게 되었어",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.8",
          "form": "어디서?",
          "original_form": "어디서?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.9",
          "form": "걔도 당한건지 걔도 한통속인건지",
          "original_form": "걔도 당한건지 걔도 한통속인건지",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.10",
          "form": "아이고...친구한테 당했구나",
          "original_form": "아이고...친구한테 당했구나",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.11",
          "form": "친구가 나 힘들어보여서 아는 언니가",
          "original_form": "친구가 나 힘들어보여서 아는 언니가",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028656.1.12",
          "form": "심리 어쩌고 한다고 같이 가자고 했는데",
          "original_form": "심리 어쩌고 한다고 같이 가자고 했는데",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.13",
          "form": "전에 들어본거같다",
          "original_form": "전에 들어본거같다",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.14",
          "form": "첨에 거절했다가 시간지나고 또 한다길래 그때 가서 같이 했거든",
          "original_form": "첨에 거절했다가 시간지나고 또 한다길래 그때 가서 같이 했거든",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.15",
          "form": "웅웅 그래서 그거로 성경배우다가",
          "original_form": "웅웅 그래서 그거로 성경배우다가",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.16",
          "form": "어라 이거 신천지?",
          "original_form": "어라 이거 신천지?",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.17",
          "form": "하고 관뒀지",
          "original_form": "하고 관뒀지",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.18",
          "form": "성경은 재미있었는데 방식이 별로였어서",
          "original_form": "성경은 재미있었는데 방식이 별로였어서",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.19",
          "form": "관둔게 어디야",
          "original_form": "관둔게 어디야",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.20",
          "form": "이게 사이비군아 했지",
          "original_form": "이게 사이비군아 했지",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.21",
          "form": "잘했어 그때",
          "original_form": "잘했어 그때",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028656.1.22",
          "form": "이 또한 지나가리",
          "original_form": "이 또한 지나가리",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028656.1.23",
          "form": "님도 불교 좋아하는구나",
          "original_form": "님도 불교 좋아하는구나",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028656.1.24",
          "form": "음 아니",
          "original_form": "음 아니",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.25",
          "form": "근데 불교가 교리가 좀 진보적이고 트여있어서",
          "original_form": "근데 불교가 교리가 좀 진보적이고 트여있어서",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.26",
          "form": "좋음",
          "original_form": "좋음",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.27",
          "form": "우리집 모태불교",
          "original_form": "우리집 모태불교",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.28",
          "form": "모태천주교인",
          "original_form": "모태천주교인",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.29",
          "form": "불교랑 천주교 같이 있는건가",
          "original_form": "불교랑 천주교 같이 있는건가",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.30",
          "form": "다있어서 종교에 뭐 편견같은것도 없고",
          "original_form": "다있어서 종교에 뭐 편견같은것도 없고",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028656.1.31",
