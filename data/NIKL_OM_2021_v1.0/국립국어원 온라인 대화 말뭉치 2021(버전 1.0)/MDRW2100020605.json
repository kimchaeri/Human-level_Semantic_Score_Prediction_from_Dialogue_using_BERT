      "utterance": [
        {
          "id": "MDRW2100020605.1.1",
          "form": "학교 다니세요?",
          "original_form": "학교 다니세요?",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020605.1.2",
          "form": "몇년전에 대학교 졸업해서",
          "original_form": "몇년전에 대학교 졸업해서",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020605.1.3",
          "form": "지금은 학생이 아니에요유ㅠ",
          "original_form": "지금은 학생이 아니에요유ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020605.1.4",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020605.1.5",
          "form": "그럼 학교 다닐 적에 기억남는 일 있어요?",
          "original_form": "그럼 학교 다닐 적에 기억남는 일 있어요?",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020605.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020605.1.7",
          "form": "야간자율학습이 10시까지였는데",
          "original_form": "야간자율학습이 10시까지였는데",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020605.1.8",
          "form": "그거 하루 빼보겠다고",
          "original_form": "그거 하루 빼보겠다고",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020605.1.9",
          "form": "선생님한테 엄청 졸랐어요ㅋㅋㅋ",
          "original_form": "선생님한테 엄청 졸랐어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020605.1.10",
          "form": "ㅋㅋㅋ아 귀여우시네요",
          "original_form": "ㅋㅋㅋ아 귀여우시네요",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020605.1.11",
          "form": "근데 왜 하루 빼보려고 했어요?",
          "original_form": "근데 왜 하루 빼보려고 했어요?",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020605.1.12",
          "form": "그냥 학생때",
          "original_form": "그냥 학생때",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020605.1.13",
          "form": "아 오늘은 집에가고싶다",
          "original_form": "아 오늘은 집에가고싶다",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020605.1.14",
          "form": "공부하기싫다",
          "original_form": "공부하기싫다",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020605.1.15",
          "form": "이런날 있자나용?ㅋㅋㅋ",
          "original_form": "이런날 있자나용?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020605.1.16",
          "form": "저도 그런 경험 있어요!",
          "original_form": "저도 그런 경험 있어요!",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020605.1.17",
          "form": "그쵸?ㅋㅋㅋ",
          "original_form": "그쵸?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020605.1.18",
          "form": "그래서 저 오늘 공부할 기분 아니라고 야자안하겠다고 했어요ㅋㅋㅋㅋ 지금 생각해보니 엄청 당돌했네요",
          "original_form": "그래서 저 오늘 공부할 기분 아니라고 야자안하겠다고 했어요ㅋㅋㅋㅋ 지금 생각해보니 엄청 당돌했네요",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020605.1.19",
          "form": "야자 필수이셨어요?",
          "original_form": "야자 필수이셨어요?",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020605.1.20",
          "form": "그러게요ㅋㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020605.1.21",
          "form": "네 ㅜㅜ 저 특목고 나왔거든요..",
          "original_form": "네 ㅜㅜ 저 특목고 나왔거든요..",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020605.1.22",
          "form": "아이고ㅠㅠ 엄청 힘드셨겠네요ㅠㅠ",
          "original_form": "아이고ㅠㅠ 엄청 힘드셨겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020605.1.23",
          "form": "학원 다니는 친구들은 야자 빼주더라구요...",
          "original_form": "학원 다니는 친구들은 야자 빼주더라구요...",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020605.1.24",
          "form": "전 학원 안다녀서 야자를 매일 ㅜㅜ",
          "original_form": "전 학원 안다녀서 야자를 매일 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020605.1.25",
          "form": "헉 학원은 빼줬군요ㅠㅠ",
          "original_form": "헉 학원은 빼줬군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:32"
        }
      ]
    }
  ]
