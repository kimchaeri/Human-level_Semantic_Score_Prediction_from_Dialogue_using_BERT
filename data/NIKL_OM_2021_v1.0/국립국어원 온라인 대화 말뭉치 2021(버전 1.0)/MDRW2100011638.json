      "utterance": [
        {
          "id": "MDRW2100011638.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011638.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011638.1.3",
          "form": "최근에 보신 영화 있으세요?",
          "original_form": "최근에 보신 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011638.1.4",
          "form": "블랙위도우 봤어요!",
          "original_form": "블랙위도우 봤어요!",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011638.1.5",
          "form": "평소에도 마블영화 종종 보셨어용?",
          "original_form": "평소에도 마블영화 종종 보셨어용?",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011638.1.6",
          "form": "마블은 다 챙겨보는 편이에요ㅎㅎ",
          "original_form": "마블은 다 챙겨보는 편이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011638.1.7",
          "form": "아항ㅋㅋ",
          "original_form": "아항ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011638.1.8",
          "form": "저도 최근에 영화 볼일이 많아서",
          "original_form": "저도 최근에 영화 볼일이 많아서",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011638.1.9",
          "form": "블위도 보고 모가디슈도 봤는데",
          "original_form": "블위도 보고 모가디슈도 봤는데",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011638.1.10",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011638.1.11",
          "form": "오저도 모가디슈도 봤어요ㅎㅎ",
          "original_form": "오저도 모가디슈도 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011638.1.12",
          "form": "코로나 영향 있긴한데",
          "original_form": "코로나 영향 있긴한데",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011638.1.13",
          "form": "재밌는 영화는 그래도 바로 다들 알아보는거같아요",
          "original_form": "재밌는 영화는 그래도 바로 다들 알아보는거같아요",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011638.1.14",
          "form": "간만에 둘다 넘 재밌어서",
          "original_form": "간만에 둘다 넘 재밌어서",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011638.1.15",
          "form": "모가디슈는 두번이나 봤네요ㅎㅎ",
          "original_form": "모가디슈는 두번이나 봤네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011638.1.16",
          "form": "조인성 너무 멋있어요",
          "original_form": "조인성 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011638.1.17",
          "form": "저는 그 북한 태준기?",
          "original_form": "저는 그 북한 태준기?",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011638.1.18",
          "form": "역할하시는분",
          "original_form": "역할하시는분",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011638.1.19",
          "form": "눈에 확 들어오더라구요ㅋㅋ",
          "original_form": "눈에 확 들어오더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011638.1.20",
          "form": "조인성은 진짜ㅠㅠ 나이도 안먹고",
          "original_form": "조인성은 진짜ㅠㅠ 나이도 안먹고",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011638.1.21",
          "form": "나만 먹네..",
          "original_form": "나만 먹네..",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011638.1.22",
          "form": "태준기 역 배우 요즘 많이 나오더라구요",
          "original_form": "태준기 역 배우 요즘 많이 나오더라구요",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011638.1.23",
          "form": "맞아요 뭔가 이제 막 뜨려고 그러나봐요",
          "original_form": "맞아요 뭔가 이제 막 뜨려고 그러나봐요",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011638.1.24",
          "form": "영화에 연기 구멍 없어서 좋았어요",
          "original_form": "영화에 연기 구멍 없어서 좋았어요",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011638.1.25",
          "form": "맞아요 영화 긴장감도있고",
          "original_form": "맞아요 영화 긴장감도있고",
          "speaker_id": "1",
          "time": "20210824 14:57"
        }
      ]
    }
