      "utterance": [
        {
          "id": "MDRW2100029590.1.1",
          "form": "드롭돔해보셨나요",
          "original_form": "드롭돔해보셨나요",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029590.1.2",
          "form": "심즈도 구경만했는데",
          "original_form": "심즈도 구경만했는데",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029590.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029590.1.4",
          "form": "드롭돔 처음들어봅니다",
          "original_form": "드롭돔 처음들어봅니다",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.5",
          "form": "이제스팀깔아서",
          "original_form": "이제스팀깔아서",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.6",
          "form": "사면 해볼수있어요",
          "original_form": "사면 해볼수있어요",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.7",
          "form": "스팀이 뭐죠",
          "original_form": "스팀이 뭐죠",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.8",
          "form": "컴터에 까는건데",
          "original_form": "컴터에 까는건데",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.9",
          "form": "스팀에 여러게임이 있여ㅓ",
          "original_form": "스팀에 여러게임이 있여ㅓ",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.10",
          "form": "욜 전문가 느낌납니다",
          "original_form": "욜 전문가 느낌납니다",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.11",
          "form": "아이디하나 만들어놓으면",
          "original_form": "아이디하나 만들어놓으면",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.12",
          "form": "게임으로 흥한자",
          "original_form": "게임으로 흥한자",
          "speaker_id": "1",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.13",
          "form": "구매해서 다양하게 즐길ㅅ있어요",
          "original_form": "구매해서 다양하게 즐길ㅅ있어요",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100029590.1.14",
          "form": "철권 세일할때 샀어요",
          "original_form": "철권 세일할때 샀어요",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029590.1.15",
          "form": "좋네요 별게다있군요",
          "original_form": "좋네요 별게다있군요",
          "speaker_id": "1",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029590.1.16",
          "form": "국적바꿔서",
          "original_form": "국적바꿔서",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029590.1.17",
          "form": "아? 왜 국적 바꿔서 사셨죠",
          "original_form": "아? 왜 국적 바꿔서 사셨죠",
          "speaker_id": "1",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029590.1.18",
          "form": "나라별로 가격을 다르게팔아서 ㅋㅋ 국적 바꿨더니 좀 싸더라고요",
          "original_form": "나라별로 가격을 다르게팔아서 ㅋㅋ 국적 바꿨더니 좀 싸더라고요",
          "speaker_id": "2",
          "time": "20210903 10:45"
        }
      ]
