      "utterance": [
        {
          "id": "MDRW2100010501.1.1",
          "form": "지금 비 엄청오죠!",
          "original_form": "지금 비 엄청오죠!",
          "speaker_id": "1",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010501.1.2",
          "form": "으으 쏟아집니다",
          "original_form": "으으 쏟아집니다",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010501.1.3",
          "form": "태풍이랑 장마라고 그러던데",
          "original_form": "태풍이랑 장마라고 그러던데",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010501.1.4",
          "form": "하늘에 구멍뚫린 것 같아요",
          "original_form": "하늘에 구멍뚫린 것 같아요",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010501.1.5",
          "form": "근데 오늘이 그 제일 더운 날이 지나가는 날이래요",
          "original_form": "근데 오늘이 그 제일 더운 날이 지나가는 날이래요",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010501.1.6",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010501.1.7",
          "form": "뭔가 이거오면 진짜 가을이 올것같은 느낌?",
          "original_form": "뭔가 이거오면 진짜 가을이 올것같은 느낌?",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010501.1.8",
          "form": "금방 또 추워질까 걱정이네요",
          "original_form": "금방 또 추워질까 걱정이네요",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010501.1.9",
          "form": "맞아요 오늘이",
          "original_form": "맞아요 오늘이",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010501.1.10",
          "form": "처서라고 제일 더운 날이 가는 날이래요 이제 가을 시작된거죠",
          "original_form": "처서라고 제일 더운 날이 가는 날이래요 이제 가을 시작된거죠",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010501.1.11",
          "form": "가을 옷 미리 구매하셨나요?",
          "original_form": "가을 옷 미리 구매하셨나요?",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010501.1.12",
          "form": "긴팔로 슬슬 바꿔야 될 때인 것 같죠",
          "original_form": "긴팔로 슬슬 바꿔야 될 때인 것 같죠",
          "speaker_id": "2",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010501.1.13",
          "form": "작년에 입던거 꺼내입어야죠 ㅋㅋ",
          "original_form": "작년에 입던거 꺼내입어야죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:03"
        },
        {
          "id": "MDRW2100010501.1.14",
          "form": "코로나 시작되고나서는 옷 소비가 팍줄어서",
          "original_form": "코로나 시작되고나서는 옷 소비가 팍줄어서",
          "speaker_id": "1",
          "time": "20210823 21:03"
        },
        {
          "id": "MDRW2100010501.1.15",
          "form": "후드티 사랑",
          "original_form": "후드티 사랑{emoji:♡}",
          "speaker_id": "1",
          "time": "20210823 21:04"
        },
        {
          "id": "MDRW2100010501.1.16",
          "form": "후드티 최고죠",
          "original_form": "후드티 최고죠{emoji:♡}",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010501.1.17",
          "form": "저도 요즘 걍..운동복만 입고 다니는 것 같아요",
          "original_form": "저도 요즘 걍..운동복만 입고 다니는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010501.1.18",
          "form": "그러다가 추워지면 바람막이 하나 입고",
          "original_form": "그러다가 추워지면 바람막이 하나 입고",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010501.1.19",
          "form": "잡스마냥 똑같은 옷만 입고다닙니닼ㅋㅋ",
          "original_form": "잡스마냥 똑같은 옷만 입고다닙니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010501.1.20",
          "form": "편한게 제일이죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "편한게 제일이죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010501.1.21",
          "form": "저는 여름에 검정티 검정바지 고정이잖아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 여름에 검정티 검정바지 고정이잖아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010501.1.22",
          "form": "헉 맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010501.1.23",
          "form": "겨울 가을되면 그 위에 후드티 색만 바껴욬ㅋㅋㅋㅋ",
          "original_form": "겨울 가을되면 그 위에 후드티 색만 바껴욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010501.1.24",
          "form": "아 진짜로 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜로 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100010501.1.25",
          "form": "이제 롱패딩 나오기 시작하면",
          "original_form": "이제 롱패딩 나오기 시작하면",
          "speaker_id": "1",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100010501.1.26",
          "form": "맨날 김밥으롶사는거죠 뭐",
          "original_form": "맨날 김밥으롶사는거죠 뭐",
          "speaker_id": "1",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100010501.1.27",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ 가을 길었으면 좋겠는데 태풍만 길게 생겼네요ㅜㅜ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ 가을 길었으면 좋겠는데 태풍만 길게 생겼네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100010501.1.28",
          "form": "지금 여기는 비그쳤는데",
          "original_form": "지금 여기는 비그쳤는데",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100010501.1.29",
          "form": "그쪽은 많이오고있나요?!",
          "original_form": "그쪽은 많이오고있나요?!",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100010501.1.30",
          "form": "헉 여기도 그치기는 했어요",
          "original_form": "헉 여기도 그치기는 했어요",
          "speaker_id": "2",
          "time": "20210824 01:27"
        },
        {
          "id": "MDRW2100010501.1.31",
          "form": "바람만 부네요!!!",
          "original_form": "바람만 부네요!!!",
          "speaker_id": "2",
          "time": "20210824 01:27"
        },
        {
          "id": "MDRW2100010501.1.32",
          "form": "근데 비오는 날 밤 좋지 않나요",
          "original_form": "근데 비오는 날 밤 좋지 않나요",
          "speaker_id": "2",
          "time": "20210824 01:28"
        },
        {
          "id": "MDRW2100010501.1.33",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 03:04"
        },
        {
          "id": "MDRW2100010501.1.34",
          "form": "일단 밖이아니라서 행복하게 빗소리를 들을 수 있어요",
          "original_form": "일단 밖이아니라서 행복하게 빗소리를 들을 수 있어요",
          "speaker_id": "1",
          "time": "20210824 03:04"
        },
        {
          "id": "MDRW2100010501.1.35",
          "form": "책읽을때 최곤거같아요",
          "original_form": "책읽을때 최곤거같아요",
          "speaker_id": "1",
