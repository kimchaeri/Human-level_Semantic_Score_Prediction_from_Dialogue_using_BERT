      "utterance": [
        {
          "id": "MDRW2100045353.1.1",
          "form": "좋아하는 과일 있으신가요",
          "original_form": "좋아하는 과일 있으신가요",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045353.1.2",
          "form": "일단 여름은 수박",
          "original_form": "일단 여름은 수박",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.3",
          "form": "이번 수박 맛있더라구요",
          "original_form": "이번 수박 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.5",
          "form": "저는 여름에는 복숭아가 좋던데",
          "original_form": "저는 여름에는 복숭아가 좋던데",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.6",
          "form": "저는 상큼한 복숭아 또 좋고 살구",
          "original_form": "저는 상큼한 복숭아 또 좋고 살구",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.7",
          "form": "복숭아 근데 달달한 게 자라 달달한 게 잘 없더라고요",
          "original_form": "복숭아 근데 달달한 게 자라 달달한 게 잘 없더라고요",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.8",
          "form": "비가 많이 와서 그런가",
          "original_form": "비가 많이 와서 그런가",
          "speaker_id": "1",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045353.1.9",
          "form": "아 봄에는 딸기도 좋아요",
          "original_form": "아 봄에는 딸기도 좋아요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045353.1.10",
          "form": "딸기는 비싸서 잘못 먹었어요",
          "original_form": "딸기는 비싸서 잘못 먹었어요",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045353.1.11",
          "form": "요즘은 메론이 좋대요",
          "original_form": "요즘은 메론이 좋대요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045353.1.12",
          "form": "메론도 좀 비싸던데",
          "original_form": "메론도 좀 비싸던데",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045353.1.13",
          "form": "글켄해요",
          "original_form": "글켄해요",
          "speaker_id": "2",
