      "utterance": [
        {
          "id": "MDRW2100011129.1.1",
          "form": "여행 어디 다녀오셨나요?",
          "original_form": "여행 어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210824 01:29"
        },
        {
          "id": "MDRW2100011129.1.2",
          "form": "저는 제주 여행 좋아해요!",
          "original_form": "저는 제주 여행 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 01:35"
        },
        {
          "id": "MDRW2100011129.1.3",
          "form": "저도 넘가고싶어요 제주도ㅠㅠ",
          "original_form": "저도 넘가고싶어요 제주도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011129.1.4",
          "form": "올해다녀오셨어요?",
          "original_form": "올해다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210824 01:36"
        },
        {
          "id": "MDRW2100011129.1.5",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011129.1.6",
          "form": "섭지코지 진짜 추천합니다",
          "original_form": "섭지코지 진짜 추천합니다",
          "speaker_id": "1",
          "time": "20210824 01:37"
        },
        {
          "id": "MDRW2100011129.1.7",
          "form": "6월쯤 가려다 못갔거든요",
          "original_form": "6월쯤 가려다 못갔거든요",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011129.1.8",
          "form": "친구랑 날짜안맞아서",
          "original_form": "친구랑 날짜안맞아서",
          "speaker_id": "2",
          "time": "20210824 01:38"
        },
        {
          "id": "MDRW2100011129.1.9",
          "form": "그런데 지금은 제주도 4단계라ㅠㅠ",
          "original_form": "그런데 지금은 제주도 4단계라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011129.1.10",
          "form": "네 그래서 올해는 가기 글렀다 생각중이요",
          "original_form": "네 그래서 올해는 가기 글렀다 생각중이요",
          "speaker_id": "2",
          "time": "20210824 01:40"
        },
        {
          "id": "MDRW2100011129.1.11",
          "form": "작년부터 아무데도 못갔어요 넘 우울ㅠㅠ",
          "original_form": "작년부터 아무데도 못갔어요 넘 우울ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:40"
        },
        {
          "id": "MDRW2100011129.1.12",
          "form": "하진짜 해외는 언제 나갈수있을지",
          "original_form": "하진짜 해외는 언제 나갈수있을지",
          "speaker_id": "1",
          "time": "20210824 01:42"
        },
        {
          "id": "MDRW2100011129.1.13",
          "form": "그니까요 매년  한번나가는게 낙이였거든요",
          "original_form": "그니까요 매년  한번나가는게 낙이였거든요",
          "speaker_id": "2",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011129.1.14",
          "form": "어디어디 가보셨어요?",
          "original_form": "어디어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011129.1.15",
          "form": "마지막여행지는 다낭이요",
          "original_form": "마지막여행지는 다낭이요",
          "speaker_id": "2",
          "time": "20210824 01:44"
        },
        {
          "id": "MDRW2100011129.1.16",
          "form": "name1님도 많이가보셨어요?",
          "original_form": "&name1&님도 많이가보셨어요?",
          "speaker_id": "2",
          "time": "20210824 01:45"
        },
        {
          "id": "MDRW2100011129.1.17",
          "form": "오 다낭 저는 세부요!",
          "original_form": "오 다낭 저는 세부요!",
          "speaker_id": "1",
          "time": "20210824 01:45"
        }
      ]
