      "utterance": [
        {
          "id": "MDRW2100007560.1.1",
          "form": "저는 약간",
          "original_form": "저는 약간",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007560.1.2",
          "form": "동생이 아빠 부르거든요",
          "original_form": "동생이 아빠 부르거든요",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007560.1.3",
          "form": "그럼 앞에서 혼나고 뒤에서 또 때렸어요",
          "original_form": "그럼 앞에서 혼나고 뒤에서 또 때렸어요",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007560.1.4",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007560.1.5",
          "form": "누나는. 지지않아.",
          "original_form": "누나는. 지지않아.",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.6",
          "form": "니가 일러?",
          "original_form": "니가 일러?",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.8",
          "form": "두고보자 자식아",
          "original_form": "두고보자 자식아",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.9",
          "form": "근데 남동생은 좀",
          "original_form": "근데 남동생은 좀",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.10",
          "form": "맞을만해요",
          "original_form": "맞을만해요",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.12",
          "form": "좀",
          "original_form": "좀",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.13",
          "form": "제가 그나마 때려서 저정도지",
          "original_form": "제가 그나마 때려서 저정도지",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.14",
          "form": "뭐랄까 자매와는 다른",
          "original_form": "뭐랄까 자매와는 다른",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.15",
          "form": "그마저도 안했음",
          "original_form": "그마저도 안했음",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.16",
          "form": "진짜 사람 만들어 준거죠",
          "original_form": "진짜 사람 만들어 준거죠",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.17",
          "form": "진짜 망나니됐을듯",
          "original_form": "진짜 망나니됐을듯",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007560.1.18",
          "form": "여동생이었음 안때렸을거에요",
          "original_form": "여동생이었음 안때렸을거에요",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007560.1.19",
          "form": "진짜 집안일도 안하고 맨날 도망가고",
          "original_form": "진짜 집안일도 안하고 맨날 도망가고",
          "speaker_id": "1",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007560.1.20",
          "form": "이러는데 어찌..참나요ㅎ",
          "original_form": "이러는데 어찌..참나요ㅎ",
          "speaker_id": "1",
          "time": "20210822 03:17"
        }
      ]
