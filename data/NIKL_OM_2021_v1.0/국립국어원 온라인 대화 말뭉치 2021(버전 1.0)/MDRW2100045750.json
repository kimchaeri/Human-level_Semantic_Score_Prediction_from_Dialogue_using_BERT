      "utterance": [
        {
          "id": "MDRW2100045750.1.1",
          "form": "안녕하세요, 떡볶이 좋아하시나요?",
          "original_form": "안녕하세요, 떡볶이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045750.1.2",
          "form": "안녕하세요~ 너무 좋아해요",
          "original_form": "안녕하세요~ 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045750.1.3",
          "form": "저도 너무 좋아해요ㅠㅠ",
          "original_form": "저도 너무 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045750.1.4",
          "form": "즉떡에 치즈밥 너무 맛있죠",
          "original_form": "즉떡에 치즈밥 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045750.1.5",
          "form": "진짜 떡볶이 중 최고는 즉떡인 것 닽아요",
          "original_form": "진짜 떡볶이 중 최고는 즉떡인 것 닽아요",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045750.1.6",
          "form": "로제도 드셔보셨나요?",
          "original_form": "로제도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045750.1.7",
          "form": "로ㅈㅔ는 걸작에서만요",
          "original_form": "로ㅈㅔ는 걸작에서만요",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045750.1.8",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045750.1.9",
          "form": "네 저는 배떡에서요!",
          "original_form": "네 저는 배떡에서요!",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045750.1.10",
          "form": "오 근데 걸작 차돌떡볶이도 맛있어요!",
          "original_form": "오 근데 걸작 차돌떡볶이도 맛있어요!",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045750.1.11",
          "form": "오 배떡 맛있나요?",
          "original_form": "오 배떡 맛있나요?",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045750.1.12",
          "form": "네 맛있어요ㅠㅠㅠ",
          "original_form": "네 맛있어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100045750.1.13",
          "form": "배떳떡에 인기가 많더라구요!",
          "original_form": "배떳떡에 인기가 많더라구요!",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045750.1.14",
          "form": "저도 이슈가 되어서 알았어요ㅋㅋ",
          "original_form": "저도 이슈가 되어서 알았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045750.1.15",
          "form": "그래도 시장 떡볶이도 익숙한 맛이라 좋아해요",
          "original_form": "그래도 시장 떡볶이도 익숙한 맛이라 좋아해요",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100045750.1.16",
          "form": "헉 시장떡볶이도 맛있죠",
          "original_form": "헉 시장떡볶이도 맛있죠",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045750.1.17",
          "form": "그 특유의 달달한 맛",
          "original_form": "그 특유의 달달한 맛",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045750.1.18",
          "form": "엽떡 이후로 배달떡볶이맛만 늘어나고 시장떡볶이 점유율이 적어져서 아쉬워요",
          "original_form": "엽떡 이후로 배달떡볶이맛만 늘어나고 시장떡볶이 점유율이 적어져서 아쉬워요",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045750.1.19",
          "form": "맞아요 진짜ㅠㅠㅠ",
          "original_form": "맞아요 진짜ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045750.1.20",
          "form": "다 맛이 비슷해졌어요ㅜㅜ",
          "original_form": "다 맛이 비슷해졌어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 07:40"
        }
