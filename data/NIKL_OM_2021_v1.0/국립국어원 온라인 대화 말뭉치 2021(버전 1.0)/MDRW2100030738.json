      "utterance": [
        {
          "id": "MDRW2100030738.1.1",
          "form": "시간에 중간에 맞춰서 거야 해서 괜찮아요",
          "original_form": "시간에 중간에 맞춰서 거야 해서 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030738.1.2",
          "form": "세탁기는 통돌이로 사려고요",
          "original_form": "세탁기는 통돌이로 사려고요",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030738.1.3",
          "form": "귀찮아요",
          "original_form": "귀찮아요",
          "speaker_id": "2",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030738.1.4",
          "form": "티비는 안 살까 싶어요",
          "original_form": "티비는 안 살까 싶어요",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030738.1.5",
          "form": "저도 통돌이가 괜찮은 것 같네요",
          "original_form": "저도 통돌이가 괜찮은 것 같네요",
          "speaker_id": "2",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030738.1.6",
          "form": "노트북으로 보면 되니까",
          "original_form": "노트북으로 보면 되니까",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030738.1.7",
          "form": "통돌이 해야 이불이 들어가요",
          "original_form": "통돌이 해야 이불이 들어가요",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.8",
          "form": "큰 화면으로 보는 거랑 느낌이 다른데 괜찮겠어요",
          "original_form": "큰 화면으로 보는 거랑 느낌이 다른데 괜찮겠어요",
          "speaker_id": "2",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.9",
          "form": "빔 프로젝트는 어떨까요",
          "original_form": "빔 프로젝트는 어떨까요",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.10",
          "form": "더 크게 크게",
          "original_form": "더 크게 크게",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.11",
          "form": "그건 괜찮은 생각이네요",
          "original_form": "그건 괜찮은 생각이네요",
          "speaker_id": "2",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.12",
          "form": "일단 집을 구하는게 먼저 겠네요",
          "original_form": "일단 집을 구하는게 먼저 겠네요",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.13",
          "form": "집도 없는데",
          "original_form": "집도 없는데",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.14",
          "form": "굳이 TV 사지 말고 빈 프로젝트를 활용하세요",
          "original_form": "굳이 TV 사지 말고 빈 프로젝트를 활용하세요",
          "speaker_id": "2",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.15",
          "form": "그럴까요",
          "original_form": "그럴까요",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.16",
          "form": "영화 볼 때 좋겠죠",
          "original_form": "영화 볼 때 좋겠죠",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.17",
          "form": "집을 쇼핑을 해야 하는 건요",
          "original_form": "집을 쇼핑을 해야 하는 건요",
          "speaker_id": "2",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.18",
          "form": "그게 가장 큰 숙제입니다",
          "original_form": "그게 가장 큰 숙제입니다",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030738.1.19",
          "form": "쉬고 싶으면 얘기하세요",
          "original_form": "쉬고 싶으면 얘기하세요",
          "speaker_id": "1",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030738.1.20",
          "form": "침이 마르도록 이야기 하네요",
          "original_form": "침이 마르도록 이야기 하네요",
          "speaker_id": "1",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030738.1.21",
          "form": "캬캬캬",
          "original_form": "캬캬캬",
          "speaker_id": "1",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030738.1.22",
