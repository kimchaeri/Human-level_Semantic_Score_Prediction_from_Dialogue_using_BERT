          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005459.1.13",
          "form": "인턴중에 창업 한거였나",
          "original_form": "인턴중에 창업 한거였나",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005459.1.14",
          "form": "왜 이 얘기는 새삼스럽지",
          "original_form": "왜 이 얘기는 새삼스럽지",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005459.1.15",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.16",
          "form": "여튼 아이스크림 가게는",
          "original_form": "여튼 아이스크림 가게는",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.17",
          "form": "음...교훈",
          "original_form": "음...교훈",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.18",
          "form": "자영업은 아니다",
          "original_form": "자영업은 아니다",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.19",
          "form": "일단 나는 아니다",
          "original_form": "일단 나는 아니다",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.20",
          "form": "나는 사무직 인간이다",
          "original_form": "나는 사무직 인간이다",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.21",
          "form": "큰 깨달음을 얻었지",
          "original_form": "큰 깨달음을 얻었지",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005459.1.22",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005459.1.23",
          "form": "근데 자영업은",
          "original_form": "근데 자영업은",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005459.1.24",
          "form": "아니다싶으면 빨리 손 떼는게 맞어...",
          "original_form": "아니다싶으면 빨리 손 떼는게 맞어...",
