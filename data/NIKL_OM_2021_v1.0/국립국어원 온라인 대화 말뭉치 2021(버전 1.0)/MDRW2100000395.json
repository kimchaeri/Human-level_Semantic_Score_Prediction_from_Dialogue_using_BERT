      "utterance": [
        {
          "id": "MDRW2100000395.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000395.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000395.1.3",
          "form": "요즘 여행못가서 답답하죵",
          "original_form": "요즘 여행못가서 답답하죵",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000395.1.4",
          "form": "네~맞아요 여름 휴가 못가셨나요?",
          "original_form": "네~맞아요 여름 휴가 못가셨나요?",
          "speaker_id": "1",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000395.1.5",
          "form": "천안에 잠깐 다녀왓어요",
          "original_form": "천안에 잠깐 다녀왓어요",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000395.1.6",
          "form": "아~! 천안 어떤가요?",
          "original_form": "아~! 천안 어떤가요?",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000395.1.7",
          "form": "천안 독립기념관 아세요?",
          "original_form": "천안 독립기념관 아세요?",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000395.1.8",
          "form": "아~들어봤어요",
          "original_form": "아~들어봤어요",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000395.1.9",
          "form": "ㅋ거기 산속이라서 좋아요",
          "original_form": "ㅋ거기 산속이라서 좋아요",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000395.1.10",
          "form": "역사적인것도 보구",
          "original_form": "역사적인것도 보구",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000395.1.11",
          "form": "아~~한번 가봐야 겠어요",
          "original_form": "아~~한번 가봐야 겠어요",
          "speaker_id": "1",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000395.1.12",
          "form": "네~~!!교육적으로",
          "original_form": "네~~!!교육적으로",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000395.1.13",
          "form": "학교에서도 많이 온다고 하니",
          "original_form": "학교에서도 많이 온다고 하니",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000395.1.14",
          "form": "천안 호두과자두 맛나고",
          "original_form": "천안 호두과자두 맛나고",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000395.1.15",
          "form": "아 그렇군요~ 의미있는 여행이 되겠어요",
          "original_form": "아 그렇군요~ 의미있는 여행이 되겠어요",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000395.1.16",
          "form": "예전에 다녀온 여행지가있으신가요?",
          "original_form": "예전에 다녀온 여행지가있으신가요?",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000395.1.17",
          "form": "저는 코로나땜에 잘 못갔어요ㅠ",
          "original_form": "저는 코로나땜에 잘 못갔어요ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000395.1.18",
          "form": "코로나 전에도 없으세요",
          "original_form": "코로나 전에도 없으세요",
          "speaker_id": "2",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000395.1.19",
          "form": "그전에는 제주도요",
          "original_form": "그전에는 제주도요",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000395.1.20",
          "form": "제주도 좋아요",
          "original_form": "제주도 좋아요",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000395.1.21",
          "form": "렌트해서 다녀보니 좋아요",
          "original_form": "렌트해서 다녀보니 좋아요",
          "speaker_id": "2",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000395.1.22",
          "form": "네~너무 좋아요 ㅠㅠ",
          "original_form": "네~너무 좋아요 ㅠㅠ",
