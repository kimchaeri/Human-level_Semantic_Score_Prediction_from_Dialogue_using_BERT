      "utterance": [
        {
          "id": "MDRW2100040844.1.1",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100040844.1.2",
          "form": "아직이요 name1님은 드셨나요?",
          "original_form": "아직이요 &name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100040844.1.3",
          "form": "네 방금 먹었어요",
          "original_form": "네 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100040844.1.4",
          "form": "저녁식사 뭐 드실 예정이예요?",
          "original_form": "저녁식사 뭐 드실 예정이예요?",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100040844.1.5",
          "form": "저는 아직 모르겠어요",
          "original_form": "저는 아직 모르겠어요",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100040844.1.6",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100040844.1.7",
          "form": "저는 저녁은 코울슬로 샐러드 간단히 먹었어요",
          "original_form": "저는 저녁은 코울슬로 샐러드 간단히 먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100040844.1.8",
          "form": "다이어트 중이신가요?",
          "original_form": "다이어트 중이신가요?",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100040844.1.9",
          "form": "며칠 뒤에 사진 찍을 일이 있어서요",
          "original_form": "며칠 뒤에 사진 찍을 일이 있어서요",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100040844.1.10",
          "form": "근데 주말동안 너무 잘먹었어요",
          "original_form": "근데 주말동안 너무 잘먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100040844.1.11",
          "form": "그럼 물을 줄이셔야겠네요",
          "original_form": "그럼 물을 줄이셔야겠네요",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100040844.1.12",
          "form": "아 물을 적게 마셔야 할까요?",
          "original_form": "아 물을 적게 마셔야 할까요?",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100040844.1.13",
          "form": "자극적인 음식만 피하려고 했는데",
          "original_form": "자극적인 음식만 피하려고 했는데",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100040844.1.14",
          "form": "아니 물을 마시면 얼굴이 부어서 중요한 날 전날 밤에는 물을 안마신다는 얘기가 있잖아요",
          "original_form": "아니 물을 마시면 얼굴이 부어서 중요한 날 전날 밤에는 물을 안마신다는 얘기가 있잖아요",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100040844.1.15",
          "form": "자극적인 음식을 피하는게 가장 좋지만요",
          "original_form": "자극적인 음식을 피하는게 가장 좋지만요",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100040844.1.16",
          "form": "아아 그쵸",
          "original_form": "아아 그쵸",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100040844.1.17",
          "form": "주말동안 매운음식 당겨서 먹었더니",
          "original_form": "주말동안 매운음식 당겨서 먹었더니",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100040844.1.18",
          "form": "확실히 오늘 얼굴이랑 몸이 붓더라고요",
          "original_form": "확실히 오늘 얼굴이랑 몸이 붓더라고요",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100040844.1.19",
          "form": "맞아요 촬영있으시면 급하게 살을 빼기 보다는 붓기관리를 잘하셔야겠어요",
          "original_form": "맞아요 촬영있으시면 급하게 살을 빼기 보다는 붓기관리를 잘하셔야겠어요",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100040844.1.20",
          "form": "네 그래서 며칠동안은 슴슴한 간의 음식을 먹으려고요",
          "original_form": "네 그래서 며칠동안은 슴슴한 간의 음식을 먹으려고요",
          "speaker_id": "1",
          "time": "20210906 19:27"
        }
      ]
