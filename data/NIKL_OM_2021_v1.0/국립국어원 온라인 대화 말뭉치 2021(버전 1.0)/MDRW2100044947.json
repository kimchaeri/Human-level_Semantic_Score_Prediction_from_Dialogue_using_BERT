        {
          "id": "MDRW2100044947.1.1",
          "form": "저도 환장해요",
          "original_form": "저도 환장해요",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044947.1.2",
          "form": "무슨 초밥 좋아하시나요",
          "original_form": "무슨 초밥 좋아하시나요",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044947.1.3",
          "form": "연어 광어 특히 환장합니다",
          "original_form": "연어 광어 특히 환장합니다",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044947.1.4",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044947.1.5",
          "form": "헐 저도 연어 광어",
          "original_form": "헐 저도 연어 광어",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044947.1.6",
          "form": "두개가 최고에요",
          "original_form": "두개가 최고에요",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044947.1.7",
          "form": "name2님 저랑 잘 맞으시네요?",
          "original_form": "&name2&님 저랑 잘 맞으시네요?",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044947.1.8",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044947.1.9",
          "form": "연어 홀스래디쉬에 먹는거 좋아하세요?",
          "original_form": "연어 홀스래디쉬에 먹는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044947.1.10",
          "form": "그 하얀 소스가 그건가요?",
          "original_form": "그 하얀 소스가 그건가요?",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044947.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044947.1.12",
          "form": "음 좋아하는건 아닌데 그래도 잘 먹어요 ㅋㅋㅋㅋ",
          "original_form": "음 좋아하는건 아닌데 그래도 잘 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044947.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044947.1.14",
          "form": "저도ㅎㅎㅎ",
          "original_form": "저도ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044947.1.15",
          "form": "연어는 와사비 넣은 간장이 최곤거 같아요",
          "original_form": "연어는 와사비 넣은 간장이 최곤거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044947.1.16",
          "form": "꺄 저도요",
          "original_form": "꺄 저도요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044947.1.17",
          "form": "근데 가끔 초장 찍어드시는 분들도 있더라구요",
          "original_form": "근데 가끔 초장 찍어드시는 분들도 있더라구요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044947.1.18",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044947.1.19",
          "form": "초장 찍으면 연어 맛이 사라지던데ㅠㅠㅠ",
          "original_form": "초장 찍으면 연어 맛이 사라지던데ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044947.1.20",
          "form": "그니까요 초장맛밖에 안나요",
          "original_form": "그니까요 초장맛밖에 안나요",
