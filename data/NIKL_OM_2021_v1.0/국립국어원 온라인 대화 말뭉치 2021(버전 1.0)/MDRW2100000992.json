      "utterance": [
        {
          "id": "MDRW2100000992.1.1",
          "form": "오늘 날씨가 가을같아요",
          "original_form": "오늘 날씨가 가을같아요",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000992.1.2",
          "form": "완연한 가을이에요\r\n볕은 따갑지만 \r\n시원하네요",
          "original_form": "완연한 가을이에요\r\n볕은 따갑지만 \r\n시원하네요",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000992.1.3",
          "form": "그러게요 밤되면 살짝 선선하면서도 서늘하지만",
          "original_form": "그러게요 밤되면 살짝 선선하면서도 서늘하지만",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000992.1.4",
          "form": "새벽엔 춥더라구요",
          "original_form": "새벽엔 춥더라구요",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000992.1.5",
          "form": "그것또한 여름에서 가을로 넘어가는 매력인거같아요",
          "original_form": "그것또한 여름에서 가을로 넘어가는 매력인거같아요",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000992.1.6",
          "form": "감기조심해야해요",
          "original_form": "감기조심해야해요",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000992.1.7",
          "form": "가을이 짧아서 지금 누려야해요 ㅎ",
          "original_form": "가을이 짧아서 지금 누려야해요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000992.1.8",
          "form": "맞아요 가을넘어갈때 감기들기 쉽상이더라구요",
          "original_form": "맞아요 가을넘어갈때 감기들기 쉽상이더라구요",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000992.1.9",
          "form": "지금 하늘 보니까 엄청 맑네요",
          "original_form": "지금 하늘 보니까 엄청 맑네요",
          "speaker_id": "1",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000992.1.10",
          "form": "빨래하기 좋은날씨다",
          "original_form": "빨래하기 좋은날씨다",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000992.1.11",
          "form": "제가 요즘 달고사는 말이에요 ㅎ",
          "original_form": "제가 요즘 달고사는 말이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000992.1.12",
          "form": "ㅋㅋㅋㅋㅋ그러네요 바람에 선선하게 마르겠어요!",
          "original_form": "ㅋㅋㅋㅋㅋ그러네요 바람에 선선하게 마르겠어요!",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000992.1.13",
          "form": "습기가 없어서 뽀송뽀송",
          "original_form": "습기가 없어서 뽀송뽀송",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000992.1.14",
          "form": "이불 뽀송뽀송한거 덮고 자면 딱이겠어요",
          "original_form": "이불 뽀송뽀송한거 덮고 자면 딱이겠어요",
          "speaker_id": "1",
          "time": "20210820 11:31"
        },
        {
          "id": "MDRW2100000992.1.15",
          "form": "맞아요 전 빨래 널고 맑은 하늘 보면 너무 좋더라구요",
          "original_form": "맞아요 전 빨래 널고 맑은 하늘 보면 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000992.1.16",
          "form": "이제 긴옷 빼야겠어요 그쵸",
          "original_form": "이제 긴옷 빼야겠어요 그쵸",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000992.1.17",
          "form": "아맞아요! 저안그래도 오늘 얇은 긴팔 입고왔어요",
          "original_form": "아맞아요! 저안그래도 오늘 얇은 긴팔 입고왔어요",
          "speaker_id": "1",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000992.1.18",
          "form": "잘했어요 낮엔 덥지만 일교차가 커서",
          "original_form": "잘했어요 낮엔 덥지만 일교차가 커서",
          "speaker_id": "2",
          "time": "20210820 11:32"
        },
        {
          "id": "MDRW2100000992.1.19",
          "form": "밤에는 쌀쌀해서 긴팔 챙겨서 다니셔야해요~!",
          "original_form": "밤에는 쌀쌀해서 긴팔 챙겨서 다니셔야해요~!",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000992.1.20",
          "form": "어디는 비온다던데 거긴 어때요?",
          "original_form": "어디는 비온다던데 거긴 어때요?",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000992.1.21",
          "form": "제가 있는곳은 하늘 되게 맑아요 청량하니",
          "original_form": "제가 있는곳은 하늘 되게 맑아요 청량하니",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000992.1.22",
          "form": "비오나요?",
          "original_form": "비오나요?",
          "speaker_id": "1",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000992.1.23",
          "form": "저 있는곳도 엄청 맑아요",
          "original_form": "저 있는곳도 엄청 맑아요",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000992.1.24",
          "form": "매미소리 환장해요 ㅎ",
          "original_form": "매미소리 환장해요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:33"
        },
        {
          "id": "MDRW2100000992.1.25",
          "form": "코로나만 아니면 떠나고 싶네요 ㅎ",
          "original_form": "코로나만 아니면 떠나고 싶네요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 11:34"
        },
        {
          "id": "MDRW2100000992.1.26",
          "form": "저두요!ㅎ.ㅎ 드라이브하면서",
          "original_form": "저두요!ㅎ.ㅎ 드라이브하면서",
          "speaker_id": "1",
          "time": "20210820 11:35"
        },
        {
          "id": "MDRW2100000992.1.27",
          "form": "이날씨를 즐기고싶어요 ㅎ.ㅎ",
          "original_form": "이날씨를 즐기고싶어요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210820 11:35"
        },
        {
          "id": "MDRW2100000992.1.28",
          "form": "맞아요 날씨 너무 좋아",
          "original_form": "맞아요 날씨 너무 좋아",
          "speaker_id": "2",
          "time": "20210820 11:35"
        },
        {
          "id": "MDRW2100000992.1.29",
          "form": "근데 곧 태풍 온담서요 ",
          "original_form": "근데 곧 태풍 온담서요 ",
          "speaker_id": "2",
          "time": "20210820 11:35"
        },
        {
          "id": "MDRW2100000992.1.30",
          "form": "가을태풍온다고 하네요ㅠㅠ윽..",
          "original_form": "가을태풍온다고 하네요ㅠㅠ윽..",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100000992.1.31",
          "form": "비오는날에는 나가면 넘 찝찝해서..",
          "original_form": "비오는날에는 나가면 넘 찝찝해서..",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100000992.1.32",
          "form": "비오는날 조아하세요?",
          "original_form": "비오는날 조아하세요?",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100000992.1.33",
          "form": "집에서 구경하는것만요",
          "original_form": "집에서 구경하는것만요",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100000992.1.34",
          "form": "돌아다니는건 질색이에요 ㅠ",
          "original_form": "돌아다니는건 질색이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100000992.1.35",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100000992.1.36",
          "form": "저두요 카페가는정도?",
          "original_form": "저두요 카페가는정도?",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100000992.1.37",
          "form": "신발젖는게 싫어서 비오는날이 더 싫은거 같아요",
          "original_form": "신발젖는게 싫어서 비오는날이 더 싫은거 같아요",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100000992.1.38",
          "form": "맞아요 찝찝해요",
          "original_form": "맞아요 찝찝해요",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100000992.1.39",
          "form": "센치해지고 ㅎ",
          "original_form": "센치해지고 ㅎ",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100000992.1.40",
          "form": "ㅎ.ㅎ..태풍이 심하게 안와야할텐데..",
          "original_form": "ㅎ.ㅎ..태풍이 심하게 안와야할텐데..",
          "speaker_id": "1",
          "time": "20210820 12:20"
        }
      ]
