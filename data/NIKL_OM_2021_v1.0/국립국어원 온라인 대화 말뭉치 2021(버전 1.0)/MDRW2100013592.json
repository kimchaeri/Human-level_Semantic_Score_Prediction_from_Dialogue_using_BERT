      "utterance": [
        {
          "id": "MDRW2100013592.1.1",
          "form": "콜라보제품을 구매해본적이 있으십니까",
          "original_form": "콜라보제품을 구매해본적이 있으십니까",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013592.1.2",
          "form": "없지만 소식은 들었어요",
          "original_form": "없지만 소식은 들었어요",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013592.1.3",
          "form": "곰표가 밀가루회사죠?",
          "original_form": "곰표가 밀가루회사죠?",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013592.1.4",
          "form": "네 밀가루회사에요",
          "original_form": "네 밀가루회사에요",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013592.1.5",
          "form": "요즘 콜라보 많이 출시하더라구오",
          "original_form": "요즘 콜라보 많이 출시하더라구오",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013592.1.6",
          "form": "맥주랑 팝콘을 봤던거 같아요ㅋㅋ",
          "original_form": "맥주랑 팝콘을 봤던거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100013592.1.7",
          "form": "저 팝콘은 먹어봤어요",
          "original_form": "저 팝콘은 먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013592.1.8",
          "form": "씨유에서 팔더라구요",
          "original_form": "씨유에서 팔더라구요",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013592.1.9",
          "form": "무슨 맛이였어요?그냥 평범한 팝콘이겠죠?",
          "original_form": "무슨 맛이였어요?그냥 평범한 팝콘이겠죠?",
          "speaker_id": "1",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013592.1.10",
          "form": "네 좀 짭짤한.",
          "original_form": "네 좀 짭짤한.",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013592.1.11",
          "form": "별 특색은 없었어요",
          "original_form": "별 특색은 없었어요",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013592.1.12",
          "form": "그 젤리도 있다던데",
          "original_form": "그 젤리도 있다던데",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013592.1.13",
          "form": "어떤분이 절대 먹지말라더라구요",
          "original_form": "어떤분이 절대 먹지말라더라구요",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013592.1.14",
          "form": "곰표 젤리요?",
          "original_form": "곰표 젤리요?",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013592.1.15",
          "form": "맛없나보다ㅋㅋㅋㅋ",
          "original_form": "맛없나보다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013592.1.16",
          "form": "그 돈을 땅바닥에 버리래요",
          "original_form": "그 돈을 땅바닥에 버리래요",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013592.1.17",
          "form": "진짜 별로였나봐요ㅋㅋ",
          "original_form": "진짜 별로였나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013592.1.18",
          "form": "젤리가 맛없기 쉽지않은데ㅋㅋㅋㅋ",
          "original_form": "젤리가 맛없기 쉽지않은데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013592.1.19",
          "form": "진짜 경악을 하면서 말리시길래",
          "original_form": "진짜 경악을 하면서 말리시길래",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013592.1.20",
          "form": "눈길도안주기로 마음먹었습니다",
          "original_form": "눈길도안주기로 마음먹었습니다",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013592.1.21",
          "form": "맞아요 누가 뜯어 말릴땐 이유가 있더라구요ㅋㅋㅋ",
          "original_form": "맞아요 누가 뜯어 말릴땐 이유가 있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:28"
