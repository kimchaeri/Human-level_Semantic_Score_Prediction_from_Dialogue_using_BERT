      "utterance": [
        {
          "id": "MDRW2100014091.1.1",
          "form": "좋아하는 음료수 있으세요?",
          "original_form": "좋아하는 음료수 있으세요?",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014091.1.2",
          "form": "토레타즐겨마시고있어요",
          "original_form": "토레타즐겨마시고있어요",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014091.1.3",
          "form": "토레타 어떤점이 맛있나요??!",
          "original_form": "토레타 어떤점이 맛있나요??!",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014091.1.4",
          "form": "수분이가득하고 과일야채랑 같이 있다고하니",
          "original_form": "수분이가득하고 과일야채랑 같이 있다고하니",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014091.1.5",
          "form": "광고보니깐좋더라구요",
          "original_form": "광고보니깐좋더라구요",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014091.1.6",
          "form": "오로나민c도좋고",
          "original_form": "오로나민c도좋고",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014091.1.7",
          "form": "주로광고보고ㅋㅋㅋㄱ",
          "original_form": "주로광고보고ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014091.1.8",
          "form": "아 ",
          "original_form": "아 ",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014091.1.9",
          "form": "저는 토레타 진짜 맛 궁금해서",
          "original_form": "저는 토레타 진짜 맛 궁금해서",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014091.1.10",
          "form": "친구 한테 달라해서 조금 먹어봤는데",
          "original_form": "친구 한테 달라해서 조금 먹어봤는데",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014091.1.11",
          "form": "맛이 오묘하더라고요",
          "original_form": "맛이 오묘하더라고요",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014091.1.12",
          "form": "수분음료라서ㅋㅋㅋㅋ",
          "original_form": "수분음료라서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014091.1.13",
          "form": "오늘 점심은 뭐드셨어요?",
          "original_form": "오늘 점심은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014091.1.14",
          "form": "저는 햄버거 먹었어요ㅎㅎ",
          "original_form": "저는 햄버거 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014091.1.15",
          "form": "무슨햄버거요?",
          "original_form": "무슨햄버거요?",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014091.1.16",
          "form": "저는 어제 버거킹 7000원행사 해서 과트로 치즈와퍼 저녁으로먹었어요",
          "original_form": "저는 어제 버거킹 7000원행사 해서 과트로 치즈와퍼 저녁으로먹었어요",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014091.1.17",
          "form": "버거킹도 맛있죠!",
          "original_form": "버거킹도 맛있죠!",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014091.1.18",
          "form": "저는 KFC꺼먹었어요",
          "original_form": "저는 KFC꺼먹었어요",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014091.1.19",
          "form": "저는 점심으로 소고기무굿먹었습니다",
          "original_form": "저는 점심으로 소고기무굿먹었습니다",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014091.1.20",
          "form": "우와 맛있었겠닥ㅋㅋㄱㅋ",
          "original_form": "우와 맛있었겠닥ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014091.1.21",
          "form": "저녁은 계획있으세요?",
