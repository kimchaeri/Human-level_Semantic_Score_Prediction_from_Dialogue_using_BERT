      "utterance": [
        {
          "id": "MDRW2100034879.1.1",
          "form": "안녕하세용 틴트랑 파운데이션 어디꺼 쓰세요?",
          "original_form": "안녕하세용 틴트랑 파운데이션 어디꺼 쓰세요?",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034879.1.2",
          "form": "틴트는 마스크때문에 안쓰고 파운데이션 대신 비비 써요. name1님은요?",
          "original_form": "틴트는 마스크때문에 안쓰고 파운데이션 대신 비비 써요. &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034879.1.3",
          "form": "비비 뭐쓰시나요? 괜찮았던거 있으세용?",
          "original_form": "비비 뭐쓰시나요? 괜찮았던거 있으세용?",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034879.1.4",
          "form": "저는 파운데이션은 헤라 블랙쿠션 쓰고",
          "original_form": "저는 파운데이션은 헤라 블랙쿠션 쓰고",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034879.1.5",
          "form": "틴트는 입생로랑 따뚜아쥬 써요!",
          "original_form": "틴트는 입생로랑 따뚜아쥬 써요!",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034879.1.6",
          "form": "저 애터미꺼 써요 가성비 좋더라고요",
          "original_form": "저 애터미꺼 써요 가성비 좋더라고요",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034879.1.7",
          "form": "거기껀 생리대 잘 썼는데",
          "original_form": "거기껀 생리대 잘 썼는데",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034879.1.8",
          "form": "비비크림도 좋나봐요",
          "original_form": "비비크림도 좋나봐요",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034879.1.9",
          "form": "오홍ㅋㅋㅋ",
          "original_form": "오홍ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034879.1.10",
          "form": "애터미에서 생리대가 나와요? 여기 기초도 괜찮거든요.",
          "original_form": "애터미에서 생리대가 나와요? 여기 기초도 괜찮거든요.",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034879.1.11",
          "form": "네네! 애터미 치약이랑 생리대랑 이거 두개 잘 썼고",
          "original_form": "네네! 애터미 치약이랑 생리대랑 이거 두개 잘 썼고",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034879.1.12",
          "form": "여기 고등어도 팔더라구요ㅋㅋㅋㅋ",
          "original_form": "여기 고등어도 팔더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034879.1.13",
          "form": "기초도 좋구나",
          "original_form": "기초도 좋구나",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034879.1.14",
          "form": "고등어가 팔아요?? 전 화장품만 파는줄 알았는데 ㅋㅋ 여기 기초가 싸거든요",
          "original_form": "고등어가 팔아요?? 전 화장품만 파는줄 알았는데 ㅋㅋ 여기 기초가 싸거든요",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034879.1.15",
          "form": "기초 혹시 순한가요? 급 땡기네요ㅋㅋㅋㅋ",
          "original_form": "기초 혹시 순한가요? 급 땡기네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034879.1.16",
          "form": "네네 여기 화장지도팔고 진짜 이것저것 다 팔아용",
          "original_form": "네네 여기 화장지도팔고 진짜 이것저것 다 팔아용",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034879.1.17",
          "form": "전 고딩때 여기 기초 처음 썼는데 좋았어요 ㅎ",
          "original_form": "전 고딩때 여기 기초 처음 썼는데 좋았어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034879.1.18",
          "form": "어머 오래쓰셨나봐용",
          "original_form": "어머 오래쓰셨나봐용",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034879.1.19",
          "form": "기초에 정착하기가 쉽지않은데!",
          "original_form": "기초에 정착하기가 쉽지않은데!",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034879.1.20",
          "form": "그거 쓰다 질려서 지금은 제이준 기초 써요",
          "original_form": "그거 쓰다 질려서 지금은 제이준 기초 써요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100034879.1.21",
          "form": "제이준 마스크팩으로 유명해서 저도 써봤는데",
          "original_form": "제이준 마스크팩으로 유명해서 저도 써봤는데",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100034879.1.22",
          "form": "대박 좋았어요ㅋㅋㅋ",
          "original_form": "대박 좋았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100034879.1.23",
          "form": "오~ 아시는군요~ 연두색 팩이 제일 좋던데요.",
          "original_form": "오~ 아시는군요~ 연두색 팩이 제일 좋던데요.",
          "speaker_id": "2",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100034879.1.24",
          "form": "연두색...메모...ㅋㅋㅋㅋ 저는 하늘색? 파란색 썼는데",
          "original_form": "연두색...메모...ㅋㅋㅋㅋ 저는 하늘색? 파란색 썼는데",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100034879.1.25",
          "form": "이것도 꼭 써볼게요!",
          "original_form": "이것도 꼭 써볼게요!",
          "speaker_id": "1",
          "time": "20210828 21:48"
        }
      ]
    }
