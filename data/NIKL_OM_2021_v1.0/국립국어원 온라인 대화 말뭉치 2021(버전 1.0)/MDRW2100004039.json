      "utterance": [
        {
          "id": "MDRW2100004039.1.1",
          "form": "언니 오늘 온데 ?",
          "original_form": "언니 오늘 온데 ?",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004039.1.2",
          "form": "전화오긴했었는데",
          "original_form": "전화오긴했었는데",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004039.1.3",
          "form": "그럼 온다는 건가",
          "original_form": "그럼 온다는 건가",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004039.1.4",
          "form": "궁금하면 물어보든가",
          "original_form": "궁금하면 물어보든가",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004039.1.5",
          "form": "아니 안할볼래 물어봤다가 오면 어떻게해",
          "original_form": "아니 안할볼래 물어봤다가 오면 어떻게해",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004039.1.6",
          "form": "오면 오는거지",
          "original_form": "오면 오는거지",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004039.1.7",
          "form": "그냥 쉴래 ㅋㅋㅋ 언니에게 물어봤다가 오라고 ? 로 알아듣고 바로 올꺼 같아서 겁나네",
          "original_form": "그냥 쉴래 ㅋㅋㅋ 언니에게 물어봤다가 오라고 ? 로 알아듣고 바로 올꺼 같아서 겁나네",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004039.1.8",
          "form": "겁이나?",
          "original_form": "겁이나?",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004039.1.9",
          "form": "오면 내방 들어갈꺼고 그럼 또 어질러 질테고",
          "original_form": "오면 내방 들어갈꺼고 그럼 또 어질러 질테고",
          "speaker_id": "1",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004039.1.10",
          "form": "거기 평소에도 어질러진 방 아니었니",
          "original_form": "거기 평소에도 어질러진 방 아니었니",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004039.1.11",
          "form": "아닌데 정리된 방인데",
          "original_form": "아닌데 정리된 방인데",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004039.1.12",
          "form": "정리된 방이라고 ?",
          "original_form": "정리된 방이라고 ?",
          "speaker_id": "2",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004039.1.13",
          "form": "나의 기준에 깨끗한 방이지  바닥에 머리카락 있는 것만 좀 빼면",
          "original_form": "나의 기준에 깨끗한 방이지  바닥에 머리카락 있는 것만 좀 빼면",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004039.1.14",
          "form": "기준이 이상하구나",
          "original_form": "기준이 이상하구나",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004039.1.15",
          "form": "이상한지 모르겠는데",
          "original_form": "이상한지 모르겠는데",
          "speaker_id": "1",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004039.1.16",
          "form": "청소좀해",
          "original_form": "청소좀해",
          "speaker_id": "2",
          "time": "20210821 14:48"
        },
        {
          "id": "MDRW2100004039.1.17",
          "form": "어 알았어",
          "original_form": "어 알았어",
          "speaker_id": "1",
          "time": "20210821 14:49"
        }
