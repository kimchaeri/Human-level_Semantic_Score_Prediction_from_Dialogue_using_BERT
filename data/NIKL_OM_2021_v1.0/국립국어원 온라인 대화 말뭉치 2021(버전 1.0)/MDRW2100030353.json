      "utterance": [
        {
          "id": "MDRW2100030353.1.1",
          "form": "음식은 삶이죠",
          "original_form": "음식은 삶이죠",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030353.1.2",
          "form": "밤샐 수 있죠 음식 이야기",
          "original_form": "밤샐 수 있죠 음식 이야기",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030353.1.3",
          "form": "음식은 진리입니다",
          "original_form": "음식은 진리입니다",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.4",
          "form": "족발 좋아하나요",
          "original_form": "족발 좋아하나요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.5",
          "form": "다 잘 먹고 살려고 돈 버는 거죠",
          "original_form": "다 잘 먹고 살려고 돈 버는 거죠",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.6",
          "form": "대신동 족발은 맛이 변했어요",
          "original_form": "대신동 족발은 맛이 변했어요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.7",
          "form": "매우 좋아합니다",
          "original_form": "매우 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.8",
          "form": "특히 불족발을 좋아해요",
          "original_form": "특히 불족발을 좋아해요",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.9",
          "form": "비싸고 느끼해요",
          "original_form": "비싸고 느끼해요",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.10",
          "form": "불족발 맛집은 어디예요",
          "original_form": "불족발 맛집은 어디예요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.11",
          "form": "부산진 족발 일하고 있는데",
          "original_form": "부산진 족발 일하고 있는데",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.12",
          "form": "너무 비싸요",
          "original_form": "너무 비싸요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.13",
          "form": "부산진 족발 일하고 있는데",
          "original_form": "부산진 족발 일하고 있는데",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.14",
          "form": "이라고있는데",
          "original_form": "이라고있는데",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.15",
          "form": "부산진 족발",
          "original_form": "부산진 족발",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.16",
          "form": "같은 양의 고기도 많이 주고 뼈를 적게 줘요",
          "original_form": "같은 양의 고기도 많이 주고 뼈를 적게 줘요",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.17",
          "form": "어디에 있나요",
          "original_form": "어디에 있나요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030353.1.18",
          "form": "고기 질도 좋구요 양도 많아요",
          "original_form": "고기 질도 좋구요 양도 많아요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.19",
          "form": "부산역에서 시켜 먹어 봐서 위치는 잘 모르겠네요",
          "original_form": "부산역에서 시켜 먹어 봐서 위치는 잘 모르겠네요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.20",
          "form": "오 정직 하네요",
          "original_form": "오 정직 하네요",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.21",
          "form": "족발은 양이 많아 야죠",
          "original_form": "족발은 양이 많아 야죠",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.22",
          "form": "정말 맛있었는데 친구가 포장으로 사봤는데 맛있었다고 하더라구요",
          "original_form": "정말 맛있었는데 친구가 포장으로 사봤는데 맛있었다고 하더라구요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.23",
          "form": "비싸니까요",
          "original_form": "비싸니까요",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.24",
          "form": "매우 만족한다고 했어요",
          "original_form": "매우 만족한다고 했어요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.25",
          "form": "부산역에서 배달 오네요",
          "original_form": "부산역에서 배달 오네요",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.26",
          "form": "족발과 같이 먹는 막국수도 정말 맛있죠",
          "original_form": "족발과 같이 먹는 막국수도 정말 맛있죠",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.27",
          "form": "저는 족발 냉채 좋아해요",
          "original_form": "저는 족발 냉채 좋아해요",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030353.1.28",
          "form": "부산역에서 시켰던 거라 부산진 해서 부산역으로 배달 온 거죠",
          "original_form": "부산역에서 시켰던 거라 부산진 해서 부산역으로 배달 온 거죠",
          "speaker_id": "1",
          "time": "20210904 00:04"
        }
      ]
    }
