          "id": "MDRW2100036740.1.1",
          "form": "name1님은 아침드셨어여?",
          "original_form": "&name1&님은 아침드셨어여?",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036740.1.2",
          "form": "복숭아 조금 먹었어요",
          "original_form": "복숭아 조금 먹었어요",
          "speaker_id": "1",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100036740.1.3",
          "form": "아요즘 복숭아맛있죠ㅠ비싸서딱복",
          "original_form": "아요즘 복숭아맛있죠ㅠ비싸서딱복",
          "speaker_id": "2",
          "time": "20210831 11:13"
        },
        {
          "id": "MDRW2100036740.1.4",
          "form": "요즘엔 딱복이 더 비싸요?",
          "original_form": "요즘엔 딱복이 더 비싸요?",
          "speaker_id": "1",
          "time": "20210831 11:14"
        },
        {
          "id": "MDRW2100036740.1.5",
          "form": "복숭아뭐든지 비싼것같아요ㅜ",
          "original_form": "복숭아뭐든지 비싼것같아요ㅜ",
          "speaker_id": "2",
          "time": "20210831 11:15"
        },
        {
          "id": "MDRW2100036740.1.6",
          "form": "올해아직 못먹었어여ㅠㅠ",
          "original_form": "올해아직 못먹었어여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 11:15"
        },
        {
          "id": "MDRW2100036740.1.7",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036740.1.8",
          "form": "자두도 맛있는데",
          "original_form": "자두도 맛있는데",
          "speaker_id": "1",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036740.1.9",
          "form": "비싸더라구요",
          "original_form": "비싸더라구요",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036740.1.10",
          "form": "지금 한참나올때라",
          "original_form": "지금 한참나올때라",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036740.1.11",
          "form": "작고비싸고",
          "original_form": "작고비싸고",
          "speaker_id": "2",
          "time": "20210831 11:19"
        },
        {
          "id": "MDRW2100036740.1.12",
