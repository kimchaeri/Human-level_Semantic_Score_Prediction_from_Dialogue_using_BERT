      "utterance": [
        {
          "id": "MDRW2100037593.1.1",
          "form": "뭐 우리 앞이라서 안 한다고는 하던데",
          "original_form": "뭐 우리 앞이라서 안 한다고는 하던데",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100037593.1.2",
          "form": "우리가 보기에는 그냥 진짜 버릇이 없어",
          "original_form": "우리가 보기에는 그냥 진짜 버릇이 없어",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100037593.1.3",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100037593.1.4",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100037593.1.5",
          "form": "여자어른말은 아예 안들어",
          "original_form": "여자어른말은 아예 안들어",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.6",
          "form": "버릇없으면 좀 그렇다 ㅠㅠ",
          "original_form": "버릇없으면 좀 그렇다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.7",
          "form": "헉 심하네",
          "original_form": "헉 심하네",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.8",
          "form": "이번에 캠핑가서도 좀 짜증났는데 \r\nname3엄마아빠가 아무말도 안하는거야",
          "original_form": "이번에 캠핑가서도 좀 짜증났는데 \r\n&name3&엄마아빠가 아무말도 안하는거야",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.9",
          "form": "지금 그렇게 오냐오냐 키우면은 더 심해질텐데 ㅠㅠ",
          "original_form": "지금 그렇게 오냐오냐 키우면은 더 심해질텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.10",
          "form": "name3가 name4 앞에서는 혼내지마라고 얘기했다면서",
          "original_form": "&name3&가 &name4& 앞에서는 혼내지마라고 얘기했다면서",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.11",
          "form": "아이고 그렇구나 ㅠㅠ",
          "original_form": "아이고 그렇구나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.12",
          "form": "우리만 맨날 name4 혼내고",
          "original_form": "우리만 맨날 &name4& 혼내고",
          "speaker_id": "2",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.13",
          "form": "그래도 터질 엄마 아빠들은 터지는데 ㅠㅠ",
          "original_form": "그래도 터질 엄마 아빠들은 터지는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037593.1.14",
          "form": "조심한다캐도 혼낼때는 혼내니깐",
          "original_form": "조심한다캐도 혼낼때는 혼내니깐",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.15",
          "form": "같이싸우면",
          "original_form": "같이싸우면",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.16",
          "form": "name4이 더 많이 혼나니까",
          "original_form": "&name4&이 더 많이 혼나니까",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.17",
          "form": "맨날 억울해한다",
          "original_form": "맨날 억울해한다",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.18",
          "form": "아고 언니입장에서는 그게 또 속상하지",
          "original_form": "아고 언니입장에서는 그게 또 속상하지",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.19",
          "form": "이제 우리도 name3랑 있을때는 화안내려고",
          "original_form": "이제 우리도 &name3&랑 있을때는 화안내려고",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.20",
          "form": "도긴개긴인데",
          "original_form": "도긴개긴인데",
          "speaker_id": "2",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.21",
          "form": "뭔지는 알겠다.",
          "original_form": "뭔지는 알겠다.",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100037593.1.22",
          "form": "내아들만 혼나는 느낌",
          "original_form": "내아들만 혼나는 느낌",
          "speaker_id": "2",
          "time": "20210901 12:49"
