        {
          "id": "MDRW2100030585.1.2",
          "form": "잘잤나요",
          "original_form": "잘잤나요",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030585.1.3",
          "form": "안녕하세요 ㅋㅋ",
          "original_form": "안녕하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100030585.1.4",
          "form": "좋은모닝",
          "original_form": "좋은모닝",
          "speaker_id": "1",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100030585.1.5",
          "form": "날씨가 좋네요",
          "original_form": "날씨가 좋네요",
          "speaker_id": "1",
          "time": "20210904 09:32"
        },
        {
          "id": "MDRW2100030585.1.6",
          "form": "ㅋㅋ그러게요 결혼하셨나요?",
          "original_form": "ㅋㅋ그러게요 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100030585.1.7",
          "form": "안했습니다",
          "original_form": "안했습니다",
          "speaker_id": "1",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100030585.1.8",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210904 09:58"
        },
        {
          "id": "MDRW2100030585.1.9",
          "form": "저도 안했습니다",
          "original_form": "저도 안했습니다",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030585.1.10",
          "form": "날씨좋은데 데이트 가시나용",
          "original_form": "날씨좋은데 데이트 가시나용",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030585.1.11",
          "form": "어머니랑 데이트 중입니다",
          "original_form": "어머니랑 데이트 중입니다",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030585.1.12",
          "form": "요즘 편찮으셔서 제가 챙겨드리고있네요",
          "original_form": "요즘 편찮으셔서 제가 챙겨드리고있네요",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030585.1.13",
          "form": "건강이 최고입니다",
          "original_form": "건강이 최고입니다",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030585.1.14",
          "form": "오 멋지세요",
          "original_form": "오 멋지세요",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030585.1.15",
          "form": "맞아요 어머니랑 시간 보내는거 너무 소중하고 좋아요 ㅠㅠ",
          "original_form": "맞아요 어머니랑 시간 보내는거 너무 소중하고 좋아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030585.1.16",
          "form": "부모님이 늙으시니 귀여운 아이가 되네요 ㅎㅎ",
          "original_form": "부모님이 늙으시니 귀여운 아이가 되네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030585.1.17",
          "form": "부모님 건강은 괜찮으시죠?",
          "original_form": "부모님 건강은 괜찮으시죠?",
          "speaker_id": "1",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030585.1.18",
          "form": "네 아직은 괜찮으십니다 ㅋㅋ",
          "original_form": "네 아직은 괜찮으십니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030585.1.19",
          "form": "어머니께서 결혼 하라는 말씀은 안하시나요?",
          "original_form": "어머니께서 결혼 하라는 말씀은 안하시나요?",
          "speaker_id": "2",
          "time": "20210904 10:11"
        },
        {
          "id": "MDRW2100030585.1.20",
          "form": "뭐 얘기가 없는건 아닌데 워낙 제 앞가림은 잘해서",
          "original_form": "뭐 얘기가 없는건 아닌데 워낙 제 앞가림은 잘해서",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030585.1.21",
          "form": "딱히 푸쉬가 없네요",
          "original_form": "딱히 푸쉬가 없네요",
          "speaker_id": "1",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030585.1.22",
          "form": "부모님께서는 뭔가 본인들이 안계실때의",
          "original_form": "부모님께서는 뭔가 본인들이 안계실때의",
          "speaker_id": "2",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030585.1.23",
          "form": "자녀를 걱정하시는것같아요",
          "original_form": "자녀를 걱정하시는것같아요",
          "speaker_id": "2",
          "time": "20210904 10:12"
        },
        {
          "id": "MDRW2100030585.1.24",
          "form": "아무래도...",
          "original_form": "아무래도...",
          "speaker_id": "1",
          "time": "20210904 10:13"
        },
        {
          "id": "MDRW2100030585.1.25",
          "form": "으 생각하기 싫네요ㅜ",
          "original_form": "으 생각하기 싫네요ㅜ",
