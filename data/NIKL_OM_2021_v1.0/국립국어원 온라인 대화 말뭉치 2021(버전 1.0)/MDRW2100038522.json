        {
          "id": "MDRW2100038522.1.1",
          "form": "잘산템 있나요",
          "original_form": "잘산템 있나요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100038522.1.2",
          "form": "손목이아파서",
          "original_form": "손목이아파서",
          "speaker_id": "1",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100038522.1.3",
          "form": "손목보호대 잘산템이에요",
          "original_form": "손목보호대 잘산템이에요",
          "speaker_id": "1",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100038522.1.4",
          "form": "와 어떤거 쓰세요 저마침 찾는중",
          "original_form": "와 어떤거 쓰세요 저마침 찾는중",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100038522.1.5",
          "form": "네이버에 검색해서",
          "original_form": "네이버에 검색해서",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100038522.1.6",
          "form": "리뷰많은거암거나삿어욤!!",
          "original_form": "리뷰많은거암거나삿어욤!!",
          "speaker_id": "1",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100038522.1.7",
          "form": "아정말요 네이버에서",
          "original_form": "아정말요 네이버에서",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100038522.1.8",
          "form": "저도 찾아보려고요ㅠㅠ",
          "original_form": "저도 찾아보려고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100038522.1.9",
          "form": "name2님은 잘산템 잇으신가요",
          "original_form": "&name2&님은 잘산템 잇으신가요",
          "speaker_id": "1",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100038522.1.10",
          "form": "저는 전기모기채 잘산거같아요",
          "original_form": "저는 전기모기채 잘산거같아요",
          "speaker_id": "2",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100038522.1.11",
          "form": "ㅋㅋ아 진짜 그것도 꿀템이네요",
          "original_form": "ㅋㅋ아 진짜 그것도 꿀템이네요",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100038522.1.12",
          "form": "정말 요긴했어요 첨엠 미안햄ㅅ는데",
          "original_form": "정말 요긴했어요 첨엠 미안햄ㅅ는데",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100038522.1.13",
          "form": "처음엔 미안했지만 모기에게",
          "original_form": "처음엔 미안했지만 모기에게",
          "speaker_id": "2",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100038522.1.14",
          "form": "ㅋㅋ저희집에도 하나 잇어요",
          "original_form": "ㅋㅋ저희집에도 하나 잇어요",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100038522.1.15",
          "form": "올해 다행이 집에 모기가없어서 한번도 안썻어요",
          "original_form": "올해 다행이 집에 모기가없어서 한번도 안썻어요",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100038522.1.16",
          "form": "오 정말요",
          "original_form": "오 정말요",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100038522.1.17",
          "form": "다행이1니다ㅠㅠ",
          "original_form": "다행이1니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100038522.1.18",
          "form": "저희는 9층인데 자뀨 어디서 오는지ㅜㅜ",
          "original_form": "저희는 9층인데 자뀨 어디서 오는지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100038522.1.19",
          "form": "손목보호대언능사세요",
          "original_form": "손목보호대언능사세요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100038522.1.20",
          "form": "확실히 덜아파요",
          "original_form": "확실히 덜아파요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100038522.1.21",
          "form": "일할때 계속 착용중입니다 ㅎㅎ",
          "original_form": "일할때 계속 착용중입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100038522.1.22",
          "form": "오정말요 바로사야지",
          "original_form": "오정말요 바로사야지",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100038522.1.23",
          "form": "손 다 감싸는고에요?",
          "original_form": "손 다 감싸는고에요?",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100038522.1.24",
          "form": "엄지에 고리끼우는것도사고",
          "original_form": "엄지에 고리끼우는것도사고",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100038522.1.25",
          "form": "그냥 손목만감싸는거 삿는데",
          "original_form": "그냥 손목만감싸는거 삿는데",
          "speaker_id": "1",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100038522.1.26",
          "form": "아 두개 사셨균요",
          "original_form": "아 두개 사셨균요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100038522.1.27",
          "form": "손 자주 씻으니까 엄지에고리끼우는건 쫌 불편해욤",
          "original_form": "손 자주 씻으니까 엄지에고리끼우는건 쫌 불편해욤",
          "speaker_id": "1",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100038522.1.28",
          "form": "둘 즁 고민이에요",
          "original_form": "둘 즁 고민이에요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100038522.1.29",
          "form": "아하 그로네요",
          "original_form": "아하 그로네요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100038522.1.30",
          "form": "오로지라는 브랜드 좋아보여요",
          "original_form": "오로지라는 브랜드 좋아보여요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100038522.1.31",
          "form": "일단 언능 사세유",
          "original_form": "일단 언능 사세유",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100038522.1.32",
