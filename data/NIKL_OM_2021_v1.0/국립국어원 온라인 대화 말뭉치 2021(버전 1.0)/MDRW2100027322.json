      "utterance": [
        {
          "id": "MDRW2100027322.1.1",
          "form": "닭껍질튀김",
          "original_form": "닭껍질튀김",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.2",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.3",
          "form": "에는 칭타오죠",
          "original_form": "에는 칭타오죠",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.4",
          "form": "존맛입니다",
          "original_form": "존맛입니다",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.5",
          "form": "맞아요 칭따오랑",
          "original_form": "맞아요 칭따오랑",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.6",
          "form": "잘어울리죠",
          "original_form": "잘어울리죠",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.7",
          "form": "느끼할땐",
          "original_form": "느끼할땐",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.8",
          "form": "시원한 아이스크림도",
          "original_form": "시원한 아이스크림도",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.9",
          "form": "칭따오ㅋ",
          "original_form": "칭따오ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.10",
          "form": "어울립니다",
          "original_form": "어울립니다",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.11",
          "form": "시간되시면 피자도 같이 드셔보세요",
          "original_form": "시간되시면 피자도 같이 드셔보세요",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.12",
          "form": "크 맛있겠네요",
          "original_form": "크 맛있겠네요",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027322.1.13",
          "form": "피자요?",
          "original_form": "피자요?",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.15",
          "form": "부평동에",
          "original_form": "부평동에",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.16",
          "form": "그걸 다먹을 수 있대요?",
          "original_form": "그걸 다먹을 수 있대요?",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.17",
          "form": "이음식들을 한곳에서",
          "original_form": "이음식들을 한곳에서",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.18",
          "form": "파는곳이 있어요",
          "original_form": "파는곳이 있어요",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.19",
          "form": "세상에나",
          "original_form": "세상에나",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.20",
          "form": "지역 맛집이에요",
          "original_form": "지역 맛집이에요",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.21",
          "form": "인간의 배가 아니네요",
          "original_form": "인간의 배가 아니네요",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.22",
          "form": "양도 많아요",
          "original_form": "양도 많아요",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027322.1.23",
          "form": "지역맛집이네요",
          "original_form": "지역맛집이네요",
