      "utterance": [
        {
          "id": "MDRW2100007449.1.1",
          "form": "요즘 육아하시기 어때요?",
          "original_form": "요즘 육아하시기 어때요?",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007449.1.2",
          "form": "어릴때에 비하면 나은거같아요",
          "original_form": "어릴때에 비하면 나은거같아요",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007449.1.3",
          "form": "지금은 그래도 말도하고 눈치도 있고",
          "original_form": "지금은 그래도 말도하고 눈치도 있고",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007449.1.4",
          "form": "요즘 얼집은 가나요?",
          "original_form": "요즘 얼집은 가나요?",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007449.1.5",
          "form": "혼자서 할줄아는것도 생기고요 ㅎ",
          "original_form": "혼자서 할줄아는것도 생기고요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007449.1.6",
          "form": "네 유치원가요 ㅎ",
          "original_form": "네 유치원가요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007449.1.7",
          "form": "벌써 유치원 가는군요ㅎ",
          "original_form": "벌써 유치원 가는군요ㅎ",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007449.1.8",
          "form": "다컸네요",
          "original_form": "다컸네요",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007449.1.9",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007449.1.10",
          "form": "유치원에 선생님이 전화주셔서 이야기해주시는데",
          "original_form": "유치원에 선생님이 전화주셔서 이야기해주시는데",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007449.1.11",
          "form": "이젠 엄마도 도와주고 심부름도 할 나이겠네요",
          "original_form": "이젠 엄마도 도와주고 심부름도 할 나이겠네요",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007449.1.12",
          "form": "집에서는 싸우고 때리고 하드만 유치원에서는 지나가다마주치면 하트날리고 그런데요 ㅋ",
          "original_form": "집에서는 싸우고 때리고 하드만 유치원에서는 지나가다마주치면 하트날리고 그런데요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007449.1.13",
          "form": "어머 귀여워라ㅎㅎ",
          "original_form": "어머 귀여워라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007449.1.14",
          "form": "확실히 집이랑 밖이라 행동이 다른가봐요",
          "original_form": "확실히 집이랑 밖이라 행동이 다른가봐요",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007449.1.15",
          "form": "집에서는 싸운다고하니 선생님 놀라시더라구요",
          "original_form": "집에서는 싸운다고하니 선생님 놀라시더라구요",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007449.1.16",
          "form": "집에선 본성격이 나오는건가요ㅎㅎ",
          "original_form": "집에선 본성격이 나오는건가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007449.1.17",
          "form": "요즘 애들은 마스크쓰는게 일상이라",
          "original_form": "요즘 애들은 마스크쓰는게 일상이라",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007449.1.18",
          "form": "안쓴사람보면 자꾸 큰소리로 이야기행ᆢ",
          "original_form": "안쓴사람보면 자꾸 큰소리로 이야기행ᆢ",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007449.1.19",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007449.1.20",
          "form": "어 저사람 마스크안썼다 이러면서 손가락으로 가리켜요 ㅋ",
          "original_form": "어 저사람 마스크안썼다 이러면서 손가락으로 가리켜요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007449.1.21",
          "form": "ㅎㅎ 엄마가 대략난감",
          "original_form": "ㅎㅎ 엄마가 대략난감",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007449.1.22",
          "form": "그래도 옳은 일 한건 맞자나요",
          "original_form": "그래도 옳은 일 한건 맞자나요",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007449.1.23",
          "form": "그래서 지난번에는 지나가던 고딩이 저희딸지적질받고 주머니에서 마스크꺼내서쓰고가더라구요",
          "original_form": "그래서 지난번에는 지나가던 고딩이 저희딸지적질받고 주머니에서 마스크꺼내서쓰고가더라구요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007449.1.24",
          "form": "턱마스크한것도 저러면안되는데 \r\n저러면 코로나바이러스가 들어가는데 카면서 참견하고  ㅎㅎ",
          "original_form": "턱마스크한것도 저러면안되는데 \r\n저러면 코로나바이러스가 들어가는데 카면서 참견하고  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007449.1.25",
          "form": "오ㅎ 대견한데요 저라면 뿌듯했을듯",
          "original_form": "오ㅎ 대견한데요 저라면 뿌듯했을듯",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007449.1.26",
          "form": "내가 자식을 잘키웠구나~",
          "original_form": "내가 자식을 잘키웠구나~",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007449.1.27",
          "form": "그런가요? ㅎㅎ 저는 괜히 죄송스럽던데오ㅡㅎ",
          "original_form": "그런가요? ㅎㅎ 저는 괜히 죄송스럽던데오ㅡㅎ",
          "speaker_id": "2",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007449.1.28",
          "form": "괜한 오지랍같은 느낌이라 ㅎㅎ",
          "original_form": "괜한 오지랍같은 느낌이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:26"
        }
