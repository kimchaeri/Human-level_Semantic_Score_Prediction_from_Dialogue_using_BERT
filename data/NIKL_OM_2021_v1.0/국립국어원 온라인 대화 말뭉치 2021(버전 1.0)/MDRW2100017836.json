      "utterance": [
        {
          "id": "MDRW2100017836.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017836.1.2",
          "form": "코로나언제끝나",
          "original_form": "코로나언제끝나",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017836.1.3",
          "form": "아 여행 진짜 ㅠㅠ",
          "original_form": "아 여행 진짜 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017836.1.4",
          "form": "넌 어디 젤 가고 싶음?",
          "original_form": "넌 어디 젤 가고 싶음?",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017836.1.5",
          "form": "유럽이 너무가고싶음",
          "original_form": "유럽이 너무가고싶음",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.6",
          "form": "아 나도 유럽...",
          "original_form": "아 나도 유럽...",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.7",
          "form": "동양이랑 확다른 그느낌이",
          "original_form": "동양이랑 확다른 그느낌이",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.8",
          "form": "진짜좋더라",
          "original_form": "진짜좋더라",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.9",
          "form": "옛날부터 진짜 위시리스트임 ㅠ",
          "original_form": "옛날부터 진짜 위시리스트임 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.10",
          "form": "(사진으로만가봄)",
          "original_form": "(사진으로만가봄)",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.11",
          "form": "ㅁㅈㅁㅈ 그냥 거리만 걸어도 내가 여행왔다는 느낌이 물씬 나잖아",
          "original_form": "ㅁㅈㅁㅈ 그냥 거리만 걸어도 내가 여행왔다는 느낌이 물씬 나잖아",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.12",
          "form": "응응 그리고",
          "original_form": "응응 그리고",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.13",
          "form": "그리고 난 문화유산이 너무 보고싶다 ㅠ",
          "original_form": "그리고 난 문화유산이 너무 보고싶다 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.14",
          "form": "파리한달다녀온 친구가",
          "original_form": "파리한달다녀온 친구가",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017836.1.15",
          "form": "그냥 아침에 자전거타고",
          "original_form": "그냥 아침에 자전거타고",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.16",
          "form": "박물관이랑 미술관도 직접 눈으로 보고 싶어",
          "original_form": "박물관이랑 미술관도 직접 눈으로 보고 싶어",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.17",
          "form": "국가 건너다니고 이런거",
          "original_form": "국가 건너다니고 이런거",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.18",
          "form": "캬...EU의 맛",
          "original_form": "캬...EU의 맛",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.19",
          "form": "말해주는데진짜 신기하면서 해보고싶었음",
          "original_form": "말해주는데진짜 신기하면서 해보고싶었음",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.20",
          "form": "2024파리올림픽인데",
          "original_form": "2024파리올림픽인데",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.21",
          "form": "그때함 찾아가봐야하지않겠니",
          "original_form": "그때함 찾아가봐야하지않겠니",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.22",
          "form": "코로나..그 전에는 끝나겠지?",
          "original_form": "코로나..그 전에는 끝나겠지?",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017836.1.23",
          "form": "빡세게 돈모으자구",
          "original_form": "빡세게 돈모으자구",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.24",
          "form": "ㅠㅠ 흑흑 유럽여행 진짜 가고싶다",
          "original_form": "ㅠㅠ 흑흑 유럽여행 진짜 가고싶다",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.25",
          "form": "과학자들을 믿자",
          "original_form": "과학자들을 믿자",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.26",
          "form": "한 석달은 다녀오고싶음",
          "original_form": "한 석달은 다녀오고싶음",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.27",
          "form": "ㄹㅇㄹㅇ",
          "original_form": "ㄹㅇㄹㅇ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.28",
          "form": "나는 계획겁나빡세게세우는편인데",
          "original_form": "나는 계획겁나빡세게세우는편인데",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.29",
          "form": "그정도길게잡으멵",
          "original_form": "그정도길게잡으멵",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.30",
          "form": "좀 여유가질수있을거같아서",
          "original_form": "좀 여유가질수있을거같아서",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017836.1.31",
          "form": "일상처럼 누리기? 이런거해보고싶음",
          "original_form": "일상처럼 누리기? 이런거해보고싶음",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017836.1.32",
          "form": "너 대만은",
          "original_form": "너 대만은",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017836.1.33",
          "form": "그냥 먹을거리때문에",
          "original_form": "그냥 먹을거리때문에",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017836.1.34",
          "form": "가고싶은거니",
          "original_form": "가고싶은거니",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017836.1.35",
          "form": "먹을거리도 있고",
          "original_form": "먹을거리도 있고",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017836.1.36",
          "form": "동남아쪽은 또 동남아 특유의 분위기가 있자너",
          "original_form": "동남아쪽은 또 동남아 특유의 분위기가 있자너",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017836.1.37",
          "form": "ㅋㅋㅋㅋ맞아 나 가본거라곤",
          "original_form": "ㅋㅋㅋㅋ맞아 나 가본거라곤",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017836.1.38",
          "form": "동아시아뿐이라",
          "original_form": "동아시아뿐이라",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017836.1.39",
          "form": "동남아 궁금하긴하다",
          "original_form": "동남아 궁금하긴하다",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017836.1.40",
          "form": "언제꼭..가보는걸로",
          "original_form": "언제꼭..가보는걸로",
          "speaker_id": "1",
          "time": "20210826 21:27"
        }
      ]
    }
  ]
