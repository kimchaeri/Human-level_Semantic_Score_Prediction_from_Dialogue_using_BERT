      "utterance": [
        {
          "id": "MDRW2100041992.1.1",
          "form": "최근구매하신거있나요?!",
          "original_form": "최근구매하신거있나요?!",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041992.1.2",
          "form": "네 ㅋㅋ  가을옷 샀어요ㅋ",
          "original_form": "네 ㅋㅋ  가을옷 샀어요ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041992.1.3",
          "form": "최근에 사신거 뭐있으세요?",
          "original_form": "최근에 사신거 뭐있으세요?",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041992.1.4",
          "form": "저는 발목 아대샀어요",
          "original_form": "저는 발목 아대샀어요",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100041992.1.5",
          "form": "가을옷 어떤거 사셨어요?!",
          "original_form": "가을옷 어떤거 사셨어요?!",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100041992.1.6",
          "form": "발목아프세요?",
          "original_form": "발목아프세요?",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041992.1.7",
          "form": "애들 옷이 너무 부족해서 애들옷좀샀어요ㅋ",
          "original_form": "애들 옷이 너무 부족해서 애들옷좀샀어요ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100041992.1.8",
          "form": "넘어져서 접질렀어요ㅠ",
          "original_form": "넘어져서 접질렀어요ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100041992.1.9",
          "form": "아 아이가 있으시군요!",
          "original_form": "아 아이가 있으시군요!",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100041992.1.10",
          "form": "아이고ㅜ아프셨겠어요.ㅜㅠ",
          "original_form": "아이고ㅜ아프셨겠어요.ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100041992.1.11",
          "form": "생각만해도 아프네요.ㅜㅜ",
          "original_form": "생각만해도 아프네요.ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100041992.1.12",
          "form": "네 애들이 금방자라요ㅋ",
          "original_form": "네 애들이 금방자라요ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041992.1.13",
          "form": "맞아요 애기옷은 사면 금방금방 크죠ㅠ",
          "original_form": "맞아요 애기옷은 사면 금방금방 크죠ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041992.1.14",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041992.1.15",
          "form": "몇번안입고 또 사고 그러네요ㅋㅋ",
          "original_form": "몇번안입고 또 사고 그러네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041992.1.16",
          "form": "조금 크게사도 금방크죠?!",
          "original_form": "조금 크게사도 금방크죠?!",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041992.1.17",
          "form": "네네ㅋㅋ 매년 사고ㅋㅋ 계절마다 사게되네요ㅋ",
          "original_form": "네네ㅋㅋ 매년 사고ㅋㅋ 계절마다 사게되네요ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041992.1.18",
          "form": "근데 또 가격이 비싸더라고요",
          "original_form": "근데 또 가격이 비싸더라고요",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100041992.1.19",
          "form": "맞아요ㅋㅋ 점점  비싸져욬ㅋ",
          "original_form": "맞아요ㅋㅋ 점점  비싸져욬ㅋ",
          "speaker_id": "1",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100041992.1.20",
          "form": "쪼끄만데 비싸서 원단값이 아니구나,,,싶더라고요ㅋㅋ",
          "original_form": "쪼끄만데 비싸서 원단값이 아니구나,,,싶더라고요ㅋㅋ",
