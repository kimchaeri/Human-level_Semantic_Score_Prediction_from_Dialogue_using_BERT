        {
          "id": "MDRW2100002973.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002973.1.3",
          "form": "ㅋㅋㅋ 머행",
          "original_form": "ㅋㅋㅋ 머행",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002973.1.4",
          "form": "아아ㅋㅋㅋ 지금 씻고나왔어.ㅋ",
          "original_form": "아아ㅋㅋㅋ 지금 씻고나왔어.ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002973.1.5",
          "form": "너는 뭐해?",
          "original_form": "너는 뭐해?",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002973.1.6",
          "form": "밥먹이는중ㅠ늦게일어나서",
          "original_form": "밥먹이는중ㅠ늦게일어나서",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002973.1.7",
          "form": "지금 밥먹고있다고?",
          "original_form": "지금 밥먹고있다고?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002973.1.8",
          "form": "낮잠잤어?",
          "original_form": "낮잠잤어?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002973.1.9",
          "form": "둘째랑?",
          "original_form": "둘째랑?",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002973.1.10",
          "form": "나는안잤는데 둘째가늦게일어났어ㅜ",
          "original_form": "나는안잤는데 둘째가늦게일어났어ㅜ",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002973.1.11",
          "form": "아아ㅋㅋ 늦게일어났네ㅋㅋㅋ뭐먹어?",
          "original_form": "아아ㅋㅋ 늦게일어났네ㅋㅋㅋ뭐먹어?",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002973.1.12",
          "form": "치킨ㅎㅎ어제먹다남은거",
          "original_form": "치킨ㅎㅎ어제먹다남은거",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002973.1.13",
          "form": "치킨 좋지ㅋㅋ",
          "original_form": "치킨 좋지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002973.1.14",
          "form": "맛있겠다ㅋ",
          "original_form": "맛있겠다ㅋ",
          "speaker_id": "2",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002973.1.15",
          "form": "네네치킨은 잘먹더라..ㅋㅋㅋ희안해",
          "original_form": "네네치킨은 잘먹더라..ㅋㅋㅋ희안해",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002973.1.16",
          "form": "응응 맞아 우리둘째도 치킨은 잘먹더라",
          "original_form": "응응 맞아 우리둘째도 치킨은 잘먹더라",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002973.1.17",
          "form": "ㅋㅋㅋ편식해서 그렇지ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ편식해서 그렇지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002973.1.18",
          "form": "치킨은 거부할수없는ㅋㅋ",
