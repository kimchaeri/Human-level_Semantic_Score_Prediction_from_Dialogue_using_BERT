      "utterance": [
        {
          "id": "MDRW2100002336.1.1",
          "form": "홍...연애와 결혼....",
          "original_form": "홍...연애와 결혼....",
          "speaker_id": "2",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002336.1.2",
          "form": "러떤가요",
          "original_form": "러떤가요",
          "speaker_id": "2",
          "time": "20210820 09:59"
        },
        {
          "id": "MDRW2100002336.1.3",
