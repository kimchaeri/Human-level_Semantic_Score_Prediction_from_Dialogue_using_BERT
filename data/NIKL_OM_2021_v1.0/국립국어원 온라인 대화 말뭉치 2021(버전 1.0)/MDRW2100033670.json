      "utterance": [
        {
          "id": "MDRW2100033670.1.1",
          "form": "알비노 고릴라를 연구하는데 수화로 대화",
          "original_form": "알비노 고릴라를 연구하는데 수화로 대화",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100033670.1.2",
          "form": "말로 안하고 수화로 대화?",
          "original_form": "말로 안하고 수화로 대화?",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100033670.1.3",
          "form": "드웨인 존슨은 유인원전문가",
          "original_form": "드웨인 존슨은 유인원전문가",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100033670.1.4",
          "form": "ㅋㅋㅋ  좀 웃긴다 이상하다",
          "original_form": "ㅋㅋㅋ  좀 웃긴다 이상하다",
          "speaker_id": "1",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100033670.1.5",
          "form": "고릴라랑 드웨인 존슨이 앞에 대화가 없어졌다",
          "original_form": "고릴라랑 드웨인 존슨이 앞에 대화가 없어졌다",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100033670.1.6",
          "form": "고릴라가 괴수로 변하는건 아니고?",
          "original_form": "고릴라가 괴수로 변하는건 아니고?",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100033670.1.7",
          "form": "그 고릴라가 그 화학물질에 노출되어서 엄청 커진다",
          "original_form": "그 고릴라가 그 화학물질에 노출되어서 엄청 커진다",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100033670.1.8",
          "form": "왠지 그럴거 같드라",
          "original_form": "왠지 그럴거 같드라",
          "speaker_id": "1",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100033670.1.9",
          "form": "다른 가스를 마신 특대 악어 등이 커지고 괴수로 변해서 도심파괴",
          "original_form": "다른 가스를 마신 특대 악어 등이 커지고 괴수로 변해서 도심파괴",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100033670.1.10",
          "form": "늑대 ㅋㅋ",
          "original_form": "늑대 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100033670.1.11",
          "form": "원작이 만화는 아니지?",
          "original_form": "원작이 만화는 아니지?",
          "speaker_id": "1",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100033670.1.12",
          "form": "여기서 드웨인존슨이 해독제를 찾기 위해서",
          "original_form": "여기서 드웨인존슨이 해독제를 찾기 위해서",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100033670.1.13",
          "form": "어떻게 하는데?",
          "original_form": "어떻게 하는데?",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100033670.1.14",
          "form": "이게 게임을 원작으로 한단다",
          "original_form": "이게 게임을 원작으로 한단다",
          "speaker_id": "2",
          "time": "20210824 22:29"
        }
      ]
