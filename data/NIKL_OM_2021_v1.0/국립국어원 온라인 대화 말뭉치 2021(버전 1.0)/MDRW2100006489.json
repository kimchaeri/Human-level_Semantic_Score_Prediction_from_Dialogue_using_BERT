      "utterance": [
        {
          "id": "MDRW2100006489.1.1",
          "form": "네 이제 이차 맞아요",
          "original_form": "네 이제 이차 맞아요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006489.1.2",
          "form": "어디에서 맞아",
          "original_form": "어디에서 맞아",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006489.1.3",
          "form": "길병원에서 맞아요",
          "original_form": "길병원에서 맞아요",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006489.1.4",
          "form": "그럼 내일은 아무것도 하지마",
          "original_form": "그럼 내일은 아무것도 하지마",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006489.1.5",
          "form": "안그래도 그러려고요",
          "original_form": "안그래도 그러려고요",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100006489.1.6",
          "form": "괜히 무리하면 크게 아플 수 있어",
          "original_form": "괜히 무리하면 크게 아플 수 있어",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006489.1.7",
          "form": "그러니까요 저 무리하면 힘들어서 안돼요",
          "original_form": "그러니까요 저 무리하면 힘들어서 안돼요",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100006489.1.8",
          "form": "집안일 해야 될 거 있으면 언니 시켜",
          "original_form": "집안일 해야 될 거 있으면 언니 시켜",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006489.1.9",
          "form": "네 안그래도 그럴생각이에요",
          "original_form": "네 안그래도 그럴생각이에요",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100006489.1.10",
          "form": "밥은 있는 거 대충 먹어도 돼",
          "original_form": "밥은 있는 거 대충 먹어도 돼",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100006489.1.11",
          "form": "근데 좀 그래요",
          "original_form": "근데 좀 그래요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006489.1.12",
          "form": "맛있는거 먹어야되는데",
          "original_form": "맛있는거 먹어야되는데",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006489.1.13",
          "form": "뭐가 어때 한두 끼 정도는 대충 먹어도 돼",
          "original_form": "뭐가 어때 한두 끼 정도는 대충 먹어도 돼",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006489.1.14",
          "form": "그럼 대충 드릴게요 어쩔수없다 ㅋㅋㅋ",
          "original_form": "그럼 대충 드릴게요 어쩔수없다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100006489.1.15",
          "form": "응 김만 있어도 잘 먹을 수 있어",
          "original_form": "응 김만 있어도 잘 먹을 수 있어",
          "speaker_id": "1",
