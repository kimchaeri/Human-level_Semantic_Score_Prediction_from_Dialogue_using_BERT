      "utterance": [
        {
          "id": "MDRW2100016832.1.1",
          "form": "그러니깐ㅋ",
          "original_form": "그러니깐ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.2",
          "form": "일단 된장국",
          "original_form": "일단 된장국",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.3",
          "form": "있고ㅋㅋ",
          "original_form": "있고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.4",
          "form": "계란말이해서 먹을까싶다ㅋ",
          "original_form": "계란말이해서 먹을까싶다ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.5",
          "form": "된장국 있으면 끝났네",
          "original_form": "된장국 있으면 끝났네",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.6",
          "form": "언니는?ㅋㅋ",
          "original_form": "언니는?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.7",
          "form": "좋구만 계란말이",
          "original_form": "좋구만 계란말이",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.8",
          "form": "난 아직 뭐 먹을지 고민 중",
          "original_form": "난 아직 뭐 먹을지 고민 중",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.9",
          "form": "냉장고를 열어봐야 되는데",
          "original_form": "냉장고를 열어봐야 되는데",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016832.1.10",
          "form": "뭐 없네.....",
          "original_form": "뭐 없네.....",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016832.1.11",
          "form": "ㅋㅋ언니 우리집도 뭐 없다. 그냥 ㅋㅋㅋ 계란이 보이길래 ㅋㅋㅋ",
          "original_form": "ㅋㅋ언니 우리집도 뭐 없다. 그냥 ㅋㅋㅋ 계란이 보이길래 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016832.1.12",
          "form": "우리도 오늘  계란찜 해먹어야지",
          "original_form": "우리도 오늘  계란찜 해먹어야지",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016832.1.13",
          "form": "계란이 보이네ㅋㅋㅋㅋㅋ",
          "original_form": "계란이 보이네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016832.1.14",
          "form": "그래 언니 간단하게 먹자 ㅋㅋㅋ",
          "original_form": "그래 언니 간단하게 먹자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016832.1.15",
          "form": "진짜 ㅋㅋ 밥도 해야되는데 귀찮다 ㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋ 밥도 해야되는데 귀찮다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016832.1.16",
          "form": "그래 날씨가 더우니",
          "original_form": "그래 날씨가 더우니",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016832.1.17",
          "form": "밥하기도 싫고",
          "original_form": "밥하기도 싫고",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016832.1.18",
          "form": "날씨가 좋아 도",
          "original_form": "날씨가 좋아 도",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016832.1.19",
          "form": "밥하기는 싫다ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "밥하기는 싫다ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.20",
          "form": "그러니깐은 언니 ㅠㅠㅠ 나두 그렇다",
          "original_form": "그러니깐은 언니 ㅠㅠㅠ 나두 그렇다",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.21",
          "form": "매일 반복이다.",
          "original_form": "매일 반복이다.",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.22",
          "form": "비슬산에 또 큰 까페 생겼다던데",
          "original_form": "비슬산에 또 큰 까페 생겼다던데",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.23",
          "form": "커피 맛있겠다",
          "original_form": "커피 맛있겠다",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.24",
          "form": "응응 카페 예쁜거 많던데 ㅋㅋ",
          "original_form": "응응 카페 예쁜거 많던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.25",
          "form": "언제가 보니ㅋㅋㅋㅋ",
          "original_form": "언제가 보니ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016832.1.26",
          "form": "힐데 스도 이제는 커피 맛을 알아서",
          "original_form": "힐데 스도 이제는 커피 맛을 알아서",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016832.1.27",
          "form": "그러게 ㅠㅠ 일하니깐 더 못가보제?",
          "original_form": "그러게 ㅠㅠ 일하니깐 더 못가보제?",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016832.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ힐데스 ㅋㅋㅋ아 웃겨 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ힐데스 ㅋㅋㅋ아 웃겨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016832.1.29",
          "form": "그래도 일하면서 매일 커피 마시니까",
          "original_form": "그래도 일하면서 매일 커피 마시니까",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016832.1.30",
          "form": "힐데스ㅋㅋㅋㅋㄱ",
          "original_form": "힐데스ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016832.1.31",
          "form": "일하면서 매일 마신다고?? ㅋㅋ대박이네. 진짜 커피맛 알게됐구나 ㅋㅋ",
          "original_form": "일하면서 매일 마신다고?? ㅋㅋ대박이네. 진짜 커피맛 알게됐구나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016832.1.32",
          "form": "나는 김치볶음밥먹을까 생각중이다ㅋ",
          "original_form": "나는 김치볶음밥먹을까 생각중이다ㅋ",
          "speaker_id": "2",
          "time": "20210906 12:40"
        }
      ]
