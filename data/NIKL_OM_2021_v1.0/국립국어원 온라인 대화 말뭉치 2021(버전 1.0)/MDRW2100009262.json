      "utterance": [
        {
          "id": "MDRW2100009262.1.1",
          "form": "겨울여행 가보신 적 있으세요?",
          "original_form": "겨울여행 가보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009262.1.2",
          "form": "겨울엔 거의 스키장으로 다닌 것 같아요",
          "original_form": "겨울엔 거의 스키장으로 다닌 것 같아요",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.3",
          "form": "아 맞네요 스키장",
          "original_form": "아 맞네요 스키장",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.4",
          "form": "스키 잘 타세요??",
          "original_form": "스키 잘 타세요??",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.5",
          "form": "아뇨 그냥 가족들이 좋아해서..",
          "original_form": "아뇨 그냥 가족들이 좋아해서..",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.6",
          "form": "저두 스키 잘 못타서",
          "original_form": "저두 스키 잘 못타서",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.7",
          "form": "스키장 가도 그냥 저는 수영장에서 놀아요",
          "original_form": "스키장 가도 그냥 저는 수영장에서 놀아요",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.8",
          "form": "그런 데가 있나요?",
          "original_form": "그런 데가 있나요?",
          "speaker_id": "2",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.9",
          "form": "강원도 알펜시아 갔더니 둘다 있더라구요",
          "original_form": "강원도 알펜시아 갔더니 둘다 있더라구요",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100009262.1.10",
          "form": "강원도 어디인가요?",
          "original_form": "강원도 어디인가요?",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009262.1.11",
          "form": "이번에 평창올림픽 한 거기에요!!",
          "original_form": "이번에 평창올림픽 한 거기에요!!",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009262.1.12",
          "form": "평창이에요!!",
          "original_form": "평창이에요!!",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009262.1.13",
          "form": "오 가깝네요",
          "original_form": "오 가깝네요",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009262.1.14",
          "form": "기억해둬야겠어요",
          "original_form": "기억해둬야겠어요",
          "speaker_id": "2",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009262.1.15",
          "form": "네 낮에는 강릉 속초 갔다오면 좋더라구요",
          "original_form": "네 낮에는 강릉 속초 갔다오면 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100009262.1.16",
          "form": "정말 좋은 곳이네요",
          "original_form": "정말 좋은 곳이네요",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009262.1.17",
          "form": "네 거기 수영장 노천탕 강추!!",
          "original_form": "네 거기 수영장 노천탕 강추!!",
          "speaker_id": "1",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009262.1.18",
          "form": "와 노천탕도 있굼요",
          "original_form": "와 노천탕도 있굼요",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009262.1.19",
          "form": "좋은 정보 감사합니다",
          "original_form": "좋은 정보 감사합니다",
          "speaker_id": "2",
          "time": "20210822 22:26"
        }
      ]
