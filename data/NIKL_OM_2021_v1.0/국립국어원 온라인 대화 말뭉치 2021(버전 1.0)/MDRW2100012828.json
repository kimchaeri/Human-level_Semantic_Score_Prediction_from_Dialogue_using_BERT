        {
          "id": "MDRW2100012828.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 02:06"
        },
        {
          "id": "MDRW2100012828.1.2",
          "form": "네,안녕하세요",
          "original_form": "네,안녕하세요",
          "speaker_id": "2",
          "time": "20210825 02:06"
        },
        {
          "id": "MDRW2100012828.1.3",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012828.1.4",
          "form": "네,좋아하는편이에요",
          "original_form": "네,좋아하는편이에요",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100012828.1.5",
          "form": "어떤브랜드 좋아하세요?",
          "original_form": "어떤브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012828.1.6",
          "form": "전 신라면,참깨라면 좋아해요~",
          "original_form": "전 신라면,참깨라면 좋아해요~",
          "speaker_id": "2",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100012828.1.7",
          "form": "짜파게티vs비빔면은요?",
          "original_form": "짜파게티vs비빔면은요?",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100012828.1.8",
          "form": "아.둘다좋아하는데 짜파게티요~",
          "original_form": "아.둘다좋아하는데 짜파게티요~",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100012828.1.9",
          "form": "새로나온 신라면볶음면? 먹어보셨어요?",
          "original_form": "새로나온 신라면볶음면? 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012828.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100012828.1.11",
          "form": "그거 맛있다면서요?",
          "original_form": "그거 맛있다면서요?",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100012828.1.12",
          "form": "어때요??",
          "original_form": "어때요??",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100012828.1.13",
          "form": "전 그냥 국물없는버전?맛인데요ㅎㅎ",
          "original_form": "전 그냥 국물없는버전?맛인데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100012828.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100012828.1.15",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100012828.1.16",
          "form": "저먹어보고싶어요",
          "original_form": "저먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100012828.1.17",
          "form": "한번사드셔보세요~",
          "original_form": "한번사드셔보세요~",
          "speaker_id": "2",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100012828.1.18",
          "form": "근처에 팔면",
          "original_form": "근처에 팔면",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100012828.1.19",
          "form": "먹어보고싶네요",
          "original_form": "먹어보고싶네요",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100012828.1.20",
          "form": "쉽게보여서 금방사실수있을거같아요",
          "original_form": "쉽게보여서 금방사실수있을거같아요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100012828.1.21",
          "form": "편의점ㅈ에도",
          "original_form": "편의점ㅈ에도",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012828.1.22",
          "form": "파나요?",
          "original_form": "파나요?",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100012828.1.23",
          "form": "제가 편의점라면코너안봐서 모르겠어요ㅎ",
          "original_form": "제가 편의점라면코너안봐서 모르겠어요ㅎ",
