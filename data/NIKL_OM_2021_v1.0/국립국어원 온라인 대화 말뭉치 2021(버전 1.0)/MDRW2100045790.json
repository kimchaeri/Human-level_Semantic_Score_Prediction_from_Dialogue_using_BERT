      "utterance": [
        {
          "id": "MDRW2100045790.1.1",
          "form": "포도잎도 드시내요 ㅋㅋㅋㅋ",
          "original_form": "포도잎도 드시내요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045790.1.2",
          "form": "되는대로 생식중",
          "original_form": "되는대로 생식중",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045790.1.3",
          "form": "아이템수가 생각보다 많네요",
          "original_form": "아이템수가 생각보다 많네요",
          "speaker_id": "2",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100045790.1.4",
          "form": "비도내리나봐여",
          "original_form": "비도내리나봐여",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100045790.1.5",
          "form": "빗물도 받아먹더라구요 ㅠ",
          "original_form": "빗물도 받아먹더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100045790.1.6",
          "form": "얼른 바가지 놓아!",
          "original_form": "얼른 바가지 놓아!",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100045790.1.7",
          "form": "씻으러 가신다는데 ㅋㅋㅋㅋ",
          "original_form": "씻으러 가신다는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100045790.1.8",
          "form": "밖에서 씻기 레전드 ㄷㄷ",
          "original_form": "밖에서 씻기 레전드 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045790.1.9",
          "form": "물도 이제 줄어들어가나봐요",
          "original_form": "물도 이제 줄어들어가나봐요",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045790.1.10",
          "form": "디테일 보소",
          "original_form": "디테일 보소",
          "speaker_id": "1",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045790.1.11",
          "form": "힘든삶이네요 ㅜㅜ",
          "original_form": "힘든삶이네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045790.1.12",
          "form": "어려워서  더 잼는듯",
          "original_form": "어려워서  더 잼는듯",
