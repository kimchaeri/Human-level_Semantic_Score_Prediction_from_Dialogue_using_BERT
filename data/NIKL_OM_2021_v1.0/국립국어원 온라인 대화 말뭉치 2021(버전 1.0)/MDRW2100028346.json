      "utterance": [
        {
          "id": "MDRW2100028346.1.1",
          "form": "이열 나도 그 작가님 신작 사서 어제 포장 뜯었어",
          "original_form": "이열 나도 그 작가님 신작 사서 어제 포장 뜯었어",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100028346.1.2",
          "form": "책 진짜 안읽는듯...",
          "original_form": "책 진짜 안읽는듯...",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100028346.1.3",
          "form": "윤고은작가 맞나???",
          "original_form": "윤고은작가 맞나???",
          "speaker_id": "1",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100028346.1.4",
          "form": "예스예스",
          "original_form": "예스예스",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028346.1.5",
          "form": "난 한강작가 4.3으로 새로 쓴 책 읽고싶은데",
          "original_form": "난 한강작가 4.3으로 새로 쓴 책 읽고싶은데",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028346.1.6",
          "form": "또 눈물줄줄흘릴것같아서 두렵",
          "original_form": "또 눈물줄줄흘릴것같아서 두렵",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028346.1.7",
          "form": "아 아 그거 탐라에서 봤어",
          "original_form": "아 아 그거 탐라에서 봤어",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028346.1.8",
          "form": "그렇겠지~~~",
          "original_form": "그렇겠지~~~",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028346.1.9",
          "form": "사실 요즘 다들 뭐읽는지 잘 모르겠어",
          "original_form": "사실 요즘 다들 뭐읽는지 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028346.1.10",
          "form": "나도... 난 얼마전에",
          "original_form": "나도... 난 얼마전에",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028346.1.11",
          "form": "긴긴밤 읽고 울었어",
          "original_form": "긴긴밤 읽고 울었어",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028346.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028346.1.13",
          "form": "그거 동화 아님??",
          "original_form": "그거 동화 아님??",
          "speaker_id": "2",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100028346.1.14",
          "form": "맞아요 청소년도서",
          "original_form": "맞아요 청소년도서",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028346.1.15",
          "form": "펭귄이랑 코뿔소 얘기인데 진짜 눈물남",
          "original_form": "펭귄이랑 코뿔소 얘기인데 진짜 눈물남",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028346.1.16",
          "form": "어 너 읽는거 보니까 그래보이더라...",
          "original_form": "어 너 읽는거 보니까 그래보이더라...",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028346.1.17",
          "form": "그걸 어디서 찾았어?",
          "original_form": "그걸 어디서 찾았어?",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028346.1.18",
          "form": "sns에서 봤는데 읽어봐야지 하다 선물받아가지고 얼마전에 봣다네요ㅎ.ㅎ",
          "original_form": "sns에서 봤는데 읽어봐야지 하다 선물받아가지고 얼마전에 봣다네요ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028346.1.19",
          "form": "아 위에 탐라라고 해놨네",
          "original_form": "아 위에 탐라라고 해놨네",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028346.1.20",
          "form": "머쓱^^,, 암튼 나도 탐라에서 봣어",
          "original_form": "머쓱^^,, 암튼 나도 탐라에서 봣어",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028346.1.21",
          "form": "앗 SNS라고 했어야했나",
          "original_form": "앗 SNS라고 했어야했나",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028346.1.22",
          "form": "암튼 글쿠나...",
          "original_form": "암튼 글쿠나...",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028346.1.23",
          "form": "뭔가 비슷한 사람들 모인 곳이라 그런가 글 테이스트도 비슷하게 맞는듯",
          "original_form": "뭔가 비슷한 사람들 모인 곳이라 그런가 글 테이스트도 비슷하게 맞는듯",
          "speaker_id": "2",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028346.1.24",
          "form": "ㅁㅈ 그런것같애",
          "original_form": "ㅁㅈ 그런것같애",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100028346.1.25",
          "form": "트위터 사람들이 조국의 시간 읽을 것 같진 않지(ㅋㅋㅋㅋㅋ",
          "original_form": "트위터 사람들이 조국의 시간 읽을 것 같진 않지(ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028346.1.26",
          "form": "조국의 시간ㅋㅋㅋㅋㅋㅋㅋ 그건 또 무슨 책이에요",
          "original_form": "조국의 시간ㅋㅋㅋㅋㅋㅋㅋ 그건 또 무슨 책이에요",
          "speaker_id": "1",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028346.1.27",
          "form": "몰러 근데 그 사람 조국 맞긴 한듯ㅋㅋㅋㅋㄱ",
          "original_form": "몰러 근데 그 사람 조국 맞긴 한듯ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028346.1.28",
          "form": "이거 되게 오랫동안 베스트셀러였어서",
          "original_form": "이거 되게 오랫동안 베스트셀러였어서",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028346.1.29",
          "form": "아 요즘 사람들 이런거 보는구나... 하고",
          "original_form": "아 요즘 사람들 이런거 보는구나... 하고",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028346.1.30",
          "form": "아..\n 아... 아 그 조국ㅋㅋ ㅋ ㅋㅋㅋㅋ",
          "original_form": "아..\n 아... 아 그 조국ㅋㅋ ㅋ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028346.1.31",
          "form": "근데 문학도 진짜 보는사람만 보고",
          "original_form": "근데 문학도 진짜 보는사람만 보고",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028346.1.32",
          "form": "요즘은 자기계발서랑 금융 에세이",
          "original_form": "요즘은 자기계발서랑 금융 에세이",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028346.1.33",
          "form": "이런거 많이 팔리는 것 같다고 생각....",
          "original_form": "이런거 많이 팔리는 것 같다고 생각....",
