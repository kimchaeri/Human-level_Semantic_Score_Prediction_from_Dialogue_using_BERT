      "utterance": [
        {
          "id": "MDRW2100042822.1.1",
          "form": "저도 한살림 사먹어요",
          "original_form": "저도 한살림 사먹어요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042822.1.2",
          "form": "한살림 드시는군요",
          "original_form": "한살림 드시는군요",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042822.1.3",
          "form": "응 저도 한살림 가고 싶은데",
          "original_form": "응 저도 한살림 가고 싶은데",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042822.1.4",
          "form": "저희 동네는 자연드림 밖에 없어가지고",
          "original_form": "저희 동네는 자연드림 밖에 없어가지고",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042822.1.5",
          "form": "한살림이 좀 더 까다롭죠",
          "original_form": "한살림이 좀 더 까다롭죠",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042822.1.6",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042822.1.7",
          "form": "일부만 그렇게 하고 있어요",
          "original_form": "일부만 그렇게 하고 있어요",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042822.1.8",
          "form": "유기농 식재료 위주로요",
          "original_form": "유기농 식재료 위주로요",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042822.1.9",
          "form": "한살림이 통과?? 되는 기준이 더 까다롭다고 하더라구요",
          "original_form": "한살림이 통과?? 되는 기준이 더 까다롭다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042822.1.10",
          "form": "그래서 물건이 제한적이라고",
          "original_form": "그래서 물건이 제한적이라고",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042822.1.11",
          "form": "얼마전에 청귤청 담았는데",
          "original_form": "얼마전에 청귤청 담았는데",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042822.1.12",
          "form": "그건 자연농 농부님 직거래로 샀어요",
          "original_form": "그건 자연농 농부님 직거래로 샀어요",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042822.1.13",
          "form": "우와 청귤청 맛있겠어요",
          "original_form": "우와 청귤청 맛있겠어요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042822.1.14",
          "form": "사실 원래 풋귤이구",
          "original_form": "사실 원래 풋귤이구",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042822.1.15",
          "form": "천귤은 다른건데 같이 섞어부른대요",
          "original_form": "천귤은 다른건데 같이 섞어부른대요",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042822.1.16",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100042822.1.17",
          "form": "귤따기 체험하러 한번 가보고싶던데",
          "original_form": "귤따기 체험하러 한번 가보고싶던데",
          "speaker_id": "1",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100042822.1.18",
          "form": "아이들이랑 하면 좋아요",
          "original_form": "아이들이랑 하면 좋아요",
          "speaker_id": "2",
          "time": "20210907 09:55"
        },
        {
          "id": "MDRW2100042822.1.19",
          "form": "오래하면 허리 아프지만요",
          "original_form": "오래하면 허리 아프지만요",
          "speaker_id": "2",
          "time": "20210907 09:55"
        },
        {
          "id": "MDRW2100042822.1.20",
          "form": "네 ㅎㅎ  귤 따기 체험하러 제주 갔던 지인이",
          "original_form": "네 ㅎㅎ  귤 따기 체험하러 제주 갔던 지인이",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042822.1.21",
          "form": "너무 좋았다고 엄청 강추하더라구요",
          "original_form": "너무 좋았다고 엄청 강추하더라구요",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042822.1.22",
          "form": "감귤밭은 서귀보라 전원느낌도 더 많구요",
          "original_form": "감귤밭은 서귀보라 전원느낌도 더 많구요",
          "speaker_id": "2",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042822.1.23",
          "form": "코로나가 좀 잠잠해지면",
          "original_form": "코로나가 좀 잠잠해지면",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042822.1.24",
          "form": "제주 가서 감귤따기 꼭 하고 싶네요",
          "original_form": "제주 가서 감귤따기 꼭 하고 싶네요",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042822.1.25",
          "form": "네 꼭이요",
          "original_form": "네 꼭이요",
          "speaker_id": "2",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042822.1.26",
          "form": "애들데리고 집콕하시느라 수고하셨어요",
          "original_form": "애들데리고 집콕하시느라 수고하셨어요",
