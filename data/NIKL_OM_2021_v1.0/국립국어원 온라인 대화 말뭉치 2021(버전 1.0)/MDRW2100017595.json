      "utterance": [
        {
          "id": "MDRW2100017595.1.1",
          "form": "여행하고싶은데 있어?",
          "original_form": "여행하고싶은데 있어?",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017595.1.2",
          "form": "나 일본정도?",
          "original_form": "나 일본정도?",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017595.1.3",
          "form": "내가 그렇게 막 아웃도어파는",
          "original_form": "내가 그렇게 막 아웃도어파는",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017595.1.4",
          "form": "일본? 오사카?",
          "original_form": "일본? 오사카?",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017595.1.5",
          "form": "아니잖아",
          "original_form": "아니잖아",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017595.1.6",
          "form": "유니버셜 스튜디오 좋더라",
          "original_form": "유니버셜 스튜디오 좋더라",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017595.1.7",
          "form": "오 맞아",
          "original_form": "오 맞아",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017595.1.8",
          "form": "아 잠만",
          "original_form": "아 잠만",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017595.1.9",
          "form": "간만에 아빠가 기분 좋다고",
          "original_form": "간만에 아빠가 기분 좋다고",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017595.1.10",
          "form": "치킨시켜준대서",
          "original_form": "치킨시켜준대서",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017595.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017595.1.12",
          "form": "유니버셜 스튜디오는 많이 들어봤어",
          "original_form": "유니버셜 스튜디오는 많이 들어봤어",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017595.1.13",
          "form": "ㅇㅇ 체험공간 잘되어있어서 좋아",
          "original_form": "ㅇㅇ 체험공간 잘되어있어서 좋아",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017595.1.14",
          "form": "거기에 만화고기 판다고",
          "original_form": "거기에 만화고기 판다고",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017595.1.15",
          "form": "아 ㅋㅋㅋㅋ 그렇긴 한데",
          "original_form": "아 ㅋㅋㅋㅋ 그렇긴 한데",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017595.1.16",
          "form": "가성비 구려....",
          "original_form": "가성비 구려....",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017595.1.17",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017595.1.18",
          "form": "시켜봤어???",
          "original_form": "시켜봤어???",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017595.1.19",
          "form": "아니 그건 아닌데",
          "original_form": "아니 그건 아닌데",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017595.1.20",
          "form": "안시킨데는 이유가 잇겠지 ...",
          "original_form": "안시킨데는 이유가 잇겠지 ...",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017595.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017595.1.22",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017595.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사실 그런 명소도 있지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ사실 그런 명소도 있지만",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017595.1.24",
          "form": "현지 오타쿠거리를",
          "original_form": "현지 오타쿠거리를",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017595.1.25",
          "form": "가보고싶긴 해",
          "original_form": "가보고싶긴 해",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017595.1.26",
          "form": "ㄹㅇ오타쿠스러울까 궁금해서",
          "original_form": "ㄹㅇ오타쿠스러울까 궁금해서",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017595.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017595.1.28",
          "form": "그 비슷한거",
          "original_form": "그 비슷한거",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017595.1.29",
          "form": "가봤는데",
          "original_form": "가봤는데",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017595.1.30",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.31",
          "form": "아 맞아 그 새연키링",
          "original_form": "아 맞아 그 새연키링",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.32",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.33",
          "form": "언니가 사온거였지",
          "original_form": "언니가 사온거였지",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.34",
          "form": "거기 숙소랑 가까웠어서",
          "original_form": "거기 숙소랑 가까웠어서",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.35",
          "form": "근데 내가 일본어를 몰라서 그런지",
          "original_form": "근데 내가 일본어를 몰라서 그런지",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.36",
          "form": "그냥 만화방같은 느낌...?",
          "original_form": "그냥 만화방같은 느낌...?",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.37",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.38",
          "form": "ㅇㅏ 뭔말해야대지",
          "original_form": "ㅇㅏ 뭔말해야대지",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017595.1.39",
          "form": "난...난 호캉스가 제일",
          "original_form": "난...난 호캉스가 제일",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.40",
          "form": "하고싶은사람인데",
          "original_form": "하고싶은사람인데",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.41",
          "form": "그것도 여행계획이긴 한데",
          "original_form": "그것도 여행계획이긴 한데",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.42",
          "form": "작정하고 말하려니까 할말이 업네",
          "original_form": "작정하고 말하려니까 할말이 업네",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.43",
          "form": "호캉스 좋아",
          "original_form": "호캉스 좋아",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.44",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.45",
          "form": "이번에 서울에 있는 호텔 호캉스 갔다왔눈대",
          "original_form": "이번에 서울에 있는 호텔 호캉스 갔다왔눈대",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.46",
          "form": "오 맞아",
          "original_form": "오 맞아",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.47",
          "form": "이래서 돈을 버는구나 싶었움",
          "original_form": "이래서 돈을 버는구나 싶었움",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.48",
          "form": "그 회사가 정해준",
          "original_form": "그 회사가 정해준",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.49",
          "form": "그 휴가?",
          "original_form": "그 휴가?",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017595.1.50",
          "form": "일반 숙소들이랑 차이 많이 나?",
          "original_form": "일반 숙소들이랑 차이 많이 나?",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017595.1.51",
          "form": "ㅇㅇ 회사가 점지해준 휴가",
          "original_form": "ㅇㅇ 회사가 점지해준 휴가",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017595.1.52",
          "form": "서울에 있는 파크하얏트 라는 호텔이엇음",
          "original_form": "서울에 있는 파크하얏트 라는 호텔이엇음",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017595.1.53",
          "form": "코엑스 맞은편에 있고",
          "original_form": "코엑스 맞은편에 있고",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017595.1.54",
          "form": "내가 묵은 방은 시티뷰 10층이엇는데",
          "original_form": "내가 묵은 방은 시티뷰 10층이엇는데",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.55",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.56",
          "form": "침대가 일단 엄청 컷음",
          "original_form": "침대가 일단 엄청 컷음",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.57",
          "form": "그런 방이긴 했지만 ㅋㅋㅋㅋㅋ",
          "original_form": "그런 방이긴 했지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.58",
          "form": "그 침대에 누워서",
          "original_form": "그 침대에 누워서",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.59",
          "form": "차가 막히는 강남 거리를 보고잇으면",
          "original_form": "차가 막히는 강남 거리를 보고잇으면",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.60",
          "form": "기뷴이 되게 묘하고...",
          "original_form": "기뷴이 되게 묘하고...",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.61",
          "form": "이래서 사람들이 호캉스를 가는구나 싶었음 ㅋㅋㅋ",
          "original_form": "이래서 사람들이 호캉스를 가는구나 싶었음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.62",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.63",
          "form": "아 그",
          "original_form": "아 그",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017595.1.64",
          "form": "유사 이택언 체험?",
          "original_form": "유사 이택언 체험?",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017595.1.65",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017595.1.66",
          "form": "나중에 사진 보여줄게",
          "original_form": "나중에 사진 보여줄게",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017595.1.67",
          "form": "ㅇㅇㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017595.1.68",
          "form": "뭔가 내부적으로 더 좋은건 없엇어?",
          "original_form": "뭔가 내부적으로 더 좋은건 없엇어?",
          "speaker_id": "2",
          "time": "20210826 20:35"
