      "utterance": [
        {
          "id": "MDRW2100008341.1.1",
          "form": "좋아하는 과일이 있나요?",
          "original_form": "좋아하는 과일이 있나요?",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008341.1.2",
          "form": "바나나 좋아해요!",
          "original_form": "바나나 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008341.1.3",
          "form": "바나나를 좋아하는 이유가 있나요?",
          "original_form": "바나나를 좋아하는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008341.1.4",
          "form": "달콤하고 먹기 편해서 인것같아요",
          "original_form": "달콤하고 먹기 편해서 인것같아요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008341.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008341.1.6",
          "form": "전 자몽 좋아해요",
          "original_form": "전 자몽 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008341.1.7",
          "form": "약간 쌉싸름한게 매력인거같아요",
          "original_form": "약간 쌉싸름한게 매력인거같아요",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008341.1.8",
          "form": "아 자몽 맛있죠",
          "original_form": "아 자몽 맛있죠",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008341.1.9",
          "form": "허니자몽블랙티도 좋아하시나요?",
          "original_form": "허니자몽블랙티도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008341.1.10",
          "form": "그거 맛있더라구요",
          "original_form": "그거 맛있더라구요",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008341.1.11",
          "form": "name2님은 어떠세요?",
          "original_form": "&name2&님은 어떠세요?",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008341.1.12",
          "form": "저도 되게 좋아해요",
          "original_form": "저도 되게 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008341.1.13",
          "form": "그 티백으로 된거 선물받아서 먹어봤는데",
          "original_form": "그 티백으로 된거 선물받아서 먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008341.1.14",
          "form": "티백도있구나",
          "original_form": "티백도있구나",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.15",
          "form": "카페가서 먹는거랑 비슷하더라고요",
          "original_form": "카페가서 먹는거랑 비슷하더라고요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.16",
          "form": "건조 자몽도 같이 넣어줘요",
          "original_form": "건조 자몽도 같이 넣어줘요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.17",
          "form": "오 한 번 구입해서 집에서 먹어봐야겠어오",
          "original_form": "오 한 번 구입해서 집에서 먹어봐야겠어오",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.18",
          "form": "추천해요!",
          "original_form": "추천해요!",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.19",
          "form": "좋은 생각이에요",
          "original_form": "좋은 생각이에요",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.20",
          "form": "좋아하는 음식은",
          "original_form": "좋아하는 음식은",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008341.1.21",
          "form": "햄버거 좋아해요",
