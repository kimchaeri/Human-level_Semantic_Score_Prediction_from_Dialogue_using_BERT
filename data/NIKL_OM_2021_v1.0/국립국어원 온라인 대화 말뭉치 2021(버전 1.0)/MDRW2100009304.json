      "utterance": [
        {
          "id": "MDRW2100009304.1.1",
          "form": "중고거래 하시나여",
          "original_form": "중고거래 하시나여",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009304.1.2",
          "form": "당근 몇번해봤어요 ㅋㅋ",
          "original_form": "당근 몇번해봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100009304.1.3",
          "form": "당근 재밌더라고요",
          "original_form": "당근 재밌더라고요",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009304.1.4",
          "form": "좀 중독성도 있어요 구경하는재미?",
          "original_form": "좀 중독성도 있어요 구경하는재미?",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009304.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009304.1.6",
          "form": "팔땐 사실 크게 이득은 없는거같앙ㆍ",
          "original_form": "팔땐 사실 크게 이득은 없는거같앙ㆍ",
          "speaker_id": "2",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009304.1.7",
          "form": "아요",
          "original_form": "아요",
          "speaker_id": "2",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009304.1.8",
          "form": "뭐 안쓰면 0인거 파는거니까요 ㅎㅎ",
          "original_form": "뭐 안쓰면 0인거 파는거니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009304.1.9",
          "form": "대신 괜히 사고싶어지는건 많은거같아요",
          "original_form": "대신 괜히 사고싶어지는건 많은거같아요",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009304.1.10",
          "form": "그건 단점이네요ㅎㅎ",
          "original_form": "그건 단점이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009304.1.11",
          "form": "지방이라서  매물이 적은것도 단점이구요ㅜ",
          "original_form": "지방이라서  매물이 적은것도 단점이구요ㅜ",
          "speaker_id": "2",
          "time": "20210823 00:52"
        },
        {
          "id": "MDRW2100009304.1.12",
          "form": "아 그거 안좋네요 ㅎㅎ",
          "original_form": "아 그거 안좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 01:28"
        },
        {
          "id": "MDRW2100009304.1.13",
          "form": "전에 6천원 섬유향수 2천원에 팔았는데",
          "original_form": "전에 6천원 섬유향수 2천원에 팔았는데",
          "speaker_id": "2",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009304.1.14",
          "form": "되게 손해만 본 기분이었어요 ㅋㅋ",
          "original_form": "되게 손해만 본 기분이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009304.1.15",
          "form": "다음엔 좋은 기회 있을겁니다",
          "original_form": "다음엔 좋은 기회 있을겁니다",
