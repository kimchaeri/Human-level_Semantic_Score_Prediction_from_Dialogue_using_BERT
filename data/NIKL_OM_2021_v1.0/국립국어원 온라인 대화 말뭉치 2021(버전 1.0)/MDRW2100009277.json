      "utterance": [
        {
          "id": "MDRW2100009277.1.1",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009277.1.2",
          "form": "저는 비자발적 솔로입니다..ㅋㅋㅋ",
          "original_form": "저는 비자발적 솔로입니다..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009277.1.3",
          "form": "애인 없으시죠?",
          "original_form": "애인 없으시죠?",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009277.1.4",
          "form": "ㅋㅋㅋㅋㅋ전 배우자가 있네요 ..ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ전 배우자가 있네요 ..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009277.1.5",
          "form": "비자발적은 뭐죵?",
          "original_form": "비자발적은 뭐죵?",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009277.1.6",
          "form": "아.. 애인은 없으신걸로..ㅋㅋㅋ \r\n솔로이고 싶지 않은데 솔로요..",
          "original_form": "아.. 애인은 없으신걸로..ㅋㅋㅋ \r\n솔로이고 싶지 않은데 솔로요..",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.8",
          "form": "너무 잘생겨서 거부감 막 있나봐요..",
          "original_form": "너무 잘생겨서 거부감 막 있나봐요..",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.9",
          "form": "왜요 ㅠㅠ 좋은 인연생길거예용",
          "original_form": "왜요 ㅠㅠ 좋은 인연생길거예용",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.10",
          "form": "결혼은 언제하셧나요?",
          "original_form": "결혼은 언제하셧나요?",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.12",
          "form": "남자분이신가요?ㅋㅋㅋ",
          "original_form": "남자분이신가요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009277.1.13",
          "form": "아.. 성별 이런거 안보인나봐요?",
          "original_form": "아.. 성별 이런거 안보인나봐요?",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.14",
          "form": "네.. 남자요",
          "original_form": "네.. 남자요",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.15",
          "form": "안보이는거같아요",
          "original_form": "안보이는거같아요",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.16",
          "form": "여자분이신줄 ㅋㅋㅋ",
          "original_form": "여자분이신줄 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.17",
          "form": "넘 잘생겨서 그럴거예요 ...",
          "original_form": "넘 잘생겨서 그럴거예요 ...",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.18",
          "form": "아.. 상대정보가 아예 안보이는거구낭..",
          "original_form": "아.. 상대정보가 아예 안보이는거구낭..",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.19",
          "form": "왜 여자 같앗어요?",
          "original_form": "왜 여자 같앗어요?",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009277.1.20",
          "form": "그냥 뭔가 대화가 잘통해서요 ㅎㅎ",
          "original_form": "그냥 뭔가 대화가 잘통해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009277.1.21",
          "form": "ㅋㅋㅋ 잘생긴 애들 특징있잖아요... \r\n남친보다 여친 많은거.. ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 잘생긴 애들 특징있잖아요... \r\n남친보다 여친 많은거.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009277.1.22",
          "form": "결혼 무섭지 않았나요?",
          "original_form": "결혼 무섭지 않았나요?",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
