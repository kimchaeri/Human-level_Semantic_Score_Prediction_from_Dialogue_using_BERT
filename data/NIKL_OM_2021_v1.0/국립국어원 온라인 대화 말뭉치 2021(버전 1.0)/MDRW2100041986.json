        },
        {
          "id": "MDRW2100041986.1.2",
          "form": "돈이 모자라요(?!)",
          "original_form": "돈이 모자라요(?!)",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041986.1.3",
          "form": "저도 늘 돈이 모자라요...ㅋㅋㅋ",
          "original_form": "저도 늘 돈이 모자라요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041986.1.4",
          "form": "지금 한도 없이 사실 수 있다고 하면 뭘 사시겠어요?",
          "original_form": "지금 한도 없이 사실 수 있다고 하면 뭘 사시겠어요?",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041986.1.5",
          "form": "전 부동산...",
          "original_form": "전 부동산...",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041986.1.6",
