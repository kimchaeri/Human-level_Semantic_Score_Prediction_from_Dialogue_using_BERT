      "utterance": [
        {
          "id": "MDRW2100024262.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024262.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024262.1.3",
          "form": "전 영화 보는거 좋아해요  극장은 못가고  앱으로 봐요",
          "original_form": "전 영화 보는거 좋아해요  극장은 못가고  앱으로 봐요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024262.1.4",
          "form": "뮤지컬은 비싸서 못보고..",
          "original_form": "뮤지컬은 비싸서 못보고..",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024262.1.5",
          "form": "저는 다 좋아해서 에전에는 보러갔는데",
          "original_form": "저는 다 좋아해서 에전에는 보러갔는데",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024262.1.6",
          "form": "코시국엔 진짜 못보고있어요",
          "original_form": "코시국엔 진짜 못보고있어요",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024262.1.7",
          "form": "기생충이 마지막이였어요 ㅜㅠ",
          "original_form": "기생충이 마지막이였어요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024262.1.8",
          "form": "최근엔  아들이랑 귀멸의 칼날  봤어요",
          "original_form": "최근엔  아들이랑 귀멸의 칼날  봤어요",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024262.1.9",
          "form": "귀멸의 카날은 머예요?",
          "original_form": "귀멸의 카날은 머예요?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024262.1.10",
          "form": "애니?",
          "original_form": "애니?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024262.1.11",
          "form": "네  애니에요",
          "original_form": "네  애니에요",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024262.1.12",
          "form": "아...저희애들은 아직 콩순이 극장판 볼나이라 ㅋ",
          "original_form": "아...저희애들은 아직 콩순이 극장판 볼나이라 ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024262.1.13",
          "form": "언능키워서 같이 영화볼날이 ㅎㅎ",
          "original_form": "언능키워서 같이 영화볼날이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024262.1.14",
          "form": "멀 가장 보러가고싶은세요?",
          "original_form": "멀 가장 보러가고싶은세요?",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024262.1.15",
          "form": "글쎄요...  뮤지컬 공연을 한번 보고 싶은데",
          "original_form": "글쎄요...  뮤지컬 공연을 한번 보고 싶은데",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024262.1.16",
          "form": "저는 조승우 지킬앤하이드 다시보고싶어요",
          "original_form": "저는 조승우 지킬앤하이드 다시보고싶어요",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024262.1.17",
          "form": "뮤지컬",
          "original_form": "뮤지컬",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024262.1.18",
          "form": "요즘 피아노연주도 넘 좋구요 조수미씨 목소리도 넘 아름다워요",
          "original_form": "요즘 피아노연주도 넘 좋구요 조수미씨 목소리도 넘 아름다워요",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024262.1.19",
          "form": "어제 아베마리아 들었거든요",
          "original_form": "어제 아베마리아 들었거든요",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024262.1.20",
          "form": "조수미님 소름이죠",
          "original_form": "조수미님 소름이죠",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024262.1.21",
          "form": "이제 나이도 많지않아요?",
          "original_form": "이제 나이도 많지않아요?",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024262.1.22",
          "form": "그렇죠~  사람 목소리가 아름답다 느끼게된  몇안되는 사람인것 같아요",
          "original_form": "그렇죠~  사람 목소리가 아름답다 느끼게된  몇안되는 사람인것 같아요",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024262.1.23",
          "form": "하연우랑 안예은 노래에 한때 빠져서 종일 들었는데",
          "original_form": "하연우랑 안예은 노래에 한때 빠져서 종일 들었는데",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024262.1.24",
          "form": "하연우 오랫만에 이름들어봐요",
          "original_form": "하연우 오랫만에 이름들어봐요",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024262.1.25",
          "form": "안예은 이름은 어디선가 들어본거같은데",
          "original_form": "안예은 이름은 어디선가 들어본거같은데",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024262.1.26",
          "form": "잘모르겠어요",
          "original_form": "잘모르겠어요",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024262.1.27",
          "form": "홍연이 그래도 잘 알려진 노래죠",
          "original_form": "홍연이 그래도 잘 알려진 노래죠",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024262.1.28",
          "form": "티비 오디션 프로에 나와서 가수가 됐어요  목소리가 특이해요",
          "original_form": "티비 오디션 프로에 나와서 가수가 됐어요  목소리가 특이해요",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024262.1.29",
          "form": "아 목소리들으니 알것같기도하네요",
          "original_form": "아 목소리들으니 알것같기도하네요",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100024262.1.30",
          "form": "찾아보고왔어요",
          "original_form": "찾아보고왔어요",
          "speaker_id": "1",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024262.1.31",
          "form": "K팝스타 출신이네요",
          "original_form": "K팝스타 출신이네요",
          "speaker_id": "1",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024262.1.32",
          "form": "매력적이네요",
          "original_form": "매력적이네요",
          "speaker_id": "1",
          "time": "20210831 00:37"
        }
      ]
    }
