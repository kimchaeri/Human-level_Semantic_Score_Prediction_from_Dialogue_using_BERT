      "utterance": [
        {
          "id": "MDRW2100011669.1.1",
          "form": "요즘 보시는 프로 있으세요?",
          "original_form": "요즘 보시는 프로 있으세요?",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011669.1.2",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011669.1.3",
          "form": "저 요즘 넷플로",
          "original_form": "저 요즘 넷플로",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011669.1.4",
          "form": "체인지데이즈",
          "original_form": "체인지데이즈",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011669.1.5",
          "form": "보고있어요",
          "original_form": "보고있어요",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011669.1.6",
          "form": "넷플ㅎㅎ 그거 재밌나요?",
          "original_form": "넷플ㅎㅎ 그거 재밌나요?",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011669.1.7",
          "form": "환승연애나 이거나 둘다 K유교걸에겐...",
          "original_form": "환승연애나 이거나 둘다 K유교걸에겐...",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011669.1.8",
          "form": "너무 자극적인데요",
          "original_form": "너무 자극적인데요",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011669.1.9",
          "form": "아 전 그런 일반인들 연애 뭔가 못보겠어요ㅋㅋ",
          "original_form": "아 전 그런 일반인들 연애 뭔가 못보겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.10",
          "form": "역시 남의 연애가 제일 재밌는거같기도하고...",
          "original_form": "역시 남의 연애가 제일 재밌는거같기도하고...",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.12",
          "form": "맞아요 저도 처음엔 이걸 왜볼까..",
          "original_form": "맞아요 저도 처음엔 이걸 왜볼까..",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.13",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.14",
          "form": "보다보니까 정주행...",
          "original_form": "보다보니까 정주행...",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.15",
          "form": "너무 몰입하게 돼요ㅋㅋ진짜 각본없는 현실같구ㅋㅋ",
          "original_form": "너무 몰입하게 돼요ㅋㅋ진짜 각본없는 현실같구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011669.1.16",
          "form": "저는 아 이거분명 각본같은데...",
          "original_form": "저는 아 이거분명 각본같은데...",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011669.1.17",
          "form": "이러면서 보게되더라구요",
          "original_form": "이러면서 보게되더라구요",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011669.1.18",
          "form": "티비보다는 그냥 핸드폰으로",
          "original_form": "티비보다는 그냥 핸드폰으로",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011669.1.19",
          "form": "이렇게 보는게 이제는 편하니까",
          "original_form": "이렇게 보는게 이제는 편하니까",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011669.1.20",
          "form": "넷플릭스 쓱...",
          "original_form": "넷플릭스 쓱...",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011669.1.21",
          "form": "아 그렇구나ㅋㅋㅋ그쵸 저는 그냥 지나가면서 보기ㅋㅋ",
          "original_form": "아 그렇구나ㅋㅋㅋ그쵸 저는 그냥 지나가면서 보기ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011669.1.22",
          "form": "네 보고싶을때 보니까 티비를 거의 안보게돼요ㅋㅋ",
          "original_form": "네 보고싶을때 보니까 티비를 거의 안보게돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011669.1.23",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011669.1.24",
          "form": "그리고 막 10-20분짜리로",
          "original_form": "그리고 막 10-20분짜리로",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011669.1.25",
          "form": "짧게 영상 보다보니까",
          "original_form": "짧게 영상 보다보니까",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011669.1.26",
          "form": "티비로 한 프로그램 다보는게",
          "original_form": "티비로 한 프로그램 다보는게",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011669.1.27",
          "form": "이제는 좀 힘든거같아요",
          "original_form": "이제는 좀 힘든거같아요",
          "speaker_id": "1",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011669.1.28",
          "form": "저도 그래요 영화도 끊어보구ㅠㅠ",
          "original_form": "저도 그래요 영화도 끊어보구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:25"
        }
      ]
    }
