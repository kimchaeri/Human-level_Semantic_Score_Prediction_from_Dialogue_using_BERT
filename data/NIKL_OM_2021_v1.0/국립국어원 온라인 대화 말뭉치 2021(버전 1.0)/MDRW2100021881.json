      "utterance": [
        {
          "id": "MDRW2100021881.1.1",
          "form": "이태원은 식당같은건 좋은데",
          "original_form": "이태원은 식당같은건 좋은데",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021881.1.2",
          "form": "가서 살기엔 치안이 썩 좋진 않았어요",
          "original_form": "가서 살기엔 치안이 썩 좋진 않았어요",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021881.1.3",
          "form": "그치만 카페는 맛있더라구요",
          "original_form": "그치만 카페는 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100021881.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021881.1.5",
          "form": "요즘 집에서 아무래도 많이 먹게 되는데",
          "original_form": "요즘 집에서 아무래도 많이 먹게 되는데",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021881.1.6",
          "form": "최근에 맛난거 머 드셨나요",
          "original_form": "최근에 맛난거 머 드셨나요",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021881.1.7",
          "form": "전 배달은 치킨 피자 이정도 시키고",
          "original_form": "전 배달은 치킨 피자 이정도 시키고",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021881.1.8",
          "form": "집에서 요리는 대충,, 백종원 레시피를 해먹어요",
          "original_form": "집에서 요리는 대충,, 백종원 레시피를 해먹어요",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021881.1.9",
          "form": "최근한건 김라면이네요 ㅋㅋ",
          "original_form": "최근한건 김라면이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021881.1.10",
          "form": "오 김라면???",
          "original_form": "오 김라면???",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021881.1.11",
          "form": "네 김 깨 들기름 거기에 라면스프 좀 넣고 비빈 냉랴면입니다",
          "original_form": "네 김 깨 들기름 거기에 라면스프 좀 넣고 비빈 냉랴면입니다",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021881.1.12",
          "form": "김넣고 끌이는거군요",
          "original_form": "김넣고 끌이는거군요",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021881.1.13",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021881.1.14",
          "form": "비빔에다 냉이라니",
          "original_form": "비빔에다 냉이라니",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021881.1.15",
          "form": "아뇨 김은 나중에 냉랴면에 무쳐요",
          "original_form": "아뇨 김은 나중에 냉랴면에 무쳐요",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021881.1.16",
          "form": "메밀소바맛이 나더라구요",
          "original_form": "메밀소바맛이 나더라구요",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.17",
          "form": "팔도비빔면이라는 마법의 양념이 이짜냐여",
          "original_form": "팔도비빔면이라는 마법의 양념이 이짜냐여",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.18",
          "form": "그맛이랑 달라여 ㅋㅋㅋ",
          "original_form": "그맛이랑 달라여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.19",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.20",
          "form": "그냥 맨 라면에 김 깨 들기름 설탕 식초 그리고 라면스프 쬐끔",
          "original_form": "그냥 맨 라면에 김 깨 들기름 설탕 식초 그리고 라면스프 쬐끔",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.21",
          "form": "조합이에요",
          "original_form": "조합이에요",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.22",
          "form": "들기름드가면 약간 고급진 맛 나여",
          "original_form": "들기름드가면 약간 고급진 맛 나여",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021881.1.23",
          "form": "네 그래서 라면인데 재료단가가 확올라갔어요",
          "original_form": "네 그래서 라면인데 재료단가가 확올라갔어요",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021881.1.24",
          "form": "국산 들기름 2스푼을 인당 넣었더니 ㅡㅡ",
          "original_form": "국산 들기름 2스푼을 인당 넣었더니 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021881.1.25",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021881.1.26",
          "form": "들기름 국산",
          "original_form": "들기름 국산",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021881.1.27",
          "form": "들기름 맛이써요",
          "original_form": "들기름 맛이써요",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.28",
          "form": "저는 어머니까 참기름만 쓰시는데",
          "original_form": "저는 어머니까 참기름만 쓰시는데",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.29",
          "form": "ㅠㅠ 이게 비싸가지구,,아까멱는데 아끼다 똥될까ㅏ봐 그냥 먹어어요",
          "original_form": "ㅠㅠ 이게 비싸가지구,,아까멱는데 아끼다 똥될까ㅏ봐 그냥 먹어어요",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.30",
          "form": "들기름 참기름 국산은 다 비싸요",
          "original_form": "들기름 참기름 국산은 다 비싸요",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.31",
          "form": "들기름이 약간 유통기한이 짧다고 하셨어요",
          "original_form": "들기름이 약간 유통기한이 짧다고 하셨어요",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.32",
          "form": "참기름 들기름 둘다 짧아용 한 육개월 될라나",
          "original_form": "참기름 들기름 둘다 짧아용 한 육개월 될라나",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.33",
          "form": "경상도 음식이랑 잘 안맞는지",
          "original_form": "경상도 음식이랑 잘 안맞는지",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021881.1.34",
          "form": "작게 작게 뽑아서 후딱 먹어야해여",
          "original_form": "작게 작게 뽑아서 후딱 먹어야해여",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.35",
          "form": "어머니 경상도 분이셔서요",
          "original_form": "어머니 경상도 분이셔서요",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.36",
          "form": "아항 저도 부모님이 경상도에요",
          "original_form": "아항 저도 부모님이 경상도에요",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.37",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.38",
          "form": "다 거짓말이고",
          "original_form": "다 거짓말이고",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.39",
          "form": "들기름 걍 안쓰시는듯",
          "original_form": "들기름 걍 안쓰시는듯",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.40",
          "form": "전 좋아하는 맛이예여",
          "original_form": "전 좋아하는 맛이예여",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021881.1.41",
          "form": "전 최근에 마라탕 시켜먹었는데",
          "original_form": "전 최근에 마라탕 시켜먹었는데",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.42",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.43",
          "form": "제가 원한 맛이 아니라서",
          "original_form": "제가 원한 맛이 아니라서",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.44",
          "form": "힘들었오요",
          "original_form": "힘들었오요",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.45",
          "form": "제주도 마라탕이라 좀 맛없어ㅓㅆ을까요?",
          "original_form": "제주도 마라탕이라 좀 맛없어ㅓㅆ을까요?",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.46",
          "form": "1인분인데 양이 많고 애매한 맛이라..",
          "original_form": "1인분인데 양이 많고 애매한 맛이라..",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.47",
          "form": "현지맛을 좋아하세요 한국스러운 맛을 좋아하세요?",
          "original_form": "현지맛을 좋아하세요 한국스러운 맛을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.48",
          "form": "별점 이런거 보고 시켰어야해ㅛ는데",
          "original_form": "별점 이런거 보고 시켰어야해ㅛ는데",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021881.1.49",
          "form": "현지스러움맛이여!",
          "original_form": "현지스러움맛이여!",
          "speaker_id": "2",
          "time": "20210828 23:37"
        }
      ]
    }
