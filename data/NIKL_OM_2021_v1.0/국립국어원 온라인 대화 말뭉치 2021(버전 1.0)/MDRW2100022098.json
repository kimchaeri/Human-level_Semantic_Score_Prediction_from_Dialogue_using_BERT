      "utterance": [
        {
          "id": "MDRW2100022098.1.1",
          "form": "저 그냥 동네 맛집에서 시켜먹었어요!",
          "original_form": "저 그냥 동네 맛집에서 시켜먹었어요!",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022098.1.2",
          "form": "아 그 불닭",
          "original_form": "아 그 불닭",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022098.1.3",
          "form": "잘생각하셧어요.\r\n어느브랜드죠??불닭이?",
          "original_form": "잘생각하셧어요.\r\n어느브랜드죠??불닭이?",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022098.1.4",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022098.1.5",
          "form": "동네에 맛잇는게 너무 많으신가봐요",
          "original_form": "동네에 맛잇는게 너무 많으신가봐요",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022098.1.6",
          "form": "까르보크림불닭 먹어봤어요!!!",
          "original_form": "까르보크림불닭 먹어봤어요!!!",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100022098.1.7",
          "form": "맵찔이들을 위한 거라고 하길래..",
          "original_form": "맵찔이들을 위한 거라고 하길래..",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.8",
          "form": "ㅋㅋㅋㅋㅋ 그거 엄청 순하긴하죠",
          "original_form": "ㅋㅋㅋㅋㅋ 그거 엄청 순하긴하죠",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.9",
          "form": "맛있더라구오 살짝 배신감이 들었어요",
          "original_form": "맛있더라구오 살짝 배신감이 들었어요",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.10",
          "form": "현명하신 선택이시네요",
          "original_form": "현명하신 선택이시네요",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.11",
          "form": "이런 맛있는 걸 강한 자들만 먹을 수 있었구나 이런 배신감",
          "original_form": "이런 맛있는 걸 강한 자들만 먹을 수 있었구나 이런 배신감",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그쵸. ㅜㅜㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그쵸. ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.13",
          "form": "제 사촌동생이",
          "original_form": "제 사촌동생이",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.14",
          "form": "그래서 제가 불닭을 못잃어요",
          "original_form": "그래서 제가 불닭을 못잃어요",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.15",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.16",
          "form": "먹다남은 피자에 불닭올려먹으면 맛있대요..",
          "original_form": "먹다남은 피자에 불닭올려먹으면 맛있대요..",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100022098.1.17",
          "form": "해보셨나요..",
          "original_form": "해보셨나요..",
          "speaker_id": "2",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100022098.1.18",
          "form": "오... 아뇨 근데 무조건맛잇어서 ㅋㅋㅋㅋ",
          "original_form": "오... 아뇨 근데 무조건맛잇어서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100022098.1.19",
          "form": "불닭볶음면은 떡볶이랑 섞어먹어도 맛잇더라고요",
          "original_form": "불닭볶음면은 떡볶이랑 섞어먹어도 맛잇더라고요",
          "speaker_id": "1",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100022098.1.20",
          "form": "맛있는거+맛있는거라서 그런가봐요 ㅋㅋㅋㅋ",
          "original_form": "맛있는거+맛있는거라서 그런가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:12"
        },
        {
          "id": "MDRW2100022098.1.21",
          "form": "한국에선 매운 거 잘 못 먹으면 손해인 것 같아요",
          "original_form": "한국에선 매운 거 잘 못 먹으면 손해인 것 같아요",
          "speaker_id": "2",
          "time": "20210829 03:13"
        }
      ]
