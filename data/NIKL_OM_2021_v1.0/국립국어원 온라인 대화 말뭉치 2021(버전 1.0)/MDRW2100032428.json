      "utterance": [
        {
          "id": "MDRW2100032428.1.1",
          "form": "뭐 드실계획인가요?",
          "original_form": "뭐 드실계획인가요?",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100032428.1.2",
          "form": "저 포케 먹고 싶어요!!",
          "original_form": "저 포케 먹고 싶어요!!",
          "speaker_id": "2",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100032428.1.3",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210905 18:09"
        },
        {
          "id": "MDRW2100032428.1.4",
          "form": "네네, 건강하게 맛있어요",
          "original_form": "네네, 건강하게 맛있어요",
          "speaker_id": "2",
          "time": "20210905 18:09"
        },
        {
          "id": "MDRW2100032428.1.5",
          "form": "건강한맛 좋죠",
          "original_form": "건강한맛 좋죠",
          "speaker_id": "1",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100032428.1.6",
          "form": "전 아직 뭘먹을지 못정했어요",
          "original_form": "전 아직 뭘먹을지 못정했어요",
          "speaker_id": "1",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100032428.1.7",
          "form": "아니, 저런\r\n땡기는 게 없으신가요",
          "original_form": "아니, 저런\r\n땡기는 게 없으신가요",
          "speaker_id": "2",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100032428.1.8",
          "form": "그러네요 ㅜ",
          "original_form": "그러네요 ㅜ",
          "speaker_id": "1",
          "time": "20210905 18:47"
        },
        {
          "id": "MDRW2100032428.1.9",
          "form": "좀 자극적인걸 먹어야하나",
          "original_form": "좀 자극적인걸 먹어야하나",
          "speaker_id": "1",
          "time": "20210905 18:47"
        },
        {
          "id": "MDRW2100032428.1.10",
          "form": "자극? 마라탕? 떡볶이?",
          "original_form": "자극? 마라탕? 떡볶이?",
          "speaker_id": "2",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032428.1.11",
          "form": "마라쪽이 좀 땡기긴하는데",
          "original_form": "마라쪽이 좀 땡기긴하는데",
          "speaker_id": "1",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032428.1.12",
          "form": "오늘말고 다음에요ㅋㅋ",
          "original_form": "오늘말고 다음에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032428.1.13",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032428.1.14",
          "form": "옛날엔 좀 먹었는데 지금은 맵찔이에요",
          "original_form": "옛날엔 좀 먹었는데 지금은 맵찔이에요",
          "speaker_id": "2",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032428.1.15",
          "form": "한참 안먹으면 변하긴하더라구요",
          "original_form": "한참 안먹으면 변하긴하더라구요",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032428.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
