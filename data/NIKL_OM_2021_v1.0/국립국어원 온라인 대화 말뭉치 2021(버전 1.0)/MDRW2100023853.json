        {
          "id": "MDRW2100023853.1.1",
          "form": "안녕하세요! name2은 드라마 좋아하시나요?",
          "original_form": "안녕하세요! &name2&은 드라마 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 19:00"
        },
        {
          "id": "MDRW2100023853.1.2",
          "form": "요즘 출퇴근길에 보고있어요ㅋㅋ",
          "original_form": "요즘 출퇴근길에 보고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023853.1.3",
          "form": "재밌게 보신거 있나요?",
          "original_form": "재밌게 보신거 있나요?",
          "speaker_id": "2",
          "time": "20210830 19:01"
        },
        {
          "id": "MDRW2100023853.1.4",
          "form": "저 비숲이랑 로스쿨 엄청 재밌게 봤어요 ㅎ.ㅎ!!",
          "original_form": "저 비숲이랑 로스쿨 엄청 재밌게 봤어요 ㅎ.ㅎ!!",
          "speaker_id": "1",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100023853.1.5",
          "form": "name2 최애 드라마는 뭔가요?",
          "original_form": "&name2& 최애 드라마는 뭔가요?",
          "speaker_id": "1",
          "time": "20210830 19:02"
        },
        {
          "id": "MDRW2100023853.1.6",
          "form": "저는 도깨비!!!!",
          "original_form": "저는 도깨비!!!!",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023853.1.7",
          "form": "완전좋아해요ㅠㅠ",
          "original_form": "완전좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:46"
        },
        {
          "id": "MDRW2100023853.1.8",
          "form": "헐!! 저도 재밌게 봤어요",
          "original_form": "헐!! 저도 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023853.1.9",
          "form": "이동욱이 너무 멋졌던...",
          "original_form": "이동욱이 너무 멋졌던...",
          "speaker_id": "1",
          "time": "20210830 19:47"
        },
        {
          "id": "MDRW2100023853.1.10",
          "form": "아 완전....짱이죠",
          "original_form": "아 완전....짱이죠",
          "speaker_id": "2",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023853.1.11",
          "form": "최애 캐릭터 누구셨어요! name2은,",
          "original_form": "최애 캐릭터 누구셨어요! &name2&은,",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023853.1.12",
          "form": "오늘부터 너는 나의봄이다? 보고있어요!",
          "original_form": "오늘부터 너는 나의봄이다? 보고있어요!",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023853.1.13",
          "form": "!?",
          "original_form": "!?",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023853.1.14",
          "form": "서현진 나온다해서",
          "original_form": "서현진 나온다해서",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023853.1.15",
          "form": "저는 공유ㅠㅠ",
          "original_form": "저는 공유ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023853.1.16",
          "form": "넘멋져",
          "original_form": "넘멋져",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023853.1.17",
          "form": "헛 너는 나의 봄 저도 한 6화?까지 봤어요",
          "original_form": "헛 너는 나의 봄 저도 한 6화?까지 봤어요",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023853.1.18",
          "form": "전 윤박이 넘 좋더라고요 ㅠ.^",
          "original_form": "전 윤박이 넘 좋더라고요 ㅠ.^",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023853.1.19",
          "form": "오",
