      "utterance": [
        {
          "id": "MDRW2100002800.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002800.1.2",
          "form": "네! 동물 키웁니다!",
          "original_form": "네! 동물 키웁니다!",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002800.1.3",
          "form": "고양이 1마리 키우고 있어요!",
          "original_form": "고양이 1마리 키우고 있어요!",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002800.1.4",
          "form": "털관리는요?",
          "original_form": "털관리는요?",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002800.1.5",
          "form": "돌돌이라던지 빗으로 관리중이에요!",
          "original_form": "돌돌이라던지 빗으로 관리중이에요!",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002800.1.6",
          "form": "부지런하시네여",
          "original_form": "부지런하시네여",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002800.1.7",
          "form": "동물들을 키우니",
          "original_form": "동물들을 키우니",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002800.1.8",
          "form": "부지런해지더라고요 ㅎㅎ",
          "original_form": "부지런해지더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002800.1.9",
          "form": "아.그런가요",
          "original_form": "아.그런가요",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002800.1.10",
          "form": "네 이게 애기키우는 느낌이라고 아시면 될거같아요",
          "original_form": "네 이게 애기키우는 느낌이라고 아시면 될거같아요",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002800.1.11",
          "form": "엄청 부지런해져요",
          "original_form": "엄청 부지런해져요",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002800.1.12",
          "form": "저도 키우고 싶아요",
          "original_form": "저도 키우고 싶아요",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002800.1.13",
          "form": "한번쯤 키워보시는것도",
          "original_form": "한번쯤 키워보시는것도",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002800.1.14",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002800.1.15",
          "form": "네 도전해야겠어요",
          "original_form": "네 도전해야겠어요",
          "speaker_id": "1",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002800.1.16",
          "form": "개인적으로 고양이 키우시는걸 추천드려요!",
          "original_form": "개인적으로 고양이 키우시는걸 추천드려요!",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002800.1.17",
          "form": "강아지는 해야할게 많아서",
          "original_form": "강아지는 해야할게 많아서",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002800.1.18",
          "form": "고양이 더 어려운 가 아니에여?",
          "original_form": "고양이 더 어려운 가 아니에여?",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002800.1.19",
          "form": "개인적 생각으로",
          "original_form": "개인적 생각으로",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002800.1.20",
          "form": "강아지는 훈련 부터 시켜야할게 많은데",
          "original_form": "강아지는 훈련 부터 시켜야할게 많은데",
          "speaker_id": "2",
          "time": "20210820 15:53"
        }
      ]
