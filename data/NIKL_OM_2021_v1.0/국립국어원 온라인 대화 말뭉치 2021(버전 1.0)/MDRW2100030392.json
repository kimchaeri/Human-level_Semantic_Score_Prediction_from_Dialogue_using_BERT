      "utterance": [
        {
          "id": "MDRW2100030392.1.1",
          "form": "네 맛있었슴니댜",
          "original_form": "네 맛있었슴니댜",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030392.1.2",
          "form": "사실 막입이라 다 똑같혀..",
          "original_form": "사실 막입이라 다 똑같혀..",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030392.1.3",
          "form": "스페인갔을 때 포르투갈도 갔던가???",
          "original_form": "스페인갔을 때 포르투갈도 갔던가???",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030392.1.4",
          "form": "그냥 맛만 좋으면 다 조아..",
          "original_form": "그냥 맛만 좋으면 다 조아..",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030392.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030392.1.6",
          "form": "저는 사실 파리바게트 에그타도 좋음요",
          "original_form": "저는 사실 파리바게트 에그타도 좋음요",
          "speaker_id": "1",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030392.1.7",
          "form": "네넹 진짜 맛없는 것만 아니면  빵 조아합니댜",
          "original_form": "네넹 진짜 맛없는 것만 아니면  빵 조아합니댜",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030392.1.8",
          "form": "내일 님 꿩 가나오",
          "original_form": "내일 님 꿩 가나오",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030392.1.9",
          "form": "아니 오늘 이군여",
          "original_form": "아니 오늘 이군여",
          "speaker_id": "2",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100030392.1.10",
          "form": "냉 오늘임미다",
          "original_form": "냉 오늘임미다",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.11",
          "form": "님 오늘 샹치 몇시??",
          "original_form": "님 오늘 샹치 몇시??",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.12",
          "form": "영화보고나서 뭐드심미까?",
          "original_form": "영화보고나서 뭐드심미까?",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.13",
          "form": "일요일 1시!",
          "original_form": "일요일 1시!",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.14",
          "form": "아 내일이구나",
          "original_form": "아 내일이구나",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.15",
          "form": "내일은 몰라오",
          "original_form": "내일은 몰라오",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.16",
          "form": "오늘은 뭐하나오",
          "original_form": "오늘은 뭐하나오",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.17",
          "form": "뭐먹지...",
          "original_form": "뭐먹지...",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.18",
          "form": "부추전 다 먹음???",
          "original_form": "부추전 다 먹음???",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.19",
          "form": "난 서울갈듯.....남친 목요일 1차 맞아서",
          "original_form": "난 서울갈듯.....남친 목요일 1차 맞아서",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030392.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030392.1.21",
          "form": "아녀 조금 남아서 내일 묵을 겁니댜",
          "original_form": "아녀 조금 남아서 내일 묵을 겁니댜",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030392.1.22",
          "form": "내일 대전가서 맛난거 드십셔",
          "original_form": "내일 대전가서 맛난거 드십셔",
          "speaker_id": "2",
