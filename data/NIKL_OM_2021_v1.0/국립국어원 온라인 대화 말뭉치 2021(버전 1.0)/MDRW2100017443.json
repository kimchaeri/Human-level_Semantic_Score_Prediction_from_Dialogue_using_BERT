      "utterance": [
        {
          "id": "MDRW2100017443.1.1",
          "form": "혹시 저녁은 드셨어요?",
          "original_form": "혹시 저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100017443.1.2",
          "form": "아직 안먹었어용",
          "original_form": "아직 안먹었어용",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017443.1.3",
          "form": "저 지금 김밥이 먹고 싶은데 식중독 때문에 못 먹고 있어요ㅠㅠ",
          "original_form": "저 지금 김밥이 먹고 싶은데 식중독 때문에 못 먹고 있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017443.1.4",
          "form": "오늘 집단으로 걸렸다네요",
          "original_form": "오늘 집단으로 걸렸다네요",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017443.1.5",
          "form": "김밥 여름철에 잘 상하나봐요ㅜㅜ",
          "original_form": "김밥 여름철에 잘 상하나봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017443.1.6",
          "form": "네 1시간만 있어도 위험한것같아요ㅠㅠ",
          "original_form": "네 1시간만 있어도 위험한것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017443.1.7",
          "form": "지금 뭐 먹을지 고민 중이에요 추천해 주실 만한 메뉴",
          "original_form": "지금 뭐 먹을지 고민 중이에요 추천해 주실 만한 메뉴",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017443.1.8",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017443.1.9",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017443.1.10",
          "form": "매운 거 잘 못 먹어요 엽기떡볶이 1단계도 저한테는 맵더라구요ㅠㅠㅜ",
          "original_form": "매운 거 잘 못 먹어요 엽기떡볶이 1단계도 저한테는 맵더라구요ㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017443.1.11",
          "form": "그러면 햄버거 추천드려용",
          "original_form": "그러면 햄버거 추천드려용",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017443.1.12",
          "form": "햄버거 어느 브랜드 주로 자주 드세요?",
          "original_form": "햄버거 어느 브랜드 주로 자주 드세요?",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017443.1.13",
          "form": "맥도날드요!!",
          "original_form": "맥도날드요!!",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017443.1.14",
          "form": "이번에 마늘버거 나온 거 혹시 드셔보셨어요?",
          "original_form": "이번에 마늘버거 나온 거 혹시 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017443.1.15",
          "form": "아니요 아직 안먹어봤어용",
          "original_form": "아니요 아직 안먹어봤어용",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017443.1.16",
          "form": "그거 되게 맛있다고 요즘 유명하더라구요",
          "original_form": "그거 되게 맛있다고 요즘 유명하더라구요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017443.1.17",
          "form": "마늘 때문에 안 느끼 하대요",
          "original_form": "마늘 때문에 안 느끼 하대요",
          "speaker_id": "1",
          "time": "20210826 19:31"
        }
      ]
