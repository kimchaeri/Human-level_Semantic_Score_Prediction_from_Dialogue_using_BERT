      "utterance": [
        {
          "id": "MDRW2100007784.1.1",
          "form": "안녕하세요 좋아하시는 음식 있으세요?",
          "original_form": "안녕하세요 좋아하시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210822 10:41"
        },
        {
          "id": "MDRW2100007784.1.2",
          "form": "너무 많아요ㅋㅋㅋㅋ",
          "original_form": "너무 많아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007784.1.3",
          "form": "식재료는 버섯 좋아해요",
          "original_form": "식재료는 버섯 좋아해요",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007784.1.4",
          "form": "영혼의 음식은 카레 디저트로는 단 거 사랑해요",
          "original_form": "영혼의 음식은 카레 디저트로는 단 거 사랑해요",
          "speaker_id": "2",
          "time": "20210822 10:42"
        },
        {
          "id": "MDRW2100007784.1.5",
          "form": "어떤 거 좋아하세요?",
          "original_form": "어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007784.1.6",
          "form": "디저트 단 거 진짜 좋죠 저도 단 거에 환장해요 ㅋㅋㅋ",
          "original_form": "디저트 단 거 진짜 좋죠 저도 단 거에 환장해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007784.1.7",
          "form": "ㅋㅋㅋ단 거 먹어야 기운도 나고 마지막 위도 채워져요",
          "original_form": "ㅋㅋㅋ단 거 먹어야 기운도 나고 마지막 위도 채워져요",
          "speaker_id": "2",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007784.1.8",
          "form": "마카롱도 택배가 되길래 고심 중이에요 요새ㅋㅋ",
          "original_form": "마카롱도 택배가 되길래 고심 중이에요 요새ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007784.1.9",
          "form": "와 마카롱도 택배로 보내지는군요",
          "original_form": "와 마카롱도 택배로 보내지는군요",
          "speaker_id": "1",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007784.1.10",
          "form": "저도 집근처에 마카롱가게가 있는데 가끔 사먹거든요",
          "original_form": "저도 집근처에 마카롱가게가 있는데 가끔 사먹거든요",
          "speaker_id": "1",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007784.1.11",
          "form": "오 집근처 좋겠다",
          "original_form": "오 집근처 좋겠다",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007784.1.12",
          "form": "무슨 맛이 젤 취향이신가요?",
          "original_form": "무슨 맛이 젤 취향이신가요?",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007784.1.13",
          "form": "저는 초코들어간걸 좋아하는데 우유맛인가 그것도 맛있더라고요",
          "original_form": "저는 초코들어간걸 좋아하는데 우유맛인가 그것도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007784.1.14",
          "form": "오오 우유맛은 첨 봐요",
          "original_form": "오오 우유맛은 첨 봐요",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007784.1.15",
          "form": "저 우유덕후라 설레네요ㅋㅋㅋㅋ",
          "original_form": "저 우유덕후라 설레네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007784.1.16",
          "form": "저는 레몬같이 살짝 상코미한 게 가미된 거 좋아해요",
          "original_form": "저는 레몬같이 살짝 상코미한 게 가미된 거 좋아해요",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007784.1.17",
          "form": "오 우유덕후시군요 그러면 혹시 우유에 타먹는 그런것도 좋아하세요?",
          "original_form": "오 우유덕후시군요 그러면 혹시 우유에 타먹는 그런것도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007784.1.18",
          "form": "네 완전",
          "original_form": "네 완전",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007784.1.19",
          "form": "우유에 오렌지주스 타드셔보셨어요?",
          "original_form": "우유에 오렌지주스 타드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007784.1.20",
          "form": "헉 아니요 맛있나요??",
          "original_form": "헉 아니요 맛있나요??",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007784.1.21",
          "form": "ㅇㅇ 맛나요! 은근 치즈같구",
          "original_form": "ㅇㅇ 맛나요! 은근 치즈같구",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007784.1.22",
          "form": "비졀만 조금 무시해주심 돼요ㅋㅋㅋ",
          "original_form": "비졀만 조금 무시해주심 돼요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007784.1.23",
          "form": "우유에 타 먹는 거는 모요???",
          "original_form": "우유에 타 먹는 거는 모요???",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007784.1.24",
          "form": "제가 예전어 만월회라는데에서 우유에 타먹는 걸 샀는데",
          "original_form": "제가 예전어 만월회라는데에서 우유에 타먹는 걸 샀는데",
          "speaker_id": "1",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007784.1.25",
          "form": "초코맛 말차맛 사서 먹었더니",
          "original_form": "초코맛 말차맛 사서 먹었더니",
          "speaker_id": "1",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007784.1.26",
          "form": "우유 잘 안먹는 저도 맛있게 먹을 수 있더라고요!",
          "original_form": "우유 잘 안먹는 저도 맛있게 먹을 수 있더라고요!",
          "speaker_id": "1",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007784.1.27",
          "form": "만월회 첨 들어봐요",
          "original_form": "만월회 첨 들어봐요",
          "speaker_id": "2",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007784.1.28",
          "form": "아 우유 잘 안 드시는구나...",
          "original_form": "아 우유 잘 안 드시는구나...",
          "speaker_id": "2",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007784.1.29",
          "form": "우유를 먹으면 맨날 배탈이 나더라구요 ㅠ",
          "original_form": "우유를 먹으면 맨날 배탈이 나더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007784.1.30",
          "form": "초코라떼 말차라떼 였겠어요ㅋㅋㅋ",
          "original_form": "초코라떼 말차라떼 였겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007784.1.31",
          "form": "맞아요 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007784.1.32",
          "form": "아 마자요 그런 분들 계시죠",
          "original_form": "아 마자요 그런 분들 계시죠",
          "speaker_id": "2",
          "time": "20210822 10:52"
        }
      ]
