      "utterance": [
        {
          "id": "MDRW2100006728.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100006728.1.2",
          "form": "좋아하죠",
          "original_form": "좋아하죠",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006728.1.3",
          "form": "어떤 극 좋아하세요?",
          "original_form": "어떤 극 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006728.1.4",
          "form": "최근에 옥상달빛 공연봤어요",
          "original_form": "최근에 옥상달빛 공연봤어요",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006728.1.5",
          "form": "오랜만에 공연인데다가 티켓 무료당첨돼서 너무 좋았어요",
          "original_form": "오랜만에 공연인데다가 티켓 무료당첨돼서 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006728.1.6",
          "form": "헉 진짜 좋으셨을 것 같아요ㅠㅠ",
          "original_form": "헉 진짜 좋으셨을 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006728.1.7",
          "form": "저는 연극은 가끔보러가는데",
          "original_form": "저는 연극은 가끔보러가는데",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006728.1.8",
          "form": "더 헬멧이라는 극이 기억에 남네요",
          "original_form": "더 헬멧이라는 극이 기억에 남네요",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006728.1.9",
          "form": "서울 사시나요?",
          "original_form": "서울 사시나요?",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006728.1.10",
          "form": "네 서울에 있어요!",
          "original_form": "네 서울에 있어요!",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006728.1.11",
          "form": "역시 서울이 문화생활하기 좋죠",
          "original_form": "역시 서울이 문화생활하기 좋죠",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006728.1.12",
          "form": "지역은 연극 선택폭도 좁고",
          "original_form": "지역은 연극 선택폭도 좁고",
          "speaker_id": "1",
          "time": "20210908 07:54"
        },
        {
          "id": "MDRW2100006728.1.13",
          "form": "공연 횟수도 적고",
          "original_form": "공연 횟수도 적고",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006728.1.14",
          "form": "그죠...",
          "original_form": "그죠...",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006728.1.15",
          "form": "그래도 광주에도 큰 극장이 있잖아요",
          "original_form": "그래도 광주에도 큰 극장이 있잖아요",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006728.1.16",
          "form": "몇 해 전 해변의 아인슈타인이 내한했을 때 못 본 게 너무 슬퍼요",
          "original_form": "몇 해 전 해변의 아인슈타인이 내한했을 때 못 본 게 너무 슬퍼요",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006728.1.17",
          "form": "아 그런연극이 있어요?",
          "original_form": "아 그런연극이 있어요?",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006728.1.18",
          "form": "네 유튜브에 검색하면 몇 몇 장면이 나오기는 하는데",
          "original_form": "네 유튜브에 검색하면 몇 몇 장면이 나오기는 하는데",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006728.1.19",
          "form": "아마 다시 안 올 거예요... 무대장치도 폐기한 걸로 알고 있어요",
          "original_form": "아마 다시 안 올 거예요... 무대장치도 폐기한 걸로 알고 있어요",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006728.1.20",
          "form": "다시 기회가 있을거에요",
          "original_form": "다시 기회가 있을거에요",
          "speaker_id": "1",
          "time": "20210908 08:02"
        }
