      "utterance": [
        {
          "id": "MDRW2100047658.1.1",
          "form": "그 이후로 동그랑땡은 사먹습니다.ㅋㅋ",
          "original_form": "그 이후로 동그랑땡은 사먹습니다.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100047658.1.2",
          "form": "아하 ㅋㅋ  가끔은 돈이 해결해주는게 베스트일때도있어요",
          "original_form": "아하 ㅋㅋ  가끔은 돈이 해결해주는게 베스트일때도있어요",
          "speaker_id": "2",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100047658.1.3",
          "form": "요새는 언리미트라는 비건고기제품도 나와요",
          "original_form": "요새는 언리미트라는 비건고기제품도 나와요",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100047658.1.4",
          "form": "맞아요 ㅋㅋㅋ 와 비건음식 드시나요??",
          "original_form": "맞아요 ㅋㅋㅋ 와 비건음식 드시나요??",
          "speaker_id": "1",
          "time": "20210827 00:41"
        },
        {
          "id": "MDRW2100047658.1.5",
          "form": "저는 그냥 환경에 관심이 있어서",
          "original_form": "저는 그냥 환경에 관심이 있어서",
          "speaker_id": "2",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100047658.1.6",
          "form": "고기 줄이는 제품을 이용해본적이 있어요",
          "original_form": "고기 줄이는 제품을 이용해본적이 있어요",
          "speaker_id": "2",
          "time": "20210827 00:42"
        },
        {
          "id": "MDRW2100047658.1.7",
          "form": "와 멋지세요 마인드가",
          "original_form": "와 멋지세요 마인드가",
          "speaker_id": "1",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100047658.1.8",
          "form": "쓰레기도 줄이고요ㅎㅎ",
          "original_form": "쓰레기도 줄이고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:43"
        },
        {
          "id": "MDRW2100047658.1.9",
          "form": "애들도 깨끗한환경을 오래 누려야죠",
          "original_form": "애들도 깨끗한환경을 오래 누려야죠",
          "speaker_id": "2",
          "time": "20210827 00:44"
        },
        {
          "id": "MDRW2100047658.1.10",
          "form": "맞아요ㅠㅠ 저도분리수거진짜 열심히 해요",
          "original_form": "맞아요ㅠㅠ 저도분리수거진짜 열심히 해요",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100047658.1.11",
          "form": "저둥 식구많아서 더 자주하시겠네요",
          "original_form": "저둥 식구많아서 더 자주하시겠네요",
          "speaker_id": "2",
          "time": "20210827 09:21"
        },
        {
          "id": "MDRW2100047658.1.12",
          "form": "네 신랑이 버려주긴 해요 저는 분리만ㅋㅋㅋ",
          "original_form": "네 신랑이 버려주긴 해요 저는 분리만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100047658.1.13",
          "form": "오 좋은 파트너네요",
          "original_form": "오 좋은 파트너네요",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100047658.1.14",
          "form": "글쎄요.\n그것만해줘서ㄱㅋ",
          "original_form": "글쎄요.\n그것만해줘서ㄱㅋ",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100047658.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ아...파트너가 아니고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아...파트너가 아니고",
          "speaker_id": "2",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100047658.1.16",
          "form": "혹시 큰아들인가요ㅋㅋㅋ",
          "original_form": "혹시 큰아들인가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100047658.1.17",
          "form": "아녀 웬수요 ㅋㅋ",
          "original_form": "아녀 웬수요 ㅋㅋ",
