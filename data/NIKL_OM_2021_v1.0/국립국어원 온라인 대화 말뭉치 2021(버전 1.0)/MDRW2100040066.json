      "utterance": [
        {
          "id": "MDRW2100040066.1.1",
          "form": "지금보니까 세일 안하네 ㅜㅜ",
          "original_form": "지금보니까 세일 안하네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 02:43"
        },
        {
          "id": "MDRW2100040066.1.2",
          "form": "지금 8천원 ㅜ",
          "original_form": "지금 8천원 ㅜ",
          "speaker_id": "1",
          "time": "20210904 02:43"
        },
        {
          "id": "MDRW2100040066.1.3",
          "form": "아ㅜㅜ내일도 봐봐야짘ㅋㅋ",
          "original_form": "아ㅜㅜ내일도 봐봐야짘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:43"
        },
        {
          "id": "MDRW2100040066.1.4",
          "form": "나중에 세일하는거보면 얘기해줄게 ㅋㅋㅋ",
          "original_form": "나중에 세일하는거보면 얘기해줄게 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:43"
        },
        {
          "id": "MDRW2100040066.1.5",
          "form": "오키오키 나도 봐보고 얘기할겡!",
          "original_form": "오키오키 나도 봐보고 얘기할겡!",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.6",
          "form": "너가 산 가격이 진짜",
          "original_form": "너가 산 가격이 진짜",
          "speaker_id": "1",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.7",
          "form": "요구르트 뭐샀어?",
          "original_form": "요구르트 뭐샀어?",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.8",
          "form": "싸게 산거긴 한듯",
          "original_form": "싸게 산거긴 한듯",
          "speaker_id": "1",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.10",
          "form": "싸게 산거구나!",
          "original_form": "싸게 산거구나!",
          "speaker_id": "2",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.11",
          "form": "요구하이 ㅋㅋㅋ 알아?",
          "original_form": "요구하이 ㅋㅋㅋ 알아?",
          "speaker_id": "1",
          "time": "20210904 02:44"
        },
        {
          "id": "MDRW2100040066.1.12",
          "form": "앗 좀 큰거아냐?",
          "original_form": "앗 좀 큰거아냐?",
          "speaker_id": "2",
          "time": "20210904 03:24"
        },
        {
          "id": "MDRW2100040066.1.13",
          "form": "입구가 파랑색이엇나",
          "original_form": "입구가 파랑색이엇나",
          "speaker_id": "2",
          "time": "20210904 03:25"
        },
        {
          "id": "MDRW2100040066.1.14",
          "form": "먹어본거같은데",
          "original_form": "먹어본거같은데",
          "speaker_id": "2",
          "time": "20210904 03:25"
        },
        {
          "id": "MDRW2100040066.1.15",
          "form": "어맞어맞어 ㅋㅋㅋㅋ",
          "original_form": "어맞어맞어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 03:31"
        },
        {
          "id": "MDRW2100040066.1.16",
          "form": "맛있더라구 ㅋㅋㅋㅋㅋ",
          "original_form": "맛있더라구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 03:31"
        },
        {
          "id": "MDRW2100040066.1.17",
          "form": "쿠팡에 있길래 시킴",
          "original_form": "쿠팡에 있길래 시킴",
          "speaker_id": "1",
          "time": "20210904 03:31"
        },
        {
          "id": "MDRW2100040066.1.18",
          "form": "문앞에 와있는데 다들 깰까봐 못열고있네 ㅋㅋㅋㅋ",
          "original_form": "문앞에 와있는데 다들 깰까봐 못열고있네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 03:31"
        },
        {
          "id": "MDRW2100040066.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ그거 그냥 요구르트랑 맛이 살짝 달랏던것같애",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그거 그냥 요구르트랑 맛이 살짝 달랏던것같애",
          "speaker_id": "2",
          "time": "20210904 03:46"
        },
        {
          "id": "MDRW2100040066.1.20",
          "form": "진짜그럼 속터짐ㅜㅜ",
          "original_form": "진짜그럼 속터짐ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 03:46"
        },
        {
          "id": "MDRW2100040066.1.21",
          "form": "어 조금 더 상큼함 ㅋㅋㅋㅋ",
          "original_form": "어 조금 더 상큼함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 03:54"
        },
        {
          "id": "MDRW2100040066.1.22",
          "form": "난 요즘 첵스초코에 빠짐ㅋㅋ",
          "original_form": "난 요즘 첵스초코에 빠짐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 03:54"
        },
        {
          "id": "MDRW2100040066.1.23",
          "form": "한달도 안돼서 또 삼ㅋㅋ",
          "original_form": "한달도 안돼서 또 삼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 03:55"
        },
        {
          "id": "MDRW2100040066.1.24",
          "form": "ㅋㅋㅋㅋㅋ 원래도 좋아했잖아 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 원래도 좋아했잖아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 03:55"
        }
      ]
