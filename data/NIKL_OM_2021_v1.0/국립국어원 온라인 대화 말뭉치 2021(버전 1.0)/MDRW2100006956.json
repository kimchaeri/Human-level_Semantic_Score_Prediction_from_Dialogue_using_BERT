      "utterance": [
        {
          "id": "MDRW2100006956.1.1",
          "form": "안녕하세요 당근 이용하시나요?",
          "original_form": "안녕하세요 당근 이용하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006956.1.2",
          "form": "네 몇 번 이용해봤어요",
          "original_form": "네 몇 번 이용해봤어요",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006956.1.3",
          "form": "아 혹시 뭐뭐 사셨어요?",
          "original_form": "아 혹시 뭐뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006956.1.4",
          "form": "산적은 없고 팔기만 했어요",
          "original_form": "산적은 없고 팔기만 했어요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006956.1.5",
          "form": "아 저도 산적은 없는거 같아요",
          "original_form": "아 저도 산적은 없는거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006956.1.6",
          "form": "아직은 새거가 좋더라고요 ㅎㄹㄹ",
          "original_form": "아직은 새거가 좋더라고요 ㅎㄹㄹ",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006956.1.7",
          "form": "그쵸 뭔가 꺼려지는게 있어서 저도 팔기만 했네요ㅋㅋ",
          "original_form": "그쵸 뭔가 꺼려지는게 있어서 저도 팔기만 했네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006956.1.8",
          "form": "주로 쇼핑은 어디서 하세요?",
          "original_form": "주로 쇼핑은 어디서 하세요?",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006956.1.9",
          "form": "저는 네이버쇼핑이랑 동네마트요! 지역페이가 쏠쏠해요",
          "original_form": "저는 네이버쇼핑이랑 동네마트요! 지역페이가 쏠쏠해요",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100006956.1.10",
          "form": "맞아요 저도 동네마트 좋아해요 ㅎㅎ 네이버쇼핑은 포인트 모으는 재미가 쏠쏠해요",
          "original_form": "맞아요 저도 동네마트 좋아해요 ㅎㅎ 네이버쇼핑은 포인트 모으는 재미가 쏠쏠해요",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006956.1.11",
          "form": "그쵸 혹시 네이버라이브도 보시나요?ㅋㅋ",
          "original_form": "그쵸 혹시 네이버라이브도 보시나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006956.1.12",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006956.1.13",
          "form": "폐지줍줍",
          "original_form": "폐지줍줍",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006956.1.14",
          "form": "아 그거 약간 한번 모으기시작하면 약간 집착이 되더라구요ㅎㅎ",
