      "utterance": [
        {
          "id": "MDRW2100014715.1.1",
          "form": "공연,전시회 안간지 오래됐네",
          "original_form": "공연,전시회 안간지 오래됐네",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014715.1.2",
          "form": "아 그래도 영화관은 가봤네",
          "original_form": "아 그래도 영화관은 가봤네",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014715.1.3",
          "form": "최근에 어떤 영화를 봤니",
          "original_form": "최근에 어떤 영화를 봤니",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014715.1.4",
          "form": "최근이라고 해도 두달 전이지만",
          "original_form": "최근이라고 해도 두달 전이지만",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014715.1.5",
          "form": "더 콰이어트플레이스 2를 봤어",
          "original_form": "더 콰이어트플레이스 2를 봤어",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014715.1.6",
          "form": "처음 들어보는 영화야",
          "original_form": "처음 들어보는 영화야",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014715.1.7",
          "form": "재밌었어?",
          "original_form": "재밌었어?",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014715.1.8",
          "form": "1편을 재밌게 봐서",
          "original_form": "1편을 재밌게 봐서",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014715.1.9",
          "form": "개봉했길래 봤는데",
          "original_form": "개봉했길래 봤는데",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014715.1.10",
          "form": "1편이랑 다른게 없어서 그냥그랬어",
          "original_form": "1편이랑 다른게 없어서 그냥그랬어",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014715.1.11",
          "form": "아마 3편도 나올것 같은 느낌이 들어",
          "original_form": "아마 3편도 나올것 같은 느낌이 들어",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014715.1.12",
          "form": "무슨 종류야?",
          "original_form": "무슨 종류야?",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014715.1.13",
          "form": "재난 좀비 로맨스 코믹",
          "original_form": "재난 좀비 로맨스 코믹",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014715.1.14",
          "form": "히어로 스릴러",
          "original_form": "히어로 스릴러",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014715.1.15",
          "form": "재난이지..?",
          "original_form": "재난이지..?",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014715.1.16",
          "form": "외계인도 나와",
          "original_form": "외계인도 나와",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014715.1.17",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014715.1.18",
          "form": "재밌겠네",
          "original_form": "재밌겠네",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014715.1.19",
          "form": "흥미로운 주제야",
          "original_form": "흥미로운 주제야",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014715.1.20",
          "form": "소리에 민감한 외계인이여서 영화내내 조용하게 내용이 흘러가",
          "original_form": "소리에 민감한 외계인이여서 영화내내 조용하게 내용이 흘러가",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014715.1.21",
          "form": "그건 별로인데",
          "original_form": "그건 별로인데",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014715.1.22",
          "form": "나도 공포영화인줄 알았는데",
          "original_form": "나도 공포영화인줄 알았는데",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.23",
          "form": "야단법석이 좋아",
          "original_form": "야단법석이 좋아",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.24",
          "form": "아니더라고",
          "original_form": "아니더라고",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.25",
          "form": "그래도 재밌게는 봤어",
          "original_form": "그래도 재밌게는 봤어",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.26",
          "form": "넌 공포영화도 잘보지",
          "original_form": "넌 공포영화도 잘보지",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.27",
          "form": "그럼 만족이지",
          "original_form": "그럼 만족이지",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.28",
          "form": "공포영화 이번 여름에 못 봤네",
          "original_form": "공포영화 이번 여름에 못 봤네",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.29",
          "form": "난 최근에 친구랑",
          "original_form": "난 최근에 친구랑",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.30",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.31",
          "form": "블랙위도우를 봤는데",
          "original_form": "블랙위도우를 봤는데",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.32",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.33",
          "form": "근 5년간 본 영화중에 가장 좋았어",
          "original_form": "근 5년간 본 영화중에 가장 좋았어",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014715.1.34",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.35",
          "form": "그정도로 재밌었어!",
          "original_form": "그정도로 재밌었어!",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.36",
          "form": "심다친구랑 너무 재밌어서 방방 뛰면서 봤어",
          "original_form": "심다친구랑 너무 재밌어서 방방 뛰면서 봤어",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.37",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.38",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.39",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.40",
          "form": "그정도 였구나",
          "original_form": "그정도 였구나",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.41",
          "form": "여자주인공이 다 해먹고",
          "original_form": "여자주인공이 다 해먹고",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.42",
          "form": "오오 멋있다",
          "original_form": "오오 멋있다",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.43",
          "form": "여자주인공의 여자 지인들이 다 해",
          "original_form": "여자주인공의 여자 지인들이 다 해",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.44",
          "form": "걸크러쉬야",
          "original_form": "걸크러쉬야",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.45",
          "form": "근데 웃겨",
          "original_form": "근데 웃겨",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.46",
          "form": "너무 멋있다",
          "original_form": "너무 멋있다",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014715.1.47",
          "form": "코믹을 놓치지 않았어",
          "original_form": "코믹을 놓치지 않았어",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.48",
          "form": "궁금해졌어",
          "original_form": "궁금해졌어",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.49",
          "form": "갑자기 영화관 가고 싶어졌다",
          "original_form": "갑자기 영화관 가고 싶어졌다",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.50",
          "form": "심다랑 웃다가 껄껄할뻔",
          "original_form": "심다랑 웃다가 껄껄할뻔",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.51",
          "form": "요즘은 상영 뭐 하지",
          "original_form": "요즘은 상영 뭐 하지",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.52",
          "form": "정말 추천해",
          "original_form": "정말 추천해",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.53",
          "form": "나 디비디로 집에서 다시 볼려고",
          "original_form": "나 디비디로 집에서 다시 볼려고",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.54",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.55",
          "form": "진짜 재밌었나보다",
          "original_form": "진짜 재밌었나보다",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.56",
          "form": "분명 너 맘에도 들꺼야",
          "original_form": "분명 너 맘에도 들꺼야",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014715.1.57",
          "form": "참고로 난 마블 영화를 싫어해",
          "original_form": "참고로 난 마블 영화를 싫어해",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.58",
          "form": "나중에 넷플릭스에 나오면 봐야겠다",
          "original_form": "나중에 넷플릭스에 나오면 봐야겠다",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.59",
          "form": "하지만 좋았지",
          "original_form": "하지만 좋았지",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.60",
          "form": "그정도라니",
          "original_form": "그정도라니",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.61",
          "form": "대화가 끝났어",
          "original_form": "대화가 끝났어",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.62",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.63",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.64",
          "form": "너가 주제를 골라보련",
          "original_form": "너가 주제를 골라보련",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014715.1.65",
          "form": "그만할래..",
          "original_form": "그만할래..",
          "speaker_id": "2",
