      "utterance": [
        {
          "id": "MDRW2100008326.1.1",
          "form": "즐겨보시는 것 있나요?",
          "original_form": "즐겨보시는 것 있나요?",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008326.1.2",
          "form": "전 유튜브 자주 봐요!",
          "original_form": "전 유튜브 자주 봐요!",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008326.1.3",
          "form": "즐겨보시는 유튜버 있나요?",
          "original_form": "즐겨보시는 유튜버 있나요?",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008326.1.4",
          "form": "음 여러가지 많이봐요!",
          "original_form": "음 여러가지 많이봐요!",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008326.1.5",
          "form": "게임방송도 보고",
          "original_form": "게임방송도 보고",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008326.1.6",
          "form": "먹방도 보고",
          "original_form": "먹방도 보고",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008326.1.7",
          "form": "무슨 유튜브 많이 보세요?",
          "original_form": "무슨 유튜브 많이 보세요?",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008326.1.8",
          "form": "저는 주로 겜방이요!",
          "original_form": "저는 주로 겜방이요!",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008326.1.9",
          "form": "직접하는 거 말고 보는 것도 재밌더라구요 ㅋㅋㅋㅋ",
          "original_form": "직접하는 거 말고 보는 것도 재밌더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008326.1.10",
          "form": "오 무슨게임 좋아하세요?",
          "original_form": "오 무슨게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008326.1.11",
          "form": "저도 보는거 좋아해요",
          "original_form": "저도 보는거 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100008326.1.12",
          "form": "저는 마인크래프트로 컨텐츠 만드는 거 주로 봐요!",
          "original_form": "저는 마인크래프트로 컨텐츠 만드는 거 주로 봐요!",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008326.1.13",
          "form": "그 외에는 스토리겜 좋아해요",
          "original_form": "그 외에는 스토리겜 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008326.1.14",
          "form": "오 저도 마크방송 보는데 ㅋㅋㅋ",
          "original_form": "오 저도 마크방송 보는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008326.1.15",
          "form": "자빱 보세요?",
          "original_form": "자빱 보세요?",
          "speaker_id": "2",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008326.1.16",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008326.1.17",
          "form": "제가 유트브 보게 된 계기가 자빱이에요",
          "original_form": "제가 유트브 보게 된 계기가 자빱이에요",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008326.1.18",
          "form": "아 진짜 재밌죠 ㅋㅋ",
          "original_form": "아 진짜 재밌죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008326.1.19",
          "form": "컨텐츠 다 챙겨보셨어요?",
          "original_form": "컨텐츠 다 챙겨보셨어요?",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008326.1.20",
          "form": "네 우정리2까지 다 봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 우정리2까지 다 봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008326.1.21",
          "form": "자매꽃 할 때 처음 접하고 그 뒤로 챙겨봤어요",
          "original_form": "자매꽃 할 때 처음 접하고 그 뒤로 챙겨봤어요",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008326.1.22",
          "form": "우정리2 보셨나요??",
          "original_form": "우정리2 보셨나요??",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008326.1.23",
          "form": "아 보고있죠!!",
          "original_form": "아 보고있죠!!",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008326.1.24",
          "form": "진짜 너무 재밌어요",
          "original_form": "진짜 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008326.1.25",
          "form": "하... 우리.. 친구좌나 얘드라.....",
          "original_form": "하... 우리.. 친구좌나 얘드라.....",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008326.1.26",
          "form": "하..우정리가 매정리가 되었죠..",
          "original_form": "하..우정리가 매정리가 되었죠..",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008326.1.27",
          "form": "우정리1 생방 못 봐서 아쉬웠는데",
          "original_form": "우정리1 생방 못 봐서 아쉬웠는데",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008326.1.28",
          "form": "우정리2는 챙겨볼 수 있어서 정말 좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "우정리2는 챙겨볼 수 있어서 정말 좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:01"
        },
        {
          "id": "MDRW2100008326.1.29",
          "form": "아 저도여 ㅠㅠㅠ",
          "original_form": "아 저도여 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008326.1.30",
          "form": "저는 서리사막부터 생방 챙겨봐서",
          "original_form": "저는 서리사막부터 생방 챙겨봐서",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008326.1.31",
          "form": "다른 것들 생방으로 못본게 너무 아숴요 ㅠㅠ",
          "original_form": "다른 것들 생방으로 못본게 너무 아숴요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008326.1.32",
          "form": "서리사막도 대박이었죠",
          "original_form": "서리사막도 대박이었죠",
          "speaker_id": "1",
          "time": "20210822 16:05"
        }
      ]
