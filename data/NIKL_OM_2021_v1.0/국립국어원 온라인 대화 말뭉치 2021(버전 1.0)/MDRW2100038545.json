      "utterance": [
        {
          "id": "MDRW2100038545.1.1",
          "form": "바프 찍는 데 속옷을 따로 사야해서",
          "original_form": "바프 찍는 데 속옷을 따로 사야해서",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038545.1.2",
          "form": "그 어디더라 미국 브랜드...",
          "original_form": "그 어디더라 미국 브랜드...",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038545.1.3",
          "form": "맞아요 뭔 이벤트속옷",
          "original_form": "맞아요 뭔 이벤트속옷",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038545.1.4",
          "form": "빅시여?",
          "original_form": "빅시여?",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038545.1.5",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038545.1.6",
          "form": "빅토리아 시크릿에서 직구하고",
          "original_form": "빅토리아 시크릿에서 직구하고",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038545.1.7",
          "form": "전신제모도 받고",
          "original_form": "전신제모도 받고",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.8",
          "form": "패디큐어랑 네일도 하고",
          "original_form": "패디큐어랑 네일도 하고",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.9",
          "form": "네 가터벨트입고",
          "original_form": "네 가터벨트입고",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.10",
          "form": "아휴ㅜㅜ",
          "original_form": "아휴ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.11",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.12",
          "form": "메이크업도 따로 받고",
          "original_form": "메이크업도 따로 받고",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.13",
          "form": "진짜 뭔 av화보집이에요",
          "original_form": "진짜 뭔 av화보집이에요",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.14",
          "form": "그냥....바비인형이 되고 싶은건지",
          "original_form": "그냥....바비인형이 되고 싶은건지",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.15",
          "form": "그니까요;",
          "original_form": "그니까요;",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.16",
          "form": "아니 남자들이 그거의 반이라도 하냐구요ㅠ",
          "original_form": "아니 남자들이 그거의 반이라도 하냐구요ㅠ",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.17",
          "form": "그게 나중에 봤을때 민망할거같은데",
          "original_form": "그게 나중에 봤을때 민망할거같은데",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.18",
          "form": "다 자기만족이라면서 하는데 솔직히",
          "original_form": "다 자기만족이라면서 하는데 솔직히",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.19",
          "form": "그니까요 ㅋㅋㅋ 흑역사 아닌가요",
          "original_form": "그니까요 ㅋㅋㅋ 흑역사 아닌가요",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038545.1.20",
          "form": "과연 그게 자기만족일지 자문해봐야해요",
          "original_form": "과연 그게 자기만족일지 자문해봐야해요",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038545.1.21",
          "form": "자기만족이면서 내시선으로본느게아니라",
          "original_form": "자기만족이면서 내시선으로본느게아니라",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038545.1.22",
          "form": "남의시선으로보는 나를 만들잖아요",
          "original_form": "남의시선으로보는 나를 만들잖아요",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038545.1.23",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038545.1.24",
          "form": "name1님이랑 대화가 잘 통하네요!!!",
          "original_form": "&name1&님이랑 대화가 잘 통하네요!!!",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038545.1.25",
          "form": "이게 진짜 내가 바라보는건지 남을의식하며 보는거지 알필요가있다봐요ㅜ",
          "original_form": "이게 진짜 내가 바라보는건지 남을의식하며 보는거지 알필요가있다봐요ㅜ",
          "speaker_id": "1",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038545.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038545.1.27",
          "form": "저도 어릴땐 그렇게 시선의식많이했었거든여",
          "original_form": "저도 어릴땐 그렇게 시선의식많이했었거든여",
          "speaker_id": "1",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038545.1.28",
          "form": "사실 저도 옛날에는 외모강박이 있었는데",
          "original_form": "사실 저도 옛날에는 외모강박이 있었는데",
