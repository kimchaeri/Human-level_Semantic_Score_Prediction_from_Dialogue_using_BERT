      "utterance": [
        {
          "id": "MDRW2100028286.1.1",
          "form": "학교 생활이 그립니?ㅋㅋㅋㅋ",
          "original_form": "학교 생활이 그립니?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028286.1.2",
          "form": "학교보다는 회사가 나아",
          "original_form": "학교보다는 회사가 나아",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028286.1.3",
          "form": "학교 생활은 끔찍했어",
          "original_form": "학교 생활은 끔찍했어",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028286.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028286.1.5",
          "form": "학교는 돈을 안 주니까",
          "original_form": "학교는 돈을 안 주니까",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028286.1.6",
          "form": "과제도 해야 해",
          "original_form": "과제도 해야 해",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028286.1.7",
          "form": "맞아 하지만 방학이 있어서 좋았어",
          "original_form": "맞아 하지만 방학이 있어서 좋았어",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028286.1.8",
          "form": "맞아 학교 다닐 때는 그게좋지",
          "original_form": "맞아 학교 다닐 때는 그게좋지",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028286.1.9",
          "form": "회사도 방학 있으면 좋겠어",
          "original_form": "회사도 방학 있으면 좋겠어",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028286.1.10",
          "form": "맞아 학교처럼",
          "original_form": "맞아 학교처럼",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028286.1.11",
          "form": "넌 방학때 주로 뭐했니",
          "original_form": "넌 방학때 주로 뭐했니",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028286.1.12",
          "form": "숙제했지 너는?",
          "original_form": "숙제했지 너는?",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028286.1.13",
          "form": "나도 숙제했어",
          "original_form": "나도 숙제했어",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028286.1.14",
          "form": "그리고 학원다녔오",
          "original_form": "그리고 학원다녔오",
          "speaker_id": "2",
          "time": "20210902 20:02"
        },
        {
          "id": "MDRW2100028286.1.15",
