      "utterance": [
        {
          "id": "MDRW2100043893.1.1",
          "form": "스파게티덕에 용서받은겨",
          "original_form": "스파게티덕에 용서받은겨",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043893.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043893.1.3",
          "form": "그럼 다음에 짱 맛았는 스파게티 같이 먹으러 가 줘",
          "original_form": "그럼 다음에 짱 맛았는 스파게티 같이 먹으러 가 줘",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043893.1.4",
          "form": "맛있는",
          "original_form": "맛있는",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043893.1.5",
          "form": "좋아 스파게티",
          "original_form": "좋아 스파게티",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043893.1.6",
          "form": "넌 면이 굵은게 좋니",
          "original_form": "넌 면이 굵은게 좋니",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043893.1.7",
          "form": "아직 제일 기본 스파게티면밖에 안 먹어봤어",
          "original_form": "아직 제일 기본 스파게티면밖에 안 먹어봤어",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043893.1.8",
          "form": "다른 모양의 파스타는 안 먹어봤어",
          "original_form": "다른 모양의 파스타는 안 먹어봤어",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043893.1.9",
          "form": "난 두꺼운면도 좋더라",
          "original_form": "난 두꺼운면도 좋더라",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043893.1.10",
          "form": "그 넓적한 면?",
          "original_form": "그 넓적한 면?",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043893.1.11",
          "form": "응 얇은면은 짜더라",
          "original_form": "응 얇은면은 짜더라",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043893.1.12",
          "form": "헐 진짜? 오히려 두꺼운 면이 소스가 많이 묻어서 짠 줄 알았는데",
          "original_form": "헐 진짜? 오히려 두꺼운 면이 소스가 많이 묻어서 짠 줄 알았는데",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043893.1.13",
          "form": "얇은면이 소스를 더 많이 가져가",
          "original_form": "얇은면이 소스를 더 많이 가져가",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043893.1.14",
          "form": "아 그리고 중간에 구멍난 면",
          "original_form": "아 그리고 중간에 구멍난 면",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043893.1.15",
          "form": "의외다 그렇구나.. 처음 알았다ㅋㅋㅋ",
          "original_form": "의외다 그렇구나.. 처음 알았다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043893.1.16",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043893.1.17",
          "form": "그게 제일 짜",
          "original_form": "그게 제일 짜",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043893.1.18",
          "form": "짜면 맛있던데",
          "original_form": "짜면 맛있던데",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043893.1.19",
          "form": "해롭고 맛있어",
          "original_form": "해롭고 맛있어",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043893.1.20",
          "form": "물많이 마셔야겠어",
          "original_form": "물많이 마셔야겠어",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043893.1.21",
          "form": "ㅋㅋㅋㅋㅋ다음에 구멍난 면도 먹어볼게",
          "original_form": "ㅋㅋㅋㅋㅋ다음에 구멍난 면도 먹어볼게",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043893.1.22",
          "form": "짠건 목이 따가워",
          "original_form": "짠건 목이 따가워",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043893.1.23",
          "form": "그것도 그래 해롭고 맛있고 괴로워",
          "original_form": "그것도 그래 해롭고 맛있고 괴로워",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043893.1.24",
          "form": "난 그래서 그런가 좀 덜짠게 좋아졌어",
          "original_form": "난 그래서 그런가 좀 덜짠게 좋아졌어",
          "speaker_id": "2",
          "time": "20210907 20:53"
