      "utterance": [
        {
          "id": "MDRW2100039850.1.1",
          "form": "저두 재택중에.  진짜",
          "original_form": "저두 재택중에.  진짜",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039850.1.2",
          "form": "아니 이런",
          "original_form": "아니 이런",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039850.1.3",
          "form": "아침 점심 저녁 3끼 그리구 디저트까지",
          "original_form": "아침 점심 저녁 3끼 그리구 디저트까지",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039850.1.4",
          "form": "배딜 시켜먹었어요..",
          "original_form": "배딜 시켜먹었어요..",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039850.1.5",
          "form": "오마이갓 디저트 근데 ㄹㅇ 기본으로 자리잡는다구요",
          "original_form": "오마이갓 디저트 근데 ㄹㅇ 기본으로 자리잡는다구요",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039850.1.6",
          "form": "밥 다 먹었다?",
          "original_form": "밥 다 먹었다?",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039850.1.7",
          "form": "부른 배 통통 두드리며...",
          "original_form": "부른 배 통통 두드리며...",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039850.1.8",
          "form": "디저트 찾기... ^.ㅠ",
          "original_form": "디저트 찾기... ^.ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039850.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그니가여..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 그니가여..",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039850.1.10",
          "form": "배딜 액삭제해놓고 아 괜히 삭제했다고 다시 깔고있는 나를 발견..",
          "original_form": "배딜 액삭제해놓고 아 괜히 삭제했다고 다시 깔고있는 나를 발견..",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039850.1.11",
          "form": "아... name2님 완전 저 보는 거 같어요...",
          "original_form": "아... &name2&님 완전 저 보는 거 같어요...",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039850.1.12",
          "form": "삭제한다? 다음끼에 바로 다시 깔아요",
          "original_form": "삭제한다? 다음끼에 바로 다시 깔아요",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039850.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 그니가요 ㅠㅠ 어케하면 좋나요..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 그니가요 ㅠㅠ 어케하면 좋나요..",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039850.1.14",
          "form": "글구 배달 어플쓰면 진짜 돈이 줄줄새요..",
          "original_form": "글구 배달 어플쓰면 진짜 돈이 줄줄새요..",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039850.1.15",
          "form": "아 그러니까요!!!! 저 저번에 잔고 없어서 봤는데",
          "original_form": "아 그러니까요!!!! 저 저번에 잔고 없어서 봤는데",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039850.1.16",
          "form": "다 제가 먹은 배달비가 맞더라고요",
          "original_form": "다 제가 먹은 배달비가 맞더라고요",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039850.1.17",
          "form": "그니가요..저도.. 맨날 왤케 돈이없지?",
          "original_form": "그니가요..저도.. 맨날 왤케 돈이없지?",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039850.1.18",
          "form": "1 먹을거",
          "original_form": "1 먹을거",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039850.1.19",
          "form": "2 먹을거.. 먹을거로 이미 다 썼고요..",
          "original_form": "2 먹을거.. 먹을거로 이미 다 썼고요..",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039850.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 저랑 완전 똑같어요 ^.^ 나참 행복하려고 먹는 거니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 저랑 완전 똑같어요 ^.^ 나참 행복하려고 먹는 거니까",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039850.1.21",
          "form": "슬쩍 눈감아주자고요",
          "original_form": "슬쩍 눈감아주자고요",
          "speaker_id": "1",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039850.1.22",
          "form": "휴.. 얼른 곧 휴 바사삭이나 먹고싶어요 저..",
          "original_form": "휴.. 얼른 곧 휴 바사삭이나 먹고싶어요 저..",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039850.1.23",
          "form": "배달비 한번 아껴봅시다..",
          "original_form": "배달비 한번 아껴봅시다..",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039850.1.24",
          "form": "아놔 금지어 걸리는 거 너무 웃기구만요",
          "original_form": "아놔 금지어 걸리는 거 너무 웃기구만요",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.25",
          "form": "저도 곧  휴 바사삭 완전 사랑해서",
          "original_form": "저도 곧  휴 바사삭 완전 사랑해서",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.26",
          "form": "^.^ 기대하고 있답니다요",
          "original_form": "^.^ 기대하고 있답니다요",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.27",
          "form": "곧  휴 바사삭 잘게 잘라서 치밥 해먹어도",
          "original_form": "곧  휴 바사삭 잘게 잘라서 치밥 해먹어도",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.28",
          "form": "맛난 거 아시나요!",
          "original_form": "맛난 거 아시나요!",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도요.. 제 최애치킨 곧 휴 바사삭이라구요!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도요.. 제 최애치킨 곧 휴 바사삭이라구요!!!!",
          "speaker_id": "2",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.30",
          "form": "앗 맞아요!!!!!!!!",
          "original_form": "앗 맞아요!!!!!!!!",
          "speaker_id": "2",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.31",
          "form": "근데 살짝 그럴게 남기려면 두마리 시켜야한다는점",
          "original_form": "근데 살짝 그럴게 남기려면 두마리 시켜야한다는점",
          "speaker_id": "2",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.32",
          "form": "명심해여합니다..",
          "original_form": "명심해여합니다..",
          "speaker_id": "2",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039850.1.33",
          "form": "전 긍구 곧 휴 바사삭에서 같이주는 소스가 너무 좋아요",
          "original_form": "전 긍구 곧 휴 바사삭에서 같이주는 소스가 너무 좋아요",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039850.1.34",
          "form": "그 소스가ㅠ너무 맛있어요",
          "original_form": "그 소스가ㅠ너무 맛있어요",
          "speaker_id": "2",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039850.1.35",
          "form": "아 고블링 마블링 녀석들 말씀하시는 거군요",
          "original_form": "아 고블링 마블링 녀석들 말씀하시는 거군요",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039850.1.36",
          "form": "맞아요 전 소스 듀개씩 먹어요 ㅎ.ㅎ !!!",
          "original_form": "맞아요 전 소스 듀개씩 먹어요 ㅎ.ㅎ !!!",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039850.1.37",
          "form": "name2님 1인 1닭 하시나요?",
          "original_form": "&name2&님 1인 1닭 하시나요?",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039850.1.38",
          "form": "전... 쌉가능입니댜",
          "original_form": "전... 쌉가능입니댜",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039850.1.39",
