      "utterance": [
        {
          "id": "MDRW2100007559.1.1",
          "form": "와 종교? 저 무굔데 요즘 불교에 관심 가요",
          "original_form": "와 종교? 저 무굔데 요즘 불교에 관심 가요",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007559.1.2",
          "form": "ㅋㅋㅋㅋ 그러세요? 저도 무교예요",
          "original_form": "ㅋㅋㅋㅋ 그러세요? 저도 무교예요",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007559.1.3",
          "form": "왜 불교에 관심 가세요?",
          "original_form": "왜 불교에 관심 가세요?",
          "speaker_id": "2",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007559.1.4",
          "form": "불교는",
          "original_form": "불교는",
          "speaker_id": "1",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007559.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007559.1.6",
          "form": "경건해요...",
          "original_form": "경건해요...",
          "speaker_id": "1",
          "time": "20210822 03:35"
        },
        {
          "id": "MDRW2100007559.1.7",
          "form": "ㅋㅋㅋㅋㅋ 그런가요 절에 가보셨어요? 왜 경건하다고 느끼셨나요",
          "original_form": "ㅋㅋㅋㅋㅋ 그런가요 절에 가보셨어요? 왜 경건하다고 느끼셨나요",
          "speaker_id": "2",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007559.1.8",
          "form": "경건...경건보다는.....",
          "original_form": "경건...경건보다는.....",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007559.1.9",
          "form": "자연친화적이라 좋아요",
          "original_form": "자연친화적이라 좋아요",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007559.1.10",
          "form": "성당은 다녀봤는데 제 취향 아니었어서ㅜㅜ",
          "original_form": "성당은 다녀봤는데 제 취향 아니었어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 03:37"
        },
        {
          "id": "MDRW2100007559.1.11",
          "form": "아하!! 맞아요 절은 산속에 있고 공기도 좋고 그러니까 마음이 더 편한것같아요",
          "original_form": "아하!! 맞아요 절은 산속에 있고 공기도 좋고 그러니까 마음이 더 편한것같아요",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007559.1.12",
          "form": "저도 엄마따라서는 가끔 가거든요",
          "original_form": "저도 엄마따라서는 가끔 가거든요",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007559.1.13",
          "form": "엄마도 무교긴한데ㅋㅋㅋㅋ",
          "original_form": "엄마도 무교긴한데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007559.1.14",
          "form": "성당은 어때요? 한두번밖에 안가봤는데",
          "original_form": "성당은 어때요? 한두번밖에 안가봤는데",
          "speaker_id": "2",
          "time": "20210822 03:40"
        },
        {
          "id": "MDRW2100007559.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ절 그냥 구경하기 좋죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ절 그냥 구경하기 좋죠",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007559.1.16",
          "form": "성당은...홀리해요",
          "original_form": "성당은...홀리해요",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007559.1.17",
          "form": "그리고 노잼...인데 지금가면 재밌을지도? 고해성사가 싫어서 나왔어요",
          "original_form": "그리고 노잼...인데 지금가면 재밌을지도? 고해성사가 싫어서 나왔어요",
          "speaker_id": "1",
          "time": "20210822 03:41"
        },
        {
          "id": "MDRW2100007559.1.18",
          "form": "아하 저는 고해성사 해보고 싶던데..",
          "original_form": "아하 저는 고해성사 해보고 싶던데..",
          "speaker_id": "2",
          "time": "20210822 03:54"
        },
        {
          "id": "MDRW2100007559.1.19",
          "form": "왜 싫었어요?",
          "original_form": "왜 싫었어요?",
          "speaker_id": "2",
          "time": "20210822 03:55"
        },
        {
          "id": "MDRW2100007559.1.20",
          "form": "그게",
          "original_form": "그게",
          "speaker_id": "1",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007559.1.21",
          "form": "어차피 다 티나요 저인거",
          "original_form": "어차피 다 티나요 저인거",
          "speaker_id": "1",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007559.1.22",
          "form": "쪽팔려요ㅠ",
          "original_form": "쪽팔려요ㅠ",
          "speaker_id": "1",
          "time": "20210822 04:00"
        },
        {
          "id": "MDRW2100007559.1.23",
          "form": "아하.. 비밀로는 해도 계시는 목사님(?) 이시니까..",
          "original_form": "아하.. 비밀로는 해도 계시는 목사님(?) 이시니까..",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007559.1.24",
          "form": "성당에서는 목사님이 아니라 뭐라고 하나요.. 아 신부님!!",
          "original_form": "성당에서는 목사님이 아니라 뭐라고 하나요.. 아 신부님!!",
          "speaker_id": "2",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007559.1.25",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 04:07"
        },
        {
          "id": "MDRW2100007559.1.26",
          "form": "그리고 성당은 지역당 하나밖에 없어서...",
          "original_form": "그리고 성당은 지역당 하나밖에 없어서...",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007559.1.27",
          "form": "다 아는 사이에 갑자기 고백하는 느낌이ㅠㅠ",
          "original_form": "다 아는 사이에 갑자기 고백하는 느낌이ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007559.1.28",
          "form": "고해성사x 고백성사o....",
          "original_form": "고해성사x 고백성사o....",
          "speaker_id": "1",
          "time": "20210822 04:08"
        },
        {
          "id": "MDRW2100007559.1.29",
          "form": "ㅋㅋㅋㅋ 그랬군요.. ㅋㅋㅋㅋㅋ 저도 호기심에라도 성당에 가게 된다면 유념할게요",
          "original_form": "ㅋㅋㅋㅋ 그랬군요.. ㅋㅋㅋㅋㅋ 저도 호기심에라도 성당에 가게 된다면 유념할게요",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007559.1.30",
          "form": "고해성사는 꼭 해야한다고 들었는데",
          "original_form": "고해성사는 꼭 해야한다고 들었는데",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007559.1.31",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210822 04:09"
        },
        {
          "id": "MDRW2100007559.1.32",
          "form": "옹 그건 아니예요",
          "original_form": "옹 그건 아니예요",
          "speaker_id": "1",
          "time": "20210822 04:11"
        },
        {
          "id": "MDRW2100007559.1.33",
          "form": "영성체받으면 하라고 하긴하는데",
          "original_form": "영성체받으면 하라고 하긴하는데",
          "speaker_id": "1",
          "time": "20210822 04:11"
        },
        {
          "id": "MDRW2100007559.1.34",
          "form": "ㅋㅋㅋ고해성사가 명목상... 익명이니까 공식적으로 확인할 방도가 없어요",
          "original_form": "ㅋㅋㅋ고해성사가 명목상... 익명이니까 공식적으로 확인할 방도가 없어요",
