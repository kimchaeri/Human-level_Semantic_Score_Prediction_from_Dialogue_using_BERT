      "utterance": [
        {
          "id": "MDRW2100012091.1.1",
          "form": "안녕하세요! name2님은 먹는 거 좋아하시나요 :> ?",
          "original_form": "안녕하세요! &name2&님은 먹는 거 좋아하시나요 :> ?",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012091.1.2",
          "form": "먹는낙으로 살죠ㅎㅎ",
          "original_form": "먹는낙으로 살죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012091.1.3",
          "form": "헐 저도요! 가장 좋아하는 음식이 뭐예요!?",
          "original_form": "헐 저도요! 가장 좋아하는 음식이 뭐예요!?",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012091.1.4",
          "form": "저녁드셨어요?",
          "original_form": "저녁드셨어요?",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012091.1.5",
          "form": "전 고기요ㅋ최애음식",
          "original_form": "전 고기요ㅋ최애음식",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012091.1.6",
          "form": "헐 저도 고기 좋아해요!",
          "original_form": "헐 저도 고기 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012091.1.7",
          "form": "저녁 아직 안 먹었는데 간단하게 과일 먹으려고요 ㅎㄹ",
          "original_form": "저녁 아직 안 먹었는데 간단하게 과일 먹으려고요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012091.1.8",
          "form": "과일로 때우시는거예요?",
          "original_form": "과일로 때우시는거예요?",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012091.1.9",
          "form": "무슨과일 좋아해요?",
          "original_form": "무슨과일 좋아해요?",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012091.1.10",
          "form": "전 복숭아랑 샤인머스캣 정말 좋아해요!",
          "original_form": "전 복숭아랑 샤인머스캣 정말 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012091.1.11",
          "form": "마카롱님은요?",
          "original_form": "마카롱님은요?",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012091.1.12",
          "form": "비싸서 못먹죠ㅋ 샤인머스캣",
          "original_form": "비싸서 못먹죠ㅋ 샤인머스캣",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012091.1.13",
          "form": "히히 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ 너무 비싸더라고요",
          "original_form": "히히 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ 너무 비싸더라고요",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012091.1.14",
          "form": "요샌 수박도 엄청 비싸잖아요",
          "original_form": "요샌 수박도 엄청 비싸잖아요",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012091.1.15",
          "form": "망고도 맛있는데 비싸고ㅎ",
          "original_form": "망고도 맛있는데 비싸고ㅎ",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012091.1.16",
          "form": "헐 맞아요 ㅠ ㅠ 전 그래서 냉동 망고를 자주 먹어요!",
          "original_form": "헐 맞아요 ㅠ ㅠ 전 그래서 냉동 망고를 자주 먹어요!",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012091.1.17",
          "form": "name2님도 냉동망고 드시나요?",
          "original_form": "&name2&님도 냉동망고 드시나요?",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012091.1.18",
          "form": "가끔사먹는데 냉동은 잘안먹게 되더라구요",
          "original_form": "가끔사먹는데 냉동은 잘안먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012091.1.19",
          "form": "아무래도 냉동보다는 생과일이 확실히 맛있죠 ㅠ",
          "original_form": "아무래도 냉동보다는 생과일이 확실히 맛있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012091.1.20",
          "form": "돈이없네요ㅋ",
          "original_form": "돈이없네요ㅋ",
          "speaker_id": "2",
          "time": "20210824 22:20"
        }
      ]
