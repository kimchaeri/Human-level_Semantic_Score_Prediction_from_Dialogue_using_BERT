        {
          "id": "MDRW2100011955.1.1",
          "form": "헐나도",
          "original_form": "헐나도",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011955.1.2",
          "form": "아이슬란드 가보고싶어",
          "original_form": "아이슬란드 가보고싶어",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011955.1.3",
          "form": "겨울에 아이슬란드 영국 돌고오면되겠다",
          "original_form": "겨울에 아이슬란드 영국 돌고오면되겠다",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011955.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011955.1.5",
          "form": "핀란드나",
          "original_form": "핀란드나",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011955.1.6",
          "form": "스웨덴도 가보고 싶어",
          "original_form": "스웨덴도 가보고 싶어",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011955.1.7",
          "form": "눈 오는거 너무 기대돼",
          "original_form": "눈 오는거 너무 기대돼",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011955.1.8",
          "form": "그리고 핀란드 산타마을 진짜 어릴때 부터 가보고 싶었어",
          "original_form": "그리고 핀란드 산타마을 진짜 어릴때 부터 가보고 싶었어",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011955.1.9",
          "form": "오 산타마을이 핀란드에 ㅣㅅ구나",
          "original_form": "오 산타마을이 핀란드에 ㅣㅅ구나",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011955.1.10",
          "form": "스웨덴인지 핀란드인지 잘 기억이 안나는데",
          "original_form": "스웨덴인지 핀란드인지 잘 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011955.1.11",
          "form": "엄청 추워서 바다가 얼고 그 바다를 건너가면 있다고 했어",
          "original_form": "엄청 추워서 바다가 얼고 그 바다를 건너가면 있다고 했어",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011955.1.12",
          "form": "그리고 유럽국가들은 붙어 있잖아",
          "original_form": "그리고 유럽국가들은 붙어 있잖아",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011955.1.13",
          "form": "그래서 국경 밟아보고 싶어",
          "original_form": "그래서 국경 밟아보고 싶어",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011955.1.14",
          "form": "영국에 그리니치 천문대도 가고 싶고",
          "original_form": "영국에 그리니치 천문대도 가고 싶고",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011955.1.15",
          "form": "나도 국경밟으면서",
          "original_form": "나도 국경밟으면서",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011955.1.16",
          "form": "여긴어디~ 여긴어디~ 하면서놀고싶음ㅋㅋㅋㅋㅋ",
          "original_form": "여긴어디~ 여긴어디~ 하면서놀고싶음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011955.1.17",
          "form": "근데 아는거되게많다",
          "original_form": "근데 아는거되게많다",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.18",
          "form": "동시에 두곳에 서있기",
          "original_form": "동시에 두곳에 서있기",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.19",
          "form": "난 걍 유럽가고싶다~ 말만하고 아는건없음 ㅎㅎ",
          "original_form": "난 걍 유럽가고싶다~ 말만하고 아는건없음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.20",
          "form": "갑자기 아는게 많다가 되나요. ..?",
          "original_form": "갑자기 아는게 많다가 되나요. ..?",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.21",
          "form": "네 됩니다",
          "original_form": "네 됩니다",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.22",
          "form": "나 나라 조사해서 발표하는거 많이 했어서...",
          "original_form": "나 나라 조사해서 발표하는거 많이 했어서...",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.23",
          "form": "아하ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.24",
          "form": "영어 중국어 한국어로 다 한번씩 해본듯",
          "original_form": "영어 중국어 한국어로 다 한번씩 해본듯",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011955.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011955.1.26",
          "form": "wow....멋져언니",
          "original_form": "wow....멋져언니",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011955.1.27",
          "form": "난 여름에도 유럽가보고싶어",
          "original_form": "난 여름에도 유럽가보고싶어",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011955.1.28",
          "form": "아시아처럼 습하지않고",
          "original_form": "아시아처럼 습하지않고",
          "speaker_id": "2",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011955.1.29",
          "form": "건조한 여름 경험해보고싶음ㅋㅋㅋㅋ",
          "original_form": "건조한 여름 경험해보고싶음ㅋㅋㅋㅋ",
