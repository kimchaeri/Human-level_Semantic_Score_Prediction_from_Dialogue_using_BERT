      "utterance": [
        {
          "id": "MDRW2100042750.1.1",
          "form": "그러게요 어제밤도오더니",
          "original_form": "그러게요 어제밤도오더니",
          "speaker_id": "2",
          "time": "20210907 08:18"
        },
        {
          "id": "MDRW2100042750.1.2",
          "form": "네 언제까지 내리려나요ㅜ",
          "original_form": "네 언제까지 내리려나요ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:19"
        },
        {
          "id": "MDRW2100042750.1.3",
          "form": "출근길에 비 오니 더 축처지네요",
          "original_form": "출근길에 비 오니 더 축처지네요",
          "speaker_id": "1",
          "time": "20210907 08:19"
        },
        {
          "id": "MDRW2100042750.1.4",
          "form": "ㅠㅜㅜ",
          "original_form": "ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042750.1.5",
          "form": "그죠출근길에비면..진짜싫ㅎ은딩",
          "original_form": "그죠출근길에비면..진짜싫ㅎ은딩",
          "speaker_id": "2",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042750.1.6",
          "form": "네네 축축...",
          "original_form": "네네 축축...",
          "speaker_id": "1",
          "time": "20210907 08:20"
        },
        {
          "id": "MDRW2100042750.1.7",
          "form": "ㅜㅡㅜ 걸어서출근ㅎㅏ시나여",
          "original_form": "ㅜㅡㅜ 걸어서출근ㅎㅏ시나여",
          "speaker_id": "2",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042750.1.8",
          "form": "지하철 타고요",
          "original_form": "지하철 타고요",
          "speaker_id": "1",
          "time": "20210907 08:21"
        },
        {
          "id": "MDRW2100042750.1.9",
          "form": "비 오면 서로의 우산으로 습기를 전달하죠ㅜ",
          "original_form": "비 오면 서로의 우산으로 습기를 전달하죠ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042750.1.10",
          "form": "지하철이 비오는 우간",
          "original_form": "지하철이 비오는 우간",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042750.1.11",
          "form": "그니까요ㅠㅋㅋㅋㅋ",
          "original_form": "그니까요ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042750.1.12",
          "form": "네 지금 상황이 그래용ㅜ",
          "original_form": "네 지금 상황이 그래용ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100042750.1.13",
          "form": "흑흑...얼른탈출하십시용...",
          "original_form": "흑흑...얼른탈출하십시용...",
          "speaker_id": "2",
          "time": "20210907 08:25"
        },
        {
          "id": "MDRW2100042750.1.14",
          "form": "네네 축축한 지하철 탈출!!",
          "original_form": "네네 축축한 지하철 탈출!!",
          "speaker_id": "1",
          "time": "20210907 08:26"
        },
        {
          "id": "MDRW2100042750.1.15",
          "form": "비 오는 아침은 정말 피곤하네용",
          "original_form": "비 오는 아침은 정말 피곤하네용",
          "speaker_id": "1",
          "time": "20210907 08:27"
        },
        {
          "id": "MDRW2100042750.1.16",
          "form": "사무실은 갠차는가용",
          "original_form": "사무실은 갠차는가용",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042750.1.17",
          "form": "네네 사무실은 쾌적합니다~",
          "original_form": "네네 사무실은 쾌적합니다~",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042750.1.18",
          "form": "ㅋㅋ 뭐입고 출근 하셧나용",
          "original_form": "ㅋㅋ 뭐입고 출근 하셧나용",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042750.1.19",
          "form": "얇은 긴팔 셔츠요ㅎㅎ",
          "original_form": "얇은 긴팔 셔츠요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042750.1.20",
          "form": "얇은 긴팔 셔츠 입고 출근했어요",
