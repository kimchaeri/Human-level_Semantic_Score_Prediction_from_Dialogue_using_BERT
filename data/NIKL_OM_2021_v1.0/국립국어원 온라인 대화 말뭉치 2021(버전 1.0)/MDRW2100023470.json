      "utterance": [
        {
          "id": "MDRW2100023470.1.1",
          "form": "가을 장마가 길다",
          "original_form": "가을 장마가 길다",
          "speaker_id": "2",
          "time": "20210830 06:10"
        },
        {
          "id": "MDRW2100023470.1.2",
          "form": "아침에 시원해서 좋다",
          "original_form": "아침에 시원해서 좋다",
          "speaker_id": "1",
          "time": "20210830 06:11"
        },
        {
          "id": "MDRW2100023470.1.3",
          "form": "나 아침밥 먹으러간다",
          "original_form": "나 아침밥 먹으러간다",
          "speaker_id": "1",
          "time": "20210830 06:11"
        },
        {
          "id": "MDRW2100023470.1.4",
          "form": "가을 좋구나",
          "original_form": "가을 좋구나",
          "speaker_id": "1",
          "time": "20210830 08:32"
        },
        {
          "id": "MDRW2100023470.1.5",
          "form": "가을인데 비가 계속 와서 ㅋㅋ 나가기 힘든뎅",
          "original_form": "가을인데 비가 계속 와서 ㅋㅋ 나가기 힘든뎅",
          "speaker_id": "2",
          "time": "20210830 08:33"
        },
        {
          "id": "MDRW2100023470.1.6",
          "form": "가을 장마라서 그렇지",
          "original_form": "가을 장마라서 그렇지",
          "speaker_id": "1",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100023470.1.7",
          "form": "더운 것보다 이런 날씨가 더 좋아",
          "original_form": "더운 것보다 이런 날씨가 더 좋아",
          "speaker_id": "1",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100023470.1.8",
          "form": "장마 끝났는데 ㅋㅋ 날씨만 좋구먼",
          "original_form": "장마 끝났는데 ㅋㅋ 날씨만 좋구먼",
          "speaker_id": "2",
          "time": "20210905 08:14"
        },
        {
          "id": "MDRW2100023470.1.9",
          "form": "다음 주에 또 비온대",
          "original_form": "다음 주에 또 비온대",
          "speaker_id": "1",
          "time": "20210905 08:14"
        },
        {
          "id": "MDRW2100023470.1.10",
          "form": "놀러가기 아~~주 좋은 날씨여 ㅋ",
          "original_form": "놀러가기 아~~주 좋은 날씨여 ㅋ",
          "speaker_id": "2",
          "time": "20210905 08:14"
        },
        {
          "id": "MDRW2100023470.1.11",
          "form": "가을 소풍은 진고개로 고고씽",
          "original_form": "가을 소풍은 진고개로 고고씽",
          "speaker_id": "1",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100023470.1.12",
          "form": "쳇~ 요즘은 여름보다 가을에 비가 더 많이 오는듯",
          "original_form": "쳇~ 요즘은 여름보다 가을에 비가 더 많이 오는듯",
          "speaker_id": "2",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100023470.1.13",
          "form": "기상이변이다",
          "original_form": "기상이변이다",
          "speaker_id": "1",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100023470.1.14",
          "form": "ㅋㅋ 비 많이 온다고 기상이변이면 어쩌누",
          "original_form": "ㅋㅋ 비 많이 온다고 기상이변이면 어쩌누",
          "speaker_id": "2",
          "time": "20210905 08:15"
        },
        {
          "id": "MDRW2100023470.1.15",
          "form": "여름에 비 안오고 가을에 오니까",
          "original_form": "여름에 비 안오고 가을에 오니까",
          "speaker_id": "1",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100023470.1.16",
          "form": "그래도 하늘도 높고~ 살찌기 좋은 계절이야 ㅋ",
          "original_form": "그래도 하늘도 높고~ 살찌기 좋은 계절이야 ㅋ",
          "speaker_id": "2",
          "time": "20210905 08:17"
        },
        {
          "id": "MDRW2100023470.1.17",
          "form": "아침에는 선선해서 자기 좋다",
          "original_form": "아침에는 선선해서 자기 좋다",
          "speaker_id": "1",
          "time": "20210905 08:19"
        }
      ]
