        {
          "id": "MDRW2100008865.1.1",
          "form": "최근 잘샀다 싶은 꿀템  있으세요",
          "original_form": "최근 잘샀다 싶은 꿀템  있으세요",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008865.1.2",
          "form": "정원샵에서 불곱창샀어요ㅎ 싸게",
          "original_form": "정원샵에서 불곱창샀어요ㅎ 싸게",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008865.1.3",
          "form": "주로 먹을거사요",
          "original_form": "주로 먹을거사요",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008865.1.4",
          "form": "님은 뭐 쇼핑한거있나요?",
          "original_form": "님은 뭐 쇼핑한거있나요?",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008865.1.5",
          "form": "저는 홈쇼핑에서 장어샀어요",
          "original_form": "저는 홈쇼핑에서 장어샀어요",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008865.1.6",
          "form": "10개 한세트",
          "original_form": "10개 한세트",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008865.1.7",
          "form": "고민하다 20개 샀네요 ㅋㅋ",
          "original_form": "고민하다 20개 샀네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008865.1.8",
          "form": "오 장어요?ㅎ",
          "original_form": "오 장어요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008865.1.9",
          "form": "20마리?",
          "original_form": "20마리?",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008865.1.10",
          "form": "그건그냥 구워먹나요?",
          "original_form": "그건그냥 구워먹나요?",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008865.1.11",
          "form": "그자리에서 삼등분해서 냉동포장?했더라고요",
          "original_form": "그자리에서 삼등분해서 냉동포장?했더라고요",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008865.1.12",
          "form": "굽기만제가 구우면되요",
          "original_form": "굽기만제가 구우면되요",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008865.1.13",
          "form": "근데 좀 요령이 필요하겠다 싶었어요",
          "original_form": "근데 좀 요령이 필요하겠다 싶었어요",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008865.1.14",
          "form": "장어는 한번도 사본적이 없는데 ㅎㅎ",
          "original_form": "장어는 한번도 사본적이 없는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008865.1.15",
          "form": "몸보신 되겠어요",
          "original_form": "몸보신 되겠어요",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008865.1.16",
          "form": "구울때 수분 최대한제거하고 약한불에 은은하게구워야해요",
          "original_form": "구울때 수분 최대한제거하고 약한불에 은은하게구워야해요",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008865.1.17",
          "form": "뭐 생선굽는게 다 그렇지만 ㅋㅋ",
          "original_form": "뭐 생선굽는게 다 그렇지만 ㅋㅋ",
