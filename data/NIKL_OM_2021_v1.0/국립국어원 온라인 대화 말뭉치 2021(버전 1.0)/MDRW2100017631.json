      "utterance": [
        {
          "id": "MDRW2100017631.1.1",
          "form": "홈트하면 체력은 붙는거같아요",
          "original_form": "홈트하면 체력은 붙는거같아요",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017631.1.2",
          "form": "더 많이 먹기 위해 운동합니다",
          "original_form": "더 많이 먹기 위해 운동합니다",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017631.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017631.1.4",
          "form": "맞아요ㅋㅋ 먹기위해 운동ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 먹기위해 운동ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017631.1.5",
          "form": "팔뚝에 근육생기고 힙업을 위해 ㅎㅎ",
          "original_form": "팔뚝에 근육생기고 힙업을 위해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017631.1.6",
          "form": "플랭크와 스쿼트가 진리긴해요",
          "original_form": "플랭크와 스쿼트가 진리긴해요",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017631.1.7",
          "form": "스쿼트는 혈당관리에도 좋다네요",
          "original_form": "스쿼트는 혈당관리에도 좋다네요",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017631.1.8",
          "form": "아 처음 알았어요!",
          "original_form": "아 처음 알았어요!",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017631.1.9",
          "form": "하체가 튼튼해야한다더라고요 나이먹을수록",
          "original_form": "하체가 튼튼해야한다더라고요 나이먹을수록",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017631.1.10",
          "form": "네 맞아요 허벅지근육 필수래요",
          "original_form": "네 맞아요 허벅지근육 필수래요",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017631.1.11",
          "form": "다이어트는 끝이 없고",
          "original_form": "다이어트는 끝이 없고",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017631.1.12",
          "form": "식단은 안하시죠?",
          "original_form": "식단은 안하시죠?",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017631.1.13",
          "form": "네 유혹을 못이기네요ㅜㅜ",
          "original_form": "네 유혹을 못이기네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017631.1.14",
          "form": "근데 안하는게 좋아요 스트레스가 더 안좋으니까요ㅋㅋ",
          "original_form": "근데 안하는게 좋아요 스트레스가 더 안좋으니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017631.1.15",
          "form": "네ㅋㅋ 하긴해야하는데..이놈의 의지ㅠ",
          "original_form": "네ㅋㅋ 하긴해야하는데..이놈의 의지ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017631.1.16",
          "form": "야식먹어서 위가 늘어난것같아요",
          "original_form": "야식먹어서 위가 늘어난것같아요",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017631.1.17",
          "form": "회사다니면 더더욱 안되죠...ㅜㅠ",
          "original_form": "회사다니면 더더욱 안되죠...ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017631.1.18",
          "form": "간식과 야식",
          "original_form": "간식과 야식",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017631.1.19",
          "form": "적어도 야식은 끊어보려구요ㅠ",
          "original_form": "적어도 야식은 끊어보려구요ㅠ",
