        },
        {
          "id": "MDRW2100019066.1.2",
          "form": "해워니랑 사찰에 한번 같이 가보고 싶어요 어때요",
          "original_form": "해워니랑 사찰에 한번 같이 가보고 싶어요 어때요",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019066.1.3",
          "form": "종교없지만 굳이 고르자면 불교입니다",
          "original_form": "종교없지만 굳이 고르자면 불교입니다",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019066.1.4",
          "form": "좋습니다 좋아요",
