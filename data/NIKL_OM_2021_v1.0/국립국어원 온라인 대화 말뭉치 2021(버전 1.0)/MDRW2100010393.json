      "utterance": [
        {
          "id": "MDRW2100010393.1.1",
          "form": "안녕하세요 중고거래 해보신적 있나요?",
          "original_form": "안녕하세요 중고거래 해보신적 있나요?",
          "speaker_id": "1",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010393.1.2",
          "form": "당근마켓으로 몇 번 해본적 있어요!",
          "original_form": "당근마켓으로 몇 번 해본적 있어요!",
          "speaker_id": "2",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010393.1.3",
          "form": "저도요! 찜해둔 상품이 있나요?",
          "original_form": "저도요! 찜해둔 상품이 있나요?",
          "speaker_id": "1",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010393.1.4",
          "form": "막 찜해뒀던 무드등이 팔려서 지금은 없어요ㅠ",
          "original_form": "막 찜해뒀던 무드등이 팔려서 지금은 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010393.1.5",
          "form": "그럴때 너무아쉬워요ㅠㅜㅜ",
          "original_form": "그럴때 너무아쉬워요ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010393.1.6",
          "form": "중고거래는 타이밍이라는 사실을 항상 뒤늦게 깨달아요ㅠ",
          "original_form": "중고거래는 타이밍이라는 사실을 항상 뒤늦게 깨달아요ㅠ",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010393.1.7",
          "form": "진짜 어려워용 일찍사면 비싸고요",
          "original_form": "진짜 어려워용 일찍사면 비싸고요",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010393.1.8",
          "form": "정확한 타이밍을 잘 찾으면 어케 할인도 받을 수 있는데 말이죠ㅠ",
          "original_form": "정확한 타이밍을 잘 찾으면 어케 할인도 받을 수 있는데 말이죠ㅠ",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100010393.1.9",
          "form": "맞아요ㅠ 어렵긴해두 거래성사되면 뿌듯해요!",
          "original_form": "맞아요ㅠ 어렵긴해두 거래성사되면 뿌듯해요!",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100010393.1.10",
          "form": "중고로 판매해보셨나요?",
          "original_form": "중고로 판매해보셨나요?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100010393.1.11",
          "form": "네! 파우치 판매해봤어용",
          "original_form": "네! 파우치 판매해봤어용",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100010393.1.12",
          "form": "어떤 플렛폼을 이용하셨나요?",
          "original_form": "어떤 플렛폼을 이용하셨나요?",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100010393.1.13",
          "form": "당근마켓이죠!",
          "original_form": "당근마켓이죠!",
          "speaker_id": "1",
          "time": "20210825 17:31"
        }
      ]
