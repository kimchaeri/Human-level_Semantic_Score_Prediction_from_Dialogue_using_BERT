      "utterance": [
        {
          "id": "MDRW2100022396.1.1",
          "form": "나는 가을이  좋아",
          "original_form": "나는 가을이  좋아",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022396.1.2",
          "form": "나도 가을",
          "original_form": "나도 가을",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022396.1.3",
          "form": "가을이 짱이지",
          "original_form": "가을이 짱이지",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022396.1.4",
          "form": "얼른 가을 왔으면 좋겠다..",
          "original_form": "얼른 가을 왔으면 좋겠다..",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022396.1.5",
          "form": "날씨가 너무 춥지도 덥지도 딱 적당히 선선한게 예쁜 옷 입기도 딱 좋아",
          "original_form": "날씨가 너무 춥지도 덥지도 딱 적당히 선선한게 예쁜 옷 입기도 딱 좋아",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022396.1.6",
          "form": "근데 나 가을 옷 없어",
          "original_form": "근데 나 가을 옷 없어",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022396.1.7",
          "form": "슬리브 말고 가을에 입을 옷이 없어ㅠㅠ",
          "original_form": "슬리브 말고 가을에 입을 옷이 없어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022396.1.8",
          "form": "니트 하나 사는거 어때",
          "original_form": "니트 하나 사는거 어때",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022396.1.9",
          "form": "난 나염니트? 사려구",
          "original_form": "난 나염니트? 사려구",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100022396.1.10",
          "form": "여름에 할인해서 샀는데 주문폭주로 밀리다가 재고 없다고 취소당했어..",
          "original_form": "여름에 할인해서 샀는데 주문폭주로 밀리다가 재고 없다고 취소당했어..",
          "speaker_id": "1",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022396.1.11",
          "form": "나염니트 나도 알아봐야겠다",
          "original_form": "나염니트 나도 알아봐야겠다",
          "speaker_id": "1",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022396.1.12",
          "form": "괜찮은거 찾음 나도 보여조",
          "original_form": "괜찮은거 찾음 나도 보여조",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022396.1.13",
          "form": "이번주도 역시 비오더라",
          "original_form": "이번주도 역시 비오더라",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022396.1.14",
          "form": "부산은 일주일 중 하루빼고 다 비왔어",
          "original_form": "부산은 일주일 중 하루빼고 다 비왔어",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022396.1.15",
          "form": "쨍쨍하다가 갑자기 비오고 난리였어",
          "original_form": "쨍쨍하다가 갑자기 비오고 난리였어",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022396.1.16",
          "form": "분명 일기예보에는 그때 비 안 온다 했는데 비 오고",
          "original_form": "분명 일기예보에는 그때 비 안 온다 했는데 비 오고",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022396.1.17",
          "form": "내일은 비 안오고 목요일까지 비오다가 맑아지네ㅣ",
          "original_form": "내일은 비 안오고 목요일까지 비오다가 맑아지네ㅣ",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100022396.1.18",
          "form": "역시 일기예보는 믿을 게 ㅁ,,,ㄷㄷ,,,",
          "original_form": "역시 일기예보는 믿을 게 ㅁ,,,ㄷㄷ,,,",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022396.1.19",
          "form": "난 비만 안 오면 돼",
          "original_form": "난 비만 안 오면 돼",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022396.1.20",
          "form": "그런데 오늘도 비가 오네ㅠㅠ",
          "original_form": "그런데 오늘도 비가 오네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022396.1.21",
          "form": "난 비가 와야돼,,",
          "original_form": "난 비가 와야돼,,",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022396.1.22",
          "form": "그래야 현장을 안나가거든,,,",
          "original_form": "그래야 현장을 안나가거든,,,",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022396.1.23",
          "form": "우린 비가 오나 눈이 오나 출근..ㅠㅠ",
          "original_form": "우린 비가 오나 눈이 오나 출근..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022396.1.24",
          "form": "실내여서 다행이네,,,",
          "original_form": "실내여서 다행이네,,,",
          "speaker_id": "2",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022396.1.25",
          "form": "그렇지ㅠㅠ",
          "original_form": "그렇지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:07"
        }
      ]
    }
