        },
        {
          "id": "MDRW2100046296.1.2",
          "form": "이벤트 신청합니다",
          "original_form": "이벤트 신청합니다",
          "speaker_id": "1",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.4",
          "form": "요즘 취미가 어떻게 되시나요?",
          "original_form": "요즘 취미가 어떻게 되시나요?",
          "speaker_id": "2",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.5",
          "form": "주식이요",
          "original_form": "주식이요",
          "speaker_id": "1",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.6",
          "form": "요즘 주식장은 어떤가요? 괜찮은가요..?",
          "original_form": "요즘 주식장은 어떤가요? 괜찮은가요..?",
          "speaker_id": "2",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.7",
          "form": "재밌네요 소액으로 하니까",
          "original_form": "재밌네요 소액으로 하니까",
          "speaker_id": "1",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.8",
          "form": "그냥 저냥 괜찮은거 갘아요",
          "original_form": "그냥 저냥 괜찮은거 갘아요",
          "speaker_id": "1",
          "time": "20211015 17:57"
        },
        {
          "id": "MDRW2100046296.1.9",
          "form": "저는 요즘 하락장이라..",
          "original_form": "저는 요즘 하락장이라..",
          "speaker_id": "2",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.10",
          "form": "매일매일 소액이지만 밥값정도는 벌고 있으니",
          "original_form": "매일매일 소액이지만 밥값정도는 벌고 있으니",
          "speaker_id": "1",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.11",
          "form": "힘이 안나네요...",
          "original_form": "힘이 안나네요...",
          "speaker_id": "2",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.12",
          "form": "오 수입이 괜찮은데요?",
          "original_form": "오 수입이 괜찮은데요?",
          "speaker_id": "2",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.13",
          "form": "그냥 매일 단타로 밥값벌기를 목표로 하니 수익률이 나쁘지 않네요",
          "original_form": "그냥 매일 단타로 밥값벌기를 목표로 하니 수익률이 나쁘지 않네요",
          "speaker_id": "1",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.14",
          "form": "아 단타로 주식하시는구나~",
          "original_form": "아 단타로 주식하시는구나~",
          "speaker_id": "2",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.15",
          "form": "전 장투입니다 ㅎㅎ",
          "original_form": "전 장투입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.16",
          "form": "수익율인가 수익률인가",
          "original_form": "수익율인가 수익률인가",
          "speaker_id": "1",
          "time": "20211015 17:58"
        },
        {
          "id": "MDRW2100046296.1.17",
          "form": "수익률입니다!",
          "original_form": "수익률입니다!",
          "speaker_id": "2",
          "time": "20211015 17:59"
        },
        {
          "id": "MDRW2100046296.1.18",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20211015 18:00"
        },
        {
          "id": "MDRW2100046296.1.19",
          "form": "저는 지금 와이프가 출산중이에요",
          "original_form": "저는 지금 와이프가 출산중이에요",
          "speaker_id": "1",
          "time": "20211015 18:00"
        },
        {
          "id": "MDRW2100046296.1.20",
          "form": "비트코인은 안하시나요?",
          "original_form": "비트코인은 안하시나요?",
          "speaker_id": "2",
          "time": "20211015 18:00"
        },
        {
          "id": "MDRW2100046296.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211015 18:00"
        },
        {
          "id": "MDRW2100046296.1.22",
          "form": "코인은 안합니다",
          "original_form": "코인은 안합니다",
          "speaker_id": "1",
          "time": "20211015 18:00"
        },
        {
          "id": "MDRW2100046296.1.23",
          "form": "아하 주식만 하시는구나~",
          "original_form": "아하 주식만 하시는구나~",
          "speaker_id": "2",
          "time": "20211015 18:00"
        },
        {
          "id": "MDRW2100046296.1.24",
          "form": "주식 단타 성공하셔서 큰 수익 내시길 바랄게요! 아내분 출산도 화이팅입니다! 오늘 남은 하루 잘 보내세요!",
          "original_form": "주식 단타 성공하셔서 큰 수익 내시길 바랄게요! 아내분 출산도 화이팅입니다! 오늘 남은 하루 잘 보내세요!",
          "speaker_id": "2",
          "time": "20211015 18:01"
        }
      ]
