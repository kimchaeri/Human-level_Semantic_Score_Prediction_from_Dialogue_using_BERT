      "utterance": [
        {
          "id": "MDRW2100004651.1.1",
          "form": "스포츠 경기보는거 좋아하세요?",
          "original_form": "스포츠 경기보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004651.1.2",
          "form": "챙겨보는 편은 아닌데",
          "original_form": "챙겨보는 편은 아닌데",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004651.1.3",
          "form": "이번 올림픽 참 재밌게봤어요",
          "original_form": "이번 올림픽 참 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004651.1.4",
          "form": "개막식이나 폐막식도 보셨어요?",
          "original_form": "개막식이나 폐막식도 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004651.1.5",
          "form": "재미없다고 들어서 안봤는데",
          "original_form": "재미없다고 들어서 안봤는데",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004651.1.6",
          "form": "어땠나요?",
          "original_form": "어땠나요?",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004651.1.7",
          "form": "저는 개막식때 픽토그램 되게 재밌더라구요",
          "original_form": "저는 개막식때 픽토그램 되게 재밌더라구요",
          "speaker_id": "1",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004651.1.8",
          "form": "아 그건 영상으로 봤었어요",
          "original_form": "아 그건 영상으로 봤었어요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004651.1.9",
          "form": "잘만들었더라고요",
          "original_form": "잘만들었더라고요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004651.1.10",
          "form": "뭔가 일본 답기도했고",
          "original_form": "뭔가 일본 답기도했고",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004651.1.11",
          "form": "그래도 저는 여자배구 제일 재밌개봤내요",
          "original_form": "그래도 저는 여자배구 제일 재밌개봤내요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004651.1.12",
          "form": "너무 흥미진진했어요",
          "original_form": "너무 흥미진진했어요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004651.1.13",
          "form": "여자배구 메달 없어서 아쉬웠네요",
          "original_form": "여자배구 메달 없어서 아쉬웠네요",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004651.1.14",
          "form": "그래도 선수들 다 멋졌었죠",
          "original_form": "그래도 선수들 다 멋졌었죠",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004651.1.15",
          "form": "평소에 슬갸보새요 스포츠경기?",
          "original_form": "평소에 슬갸보새요 스포츠경기?",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100004651.1.16",
          "form": "올림픽이나 월드컵같이 큰 국제대회는 되게 많이보는거같아요",
          "original_form": "올림픽이나 월드컵같이 큰 국제대회는 되게 많이보는거같아요",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004651.1.17",
          "form": "월드컵 자주 보세요?",
          "original_form": "월드컵 자주 보세요?",
          "speaker_id": "1",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004651.1.18",
          "form": "할때마다 몇개는 챙겨보게되긴 하는것같아요",
          "original_form": "할때마다 몇개는 챙겨보게되긴 하는것같아요",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004651.1.19",
          "form": "잘 몰라도 보다보면 재밌더라고요",
          "original_form": "잘 몰라도 보다보면 재밌더라고요",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004651.1.20",
          "form": "특히 좋아하시는거 잇으세요?",
          "original_form": "특히 좋아하시는거 잇으세요?",
          "speaker_id": "2",
          "time": "20210821 18:32"
        },
        {
          "id": "MDRW2100004651.1.21",
          "form": "축구나 수영같은 룰 몰라도 쉽게볼수있는거 좋아하는거같아요",
          "original_form": "축구나 수영같은 룰 몰라도 쉽게볼수있는거 좋아하는거같아요",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004651.1.22",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004651.1.23",
          "form": "확실히 룰을 모르면 봐도 잘 모르겠더라고요",
          "original_form": "확실히 룰을 모르면 봐도 잘 모르겠더라고요",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004651.1.24",
          "form": "전 야구가 특히 그랬어요",
          "original_form": "전 야구가 특히 그랬어요",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004651.1.25",
          "form": "아 저도 야구 볼줄 몰라서 재미 없더라고요",
          "original_form": "아 저도 야구 볼줄 몰라서 재미 없더라고요",
          "speaker_id": "1",
          "time": "20210821 19:39"
        }
      ]
    }
