      "utterance": [
        {
          "id": "MDRW2100038599.1.1",
          "form": "사실 추천받아먹은거라 다른거 안머거봣어요 ㅋㅋ",
          "original_form": "사실 추천받아먹은거라 다른거 안머거봣어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100038599.1.2",
          "form": "근데 맛있길래 ㅋㅋ 그거만 사먹어요",
          "original_form": "근데 맛있길래 ㅋㅋ 그거만 사먹어요",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100038599.1.3",
          "form": "많이 맵나요?",
          "original_form": "많이 맵나요?",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100038599.1.4",
          "form": "ㅋㅋ 보통 매운맛 많이매운맛 있어요",
          "original_form": "ㅋㅋ 보통 매운맛 많이매운맛 있어요",
          "speaker_id": "2",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100038599.1.5",
          "form": "전 매운거잘먹어서  보통맛은 안매워요",
          "original_form": "전 매운거잘먹어서  보통맛은 안매워요",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100038599.1.6",
          "form": "그럼 많이매운맛도 드시나요??",
          "original_form": "그럼 많이매운맛도 드시나요??",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038599.1.7",
          "form": "넹 ! ㅋㅋ 그건 좀 매워요",
          "original_form": "넹 ! ㅋㅋ 그건 좀 매워요",
          "speaker_id": "2",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038599.1.8",
          "form": "와 핵불닭정도 되려나요?ㅋㅋ",
          "original_form": "와 핵불닭정도 되려나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100038599.1.9",
          "form": "그거보단 안매워요 ㅋㅋㅋㅋ",
          "original_form": "그거보단 안매워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038599.1.10",
          "form": "그냥 불닭보다 조금 매운정도?",
          "original_form": "그냥 불닭보다 조금 매운정도?",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100038599.1.11",
          "form": "매운거 정말 잘드시나보네요ㅋㅋㅋ 전 불닭 기본 먹으면 울면서 먹어요ㅋㅋㅋ",
          "original_form": "매운거 정말 잘드시나보네요ㅋㅋㅋ 전 불닭 기본 먹으면 울면서 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100038599.1.12",
          "form": "ㅋㅋㅋㅋㅋ엽떡은요",
          "original_form": "ㅋㅋㅋㅋㅋ엽떡은요",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100038599.1.13",
          "form": "착한맛만 먹어요ㅎㅎ 요즘은 로제 떡볶이도 먹고요",
          "original_form": "착한맛만 먹어요ㅎㅎ 요즘은 로제 떡볶이도 먹고요",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100038599.1.14",
          "form": "오 로제맛잇죠 어디꺼조아하세요",
          "original_form": "오 로제맛잇죠 어디꺼조아하세요",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100038599.1.15",
          "form": "배떡 자주 먹어요ㅋㅋㅋ",
          "original_form": "배떡 자주 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100038599.1.16",
