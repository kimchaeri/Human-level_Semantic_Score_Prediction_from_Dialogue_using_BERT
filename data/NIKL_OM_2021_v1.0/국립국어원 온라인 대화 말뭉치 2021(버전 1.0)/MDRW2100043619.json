      "utterance": [
        {
          "id": "MDRW2100043619.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043619.1.2",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043619.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043619.1.4",
          "form": "저는 피자좋아해서",
          "original_form": "저는 피자좋아해서",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043619.1.5",
          "form": "자주시켜먹어요",
          "original_form": "자주시켜먹어요",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043619.1.6",
          "form": "오 어디꺼 시켜드세요 저는 도미노 좋아해요",
          "original_form": "오 어디꺼 시켜드세요 저는 도미노 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043619.1.7",
          "form": "저희지역만있는",
          "original_form": "저희지역만있는",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043619.1.8",
          "form": "엘리펀트피자라는곳있는데",
          "original_form": "엘리펀트피자라는곳있는데",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043619.1.9",
          "form": "18인치고",
          "original_form": "18인치고",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043619.1.10",
          "form": "오 첨들어봐요 궁금하네요",
          "original_form": "오 첨들어봐요 궁금하네요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043619.1.11",
          "form": "맞아요 다른곳엔없더라구요",
          "original_form": "맞아요 다른곳엔없더라구요",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043619.1.12",
          "form": "가격도 저렴해요",
          "original_form": "가격도 저렴해요",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043619.1.13",
          "form": "맛있구요",
          "original_form": "맛있구요",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043619.1.14",
          "form": "가격 저렴한거 마음에 드네요 프차는 가격이 세서 ㅠㅠ",
          "original_form": "가격 저렴한거 마음에 드네요 프차는 가격이 세서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043619.1.15",
          "form": "메뉴는 어떤거 드시나요",
          "original_form": "메뉴는 어떤거 드시나요",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043619.1.16",
          "form": "음 고구마랑 감자즐겨먹어요",
          "original_form": "음 고구마랑 감자즐겨먹어요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043619.1.17",
          "form": "제가 고구마무스좋아해서",
          "original_form": "제가 고구마무스좋아해서",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043619.1.18",
          "form": "단짠단짠으로 반반먹네요",
          "original_form": "단짠단짠으로 반반먹네요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043619.1.19",
          "form": "아 고구마무스 맞아요 맛있져 달달하니..",
          "original_form": "아 고구마무스 맞아요 맛있져 달달하니..",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100043619.1.20",
          "form": "프차는 넘비싸죠",
          "original_form": "프차는 넘비싸죠",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043619.1.21",
          "form": "그거 외국인들은 신기해하더라고요",
          "original_form": "그거 외국인들은 신기해하더라고요",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043619.1.22",
          "form": "4만원 넘는것도있던데",
          "original_form": "4만원 넘는것도있던데",
          "speaker_id": "2",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043619.1.23",
          "form": "피자에 고구마무스 들어가는거",
          "original_form": "피자에 고구마무스 들어가는거",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043619.1.24",
          "form": "그쵸 비싸죠.. 프차 아무래도 이름값인 거 같아요",
          "original_form": "그쵸 비싸죠.. 프차 아무래도 이름값인 거 같아요",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043619.1.25",
          "form": "아 외국인들은 단 피자 안좋아하는거같더라구요",
          "original_form": "아 외국인들은 단 피자 안좋아하는거같더라구요",
          "speaker_id": "2",
          "time": "20210907 20:04"
        }
      ]
