      "utterance": [
        {
          "id": "MDRW2100019106.1.1",
          "form": "영화좋아하세요",
          "original_form": "영화좋아하세요",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019106.1.2",
          "form": "영화 좋아해요 ㅎㅎ",
          "original_form": "영화 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019106.1.3",
          "form": "저도용 요새 극장을 못가서ㅜㅜ",
          "original_form": "저도용 요새 극장을 못가서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019106.1.4",
          "form": "맞아요 ㅠㅠ 그래서 걍 집에서 왓챠로 본답니다 ㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 그래서 걍 집에서 왓챠로 본답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019106.1.5",
          "form": "저는 웨이브랑 넷플",
          "original_form": "저는 웨이브랑 넷플",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019106.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019106.1.7",
          "form": "모가디슈보고싶은데 못보고있네요",
          "original_form": "모가디슈보고싶은데 못보고있네요",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019106.1.8",
          "form": "아 모가디슈 반응좋더라구요!",
          "original_form": "아 모가디슈 반응좋더라구요!",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019106.1.9",
          "form": "웨이브 볼거많나요?",
          "original_form": "웨이브 볼거많나요?",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019106.1.10",
          "form": "네 제가 미드 좋아하는데",
          "original_form": "네 제가 미드 좋아하는데",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019106.1.11",
          "form": "hbo 있어서 짱좋아요",
          "original_form": "hbo 있어서 짱좋아요",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019106.1.12",
          "form": "아!! 그거 들어본적있어요",
          "original_form": "아!! 그거 들어본적있어요",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019106.1.13",
          "form": "웨이브가 아이유모델이라 관심가긴해요 ㅋㅋ",
          "original_form": "웨이브가 아이유모델이라 관심가긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019106.1.14",
          "form": "모델을 잘 선택했네요ㅋㅋ",
          "original_form": "모델을 잘 선택했네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019106.1.15",
          "form": "그러게나말이에욬ㅋㅋㅋ",
          "original_form": "그러게나말이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100019106.1.16",
          "form": "아이유는 참 노래도 잘하고",
          "original_form": "아이유는 참 노래도 잘하고",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019106.1.17",
          "form": "배우로서도 성공했고",
          "original_form": "배우로서도 성공했고",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019106.1.18",
          "form": "그쵸 너무부러워요 ㅠ성공한인생!",
          "original_form": "그쵸 너무부러워요 ㅠ성공한인생!",
