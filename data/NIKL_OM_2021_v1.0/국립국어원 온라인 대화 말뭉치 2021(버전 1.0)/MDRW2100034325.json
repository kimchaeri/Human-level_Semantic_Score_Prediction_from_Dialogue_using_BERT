      "utterance": [
        {
          "id": "MDRW2100034325.1.1",
          "form": "생각해보니까",
          "original_form": "생각해보니까",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034325.1.2",
          "form": "축제도 안 하잖아",
          "original_form": "축제도 안 하잖아",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034325.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100034325.1.4",
          "form": "너무 슬프네...",
          "original_form": "너무 슬프네...",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.5",
          "form": "얼탱스",
          "original_form": "얼탱스",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.6",
          "form": "축제 비용만 돌려줘도 10만원씩 돌릴 수 있는 거 아니냐",
          "original_form": "축제 비용만 돌려줘도 10만원씩 돌릴 수 있는 거 아니냐",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.7",
          "form": "내년에도 축제는 불가능해보이는디",
          "original_form": "내년에도 축제는 불가능해보이는디",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.8",
          "form": "하아~",
          "original_form": "하아~",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.9",
          "form": "헐 그러게",
          "original_form": "헐 그러게",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.10",
          "form": "축제 비용 뭥미",
          "original_form": "축제 비용 뭥미",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100034325.1.11",
          "form": "불꽃놀이도 안 하고 초청도 안 하면서",
          "original_form": "불꽃놀이도 안 하고 초청도 안 하면서",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.12",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.13",
          "form": "돈 날로 먹네",
          "original_form": "돈 날로 먹네",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.14",
          "form": "진짜 역겹다",
          "original_form": "진짜 역겹다",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.15",
          "form": "등록금이 한 두 푼도 아니고",
          "original_form": "등록금이 한 두 푼도 아니고",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.16",
          "form": "억울해",
          "original_form": "억울해",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.17",
          "form": "그니까!",
          "original_form": "그니까!",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100034325.1.18",
          "form": "서럽다 서러워",
          "original_form": "서럽다 서러워",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034325.1.19",
          "form": "축제도 안 열고 400 먹는 거네",
          "original_form": "축제도 안 열고 400 먹는 거네",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034325.1.20",
          "form": "나는 뭐라도 벌어보려고 심심이 뚜들기고 있는데",
          "original_form": "나는 뭐라도 벌어보려고 심심이 뚜들기고 있는데",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034325.1.21",
          "form": "가만히 앉아서 등록금 쏙쏙 받아가네;;",
          "original_form": "가만히 앉아서 등록금 쏙쏙 받아가네;;",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034325.1.22",
          "form": "진짜 억울하다",
          "original_form": "진짜 억울하다",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034325.1.23",
          "form": "몇만원도 아니고",
          "original_form": "몇만원도 아니고",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034325.1.24",
          "form": "몇백만원 ㅋㅋ",
          "original_form": "몇백만원 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034325.1.25",
          "form": "으아악 억울해...",
          "original_form": "으아악 억울해...",
          "speaker_id": "2",
          "time": "20210827 23:33"
        }
