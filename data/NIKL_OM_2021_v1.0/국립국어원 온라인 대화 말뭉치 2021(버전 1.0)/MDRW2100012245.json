      "utterance": [
        {
          "id": "MDRW2100012245.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012245.1.2",
          "form": "가끔하는 건 좋지만 자주가는 건 별로 좋아하지않아요 집을 워낙 좋아해서요 ㅎㅎ",
          "original_form": "가끔하는 건 좋지만 자주가는 건 별로 좋아하지않아요 집을 워낙 좋아해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012245.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012245.1.4",
          "form": "네 돌아다니는거",
          "original_form": "네 돌아다니는거",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012245.1.5",
          "form": "기억에 남는 여행지 있으신가요?",
          "original_form": "기억에 남는 여행지 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 07:37"
        },
        {
          "id": "MDRW2100012245.1.6",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210825 07:49"
        },
        {
          "id": "MDRW2100012245.1.7",
          "form": "좋은 곳 가셨네요!",
          "original_form": "좋은 곳 가셨네요!",
          "speaker_id": "2",
          "time": "20210825 08:04"
        },
        {
          "id": "MDRW2100012245.1.8",
          "form": "거기서 뭐 드셨어요??",
          "original_form": "거기서 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210825 08:04"
        },
        {
          "id": "MDRW2100012245.1.9",
          "form": "음식보다 풍경에",
          "original_form": "음식보다 풍경에",
          "speaker_id": "1",
          "time": "20210825 08:08"
        },
        {
          "id": "MDRW2100012245.1.10",
          "form": "아 그럼 기억에 남는 장소 있었나요?",
          "original_form": "아 그럼 기억에 남는 장소 있었나요?",
          "speaker_id": "2",
          "time": "20210825 08:10"
        },
        {
          "id": "MDRW2100012245.1.11",
          "form": "오페라하우스봤어요",
          "original_form": "오페라하우스봤어요",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100012245.1.12",
          "form": "날씨 좋을때 가셨나요?",
          "original_form": "날씨 좋을때 가셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100012245.1.13",
          "form": "오페라하우스가 하얘서 날씨 좋으면 진짜 예쁠 것 같아요",
          "original_form": "오페라하우스가 하얘서 날씨 좋으면 진짜 예쁠 것 같아요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100012245.1.14",
          "form": "네 좋았어요",
          "original_form": "네 좋았어요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100012245.1.15",
          "form": "사진은 많이 찍으셨나요?",
          "original_form": "사진은 많이 찍으셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100012245.1.16",
          "form": "네 많이요",
          "original_form": "네 많이요",
          "speaker_id": "1",
          "time": "20210825 15:59"
        }
      ]
