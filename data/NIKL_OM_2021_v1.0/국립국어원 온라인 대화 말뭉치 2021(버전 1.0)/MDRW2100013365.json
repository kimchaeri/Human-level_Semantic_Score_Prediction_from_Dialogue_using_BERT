      "utterance": [
        {
          "id": "MDRW2100013365.1.1",
          "form": "기숙사사신적있나여?",
          "original_form": "기숙사사신적있나여?",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013365.1.2",
          "form": "네 일주일이요 name1님은요,",
          "original_form": "네 일주일이요 &name1&님은요,",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013365.1.3",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013365.1.4",
          "form": "저는 학교다닐때 기숙사 살았었어요!",
          "original_form": "저는 학교다닐때 기숙사 살았었어요!",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100013365.1.5",
          "form": "기숙사 어떠셨나요?",
          "original_form": "기숙사 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100013365.1.6",
          "form": "동기들이랑 매일이 치킨파티였어요ㅋㅋ",
          "original_form": "동기들이랑 매일이 치킨파티였어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100013365.1.7",
          "form": "오 안맞는 부분은 없으셨나요?",
          "original_form": "오 안맞는 부분은 없으셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100013365.1.8",
          "form": "통금시간이 있어서 밤늦게 놀려면 외박신청해야하고 그런 점이 좀 불편했어요ㅋㅋ",
          "original_form": "통금시간이 있어서 밤늦게 놀려면 외박신청해야하고 그런 점이 좀 불편했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100013365.1.9",
          "form": "아 기숙사에서 노신게 아니라 다른 곳에서 노신거군요?",
          "original_form": "아 기숙사에서 노신게 아니라 다른 곳에서 노신거군요?",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100013365.1.10",
          "form": "배달도 시켜먹고",
          "original_form": "배달도 시켜먹고",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100013365.1.11",
          "form": "대학로에서 놀때도 있었어요ㅋㅋ",
          "original_form": "대학로에서 놀때도 있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100013365.1.12",
          "form": "오 name1 기숙사도 취사는 안되셨죠?",
          "original_form": "오 &name1& 기숙사도 취사는 안되셨죠?",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100013365.1.13",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100013365.1.14",
          "form": "그럼 대부분 배달이셨나요?",
          "original_form": "그럼 대부분 배달이셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100013365.1.15",
          "form": "아 기숙사식당이 있어서 기숙사밥 먹었어요ㅋㅋ",
          "original_form": "아 기숙사식당이 있어서 기숙사밥 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100013365.1.16",
          "form": "저녁까지 제공이 되셨나요?",
          "original_form": "저녁까지 제공이 되셨나요?",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013365.1.17",
          "form": "아점저 다 나왔어요ㅋㅋ",
          "original_form": "아점저 다 나왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100013365.1.18",
