      "utterance": [
        {
          "id": "MDRW2100008429.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008429.1.2",
          "form": "네! 근데 코로나 이후로 못가서 슬퍼하는 중이에요 ㅎㅎ",
          "original_form": "네! 근데 코로나 이후로 못가서 슬퍼하는 중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008429.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008429.1.4",
          "form": "저도 비슷하죠ㅎㅎㅎ",
          "original_form": "저도 비슷하죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008429.1.5",
          "form": "이번추석때는 갈수있을지모르겠어요...ㅎ",
          "original_form": "이번추석때는 갈수있을지모르겠어요...ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008429.1.6",
          "form": "코로나가 생각보다 너무 길어지니까요 ㅠㅠㅠ",
          "original_form": "코로나가 생각보다 너무 길어지니까요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008429.1.7",
          "form": "마음은 가까운데라도 가고싶은데..",
          "original_form": "마음은 가까운데라도 가고싶은데..",
          "speaker_id": "1",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008429.1.8",
          "form": "그니깐요ㅜㅜ",
          "original_form": "그니깐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008429.1.9",
          "form": "곧 백신 다 맞으면 곧 갈수있지않을까여?",
          "original_form": "곧 백신 다 맞으면 곧 갈수있지않을까여?",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008429.1.10",
          "form": "그러게요 다른 나라는 이제 코로나를 약간 독감처럼 생각하고 지낸다 하더라고요.. 우리나라도 그렇게 될런지",
          "original_form": "그러게요 다른 나라는 이제 코로나를 약간 독감처럼 생각하고 지낸다 하더라고요.. 우리나라도 그렇게 될런지",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008429.1.11",
          "form": "여행가실때 계획 잘 짜는 편이세요?",
          "original_form": "여행가실때 계획 잘 짜는 편이세요?",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008429.1.12",
          "form": "전 자유로운거 좋아해요ㅋㅋㅋ",
          "original_form": "전 자유로운거 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008429.1.13",
          "form": "저도요 ㅎㅎ 가고싶은 곳만 적어놓고 가요",
          "original_form": "저도요 ㅎㅎ 가고싶은 곳만 적어놓고 가요",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008429.1.14",
          "form": "주로 맛집 투어 하시나여???",
          "original_form": "주로 맛집 투어 하시나여???",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008429.1.15",
          "form": "음식은 적당히 찾아먹고 전 풍경이 좋거나 건물이 예쁜 곳으로 가서 사진찍어요 제가 들어간 사진 말고 풍경 위주로요 ㅎㅎ",
          "original_form": "음식은 적당히 찾아먹고 전 풍경이 좋거나 건물이 예쁜 곳으로 가서 사진찍어요 제가 들어간 사진 말고 풍경 위주로요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008429.1.16",
          "form": "어떤 스타일이세요?",
          "original_form": "어떤 스타일이세요?",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008429.1.17",
          "form": "전 먹는거 좋아서 맛집찾아 다닙니다ㅎㅎㅎ",
          "original_form": "전 먹는거 좋아서 맛집찾아 다닙니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008429.1.18",
          "form": "활동적인것도 좋아해서 그런것도 많이 하러다니구요!",
          "original_form": "활동적인것도 좋아해서 그런것도 많이 하러다니구요!",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
          "id": "MDRW2100008429.1.19",
          "form": "오 그러시구나 체험 같은 것도 많이 하시나봐요",
          "original_form": "오 그러시구나 체험 같은 것도 많이 하시나봐요",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008429.1.20",
          "form": "일상이랑 좀 다름을 느끼고 싶어서???????",
          "original_form": "일상이랑 좀 다름을 느끼고 싶어서???????",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008429.1.21",
          "form": "풍경 좋은데 가는거 비슷한거처럼ㅋㅋ",
          "original_form": "풍경 좋은데 가는거 비슷한거처럼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008429.1.22",
          "form": "아 맞아요 약간 여행갈때 평소랑 다른 내가 되는 게 재밌는 것 같아요 ㅎㅎ",
          "original_form": "아 맞아요 약간 여행갈때 평소랑 다른 내가 되는 게 재밌는 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008429.1.23",
          "form": "여행온 김에 한번 해보는..",
          "original_form": "여행온 김에 한번 해보는..",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008429.1.24",
          "form": "맞아요!! 괜히 더 자신감도 생기구요!!",
          "original_form": "맞아요!! 괜히 더 자신감도 생기구요!!",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008429.1.25",
          "form": "그 맛에 여행가고 그러는거 같네요",
          "original_form": "그 맛에 여행가고 그러는거 같네요",
