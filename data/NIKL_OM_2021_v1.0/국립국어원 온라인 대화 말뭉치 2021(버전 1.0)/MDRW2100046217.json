      "utterance": [
        {
          "id": "MDRW2100046217.1.1",
          "form": "점심 그럼 어떤거 드세요?",
          "original_form": "점심 그럼 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046217.1.2",
          "form": "식단 안한다해놓고 웃기지만..",
          "original_form": "식단 안한다해놓고 웃기지만..",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046217.1.3",
          "form": "오늘 점심은 샐러드입니다",
          "original_form": "오늘 점심은 샐러드입니다",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046217.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046217.1.5",
          "form": "헉 ㅋㅋㅋ ㅋ",
          "original_form": "헉 ㅋㅋㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046217.1.6",
          "form": "철저하신데요",
          "original_form": "철저하신데요",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046217.1.7",
          "form": "ㅋㄱㅋㅋㄱㄱㅋㅋㄱㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㄱㅋㅋㄱㄱㅋㅋㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.8",
          "form": "오늘 같이 먹기로 한 친구가 바디프로필을 찍는다고해서",
          "original_form": "오늘 같이 먹기로 한 친구가 바디프로필을 찍는다고해서",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맛있어서드시나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맛있어서드시나요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.10",
          "form": "샐러드 먹기로했어요!",
          "original_form": "샐러드 먹기로했어요!",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.12",
          "form": "네네 맛있어서 먹기도 해요",
          "original_form": "네네 맛있어서 먹기도 해요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.13",
          "form": "바디프로필 많이하더라구요",
          "original_form": "바디프로필 많이하더라구요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.14",
          "form": "저는 소스 왕창뿌려서",
          "original_form": "저는 소스 왕창뿌려서",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.15",
          "form": "샐러드 좋아하세요?",
          "original_form": "샐러드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046217.1.16",
          "form": "ㅋㅈㅋㅋㅋ바디프로필 찍는다는분은 소스도많이안하시려나요?",
          "original_form": "ㅋㅈㅋㅋㅋ바디프로필 찍는다는분은 소스도많이안하시려나요?",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046217.1.17",
          "form": "그럴것같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그럴것같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046217.1.18",
          "form": "저는 연어 샐러드 이런거좋아해요",
          "original_form": "저는 연어 샐러드 이런거좋아해요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046217.1.19",
          "form": "헉 연어ㅠㅠ 맛있죠",
          "original_form": "헉 연어ㅠㅠ 맛있죠",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046217.1.20",
          "form": "그쵸 연어샐러드 이런거 소스랑해서먹음맛있죠",
          "original_form": "그쵸 연어샐러드 이런거 소스랑해서먹음맛있죠",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046217.1.21",
          "form": "연어 말씀하시니까 갑자기 초밥먹고싶네요",
          "original_form": "연어 말씀하시니까 갑자기 초밥먹고싶네요",
