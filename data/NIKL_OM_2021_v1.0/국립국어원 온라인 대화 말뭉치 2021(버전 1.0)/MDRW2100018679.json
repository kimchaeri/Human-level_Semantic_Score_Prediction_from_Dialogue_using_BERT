      "utterance": [
        {
          "id": "MDRW2100018679.1.1",
          "form": "저도 장르 안가리고 그냥 많이봐요",
          "original_form": "저도 장르 안가리고 그냥 많이봐요",
          "speaker_id": "2",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018679.1.2",
          "form": "스릴러라고 해야하나",
          "original_form": "스릴러라고 해야하나",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018679.1.3",
          "form": "남들이 망작이라하는것들도 사실 많이봤어요 ㅋㅋㅋㅋ",
          "original_form": "남들이 망작이라하는것들도 사실 많이봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018679.1.4",
          "form": "보이스 같은건 좀 안 맞았어요 뭐 죄다 까만화면만 나로고",
          "original_form": "보이스 같은건 좀 안 맞았어요 뭐 죄다 까만화면만 나로고",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018679.1.5",
          "form": "나오고",
          "original_form": "나오고",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018679.1.6",
          "form": "아 저도요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 저도요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100018679.1.7",
          "form": "어떻게든 잔인한것만",
          "original_form": "어떻게든 잔인한것만",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.8",
          "form": "어두워욬ㅋㅋㅋ",
          "original_form": "어두워욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.9",
          "form": "무서운게아니라 잔인해서",
          "original_form": "무서운게아니라 잔인해서",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.10",
          "form": "잔인한거못봐요 ㅜㅜ",
          "original_form": "잔인한거못봐요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.11",
          "form": "ㅋㅋㅋ저랑비슷하시네요",
          "original_form": "ㅋㅋㅋ저랑비슷하시네요",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.12",
          "form": "근데 손더게는 잘봤어욬ㅋㅋㅋ",
          "original_form": "근데 손더게는 잘봤어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.13",
          "form": "ocn이 거의 다 이러잖아욬ㅋㅋㅋ",
          "original_form": "ocn이 거의 다 이러잖아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.14",
          "form": "어 그거말하려고했는데!",
          "original_form": "어 그거말하려고했는데!",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.15",
          "form": "맞어요 구해줘도 보다 말았어요",
          "original_form": "맞어요 구해줘도 보다 말았어요",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.16",
          "form": "손더게도 안 보긴했는데 재밌나요?",
          "original_form": "손더게도 안 보긴했는데 재밌나요?",
          "speaker_id": "1",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.17",
          "form": "그거 존잼이에요",
          "original_form": "그거 존잼이에요",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100018679.1.18",
          "form": "손더게 진짜 무섭긴한데 소재자체가 진짜 독특하잖아요 몰입감 쩔었어요",
          "original_form": "손더게 진짜 무섭긴한데 소재자체가 진짜 독특하잖아요 몰입감 쩔었어요",
