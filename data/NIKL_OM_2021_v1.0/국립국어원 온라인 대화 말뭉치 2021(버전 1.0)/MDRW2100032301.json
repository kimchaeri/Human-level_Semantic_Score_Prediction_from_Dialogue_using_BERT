      "utterance": [
        {
          "id": "MDRW2100032301.1.1",
          "form": "저는 게임을 많이 해요",
          "original_form": "저는 게임을 많이 해요",
          "speaker_id": "2",
          "time": "20210905 15:04"
        },
        {
          "id": "MDRW2100032301.1.2",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100032301.1.3",
          "form": "요샌 뭔 겜 하고 잇음?",
          "original_form": "요샌 뭔 겜 하고 잇음?",
          "speaker_id": "1",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100032301.1.4",
          "form": "쿠킹덤?",
          "original_form": "쿠킹덤?",
          "speaker_id": "1",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100032301.1.5",
          "form": "ㅇㅇ 쿠새는 쿠킹덤",
          "original_form": "ㅇㅇ 쿠새는 쿠킹덤",
          "speaker_id": "2",
          "time": "20210905 15:06"
        },
        {
          "id": "MDRW2100032301.1.6",
          "form": "아니 쿠새는 뭐야",
          "original_form": "아니 쿠새는 뭐야",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032301.1.7",
          "form": "요새",
          "original_form": "요새",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032301.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅌㅌㅌ",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032301.1.9",
          "form": "아직도 노가다심해? 아니먄 이제 덜해?",
          "original_form": "아직도 노가다심해? 아니먄 이제 덜해?",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032301.1.10",
          "form": "노가다 더 심해짐",
          "original_form": "노가다 더 심해짐",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032301.1.11",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "1",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100032301.1.12",
          "form": "그리고 쿠킹덤 요새",
          "original_form": "그리고 쿠킹덤 요새",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032301.1.13",
          "form": "난 절대 못하겟네",
          "original_form": "난 절대 못하겟네",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032301.1.14",
          "form": "일본 시장 노리고 잇더라",
          "original_form": "일본 시장 노리고 잇더라",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032301.1.15",
          "form": "난 로오히한 이후로 오토에 맛들림",
          "original_form": "난 로오히한 이후로 오토에 맛들림",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032301.1.16",
          "form": "뭐 다 글켓지 한국다음은 일본이지",
          "original_form": "뭐 다 글켓지 한국다음은 일본이지",
          "speaker_id": "1",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032301.1.17",
          "form": "오토 좋아하면 쿠킹덤 암걸림",
          "original_form": "오토 좋아하면 쿠킹덤 암걸림",
          "speaker_id": "2",
          "time": "20210905 15:08"
        },
        {
          "id": "MDRW2100032301.1.18",
          "form": "그래거 안하자너",
          "original_form": "그래거 안하자너",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100032301.1.19",
          "form": "오토최고",
          "original_form": "오토최고",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100032301.1.20",
          "form": "최손을 다해 오토를 돌린다",
          "original_form": "최손을 다해 오토를 돌린다",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100032301.1.21",
          "form": "저희도 오토를 주세요",
          "original_form": "저희도 오토를 주세요",
          "speaker_id": "2",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100032301.1.22",
          "form": "쿠킹덤 일본 광고 봤니?",
          "original_form": "쿠킹덤 일본 광고 봤니?",
          "speaker_id": "2",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100032301.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 니넨 오토로 할 수 있는게잇어?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 니넨 오토로 할 수 있는게잇어?",
          "speaker_id": "1",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100032301.1.24",
          "form": "아닝 이상해?",
          "original_form": "아닝 이상해?",
          "speaker_id": "1",
          "time": "20210905 15:10"
        },
        {
          "id": "MDRW2100032301.1.25",
          "form": "아니 일단 오토 기능이 있긴 한데",
          "original_form": "아니 일단 오토 기능이 있긴 한데",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.26",
          "form": "스테미나를 쓸 때 일일이",
          "original_form": "스테미나를 쓸 때 일일이",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.27",
          "form": "전투시작 버튼을 눌러줘야 함",
          "original_form": "전투시작 버튼을 눌러줘야 함",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.28",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.29",
          "form": "그리고 길드전은",
          "original_form": "그리고 길드전은",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.30",
          "form": "우린 20회연속잇지롱",
          "original_form": "우린 20회연속잇지롱",
          "speaker_id": "1",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.31",
          "form": "오토가 오토가 아니야..",
          "original_form": "오토가 오토가 아니야..",
          "speaker_id": "2",
          "time": "20210905 15:11"
        },
        {
          "id": "MDRW2100032301.1.32",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032301.1.33",
          "form": "애들이 멍청해서?",
          "original_form": "애들이 멍청해서?",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032301.1.34",
          "form": "타이밍 맞춰서 스킬 써야함",
          "original_form": "타이밍 맞춰서 스킬 써야함",
          "speaker_id": "2",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032301.1.35",
          "form": "그럼 뭐가 오토인데",
          "original_form": "그럼 뭐가 오토인데",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032301.1.36",
          "form": "전혀 오토가 어니잖아",
          "original_form": "전혀 오토가 어니잖아",
          "speaker_id": "1",
          "time": "20210905 15:12"
        },
        {
          "id": "MDRW2100032301.1.37",
          "form": "그리고 생산 물품 망드는건",
          "original_form": "그리고 생산 물품 망드는건",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032301.1.38",
          "form": "걍 노동 그자체야",
          "original_form": "걍 노동 그자체야",
          "speaker_id": "2",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100032301.1.39",
          "form": "이거 최저시급 줘야함",
          "original_form": "이거 최저시급 줘야함",
          "speaker_id": "2",
          "time": "20210905 15:14"
        }
      ]
    }
  ]
