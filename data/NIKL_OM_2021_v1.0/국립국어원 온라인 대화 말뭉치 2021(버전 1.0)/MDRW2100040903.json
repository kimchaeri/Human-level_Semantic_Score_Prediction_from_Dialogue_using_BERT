      "utterance": [
        {
          "id": "MDRW2100040903.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 11:56"
        },
        {
          "id": "MDRW2100040903.1.2",
          "form": "안녕하세요@",
          "original_form": "안녕하세요@",
          "speaker_id": "2",
          "time": "20210905 11:57"
        },
        {
          "id": "MDRW2100040903.1.3",
          "form": "혹시 백신맞으셨나요",
          "original_form": "혹시 백신맞으셨나요",
          "speaker_id": "1",
          "time": "20210905 11:57"
        },
        {
          "id": "MDRW2100040903.1.4",
          "form": "네 이번에 1차 맞았어요!",
          "original_form": "네 이번에 1차 맞았어요!",
          "speaker_id": "2",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040903.1.5",
          "form": "제이님은요??",
          "original_form": "제이님은요??",
          "speaker_id": "2",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040903.1.6",
          "form": "전 이번에 2차 맞았어요ㅎ",
          "original_form": "전 이번에 2차 맞았어요ㅎ",
          "speaker_id": "1",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040903.1.7",
          "form": "1차맞고 괜찮으셨나요?",
          "original_form": "1차맞고 괜찮으셨나요?",
          "speaker_id": "1",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040903.1.8",
          "form": "4일동안 죽어있었어요 ㅠㅠ",
          "original_form": "4일동안 죽어있었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100040903.1.9",
          "form": "처음엔 괜찮다가 4일째에 팔이 갑자기 아프더라구요 ㅠ",
          "original_form": "처음엔 괜찮다가 4일째에 팔이 갑자기 아프더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100040903.1.10",
          "form": "2차 많이 아프다던데 괜찮으세요?",
          "original_form": "2차 많이 아프다던데 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100040903.1.11",
          "form": "주사맞은팔요?",
          "original_form": "주사맞은팔요?",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100040903.1.12",
          "form": "저는 2차 때 몸살나듯 아팠어요ㅜ",
          "original_form": "저는 2차 때 몸살나듯 아팠어요ㅜ",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100040903.1.13",
          "form": "네 주사맞은팔이요 ㅠㅠ",
          "original_form": "네 주사맞은팔이요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100040903.1.14",
          "form": "2차때가 1차보다 더 아프다고해서 걱정돼요 ㅠㅠ",
          "original_form": "2차때가 1차보다 더 아프다고해서 걱정돼요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:01"
        },
        {
          "id": "MDRW2100040903.1.15",
          "form": "지금은 몸 괜찮으신거죠?",
          "original_form": "지금은 몸 괜찮으신거죠?",
          "speaker_id": "2",
          "time": "20210905 12:01"
        },
        {
          "id": "MDRW2100040903.1.16",
          "form": "1차 때는 졸립기만하고 팔근육통있고 기타등등 아팠고...",
          "original_form": "1차 때는 졸립기만하고 팔근육통있고 기타등등 아팠고...",
          "speaker_id": "1",
          "time": "20210905 12:01"
        },
        {
          "id": "MDRW2100040903.1.17",
          "form": "2차는 열나고 몸살처럼 아팠는데 타이레놀먹고 괜찮아졌어요ㅎ",
          "original_form": "2차는 열나고 몸살처럼 아팠는데 타이레놀먹고 괜찮아졌어요ㅎ",
          "speaker_id": "1",
          "time": "20210905 12:01"
        },
        {
          "id": "MDRW2100040903.1.18",
          "form": "주사맞은쪽 겨드랑이 임파선이 붓기도 하더라구요ㅜ",
          "original_form": "주사맞은쪽 겨드랑이 임파선이 붓기도 하더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100040903.1.19",
          "form": "사바사지만ㅜ 수다쟁이님은 안아프시길!!",
          "original_form": "사바사지만ㅜ 수다쟁이님은 안아프시길!!",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100040903.1.20",
          "form": "아이고 ㅠ 고생하셨네요 ㅠㅠ",
          "original_form": "아이고 ㅠ 고생하셨네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100040903.1.21",
          "form": "역시 타이레놀 챙겨놔야할 것 같아요 ㅠ",
          "original_form": "역시 타이레놀 챙겨놔야할 것 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100040903.1.22",
          "form": "맞아요 타이레놀 꼭 챙겨놔야해요!",
          "original_form": "맞아요 타이레놀 꼭 챙겨놔야해요!",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100040903.1.23",
          "form": "1차 때는 몸아픈거 참을만해서 타이레놀안먹었는데",
          "original_form": "1차 때는 몸아픈거 참을만해서 타이레놀안먹었는데",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040903.1.24",
          "form": "2차 때는 아침 저녁으로 먹어줬어요",
          "original_form": "2차 때는 아침 저녁으로 먹어줬어요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040903.1.25",
          "form": "아고... ㅠ",
          "original_form": "아고... ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040903.1.26",
          "form": "2차가 확실히 후폭풍이 장난 아니군요 ㅠㅠ",
          "original_form": "2차가 확실히 후폭풍이 장난 아니군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100040903.1.27",
          "form": "그냥 3일 내리 자야 괜찮아지겠어요 ㅠ",
          "original_form": "그냥 3일 내리 자야 괜찮아지겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100040903.1.28",
          "form": "자는거 진짜 추천드려요ㅜ",
          "original_form": "자는거 진짜 추천드려요ㅜ",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100040903.1.29",
          "form": "1차때도 이틀 내내 거의 기절해있었는데 하하 ㅠㅠ",
          "original_form": "1차때도 이틀 내내 거의 기절해있었는데 하하 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100040903.1.30",
          "form": "운동 하도 안해서 체력이 떨어졌나봐요 ㅠ",
          "original_form": "운동 하도 안해서 체력이 떨어졌나봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100040903.1.31",
          "form": "ㅜㅜ맞아요",
          "original_form": "ㅜㅜ맞아요",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100040903.1.32",
          "form": "운동 잘 안하고 그러니까 근육도 빠지고 그래요...ㅋㅋㅋ",
          "original_form": "운동 잘 안하고 그러니까 근육도 빠지고 그래요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:12"
        }
      ]
