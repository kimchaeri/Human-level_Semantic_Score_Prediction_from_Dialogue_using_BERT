      "utterance": [
        {
          "id": "MDRW2100003236.1.1",
          "form": "코로나 끝나면",
          "original_form": "코로나 끝나면",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100003236.1.2",
          "form": "하고싶은 모임 있나요?",
          "original_form": "하고싶은 모임 있나요?",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100003236.1.3",
          "form": "음 저는 클라이밍 배워보고 싶어서",
          "original_form": "음 저는 클라이밍 배워보고 싶어서",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100003236.1.4",
          "form": "그런 모임 해보고 싶어용",
          "original_form": "그런 모임 해보고 싶어용",
          "speaker_id": "1",
          "time": "20210825 00:15"
        },
        {
          "id": "MDRW2100003236.1.5",
          "form": "클라이밍 좋죠 !!!!",
          "original_form": "클라이밍 좋죠 !!!!",
          "speaker_id": "2",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100003236.1.6",
          "form": "전 마라톤이요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 마라톤이요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100003236.1.7",
          "form": "우와 마라톤 멋져요",
          "original_form": "우와 마라톤 멋져요",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100003236.1.8",
          "form": "마라톤 동호회는 진짜 친해지지 않을까요ㅋㅋㅋ",
          "original_form": "마라톤 동호회는 진짜 친해지지 않을까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100003236.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 네 한번",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네 한번",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100003236.1.10",
          "form": "달려보고 싶어요",
          "original_form": "달려보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100003236.1.11",
          "form": "운동 동호회도 좋은데 생각해보니까",
          "original_form": "운동 동호회도 좋은데 생각해보니까",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100003236.1.12",
          "form": "막 떡볶이 동호회 이런것도 재밌을거같아요",
          "original_form": "막 떡볶이 동호회 이런것도 재밌을거같아요",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100003236.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 헐",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 헐",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100003236.1.14",
          "form": "떡볶이",
          "original_form": "떡볶이",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100003236.1.15",
          "form": "동호회도 있나요?",
          "original_form": "동호회도 있나요?",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100003236.1.16",
          "form": "그 두끼떡볶이 만든 분이",
          "original_form": "그 두끼떡볶이 만든 분이",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100003236.1.17",
          "form": "떡볶이 동호회 했었대요ㅋㅋㅋ",
          "original_form": "떡볶이 동호회 했었대요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100003236.1.18",
          "form": "헐 ㅋㅋㅋㅋ ㅋㅋㅋㅋㅋ",
          "original_form": "헐 ㅋㅋㅋㅋ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100003236.1.19",
          "form": "떡볶이 좋아하는 사람들이",
          "original_form": "떡볶이 좋아하는 사람들이",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100003236.1.20",
          "form": "잔뜩 모였네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잔뜩 모였네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:28"
        },
        {
          "id": "MDRW2100003236.1.21",
          "form": "너무 웃기죠ㅋㅋㅋ관광버스 같은 거 빌려서",
          "original_form": "너무 웃기죠ㅋㅋㅋ관광버스 같은 거 빌려서",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100003236.1.22",
          "form": "사람들끼리 다녔대요",
          "original_form": "사람들끼리 다녔대요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100003236.1.23",
          "form": "네ㅋㅋㅋㅋㅋ 근데 엄청 재밌을 거 같아요",
