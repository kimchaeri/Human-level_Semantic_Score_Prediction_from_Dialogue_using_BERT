        {
          "id": "MDRW2100043768.1.3",
          "form": "읭",
          "original_form": "읭",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043768.1.4",
          "form": "메밀칼국수 나왔는데",
          "original_form": "메밀칼국수 나왔는데",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043768.1.5",
          "form": "걍 비빔 먹을걸 그랫나...",
          "original_form": "걍 비빔 먹을걸 그랫나...",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043768.1.6",
          "form": "으잉",
          "original_form": "으잉",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043768.1.7",
          "form": "별로야??",
          "original_form": "별로야??",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043768.1.8",
          "form": "딱히 특별한 맛은 없는듯...",
          "original_form": "딱히 특별한 맛은 없는듯...",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043768.1.9",
          "form": "ㅇㅎ......역시 맛집은",
          "original_form": "ㅇㅎ......역시 맛집은",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043768.1.10",
          "form": "맛집의 룰을 따라야하는것인가......",
          "original_form": "맛집의 룰을 따라야하는것인가......",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043768.1.11",
          "form": "그런가봐...",
          "original_form": "그런가봐...",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043768.1.12",
          "form": "지난번에 만두 맛있었는데",
          "original_form": "지난번에 만두 맛있었는데",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043768.1.13",
          "form": "양이 너무 적어서 다시 안시켰거든...",
          "original_form": "양이 너무 적어서 다시 안시켰거든...",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043768.1.14",
          "form": "ㅇㅎ......",
          "original_form": "ㅇㅎ......",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043768.1.15",
          "form": "걍 한가지",
          "original_form": "걍 한가지",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043768.1.16",
          "form": "겪어본셈 치자",
          "original_form": "겪어본셈 치자",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043768.1.17",
          "form": "쳇...",
          "original_form": "쳇...",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.18",
          "form": "그리고 사실",
          "original_form": "그리고 사실",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.19",
          "form": "여기 비빔이 그냥 비빔이 아니고",
          "original_form": "여기 비빔이 그냥 비빔이 아니고",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.20",
          "form": "회막국수라고",
          "original_form": "회막국수라고",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.21",
          "form": "명태 무친거 같이 올려놓고",
          "original_form": "명태 무친거 같이 올려놓고",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.22",
          "form": "8500원 받아서",
          "original_form": "8500원 받아서",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.23",
          "form": "ㅇㅎ 디게",
          "original_form": "ㅇㅎ 디게",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043768.1.24",
          "form": "고퀄 비빔국수구나",
          "original_form": "고퀄 비빔국수구나",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043768.1.25",
          "form": "그닥...?",
          "original_form": "그닥...?",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043768.1.26",
          "form": "양념 다 똑같고",
          "original_form": "양념 다 똑같고",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043768.1.27",
          "form": "명태 무친것만 올라가는데",
          "original_form": "명태 무친것만 올라가는데",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043768.1.28",
          "form": "이러면 이상하게 비싸더라구 ㅋㅋㅋ",
          "original_form": "이러면 이상하게 비싸더라구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043768.1.29",
          "form": "ㅇㅎ.....",
          "original_form": "ㅇㅎ.....",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043768.1.30",
          "form": "ㅇㅏ 귀찮다.....",
          "original_form": "ㅇㅏ 귀찮다.....",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043768.1.31",
          "form": "걍 야식으로 먹을까",
          "original_form": "걍 야식으로 먹을까",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043768.1.32",
          "form": "아냐 맘먹었을때 먹어 ㅋㅋㅋ",
          "original_form": "아냐 맘먹었을때 먹어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043768.1.33",
          "form": "이미 귀찮아졌어.......",
          "original_form": "이미 귀찮아졌어.......",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043768.1.34",
          "form": "ㅋㅋㅋ 그래도 지금 먹고",
          "original_form": "ㅋㅋㅋ 그래도 지금 먹고",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043768.1.35",
          "form": "야식 땡기면 그때 더 먹어",
          "original_form": "야식 땡기면 그때 더 먹어",
