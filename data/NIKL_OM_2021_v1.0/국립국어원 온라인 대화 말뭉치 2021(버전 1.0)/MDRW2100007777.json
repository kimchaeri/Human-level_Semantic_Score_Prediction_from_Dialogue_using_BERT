      "utterance": [
        {
          "id": "MDRW2100007777.1.1",
          "form": "점심은 뭐 드실 예정이세요",
          "original_form": "점심은 뭐 드실 예정이세요",
          "speaker_id": "1",
          "time": "20210822 10:33"
        },
        {
          "id": "MDRW2100007777.1.2",
          "form": "아직안정했어요",
          "original_form": "아직안정했어요",
          "speaker_id": "2",
          "time": "20210822 10:35"
        },
        {
          "id": "MDRW2100007777.1.3",
          "form": "뭐드실거예요?",
          "original_form": "뭐드실거예요?",
          "speaker_id": "2",
          "time": "20210822 10:35"
        },
        {
          "id": "MDRW2100007777.1.4",
          "form": "저 피자먹을까 생각중이에요",
          "original_form": "저 피자먹을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210822 10:35"
        },
        {
          "id": "MDRW2100007777.1.5",
          "form": "피자좋아하세요",
          "original_form": "피자좋아하세요",
          "speaker_id": "1",
          "time": "20210822 10:35"
        },
        {
          "id": "MDRW2100007777.1.6",
          "form": "우와 저는 피자는잘안먹어서",
          "original_form": "우와 저는 피자는잘안먹어서",
          "speaker_id": "2",
          "time": "20210822 10:35"
        },
        {
          "id": "MDRW2100007777.1.7",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.8",
          "form": "매일 콤비네이션만먹어요",
          "original_form": "매일 콤비네이션만먹어요",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.9",
          "form": "콤비네이션 맛있어요",
          "original_form": "콤비네이션 맛있어요",
          "speaker_id": "1",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.10",
          "form": "피자추천해주세요",
          "original_form": "피자추천해주세요",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.11",
          "form": "저는 닭 들어간거 좋아해요 핫치킨피자나 로스트치킨피자",
          "original_form": "저는 닭 들어간거 좋아해요 핫치킨피자나 로스트치킨피자",
          "speaker_id": "1",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.12",
          "form": "그건한번도안먹어봤는데",
          "original_form": "그건한번도안먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.13",
          "form": "약간 불고기 이런거는 안좋아하고 포테이토 피자도 좋아해요",
          "original_form": "약간 불고기 이런거는 안좋아하고 포테이토 피자도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.14",
          "form": "핫치킨맛있을거같아요",
          "original_form": "핫치킨맛있을거같아요",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.15",
          "form": "매운걸좋아해서",
          "original_form": "매운걸좋아해서",
          "speaker_id": "2",
          "time": "20210822 10:36"
        },
        {
          "id": "MDRW2100007777.1.16",
          "form": "매콤함이 매력적이죠",
          "original_form": "매콤함이 매력적이죠",
          "speaker_id": "1",
          "time": "20210822 10:36"
        }
      ]
