      "utterance": [
        {
          "id": "MDRW2100020120.1.1",
          "form": "오늘 한잔 하신다 하지 않았오요 ㅎㅎ?",
          "original_form": "오늘 한잔 하신다 하지 않았오요 ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020120.1.2",
          "form": "요리잘하시죠?",
          "original_form": "요리잘하시죠?",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020120.1.3",
          "form": "아아ㅎㅎ 아직못하고있네요ㅎ",
          "original_form": "아아ㅎㅎ 아직못하고있네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.4",
          "form": "잘하는건 먹는 사람이 판단 ㅋ",
          "original_form": "잘하는건 먹는 사람이 판단 ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.5",
          "form": "그냥 티비틀어놓고ㅎㅎ",
          "original_form": "그냥 티비틀어놓고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.6",
          "form": "전 즐거워요 요리가 ㅎㅎ",
          "original_form": "전 즐거워요 요리가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.7",
          "form": "그래도 주위에 클래스를 열정도면 ㅎ",
          "original_form": "그래도 주위에 클래스를 열정도면 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.8",
          "form": "잘하시는거죠ㅎㅎ",
          "original_form": "잘하시는거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.9",
          "form": "재밋으니까여 ㅎㅎ",
          "original_form": "재밋으니까여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020120.1.10",
          "form": "ㅎㅎ 재밌죠ㅎ",
          "original_form": "ㅎㅎ 재밌죠ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020120.1.11",
          "form": "오늘의 안주는 무엇입니까 ㅎㅎ",
          "original_form": "오늘의 안주는 무엇입니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020120.1.12",
          "form": "능력잇으시구만요ㅎㅎ",
          "original_form": "능력잇으시구만요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020120.1.13",
          "form": "모르겟어요ㅎㅎㅎㅎ",
          "original_form": "모르겟어요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020120.1.14",
          "form": "12시가넘어서 귀찮고ㅎㅎ 내일 일가야해서ㅎㅎ",
          "original_form": "12시가넘어서 귀찮고ㅎㅎ 내일 일가야해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.15",
          "form": "깡술은 먹지마요 속안좋아져요",
          "original_form": "깡술은 먹지마요 속안좋아져요",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.16",
          "form": "술도없어요 ㅡㅎㅎ",
          "original_form": "술도없어요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.17",
          "form": "아항 다행이네여 그럼 먹지마여 ㅎㅎ",
          "original_form": "아항 다행이네여 그럼 먹지마여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.18",
          "form": "술잘안먹어서ㅎㅎ",
          "original_form": "술잘안먹어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.19",
          "form": "술은 안좋은거임 ㅋㅋ",
          "original_form": "술은 안좋은거임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.20",
          "form": "속상할때만 한잔해요ㅎㅎ",
          "original_form": "속상할때만 한잔해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.21",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020120.1.22",
          "form": "그럼요 그럼요",
