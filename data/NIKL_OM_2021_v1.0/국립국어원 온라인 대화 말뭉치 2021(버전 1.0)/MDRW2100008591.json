      "utterance": [
        {
          "id": "MDRW2100008591.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008591.1.2",
          "form": "오늘 늦잠자서 첫끼를 1시 넘어서 먹었네요",
          "original_form": "오늘 늦잠자서 첫끼를 1시 넘어서 먹었네요",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008591.1.3",
          "form": "주말은 늦잠이죠",
          "original_form": "주말은 늦잠이죠",
          "speaker_id": "1",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008591.1.4",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008591.1.5",
          "form": "순두부찌개랑 계란후라이 먹었어요",
          "original_form": "순두부찌개랑 계란후라이 먹었어요",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008591.1.6",
          "form": "전 국밥",
          "original_form": "전 국밥",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008591.1.7",
          "form": "국밥 어떤거요?!",
          "original_form": "국밥 어떤거요?!",
          "speaker_id": "2",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008591.1.8",
          "form": "돼지요",
          "original_form": "돼지요",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008591.1.9",
          "form": "맛있겠어요 갑자기 국밥 먹고 싶어지네요",
          "original_form": "맛있겠어요 갑자기 국밥 먹고 싶어지네요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008591.1.10",
          "form": "돼지국밥은 돼지고기가 들은거죠?",
          "original_form": "돼지국밥은 돼지고기가 들은거죠?",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008591.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008591.1.12",
          "form": "전 돼지국밥은 먹어본적 없네요",
          "original_form": "전 돼지국밥은 먹어본적 없네요",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008591.1.13",
          "form": "순대국밥은 많이 먹었는데",
          "original_form": "순대국밥은 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008591.1.14",
          "form": "맛은 비슷해요 순대랑 돼지지",
          "original_form": "맛은 비슷해요 순대랑 돼지지",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008591.1.15",
          "form": "그래요??",
          "original_form": "그래요??",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008591.1.16",
          "form": "저녁은 뭐드실 예정이에요?",
          "original_form": "저녁은 뭐드실 예정이에요?",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008591.1.17",
          "form": "카레요",
          "original_form": "카레요",
          "speaker_id": "1",
