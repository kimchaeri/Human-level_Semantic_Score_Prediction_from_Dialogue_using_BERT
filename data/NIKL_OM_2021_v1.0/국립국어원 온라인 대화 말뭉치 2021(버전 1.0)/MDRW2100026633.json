      "utterance": [
        {
          "id": "MDRW2100026633.1.1",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026633.1.2",
          "form": "난 절대 문학...",
          "original_form": "난 절대 문학...",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026633.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026633.1.4",
          "form": "뭔가 문학은 다 상상하면서 읽는데",
          "original_form": "뭔가 문학은 다 상상하면서 읽는데",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.5",
          "form": "비문학은 그게 어려워...",
          "original_form": "비문학은 그게 어려워...",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.6",
          "form": "나도 뭐 비문핟이라해봤자",
          "original_form": "나도 뭐 비문핟이라해봤자",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.7",
          "form": "주식책밖에 안읽엇음ㅋㅋㅋㅋ",
          "original_form": "주식책밖에 안읽엇음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.8",
          "form": "그냥 내가 돈얘기를 젛아해서 그럴수도..",
          "original_form": "그냥 내가 돈얘기를 젛아해서 그럴수도..",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.10",
          "form": "근데 다들 돈은 좋어하는데",
          "original_form": "근데 다들 돈은 좋어하는데",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.11",
          "form": "근데 나 읽다가 충격이었던게",
          "original_form": "근데 나 읽다가 충격이었던게",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.13",
          "form": "그걸 어떻게 버느냐가",
          "original_form": "그걸 어떻게 버느냐가",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026633.1.14",
          "form": "진짜 차이가 큰듯",
          "original_form": "진짜 차이가 큰듯",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.15",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.16",
          "form": "도박",
          "original_form": "도박",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.17",
          "form": "으로",
          "original_form": "으로",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.18",
          "form": "한번에 벌고 싶어하는",
          "original_form": "한번에 벌고 싶어하는",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.19",
          "form": "사람도 있고",
          "original_form": "사람도 있고",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.20",
          "form": "말만하고 안버는 사람도 많으니까..",
          "original_form": "말만하고 안버는 사람도 많으니까..",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.21",
          "form": "암튼 뭐가 충격이였어?",
          "original_form": "암튼 뭐가 충격이였어?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.22",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026633.1.23",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.24",
          "form": "2008년에 미국발 경제위기 오고나서",
          "original_form": "2008년에 미국발 경제위기 오고나서",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.25",
          "form": "아직까지 회복을 못했다는게",
          "original_form": "아직까지 회복을 못했다는게",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.26",
          "form": "충격이었음",
          "original_form": "충격이었음",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.27",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.28",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.29",
          "form": "너무 무서운데",
          "original_form": "너무 무서운데",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.30",
          "form": "gdp도 엄청많이어르고",
          "original_form": "gdp도 엄청많이어르고",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.31",
          "form": "뭐 기술도 빠르게발전하고 그래서",
          "original_form": "뭐 기술도 빠르게발전하고 그래서",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026633.1.32",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.33",
          "form": "경제도 비슷한줄알았는데",
          "original_form": "경제도 비슷한줄알았는데",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.34",
          "form": "진짜 불과 한 16년?",
          "original_form": "진짜 불과 한 16년?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.35",
          "form": "몇년전까지도",
          "original_form": "몇년전까지도",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.36",
          "form": "미국이랑 일본은 경제회복이 너무 더뎌서 제로금리였대",
          "original_form": "미국이랑 일본은 경제회복이 너무 더뎌서 제로금리였대",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.37",
          "form": "아 일본은 뭐...",
          "original_form": "아 일본은 뭐...",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.38",
          "form": "말할게 없지",
          "original_form": "말할게 없지",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026633.1.39",
          "form": "일본은 그냥",
          "original_form": "일본은 그냥",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026633.1.40",
          "form": "가라앉는 배 같음",
          "original_form": "가라앉는 배 같음",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026633.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026633.1.42",
          "form": "다른나라 다 조금씩 성장하는데 지들은 하락함",
          "original_form": "다른나라 다 조금씩 성장하는데 지들은 하락함",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026633.1.43",
          "form": "심지어 중국도 조금은 오르는데",
          "original_form": "심지어 중국도 조금은 오르는데",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026633.1.44",
          "form": "일본은...",
          "original_form": "일본은...",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026633.1.45",
          "form": "유럽도 계속 하락중이라며",
          "original_form": "유럽도 계속 하락중이라며",
          "speaker_id": "1",
          "time": "20210901 23:27"
        }
      ]
