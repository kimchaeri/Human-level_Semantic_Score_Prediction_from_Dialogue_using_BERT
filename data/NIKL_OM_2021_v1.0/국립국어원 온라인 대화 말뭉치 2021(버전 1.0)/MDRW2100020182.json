        {
          "id": "MDRW2100020182.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020182.1.2",
          "form": "팬텀 싱어 알아",
          "original_form": "팬텀 싱어 알아",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.3",
          "form": "시즌 쓰리는 조금 봤어",
          "original_form": "시즌 쓰리는 조금 봤어",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.4",
          "form": "너 음악 좀 아는구나",
          "original_form": "너 음악 좀 아는구나",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.5",
          "form": "크로스오버라는 거 재미있더라",
          "original_form": "크로스오버라는 거 재미있더라",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.6",
          "form": "나는 크로스오버라는 거를 처음 들어 봤어",
          "original_form": "나는 크로스오버라는 거를 처음 들어 봤어",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.7",
          "form": "맞아 나도 팬텀 싱어로 처음 들어 봤어",
          "original_form": "맞아 나도 팬텀 싱어로 처음 들어 봤어",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.8",
          "form": "나는 결승에서 투표 도 했어",
          "original_form": "나는 결승에서 투표 도 했어",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.9",
          "form": "응원했어? 어떤 팀?",
          "original_form": "응원했어? 어떤 팀?",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020182.1.10",
          "form": "라포엠이라는 팀이야",
          "original_form": "라포엠이라는 팀이야",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020182.1.11",
          "form": "아라포엠이라는 그룹 되게 잘하더라 제일",
          "original_form": "아라포엠이라는 그룹 되게 잘하더라 제일",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020182.1.12",
          "form": "얼굴도 제일 잘생겼어",
          "original_form": "얼굴도 제일 잘생겼어",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020182.1.13",
          "form": "펌 싱어에서 얼굴은 잘 모를",
          "original_form": "펌 싱어에서 얼굴은 잘 모를",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020182.1.14",
          "form": "텀팬텀 싱어",
          "original_form": "텀팬텀 싱어",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020182.1.15",
          "form": "성악가라서 키가 다들 커",
          "original_form": "성악가라서 키가 다들 커",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020182.1.16",
          "form": "이번 신곡 들어봤어?",
          "original_form": "이번 신곡 들어봤어?",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020182.1.17",
          "form": "당연히 들어봤지 지금도 무한 재생 중이야",
          "original_form": "당연히 들어봤지 지금도 무한 재생 중이야",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020182.1.18",
          "form": "라포엠 되게 좋아하는 거",
          "original_form": "라포엠 되게 좋아하는 거",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020182.1.19",
          "form": "구나",
          "original_form": "구나",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020182.1.20",
          "form": "다음에 너도 같이 노래 들어보자",
          "original_form": "다음에 너도 같이 노래 들어보자",
