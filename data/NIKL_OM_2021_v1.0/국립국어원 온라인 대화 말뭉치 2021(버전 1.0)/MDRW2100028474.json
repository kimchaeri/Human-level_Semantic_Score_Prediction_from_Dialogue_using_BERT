      "utterance": [
        {
          "id": "MDRW2100028474.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028474.1.2",
          "form": "아뇨ㅡㅠ",
          "original_form": "아뇨ㅡㅠ",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028474.1.3",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100028474.1.4",
          "form": "저도 키우지 않아요",
          "original_form": "저도 키우지 않아요",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028474.1.5",
          "form": "근데 고양이 정말 좋아해요",
          "original_form": "근데 고양이 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028474.1.6",
          "form": "너무 이쁘죠 애옹",
          "original_form": "너무 이쁘죠 애옹",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028474.1.7",
          "form": "여건만되면 정말 데려오고싶어요",
          "original_form": "여건만되면 정말 데려오고싶어요",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028474.1.8",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028474.1.9",
          "form": "보호소에 애들이 너무많대요",
          "original_form": "보호소에 애들이 너무많대요",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028474.1.10",
          "form": "저는 옛날에 가끔 유기묘 보호하고 입양시켜주는 유기묘 카페 갔었어요",
          "original_form": "저는 옛날에 가끔 유기묘 보호하고 입양시켜주는 유기묘 카페 갔었어요",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028474.1.11",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028474.1.12",
          "form": "너무좋은곳이있네요ㅠ",
          "original_form": "너무좋은곳이있네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028474.1.13",
          "form": "포인핸드 같은거 가끔 보면서 나도 데려오고 싶단 생각 많이 해요",
          "original_form": "포인핸드 같은거 가끔 보면서 나도 데려오고 싶단 생각 많이 해요",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028474.1.14",
          "form": "아직 애들을 돌볼수있는 경제적 여건이 안되서",
          "original_form": "아직 애들을 돌볼수있는 경제적 여건이 안되서",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028474.1.15",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028474.1.16",
          "form": "정말 잘 해줄 수 있을 때 데려와야 맞는 것 같아요",
          "original_form": "정말 잘 해줄 수 있을 때 데려와야 맞는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028474.1.17",
          "form": "맞아요 괜한 욕심먼저 부리면",
          "original_form": "맞아요 괜한 욕심먼저 부리면",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028474.1.18",
          "form": "누구도행복할수없을거에요",
          "original_form": "누구도행복할수없을거에요",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028474.1.19",
          "form": "그날을 위해 열심히 돈을 벌어보는 걸로 ㅎㅎ",
          "original_form": "그날을 위해 열심히 돈을 벌어보는 걸로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028474.1.20",
          "form": "아 근데 저 고양이 좋아하는데",
          "original_form": "아 근데 저 고양이 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028474.1.21",
          "form": "고양이 알러지 있어서",
          "original_form": "고양이 알러지 있어서",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028474.1.22",
          "form": "데려와도 멍멍이 데려와야할거 같아요",
          "original_form": "데려와도 멍멍이 데려와야할거 같아요",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028474.1.23",
          "form": "어머 비극,,,!",
          "original_form": "어머 비극,,,!",
          "speaker_id": "2",
          "time": "20210902 20:20"
        }
      ]
