        },
        {
          "id": "MDRW2100039144.1.2",
          "form": "근데 제가 그 공대로 대학원 다니는 친구한테 물어보니까",
          "original_form": "근데 제가 그 공대로 대학원 다니는 친구한테 물어보니까",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.3",
          "form": "반도체 원하면",
          "original_form": "반도체 원하면",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.4",
          "form": "3명까지 지원해주나봐요",
          "original_form": "3명까지 지원해주나봐요",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.6",
          "form": "sem tem 찍는 데면",
          "original_form": "sem tem 찍는 데면",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.7",
          "form": "다 괜찮을거라고",
          "original_form": "다 괜찮을거라고",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.8",
          "form": "name3 교수님 랩실 괜찮을거라는데,,",
          "original_form": "&name3& 교수님 랩실 괜찮을거라는데,,",
          "speaker_id": "2",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.9",
          "form": "이제 안받으실거에요ㅜㅠ",
          "original_form": "이제 안받으실거에요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100039144.1.10",
          "form": "내년이 마지막이라고 알고있어요",
          "original_form": "내년이 마지막이라고 알고있어요",
          "speaker_id": "1",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100039144.1.11",
          "form": "왜냐면 kkk네 석사언니가 대학원 상담하면서",
          "original_form": "왜냐면 kkk네 석사언니가 대학원 상담하면서",
          "speaker_id": "1",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100039144.1.12",
          "form": "name3교수님도 너 석사 졸업할때 같이 가신다고 name4교수님이 그러셨대요",
          "original_form": "&name3&교수님도 너 석사 졸업할때 같이 가신다고 &name4&교수님이 그러셨대요",
          "speaker_id": "1",
          "time": "20210903 11:27"
        },
        {
          "id": "MDRW2100039144.1.13",
          "form": "헐 그러셨구나",
          "original_form": "헐 그러셨구나",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100039144.1.14",
          "form": "흐어어엉",
          "original_form": "흐어어엉",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100039144.1.15",
          "form": "가연씨 근데",
          "original_form": "가연씨 근데",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.16",
          "form": "어둠의 종사모 눈물 줄줄 흐릅니다",
          "original_form": "어둠의 종사모 눈물 줄줄 흐릅니다",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.18",
          "form": "삼성전자를 갈 수만 있다면",
          "original_form": "삼성전자를 갈 수만 있다면",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.19",
          "form": "저도 종사모ㅠ",
          "original_form": "저도 종사모ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.20",
          "form": "학사취업도 괜찮아요?",
          "original_form": "학사취업도 괜찮아요?",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.21",
          "form": "삼전 가고싶네요...",
          "original_form": "삼전 가고싶네요...",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.22",
          "form": "아니면 꼭 석사를 하고싶은거예요?",
          "original_form": "아니면 꼭 석사를 하고싶은거예요?",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100039144.1.23",
          "form": "삼전갈수있으면 석사는 필요할때 시작할래요ㅋㅋ",
          "original_form": "삼전갈수있으면 석사는 필요할때 시작할래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.24",
          "form": "오 그니까 취업이 먼저인거죠?",
          "original_form": "오 그니까 취업이 먼저인거죠?",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.25",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.26",
          "form": "산학인턴 어때요?",
          "original_form": "산학인턴 어때요?",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.27",
          "form": "친구한테 물어봤는데",
          "original_form": "친구한테 물어봤는데",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.29",
          "form": "TLO 알아보라고 하더라구요",
          "original_form": "TLO 알아보라고 하더라구요",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039144.1.30",
          "form": "굳이",
          "original_form": "굳이",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039144.1.31",
          "form": "대학원 갈 필요 없고",
          "original_form": "대학원 갈 필요 없고",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039144.1.32",
          "form": "산학인턴 하면",
          "original_form": "산학인턴 하면",
          "speaker_id": "2",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039144.1.33",
          "form": "인턴경험도 쌓고...",
          "original_form": "인턴경험도 쌓고...",
          "speaker_id": "2",
