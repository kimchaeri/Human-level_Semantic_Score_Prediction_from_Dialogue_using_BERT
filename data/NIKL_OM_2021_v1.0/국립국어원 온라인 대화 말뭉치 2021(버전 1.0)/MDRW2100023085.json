          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.2",
          "form": "솔직히 개콘은",
          "original_form": "솔직히 개콘은",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.3",
          "form": "10년전에 문닫았어도",
          "original_form": "10년전에 문닫았어도",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.4",
          "form": "안이상했어",
          "original_form": "안이상했어",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.5",
          "form": "ㅇㅇ 이미 열풍 끝난지 오래였음",
          "original_form": "ㅇㅇ 이미 열풍 끝난지 오래였음",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.6",
          "form": "약간 전성기를 못잃고",
          "original_form": "약간 전성기를 못잃고",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.7",
          "form": "우려먹기만 반복하다가",
          "original_form": "우려먹기만 반복하다가",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023085.1.8",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ난 김준호가",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ난 김준호가",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.9",
          "form": "뭐뭐했습니다람쥐",
          "original_form": "뭐뭐했습니다람쥐",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.10",
          "form": "아 진짜 도저히 안되겠다 싶어지니까 등떠밀리듯 종영한거지",
          "original_form": "아 진짜 도저히 안되겠다 싶어지니까 등떠밀리듯 종영한거지",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.11",
          "form": "이딴거할때",
          "original_form": "이딴거할때",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.12",
          "form": "아 답이 없구나 하고",
          "original_form": "아 답이 없구나 하고",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.13",
          "form": "안보게된듯",
          "original_form": "안보게된듯",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.14",
          "form": "ㅇㅇㅋㅋㅋㅋ 진짜 개노잼",
          "original_form": "ㅇㅇㅋㅋㅋㅋ 진짜 개노잼",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.16",
          "form": "근데 더 웃긴건",
          "original_form": "근데 더 웃긴건",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.17",
          "form": "예능ㅇ에 나와서",
          "original_form": "예능ㅇ에 나와서",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.18",
          "form": "근데 김준호는",
          "original_form": "근데 김준호는",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.19",
          "form": "계속 나와",
          "original_form": "계속 나와",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023085.1.20",
          "form": "다른 패널이 나 당신 출연 관심있게 봣다는 의미로",
          "original_form": "다른 패널이 나 당신 출연 관심있게 봣다는 의미로",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.21",
          "form": "그 드립 치면",
          "original_form": "그 드립 치면",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.22",
          "form": "저작권료 내라는 식으로 나오는거 ㅋㅋㅋㅋㅋ",
          "original_form": "저작권료 내라는 식으로 나오는거 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.23",
          "form": "돈 주고 쓰라고 해도 안써. ....",
          "original_form": "돈 주고 쓰라고 해도 안써. ....",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.24",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.25",
          "form": "얼탱",
          "original_form": "얼탱",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.26",
          "form": "ㅇㅇ절대안써",
          "original_form": "ㅇㅇ절대안써",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.27",
          "form": "쓰는 기관있으면",
          "original_form": "쓰는 기관있으면",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.28",
          "form": "거를듯",
          "original_form": "거를듯",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.29",
          "form": "진짜 노잼이야",
          "original_form": "진짜 노잼이야",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023085.1.30",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.31",
          "form": "솔직히 요즘 공공기관들",
          "original_form": "솔직히 요즘 공공기관들",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.32",
          "form": "아니 공공기관들",
          "original_form": "아니 공공기관들",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.33",
          "form": "홍보영상에 인터넷 드립좀 안썼으면",
          "original_form": "홍보영상에 인터넷 드립좀 안썼으면",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.34",
          "form": "진짜 없어보이는데 본인들만 모르나봐",
          "original_form": "진짜 없어보이는데 본인들만 모르나봐",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.35",
          "form": "접근성 낮아보이고 위트있는 발랄한 공공기관",
          "original_form": "접근성 낮아보이고 위트있는 발랄한 공공기관",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.36",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.37",
          "form": "이런거 좀 안하려고하면",
          "original_form": "이런거 좀 안하려고하면",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.38",
          "form": "안되나",
          "original_form": "안되나",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.39",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023085.1.40",
          "form": "아니 친근해지려고 하지 마....",
          "original_form": "아니 친근해지려고 하지 마....",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023085.1.41",
          "form": "대체 왜...?",
