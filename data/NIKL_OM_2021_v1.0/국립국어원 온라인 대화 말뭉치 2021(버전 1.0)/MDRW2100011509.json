      "utterance": [
        {
          "id": "MDRW2100011509.1.1",
          "form": "name2 밥은?",
          "original_form": "&name2& 밥은?",
          "speaker_id": "1",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100011509.1.2",
          "form": "과장님이랑 순대국밥먹었어",
          "original_form": "과장님이랑 순대국밥먹었어",
          "speaker_id": "2",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100011509.1.3",
          "form": "너는 머먹었엉",
          "original_form": "너는 머먹었엉",
          "speaker_id": "2",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100011509.1.4",
          "form": "파리바게뜨에서",
          "original_form": "파리바게뜨에서",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100011509.1.5",
          "form": "샐러드사다가 먹었워",
          "original_form": "샐러드사다가 먹었워",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100011509.1.6",
          "form": "뱃살 장난아님",
          "original_form": "뱃살 장난아님",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100011509.1.7",
          "form": "내앞에서 그런 폭언을하다니",
          "original_form": "내앞에서 그런 폭언을하다니",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100011509.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100011509.1.9",
          "form": "우리사무실근처에는",
          "original_form": "우리사무실근처에는",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100011509.1.10",
          "form": "아무것도 없어서 멀 먹고싶어도 못먹네 ㅜ",
          "original_form": "아무것도 없어서 멀 먹고싶어도 못먹네 ㅜ",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100011509.1.11",
          "form": "너 마통 금리얼마야?  나 오후에 마통뚫을라고",
          "original_form": "너 마통 금리얼마야?  나 오후에 마통뚫을라고",
          "speaker_id": "2",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100011509.1.12",
          "form": "나 마통 안써",
          "original_form": "나 마통 안써",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100011509.1.13",
          "form": "잘 모르겠어",
          "original_form": "잘 모르겠어",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100011509.1.14",
          "form": "너 얼마나오는데?",
          "original_form": "너 얼마나오는데?",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100011509.1.15",
          "form": "얼마가 나와야 잘 나오는거지",
          "original_form": "얼마가 나와야 잘 나오는거지",
          "speaker_id": "2",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100011509.1.16",
          "form": "나도 잘 모르겠어",
          "original_form": "나도 잘 모르겠어",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100011509.1.17",
          "form": "2~3??\r\n3~4??",
          "original_form": "2~3??\r\n3~4??",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100011509.1.18",
          "form": "신용대출 줄인다길래 만들어놓으려고",
          "original_form": "신용대출 줄인다길래 만들어놓으려고",
          "speaker_id": "2",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100011509.1.19",
          "form": "쓸일이.없지만 ㅋㅋㅋ",
          "original_form": "쓸일이.없지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:53"
        },
        {
          "id": "MDRW2100011509.1.20",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100011509.1.21",
          "form": "근데 주변에 마통 많이 뚫어 놓더라",
          "original_form": "근데 주변에 마통 많이 뚫어 놓더라",
          "speaker_id": "1",
          "time": "20210825 13:07"
        },
        {
          "id": "MDRW2100011509.1.22",
          "form": "선물왔다",
          "original_form": "선물왔다",
          "speaker_id": "1",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100011509.1.23",
          "form": "최근1 2년원청징수금액이 중요한데",
          "original_form": "최근1 2년원청징수금액이 중요한데",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100011509.1.24",
          "form": "나는 금액이 너무나 적네 ㅜ",
          "original_form": "나는 금액이 너무나 적네 ㅜ",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100011509.1.25",
          "form": "공무원우대 이런거 없어?",
          "original_form": "공무원우대 이런거 없어?",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100011509.1.26",
          "form": "생각해보니까",
          "original_form": "생각해보니까",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011509.1.27",
          "form": "나 신용대출있어서",
          "original_form": "나 신용대출있어서",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011509.1.28",
          "form": "마통 개설 안됐던것 같아",
          "original_form": "마통 개설 안됐던것 같아",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100011509.1.29",
          "form": "원청징수금액이 중요한데 나는 올해부터 시작이라 금액이 미미하네.",
          "original_form": "원청징수금액이 중요한데 나는 올해부터 시작이라 금액이 미미하네.",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100011509.1.30",
          "form": "다음에 만들어야겠어",
          "original_form": "다음에 만들어야겠어",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100011509.1.31",
          "form": "그래 그렇게해",
          "original_form": "그래 그렇게해",
          "speaker_id": "1",
          "time": "20210825 14:04"
        }
      ]
    }
  ]
