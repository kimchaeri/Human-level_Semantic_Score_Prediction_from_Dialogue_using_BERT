        },
        {
          "id": "MDRW2100042595.1.2",
          "form": "박보영배우 귀엽더리고요",
          "original_form": "박보영배우 귀엽더리고요",
          "speaker_id": "2",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042595.1.3",
          "form": "ㅎㅎ 보영배우 귀엽지 않나요?!?!",
          "original_form": "ㅎㅎ 보영배우 귀엽지 않나요?!?!",
          "speaker_id": "1",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042595.1.4",
          "form": "네 ㄱ엽더라공6",
          "original_form": "네 ㄱ엽더라공6",
          "speaker_id": "2",
          "time": "20210906 23:14"
        },
        {
          "id": "MDRW2100042595.1.5",
          "form": "ㅋㅋ맞아요 그럼 혹시 과속스캔들도 보셨나요?",
          "original_form": "ㅋㅋ맞아요 그럼 혹시 과속스캔들도 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042595.1.6",
          "form": "네 재밌기 봤아요",
          "original_form": "네 재밌기 봤아요",
          "speaker_id": "2",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042595.1.7",
          "form": "차태현배우가 연락잘하더라고요",
          "original_form": "차태현배우가 연락잘하더라고요",
          "speaker_id": "2",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042595.1.8",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042595.1.9",
          "form": "거기 배우들도 연기 전부 잘하죠!",
          "original_form": "거기 배우들도 연기 전부 잘하죠!",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100042595.1.10",
          "form": "네 연기잘해서 재밌더라고요",
          "original_form": "네 연기잘해서 재밌더라고요",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042595.1.11",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042595.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ그럼 재밌게 본 영화있나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럼 재밌게 본 영화있나요?",
          "speaker_id": "1",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042595.1.13",
          "form": "저는 도둑들 재밌었어요",
          "original_form": "저는 도둑들 재밌었어요",
          "speaker_id": "2",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042595.1.14",
          "form": "도둑들 재밌죠!",
          "original_form": "도둑들 재밌죠!",
          "speaker_id": "1",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042595.1.15",
          "form": "전지현 배우가 넘 좋더라고요ㅜㅜㅋ",
          "original_form": "전지현 배우가 넘 좋더라고요ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210906 23:16"
        },
        {
          "id": "MDRW2100042595.1.16",
          "form": "저는 김혜수배우요",
          "original_form": "저는 김혜수배우요",
          "speaker_id": "2",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100042595.1.17",
          "form": "김혜수배우! 좋죠!!!!",
          "original_form": "김혜수배우! 좋죠!!!!",
          "speaker_id": "1",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100042595.1.18",
          "form": "김혜수배우 좋아하시나요?",
          "original_form": "김혜수배우 좋아하시나요?",
