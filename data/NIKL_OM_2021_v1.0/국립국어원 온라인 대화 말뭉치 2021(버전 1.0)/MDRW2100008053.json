      "utterance": [
        {
          "id": "MDRW2100008053.1.1",
          "form": "아 요즘 운동하세요?",
          "original_form": "아 요즘 운동하세요?",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100008053.1.2",
          "form": "꾸준히 하는 운동은\r\n숨쉬기 운동이구요",
          "original_form": "꾸준히 하는 운동은\r\n숨쉬기 운동이구요",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100008053.1.3",
          "form": "가끔씩 홈트 하는데\r\n지속적으로는 안되네요ㅠ",
          "original_form": "가끔씩 홈트 하는데\r\n지속적으로는 안되네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100008053.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100008053.1.5",
          "form": "한때는 땅끄부부 운동했었는데",
          "original_form": "한때는 땅끄부부 운동했었는데",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100008053.1.6",
          "form": "왤케 요새 하기가싫져ㅜ",
          "original_form": "왤케 요새 하기가싫져ㅜ",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100008053.1.7",
          "form": "엇 저도 땅끄부부요ㅋㅋ",
          "original_form": "엇 저도 땅끄부부요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100008053.1.8",
          "form": "저도 집에서 혼자 해서 그런지\r\n유지가 안되네요ㅠ",
          "original_form": "저도 집에서 혼자 해서 그런지\r\n유지가 안되네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100008053.1.9",
          "form": "아니 그영상 그거 솔직히 십분짜리도많은뎅",
          "original_form": "아니 그영상 그거 솔직히 십분짜리도많은뎅",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100008053.1.10",
          "form": "왤케 하기싫져",
          "original_form": "왤케 하기싫져",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100008053.1.11",
          "form": "드라마보면 한시간도 순삭인디 ㅋㅋ",
          "original_form": "드라마보면 한시간도 순삭인디 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100008053.1.12",
          "form": "ㅋㅋ저도 가끔 새벽까지 넷플보다가",
          "original_form": "ㅋㅋ저도 가끔 새벽까지 넷플보다가",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100008053.1.13",
          "form": "와 이시간까지 이러고 있으면서\r\n그 10분 20분 운동은 안하냐 싶은 생각이ㅋㅋ",
          "original_form": "와 이시간까지 이러고 있으면서\r\n그 10분 20분 운동은 안하냐 싶은 생각이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100008053.1.14",
          "form": "이래서 사람들이 그 비싼돈주고 피티를 받나 싶어요 ㅋㅋ",
          "original_form": "이래서 사람들이 그 비싼돈주고 피티를 받나 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100008053.1.15",
          "form": "ㅋㅋ 돈때문에라도 가는거겠죠?",
          "original_form": "ㅋㅋ 돈때문에라도 가는거겠죠?",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100008053.1.16",
          "form": "근데 비싸도 너무 비싸더라고요",
          "original_form": "근데 비싸도 너무 비싸더라고요",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100008053.1.17",
          "form": "1회 4만원 5만원 ㄷㄷ",
          "original_form": "1회 4만원 5만원 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100008053.1.18",
          "form": "오? 그 정도면 저렴한거 아니에요",
          "original_form": "오? 그 정도면 저렴한거 아니에요",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100008053.1.19",
          "form": "남편 pt했을때\r\n회당 10만원이었던거 같은뎅",
          "original_form": "남편 pt했을때\r\n회당 10만원이었던거 같은뎅",
          "speaker_id": "1",
          "time": "20210822 23:56"
        },
        {
          "id": "MDRW2100008053.1.20",
          "form": "아 20회 이렇게 끊으면 100만원 이라더라고요",
          "original_form": "아 20회 이렇게 끊으면 100만원 이라더라고요",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100008053.1.21",
          "form": "이벤드라면서",
          "original_form": "이벤드라면서",
          "speaker_id": "2",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100008053.1.22",
          "form": "여긴 지방이기도하고욥",
          "original_form": "여긴 지방이기도하고욥",
          "speaker_id": "2",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100008053.1.23",
          "form": "10만원도 이벤트가였는데ㅜㅜ",
          "original_form": "10만원도 이벤트가였는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100008053.1.24",
          "form": "우리 돈번다고 생각하고\r\n집에서라도 홈트로 뿌셔봅시다ㅠㅋㅋ",
          "original_form": "우리 돈번다고 생각하고\r\n집에서라도 홈트로 뿌셔봅시다ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:01"
        },
        {
          "id": "MDRW2100008053.1.25",
          "form": "근데 홈트 넘하기싫어요",
          "original_form": "근데 홈트 넘하기싫어요",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100008053.1.26",
          "form": "누가 시켜야하는 스타일인건가",
          "original_form": "누가 시켜야하는 스타일인건가",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100008053.1.27",
          "form": "재능교육이 필요하다는 ㅋㅋㅋ",
          "original_form": "재능교육이 필요하다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100008053.1.28",
          "form": "자기의 일은 스스로 해야하는데 말이져",
          "original_form": "자기의 일은 스스로 해야하는데 말이져",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100008053.1.29",
          "form": "저는 재능교육 장기간 했는데도",
          "original_form": "저는 재능교육 장기간 했는데도",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100008053.1.30",
          "form": "왜 이런 어른으로 자란걸까요",
          "original_form": "왜 이런 어른으로 자란걸까요",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100008053.1.31",
          "form": "전 그래서 그냥 정처없어 걸어요",
          "original_form": "전 그래서 그냥 정처없어 걸어요",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100008053.1.32",
          "form": "그거라도 하자며 ㅋㅋ 흔히말하는 만보걷기",
          "original_form": "그거라도 하자며 ㅋㅋ 흔히말하는 만보걷기",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100008053.1.33",
          "form": "오 그럼 일일 일만보씩 하나요?",
          "original_form": "오 그럼 일일 일만보씩 하나요?",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100008053.1.34",
          "form": "2만보도해요",
          "original_form": "2만보도해요",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100008053.1.35",
          "form": "근데 빨리 걸어야 운동된대여",
          "original_form": "근데 빨리 걸어야 운동된대여",
          "speaker_id": "2",
          "time": "20210823 00:12"
        },
        {
          "id": "MDRW2100008053.1.36",
          "form": "그래도 그렇게라도\r\n걸으니 좋은거 같아요:)",
          "original_form": "그래도 그렇게라도\r\n걸으니 좋은거 같아요:)",
          "speaker_id": "1",
          "time": "20210823 00:13"
        }
      ]
