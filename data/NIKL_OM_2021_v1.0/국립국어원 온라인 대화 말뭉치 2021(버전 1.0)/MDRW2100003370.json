      "utterance": [
        {
          "id": "MDRW2100003370.1.1",
          "form": "민초파이신가요ㅋㅋ",
          "original_form": "민초파이신가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003370.1.2",
          "form": "네 저 민초파입니다 ㅎ",
          "original_form": "네 저 민초파입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003370.1.3",
          "form": "민초 좋아하세요?",
          "original_form": "민초 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003370.1.4",
          "form": "극호입니다ㅋㅋㅋ",
          "original_form": "극호입니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003370.1.5",
          "form": "스벅에 민초 먹으면 행복해져요",
          "original_form": "스벅에 민초 먹으면 행복해져요",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003370.1.6",
          "form": "헐 저랑 취향이 같네요",
          "original_form": "헐 저랑 취향이 같네요",
          "speaker_id": "1",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003370.1.7",
          "form": "저도 스벅 민초 좋아해요",
          "original_form": "저도 스벅 민초 좋아해요",
          "speaker_id": "1",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003370.1.8",
          "form": "무적쿠폰으로",
          "original_form": "무적쿠폰으로",
          "speaker_id": "2",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003370.1.9",
          "form": "이거저거 추가해서ㅋㅋ",
          "original_form": "이거저거 추가해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003370.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 알죠 자바칩도 통으로 여러개 넣으면 굿이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 알죠 자바칩도 통으로 여러개 넣으면 굿이에요",
          "speaker_id": "1",
          "time": "20210821 01:10"
        },
        {
          "id": "MDRW2100003370.1.11",
          "form": "자바칩 못건져먹으면ㅋㅋㅋ 어떻게 하세요?ㅋㅋㅋ",
          "original_form": "자바칩 못건져먹으면ㅋㅋㅋ 어떻게 하세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100003370.1.12",
          "form": "전 그냥 빨대로 훅훅!! 빨리",
          "original_form": "전 그냥 빨대로 훅훅!! 빨리",
          "speaker_id": "1",
          "time": "20210823 12:17"
        },
        {
          "id": "MDRW2100003370.1.13",
          "form": "흡입해서 먹어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "흡입해서 먹어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:17"
        },
        {
          "id": "MDRW2100003370.1.14",
          "form": "ㅋㅋㅋㅋ 막히던데",
          "original_form": "ㅋㅋㅋㅋ 막히던데",
          "speaker_id": "2",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100003370.1.15",
          "form": "제 기억엔 나왔던 거 같은. ..",
          "original_form": "제 기억엔 나왔던 거 같은. ..",
          "speaker_id": "1",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100003370.1.16",
          "form": "아니면 빨대 이용해서 먹어요ㅋㅋ",
          "original_form": "아니면 빨대 이용해서 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100003370.1.17",
          "form": "빨대로 끌어서요",
          "original_form": "빨대로 끌어서요",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100003370.1.18",
