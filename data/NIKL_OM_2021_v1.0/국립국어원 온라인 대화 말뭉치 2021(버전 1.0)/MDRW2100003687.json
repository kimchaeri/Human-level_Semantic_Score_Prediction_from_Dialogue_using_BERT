        {
          "id": "MDRW2100003687.1.1",
          "form": "나도보았다",
          "original_form": "나도보았다",
          "speaker_id": "2",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003687.1.2",
          "form": "이렇게된이상 열심히해야죠",
          "original_form": "이렇게된이상 열심히해야죠",
          "speaker_id": "1",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003687.1.3",
          "form": "가족을의ㅣ해서지",
          "original_form": "가족을의ㅣ해서지",
          "speaker_id": "2",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003687.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003687.1.5",
          "form": "암호같게보이겠지",
          "original_form": "암호같게보이겠지",
          "speaker_id": "2",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003687.1.6",
          "form": "할말이별로없으니깐요",
          "original_form": "할말이별로없으니깐요",
          "speaker_id": "1",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003687.1.7",
          "form": "대화는 참여려운거야",
          "original_form": "대화는 참여려운거야",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003687.1.8",
          "form": "평소안해서그렇죠",
          "original_form": "평소안해서그렇죠",
          "speaker_id": "1",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003687.1.9",
          "form": "하긴 화나야 대화하니",
          "original_form": "하긴 화나야 대화하니",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003687.1.10",
          "form": "일반적인걸껄요",
          "original_form": "일반적인걸껄요",
          "speaker_id": "1",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003687.1.11",
          "form": "즐거운내집",
          "original_form": "즐거운내집",
          "speaker_id": "2",
          "time": "20210821 10:48"
        },
        {
          "id": "MDRW2100003687.1.12",
          "form": "그런거 없는거아시잔아요",
          "original_form": "그런거 없는거아시잔아요",
          "speaker_id": "1",
          "time": "20210821 10:48"
        },
        {
          "id": "MDRW2100003687.1.13",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210821 10:48"
        },
        {
          "id": "MDRW2100003687.1.14",
