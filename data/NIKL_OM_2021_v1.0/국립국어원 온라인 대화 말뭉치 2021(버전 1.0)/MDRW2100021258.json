      "utterance": [
        {
          "id": "MDRW2100021258.1.1",
          "form": "앗 아직이요ㅜㅜ 오늘 저녁은 패스할 것 같아요",
          "original_form": "앗 아직이요ㅜㅜ 오늘 저녁은 패스할 것 같아요",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100021258.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100021258.1.3",
          "form": "저는 거하게 먹고왔어요 ㅠㅠ",
          "original_form": "저는 거하게 먹고왔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021258.1.4",
          "form": "치팅데이!",
          "original_form": "치팅데이!",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021258.1.5",
          "form": "오 대박 다이어트 하시는군요!",
          "original_form": "오 대박 다이어트 하시는군요!",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021258.1.6",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021258.1.7",
          "form": "마라탕이요 ㅎㅎㅎㄴ",
          "original_form": "마라탕이요 ㅎㅎㅎㄴ",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021258.1.8",
          "form": "너무 행복했어여 ㅠㅠ",
          "original_form": "너무 행복했어여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021258.1.9",
          "form": "헐 마라탕",
          "original_form": "헐 마라탕",
          "speaker_id": "2",
          "time": "20210829 07:22"
        },
        {
          "id": "MDRW2100021258.1.10",
          "form": "저 요즘 마라탕 1도 못먹었어요ㅜㅜ",
          "original_form": "저 요즘 마라탕 1도 못먹었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 07:23"
        },
        {
          "id": "MDRW2100021258.1.11",
          "form": "주 1회 마라탕은 해야하는데ㅜㅜㅜ",
          "original_form": "주 1회 마라탕은 해야하는데ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 07:23"
        },
        {
          "id": "MDRW2100021258.1.12",
          "form": "헉 우쨔서 못드셨나요 ㅠㅠ",
          "original_form": "헉 우쨔서 못드셨나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100021258.1.13",
          "form": "집근처에 없어요 흑흑",
          "original_form": "집근처에 없어요 흑흑",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100021258.1.14",
          "form": "원래 알바 하는곳 근처에 있었는데 이제 거기도 그만둬서...",
          "original_form": "원래 알바 하는곳 근처에 있었는데 이제 거기도 그만둬서...",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100021258.1.15",
          "form": "마라탕이없습니다......",
          "original_form": "마라탕이없습니다......",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100021258.1.16",
          "form": "아 ㅠㅜㅜ",
          "original_form": "아 ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100021258.1.17",
          "form": "어째서죠 이렇개 많은 가개가 생겼는데...",
          "original_form": "어째서죠 이렇개 많은 가개가 생겼는데...",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100021258.1.18",
          "form": "배달도 안도ㅑ요?",
          "original_form": "배달도 안도ㅑ요?",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100021258.1.19",
          "form": "배달은!!!!! 제맘대로 양을 담고!!!! 맘대로 재료 조절을!!! 못하더라구요ㅜㅜㅜㅜㅜ",
          "original_form": "배달은!!!!! 제맘대로 양을 담고!!!! 맘대로 재료 조절을!!! 못하더라구요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 08:05"
        },
        {
          "id": "MDRW2100021258.1.20",
          "form": "아이고아이고..",
          "original_form": "아이고아이고..",
          "speaker_id": "2",
          "time": "20210830 08:05"
        },
        {
          "id": "MDRW2100021258.1.21",
          "form": "아 맞아요 ㅠㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100021258.1.22",
          "form": "가격도 두배..",
          "original_form": "가격도 두배..",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100021258.1.23",
          "form": "무서워...",
          "original_form": "무서워...",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100021258.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 가격 젤 무섭죠...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 가격 젤 무섭죠...",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100021258.1.25",
          "form": "차라리 그래서 저는 마라 소스를 샀옹ㅅ",
          "original_form": "차라리 그래서 저는 마라 소스를 샀옹ㅅ",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100021258.1.26",
          "form": "샀어요",
          "original_form": "샀어요",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100021258.1.27",
          "form": "헉 집애서 해드새요?",
          "original_form": "헉 집애서 해드새요?",
          "speaker_id": "1",
          "time": "20210830 18:56"
        },
        {
          "id": "MDRW2100021258.1.28",
          "form": "가끔요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "가끔요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100021258.1.29",
          "form": "라면스프가 치트키입니다",
          "original_form": "라면스프가 치트키입니다",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100021258.1.30",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:31"
        },
        {
          "id": "MDRW2100021258.1.31",
          "form": "괜히 마법의 스프가 아니죠",
          "original_form": "괜히 마법의 스프가 아니죠",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100021258.1.32",
          "form": "저는 오늘 두부볶음밥 해먹었는대",
          "original_form": "저는 오늘 두부볶음밥 해먹었는대",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100021258.1.33",
          "form": "너무 맛있내요",
          "original_form": "너무 맛있내요",
          "speaker_id": "1",
