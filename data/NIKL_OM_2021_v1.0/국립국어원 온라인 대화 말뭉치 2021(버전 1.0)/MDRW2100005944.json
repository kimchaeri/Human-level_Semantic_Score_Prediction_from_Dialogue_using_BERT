      "utterance": [
        {
          "id": "MDRW2100005944.1.1",
          "form": "오늘 저녁 흠",
          "original_form": "오늘 저녁 흠",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100005944.1.2",
          "form": "부대찌개 어떠신가요?",
          "original_form": "부대찌개 어떠신가요?",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100005944.1.3",
          "form": "부대찌캐 완전 사랑합니다",
          "original_form": "부대찌캐 완전 사랑합니다",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100005944.1.4",
          "form": "오늘같은 날에 딱 맞는거같아요!",
          "original_form": "오늘같은 날에 딱 맞는거같아요!",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100005944.1.5",
          "form": "그니깐요 여기는 비가와서",
          "original_form": "그니깐요 여기는 비가와서",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005944.1.6",
          "form": "비오는날씨에먹어야죠",
          "original_form": "비오는날씨에먹어야죠",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005944.1.7",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005944.1.8",
          "form": "저는 오늘 저녁 닭갈비먹어요~",
          "original_form": "저는 오늘 저녁 닭갈비먹어요~",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005944.1.9",
          "form": "밀키트 사왔죠 ㅎㅎ",
          "original_form": "밀키트 사왔죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005944.1.10",
          "form": "우와!!",
          "original_form": "우와!!",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005944.1.11",
          "form": "드디어 닭갈비를 먹는구나",
          "original_form": "드디어 닭갈비를 먹는구나",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005944.1.12",
          "form": "난 오늘 저녁 머먹지...",
          "original_form": "난 오늘 저녁 머먹지...",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005944.1.13",
          "form": "어제 먹다 남은 홈플치킨먹어야할것같은데",
          "original_form": "어제 먹다 남은 홈플치킨먹어야할것같은데",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005944.1.14",
          "form": "부대찌개도 요세 밀키트나 배달 되던데",
          "original_form": "부대찌개도 요세 밀키트나 배달 되던데",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005944.1.15",
          "form": "함보세요!",
          "original_form": "함보세요!",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005944.1.16",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.17",
          "form": "배달의민족인가요?",
          "original_form": "배달의민족인가요?",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.19",
          "form": "네 전 배민에서 저번에 부대찌개 한번 시켜먹어봤어요 ㅋㅋ",
          "original_form": "네 전 배민에서 저번에 부대찌개 한번 시켜먹어봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.20",
          "form": "근데 맛있더라고요",
          "original_form": "근데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.21",
          "form": "예상외로..",
          "original_form": "예상외로..",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.22",
          "form": "간도 맞고 햄이랑 배합도 잘되있어서 꽤놀랬다는 ㅋㅋㅋ",
          "original_form": "간도 맞고 햄이랑 배합도 잘되있어서 꽤놀랬다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005944.1.23",
          "form": "오~~다행이에요",
          "original_form": "오~~다행이에요",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.24",
          "form": "맛있겠답",
          "original_form": "맛있겠답",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.25",
          "form": "군침도네요",
          "original_form": "군침도네요",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.26",
          "form": "아니면 전?",
          "original_form": "아니면 전?",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.27",
          "form": "전도 좋을거같아요 ㅋㅋ",
          "original_form": "전도 좋을거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.28",
          "form": "비오는날씨에 전 좋죠!!",
          "original_form": "비오는날씨에 전 좋죠!!",
          "speaker_id": "1",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.29",
          "form": "해물파전이나 김치전에 막걸리 한잔...",
          "original_form": "해물파전이나 김치전에 막걸리 한잔...",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100005944.1.30",
          "form": "카악~~",
          "original_form": "카악~~",
          "speaker_id": "1",
          "time": "20210907 17:30"
        }
      ]
