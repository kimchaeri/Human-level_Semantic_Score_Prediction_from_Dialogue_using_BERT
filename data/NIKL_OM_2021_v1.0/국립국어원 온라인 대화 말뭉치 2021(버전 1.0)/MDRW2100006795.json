      "utterance": [
        {
          "id": "MDRW2100006795.1.1",
          "form": "맞아요!@ 족발에 막구수 필수예요 ㅋㅋ",
          "original_form": "맞아요!@ 족발에 막구수 필수예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006795.1.2",
          "form": "막국수 맛으로 먹죠ㅋㅋ",
          "original_form": "막국수 맛으로 먹죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006795.1.3",
          "form": "사먹는 그 막국수맛",
          "original_form": "사먹는 그 막국수맛",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006795.1.4",
          "form": "맞아요!!! ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요!!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006795.1.5",
          "form": "보쌈도 좋아하새요?",
          "original_form": "보쌈도 좋아하새요?",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006795.1.6",
          "form": "정말 좋아하는데",
          "original_form": "정말 좋아하는데",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006795.1.7",
          "form": "사먹는건 양이 너무 적어요",
          "original_form": "사먹는건 양이 너무 적어요",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006795.1.8",
          "form": "맞아욬ㅋㅋ 보쌈 너무 비싸요",
          "original_form": "맞아욬ㅋㅋ 보쌈 너무 비싸요",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006795.1.9",
          "form": "그거 앞다리살 삶으면 만원인데ㅠ",
          "original_form": "그거 앞다리살 삶으면 만원인데ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006795.1.10",
          "form": "오늘 해먹어야겠어요ㅋ",
          "original_form": "오늘 해먹어야겠어요ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006795.1.11",
          "form": "오오 직접 요리 하시게요?",
          "original_form": "오오 직접 요리 하시게요?",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100006795.1.12",
          "form": "네 요리 좋아해서요ㅎㅎ",
          "original_form": "네 요리 좋아해서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006795.1.13",
          "form": "막국수는 비빔면으로ㅠ",
          "original_form": "막국수는 비빔면으로ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006795.1.14",
          "form": "우아 너무 좋네요!!!",
          "original_form": "우아 너무 좋네요!!!",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006795.1.15",
          "form": "비빔면도 좋아욬ㅋㅋ",
          "original_form": "비빔면도 좋아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006795.1.16",
          "form": "비빔면 진짜 한 개 두 개 고민ㅠ",
          "original_form": "비빔면 진짜 한 개 두 개 고민ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006795.1.17",
          "form": "두개는 하셔야죠!!",
          "original_form": "두개는 하셔야죠!!",
          "speaker_id": "1",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006795.1.18",
          "form": "ㅋㅋㅋ감사합니다 위로가 되네요",
          "original_form": "ㅋㅋㅋ감사합니다 위로가 되네요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006795.1.19",
          "form": "혼자 드시나요?",
          "original_form": "혼자 드시나요?",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006795.1.20",
          "form": "아 제생각만 했네요ㅋㅋㄱ",
          "original_form": "아 제생각만 했네요ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006795.1.21",
          "form": "가족이랑 해야쥬",
          "original_form": "가족이랑 해야쥬",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006795.1.22",
          "form": "오오 가족들이 좋아하시겟어요!!",
          "original_form": "오오 가족들이 좋아하시겟어요!!",
