      "utterance": [
        {
          "id": "MDRW2100018318.1.1",
          "form": "요즈음 날씨 오락가락이라 대처를 못 하겠어",
          "original_form": "요즈음 날씨 오락가락이라 대처를 못 하겠어",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.2",
          "form": "내일 비왔으면 좋겠다",
          "original_form": "내일 비왔으면 좋겠다",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.3",
          "form": "나가기 시름",
          "original_form": "나가기 시름",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.4",
          "form": "나는 안왔으면 좋겠다",
          "original_form": "나는 안왔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.5",
          "form": "집 밖을 나가는 이상",
          "original_form": "집 밖을 나가는 이상",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.6",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.7",
          "form": "비가 오면 기분 다운이야",
          "original_form": "비가 오면 기분 다운이야",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.8",
          "form": "하긴 맨날 나가지",
          "original_form": "하긴 맨날 나가지",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.9",
          "form": "하지만 근무중엔 비가 왔으면 좋겠다ㅋㅋㅋ",
          "original_form": "하지만 근무중엔 비가 왔으면 좋겠다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018318.1.10",
          "form": "알지 내 마음?ㅋㅋㅋㅋ",
          "original_form": "알지 내 마음?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018318.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 아무도 안왔으면 좋겠다는 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아무도 안왔으면 좋겠다는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018318.1.12",
          "form": "월급은 받아야지...",
          "original_form": "월급은 받아야지...",
          "speaker_id": "1",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100018318.1.13",
          "form": "점심시간, 칼퇴만 지켜주세요 제발",
          "original_form": "점심시간, 칼퇴만 지켜주세요 제발",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018318.1.14",
          "form": "이제 가을인데",
          "original_form": "이제 가을인데",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018318.1.15",
          "form": "좀 시원해지면 좋겠다",
          "original_form": "좀 시원해지면 좋겠다",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018318.1.16",
          "form": "너무 더움",
          "original_form": "너무 더움",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018318.1.17",
          "form": "맞아 낮엔 아직 덥다더라",
          "original_form": "맞아 낮엔 아직 덥다더라",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018318.1.18",
          "form": "나는 안에 있어서 잘 모르겠지만",
          "original_form": "나는 안에 있어서 잘 모르겠지만",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018318.1.19",
          "form": "다들 아직 날씨 때문에 화내더라ㅋㅋㅋ",
          "original_form": "다들 아직 날씨 때문에 화내더라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018318.1.20",
          "form": "그래도 밤은 시원해서 다행이야",
          "original_form": "그래도 밤은 시원해서 다행이야",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100018318.1.21",
          "form": "밤은 시원하긴 하지",
          "original_form": "밤은 시원하긴 하지",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018318.1.22",
          "form": "근데 비 안오면 너무 더움",
          "original_form": "근데 비 안오면 너무 더움",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018318.1.23",
          "form": "하긴 처서가 지나긴 했지만 아직 8월이구나",
          "original_form": "하긴 처서가 지나긴 했지만 아직 8월이구나",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018318.1.24",
          "form": "그래도 명절 즈음엔 시원해질거야",
          "original_form": "그래도 명절 즈음엔 시원해질거야",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018318.1.25",
          "form": "조금만 더 버티자",
          "original_form": "조금만 더 버티자",
          "speaker_id": "1",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018318.1.26",
          "form": "계절이 제 계절이 아님",
          "original_form": "계절이 제 계절이 아님",
          "speaker_id": "2",
          "time": "20210827 00:16"
        },
        {
          "id": "MDRW2100018318.1.27",
          "form": "ㅋㅋㅋㅋ그건 그렇지",
          "original_form": "ㅋㅋㅋㅋ그건 그렇지",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100018318.1.28",
          "form": "모처럼 가을 다가오는데 가을에 하고 싶은거 있나",
          "original_form": "모처럼 가을 다가오는데 가을에 하고 싶은거 있나",
          "speaker_id": "1",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018318.1.29",
          "form": "겨울도 겨울이 아니고 여름도 여름이 아님 너무 덥거나 너무 춥거나임",
          "original_form": "겨울도 겨울이 아니고 여름도 여름이 아님 너무 덥거나 너무 춥거나임",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018318.1.30",
          "form": "가을에는 책을 읽어야지",
          "original_form": "가을에는 책을 읽어야지",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018318.1.31",
          "form": "독서의 계절",
          "original_form": "독서의 계절",
          "speaker_id": "1",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018318.1.32",
          "form": "멋지구먼",
          "original_form": "멋지구먼",
          "speaker_id": "1",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018318.1.33",
          "form": "책 좋지",
          "original_form": "책 좋지",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018318.1.34",
          "form": "나도 읽어야 하는데",
          "original_form": "나도 읽어야 하는데",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018318.1.35",
          "form": "손만대고 페이지가 안넘어가",
          "original_form": "손만대고 페이지가 안넘어가",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018318.1.36",
          "form": "오늘 나가는데 비온 거 생각해서 하나 더 입고 나가려다가",
          "original_form": "오늘 나가는데 비온 거 생각해서 하나 더 입고 나가려다가",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018318.1.37",
          "form": "반팔 하나만 입고 나갔음",
          "original_form": "반팔 하나만 입고 나갔음",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018318.1.38",
          "form": "더웠겠는데",
          "original_form": "더웠겠는데",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018318.1.39",
          "form": "긴바지도 덥더라",
          "original_form": "긴바지도 덥더라",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018318.1.40",
          "form": "긴바지는 아직 무리지ㅋㅋㅋ",
          "original_form": "긴바지는 아직 무리지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018318.1.41",
          "form": "나는 아직 반팔과 반바지를 사랑해",
          "original_form": "나는 아직 반팔과 반바지를 사랑해",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018318.1.42",
          "form": "원래 사계절 내내 긴바지 입는데",
          "original_form": "원래 사계절 내내 긴바지 입는데",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018318.1.43",
          "form": "덥긴 더워",
          "original_form": "덥긴 더워",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018318.1.44",
          "form": "긴바지 입고 땀띠난 걸 잊지 못한다",
          "original_form": "긴바지 입고 땀띠난 걸 잊지 못한다",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018318.1.45",
          "form": "그래도 9월 되면 시원하겠지",
          "original_form": "그래도 9월 되면 시원하겠지",
          "speaker_id": "2",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018318.1.46",
          "form": "처서도 지났는데",
          "original_form": "처서도 지났는데",
          "speaker_id": "2",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018318.1.47",
          "form": "처서도 지났고 추석이 오고 있으니까",
          "original_form": "처서도 지났고 추석이 오고 있으니까",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018318.1.48",
          "form": "더 시원할거야",
          "original_form": "더 시원할거야",
          "speaker_id": "1",
          "time": "20210827 00:22"
        },
        {
          "id": "MDRW2100018318.1.49",
          "form": "이번 겨울은 어떨까 완전 추울 거 같음",
          "original_form": "이번 겨울은 어떨까 완전 추울 거 같음",
          "speaker_id": "2",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018318.1.50",
          "form": "이번 여름이 더웠으니 나도 그렇게 예상하고 있는데",
          "original_form": "이번 여름이 더웠으니 나도 그렇게 예상하고 있는데",
          "speaker_id": "1",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018318.1.51",
          "form": "패딩을 미리 하나 더 주문할걸",
          "original_form": "패딩을 미리 하나 더 주문할걸",
          "speaker_id": "1",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018318.1.52",
          "form": "후회중이긴해",
          "original_form": "후회중이긴해",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018318.1.53",
          "form": "미리 패딩을 하나 살까 싶긴 해",
          "original_form": "미리 패딩을 하나 살까 싶긴 해",
          "speaker_id": "2",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018318.1.54",
          "form": "너무 춥지만 않으면 좋겠다",
          "original_form": "너무 춥지만 않으면 좋겠다",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018318.1.55",
          "form": "싸게살거면 지금은 조금 늦은 감 있지만 그래도 올해 신제품 나올 시즌이니까 보고 괜찮은걸로 미리 구비하자",
          "original_form": "싸게살거면 지금은 조금 늦은 감 있지만 그래도 올해 신제품 나올 시즌이니까 보고 괜찮은걸로 미리 구비하자",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018318.1.56",
          "form": "추위에 된통당하고 사면",
          "original_form": "추위에 된통당하고 사면",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018318.1.57",
          "form": "화날거같음",
          "original_form": "화날거같음",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018318.1.58",
          "form": "미리 사놓는 게 좋지 시즌 되기 전이 싸니까",
          "original_form": "미리 사놓는 게 좋지 시즌 되기 전이 싸니까",
          "speaker_id": "2",
          "time": "20210827 00:28"
        }
