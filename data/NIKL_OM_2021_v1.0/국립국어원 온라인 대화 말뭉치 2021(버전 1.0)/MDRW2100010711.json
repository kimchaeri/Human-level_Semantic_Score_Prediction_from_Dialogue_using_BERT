      "utterance": [
        {
          "id": "MDRW2100010711.1.1",
          "form": "저녁은 드셨나요",
          "original_form": "저녁은 드셨나요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010711.1.2",
          "form": "네 회사서 먹고 도망왔어요",
          "original_form": "네 회사서 먹고 도망왔어요",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100010711.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010711.1.4",
          "form": "저는 출근해서 김천에서 포장을ㅎㅎ",
          "original_form": "저는 출근해서 김천에서 포장을ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010711.1.5",
          "form": "아 출근하셨군요ㅎ",
          "original_form": "아 출근하셨군요ㅎ",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010711.1.6",
          "form": "무슨김밥드시나요",
          "original_form": "무슨김밥드시나요",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010711.1.7",
          "form": "아 카레먹었어요ㅎ",
          "original_form": "아 카레먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010711.1.8",
          "form": "아 포장해서요?",
          "original_form": "아 포장해서요?",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010711.1.9",
          "form": "댁에서 드시고 출근하시지",
          "original_form": "댁에서 드시고 출근하시지",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100010711.1.10",
          "form": "집에서는 5시에 나와서",
          "original_form": "집에서는 5시에 나와서",
          "speaker_id": "2",
          "time": "20210823 22:30"
        },
        {
          "id": "MDRW2100010711.1.11",
          "form": "좀 빨라요ㅎㅎ",
          "original_form": "좀 빨라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010711.1.12",
          "form": "출근이 6시예요?",
          "original_form": "출근이 6시예요?",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010711.1.13",
          "form": "포장하면 어디서 드세요?",
          "original_form": "포장하면 어디서 드세요?",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100010711.1.14",
          "form": "네ㅎㅎ포장하면 역무실이오",
          "original_form": "네ㅎㅎ포장하면 역무실이오",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010711.1.15",
          "form": "냄새날텐데 상관없나보네요",
          "original_form": "냄새날텐데 상관없나보네요",
          "speaker_id": "1",
          "time": "20210823 23:01"
        },
        {
          "id": "MDRW2100010711.1.16",
          "form": "구석쪽에서 먹고 환기시설도 있으니까",
          "original_form": "구석쪽에서 먹고 환기시설도 있으니까",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100010711.1.17",
          "form": "괜찮지 싶어요 ㅎㅎ",
          "original_form": "괜찮지 싶어요 ㅎㅎ",
