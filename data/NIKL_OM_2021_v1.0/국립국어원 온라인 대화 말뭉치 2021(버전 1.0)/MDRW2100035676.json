      "utterance": [
        {
          "id": "MDRW2100035676.1.1",
          "form": "최근에 뭐 구매하셨나요??",
          "original_form": "최근에 뭐 구매하셨나요??",
          "speaker_id": "2",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100035676.1.2",
          "form": "저는 전공책이요",
          "original_form": "저는 전공책이요",
          "speaker_id": "1",
          "time": "20210830 12:08"
        },
        {
          "id": "MDRW2100035676.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 12:08"
        },
        {
          "id": "MDRW2100035676.1.4",
          "form": "전 물이여ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 물이여ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100035676.1.5",
          "form": "개강 하셨어요??",
          "original_form": "개강 하셨어요??",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100035676.1.6",
          "form": "내일 모레 해요 어디 물 구매하셨나요?",
          "original_form": "내일 모레 해요 어디 물 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100035676.1.7",
          "form": "저 그냥 카카오에서 샀어요",
          "original_form": "저 그냥 카카오에서 샀어요",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100035676.1.8",
          "form": "전공책 뭐 구매하셨나요?",
          "original_form": "전공책 뭐 구매하셨나요?",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100035676.1.9",
          "form": "저는 병리랑 성인간호요",
          "original_form": "저는 병리랑 성인간호요",
          "speaker_id": "1",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100035676.1.10",
          "form": "아하 간호쪽이군요",
          "original_form": "아하 간호쪽이군요",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100035676.1.11",
          "form": "뭐 구매하고싶은 거 있으세요?",
          "original_form": "뭐 구매하고싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100035676.1.12",
          "form": "저는 태블릿이요",
          "original_form": "저는 태블릿이요",
          "speaker_id": "1",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100035676.1.13",
          "form": "어디거요?",
          "original_form": "어디거요?",
          "speaker_id": "2",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035676.1.14",
          "form": "갤럭시꺼요",
          "original_form": "갤럭시꺼요",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100035676.1.15",
          "form": "괜찮나요",
          "original_form": "괜찮나요",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100035676.1.16",
          "form": "모르겠어요 근데 제가 사고싶은 워너비긴해요",
          "original_form": "모르겠어요 근데 제가 사고싶은 워너비긴해요",
          "speaker_id": "1",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100035676.1.17",
          "form": "아하 전 패드를",
          "original_form": "아하 전 패드를",
          "speaker_id": "2",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100035676.1.18",
          "form": "사용하거든요!",
          "original_form": "사용하거든요!",
          "speaker_id": "2",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100035676.1.19",
          "form": "우와 정말 부럽네요!!",
          "original_form": "우와 정말 부럽네요!!",
