      "utterance": [
        {
          "id": "MDRW2100028160.1.1",
          "form": "스포츠 좋아하시나요 ㅋㅋ",
          "original_form": "스포츠 좋아하시나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028160.1.2",
          "form": "아뇨 그렇게 좋아하진 않아요ㅎㅎ 야구정도 가끔보는 정도?",
          "original_form": "아뇨 그렇게 좋아하진 않아요ㅎㅎ 야구정도 가끔보는 정도?",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028160.1.3",
          "form": "야구 재밌죠 ㅎㅎ",
          "original_form": "야구 재밌죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028160.1.4",
          "form": "네ㅎㅎ 중계하는 스포츠 중에 젤 재밌는거 같아요",
          "original_form": "네ㅎㅎ 중계하는 스포츠 중에 젤 재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028160.1.5",
          "form": "어디 응원하시나요",
          "original_form": "어디 응원하시나요",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028160.1.6",
          "form": "저는 연고지팀이 한화라 한화 응원해요 정확히는 저 말고 아빠가 응원하세요ㅎㅎㅎ",
          "original_form": "저는 연고지팀이 한화라 한화 응원해요 정확히는 저 말고 아빠가 응원하세요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028160.1.7",
          "form": "오 우리 아빠도 한화에요 ㅋㅋㅋ",
          "original_form": "오 우리 아빠도 한화에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028160.1.8",
          "form": "안녕하산님 아버님도 도를 닦으시는군요ㅎㅎㅎ",
          "original_form": "안녕하산님 아버님도 도를 닦으시는군요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028160.1.9",
          "form": "아뇨 요새 안보던데요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 요새 안보던데요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028160.1.10",
          "form": "그냥 무관심ㅋㅋ",
          "original_form": "그냥 무관심ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028160.1.11",
          "form": "저희는 틀었다가 지고 있으면 쿨하게 채널 돌리셔요ㅋㅋㅋ",
          "original_form": "저희는 틀었다가 지고 있으면 쿨하게 채널 돌리셔요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028160.1.12",
          "form": "계속 돌리셔야겠는데요?",
          "original_form": "계속 돌리셔야겠는데요?",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028160.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028160.1.14",
          "form": "넹ㅎㅎㅎ 보시다가도 역전당하면 돌리고ㅋㅋ",
          "original_form": "넹ㅎㅎㅎ 보시다가도 역전당하면 돌리고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028160.1.15",
          "form": "야구 끝나는거 본 적이 별로 없죠",
          "original_form": "야구 끝나는거 본 적이 별로 없죠",
