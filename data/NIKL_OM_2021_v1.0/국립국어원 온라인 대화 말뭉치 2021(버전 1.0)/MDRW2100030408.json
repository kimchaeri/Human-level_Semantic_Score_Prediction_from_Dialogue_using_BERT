      "utterance": [
        {
          "id": "MDRW2100030408.1.1",
          "form": "단풍 보러 여행 가고 싶어요",
          "original_form": "단풍 보러 여행 가고 싶어요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030408.1.2",
          "form": "직업이에요",
          "original_form": "직업이에요",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030408.1.3",
          "form": "일하는 거 힘드네요",
          "original_form": "일하는 거 힘드네요",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.4",
          "form": "연휴에는 쉬나요",
          "original_form": "연휴에는 쉬나요",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.5",
          "form": "돈 버는 거 힘들죠",
          "original_form": "돈 버는 거 힘들죠",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.6",
          "form": "추석 연휴에는 쉬어요",
          "original_form": "추석 연휴에는 쉬어요",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.7",
          "form": "주식 장이 안 좋았거든요",
          "original_form": "주식 장이 안 좋았거든요",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.8",
          "form": "마음 고생했지요",
          "original_form": "마음 고생했지요",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.9",
          "form": "연차가 없어서 평일에 쉬기 힘들어요",
          "original_form": "연차가 없어서 평일에 쉬기 힘들어요",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.10",
          "form": "취미로 하는데 어려워요",
          "original_form": "취미로 하는데 어려워요",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030408.1.11",
          "form": "1년에 딱 한번 여름 방학 때 일주일 휴가가 있어요",
          "original_form": "1년에 딱 한번 여름 방학 때 일주일 휴가가 있어요",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.12",
          "form": "주식은 어떤가요",
          "original_form": "주식은 어떤가요",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.13",
          "form": "올해는 뭐 했어요",
          "original_form": "올해는 뭐 했어요",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.14",
          "form": "돈 많이 벌 수 있나요",
          "original_form": "돈 많이 벌 수 있나요",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.15",
          "form": "주식은 부업으로 좋아요",
          "original_form": "주식은 부업으로 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.16",
          "form": "맞아요 부업으로 딱이에요",
          "original_form": "맞아요 부업으로 딱이에요",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.17",
          "form": "목표는 50% 에요",
          "original_form": "목표는 50% 에요",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.18",
          "form": "원금에 50%",
          "original_form": "원금에 50%",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030408.1.19",
          "form": "50%라는 것은 무슨 의미인가요",
          "original_form": "50%라는 것은 무슨 의미인가요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.20",
          "form": "우와 장난 아니네요",
          "original_form": "우와 장난 아니네요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.21",
          "form": "시드머니 대비해서 수익이요",
          "original_form": "시드머니 대비해서 수익이요",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.22",
          "form": "실패 하는 것도 있지 않나요",
          "original_form": "실패 하는 것도 있지 않나요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.23",
          "form": "매달 5%에서 10% 수익이나요",
          "original_form": "매달 5%에서 10% 수익이나요",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.24",
          "form": "손절 도하죠",
          "original_form": "손절 도하죠",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.25",
          "form": "잘 하시나 봐요",
          "original_form": "잘 하시나 봐요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.26",
          "form": "지난번처럼 지수가 안 좋은 것은",
          "original_form": "지난번처럼 지수가 안 좋은 것은",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.27",
          "form": "보통 얼마씩 넣나요",
          "original_form": "보통 얼마씩 넣나요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.28",
          "form": "손절 안 해요",
          "original_form": "손절 안 해요",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030408.1.29",
          "form": "100에서 500이요",
          "original_form": "100에서 500이요",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030408.1.30",
          "form": "얼마 치씩 사나요",
          "original_form": "얼마 치씩 사나요",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030408.1.31",
          "form": "그러면 5%가 나면 5만 원에서 25만 원",
          "original_form": "그러면 5%가 나면 5만 원에서 25만 원",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030408.1.32",
          "form": "10% 수 이기면 10에서 50번은 건가요",
          "original_form": "10% 수 이기면 10에서 50번은 건가요",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030408.1.33",
          "form": "종목은 보통 15% 생각해요",
          "original_form": "종목은 보통 15% 생각해요",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030408.1.34",
          "form": "근데 비중이 큰 거는 5%",
          "original_form": "근데 비중이 큰 거는 5%",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030408.1.35",
          "form": "보통 15% 해요",
          "original_form": "보통 15% 해요",
          "speaker_id": "2",
          "time": "20210904 00:48"
        }
      ]
    }
