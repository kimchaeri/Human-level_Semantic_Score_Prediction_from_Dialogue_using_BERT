      "utterance": [
        {
          "id": "MDRW2100015195.1.1",
          "form": "왓츠인마이백 태연 것도 보셨어요?",
          "original_form": "왓츠인마이백 태연 것도 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015195.1.2",
          "form": "향수 엄청 유명해졌던데 저도 사고 싶어요...8ㅅ8",
          "original_form": "향수 엄청 유명해졌던데 저도 사고 싶어요...8ㅅ8",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015195.1.3",
          "form": "아 저는 캡쳐만 봐서요ㅠㅠ!",
          "original_form": "아 저는 캡쳐만 봐서요ㅠㅠ!",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015195.1.4",
          "form": "혹시 어디껀가요?",
          "original_form": "혹시 어디껀가요?",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015195.1.5",
          "form": "에르메스꺼요!",
          "original_form": "에르메스꺼요!",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015195.1.6",
          "form": "남자향수긴 한데 풀향이래요",
          "original_form": "남자향수긴 한데 풀향이래요",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015195.1.7",
          "form": "비싸겠네요!!!! 사실 방금 찾아봤어요ㅎㅎㅎ",
          "original_form": "비싸겠네요!!!! 사실 방금 찾아봤어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015195.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요...",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015195.1.9",
          "form": "그래서믕...향수는 많이 사지는 못하고 관심만 많습니다!",
          "original_form": "그래서믕...향수는 많이 사지는 못하고 관심만 많습니다!",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015195.1.10",
          "form": "요즘 사고싶은거 잇으세요?!",
          "original_form": "요즘 사고싶은거 잇으세요?!",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015195.1.11",
          "form": "요즘은 도톰한 자켓?점퍼 종류 사고싶어요",
          "original_form": "요즘은 도톰한 자켓?점퍼 종류 사고싶어요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015195.1.12",
          "form": "가을도 안지났는데 벌써 겨울옷 욕심...",
          "original_form": "가을도 안지났는데 벌써 겨울옷 욕심...",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015195.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015195.1.14",
          "form": "아 그쵸... 옷사아죠ㅠ",
          "original_form": "아 그쵸... 옷사아죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015195.1.15",
          "form": "저는 옷사는게 왜캐 귀찮을까요",
          "original_form": "저는 옷사는게 왜캐 귀찮을까요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015195.1.16",
          "form": "옷사는거 좋아하세요?",
          "original_form": "옷사는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015195.1.17",
          "form": "좋아는 하는데 돈아껴야해서",
          "original_form": "좋아는 하는데 돈아껴야해서",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015195.1.18",
          "form": "요새는 아이쇼핑만...열심히 하고 있습니다",
          "original_form": "요새는 아이쇼핑만...열심히 하고 있습니다",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015195.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015195.1.20",
          "form": "네이버 쇼핑 보면 좀 저렴하게 잘올라오는것같아요!",
          "original_form": "네이버 쇼핑 보면 좀 저렴하게 잘올라오는것같아요!",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015195.1.21",
          "form": "네이버쇼칭에서 아울렛윈도우요",
          "original_form": "네이버쇼칭에서 아울렛윈도우요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015195.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015195.1.23",
          "form": "네이버멤버쉽 이용하면 적립금도 쏠쏠하죠",
          "original_form": "네이버멤버쉽 이용하면 적립금도 쏠쏠하죠",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015195.1.24",
          "form": "요즘 그거 후기 쓰는게 조금 귀찮지 않나요?ㅋㅋ",
          "original_form": "요즘 그거 후기 쓰는게 조금 귀찮지 않나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015195.1.25",
          "form": "맞아요ㅠㅠ막상 쓰려면 할말도 없어요!",
          "original_form": "맞아요ㅠㅠ막상 쓰려면 할말도 없어요!",
          "speaker_id": "1",
          "time": "20210825 22:31"
        }
      ]
