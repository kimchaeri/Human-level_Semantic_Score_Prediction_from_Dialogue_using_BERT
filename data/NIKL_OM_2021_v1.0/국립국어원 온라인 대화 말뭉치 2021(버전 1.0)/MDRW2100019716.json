      "utterance": [
        {
          "id": "MDRW2100019716.1.1",
          "form": "최근 보신 책 잇나요",
          "original_form": "최근 보신 책 잇나요",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019716.1.2",
          "form": "와 책은자신이없는데요",
          "original_form": "와 책은자신이없는데요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019716.1.3",
          "form": "마음의양식이없어요",
          "original_form": "마음의양식이없어요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019716.1.4",
          "form": "저최근본거라곤일본어책..",
          "original_form": "저최근본거라곤일본어책..",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019716.1.5",
          "form": "재밌게본책있나요?",
          "original_form": "재밌게본책있나요?",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019716.1.6",
          "form": "일본어 천재 아니십니까",
          "original_form": "일본어 천재 아니십니까",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019716.1.7",
          "form": "저는 최근에 채식주의자 봤어요",
          "original_form": "저는 최근에 채식주의자 봤어요",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019716.1.8",
          "form": "오 유명한책아닙니까  재밌게봤나요?",
          "original_form": "오 유명한책아닙니까  재밌게봤나요?",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019716.1.9",
          "form": "일본어천재라고해주는사람 name1님뿐",
          "original_form": "일본어천재라고해주는사람 &name1&님뿐",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019716.1.10",
          "form": "ㅋㅋㅈㄱㅈㅋㅈㄱㅈㅋ",
          "original_form": "ㅋㅋㅈㄱㅈㅋㅈㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019716.1.11",
          "form": "애써 기피하다가 봤어요 맨부커상 받아서",
          "original_form": "애써 기피하다가 봤어요 맨부커상 받아서",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019716.1.12",
          "form": "아니 맞잖아요 당연해서 다들 천재라 안하는듯",
          "original_form": "아니 맞잖아요 당연해서 다들 천재라 안하는듯",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019716.1.13",
          "form": "당연리 천재라서",
          "original_form": "당연리 천재라서",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019716.1.14",
          "form": "ㅋㅋㅋㅋname1님은 말을이쁘게해주세요",
          "original_form": "ㅋㅋㅋㅋ&name1&님은 말을이쁘게해주세요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019716.1.15",
          "form": "기피하다가봤대",
          "original_form": "기피하다가봤대",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019716.1.16",
          "form": "ㅋㄱㅈㅋㅈㅋ",
          "original_form": "ㅋㄱㅈㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019716.1.17",
          "form": "예전에김밥집갔는데 저책이있었어요",
          "original_form": "예전에김밥집갔는데 저책이있었어요",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019716.1.18",
          "form": "오 김밥집에 특이하네요",
          "original_form": "오 김밥집에 특이하네요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019716.1.19",
          "form": "상받아서 그러려니 해요",
          "original_form": "상받아서 그러려니 해요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019716.1.20",
          "form": "아니면 뭔 이런 글을 써 했을거에요",
          "original_form": "아니면 뭔 이런 글을 써 했을거에요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019716.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019716.1.22",
          "form": "아 그러고보니 주식책사놓고아직도안읽었어요..",
          "original_form": "아 그러고보니 주식책사놓고아직도안읽었어요..",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019716.1.23",
          "form": "맞아요 저도요",
          "original_form": "맞아요 저도요",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019716.1.24",
          "form": "우리주식책읽고부자되어야하눈데",
          "original_form": "우리주식책읽고부자되어야하눈데",
          "speaker_id": "2",
          "time": "20210827 22:29"
