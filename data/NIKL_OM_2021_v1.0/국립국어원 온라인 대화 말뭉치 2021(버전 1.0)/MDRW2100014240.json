      "utterance": [
        {
          "id": "MDRW2100014240.1.1",
          "form": "쇼핑이라",
          "original_form": "쇼핑이라",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014240.1.2",
          "form": "가을옷 사기 딱좋은 시기같지않아?",
          "original_form": "가을옷 사기 딱좋은 시기같지않아?",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014240.1.3",
          "form": "인정 맞아 요즘에 계속 찾아보는중",
          "original_form": "인정 맞아 요즘에 계속 찾아보는중",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014240.1.4",
          "form": "뭘 사야할지 모르겠아",
          "original_form": "뭘 사야할지 모르겠아",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014240.1.5",
          "form": "나는 이번년은 옷 안사려고...ㅎㅎ 사고 싶은 옷은 많다.",
          "original_form": "나는 이번년은 옷 안사려고...ㅎㅎ 사고 싶은 옷은 많다.",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014240.1.6",
          "form": "어떤 거 사고싶은데?",
          "original_form": "어떤 거 사고싶은데?",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014240.1.7",
          "form": "빵캡모자,머플러, 하얀자켓, 90년대 셔츠, 등등...",
          "original_form": "빵캡모자,머플러, 하얀자켓, 90년대 셔츠, 등등...",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014240.1.8",
          "form": "90년대 셔츠는 모야?",
          "original_form": "90년대 셔츠는 모야?",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014240.1.9",
          "form": "엄... 살짝 그 시절? 셔츠?",
          "original_form": "엄... 살짝 그 시절? 셔츠?",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014240.1.10",
          "form": "오 빈티지?",
          "original_form": "오 빈티지?",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014240.1.11",
          "form": "빈티지 호호...",
          "original_form": "빈티지 호호...",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014240.1.12",
          "form": "허홍",
          "original_form": "허홍",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014240.1.13",
          "form": "하얀자켓은 왜?",
          "original_form": "하얀자켓은 왜?",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014240.1.14",
          "form": "한번쯤 사보고 싶었어ㅋㅋㅋㅋㅋ",
          "original_form": "한번쯤 사보고 싶었어ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014240.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014240.1.16",
          "form": "난 후드티 사고싶드라",
          "original_form": "난 후드티 사고싶드라",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014240.1.17",
          "form": "오오~~~ 좋지 후드티",
          "original_form": "오오~~~ 좋지 후드티",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100014240.1.18",
          "form": "날이 또 언제 추워질지 몰라서 고민돼 뭘 사야할짘ㅋㅋㅋㅋ",
          "original_form": "날이 또 언제 추워질지 몰라서 고민돼 뭘 사야할짘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100014240.1.19",
          "form": "요즘 날씨가 이러니...아휴",
          "original_form": "요즘 날씨가 이러니...아휴",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100014240.1.20",
          "form": "완전 띠요용이야",
          "original_form": "완전 띠요용이야",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100014240.1.21",
          "form": "나 맞아 최근에 컨버스 뮬 선물 받암ㅅ어",
          "original_form": "나 맞아 최근에 컨버스 뮬 선물 받암ㅅ어",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100014240.1.22",
          "form": "편하더라구",
          "original_form": "편하더라구",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100014240.1.23",
          "form": "오오.... 굳이네",
          "original_form": "오오.... 굳이네",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100014240.1.24",
          "form": "너 당근마켓해본 적 있냐",
          "original_form": "너 당근마켓해본 적 있냐",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100014240.1.25",
          "form": "없다...! 너는 당근러 아녀?",
          "original_form": "없다...! 너는 당근러 아녀?",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100014240.1.26",
          "form": "많이 했었지 ㅌㅋㅋㅋㅋㅋㅋ 용돈벌이였음",
          "original_form": "많이 했었지 ㅌㅋㅋㅋㅋㅋㅋ 용돈벌이였음",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100014240.1.27",
          "form": "근데 요즘 팔 게 없어서 안하고있어",
          "original_form": "근데 요즘 팔 게 없어서 안하고있어",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100014240.1.28",
          "form": "오... 나도 팔거 팔아야하는데ㅋㅋㅋㅋㅋㄲ",
          "original_form": "오... 나도 팔거 팔아야하는데ㅋㅋㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100014240.1.29",
          "form": "오 뭐 팔게!!",
          "original_form": "오 뭐 팔게!!",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100014240.1.30",
          "form": "아이돌 굿즈!",
          "original_form": "아이돌 굿즈!",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100014240.1.31",
          "form": "그거 예전부터 판다하지 않았냨ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거 예전부터 판다하지 않았냨ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100014240.1.32",
          "form": "얼른 청산혀",
          "original_form": "얼른 청산혀",
