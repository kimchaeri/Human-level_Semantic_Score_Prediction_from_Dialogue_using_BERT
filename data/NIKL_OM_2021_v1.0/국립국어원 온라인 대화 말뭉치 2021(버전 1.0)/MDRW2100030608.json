      "utterance": [
        {
          "id": "MDRW2100030608.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100030608.1.2",
          "form": "혹시 케이블티비에서 영화보나요?",
          "original_form": "혹시 케이블티비에서 영화보나요?",
          "speaker_id": "1",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100030608.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100030608.1.4",
          "form": "한번씩 재밌는게 해주더라구요",
          "original_form": "한번씩 재밌는게 해주더라구요",
          "speaker_id": "2",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100030608.1.5",
          "form": "그럼 봐요",
          "original_form": "그럼 봐요",
          "speaker_id": "2",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100030608.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100030608.1.7",
          "form": "막 찾아보는거까진 아니고 채널돌리다 걸리면 보는정도",
          "original_form": "막 찾아보는거까진 아니고 채널돌리다 걸리면 보는정도",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100030608.1.8",
          "form": "케이블 채널도 많아서 볼 게 제법 있어요",
          "original_form": "케이블 채널도 많아서 볼 게 제법 있어요",
          "speaker_id": "1",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100030608.1.9",
          "form": "비슷한 영화가 로테이션되니까 앞부분 놓쳤을때 다시 볼 수 있어서 좋던데요ㅋㄱㅋ",
          "original_form": "비슷한 영화가 로테이션되니까 앞부분 놓쳤을때 다시 볼 수 있어서 좋던데요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210904 10:18"
        },
        {
          "id": "MDRW2100030608.1.10",
          "form": "늘 비슷비슷한거 해주긴 하죠 그런데 명작들이 꽤 있어요",
          "original_form": "늘 비슷비슷한거 해주긴 하죠 그런데 명작들이 꽤 있어요",
          "speaker_id": "1",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100030608.1.11",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100030608.1.12",
          "form": "지금 ocn에서 킹덤오브헤븐 하는데 제가 참 좋아하는 명작영화거든요",
          "original_form": "지금 ocn에서 킹덤오브헤븐 하는데 제가 참 좋아하는 명작영화거든요",
          "speaker_id": "1",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100030608.1.13",
          "form": "오 저도 좋아해요",
          "original_form": "오 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 10:19"
        },
        {
          "id": "MDRW2100030608.1.14",
          "form": "지금 광고중이고 아직 앞부분이에요",
          "original_form": "지금 광고중이고 아직 앞부분이에요",
          "speaker_id": "1",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100030608.1.15",
          "form": "한번 틀어봐야겠네용",
          "original_form": "한번 틀어봐야겠네용",
          "speaker_id": "2",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100030608.1.16",
          "form": "올랜도블룸 거기서 완전 멋진데",
          "original_form": "올랜도블룸 거기서 완전 멋진데",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100030608.1.17",
          "form": "딱 광고중이네요 저도 봐야겠어요",
