      "utterance": [
        {
          "id": "MDRW2100045142.1.1",
          "form": "오늘 날씨 어땠어요",
          "original_form": "오늘 날씨 어땠어요",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045142.1.2",
          "form": "제가 요즘 집콕을 해서 창문으로만 날씨 확인하는데 ㅎ 지금은 춥네요",
          "original_form": "제가 요즘 집콕을 해서 창문으로만 날씨 확인하는데 ㅎ 지금은 춥네요",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045142.1.3",
          "form": "오늘 비가 왔어요",
          "original_form": "오늘 비가 왔어요",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045142.1.4",
          "form": "거기도 비가 왔나요",
          "original_form": "거기도 비가 왔나요",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045142.1.5",
          "form": "저 더비에요",
          "original_form": "저 더비에요",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045142.1.6",
          "form": "죄송하고요",
          "original_form": "죄송하고요",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045142.1.7",
          "form": "비는 안온거같아염",
          "original_form": "비는 안온거같아염",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045142.1.8",
          "form": "아 예",
          "original_form": "아 예",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045142.1.9",
          "form": "여긴 비가 많이 왔어요",
          "original_form": "여긴 비가 많이 왔어요",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045142.1.10",
          "form": "혹시 우산 있어요 ?",
          "original_form": "혹시 우산 있어요 ?",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045142.1.11",
          "form": "우산이요?",
          "original_form": "우산이요?",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045142.1.12",
          "form": "네 우산이요",
          "original_form": "네 우산이요",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.13",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.14",
          "form": "있죠",
          "original_form": "있죠",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.15",
          "form": "무슨 색이에요?",
          "original_form": "무슨 색이에요?",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.16",
          "form": "많은데",
          "original_form": "많은데",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.17",
          "form": "대표적인건 투명이죠",
          "original_form": "대표적인건 투명이죠",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.18",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.19",
          "form": "밝은 우산이 좋아요",
          "original_form": "밝은 우산이 좋아요",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.20",
          "form": "왜냐고물어봐",
          "original_form": "왜냐고물어봐",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.21",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045142.1.22",
          "form": "비오는 날은 어두워서 밝은 우산을 써야 차에서 잘보인데요",
          "original_form": "비오는 날은 어두워서 밝은 우산을 써야 차에서 잘보인데요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045142.1.23",
          "form": "사고나면 안되잖아요",
          "original_form": "사고나면 안되잖아요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        }
      ]
