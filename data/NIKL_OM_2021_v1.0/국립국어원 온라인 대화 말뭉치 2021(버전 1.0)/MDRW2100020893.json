        {
          "id": "MDRW2100020893.1.2",
          "form": "다이어트는 잘되가요?",
          "original_form": "다이어트는 잘되가요?",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020893.1.3",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020893.1.4",
          "form": "방금 빵을 잔뜩사먹었다지",
          "original_form": "방금 빵을 잔뜩사먹었다지",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020893.1.5",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020893.1.6",
          "form": "지금도 안먹었음 내가 넘 낯설겠지",
          "original_form": "지금도 안먹었음 내가 넘 낯설겠지",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020893.1.7",
          "form": "탄수화물다요트다",
          "original_form": "탄수화물다요트다",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020893.1.8",
          "form": "뜨레쥬르에 카레빵 퍼레이드를 참지못하고 ㅎㅎ",
          "original_form": "뜨레쥬르에 카레빵 퍼레이드를 참지못하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020893.1.9",
          "form": "ㅋㅋ아 그건못참지",
          "original_form": "ㅋㅋ아 그건못참지",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020893.1.10",
          "form": "식욕억제제를 먹어야할까",
          "original_form": "식욕억제제를 먹어야할까",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020893.1.11",
          "form": "카레빵 맛있겟다",
          "original_form": "카레빵 맛있겟다",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020893.1.12",
          "form": "고기까지들었어 다여트망",
          "original_form": "고기까지들었어 다여트망",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020893.1.13",
          "form": "억제제도 근데 내 식욕을 참아주지 않웅",
          "original_form": "억제제도 근데 내 식욕을 참아주지 않웅",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020893.1.14",
          "form": "계속 먹으라하지 내맘이",
          "original_form": "계속 먹으라하지 내맘이",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020893.1.15",
          "form": "그치 몸도 맘도",
          "original_form": "그치 몸도 맘도",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020893.1.16",
          "form": "그래도 이번주 저녁은 잘굶음",
          "original_form": "그래도 이번주 저녁은 잘굶음",
