      "utterance": [
        {
          "id": "MDRW2100035132.1.1",
          "form": "네네! 먹어봤어요",
          "original_form": "네네! 먹어봤어요",
          "speaker_id": "1",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035132.1.2",
          "form": "카레여왕사면 항상 그맛으로 사요",
          "original_form": "카레여왕사면 항상 그맛으로 사요",
          "speaker_id": "1",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035132.1.3",
          "form": "저흰 그것만 먹어요 ㅋㅋㅋㅋ",
          "original_form": "저흰 그것만 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035132.1.4",
          "form": "역쉬~!!",
          "original_form": "역쉬~!!",
          "speaker_id": "2",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035132.1.5",
          "form": "카레좋아하시는군요 ㅋㅋㅋㅋ",
          "original_form": "카레좋아하시는군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:28"
        },
        {
          "id": "MDRW2100035132.1.6",
          "form": "네네ㅋㅋㅋ 그 새로나온 티아시아 카레도 맛있다던데",
          "original_form": "네네ㅋㅋㅋ 그 새로나온 티아시아 카레도 맛있다던데",
          "speaker_id": "1",
          "time": "20210829 06:29"
        },
        {
          "id": "MDRW2100035132.1.7",
          "form": "아직 못먹어봐서 궁금해요ㅋㅋ",
          "original_form": "아직 못먹어봐서 궁금해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:29"
        },
        {
          "id": "MDRW2100035132.1.8",
          "form": "전 약간 하나만 파는 스타일이라 ㅋㅋㅋㅋ",
          "original_form": "전 약간 하나만 파는 스타일이라 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:30"
        },
        {
          "id": "MDRW2100035132.1.9",
          "form": "티아시아 도전해 봅니다 ㅋㅋㅋㅋ",
          "original_form": "티아시아 도전해 봅니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:31"
        },
        {
          "id": "MDRW2100035132.1.10",
          "form": "푸팟퐁이랑 무슨 마크니랑 맛이 다양했어요",
          "original_form": "푸팟퐁이랑 무슨 마크니랑 맛이 다양했어요",
          "speaker_id": "1",
          "time": "20210829 06:31"
        },
        {
          "id": "MDRW2100035132.1.11",
          "form": "개인적으로 분말로 나온 카레중에 푸팟퐁커리 버전은 처음봐서",
          "original_form": "개인적으로 분말로 나온 카레중에 푸팟퐁커리 버전은 처음봐서",
          "speaker_id": "1",
          "time": "20210829 06:31"
        },
        {
          "id": "MDRW2100035132.1.12",
          "form": "신기하더라구요ㅋㅋ",
          "original_form": "신기하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:31"
        },
        {
          "id": "MDRW2100035132.1.13",
          "form": "오~카레에 진심이심 분이군요^^",
          "original_form": "오~카레에 진심이심 분이군요^^",
          "speaker_id": "2",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035132.1.14",
          "form": "좋은정보 감사해요^^",
          "original_form": "좋은정보 감사해요^^",
          "speaker_id": "2",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035132.1.15",
          "form": "전 항상 먹는거에 진심이에요ㅋㅋ",
          "original_form": "전 항상 먹는거에 진심이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:32"
        },
        {
          "id": "MDRW2100035132.1.16",
          "form": "저도 구렇습니다^^ㅋㅋㅋㅋㅋ근데 새로운 음식에 도전을 잘 안해요;;",
          "original_form": "저도 구렇습니다^^ㅋㅋㅋㅋㅋ근데 새로운 음식에 도전을 잘 안해요;;",
          "speaker_id": "2",
          "time": "20210829 06:33"
        },
        {
          "id": "MDRW2100035132.1.17",
          "form": "실패하면 하루가 우울합니다 ㅋㅋㅋㅋ",
          "original_form": "실패하면 하루가 우울합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:33"
        },
        {
          "id": "MDRW2100035132.1.18",
          "form": "아하 그럼 꾸준히 드시는 음식은 뭔가요?",
          "original_form": "아하 그럼 꾸준히 드시는 음식은 뭔가요?",
          "speaker_id": "1",
          "time": "20210829 06:33"
        },
        {
          "id": "MDRW2100035132.1.19",
          "form": "전 불닭, 불족 이런..간이 쎈거 좋아해요~",
          "original_form": "전 불닭, 불족 이런..간이 쎈거 좋아해요~",
          "speaker_id": "2",
          "time": "20210829 06:33"
        },
        {
          "id": "MDRW2100035132.1.20",
          "form": "이런건 어디든 기본타는 하니;;",
          "original_form": "이런건 어디든 기본타는 하니;;",
          "speaker_id": "2",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035132.1.21",
          "form": "그런데..한해 한해 지날수록 속이 아파요 ㅋㅋㅋㅋㅋ",
          "original_form": "그런데..한해 한해 지날수록 속이 아파요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035132.1.22",
          "form": "오호 입맛이 저랑 완전 다르시네용ㅋㅋㅋ",
          "original_form": "오호 입맛이 저랑 완전 다르시네용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035132.1.23",
          "form": "저는 매운거나 족발 닭발은 못먹어요ㅠㅠ",
          "original_form": "저는 매운거나 족발 닭발은 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035132.1.24",
          "form": "매운거 잘드시는분들 부러워요",
          "original_form": "매운거 잘드시는분들 부러워요",
          "speaker_id": "1",
          "time": "20210829 06:34"
        },
        {
          "id": "MDRW2100035132.1.25",
          "form": "부러워 하지마세요~이거 어느순간 어느정도 맵기는 성에 안차고..속이 남아나질 않아요ㅠㅠ 저도 이걸 모루던때로 돌리고싶어요ㅠ",
          "original_form": "부러워 하지마세요~이거 어느순간 어느정도 맵기는 성에 안차고..속이 남아나질 않아요ㅠㅠ 저도 이걸 모루던때로 돌리고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100035132.1.26",
          "form": "속은 넘 아프고ㅠ 매운거 아님 먼가 늬글하고ㅠ 악순환이예요;;",
          "original_form": "속은 넘 아프고ㅠ 매운거 아님 먼가 늬글하고ㅠ 악순환이예요;;",
          "speaker_id": "2",
          "time": "20210829 06:36"
        },
        {
          "id": "MDRW2100035132.1.27",
          "form": "아 그럼 드실때 우유를 미리 드시고 식사하시면 어떨까요?",
          "original_form": "아 그럼 드실때 우유를 미리 드시고 식사하시면 어떨까요?",
          "speaker_id": "1",
          "time": "20210829 06:37"
        },
        {
          "id": "MDRW2100035132.1.28",
          "form": "마냥 부러워만했는데",
          "original_form": "마냥 부러워만했는데",
          "speaker_id": "1",
          "time": "20210829 06:37"
        },
        {
          "id": "MDRW2100035132.1.29",
          "form": "고충이 있으셨네요ㅠ",
          "original_form": "고충이 있으셨네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 06:37"
        }
      ]
