      "utterance": [
        {
          "id": "MDRW2100040143.1.1",
          "form": "저는 진구 ㅎㅎ",
          "original_form": "저는 진구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100040143.1.2",
          "form": "오 그렇군요 진구좋죠",
          "original_form": "오 그렇군요 진구좋죠",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100040143.1.3",
          "form": "가족분들과 같이 사시죠?",
          "original_form": "가족분들과 같이 사시죠?",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100040143.1.4",
          "form": "아니요 혼자 고양이들이랑요 ㅎㅎ",
          "original_form": "아니요 혼자 고양이들이랑요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100040143.1.5",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100040143.1.6",
          "form": "오 고양이집사군요",
          "original_form": "오 고양이집사군요",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100040143.1.7",
          "form": "가족과 강아지와 살아요",
          "original_form": "가족과 강아지와 살아요",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100040143.1.8",
          "form": "오 강아지 몇살이에요",
          "original_form": "오 강아지 몇살이에요",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100040143.1.9",
          "form": "네살입니다 고양이몇살이에요",
          "original_form": "네살입니다 고양이몇살이에요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100040143.1.10",
          "form": "오ㅏ 저희애들도 얼마전 4살생일 지났어요 ㅎㅎ",
          "original_form": "오ㅏ 저희애들도 얼마전 4살생일 지났어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100040143.1.11",
          "form": "오 몇마리인가요",
          "original_form": "오 몇마리인가요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100040143.1.12",
          "form": "종이 뭔가용",
          "original_form": "종이 뭔가용",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100040143.1.13",
          "form": "2마리에요",
          "original_form": "2마리에요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100040143.1.14",
          "form": "오 좋네요 저는 장모치와와입니다",
          "original_form": "오 좋네요 저는 장모치와와입니다",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100040143.1.15",
          "form": "소형견이죠?",
          "original_form": "소형견이죠?",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100040143.1.16",
          "form": "네네맞아요",
          "original_form": "네네맞아요",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100040143.1.17",
