      "utterance": [
        {
          "id": "MDRW2100018801.1.1",
          "form": "건강 챙기기 힘들죠",
          "original_form": "건강 챙기기 힘들죠",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018801.1.2",
          "form": "전 핫요가 한창 유행일때. 했는데 하고나면. 땀이 줄줄",
          "original_form": "전 핫요가 한창 유행일때. 했는데 하고나면. 땀이 줄줄",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018801.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018801.1.4",
          "form": "생각외였어요",
          "original_form": "생각외였어요",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018801.1.5",
          "form": "플라잉 요가도 배워보고 싶어요ㅎㅎ",
          "original_form": "플라잉 요가도 배워보고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.6",
          "form": "플라잉요가",
          "original_form": "플라잉요가",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.7",
          "form": "전 그 자세 해보고싶어요",
          "original_form": "전 그 자세 해보고싶어요",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.8",
          "form": "가만히 웅크려있는자세요",
          "original_form": "가만히 웅크려있는자세요",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.9",
          "form": "어떤 자세요?",
          "original_form": "어떤 자세요?",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.10",
          "form": "그게 차분해진다는데 궁금해요",
          "original_form": "그게 차분해진다는데 궁금해요",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.12",
          "form": "아 ㅎㅎㅎ",
          "original_form": "아 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018801.1.13",
          "form": "저 아는분도 그래서 했는데 잡생각만 너무 많이나서 포기하셨어요",
          "original_form": "저 아는분도 그래서 했는데 잡생각만 너무 많이나서 포기하셨어요",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018801.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018801.1.15",
          "form": "앜ㅋㅋㅋ 그렇군요",
          "original_form": "앜ㅋㅋㅋ 그렇군요",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018801.1.16",
          "form": "사람마다 다른가봐요",
          "original_form": "사람마다 다른가봐요",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018801.1.17",
          "form": "일단 공중에 있어서 그네타는것처럼 재밌어보이는데",
          "original_form": "일단 공중에 있어서 그네타는것처럼 재밌어보이는데",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018801.1.18",
          "form": "그것도 많이 힘들겠죠",
          "original_form": "그것도 많이 힘들겠죠",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018801.1.19",
          "form": "네ㅋㅋ그 분은 활동적인 운동이 맞는걸로ㅋ",
          "original_form": "네ㅋㅋ그 분은 활동적인 운동이 맞는걸로ㅋ",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018801.1.20",
          "form": "네 균형잡는데 힘이 많이 든대요",
          "original_form": "네 균형잡는데 힘이 많이 든대요",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018801.1.21",
          "form": "균형 아 그렇겠어요",
          "original_form": "균형 아 그렇겠어요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018801.1.22",
          "form": "근데 하신분이 엄청 강추하더라구요",
          "original_form": "근데 하신분이 엄청 강추하더라구요",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018801.1.23",
          "form": "생각해보니 운동강도가 셀것 같아요",
          "original_form": "생각해보니 운동강도가 셀것 같아요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018801.1.24",
          "form": "균형 잡으니까 온몸 근육을 쓰겠네요!",
          "original_form": "균형 잡으니까 온몸 근육을 쓰겠네요!",
          "speaker_id": "2",
          "time": "20210827 12:21"
        }
