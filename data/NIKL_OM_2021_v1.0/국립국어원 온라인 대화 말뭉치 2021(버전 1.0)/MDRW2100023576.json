      "utterance": [
        {
          "id": "MDRW2100023576.1.1",
          "form": "오늘의 점심메뉴는?",
          "original_form": "오늘의 점심메뉴는?",
          "speaker_id": "2",
          "time": "20210830 11:51"
        },
        {
          "id": "MDRW2100023576.1.2",
          "form": "토스트!",
          "original_form": "토스트!",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023576.1.3",
          "form": "옛날식 토스트 시켜먹었는데 너무 맛있어서 눈물이....",
          "original_form": "옛날식 토스트 시켜먹었는데 너무 맛있어서 눈물이....",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023576.1.4",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023576.1.5",
          "form": "토스트 맛있었겠다 ㅜㅜ 저는 그냥 집밥먹었어요 ㅜ 출근했다가 저녁먹으러 가는길입니다 ㅋㅋ",
          "original_form": "토스트 맛있었겠다 ㅜㅜ 저는 그냥 집밥먹었어요 ㅜ 출근했다가 저녁먹으러 가는길입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023576.1.6",
          "form": "저녁 뭐드셨어요??",
          "original_form": "저녁 뭐드셨어요??",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100023576.1.7",
          "form": "저는 타이밍 놓쳐서 지금 라면끓여먹어요ㅠ",
          "original_form": "저는 타이밍 놓쳐서 지금 라면끓여먹어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023576.1.8",
          "form": "저녁도 집밥....ㅋㅋㅋㅋㅋ 직장이 코앞이라 집에서 밥먹어요 ㅋㅋㅋ짱이죠...",
          "original_form": "저녁도 집밥....ㅋㅋㅋㅋㅋ 직장이 코앞이라 집에서 밥먹어요 ㅋㅋㅋ짱이죠...",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023576.1.9",
          "form": "라면맛있겠다.. 저지금퇴근해서 옥수수먹어요..",
          "original_form": "라면맛있겠다.. 저지금퇴근해서 옥수수먹어요..",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023576.1.10",
          "form": "밥값 절약되고 좋죠ㅎㅎㅎ",
          "original_form": "밥값 절약되고 좋죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100023576.1.11",
          "form": "저는 옥수수가 맛있을거같은데요ㅜㅜㅜ 쫀득쫀득",
          "original_form": "저는 옥수수가 맛있을거같은데요ㅜㅜㅜ 쫀득쫀득",
          "speaker_id": "1",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100023576.1.12",
          "form": "옥수수 요즘 완전 빠져있잖아요 ㅋㅋㅋ 아 너무맛잇어요 어떤옥수수좋아하세요",
          "original_form": "옥수수 요즘 완전 빠져있잖아요 ㅋㅋㅋ 아 너무맛잇어요 어떤옥수수좋아하세요",
          "speaker_id": "2",
          "time": "20210830 23:09"
        },
        {
          "id": "MDRW2100023576.1.13",
          "form": "저는 그냥 옥수수?ㅋㅋ",
          "original_form": "저는 그냥 옥수수?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100023576.1.14",
          "form": "초당옥수수라는걸 먹어보질 못했어요",
          "original_form": "초당옥수수라는걸 먹어보질 못했어요",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100023576.1.15",
          "form": "딱딱할거같아서",
          "original_form": "딱딱할거같아서",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100023576.1.16",
          "form": "저도 그냥 찰옥수수가좋아요 ㅜ 초당은 너무 사각거려요...",
          "original_form": "저도 그냥 찰옥수수가좋아요 ㅜ 초당은 너무 사각거려요...",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100023576.1.17",
          "form": "근데 정말 달긴 달아요?!",
          "original_form": "근데 정말 달긴 달아요?!",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100023576.1.18",
          "form": "어떤느낌이려나..",
          "original_form": "어떤느낌이려나..",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100023576.1.19",
          "form": "옥수수캔 맛이에요 ㅋㅋㅋ딱그느낌",
          "original_form": "옥수수캔 맛이에요 ㅋㅋㅋ딱그느낌",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100023576.1.20",
          "form": "음ㅋㅋ앞으로 먹을일 없을거같네요ㅋㅋㅋ",
          "original_form": "음ㅋㅋ앞으로 먹을일 없을거같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:20"
        },
        {
          "id": "MDRW2100023576.1.21",
          "form": "찰옥수수 아주 통통하게 익었을때 너무 맛있어요ㅠ",
          "original_form": "찰옥수수 아주 통통하게 익었을때 너무 맛있어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:20"
        },
        {
          "id": "MDRW2100023576.1.22",
          "form": "그 입안에서 탁 터지는 느낌너무좋지않나요...!",
          "original_form": "그 입안에서 탁 터지는 느낌너무좋지않나요...!",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100023576.1.23",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100023576.1.24",
          "form": "그리고 찰옥수수 특유의",
          "original_form": "그리고 찰옥수수 특유의",
          "speaker_id": "1",
          "time": "20210831 00:34"
        }
      ]
    }
