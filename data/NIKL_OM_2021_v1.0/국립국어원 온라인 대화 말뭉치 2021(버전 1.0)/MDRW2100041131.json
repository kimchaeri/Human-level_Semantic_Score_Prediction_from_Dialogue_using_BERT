      "utterance": [
        {
          "id": "MDRW2100041131.1.1",
          "form": "좋아하는 체인점 있으세요?",
          "original_form": "좋아하는 체인점 있으세요?",
          "speaker_id": "2",
          "time": "20210905 17:43"
        },
        {
          "id": "MDRW2100041131.1.2",
          "form": "서브웨이 좋아해요!!",
          "original_form": "서브웨이 좋아해요!!",
          "speaker_id": "1",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100041131.1.3",
          "form": "아 서브웨이 맛있고 건강한 느낌이에요",
          "original_form": "아 서브웨이 맛있고 건강한 느낌이에요",
          "speaker_id": "2",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100041131.1.4",
          "form": "사실 서브웨이 말고 퀴즈노즈라는 샌드위치집을 더 좋아했는데",
          "original_form": "사실 서브웨이 말고 퀴즈노즈라는 샌드위치집을 더 좋아했는데",
          "speaker_id": "1",
          "time": "20210905 17:44"
        },
        {
          "id": "MDRW2100041131.1.5",
          "form": "그닥 성공을 못했는지 다 없어지더라구요ㅠㅠ",
          "original_form": "그닥 성공을 못했는지 다 없어지더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.6",
          "form": "어 그건 처음 들어봐요 맛있나요?",
          "original_form": "어 그건 처음 들어봐요 맛있나요?",
          "speaker_id": "2",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.7",
          "form": "헉 ㅠㅠㅠ",
          "original_form": "헉 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.8",
          "form": "넵 서브웨이랑 비슷하긴한데",
          "original_form": "넵 서브웨이랑 비슷하긴한데",
          "speaker_id": "1",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.9",
          "form": "맛이 아주 조금 더 제 취향이었어요ㅋㅋㅋㅋ",
          "original_form": "맛이 아주 조금 더 제 취향이었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.10",
          "form": "근데 이미 여기가 크기엔 서브웨이가 너무 잘 자리잡았죠",
          "original_form": "근데 이미 여기가 크기엔 서브웨이가 너무 잘 자리잡았죠",
          "speaker_id": "1",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.11",
          "form": "아하 그렇군요 ㅜㅜ 안타깝네요",
          "original_form": "아하 그렇군요 ㅜㅜ 안타깝네요",
          "speaker_id": "2",
          "time": "20210905 17:45"
        },
        {
          "id": "MDRW2100041131.1.12",
          "form": "타이밍이...",
          "original_form": "타이밍이...",
          "speaker_id": "2",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.13",
          "form": "아 그리고 아직 못가봤는데",
          "original_form": "아 그리고 아직 못가봤는데",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.14",
          "form": "쉑쉑버거 언제 꼭 한 번 가보고 싶어요!",
          "original_form": "쉑쉑버거 언제 꼭 한 번 가보고 싶어요!",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.15",
          "form": "아 저도 그거 못 먹어 봤어요",
          "original_form": "아 저도 그거 못 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.16",
          "form": "먹고 싶어서 맥도날드에서 쉐이크 시켜서 먹은 적 있어",
          "original_form": "먹고 싶어서 맥도날드에서 쉐이크 시켜서 먹은 적 있어",
          "speaker_id": "2",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.17",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.18",
          "form": "웨이팅 길다고 들었는데",
          "original_form": "웨이팅 길다고 들었는데",
          "speaker_id": "1",
          "time": "20210905 17:46"
        },
        {
          "id": "MDRW2100041131.1.19",
          "form": "아직도 길지는 모르겠네요ㅠㅠ",
          "original_form": "아직도 길지는 모르겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041131.1.20",
          "form": "요즘에는 안 길다고 하던데요",
          "original_form": "요즘에는 안 길다고 하던데요",
          "speaker_id": "2",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041131.1.21",
          "form": "오 진작 한 번 도전해볼걸 그랬어요ㅋㅋㅋ",
          "original_form": "오 진작 한 번 도전해볼걸 그랬어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041131.1.22",
          "form": "쉑쉑하면 역시 쉐이크가 제일 유명하죠?",
          "original_form": "쉑쉑하면 역시 쉐이크가 제일 유명하죠?",
          "speaker_id": "1",
          "time": "20210905 17:47"
        },
        {
          "id": "MDRW2100041131.1.23",
          "form": "그렇지 않을까요 저도 잘 모르겠어요",
          "original_form": "그렇지 않을까요 저도 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210905 17:48"
        }
      ]
    }
