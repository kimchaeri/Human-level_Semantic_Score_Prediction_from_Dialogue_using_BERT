      "utterance": [
        {
          "id": "MDRW2100030987.1.1",
          "form": "결혼 할거니?",
          "original_form": "결혼 할거니?",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030987.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100030987.1.3",
          "form": "왜? 근디 나도 결혼 하기 싫름",
          "original_form": "왜? 근디 나도 결혼 하기 싫름",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100030987.1.4",
          "form": "장래희망이 결혼하고 살만큼 안정적인 월급이라고 보장 못하는데다가",
          "original_form": "장래희망이 결혼하고 살만큼 안정적인 월급이라고 보장 못하는데다가",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100030987.1.5",
          "form": "결혼할 바에는 그냥 친구 몇명이랑 동거하고싶음",
          "original_form": "결혼할 바에는 그냥 친구 몇명이랑 동거하고싶음",
          "speaker_id": "2",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030987.1.6",
          "form": "그치...역시 돈이 문제죠",
          "original_form": "그치...역시 돈이 문제죠",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030987.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 결혼 안하는 친구랑",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 결혼 안하는 친구랑",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030987.1.8",
          "form": "죽을 따까지 동거하기...",
          "original_form": "죽을 따까지 동거하기...",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030987.1.9",
          "form": "그치 그거지",
          "original_form": "그치 그거지",
          "speaker_id": "2",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030987.1.10",
          "form": "나는 외국인이면 좀 할 의향 잇음",
          "original_form": "나는 외국인이면 좀 할 의향 잇음",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030987.1.11",
          "form": "국내는 좀...",
          "original_form": "국내는 좀...",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030987.1.12",
          "form": "이열",
          "original_form": "이열",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030987.1.13",
          "form": "결혼은 그롷다 쳐도",
          "original_form": "결혼은 그롷다 쳐도",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100030987.1.14",
          "form": "연애도 안할거야?",
          "original_form": "연애도 안할거야?",
          "speaker_id": "1",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100030987.1.15",
          "form": "연애는...",
          "original_form": "연애는...",
          "speaker_id": "2",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100030987.1.16",
          "form": "글쎄?",
          "original_form": "글쎄?",
          "speaker_id": "2",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100030987.1.17",
          "form": "할 수 있을까? ㅋㅋㅋ",
          "original_form": "할 수 있을까? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100030987.1.18",
          "form": "ㅋㅋㅋㅋ 왜 결혼은 그래두 연애는 해봐야지",
