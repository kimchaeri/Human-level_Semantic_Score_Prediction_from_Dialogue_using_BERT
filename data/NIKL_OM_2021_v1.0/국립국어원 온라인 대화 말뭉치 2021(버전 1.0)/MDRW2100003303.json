      "utterance": [
        {
          "id": "MDRW2100003303.1.1",
          "form": "저는 싸이월드가 부활한다면",
          "original_form": "저는 싸이월드가 부활한다면",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100003303.1.2",
          "form": "먼저 옛날계정 염탐하기...?",
          "original_form": "먼저 옛날계정 염탐하기...?",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003303.1.3",
          "form": "어렸을때 추억이 새록새록할것같아요 ㅋㅋ",
          "original_form": "어렸을때 추억이 새록새록할것같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100003303.1.4",
          "form": "name2님은 어떠실것같아요?",
          "original_form": "&name2&님은 어떠실것같아요?",
          "speaker_id": "1",
          "time": "20210820 21:38"
        },
        {
          "id": "MDRW2100003303.1.5",
          "form": "ㅎㅎ 제것도 있을지ㅠ모르겠어요",
          "original_form": "ㅎㅎ 제것도 있을지ㅠ모르겠어요",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003303.1.6",
          "form": "기억이 안나요",
          "original_form": "기억이 안나요",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003303.1.7",
          "form": "왜자꾸 미뤄지는거예요?",
          "original_form": "왜자꾸 미뤄지는거예요?",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.8",
          "form": "그러게요 ㅠㅠ",
          "original_form": "그러게요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.9",
          "form": "오픈이 느려져서 기다리긴 하는데 어찌될지",
          "original_form": "오픈이 느려져서 기다리긴 하는데 어찌될지",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.10",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.11",
          "form": "예전 레트로의 귀환인가요",
          "original_form": "예전 레트로의 귀환인가요",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.12",
          "form": "빨리 부활이 됐으면 해요...!",
          "original_form": "빨리 부활이 됐으면 해요...!",
          "speaker_id": "1",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.13",
          "form": "엌 ㅋㅋ",
          "original_form": "엌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003303.1.14",
          "form": "아 응답하라가 이에 맞춰해야 하는데",
          "original_form": "아 응답하라가 이에 맞춰해야 하는데",
          "speaker_id": "2",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003303.1.15",
          "form": "그죠 ~",
          "original_form": "그죠 ~",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003303.1.16",
          "form": "그럼추억 돋는거죠",
          "original_form": "그럼추억 돋는거죠",
          "speaker_id": "2",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003303.1.17",
          "form": "진짜 추억탐방 이랄까",
          "original_form": "진짜 추억탐방 이랄까",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003303.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003303.1.19",
          "form": "옛날에 한게 남아있으면",
          "original_form": "옛날에 한게 남아있으면",
          "speaker_id": "1",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003303.1.20",
          "form": "이땐이랬었지 ~",
          "original_form": "이땐이랬었지 ~",
          "speaker_id": "1",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003303.1.21",
          "form": "하면서 옛날생각 할것같아요 ㅋㅋ",
          "original_form": "하면서 옛날생각 할것같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003303.1.22",
          "form": "제것도 있나 몰라요 전혀 기억이 안나요",
          "original_form": "제것도 있나 몰라요 전혀 기억이 안나요",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003303.1.23",
          "form": "전 조금 활동해서 그런가봐요",
          "original_form": "전 조금 활동해서 그런가봐요",
          "speaker_id": "2",
          "time": "20210820 21:44"
        }
