      "utterance": [
        {
          "id": "MDRW2100036893.1.1",
          "form": "점심 맛있게 드셨나요",
          "original_form": "점심 맛있게 드셨나요",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036893.1.2",
          "form": "네~ 전 오늘 불고기덮밥먹었어요ㅎㅎ",
          "original_form": "네~ 전 오늘 불고기덮밥먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036893.1.3",
          "form": "헐 맛있겠어요",
          "original_form": "헐 맛있겠어요",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036893.1.4",
          "form": "맛나게먹었어요 ㅎㅎ 식사하셨나요?",
          "original_form": "맛나게먹었어요 ㅎㅎ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036893.1.5",
          "form": "저는 간단하게 라면 끓여서 먹었어요",
          "original_form": "저는 간단하게 라면 끓여서 먹었어요",
          "speaker_id": "2",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036893.1.6",
          "form": "아하! 어떤라면드셧나요?",
          "original_form": "아하! 어떤라면드셧나요?",
          "speaker_id": "1",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100036893.1.7",
          "form": "신라면이요ㅎㅎ",
          "original_form": "신라면이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100036893.1.8",
          "form": "아하 역시 근본라면드셨군요! 저는 진라면을 더 좋아해요 ㅋㅋ",
          "original_form": "아하 역시 근본라면드셨군요! 저는 진라면을 더 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100036893.1.9",
          "form": "진라면도 좋긴 한데 그건 진순, 진매 합쳐야 맛있어서 그냥 신라면 먹었어요",
          "original_form": "진라면도 좋긴 한데 그건 진순, 진매 합쳐야 맛있어서 그냥 신라면 먹었어요",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036893.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 오 그런거였군요?ㅋㅋㅋㅋ 다합치면 저두 신라면이욬ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 오 그런거였군요?ㅋㅋㅋㅋ 다합치면 저두 신라면이욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036893.1.11",
          "form": "ㅋㅋ 역시 근본 신라면",
          "original_form": "ㅋㅋ 역시 근본 신라면",
          "speaker_id": "2",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100036893.1.12",
          "form": "맞아요 ㅋㅋ 그치만 전 라면중에선 짜파게티 젤 좋아해여",
          "original_form": "맞아요 ㅋㅋ 그치만 전 라면중에선 짜파게티 젤 좋아해여",
          "speaker_id": "1",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100036893.1.13",
          "form": "짜파게티는 물 버리기 귀찮아서 잘 안해먹어요ㅋㅋ",
          "original_form": "짜파게티는 물 버리기 귀찮아서 잘 안해먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100036893.1.14",
          "form": "앗 저는 물안버리고 애초에 물적게해서 졸여먹어요 ㅎㅎ",
          "original_form": "앗 저는 물안버리고 애초에 물적게해서 졸여먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036893.1.15",
          "form": "그게더맛나기도하구요",
          "original_form": "그게더맛나기도하구요",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036893.1.16",
          "form": "아하 그렇게 하는 방법도 있군요",
          "original_form": "아하 그렇게 하는 방법도 있군요",
          "speaker_id": "2",
          "time": "20210831 15:20"
        }
      ]
