      "utterance": [
        {
          "id": "MDRW2100043045.1.1",
          "form": "문답이나 해보자",
          "original_form": "문답이나 해보자",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043045.1.2",
          "form": "얍얍",
          "original_form": "얍얍",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043045.1.3",
          "form": "이상형은?",
          "original_form": "이상형은?",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043045.1.4",
          "form": "배려깊은 다정한 사람",
          "original_form": "배려깊은 다정한 사람",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043045.1.5",
          "form": "나는 나랑 비슷한 사람",
          "original_form": "나는 나랑 비슷한 사람",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043045.1.6",
          "form": "애인하고 같이 살고 싶은 집은?",
          "original_form": "애인하고 같이 살고 싶은 집은?",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043045.1.7",
          "form": "넓을수록 좋겠지만",
          "original_form": "넓을수록 좋겠지만",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043045.1.8",
          "form": "투룸정도면 괜찮을거 같아",
          "original_form": "투룸정도면 괜찮을거 같아",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043045.1.9",
          "form": "나도...최소 투룸정도?",
          "original_form": "나도...최소 투룸정도?",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043045.1.10",
          "form": "서로 분리될수있는 공간이 필요한거같아서",
          "original_form": "서로 분리될수있는 공간이 필요한거같아서",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043045.1.11",
          "form": "맞아맞아 서로 안보이는 공간이 있어야함",
          "original_form": "맞아맞아 서로 안보이는 공간이 있어야함",
          "speaker_id": "2",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043045.1.12",
          "form": "애인에게 사주고싶은 음식은?",
          "original_form": "애인에게 사주고싶은 음식은?",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100043045.1.13",
          "form": "좋아하는거 사주고싶은데 같이간곳중에 뷔페를 제일 좋아해서",
          "original_form": "좋아하는거 사주고싶은데 같이간곳중에 뷔페를 제일 좋아해서",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043045.1.14",
          "form": "뷔페데려가고싶다",
          "original_form": "뷔페데려가고싶다",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043045.1.15",
          "form": "나는 해산물을 둘다좋아해서",
          "original_form": "나는 해산물을 둘다좋아해서",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043045.1.16",
          "form": "오마카세 한번 데려가고싶어",
          "original_form": "오마카세 한번 데려가고싶어",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043045.1.17",
          "form": "오마카세좋지...",
          "original_form": "오마카세좋지...",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043045.1.18",
          "form": "너네 몇일되었더라?",
          "original_form": "너네 몇일되었더라?",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100043045.1.19",
          "form": "우리 500일넘었지",
          "original_form": "우리 500일넘었지",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043045.1.20",
          "form": "너네가 이제한 천일되었나?",
          "original_form": "너네가 이제한 천일되었나?",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043045.1.21",
          "form": "천일 넘었어",
          "original_form": "천일 넘었어",
          "speaker_id": "1",
          "time": "20210907 13:35"
        }
      ]
