      "utterance": [
        {
          "id": "MDRW2100008274.1.1",
          "form": "거긴 날씨어때요?",
          "original_form": "거긴 날씨어때요?",
          "speaker_id": "1",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008274.1.2",
          "form": "오늘은 다행히 햇빛이 나네요",
          "original_form": "오늘은 다행히 햇빛이 나네요",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008274.1.3",
          "form": "비 피해 없으세요?",
          "original_form": "비 피해 없으세요?",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008274.1.4",
          "form": "네 여기도 오늘은 흐리기만한거같아요",
          "original_form": "네 여기도 오늘은 흐리기만한거같아요",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008274.1.5",
          "form": "밖을 안나가봐서 그런데 기온은 어때요?",
          "original_form": "밖을 안나가봐서 그런데 기온은 어때요?",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008274.1.6",
          "form": "지금은 좀낫네요\r\n요새 햇빛도 뜨거워요 ㅋㅋ",
          "original_form": "지금은 좀낫네요\r\n요새 햇빛도 뜨거워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008274.1.7",
          "form": "집은 더워서 에어컨켰거든요",
          "original_form": "집은 더워서 에어컨켰거든요",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008274.1.8",
          "form": "밖이 더 시원할려나",
          "original_form": "밖이 더 시원할려나",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008274.1.9",
          "form": "그럴수도 있어요 저는 바닷가살아서 좀시원한편이긴해요 ㅅㄴ",
          "original_form": "그럴수도 있어요 저는 바닷가살아서 좀시원한편이긴해요 ㅅㄴ",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008274.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008274.1.11",
          "form": "오 바닷가근처시군요ㅎ 그럼 시원하시겠어요",
          "original_form": "오 바닷가근처시군요ㅎ 그럼 시원하시겠어요",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008274.1.12",
          "form": "남부지방은 비피해좀 있던데",
          "original_form": "남부지방은 비피해좀 있던데",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008274.1.13",
          "form": "어제 버스타고 가는데",
          "original_form": "어제 버스타고 가는데",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008274.1.14",
          "form": "침수된지역이있어서",
          "original_form": "침수된지역이있어서",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008274.1.15",
          "form": "차가 막힌정도?는 있었어요",
          "original_form": "차가 막힌정도?는 있었어요",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008274.1.16",
          "form": "아 아랫쪽이세요?",
          "original_form": "아 아랫쪽이세요?",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008274.1.17",
          "form": "태풍온다더라구요",
          "original_form": "태풍온다더라구요",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008274.1.18",
          "form": "어휴 어제 진짜 최악이었어요",
          "original_form": "어휴 어제 진짜 최악이었어요",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008274.1.19",
          "form": "비가 옆으로와서",
          "original_form": "비가 옆으로와서",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008274.1.20",
          "form": "아 많이 왔어요? 폭우였나요?",
          "original_form": "아 많이 왔어요? 폭우였나요?",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008274.1.21",
          "form": "폭우였던거같아요",
          "original_form": "폭우였던거같아요",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008274.1.22",
          "form": "와 비가 한꺼번에 이렇게 많이오다니 불합리해 란 생각이들었다는",
          "original_form": "와 비가 한꺼번에 이렇게 많이오다니 불합리해 란 생각이들었다는",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008274.1.23",
          "form": "빨리 비그치고 가을왔음 좋겠어요",
          "original_form": "빨리 비그치고 가을왔음 좋겠어요",
          "speaker_id": "1",
