      "utterance": [
        {
          "id": "MDRW2100025322.1.1",
          "form": "아침식사 하셨나요?",
          "original_form": "아침식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025322.1.2",
          "form": "네 요거트에 이것저것 넣어 먹었어요",
          "original_form": "네 요거트에 이것저것 넣어 먹었어요",
          "speaker_id": "2",
          "time": "20210901 10:11"
        },
        {
          "id": "MDRW2100025322.1.3",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025322.1.4",
          "form": "오 저도 요거트에 과일이랑 견과류 넣어서 먹었어요!",
          "original_form": "오 저도 요거트에 과일이랑 견과류 넣어서 먹었어요!",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025322.1.5",
          "form": "어떤것 넣어드세요?",
          "original_form": "어떤것 넣어드세요?",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025322.1.6",
          "form": "과일 채서 견과류요.. 비슷하네요..ㅎㅎ",
          "original_form": "과일 채서 견과류요.. 비슷하네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100025322.1.7",
          "form": "저녁은 맛있는것 드셨어요?",
          "original_form": "저녁은 맛있는것 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100025322.1.8",
          "form": "저녁은 소불고기 먹었어요",
          "original_form": "저녁은 소불고기 먹었어요",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100025322.1.9",
          "form": "어제 재료 사놓은게 있어서요",
          "original_form": "어제 재료 사놓은게 있어서요",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100025322.1.10",
          "form": "우와 맛있었을것 같아요~",
          "original_form": "우와 맛있었을것 같아요~",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100025322.1.11",
          "form": "저는 카레먹었어요",
          "original_form": "저는 카레먹었어요",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100025322.1.12",
          "form": "카레 맛있죠..ㅎㅎ",
          "original_form": "카레 맛있죠..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100025322.1.13",
          "form": "카레에 당근 넣는거 좋아하시나요?",
          "original_form": "카레에 당근 넣는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100025322.1.14",
          "form": "저는 좋아요.. 저희 식구들은 당근 잘 먹어요",
          "original_form": "저는 좋아요.. 저희 식구들은 당근 잘 먹어요",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100025322.1.15",
          "form": "아항 저는 엄마가 넣으셔서 그냥 먹어요 ㅋㅋㅋ",
          "original_form": "아항 저는 엄마가 넣으셔서 그냥 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100025322.1.16",
          "form": "원래는 고기를 제일 좋아해요 ㅋㅋ",
          "original_form": "원래는 고기를 제일 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100025322.1.17",
          "form": "카레에 돼지고기 넣으면 정말 맛있죠",
          "original_form": "카레에 돼지고기 넣으면 정말 맛있죠",
          "speaker_id": "2",
          "time": "20210902 06:14"
        },
        {
          "id": "MDRW2100025322.1.18",
          "form": "카레에 참치 넣기도 했어요.. 각자의 맛이 있더군요",
          "original_form": "카레에 참치 넣기도 했어요.. 각자의 맛이 있더군요",
          "speaker_id": "2",
          "time": "20210902 06:14"
        },
        {
          "id": "MDRW2100025322.1.19",
          "form": "우와 참치넣는거 신기해요",
          "original_form": "우와 참치넣는거 신기해요",
          "speaker_id": "1",
          "time": "20210902 08:13"
        },
        {
          "id": "MDRW2100025322.1.20",
          "form": "맛이깊어질것같네요!",
          "original_form": "맛이깊어질것같네요!",
          "speaker_id": "1",
          "time": "20210902 08:13"
        },
        {
          "id": "MDRW2100025322.1.21",
          "form": "참치카레 안먹어봤나요? 한번 도전해봐요",
          "original_form": "참치카레 안먹어봤나요? 한번 도전해봐요",
          "speaker_id": "2",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100025322.1.22",
          "form": "저는 돼지고기 넣는게 더 맛은 있었지만",
          "original_form": "저는 돼지고기 넣는게 더 맛은 있었지만",
          "speaker_id": "2",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100025322.1.23",
          "form": "참치도 꽤 괜찮아요",
          "original_form": "참치도 꽤 괜찮아요",
          "speaker_id": "2",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100025322.1.24",
          "form": "참치카레 도전해볼게요!!",
          "original_form": "참치카레 도전해볼게요!!",
          "speaker_id": "1",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100025322.1.25",
          "form": "참치가 정말 은근 여기저기 많이 들어가네요",
          "original_form": "참치가 정말 은근 여기저기 많이 들어가네요",
