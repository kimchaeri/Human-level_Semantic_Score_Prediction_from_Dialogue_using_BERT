      "utterance": [
        {
          "id": "MDRW2100045176.1.1",
          "form": "쿠키런 하시는 걸로 아는데 아직도 하세요?",
          "original_form": "쿠키런 하시는 걸로 아는데 아직도 하세요?",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045176.1.2",
          "form": "쿠키런 재밌어요 ! 하실래요 ?",
          "original_form": "쿠키런 재밌어요 ! 하실래요 ?",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045176.1.3",
          "form": "옛날엔 했는데",
          "original_form": "옛날엔 했는데",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045176.1.4",
          "form": "지금은 별로 안 끌려요",
          "original_form": "지금은 별로 안 끌려요",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045176.1.5",
          "form": "스토리 어디까지 나왔어요?",
          "original_form": "스토리 어디까지 나왔어요?",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045176.1.6",
          "form": "이번에 나올 새로운 용의 스토리까지요 !",
          "original_form": "이번에 나올 새로운 용의 스토리까지요 !",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045176.1.7",
          "form": "아 용쿠키 새로 나와요?",
          "original_form": "아 용쿠키 새로 나와요?",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045176.1.8",
          "form": "무슨 맛 쿠키래여?",
          "original_form": "무슨 맛 쿠키래여?",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045176.1.9",
          "form": "백련이요 ! 백련 먹을 수 있는건가요 ?",
          "original_form": "백련이요 ! 백련 먹을 수 있는건가요 ?",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045176.1.10",
          "form": "먹고 안 죽으면 다 먹을 수 있을 것 같아요",
          "original_form": "먹고 안 죽으면 다 먹을 수 있을 것 같아요",
          "speaker_id": "1",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045176.1.11",
          "form": "그런데 쿠키런 플레이 하면서",
          "original_form": "그런데 쿠키런 플레이 하면서",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045176.1.12",
          "form": "쿠키를 먹을 생각을 해요?",
          "original_form": "쿠키를 먹을 생각을 해요?",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045176.1.13",
          "form": "다들 그러지 않나요 ? 무슨 맛일지 궁금하잖아요",
          "original_form": "다들 그러지 않나요 ? 무슨 맛일지 궁금하잖아요",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045176.1.14",
          "form": "애들이 살고 싶어서 도망치는걸 보면서",
          "original_form": "애들이 살고 싶어서 도망치는걸 보면서",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045176.1.15",
          "form": "그런생각을 하셨어요?",
          "original_form": "그런생각을 하셨어요?",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045176.1.16",
          "form": "쿠키런 유저들은 원래 이렇게 잔인해여?",
          "original_form": "쿠키런 유저들은 원래 이렇게 잔인해여?",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045176.1.17",
          "form": "글을 두번씩 쓰실 정도로 놀랄 일이신가요 ?",
          "original_form": "글을 두번씩 쓰실 정도로 놀랄 일이신가요 ?",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045176.1.18",
          "form": "흠..name1님은 무슨 게잉 하시는데요 ?",
          "original_form": "흠..&name1&님은 무슨 게잉 하시는데요 ?",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045176.1.19",
          "form": "아 오류로 두번 보내진 모양이예요",
          "original_form": "아 오류로 두번 보내진 모양이예요",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045176.1.20",
          "form": "저는 라이프애프터를 하는데",
          "original_form": "저는 라이프애프터를 하는데",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045176.1.21",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045176.1.22",
          "form": "와 사후세계를 다룬 게임인가요 ?",
          "original_form": "와 사후세계를 다룬 게임인가요 ?",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045176.1.23",
          "form": "네 저랑 같이 죽어서",
          "original_form": "네 저랑 같이 죽어서",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045176.1.24",
          "form": "이게임을 해보시죠",
          "original_form": "이게임을 해보시죠",
          "speaker_id": "1",
          "time": "20210908 01:18"
        }
      ]
