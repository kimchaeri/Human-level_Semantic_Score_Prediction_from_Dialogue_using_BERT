      "utterance": [
        {
          "id": "MDRW2100032689.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100032689.1.2",
          "form": "저녁드셨나요",
          "original_form": "저녁드셨나요",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100032689.1.3",
          "form": "네 먹었어요!",
          "original_form": "네 먹었어요!",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032689.1.4",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032689.1.5",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032689.1.6",
          "form": "뮈드셨어요?!",
          "original_form": "뮈드셨어요?!",
          "speaker_id": "1",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032689.1.7",
          "form": "전 순대국밥 밀키트 먹었어요!",
          "original_form": "전 순대국밥 밀키트 먹었어요!",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032689.1.8",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032689.1.9",
          "form": "밀키트가 정말 잘나오네요",
          "original_form": "밀키트가 정말 잘나오네요",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032689.1.10",
          "form": "네ㅋㅋㅋ 엄청 간단해요 그냥 재료 넣고 3분정도 끓이니까 완성됐어요",
          "original_form": "네ㅋㅋㅋ 엄청 간단해요 그냥 재료 넣고 3분정도 끓이니까 완성됐어요",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032689.1.11",
          "form": "맛도좋나요?!ㅋㅋ",
          "original_form": "맛도좋나요?!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032689.1.12",
          "form": "네 저한텐 가게랑 별 차이 없는 느낌이에요!",
          "original_form": "네 저한텐 가게랑 별 차이 없는 느낌이에요!",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.13",
          "form": "오 저도다음에",
          "original_form": "오 저도다음에",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.14",
          "form": "밀키트 드셔본거 있으신가요?",
          "original_form": "밀키트 드셔본거 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.15",
          "form": "사봐야겠어요",
          "original_form": "사봐야겠어요",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.17",
          "form": "저는 밀푀유나베!",
          "original_form": "저는 밀푀유나베!",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.18",
          "form": "밀키트사먹어봤어요",
          "original_form": "밀키트사먹어봤어요",
          "speaker_id": "1",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.19",
          "form": "아아 그건 못먹어봤는데 맛있으셨나요?",
          "original_form": "아아 그건 못먹어봤는데 맛있으셨나요?",
          "speaker_id": "2",
          "time": "20210905 22:26"
        },
        {
          "id": "MDRW2100032689.1.20",
          "form": "양념은 맛있는데",
          "original_form": "양념은 맛있는데",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100032689.1.21",
          "form": "배추가 엄청두꺼웠어요ㅠ",
          "original_form": "배추가 엄청두꺼웠어요ㅠ",
