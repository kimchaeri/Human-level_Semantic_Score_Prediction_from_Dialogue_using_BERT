      "utterance": [
        {
          "id": "MDRW2100022511.1.1",
          "form": "name1님 학교생활은 어땠나욬ㅋㅋ",
          "original_form": "&name1&님 학교생활은 어땠나욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100022511.1.2",
          "form": "전 너무 머나먼일같아서 기억도 안나네요ㅜ",
          "original_form": "전 너무 머나먼일같아서 기억도 안나네요ㅜ",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100022511.1.3",
          "form": "그냥 두리뭉실하게 즐거웠던것만...ㅎㅎㅎ",
          "original_form": "그냥 두리뭉실하게 즐거웠던것만...ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100022511.1.4",
          "form": "저도요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:04"
        },
        {
          "id": "MDRW2100022511.1.5",
          "form": "졸업한지가 까마득이네요ㅠㅠ",
          "original_form": "졸업한지가 까마득이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:05"
        },
        {
          "id": "MDRW2100022511.1.6",
          "form": "ㅋㅋ 그르게요 요즘학번보면",
          "original_form": "ㅋㅋ 그르게요 요즘학번보면",
          "speaker_id": "2",
          "time": "20210830 00:05"
        },
        {
          "id": "MDRW2100022511.1.7",
          "form": "엄청놀란답니닼ㅋ",
          "original_form": "엄청놀란답니닼ㅋ",
          "speaker_id": "2",
          "time": "20210830 00:05"
        },
        {
          "id": "MDRW2100022511.1.8",
          "form": "애들은 다 잠들엇나여 ~",
          "original_form": "애들은 다 잠들엇나여 ~",
          "speaker_id": "1",
          "time": "20210830 00:05"
        },
        {
          "id": "MDRW2100022511.1.9",
          "form": "작년부터 학교다니학생들이 너무 안되었어요ㅠㅠ",
          "original_form": "작년부터 학교다니학생들이 너무 안되었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.10",
          "form": "그나이때 누릴생활을 제대로못하니ㅠ",
          "original_form": "그나이때 누릴생활을 제대로못하니ㅠ",
          "speaker_id": "1",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.11",
          "form": "네네 잘자고 있네요 ㅋㅋ",
          "original_form": "네네 잘자고 있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.12",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.13",
          "form": "안타까워여ㅠ",
          "original_form": "안타까워여ㅠ",
          "speaker_id": "1",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.14",
          "form": "그래서 올해 초등학교 1학년이 두학년이라고 말하더라구요",
          "original_form": "그래서 올해 초등학교 1학년이 두학년이라고 말하더라구요",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.15",
          "form": "초2도 작년에 거의 학교를 못가서.ㅠ",
          "original_form": "초2도 작년에 거의 학교를 못가서.ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.16",
          "form": "진짜 모든 생활이 망가졋으니ㅠ",
          "original_form": "진짜 모든 생활이 망가졋으니ㅠ",
          "speaker_id": "1",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100022511.1.17",
          "form": "그래도 2학기부터는",
          "original_form": "그래도 2학기부터는",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.18",
          "form": "전학년 다 쭈욱 등교라서",
          "original_form": "전학년 다 쭈욱 등교라서",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.19",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.20",
          "form": "예전보단 나아보였어요 코로나가 무섭긴하지만요ㅠㅠ",
          "original_form": "예전보단 나아보였어요 코로나가 무섭긴하지만요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.21",
          "form": "저희지역은 요새많이나와서",
          "original_form": "저희지역은 요새많이나와서",
          "speaker_id": "1",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.22",
          "form": "꺼려하시는뷴들도 많아요 진짜 등원을 시켜야사는디 등교도",
          "original_form": "꺼려하시는뷴들도 많아요 진짜 등원을 시켜야사는디 등교도",
          "speaker_id": "1",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.23",
          "form": "그렇게되면 저도 너무 무서울거같아요",
          "original_form": "그렇게되면 저도 너무 무서울거같아요",
          "speaker_id": "2",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100022511.1.24",
          "form": "어린이집 유치원은 안보내면 그만인데..",
          "original_form": "어린이집 유치원은 안보내면 그만인데..",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100022511.1.25",
          "form": "학교는 또 달라서",
          "original_form": "학교는 또 달라서",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100022511.1.26",
          "form": "맞아요 출석일수가 중요하니",
          "original_form": "맞아요 출석일수가 중요하니",
          "speaker_id": "1",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100022511.1.27",
          "form": "요즘학교 생활은",
          "original_form": "요즘학교 생활은",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100022511.1.28",
          "form": "코로나때문에 많이바뀐거같더라구요ㅠ",
          "original_form": "코로나때문에 많이바뀐거같더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100022511.1.29",
          "form": "마스크쓰고 친구들이랑도 제대로 못어울리고..",
          "original_form": "마스크쓰고 친구들이랑도 제대로 못어울리고..",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100022511.1.30",
          "form": "그쵸ㅠㅠ 어디가는것도 제약이많으니",
          "original_form": "그쵸ㅠㅠ 어디가는것도 제약이많으니",
          "speaker_id": "1",
          "time": "20210831 18:51"
        }
      ]
