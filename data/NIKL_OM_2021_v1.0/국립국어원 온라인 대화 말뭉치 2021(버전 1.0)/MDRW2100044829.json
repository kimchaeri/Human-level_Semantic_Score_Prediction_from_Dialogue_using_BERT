      "utterance": [
        {
          "id": "MDRW2100044829.1.1",
          "form": "그쵸 ㅋㅋ몸에 안좋은 거 같기두 하구요",
          "original_form": "그쵸 ㅋㅋ몸에 안좋은 거 같기두 하구요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044829.1.2",
          "form": "주말에 김밥 말아서 먹을려구용 ㅎㅎ",
          "original_form": "주말에 김밥 말아서 먹을려구용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044829.1.3",
          "form": "와 김밥도 해드시다니 대단한데요 ㄷㄷㄷ",
          "original_form": "와 김밥도 해드시다니 대단한데요 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044829.1.4",
          "form": "재료는 엄마가 해주시고 저는 마는건만 해보게요..!!!",
          "original_form": "재료는 엄마가 해주시고 저는 마는건만 해보게요..!!!",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044829.1.5",
          "form": "ㅋㅋㅋㅋ 언니가 소풍 간다해서..",
          "original_form": "ㅋㅋㅋㅋ 언니가 소풍 간다해서..",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044829.1.6",
          "form": "앗 ㅋㅋㅋ엄마가 해주는 김밥 맛잇겟어요",
          "original_form": "앗 ㅋㅋㅋ엄마가 해주는 김밥 맛잇겟어요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044829.1.7",
          "form": "김밥 잘 마세요??",
          "original_form": "김밥 잘 마세요??",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044829.1.8",
          "form": "아니요 ㅋㅋㅋ 잘 마시나요?",
          "original_form": "아니요 ㅋㅋㅋ 잘 마시나요?",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044829.1.9",
          "form": "아눀ㅋㅋ 항상 옆구리가 터져욬ㅋㅋㅋ",
          "original_form": "아눀ㅋㅋ 항상 옆구리가 터져욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044829.1.10",
          "form": "그래도 맛은 있드라구요",
          "original_form": "그래도 맛은 있드라구요",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044829.1.11",
          "form": "ㅎㅎ 김밥 튀김 아세요?",
          "original_form": "ㅎㅎ 김밥 튀김 아세요?",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044829.1.12",
          "form": "오오오 저 들어봑어요!!! ㅋㅋㅋ",
          "original_form": "오오오 저 들어봑어요!!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044829.1.13",
          "form": "계란 묻혀서 전처럼도 하드라구요!!",
          "original_form": "계란 묻혀서 전처럼도 하드라구요!!",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044829.1.14",
          "form": "ㅋㅋ주말에 한번 해보세여",
          "original_form": "ㅋㅋ주말에 한번 해보세여",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044829.1.15",
          "form": "전 맛잇고 좋앗어요",
          "original_form": "전 맛잇고 좋앗어요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044829.1.16",
          "form": "오오 좋아요!! 완전 튀기는건가요??",
          "original_form": "오오 좋아요!! 완전 튀기는건가요??",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044829.1.17",
          "form": "네네 제가 먹은건 반잘라서",
          "original_form": "네네 제가 먹은건 반잘라서",
