      "utterance": [
        {
          "id": "MDRW2100017588.1.1",
          "form": "저번에 편의점에서 웃긴거 봤다",
          "original_form": "저번에 편의점에서 웃긴거 봤다",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.2",
          "form": "뭔데요?",
          "original_form": "뭔데요?",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.3",
          "form": "곰표 맥주",
          "original_form": "곰표 맥주",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.4",
          "form": "아~ 그거요",
          "original_form": "아~ 그거요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.5",
          "form": "넌 알고 있었니??",
          "original_form": "넌 알고 있었니??",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.6",
          "form": "요즘 트렌드잖아요",
          "original_form": "요즘 트렌드잖아요",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.7",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017588.1.8",
          "form": "네 원래 곰표하면 밀가루인데 그 모양으로 맥주도 내고 또 옷도 냈어요",
          "original_form": "네 원래 곰표하면 밀가루인데 그 모양으로 맥주도 내고 또 옷도 냈어요",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017588.1.9",
          "form": "곰표 디자인으로옷에 넣어서 팔아요 나름 잘 팔린대요",
          "original_form": "곰표 디자인으로옷에 넣어서 팔아요 나름 잘 팔린대요",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017588.1.10",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017588.1.11",
          "form": "나중에 보여줘봐",
          "original_form": "나중에 보여줘봐",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017588.1.12",
          "form": "네 나름/디자인도 괜찮아요",
          "original_form": "네 나름/디자인도 괜찮아요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017588.1.13",
          "form": "근데 문제점도 있어요",
          "original_form": "근데 문제점도 있어요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017588.1.14",
          "form": "어떤거??",
          "original_form": "어떤거??",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017588.1.15",
          "form": "구두약 브랜드가 먹는 거람 콜라보 하고요",
          "original_form": "구두약 브랜드가 먹는 거람 콜라보 하고요",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017588.1.16",
          "form": "그럼 아기들이 착각하고 구두약도 먹는거 아니야?",
          "original_form": "그럼 아기들이 착각하고 구두약도 먹는거 아니야?",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017588.1.17",
          "form": "그니까요 그래서 위험하니까 안좋죠",
          "original_form": "그니까요 그래서 위험하니까 안좋죠",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017588.1.18",
          "form": "그런 점도 있네",
          "original_form": "그런 점도 있네",
          "speaker_id": "1",
          "time": "20210826 20:17"
        }
      ]
