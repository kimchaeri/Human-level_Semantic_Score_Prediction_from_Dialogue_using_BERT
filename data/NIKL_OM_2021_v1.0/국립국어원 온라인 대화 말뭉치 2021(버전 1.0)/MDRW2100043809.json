      "utterance": [
        {
          "id": "MDRW2100043809.1.1",
          "form": "무슨 굿윌헌팅 이런데 나오는 분 같아요",
          "original_form": "무슨 굿윌헌팅 이런데 나오는 분 같아요",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043809.1.2",
          "form": "ㅋㅋㅋ뭐 그것말고도",
          "original_form": "ㅋㅋㅋ뭐 그것말고도",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043809.1.3",
          "form": "한과목만 가르치시는 게 아니었을텐데 120명 대형강에서 매주퀴즈+피드백..?",
          "original_form": "한과목만 가르치시는 게 아니었을텐데 120명 대형강에서 매주퀴즈+피드백..?",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043809.1.4",
          "form": "음주가무법이나",
          "original_form": "음주가무법이나",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043809.1.5",
          "form": "노웨이입니다",
          "original_form": "노웨이입니다",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043809.1.6",
          "form": "이런것도 가르쳐주시고",
          "original_form": "이런것도 가르쳐주시고",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043809.1.7",
          "form": "ㅋㅋㅋ뭘 가르쳐주셨나요",
          "original_form": "ㅋㅋㅋ뭘 가르쳐주셨나요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043809.1.8",
          "form": "소맥 황금비율같은..",
          "original_form": "소맥 황금비율같은..",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043809.1.9",
          "form": "학생들과 한잔하러 가시구 하셨나요",
          "original_form": "학생들과 한잔하러 가시구 하셨나요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043809.1.10",
          "form": "네ㅋㅋ 가끔요ㅋㅋ",
          "original_form": "네ㅋㅋ 가끔요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043809.1.11",
          "form": "소주잔을 겹쳐서 만드는 소맥법",
          "original_form": "소주잔을 겹쳐서 만드는 소맥법",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043809.1.12",
          "form": "소맥 거품내기",
          "original_form": "소맥 거품내기",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043809.1.13",
          "form": "등등..",
          "original_form": "등등..",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043809.1.14",
          "form": "아니 무슨",
          "original_form": "아니 무슨",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043809.1.15",
          "form": "생활지식 전수 시간이었네요 ㅋㅋㅋ",
          "original_form": "생활지식 전수 시간이었네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043809.1.16",
          "form": "소맥황금비율 저두 알려주세요",
          "original_form": "소맥황금비율 저두 알려주세요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043809.1.17",
          "form": "ㅋㅋ네 또 대학원가기 직전에 제가 안가고 딴거하겠다고 땡강부렸는데 얄짤없이 한방에 꺾였습니다",
          "original_form": "ㅋㅋ네 또 대학원가기 직전에 제가 안가고 딴거하겠다고 땡강부렸는데 얄짤없이 한방에 꺾였습니다",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043809.1.18",
          "form": "역시 그분 랩으로 가셨나요",
          "original_form": "역시 그분 랩으로 가셨나요",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043809.1.19",
          "form": "네 소주잔 겹쳐서 소주는 겹치는 부분까지+맥주는 소주 한잔가득 넣으면 존맛입니다",
          "original_form": "네 소주잔 겹쳐서 소주는 겹치는 부분까지+맥주는 소주 한잔가득 넣으면 존맛입니다",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043809.1.20",
          "form": "아녀 저는 다른 랩갔어요ㅋㅋ호호할아부지 교수님이 계신곳으로갔습니다",
          "original_form": "아녀 저는 다른 랩갔어요ㅋㅋ호호할아부지 교수님이 계신곳으로갔습니다",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043809.1.21",
          "form": "ㅋㅋㅋㅋ감사합니다 다음번 소맥마실 회식자리 생기면 활용할게요..",
          "original_form": "ㅋㅋㅋㅋ감사합니다 다음번 소맥마실 회식자리 생기면 활용할게요..",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043809.1.22",
          "form": "아앗 젊은교수님 학생 뺏기셨네요(?)",
          "original_form": "아앗 젊은교수님 학생 뺏기셨네요(?)",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043809.1.23",
          "form": "왠지 스토리상 젊은 교수님이 대학원까지 이끌어가신 것 같았는데..",
          "original_form": "왠지 스토리상 젊은 교수님이 대학원까지 이끌어가신 것 같았는데..",
          "speaker_id": "2",
          "time": "20210907 20:32"
        }
      ]
