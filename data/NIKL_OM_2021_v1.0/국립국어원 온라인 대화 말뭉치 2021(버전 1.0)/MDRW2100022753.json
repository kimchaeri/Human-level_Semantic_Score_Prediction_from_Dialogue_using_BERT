      "utterance": [
        {
          "id": "MDRW2100022753.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022753.1.2",
          "form": "가끔 당근을 써요",
          "original_form": "가끔 당근을 써요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022753.1.3",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022753.1.4",
          "form": "최근에 판매하신거있나요",
          "original_form": "최근에 판매하신거있나요",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022753.1.5",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022753.1.6",
          "form": "들고 가기 힘드셨을거 같아요",
          "original_form": "들고 가기 힘드셨을거 같아요",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022753.1.7",
          "form": "습도를 위해 ㅎㅎ",
          "original_form": "습도를 위해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022753.1.8",
          "form": "저는 최근에 이어폰을 팔았어요",
          "original_form": "저는 최근에 이어폰을 팔았어요",
          "speaker_id": "2",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022753.1.9",
          "form": "많이 받았나여?",
          "original_form": "많이 받았나여?",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022753.1.10",
          "form": "음 ㅎㅎ빨리 팔려고 시세보단 싸게 판 것 같아요",
          "original_form": "음 ㅎㅎ빨리 팔려고 시세보단 싸게 판 것 같아요",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022753.1.11",
          "form": "그개 정답이라는",
          "original_form": "그개 정답이라는",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022753.1.12",
          "form": "중고거래는 좋긴한데 귀찮은점이 많기도해요",
          "original_form": "중고거래는 좋긴한데 귀찮은점이 많기도해요",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022753.1.13",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022753.1.14",
          "form": "산다고 하시고 연락을 끊는경우도 많아서요",
          "original_form": "산다고 하시고 연락을 끊는경우도 많아서요",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022753.1.15",
          "form": "맞아요 잠수",
          "original_form": "맞아요 잠수",
          "speaker_id": "1",
