      "utterance": [
        {
          "id": "MDRW2100037144.1.1",
          "form": "코로나때매 밖에 안 나가서 살찐 사람들이 많다던데 괜찮으세요?",
          "original_form": "코로나때매 밖에 안 나가서 살찐 사람들이 많다던데 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100037144.1.2",
          "form": "아아 저도 코로나 때문에 운동량이 줄긴 했어요",
          "original_form": "아아 저도 코로나 때문에 운동량이 줄긴 했어요",
          "speaker_id": "2",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100037144.1.3",
          "form": "근데 올해는 주3회 운동하려고 노력중입니다~",
          "original_form": "근데 올해는 주3회 운동하려고 노력중입니다~",
          "speaker_id": "2",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100037144.1.4",
          "form": "무슨 운동 주로 하시나요? 저는 수영했었어요",
          "original_form": "무슨 운동 주로 하시나요? 저는 수영했었어요",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037144.1.5",
          "form": "저도 수영했었는데",
          "original_form": "저도 수영했었는데",
          "speaker_id": "2",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037144.1.6",
          "form": "공립이라 계속 시설이용 중단이 돼서",
          "original_form": "공립이라 계속 시설이용 중단이 돼서",
          "speaker_id": "2",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037144.1.7",
          "form": "요즘은 필라테스 해요",
          "original_form": "요즘은 필라테스 해요",
          "speaker_id": "2",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037144.1.8",
          "form": "저희 동네에도 엄청 오랫동안 문 닫고 하더라고요",
          "original_form": "저희 동네에도 엄청 오랫동안 문 닫고 하더라고요",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037144.1.9",
          "form": "체셩 교정도 할 겸",
          "original_form": "체셩 교정도 할 겸",
          "speaker_id": "2",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037144.1.10",
          "form": "체형",
          "original_form": "체형",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037144.1.11",
          "form": "필라테스 어때요 어렵진 않나요?",
          "original_form": "필라테스 어때요 어렵진 않나요?",
          "speaker_id": "1",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037144.1.12",
          "form": "하다보니 익숙해지는 것 같아요",
          "original_form": "하다보니 익숙해지는 것 같아요",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037144.1.13",
          "form": "기구 필라테스라 기구 도움도 받고요",
          "original_form": "기구 필라테스라 기구 도움도 받고요",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037144.1.14",
          "form": "전 헬스랑 그런 필라테스 도굴 보면  무섭다는 생각이 들어요",
          "original_form": "전 헬스랑 그런 필라테스 도굴 보면  무섭다는 생각이 들어요",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037144.1.15",
          "form": "ㅎㅎ저도 그랬는데",
          "original_form": "ㅎㅎ저도 그랬는데",
          "speaker_id": "2",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037144.1.16",
          "form": "하다보니 좀 적응되더라고요",
          "original_form": "하다보니 좀 적응되더라고요",
          "speaker_id": "2",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037144.1.17",
          "form": "그렇구나 저도 한번 해 보고 싶긴 해요",
          "original_form": "그렇구나 저도 한번 해 보고 싶긴 해요",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100037144.1.18",
          "form": "네 필라테스 해보니 안하면 찌뿌둥하더라고요",
          "original_form": "네 필라테스 해보니 안하면 찌뿌둥하더라고요",
          "speaker_id": "2",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100037144.1.19",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
