      "utterance": [
        {
          "id": "MDRW2100039655.1.1",
          "form": "요즘에 뭐 쇼핑했어?",
          "original_form": "요즘에 뭐 쇼핑했어?",
          "speaker_id": "1",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039655.1.2",
          "form": "뭐샀더라..",
          "original_form": "뭐샀더라..",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039655.1.3",
          "form": "제일 최근거는 키링",
          "original_form": "제일 최근거는 키링",
          "speaker_id": "2",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100039655.1.4",
          "form": "키링 이뻤겠다 나도 키링샀어",
          "original_form": "키링 이뻤겠다 나도 키링샀어",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039655.1.5",
          "form": "또 무료이벤트했으면 좋겠어",
          "original_form": "또 무료이벤트했으면 좋겠어",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039655.1.6",
          "form": "나도 또 이벤트로 사고싶어",
          "original_form": "나도 또 이벤트로 사고싶어",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039655.1.7",
          "form": "이번에 온게 너무 귀여워서!",
          "original_form": "이번에 온게 너무 귀여워서!",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039655.1.8",
          "form": "얼마나 귀여운지 보고싶다!",
          "original_form": "얼마나 귀여운지 보고싶다!",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039655.1.9",
          "form": "요즘 포토카드뽑는것도 유행이더라",
          "original_form": "요즘 포토카드뽑는것도 유행이더라",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039655.1.10",
          "form": "아 포토카드도 뽑았는데",
          "original_form": "아 포토카드도 뽑았는데",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039655.1.11",
          "form": "이건 실사로 보여주고싶으니까 다음기회에~",
          "original_form": "이건 실사로 보여주고싶으니까 다음기회에~",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039655.1.12",
          "form": "얼마나 이쁠까ㅠㅠ 기대된다",
          "original_form": "얼마나 이쁠까ㅠㅠ 기대된다",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039655.1.13",
          "form": "난 모자 사야 돼 내 모자 대학생때 샀어",
          "original_form": "난 모자 사야 돼 내 모자 대학생때 샀어",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039655.1.14",
          "form": "오래썼네!",
          "original_form": "오래썼네!",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039655.1.15",
          "form": "난..",
          "original_form": "난..",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039655.1.16",
          "form": "그치! 이번 생일선물로 받을거야!",
          "original_form": "그치! 이번 생일선물로 받을거야!",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039655.1.17",
          "form": "생각난김에 포토카드 더 주문해야겠어",
          "original_form": "생각난김에 포토카드 더 주문해야겠어",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039655.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039655.1.19",
          "form": "멋져",
          "original_form": "멋져",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039655.1.20",
          "form": "좋은생각이야",
          "original_form": "좋은생각이야",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039655.1.21",
          "form": "그치? 친구들이 더 나보다 이쁜거 골라줄듯",
          "original_form": "그치? 친구들이 더 나보다 이쁜거 골라줄듯",
          "speaker_id": "1",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039655.1.22",
          "form": "그래도 선호하는 모자가 있지않아?",
          "original_form": "그래도 선호하는 모자가 있지않아?",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100039655.1.23",
          "form": "검은색모자면 다좋아!",
          "original_form": "검은색모자면 다좋아!",
          "speaker_id": "1",
          "time": "20210903 20:37"
        }
      ]
    }
