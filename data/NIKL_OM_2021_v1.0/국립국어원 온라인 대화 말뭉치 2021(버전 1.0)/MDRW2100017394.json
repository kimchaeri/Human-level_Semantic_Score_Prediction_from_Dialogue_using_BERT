      "utterance": [
        {
          "id": "MDRW2100017394.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017394.1.2",
          "form": "아니요 아직 안먹었어요",
          "original_form": "아니요 아직 안먹었어요",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017394.1.3",
          "form": "저도요ㅠ 뭐 드실예정이예요?",
          "original_form": "저도요ㅠ 뭐 드실예정이예요?",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017394.1.4",
          "form": "전 참치마요비빔밥 해 먹으려구요",
          "original_form": "전 참치마요비빔밥 해 먹으려구요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017394.1.5",
          "form": "집에서요?",
          "original_form": "집에서요?",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017394.1.6",
          "form": "네네 배달시키고 남은 단무지가 처치곤란이라",
          "original_form": "네네 배달시키고 남은 단무지가 처치곤란이라",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017394.1.7",
          "form": "어떻게든 해결해보려구욯ㅎㅎ",
          "original_form": "어떻게든 해결해보려구욯ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017394.1.8",
          "form": "오~어떻게 만들어요?",
          "original_form": "오~어떻게 만들어요?",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017394.1.9",
          "form": "그냥 초간단버전이라 단무지 잘게 썰고",
          "original_form": "그냥 초간단버전이라 단무지 잘게 썰고",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017394.1.10",
          "form": "참치캔에 마요네즈 섞어서 밥에 비비면 끝이에요ㅋㅋ",
          "original_form": "참치캔에 마요네즈 섞어서 밥에 비비면 끝이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017394.1.11",
          "form": "오 초간단이네요",
          "original_form": "오 초간단이네요",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017394.1.12",
          "form": "저녁 뭐 드세요?",
          "original_form": "저녁 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100017394.1.13",
          "form": "지금 너무 귀찮아서 배달시킬까 고민이에요",
          "original_form": "지금 너무 귀찮아서 배달시킬까 고민이에요",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017394.1.14",
          "form": "귀찮을 땐 배달이 최고죠",
          "original_form": "귀찮을 땐 배달이 최고죠",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017394.1.15",
          "form": "메뉴가 고민이네요",
          "original_form": "메뉴가 고민이네요",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017394.1.16",
          "form": "저녁시간 지나서 지금 주문하면 오래걸리진 않겠네요!",
          "original_form": "저녁시간 지나서 지금 주문하면 오래걸리진 않겠네요!",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017394.1.17",
          "form": "네~ 다행이에요^^",
          "original_form": "네~ 다행이에요^^",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017394.1.18",
          "form": "아직 메뉴 고민중이세요?",
          "original_form": "아직 메뉴 고민중이세요?",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017394.1.19",
          "form": "네 뭐 시킬지 보고 있어요",
          "original_form": "네 뭐 시킬지 보고 있어요",
          "speaker_id": "2",
          "time": "20210826 19:33"
        }
      ]
