      "utterance": [
        {
          "id": "MDRW2100019563.1.1",
          "form": "혹시 망원동 티라미슈 드셔보셨어요?",
          "original_form": "혹시 망원동 티라미슈 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019563.1.2",
          "form": "아뇨 처음 들어요 먹잘알이라면 섭렵해야하는 가게인가요?",
          "original_form": "아뇨 처음 들어요 먹잘알이라면 섭렵해야하는 가게인가요?",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019563.1.3",
          "form": "아뇨 그건아니고",
          "original_form": "아뇨 그건아니고",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019563.1.4",
          "form": "뮌가 새로생긴 프랜차이즈같아요",
          "original_form": "뮌가 새로생긴 프랜차이즈같아요",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019563.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019563.1.6",
          "form": "드셔보셨나해서요",
          "original_form": "드셔보셨나해서요",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019563.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019563.1.8",
          "form": "맛 궁금해서요",
          "original_form": "맛 궁금해서요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019563.1.9",
          "form": "근데 엄청 유명한 집 같아요",
          "original_form": "근데 엄청 유명한 집 같아요",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019563.1.10",
          "form": "망원동만 쳐도 검색어 다 뜨네요ㅋㅋㅋ",
          "original_form": "망원동만 쳐도 검색어 다 뜨네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019563.1.11",
          "form": "가게 이뻐여! 레트로풍",
          "original_form": "가게 이뻐여! 레트로풍",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019563.1.12",
          "form": "최근에 뭔가 많이 생겨서요",
          "original_form": "최근에 뭔가 많이 생겨서요",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019563.1.13",
          "form": "체인이",
          "original_form": "체인이",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019563.1.14",
          "form": "또 호기심 솟네요 ㅋㅋ",
          "original_form": "또 호기심 솟네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019563.1.15",
          "form": "아 그렇네요 잘 돼서 체인이 늘었나?",
          "original_form": "아 그렇네요 잘 돼서 체인이 늘었나?",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019563.1.16",
          "form": "저 약수점 지나다다 본 적 있는 것 같아요ㅋㅋㅋ",
          "original_form": "저 약수점 지나다다 본 적 있는 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019563.1.17",
          "form": "저는 요새 에그타르트 맛집 찾고있어요",
          "original_form": "저는 요새 에그타르트 맛집 찾고있어요",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019563.1.18",
          "form": "에그타르트라 ㅋㅋ",
          "original_form": "에그타르트라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019563.1.19",
          "form": "맛있는집 너무 많을거같아요 ㅋ",
          "original_form": "맛있는집 너무 많을거같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019563.1.20",
          "form": "에그타르트가 맛없기도 힘들듯 ㅋ",
          "original_form": "에그타르트가 맛없기도 힘들듯 ㅋ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019563.1.21",
          "form": "아ㅠㅠㅠㅠㅠ 그게 제가 찾는 에그타르트 맛집은요",
          "original_form": "아ㅠㅠㅠㅠㅠ 그게 제가 찾는 에그타르트 맛집은요",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019563.1.22",
          "form": "홍콩식 에그타르트집인데요",
          "original_form": "홍콩식 에그타르트집인데요",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019563.1.23",
          "form": "바닥이 패스츄리가 아니고 파이여야 하는데 이런 집은 좀 드문 것 같아요",
          "original_form": "바닥이 패스츄리가 아니고 파이여야 하는데 이런 집은 좀 드문 것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019563.1.24",
          "form": "아하 저는 그 홍콩식은 따로 안먹어본거같아요",
          "original_form": "아하 저는 그 홍콩식은 따로 안먹어본거같아요",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019563.1.25",
          "form": "빠리바게트 에그타르트가 홍콩식이에요",
          "original_form": "빠리바게트 에그타르트가 홍콩식이에요",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019563.1.26",
          "form": "그래서 제 안의 에그타르트 최고맛집은 빠바ㅋㅋㅋ",
          "original_form": "그래서 제 안의 에그타르트 최고맛집은 빠바ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:31"
        }
      ]
    }
