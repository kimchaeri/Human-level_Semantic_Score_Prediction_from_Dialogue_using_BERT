      "utterance": [
        {
          "id": "MDRW2100034867.1.1",
          "form": "과일 좋아하세요??",
          "original_form": "과일 좋아하세요??",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034867.1.2",
          "form": "과일 잘 안먹어요ㅋㅋㅋ",
          "original_form": "과일 잘 안먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034867.1.3",
          "form": "딸기정도?",
          "original_form": "딸기정도?",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034867.1.4",
          "form": "뭔가 다른 과일들은 따갑더라구여 알러지가 있나",
          "original_form": "뭔가 다른 과일들은 따갑더라구여 알러지가 있나",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034867.1.5",
          "form": "앗 그러시구나",
          "original_form": "앗 그러시구나",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034867.1.6",
          "form": "후식으로는 어떤거 많이드세요?",
          "original_form": "후식으로는 어떤거 많이드세요?",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034867.1.7",
          "form": "후식 케이크같은거요ㅋㅋㅋ",
          "original_form": "후식 케이크같은거요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034867.1.8",
          "form": "과일같은것도 좀 먹어야 건강한데 말이죠",
          "original_form": "과일같은것도 좀 먹어야 건강한데 말이죠",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034867.1.9",
          "form": "과일 좋아하시나봐요",
          "original_form": "과일 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034867.1.10",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100034867.1.11",
          "form": "어떤 과일 좋아해요",
          "original_form": "어떤 과일 좋아해요",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100034867.1.12",
          "form": "부모님이 좋아하셔서 그런지 계속 먹어서 일부분이 된 기분이예요",
          "original_form": "부모님이 좋아하셔서 그런지 계속 먹어서 일부분이 된 기분이예요",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.13",
          "form": "우와 건강한 집이네요ㅋㅋㅋ",
          "original_form": "우와 건강한 집이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.14",
          "form": "귤이나 오렌지요!",
          "original_form": "귤이나 오렌지요!",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.15",
          "form": "피부 엄청 좋으시겠다",
          "original_form": "피부 엄청 좋으시겠다",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.16",
          "form": "부모님 덕분에..!",
          "original_form": "부모님 덕분에..!",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.17",
          "form": "아쉽게도 별갠가봐요..ㅋㅋㅋ",
          "original_form": "아쉽게도 별갠가봐요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.18",
          "form": "전 무화과는 좀 좋아해서",
          "original_form": "전 무화과는 좀 좋아해서",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.19",
          "form": "이맘때쯤 무화과 나오면 엄청 먹어요",
          "original_form": "이맘때쯤 무화과 나오면 엄청 먹어요",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.20",
          "form": "과일도 잼도 디저트도ㅋㅋㅌ",
          "original_form": "과일도 잼도 디저트도ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034867.1.21",
          "form": "아 무화과잼 진짜 맛있더라구요",
          "original_form": "아 무화과잼 진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034867.1.22",
          "form": "타르트도요!!",
          "original_form": "타르트도요!!",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034867.1.23",
          "form": "네 ㅠㅠ 디저트 중에 젤 좋아여",
          "original_form": "네 ㅠㅠ 디저트 중에 젤 좋아여",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034867.1.24",
          "form": "디저트 이야기하니 급 땡기네요",
          "original_form": "디저트 이야기하니 급 땡기네요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100034867.1.25",
          "form": "그러게요 ㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:30"
        }
      ]
    }
