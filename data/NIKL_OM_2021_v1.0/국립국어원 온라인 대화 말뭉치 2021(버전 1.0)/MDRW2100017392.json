      "utterance": [
        {
          "id": "MDRW2100017392.1.1",
          "form": "청자켓 사야디 ㅋㅋ",
          "original_form": "청자켓 사야디 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017392.1.2",
          "form": "맘에드는거 찾았니ㅋㅋㅋ",
          "original_form": "맘에드는거 찾았니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017392.1.3",
          "form": "어차피 기본템 살거라 ㅋㅋ",
          "original_form": "어차피 기본템 살거라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017392.1.4",
          "form": "기본템이 은근 찾기 힘들다ㅋㅋ",
          "original_form": "기본템이 은근 찾기 힘들다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017392.1.5",
          "form": "너 폴로 스웨터도 보이즈 엑라입니?",
          "original_form": "너 폴로 스웨터도 보이즈 엑라입니?",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017392.1.6",
          "form": "타미힐피거꺼 하나 봐 놨당 ㅋㅋ",
          "original_form": "타미힐피거꺼 하나 봐 놨당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017392.1.7",
          "form": "보이즈는 다 엑라",
          "original_form": "보이즈는 다 엑라",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017392.1.8",
          "form": "아 반팔티는 라지다 ㅋㅋ",
          "original_form": "아 반팔티는 라지다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017392.1.9",
          "form": "니트는 너 한테 딱 맞아 보이던데 난 힘들지도ㅋㅋㅋ",
          "original_form": "니트는 너 한테 딱 맞아 보이던데 난 힘들지도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017392.1.10",
          "form": "니트도 어깨선은 완전 딱 안 맞아서 완전 핏되게 입을라면 라지도 가능하지 않을까 생각했음",
          "original_form": "니트도 어깨선은 완전 딱 안 맞아서 완전 핏되게 입을라면 라지도 가능하지 않을까 생각했음",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017392.1.11",
          "form": "아 진짜? 니트 너한테 여유있나? 너 입은거 자세히 보진 않아서ㅋㅋ",
          "original_form": "아 진짜? 니트 너한테 여유있나? 너 입은거 자세히 보진 않아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017392.1.12",
          "form": "품이야 뭐 당연 여유 있기야 하지 ㅋㅋ 나는 대부분 옷은 어깨와 팔길이가 관건이라",
          "original_form": "품이야 뭐 당연 여유 있기야 하지 ㅋㅋ 나는 대부분 옷은 어깨와 팔길이가 관건이라",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017392.1.13",
          "form": "언제 상세사이즈 측정 부탁드립니다ㅋㅋ",
          "original_form": "언제 상세사이즈 측정 부탁드립니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017392.1.14",
          "form": "근데 나는 산지 좀 되서 좀 늘어났을 듯",
          "original_form": "근데 나는 산지 좀 되서 좀 늘어났을 듯",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017392.1.15",
          "form": "그른가 그냥 폴로 랄프로렌 매장가면 맨 우면 보이즈 걸즈 다 있는가",
          "original_form": "그른가 그냥 폴로 랄프로렌 매장가면 맨 우면 보이즈 걸즈 다 있는가",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017392.1.16",
          "form": "청남방은 남자꺼 젤 작은것도 괜츈한거 같아서ㅋ",
          "original_form": "청남방은 남자꺼 젤 작은것도 괜츈한거 같아서ㅋ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017392.1.17",
          "form": "아닐걸 전에 알아봤을때 키즈 옷 없었던듯",
          "original_form": "아닐걸 전에 알아봤을때 키즈 옷 없었던듯",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017392.1.18",
          "form": "옷 한번 사기 힘드넴",
          "original_form": "옷 한번 사기 힘드넴",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017392.1.19",
          "form": "보이즈 엑라는 어지간하면 맞을걸 성인 남자들도 종종 입어서",
          "original_form": "보이즈 엑라는 어지간하면 맞을걸 성인 남자들도 종종 입어서",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017392.1.20",
          "form": "아 그럼 도전 해보까ㅋㅋㅋ",
          "original_form": "아 그럼 도전 해보까ㅋㅋㅋ",
