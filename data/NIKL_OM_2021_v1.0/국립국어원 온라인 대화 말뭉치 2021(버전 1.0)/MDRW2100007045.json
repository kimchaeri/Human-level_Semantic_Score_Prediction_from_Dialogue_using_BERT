      "utterance": [
        {
          "id": "MDRW2100007045.1.1",
          "form": "떡볶이 근데",
          "original_form": "떡볶이 근데",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007045.1.2",
          "form": "음식 얘기하니 금방 끝나버리네요",
          "original_form": "음식 얘기하니 금방 끝나버리네요",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007045.1.3",
          "form": "인분 측정도 이상해요",
          "original_form": "인분 측정도 이상해요",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007045.1.4",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007045.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007045.1.6",
          "form": "맞아요 인분이 정확하지가 않아요",
          "original_form": "맞아요 인분이 정확하지가 않아요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007045.1.7",
          "form": "1~2인분이라 하면 1인분이고",
          "original_form": "1~2인분이라 하면 1인분이고",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007045.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007045.1.9",
          "form": "그니까요.. 솔직히 3-4인분도 4인분으로는 좀 부족하지 않나요..",
          "original_form": "그니까요.. 솔직히 3-4인분도 4인분으로는 좀 부족하지 않나요..",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007045.1.10",
          "form": "잘먹으면 2명이서도 먹는데..",
          "original_form": "잘먹으면 2명이서도 먹는데..",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007045.1.11",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007045.1.12",
          "form": "엽떡 2인분 말고 3-4인분짜리",
          "original_form": "엽떡 2인분 말고 3-4인분짜리",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007045.1.13",
          "form": "친구랑 둘이서 먹은 적 있어요...ㅋㅋㅋ",
          "original_form": "친구랑 둘이서 먹은 적 있어요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007045.1.14",
          "form": "잘들어갈때 먹으면 둘이서도 가능해요..",
          "original_form": "잘들어갈때 먹으면 둘이서도 가능해요..",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007045.1.15",
          "form": "엽떡 얘기하니까 너무 먹고싶네요..",
          "original_form": "엽떡 얘기하니까 너무 먹고싶네요..",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007045.1.16",
          "form": "맞아요... 저도",
          "original_form": "맞아요... 저도",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007045.1.17",
          "form": "엽떡 먹고 싶네요",
          "original_form": "엽떡 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007045.1.18",
          "form": "엽떡 먹으실때 무슨 맛으로 드세요?",
          "original_form": "엽떡 먹으실때 무슨 맛으로 드세요?",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007045.1.19",
          "form": "저 맵찔이라 착한 맛이요ㅠ",
          "original_form": "저 맵찔이라 착한 맛이요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007045.1.20",
          "form": "착한맛도 맛있어요! 저는 덜매운맛 자주 먹어요 약간 쓰읍 하면서 먹긴하지만요..ㅎ",
          "original_form": "착한맛도 맛있어요! 저는 덜매운맛 자주 먹어요 약간 쓰읍 하면서 먹긴하지만요..ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007045.1.21",
          "form": "오 잘 드시네요 멋있어요ㅠ",
          "original_form": "오 잘 드시네요 멋있어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007045.1.22",
          "form": "아니에요 매운걸 좋아하는데 엄청 잘 먹는 편은 아니라서..!",
          "original_form": "아니에요 매운걸 좋아하는데 엄청 잘 먹는 편은 아니라서..!",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007045.1.23",
          "form": "저는 사실 떡보다 오뎅을 더 좋아해서 항상 엽기오뎅으로 시켜먹어요",
          "original_form": "저는 사실 떡보다 오뎅을 더 좋아해서 항상 엽기오뎅으로 시켜먹어요",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007045.1.24",
          "form": "앗 그렇군요!",
          "original_form": "앗 그렇군요!",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007045.1.25",
          "form": "저도 오뎅이 더 좋아요 ㅎㅎ",
          "original_form": "저도 오뎅이 더 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:42"
        }
      ]
    }
  ]
