        {
          "id": "MDRW2100018008.1.3",
          "form": "얼마나 만나셨어요?",
          "original_form": "얼마나 만나셨어요?",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018008.1.4",
          "form": "6년이요",
          "original_form": "6년이요",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018008.1.5",
          "form": "오래되셨네요",
          "original_form": "오래되셨네요",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018008.1.6",
          "form": "곧 결혼해요",
          "original_form": "곧 결혼해요",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018008.1.7",
          "form": "우와축하드려요",
          "original_form": "우와축하드려요",
