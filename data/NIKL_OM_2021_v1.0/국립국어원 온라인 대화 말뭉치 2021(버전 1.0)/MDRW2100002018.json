      "utterance": [
        {
          "id": "MDRW2100002018.1.1",
          "form": "웅~조선호텔 김치가 맛있다고 해서 그걸로",
          "original_form": "웅~조선호텔 김치가 맛있다고 해서 그걸로",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002018.1.2",
          "form": "집에만 있으니 김치가 모자르네 ㅋㅋ",
          "original_form": "집에만 있으니 김치가 모자르네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002018.1.3",
          "form": "그러게 말야. 매년 남았었는데 ㅎ",
          "original_form": "그러게 말야. 매년 남았었는데 ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002018.1.4",
          "form": "김치 이렇게 끝까지 다 먹은 건 처음인 것 같아",
          "original_form": "김치 이렇게 끝까지 다 먹은 건 처음인 것 같아",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002018.1.5",
          "form": "우리가 산 김치가 새콤하고 아주 육수가 맛있다네",
          "original_form": "우리가 산 김치가 새콤하고 아주 육수가 맛있다네",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002018.1.6",
          "form": "기대해봐야지 ㅎㅎ",
          "original_form": "기대해봐야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002018.1.7",
          "form": "ㅇㅇ 전에 먹었던 다른 김치는 시원한 맛이 없어서",
          "original_form": "ㅇㅇ 전에 먹었던 다른 김치는 시원한 맛이 없어서",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002018.1.8",
          "form": "이건 상품평 보니 대체로 만족이고 김치도 시원하대",
          "original_form": "이건 상품평 보니 대체로 만족이고 김치도 시원하대",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002018.1.9",
          "form": "배송은 얼마나 걸려",
          "original_form": "배송은 얼마나 걸려",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002018.1.10",
          "form": "김치 담가서 바로 보낸다니 이틀이면 되지 않을까?",
          "original_form": "김치 담가서 바로 보낸다니 이틀이면 되지 않을까?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002018.1.11",
          "form": "응 집에 삼겹살 있으니 같이 싸먹어야겠다",
          "original_form": "응 집에 삼겹살 있으니 같이 싸먹어야겠다",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002018.1.12",
          "form": "부디 맛있어야할텐데 ㅎㅎ",
          "original_form": "부디 맛있어야할텐데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002018.1.13",
          "form": "상품평이 좋다니 믿어봐야지",
          "original_form": "상품평이 좋다니 믿어봐야지",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002018.1.14",
          "form": "할인쿠폰도 받아서 좋은 가격에 샀엉",
          "original_form": "할인쿠폰도 받아서 좋은 가격에 샀엉",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002018.1.15",
          "form": "맛있으면 계속 그걸로 먹자",
          "original_form": "맛있으면 계속 그걸로 먹자",
          "speaker_id": "1",
          "time": "20210819 22:51"
        }
      ]
