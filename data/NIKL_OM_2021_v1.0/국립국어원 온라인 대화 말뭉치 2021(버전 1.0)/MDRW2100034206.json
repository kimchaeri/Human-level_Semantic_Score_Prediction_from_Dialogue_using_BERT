      "utterance": [
        {
          "id": "MDRW2100034206.1.1",
          "form": "전 서브웨이 샌드위치를 좋아해요 님은요?",
          "original_form": "전 서브웨이 샌드위치를 좋아해요 님은요?",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100034206.1.2",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100034206.1.3",
          "form": "잘시켜먹러요",
          "original_form": "잘시켜먹러요",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100034206.1.4",
          "form": "근데 소스그런거 잘몰라서",
          "original_form": "근데 소스그런거 잘몰라서",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034206.1.5",
          "form": "추천요",
          "original_form": "추천요",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034206.1.6",
          "form": "아하 배달파시구나 전 랜치에 겨자요",
          "original_form": "아하 배달파시구나 전 랜치에 겨자요",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034206.1.7",
          "form": "딸은 거기 쿠키줘서 좋아해요",
          "original_form": "딸은 거기 쿠키줘서 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034206.1.8",
          "form": "오 그게맛나요?",
          "original_form": "오 그게맛나요?",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034206.1.9",
          "form": "저도 그렇게 해야겠네요 ㅎㅎ",
          "original_form": "저도 그렇게 해야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100034206.1.10",
          "form": "전 좀 매콤한거 좋아해서 겨자 많이 랜치조금인데요",
          "original_form": "전 좀 매콤한거 좋아해서 겨자 많이 랜치조금인데요",
          "speaker_id": "1",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034206.1.11",
          "form": "따님에겐 추천하지 않아요",
          "original_form": "따님에겐 추천하지 않아요",
          "speaker_id": "1",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034206.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034206.1.13",
          "form": "그럼 맵나요?",
          "original_form": "그럼 맵나요?",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034206.1.14",
          "form": "전 매운걸 잘못먹어요",
          "original_form": "전 매운걸 잘못먹어요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034206.1.15",
          "form": "비율을 조절해야 갰네요",
          "original_form": "비율을 조절해야 갰네요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100034206.1.16",
          "form": "왜 자꾸 필터링이 나오지",
          "original_form": "왜 자꾸 필터링이 나오지",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100034206.1.17",
          "form": "아 머스타드는 살짝  넣 0 ㅓ 달라고 해보세요",
          "original_form": "아 머스타드는 살짝  넣 0 ㅓ 달라고 해보세요",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100034206.1.18",
          "form": "아까는 소스를 치어무억으면 이라고 썼다가 걸리더니",
          "original_form": "아까는 소스를 치어무억으면 이라고 썼다가 걸리더니",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034206.1.19",
          "form": "맨날 이상해요 필터링 ㅡㅡ",
          "original_form": "맨날 이상해요 필터링 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034206.1.20",
          "form": "맞아요 참나",
          "original_form": "맞아요 참나",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034206.1.21",
          "form": "표현을 어찌 하란건지",
          "original_form": "표현을 어찌 하란건지",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034206.1.22",
          "form": "아깐 닭브레스트살을 먹는 얘길 하는데도 자꾸 걸리고 타자치기 힘든데 너무합니다",
          "original_form": "아깐 닭브레스트살을 먹는 얘길 하는데도 자꾸 걸리고 타자치기 힘든데 너무합니다",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034206.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100034206.1.24",
          "form": "실컷해놓고 안되고 참나",
          "original_form": "실컷해놓고 안되고 참나",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100034206.1.25",
          "form": "맵기 조절해서 먹음되겠네요",
          "original_form": "맵기 조절해서 먹음되겠네요",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100034206.1.26",
          "form": "빵은요?",
          "original_form": "빵은요?",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100034206.1.27",
          "form": "저 이부분 고객센터 말했는데 수정한다고만 하시네요 ㅜㅜ",
          "original_form": "저 이부분 고객센터 말했는데 수정한다고만 하시네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100034206.1.28",
          "form": "빵은 매장서 먹을떈 플랫에도 먹는데요 몇시간 있다 먹을땐 그냥 두꺼운빵이요",
          "original_form": "빵은 매장서 먹을떈 플랫에도 먹는데요 몇시간 있다 먹을땐 그냥 두꺼운빵이요",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100034206.1.29",
          "form": "ㅠ 이것때문에 필터링을 고치진 않을것 같아요 언제 개선될련지",
          "original_form": "ㅠ 이것때문에 필터링을 고치진 않을것 같아요 언제 개선될련지",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100034206.1.30",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100034206.1.31",
          "form": "그렇죠 그런섯 같았어요 ㅡㅡ",
          "original_form": "그렇죠 그런섯 같았어요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100034206.1.32",
          "form": "전 기냥 무조건 추천으로 ㅋㅋ",
          "original_form": "전 기냥 무조건 추천으로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100034206.1.33",
          "form": "추천이 좋죠 무난하게 잘 골라줘요",
          "original_form": "추천이 좋죠 무난하게 잘 골라줘요",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100034206.1.34",
          "form": "저라면 비엘티 이런걸로 골라줄듯요",
          "original_form": "저라면 비엘티 이런걸로 골라줄듯요",
          "speaker_id": "1",
          "time": "20210827 21:19"
        }
      ]
