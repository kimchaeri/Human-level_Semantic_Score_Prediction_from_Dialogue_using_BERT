      "utterance": [
        {
          "id": "MDRW2100039299.1.1",
          "form": "어떤 겜에서 모인건지 여쭤봐도 되나요?",
          "original_form": "어떤 겜에서 모인건지 여쭤봐도 되나요?",
          "speaker_id": "1",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039299.1.2",
          "form": "별건 아니구",
          "original_form": "별건 아니구",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039299.1.3",
          "form": "하얀고양이 프로젝트라는 모바일 게임이에요",
          "original_form": "하얀고양이 프로젝트라는 모바일 게임이에요",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039299.1.4",
          "form": "처음 들어보는데 나중에 한번 접속해 봐야겠네요",
          "original_form": "처음 들어보는데 나중에 한번 접속해 봐야겠네요",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039299.1.5",
          "form": "한국판은 오래전에 섭종했어요",
          "original_form": "한국판은 오래전에 섭종했어요",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039299.1.6",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039299.1.7",
          "form": "그럼 글로벌 무대에서 게임을 즐기시는 거예요?",
          "original_form": "그럼 글로벌 무대에서 게임을 즐기시는 거예요?",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039299.1.8",
          "form": "일본버전으로 하고 있어요",
          "original_form": "일본버전으로 하고 있어요",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100039299.1.9",
          "form": "어 그럼 혹시 아까 동호회 분들도 일본 분들",
          "original_form": "어 그럼 혹시 아까 동호회 분들도 일본 분들",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039299.1.10",
          "form": "일본어 잘 하시나 봐요 멋집니다",
          "original_form": "일본어 잘 하시나 봐요 멋집니다",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039299.1.11",
          "form": "다 한국분들이시구",
          "original_form": "다 한국분들이시구",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039299.1.12",
          "form": "아히",
          "original_form": "아히",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039299.1.13",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039299.1.14",
          "form": "다른 분들이 해석해주시는걸로 연명하고 있어요",
          "original_form": "다른 분들이 해석해주시는걸로 연명하고 있어요",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039299.1.15",
          "form": "그러시군용",
          "original_form": "그러시군용",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100039299.1.16",
          "form": "하고 싶은 동호회 있으신가요",
          "original_form": "하고 싶은 동호회 있으신가요",
          "speaker_id": "2",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100039299.1.17",
          "form": "저는 운동해보고 싶어요",
          "original_form": "저는 운동해보고 싶어요",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100039299.1.18",
          "form": "클라이밍같은거",
          "original_form": "클라이밍같은거",
