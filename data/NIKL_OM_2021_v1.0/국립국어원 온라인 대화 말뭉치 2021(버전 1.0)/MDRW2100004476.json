        {
          "id": "MDRW2100004476.1.1",
          "form": "name1님 운동 좋아하시나요?",
          "original_form": "&name1&님 운동 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004476.1.2",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004476.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004476.1.4",
          "form": "그냥 살빼려고 홈트 자전거 걷기했어요",
          "original_form": "그냥 살빼려고 홈트 자전거 걷기했어요",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004476.1.5",
          "form": "근데 요즘은 운동 좀 해야할 것 같아서",
          "original_form": "근데 요즘은 운동 좀 해야할 것 같아서",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004476.1.6",
          "form": "그땐 미친듯하다 또 기냥 쉬고 그래요",
          "original_form": "그땐 미친듯하다 또 기냥 쉬고 그래요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.7",
          "form": "필라테스를 주 2-3회 하고 있어요",
          "original_form": "필라테스를 주 2-3회 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.8",
          "form": "맞아요 어질 안가니까요",
          "original_form": "맞아요 어질 안가니까요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.9",
          "form": "ㅋㅋ홈트는 정말 꾸준히 하기 힘들죠",
          "original_form": "ㅋㅋ홈트는 정말 꾸준히 하기 힘들죠",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.10",
          "form": "저도 2-3회는 할수 있을것 같아요",
          "original_form": "저도 2-3회는 할수 있을것 같아요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.11",
          "form": "특히 얼마 전까지 너무 더워서",
          "original_form": "특히 얼마 전까지 너무 더워서",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.12",
          "form": "네 그리고 밑에층 민폐요",
          "original_form": "네 그리고 밑에층 민폐요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.15",
          "form": "에어컨 빵빵해야해요",
          "original_form": "에어컨 빵빵해야해요",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004476.1.16",
          "form": "요가매트 깔고 하긴 했는데",
          "original_form": "요가매트 깔고 하긴 했는데",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004476.1.17",
          "form": "자세도 좀 교정하고파요",
          "original_form": "자세도 좀 교정하고파요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004476.1.18",
          "form": "그래도 걷고 뛰는 건 층간소음 걱정",
          "original_form": "그래도 걷고 뛰는 건 층간소음 걱정",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004476.1.19",
          "form": "저도 깔았는데 소용없어요",
          "original_form": "저도 깔았는데 소용없어요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004476.1.20",
          "form": "맞아요 낮에만해요",
          "original_form": "맞아요 낮에만해요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004476.1.21",
          "form": "그래도 그래요",
          "original_form": "그래도 그래요",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004476.1.22",
          "form": "저는 필라테스 하는데",
          "original_form": "저는 필라테스 하는데",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004476.1.23",
          "form": "마돈나가 필라테스로 젊을과 몸매 유지한다더라구요",
          "original_form": "마돈나가 필라테스로 젊을과 몸매 유지한다더라구요",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004476.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004476.1.25",
          "form": "자세 교정엔 효과가 있는 듯해요",
          "original_form": "자세 교정엔 효과가 있는 듯해요",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004476.1.26",
          "form": "그렇죠 갈수록 자세가 구부정해요",
          "original_form": "그렇죠 갈수록 자세가 구부정해요",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004476.1.27",
          "form": "꼭 필요한 운동이네요",
          "original_form": "꼭 필요한 운동이네요",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004476.1.28",
          "form": "살도빼야하지만 요샌 몸맵시나 자세 이뻐야 겠더라구요",
          "original_form": "살도빼야하지만 요샌 몸맵시나 자세 이뻐야 겠더라구요",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004476.1.29",
          "form": "왠지 막뿜뿜 합니다",
          "original_form": "왠지 막뿜뿜 합니다",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004476.1.30",
          "form": "하고싶어요",
