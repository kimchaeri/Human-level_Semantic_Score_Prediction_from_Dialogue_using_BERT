      "utterance": [
        {
          "id": "MDRW2100030300.1.1",
          "form": "광복절 연류 집에서 머무르기 대국민 캠페인이라는거 들어봤어??",
          "original_form": "광복절 연류 집에서 머무르기 대국민 캠페인이라는거 들어봤어??",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.2",
          "form": "나도 처음듣기는 하지만 사회적 거리두기 같은 느낌인거같아",
          "original_form": "나도 처음듣기는 하지만 사회적 거리두기 같은 느낌인거같아",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.3",
          "form": "처음 들어봐",
          "original_form": "처음 들어봐",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.4",
          "form": "그게 몬데??",
          "original_form": "그게 몬데??",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.5",
          "form": "연휴에는 원래 집에서 쉬는게 좋긴 하지",
          "original_form": "연휴에는 원래 집에서 쉬는게 좋긴 하지",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.6",
          "form": "그렇구만,,",
          "original_form": "그렇구만,,",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.7",
          "form": "나는 잘 지킬수있을거같아",
          "original_form": "나는 잘 지킬수있을거같아",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.8",
          "form": "맞아..요즘같은때에",
          "original_form": "맞아..요즘같은때에",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.9",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.10",
          "form": "나가기 힘들지",
          "original_form": "나가기 힘들지",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.11",
          "form": "나두 ㅎㅎ",
          "original_form": "나두 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.12",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.13",
          "form": "난 집에잇는거 좋아해",
          "original_form": "난 집에잇는거 좋아해",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.14",
          "form": "나가봤자 있을곳도없잖아",
          "original_form": "나가봤자 있을곳도없잖아",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.15",
          "form": "나두!",
          "original_form": "나두!",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.16",
          "form": "광복절은 며칠씩 연휴인것도 아니구 하루니깐..",
          "original_form": "광복절은 며칠씩 연휴인것도 아니구 하루니깐..",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030300.1.17",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.18",
          "form": "집에서 광복절 의의를 되새기는것도",
          "original_form": "집에서 광복절 의의를 되새기는것도",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.19",
          "form": "좋을 것 같애 ㅋㅋ",
          "original_form": "좋을 것 같애 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.20",
          "form": "맞아!! 그리고 집에거 머무르기 캠페인이라는",
          "original_form": "맞아!! 그리고 집에거 머무르기 캠페인이라는",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.21",
          "form": "취지가 좋은거같아",
          "original_form": "취지가 좋은거같아",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.22",
          "form": "누구나 참여할수있구",
          "original_form": "누구나 참여할수있구",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.23",
          "form": "코로나도 예방할수있고",
          "original_form": "코로나도 예방할수있고",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.24",
          "form": "나두 그렇게 생각해",
          "original_form": "나두 그렇게 생각해",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.25",
          "form": "그냥 노는날로 생각하기보다는",
          "original_form": "그냥 노는날로 생각하기보다는",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.26",
          "form": "광복절의 의미를 생각해볼수있는",
          "original_form": "광복절의 의미를 생각해볼수있는",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.27",
          "form": "집에 잇는김에 태극기까지 걸어놓으면 더 좋겟지 ㅎㅎ",
          "original_form": "집에 잇는김에 태극기까지 걸어놓으면 더 좋겟지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.28",
          "form": "뜻깊은게댈거야 ㅎㅎ",
          "original_form": "뜻깊은게댈거야 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.29",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030300.1.30",
          "form": "좋당!!",
          "original_form": "좋당!!",
          "speaker_id": "2",
          "time": "20210903 23:37"
        }
      ]
    }
