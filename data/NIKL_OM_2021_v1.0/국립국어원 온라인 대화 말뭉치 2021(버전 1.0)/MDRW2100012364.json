      "utterance": [
        {
          "id": "MDRW2100012364.1.1",
          "form": "뭣 모르고 접하다가",
          "original_form": "뭣 모르고 접하다가",
          "speaker_id": "2",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012364.1.2",
          "form": "애 낳고",
          "original_form": "애 낳고",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.3",
          "form": "성 ㅂㅕㅇ 걸리고",
          "original_form": "성 ㅂㅕㅇ 걸리고",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.4",
          "form": "21세기에 아직도 이렇게 정보를 숨겨서",
          "original_form": "21세기에 아직도 이렇게 정보를 숨겨서",
          "speaker_id": "1",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.5",
          "form": "그 위험성 알려줬으면",
          "original_form": "그 위험성 알려줬으면",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.6",
          "form": "거의 사기결혼 사기임신 아닌가욬ㅋㅋㅋ",
          "original_form": "거의 사기결혼 사기임신 아닌가욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.7",
          "form": "최소한 몇명은 안걸릴 수도 있었을텐데",
          "original_form": "최소한 몇명은 안걸릴 수도 있었을텐데",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.8",
          "form": "진짜 맞아요",
          "original_form": "진짜 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012364.1.9",
          "form": "겉으로는 자유로운 척 하면서",
          "original_form": "겉으로는 자유로운 척 하면서",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012364.1.10",
          "form": "국가가 방조한..",
          "original_form": "국가가 방조한..",
          "speaker_id": "1",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012364.1.11",
          "form": "계속 압박하고",
          "original_form": "계속 압박하고",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012364.1.12",
          "form": "선택의 자유를 빼앗은 느낌이네요",
          "original_form": "선택의 자유를 빼앗은 느낌이네요",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012364.1.13",
          "form": "정보를 불균등하게 공ㅇ유해서..",
          "original_form": "정보를 불균등하게 공ㅇ유해서..",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012364.1.14",
          "form": "그 시험관아기도 여자만 엄청 고생한다면서요",
          "original_form": "그 시험관아기도 여자만 엄청 고생한다면서요",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012364.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.16",
          "form": "정자는 그냥",
          "original_form": "정자는 그냥",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.17",
          "form": "배출하기만 하면 되는데",
          "original_form": "배출하기만 하면 되는데",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.18",
          "form": "어이없어요",
          "original_form": "어이없어요",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.19",
          "form": "3초찍..",
          "original_form": "3초찍..",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.21",
          "form": "전 정혈통 같은거",
          "original_form": "전 정혈통 같은거",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012364.1.22",
          "form": "왜 연구가 더딘지",
          "original_form": "왜 연구가 더딘지",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012364.1.23",
          "form": "화나요",
          "original_form": "화나요",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012364.1.24",
          "form": "이것때문에 고통받는 사람들도 많은데",
          "original_form": "이것때문에 고통받는 사람들도 많은데",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012364.1.25",
          "form": "아 진짜 ㄹㅇ",
          "original_form": "아 진짜 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012364.1.26",
          "form": "교육도 잘 안되어있고",
          "original_form": "교육도 잘 안되어있고",
          "speaker_id": "2",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012364.1.27",
          "form": "저는 정혈 하기 전에 정혈이 있는줄도 몰랐어요",
          "original_form": "저는 정혈 하기 전에 정혈이 있는줄도 몰랐어요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012364.1.28",
          "form": "처음 정혈할때 저 병난줄 ㅋㅋㅋㅋ",
          "original_form": "처음 정혈할때 저 병난줄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012364.1.29",
          "form": "와 맞아요",
          "original_form": "와 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012364.1.30",
          "form": "전 처음에 너무 아파서",
          "original_form": "전 처음에 너무 아파서",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012364.1.31",
          "form": "잠도 잘 못잤는데",
          "original_form": "잠도 잘 못잤는데",
          "speaker_id": "2",
          "time": "20210824 21:41"
        }
      ]
    }
  ]
