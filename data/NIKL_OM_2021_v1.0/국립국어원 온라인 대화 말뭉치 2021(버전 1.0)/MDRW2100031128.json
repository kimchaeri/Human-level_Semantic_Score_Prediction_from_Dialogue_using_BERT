      "utterance": [
        {
          "id": "MDRW2100031128.1.1",
          "form": "네 안한지 좀 돼서 ㅋㅋㅋ",
          "original_form": "네 안한지 좀 돼서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031128.1.2",
          "form": "오 맞아요 복귀런너 되겠네요",
          "original_form": "오 맞아요 복귀런너 되겠네요",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031128.1.3",
          "form": "그런 보상은 챙겨먹어야죠",
          "original_form": "그런 보상은 챙겨먹어야죠",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031128.1.4",
          "form": "맞아요 뿌리는건 다 받아먹어야죠 ㅎㅎ",
          "original_form": "맞아요 뿌리는건 다 받아먹어야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031128.1.5",
          "form": "그러라고 뿌리는 거니까요",
          "original_form": "그러라고 뿌리는 거니까요",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031128.1.6",
          "form": "프리미엄티켓 받을 수 있겠다",
          "original_form": "프리미엄티켓 받을 수 있겠다",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031128.1.7",
          "form": "아 그거 보상 바뀌었는데 모르셨나요?",
          "original_form": "아 그거 보상 바뀌었는데 모르셨나요?",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031128.1.8",
          "form": "네? 어떻게요?",
          "original_form": "네? 어떻게요?",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031128.1.9",
          "form": "복귀런너에 프리미엄티켓 보상",
          "original_form": "복귀런너에 프리미엄티켓 보상",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031128.1.10",
          "form": "아 ㅇ그거 없어졌나요",
          "original_form": "아 ㅇ그거 없어졌나요",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031128.1.11",
          "form": "네 없어지고 이상한것만 줘요",
          "original_form": "네 없어지고 이상한것만 줘요",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100031128.1.12",
          "form": "헐 프리미엄티켓이 제일 좋아쓴ㄴ데",
          "original_form": "헐 프리미엄티켓이 제일 좋아쓴ㄴ데",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031128.1.13",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031128.1.14",
          "form": "혜택을 자꾸 ㄷ줄이네요",
          "original_form": "혜택을 자꾸 ㄷ줄이네요",
          "speaker_id": "2",
          "time": "20210904 19:38"
        },
        {
          "id": "MDRW2100031128.1.15",
          "form": "계속 줄이면 안좋을텐데 말이죠",
          "original_form": "계속 줄이면 안좋을텐데 말이죠",
