      "utterance": [
        {
          "id": "MDRW2100002836.1.1",
          "form": "근데 왜 막굶고 빼야 할까요",
          "original_form": "근데 왜 막굶고 빼야 할까요",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.2",
          "form": "어차피 돌아 오는데",
          "original_form": "어차피 돌아 오는데",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.3",
          "form": "그게 이해가 안가요",
          "original_form": "그게 이해가 안가요",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.4",
          "form": "음 건강도 건강이지만",
          "original_form": "음 건강도 건강이지만",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.6",
          "form": "시선도 무서우니까요",
          "original_form": "시선도 무서우니까요",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.7",
          "form": "그게 젤 무섭죠",
          "original_form": "그게 젤 무섭죠",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002836.1.8",
          "form": "우리 남편이 멕시코 살면 전 미인이래요",
          "original_form": "우리 남편이 멕시코 살면 전 미인이래요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002836.1.9",
          "form": "거긴 워낙 뚱뚱해서",
          "original_form": "거긴 워낙 뚱뚱해서",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002836.1.10",
          "form": "시선이 여기 같지 않데요",
          "original_form": "시선이 여기 같지 않데요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002836.1.11",
          "form": "맞아요 우리나라만 난리에요",
          "original_form": "맞아요 우리나라만 난리에요",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002836.1.12",
          "form": "저어릴때 그땐 지금처럼은 아니였지만",
          "original_form": "저어릴때 그땐 지금처럼은 아니였지만",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002836.1.13",
          "form": "그 기준도 너무 높아요",
          "original_form": "그 기준도 너무 높아요",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002836.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002836.1.15",
          "form": "그 몸무게가 표준이라니",
          "original_form": "그 몸무게가 표준이라니",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002836.1.16",
          "form": "저도 어릴땐 평범했죠",
          "original_form": "저도 어릴땐 평범했죠",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002836.1.17",
          "form": "66입을때였는데  지인오빠가 귀국할때 옷사준다해서\r\n솔직히 어릴때  66도 말하기가 챙피해서",
          "original_form": "66입을때였는데  지인오빠가 귀국할때 옷사준다해서\r\n솔직히 어릴때  66도 말하기가 챙피해서",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.18",
          "form": "살쪄서 내사이즈 없다고 막 그랬더니",
          "original_form": "살쪄서 내사이즈 없다고 막 그랬더니",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.19",
          "form": "맞아요 그땐 그랬어요",
          "original_form": "맞아요 그땐 그랬어요",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.20",
          "form": "지인이 샵에가서  뚱뚱하다는데  66사이즈 있냐고 그랬데요",
          "original_form": "지인이 샵에가서  뚱뚱하다는데  66사이즈 있냐고 그랬데요",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.21",
          "form": "사이즈 민감해요 그땐 더",
          "original_form": "사이즈 민감해요 그땐 더",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.22",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.23",
          "form": "이오빠도  여자 사이즈 몰라서 ㅋㅋㅋ",
          "original_form": "이오빠도  여자 사이즈 몰라서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002836.1.24",
          "form": "무슨 66",
          "original_form": "무슨 66",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.25",
          "form": "미국직원이 그랫데요",
          "original_form": "미국직원이 그랫데요",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.26",
          "form": "66은 엄청 날씬한거라고  엄청 많다고요 ㅎㅎ",
          "original_form": "66은 엄청 날씬한거라고  엄청 많다고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.27",
          "form": "아 제 기준에 66이 뚱뚱했어요 어릴때는 ㅎㅎ",
          "original_form": "아 제 기준에 66이 뚱뚱했어요 어릴때는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.28",
          "form": "오 그렇죠 미국은 다 뚱뚱하니",
          "original_form": "오 그렇죠 미국은 다 뚱뚱하니",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.29",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.30",
          "form": "와 그래서 말못했는데",
          "original_form": "와 그래서 말못했는데",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002836.1.31",
          "form": "ㅎㅎ 우리나라만  숫자에 집착 하나봐요",
          "original_form": "ㅎㅎ 우리나라만  숫자에 집착 하나봐요",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002836.1.32",
          "form": "맞아요 숫자 지겹네요",
          "original_form": "맞아요 숫자 지겹네요",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002836.1.33",
          "form": "우리도 이민 가야 하나요",
          "original_form": "우리도 이민 가야 하나요",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002836.1.34",
          "form": "가면 날씬이래요",
          "original_form": "가면 날씬이래요",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002836.1.35",
          "form": "ㅎㅎ 가고 싶네요 ㅋㅋㅋ",
          "original_form": "ㅎㅎ 가고 싶네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002836.1.36",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210820 15:59"
        }
      ]
