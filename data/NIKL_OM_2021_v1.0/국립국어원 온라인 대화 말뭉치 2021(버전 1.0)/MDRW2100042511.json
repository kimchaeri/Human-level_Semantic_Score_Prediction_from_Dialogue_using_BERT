      "utterance": [
        {
          "id": "MDRW2100042511.1.1",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042511.1.2",
          "form": "아니 나같은 라이트 유저는",
          "original_form": "아니 나같은 라이트 유저는",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042511.1.3",
          "form": "5성 하나 뽑아준것만으로도",
          "original_form": "5성 하나 뽑아준것만으로도",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042511.1.4",
          "form": "엄청난 결심을 한거라고",
          "original_form": "엄청난 결심을 한거라고",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042511.1.5",
          "form": "ㅋㅋㅋㅋㅋ22",
          "original_form": "ㅋㅋㅋㅋㅋ22",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042511.1.6",
          "form": "천장도 엄청 높으면서",
          "original_form": "천장도 엄청 높으면서",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042511.1.7",
          "form": "ㅇㅇ심지어 그냥 천장도 아니고",
          "original_form": "ㅇㅇ심지어 그냥 천장도 아니고",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.8",
          "form": "반천장이면서",
          "original_form": "반천장이면서",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.9",
          "form": "ㅇㅇㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.10",
          "form": "아 맞아 혹시",
          "original_form": "아 맞아 혹시",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.11",
          "form": "원스토어 출석 해?",
          "original_form": "원스토어 출석 해?",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.12",
          "form": "이거 하면 매일 도장주는데",
          "original_form": "이거 하면 매일 도장주는데",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.13",
          "form": "15개 모으면 3천원이고",
          "original_form": "15개 모으면 3천원이고",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.14",
          "form": "카톡 임티 사는데도 쓸 수 잇대",
          "original_form": "카톡 임티 사는데도 쓸 수 잇대",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.15",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.16",
          "form": "안해",
          "original_form": "안해",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.17",
          "form": "옛날에는 했었는데",
          "original_form": "옛날에는 했었는데",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.18",
          "form": "나 한동안 안하다가",
          "original_form": "나 한동안 안하다가",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.19",
          "form": "임티나 살까 하고 다시 시작함",
          "original_form": "임티나 살까 하고 다시 시작함",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.20",
          "form": "요새 과금할 게임이 없어서.......",
          "original_form": "요새 과금할 게임이 없어서.......",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042511.1.21",
          "form": "임티 요새",
          "original_form": "임티 요새",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.22",
          "form": "글킨하지...ㅎ..",
          "original_form": "글킨하지...ㅎ..",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.23",
          "form": "걍 잘 안쓰게되고",
          "original_form": "걍 잘 안쓰게되고",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.24",
          "form": "끌리는것도 별로 없더라구",
          "original_form": "끌리는것도 별로 없더라구",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.25",
          "form": "예전에 럽프듀 과금하느라",
          "original_form": "예전에 럽프듀 과금하느라",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.26",
          "form": "진짜 꼬박꼬박 썼었는데....",
          "original_form": "진짜 꼬박꼬박 썼었는데....",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.27",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.28",
          "form": "극한의 효율 땡겨가면서",
          "original_form": "극한의 효율 땡겨가면서",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.29",
          "form": "날짜 맞춰서 과금하고",
          "original_form": "날짜 맞춰서 과금하고",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042511.1.30",
          "form": "그랬지.......",
          "original_form": "그랬지.......",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042511.1.31",
          "form": "다 옛날 일이지만",
          "original_form": "다 옛날 일이지만",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042511.1.32",
          "form": "와 진짜 옛날이고",
          "original_form": "와 진짜 옛날이고",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042511.1.33",
          "form": "ㅎㅏ......",
          "original_form": "ㅎㅏ......",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042511.1.34",
          "form": "다시는 그렇게 전심전력으로 겜 못할듯",
          "original_form": "다시는 그렇게 전심전력으로 겜 못할듯",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042511.1.35",
          "form": "ㅇㅇ진짜 옛날이야",
          "original_form": "ㅇㅇ진짜 옛날이야",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042511.1.36",
          "form": "ㅇㅇ......",
          "original_form": "ㅇㅇ......",
          "speaker_id": "2",
          "time": "20210906 22:37"
        }
      ]
    }
