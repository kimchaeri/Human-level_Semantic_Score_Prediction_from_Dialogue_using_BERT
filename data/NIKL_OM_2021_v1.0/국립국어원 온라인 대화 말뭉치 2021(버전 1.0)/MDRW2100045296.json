      "utterance": [
        {
          "id": "MDRW2100045296.1.1",
          "form": "그리고 뭔ㄱㅏ 만두는",
          "original_form": "그리고 뭔ㄱㅏ 만두는",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045296.1.2",
          "form": "혼자만 먹긴",
          "original_form": "혼자만 먹긴",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045296.1.3",
          "form": "모자라",
          "original_form": "모자라",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045296.1.4",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045296.1.5",
          "form": "뭔가 국물이 잇어야해",
          "original_form": "뭔가 국물이 잇어야해",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045296.1.6",
          "form": "ㅇㅇ그리고 내가 물 마시는 방법도",
          "original_form": "ㅇㅇ그리고 내가 물 마시는 방법도",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045296.1.7",
          "form": "다른걸 좐나게 먹어서",
          "original_form": "다른걸 좐나게 먹어서",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.8",
          "form": "목맥히게해서",
          "original_form": "목맥히게해서",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.9",
          "form": "마시는거라",
          "original_form": "마시는거라",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.10",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.11",
          "form": "뭔가 먹긴 해야대",
          "original_form": "뭔가 먹긴 해야대",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.12",
          "form": "그건 고문이자나",
          "original_form": "그건 고문이자나",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.13",
          "form": "아냐 육포같은거 사다놓고",
          "original_form": "아냐 육포같은거 사다놓고",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.14",
          "form": "와 육포... 먹고싶다...",
          "original_form": "와 육포... 먹고싶다...",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.15",
          "form": "하루죙일 좔강좔강 씹으면서",
          "original_form": "하루죙일 좔강좔강 씹으면서",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.16",
          "form": "짤때마다 물마시고",
          "original_form": "짤때마다 물마시고",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.17",
          "form": "그런거야",
          "original_form": "그런거야",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045296.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건 좀",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건 좀",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.19",
          "form": "제로섬 같은데",
          "original_form": "제로섬 같은데",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.20",
          "form": "아냐 내가 그래도",
          "original_form": "아냐 내가 그래도",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.21",
          "form": "그럴땐 물을 많이 마셔",
          "original_form": "그럴땐 물을 많이 마셔",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.22",
          "form": "놀랍게도 내가",
          "original_form": "놀랍게도 내가",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.23",
          "form": "굉장히 싱겁게 먹는 스타일이라",
          "original_form": "굉장히 싱겁게 먹는 스타일이라",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.25",
          "form": "육포먹고싶다",
          "original_form": "육포먹고싶다",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045296.1.26",
          "form": "난 아까 먹었어",
          "original_form": "난 아까 먹었어",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045296.1.27",
          "form": "정확히 위에 쓴대로",
          "original_form": "정확히 위에 쓴대로",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045296.1.28",
          "form": "아 액상과당",
          "original_form": "아 액상과당",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045296.1.29",
          "form": "그만마셔야되는데",
          "original_form": "그만마셔야되는데",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045296.1.30",
          "form": "단게 너무 좋아......",
          "original_form": "단게 너무 좋아......",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045296.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045296.1.32",
          "form": "뭐 마셧어?",
          "original_form": "뭐 마셧어?",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045296.1.33",
          "form": "콜라....",
          "original_form": "콜라....",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.34",
          "form": "ㅇㅎ ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎ ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.35",
          "form": "제로팹시 라임맛이 글케 맛있다던데",
          "original_form": "제로팹시 라임맛이 글케 맛있다던데",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.36",
          "form": "콜라 좋지...",
          "original_form": "콜라 좋지...",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.37",
          "form": "차라리 그쪽으로",
          "original_form": "차라리 그쪽으로",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.38",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.39",
          "form": "갈아타볼까?",
          "original_form": "갈아타볼까?",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.40",
          "form": "먹어본 입장에서는",
          "original_form": "먹어본 입장에서는",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.41",
          "form": "그래봤자 제로콜라...",
          "original_form": "그래봤자 제로콜라...",
          "speaker_id": "1",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.42",
          "form": "글킨한데",
          "original_form": "글킨한데",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045296.1.43",
          "form": "나 진짜 그만 마셔야해서",
          "original_form": "나 진짜 그만 마셔야해서",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045296.1.44",
          "form": "ㅇㅎ... 그럼 바꾸는 것도 좋지",
          "original_form": "ㅇㅎ... 그럼 바꾸는 것도 좋지",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045296.1.45",
          "form": "니코틴패치든",
          "original_form": "니코틴패치든",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045296.1.46",
          "form": "전자담배든",
          "original_form": "전자담배든",
          "speaker_id": "2",
          "time": "20210908 01:54"
        }
      ]
    }
