        },
        {
          "id": "MDRW2100029085.1.2",
          "form": "name1은요!",
          "original_form": "&name1&은요!",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029085.1.3",
          "form": "알고 있어요",
          "original_form": "알고 있어요",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.4",
          "form": "예전에 말했어요 우리",
          "original_form": "예전에 말했어요 우리",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.6",
          "form": "아 씨",
          "original_form": "아 씨",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.7",
          "form": "저는 결혼 했는데요",
          "original_form": "저는 결혼 했는데요",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.8",
          "form": "물어봐서  헷갈렸나 했잖아요!",
          "original_form": "물어봐서  헷갈렸나 했잖아요!",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.9",
          "form": "매일이 연애쥬ㅎ",
          "original_form": "매일이 연애쥬ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029085.1.10",
          "form": "누가요 ㅋㅋ",
          "original_form": "누가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029085.1.11",
          "form": "name2이요?",
          "original_form": "&name2&이요?",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029085.1.12",
          "form": "순간 제가 name1  아닌 누구랑 이 얘기했나 고민함ㅋㅋ",
          "original_form": "순간 제가 &name1&  아닌 누구랑 이 얘기했나 고민함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029085.1.13",
          "form": "저겠습니꽈!",
          "original_form": "저겠습니꽈!",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029085.1.14",
          "form": "의리에요 지금은 ㅋㅋ",
          "original_form": "의리에요 지금은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029085.1.15",
          "form": "비혼해도 연애해요",
          "original_form": "비혼해도 연애해요",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029085.1.16",
          "form": "혼자 보내기엔",
          "original_form": "혼자 보내기엔",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029085.1.17",
          "form": "너무 젊어요",
          "original_form": "너무 젊어요",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029085.1.18",
          "form": "어..전 큰 신념이 있는게 아니라",
          "original_form": "어..전 큰 신념이 있는게 아니라",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029085.1.19",
          "form": "연애가 정말 안맞아서 비혼인 거라ㅜ",
          "original_form": "연애가 정말 안맞아서 비혼인 거라ㅜ",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029085.1.20",
          "form": "무성애 자 라고 하쥬",
          "original_form": "무성애 자 라고 하쥬",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029085.1.21",
          "form": "연애할 때 엄청 스트레스였어요ㅜ",
          "original_form": "연애할 때 엄청 스트레스였어요ㅜ",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029085.1.22",
          "form": "ㅋㅋㅋ대화 나눠보면 매력이 팡팡터지시는데",
          "original_form": "ㅋㅋㅋ대화 나눠보면 매력이 팡팡터지시는데",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029085.1.23",
          "form": "아깝군요!!",
          "original_form": "아깝군요!!",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029085.1.24",
          "form": "제가 실제 보면 어마무시 도도한 차도녀입니돠",
