      "utterance": [
        {
          "id": "MDRW2100017591.1.1",
          "form": "신문에서 들어본 것도 같은데",
          "original_form": "신문에서 들어본 것도 같은데",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017591.1.2",
          "form": "사진찍는건가요?",
          "original_form": "사진찍는건가요?",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017591.1.3",
          "form": "네네 몸매 만들어서 몸 사진찍는거에요",
          "original_form": "네네 몸매 만들어서 몸 사진찍는거에요",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017591.1.4",
          "form": "근데 그거 하느라 운동을 너무 심하게해서 건강이 나빠진다고 들었어요",
          "original_form": "근데 그거 하느라 운동을 너무 심하게해서 건강이 나빠진다고 들었어요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017591.1.5",
          "form": "네 식단 엄청신경쓰고 운동도 빡세게 해아해서",
          "original_form": "네 식단 엄청신경쓰고 운동도 빡세게 해아해서",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017591.1.6",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017591.1.7",
          "form": "제 친구들도 오히려 병얻어온 케이스가 많아요",
          "original_form": "제 친구들도 오히려 병얻어온 케이스가 많아요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017591.1.8",
          "form": "뭐든 과유불급인건데",
          "original_form": "뭐든 과유불급인건데",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017591.1.9",
          "form": "그래서 막 자신감과 아름다움 건강을 얻으려다 질병만 얻었다고",
          "original_form": "그래서 막 자신감과 아름다움 건강을 얻으려다 질병만 얻었다고",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017591.1.10",
          "form": "그쵸그쵸 과유불급",
          "original_form": "그쵸그쵸 과유불급",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017591.1.11",
          "form": "운동도 적당히 꾸준히 해야지 걱정스럽네요",
          "original_form": "운동도 적당히 꾸준히 해야지 걱정스럽네요",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017591.1.12",
          "form": "동감입니다",
          "original_form": "동감입니다",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017591.1.13",
          "form": "건강이 정말 중요해요",
          "original_form": "건강이 정말 중요해요",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017591.1.14",
          "form": "그쵸 요즘 더 느껴요",
          "original_form": "그쵸 요즘 더 느껴요",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017591.1.15",
          "form": "젊을때  잘 챙겨야해요",
          "original_form": "젊을때  잘 챙겨야해요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017591.1.16",
          "form": "코로나때문에 더 건강의 중요성을 느껴요",
          "original_form": "코로나때문에 더 건강의 중요성을 느껴요",
