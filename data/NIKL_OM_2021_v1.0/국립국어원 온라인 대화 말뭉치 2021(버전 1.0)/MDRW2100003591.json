      "utterance": [
        {
          "id": "MDRW2100003591.1.1",
          "form": "요즘 날씨 되게 시원하지 않나요?",
          "original_form": "요즘 날씨 되게 시원하지 않나요?",
          "speaker_id": "1",
          "time": "20210821 03:04"
        },
        {
          "id": "MDRW2100003591.1.2",
          "form": "맞아요 진짜 시원해졌어요",
          "original_form": "맞아요 진짜 시원해졌어요",
          "speaker_id": "2",
          "time": "20210821 03:04"
        },
        {
          "id": "MDRW2100003591.1.3",
          "form": "이제 가을인거같아요",
          "original_form": "이제 가을인거같아요",
          "speaker_id": "2",
          "time": "20210821 03:04"
        },
        {
          "id": "MDRW2100003591.1.4",
          "form": "그쵸 아직 8월인데",
          "original_form": "그쵸 아직 8월인데",
          "speaker_id": "1",
          "time": "20210821 03:04"
        },
        {
          "id": "MDRW2100003591.1.5",
          "form": "올해는 빨리 선선해진 느낌",
          "original_form": "올해는 빨리 선선해진 느낌",
          "speaker_id": "1",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003591.1.6",
          "form": "입추가 8월 초였나..?",
          "original_form": "입추가 8월 초였나..?",
          "speaker_id": "2",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003591.1.7",
          "form": "언제인지 모르겠는데 그때이후로",
          "original_form": "언제인지 모르겠는데 그때이후로",
          "speaker_id": "2",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003591.1.8",
          "form": "확 시원해진거같아요",
          "original_form": "확 시원해진거같아요",
          "speaker_id": "2",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003591.1.9",
          "form": "와 딱 입추부터였나요?",
          "original_form": "와 딱 입추부터였나요?",
          "speaker_id": "1",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003591.1.10",
          "form": "되게 칼같네요",
          "original_form": "되게 칼같네요",
          "speaker_id": "1",
          "time": "20210821 03:05"
        },
        {
          "id": "MDRW2100003591.1.11",
          "form": "진짜 조상의 지혜",
          "original_form": "진짜 조상의 지혜",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.12",
          "form": "입추부터 엄청 시원해졌어요",
          "original_form": "입추부터 엄청 시원해졌어요",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.13",
          "form": "그래서 후드집업 입고다녀요 ㅋㅋㅋ",
          "original_form": "그래서 후드집업 입고다녀요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.14",
          "form": "신기해요ㅋㅋㅋ",
          "original_form": "신기해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.15",
          "form": "이제 가을옷 꺼내야될 때인가",
          "original_form": "이제 가을옷 꺼내야될 때인가",
          "speaker_id": "1",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.16",
          "form": "근데 오늘부터 비온다고 하더라구요",
          "original_form": "근데 오늘부터 비온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.17",
          "form": "앗 정말요?",
          "original_form": "앗 정말요?",
          "speaker_id": "1",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.18",
          "form": "슬슬 자켓같은거 걸쳐도 안더울거같아요",
          "original_form": "슬슬 자켓같은거 걸쳐도 안더울거같아요",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.19",
          "form": "네네 오늘 서울은 비왔어요 ㅠㅠ 아까밤에",
          "original_form": "네네 오늘 서울은 비왔어요 ㅠㅠ 아까밤에",
          "speaker_id": "2",
          "time": "20210821 03:06"
        },
        {
          "id": "MDRW2100003591.1.20",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210821 03:07"
        },
        {
          "id": "MDRW2100003591.1.21",
          "form": "어차피 오늘 안 나갈 예정이라 ㅋㅋㅋ",
          "original_form": "어차피 오늘 안 나갈 예정이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 03:07"
        },
        {
          "id": "MDRW2100003591.1.22",
          "form": "아 ㅋㅋㅋ!! 비오는날 안나가면 좋죠!",
          "original_form": "아 ㅋㅋㅋ!! 비오는날 안나가면 좋죠!",
          "speaker_id": "2",
          "time": "20210821 03:07"
        },
        {
          "id": "MDRW2100003591.1.23",
          "form": "빗소리 듣고 좋을거같아요 ㅋㅋㅋ!!",
          "original_form": "빗소리 듣고 좋을거같아요 ㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210821 03:07"
        },
        {
          "id": "MDRW2100003591.1.24",
          "form": "아 맞아요 너무 좋아요",
          "original_form": "아 맞아요 너무 좋아요",
          "speaker_id": "1",
          "time": "20210821 03:07"
        },
        {
          "id": "MDRW2100003591.1.25",
          "form": "요즘 미세먼지 별로없는거 같지 않아요?",
          "original_form": "요즘 미세먼지 별로없는거 같지 않아요?",
          "speaker_id": "2",
          "time": "20210821 03:08"
        },
        {
          "id": "MDRW2100003591.1.26",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "1",
          "time": "20210821 03:08"
        }
