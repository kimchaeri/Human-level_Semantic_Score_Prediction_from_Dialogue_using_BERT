      "utterance": [
        {
          "id": "MDRW2100023937.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023937.1.2",
          "form": "중국이나 일본여행 해봤어요?",
          "original_form": "중국이나 일본여행 해봤어요?",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023937.1.3",
          "form": "아니요ㅜㅜ",
          "original_form": "아니요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023937.1.4",
          "form": "가보신적 있으세요?",
          "original_form": "가보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023937.1.5",
          "form": "네~! 저 중국여행 가본 적 있어요!ㅎㅎ",
          "original_form": "네~! 저 중국여행 가본 적 있어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023937.1.6",
          "form": "오 어디를 가보셨어요?",
          "original_form": "오 어디를 가보셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023937.1.7",
          "form": "중국에 항저우 어딘지 아세요?",
          "original_form": "중국에 항저우 어딘지 아세요?",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023937.1.8",
          "form": "네네 오 코로나 전에 갔다오신거죠?",
          "original_form": "네네 오 코로나 전에 갔다오신거죠?",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023937.1.9",
          "form": "그럼요 한참 예전에 다녀왔어요~",
          "original_form": "그럼요 한참 예전에 다녀왔어요~",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023937.1.10",
          "form": "거기 밑에 항저우도 아세요? 사실 항저우를 먼저 갔어요",
          "original_form": "거기 밑에 항저우도 아세요? 사실 항저우를 먼저 갔어요",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023937.1.11",
          "form": "네네 그리고 항저우랑 어디가셨어요?",
          "original_form": "네네 그리고 항저우랑 어디가셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023937.1.12",
          "form": "항저우랑 상하이 다녀온게 다네요 ㅜㅜ",
          "original_form": "항저우랑 상하이 다녀온게 다네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023937.1.13",
          "form": "거기에 우전여행 했어요 되게 옛스럽더라구요",
          "original_form": "거기에 우전여행 했어요 되게 옛스럽더라구요",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023937.1.14",
          "form": "앗 우전여행이 뭔가요?",
          "original_form": "앗 우전여행이 뭔가요?",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100023937.1.15",
          "form": "그냥 상하이 여행 하는건데 우전이라는 지역이 있어요!",
          "original_form": "그냥 상하이 여행 하는건데 우전이라는 지역이 있어요!",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023937.1.16",
          "form": "거기 지역을 방문해서 둘러보고 관광하는걸 우전여행이라고 해요",
          "original_form": "거기 지역을 방문해서 둘러보고 관광하는걸 우전여행이라고 해요",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023937.1.17",
          "form": "아하 지역이름 이었군요",
          "original_form": "아하 지역이름 이었군요",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023937.1.18",
          "form": "저는 일드를 한때 열심히봐서요",
          "original_form": "저는 일드를 한때 열심히봐서요",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023937.1.19",
          "form": "일본에 대한 로망이  좀 있어요 ㅋㅋ",
          "original_form": "일본에 대한 로망이  좀 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023937.1.20",
          "form": "네네 ㅋㅋ 그럼 일본 아직 여행 안가보신거죠?",
          "original_form": "네네 ㅋㅋ 그럼 일본 아직 여행 안가보신거죠?",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100023937.1.21",
          "form": "일본 어디 가보고 싶어요?",
          "original_form": "일본 어디 가보고 싶어요?",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100023937.1.22",
          "form": "네 안가봤는데 뭔가 일본문화를 경험하고 싶어요 ㅋㅋ",
          "original_form": "네 안가봤는데 뭔가 일본문화를 경험하고 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100023937.1.23",
          "form": "드라마나오는 편의점이나",
          "original_form": "드라마나오는 편의점이나",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100023937.1.24",
          "form": "아 일본 전통 여관? 이라던가!",
          "original_form": "아 일본 전통 여관? 이라던가!",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100023937.1.25",
          "form": "아 뭔지 알아요! ㅋㅋㅋ",
          "original_form": "아 뭔지 알아요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023937.1.26",
          "form": "그 일본전통 가옥같은거 말씀이죠?",
          "original_form": "그 일본전통 가옥같은거 말씀이죠?",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023937.1.27",
          "form": "막 기모노 같은 것도 입고 ㅎㅎ",
          "original_form": "막 기모노 같은 것도 입고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023937.1.28",
          "form": "료칸이라고도하고",
          "original_form": "료칸이라고도하고",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100023937.1.29",
          "form": "온천도 궁금하고요ㅋㅋㅋ",
          "original_form": "온천도 궁금하고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100023937.1.30",
          "form": "어 맞아요 ㅠㅠ 일본 온천이 그렇게 좋다던데",
          "original_form": "어 맞아요 ㅠㅠ 일본 온천이 그렇게 좋다던데",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023937.1.31",
          "form": "저도 가보면 온천체험 한 번 해보고 싶어요.. 로망",
          "original_form": "저도 가보면 온천체험 한 번 해보고 싶어요.. 로망",
          "speaker_id": "1",
          "time": "20210830 23:43"
        }
      ]
    }
