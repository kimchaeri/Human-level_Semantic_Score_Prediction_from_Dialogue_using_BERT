      "utterance": [
        {
          "id": "MDRW2100045151.1.1",
          "form": "아니오ㅋㅋㅋㅋ 분모자 좋아하세요?",
          "original_form": "아니오ㅋㅋㅋㅋ 분모자 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045151.1.2",
          "form": "분모자 쫄깃해서 맛나더라고요",
          "original_form": "분모자 쫄깃해서 맛나더라고요",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045151.1.3",
          "form": "떡도 아닌게...",
          "original_form": "떡도 아닌게...",
          "speaker_id": "1",
          "time": "20210908 01:03"
        },
        {
          "id": "MDRW2100045151.1.4",
          "form": "저는 그거 말고 연근이나 단호박 넣어요",
          "original_form": "저는 그거 말고 연근이나 단호박 넣어요",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045151.1.5",
          "form": "연근 넣는 건 처음 봤어요",
          "original_form": "연근 넣는 건 처음 봤어요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045151.1.6",
          "form": "호불호 갈리는 재료라",
          "original_form": "호불호 갈리는 재료라",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045151.1.7",
          "form": "많이는 안 넣고 몇 개 넣으면 아삭아삭해서 좋아요",
          "original_form": "많이는 안 넣고 몇 개 넣으면 아삭아삭해서 좋아요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045151.1.8",
          "form": "마라탕 재료 딱 3개만 고를 수 있다고 하면 뭐 넣으실래요?",
          "original_form": "마라탕 재료 딱 3개만 고를 수 있다고 하면 뭐 넣으실래요?",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045151.1.9",
          "form": "음.... 일단 숙주랑 청경채랑 라면아님 당면이요",
          "original_form": "음.... 일단 숙주랑 청경채랑 라면아님 당면이요",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045151.1.10",
          "form": "앗 고기 고기 넣어야하는대",
          "original_form": "앗 고기 고기 넣어야하는대",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045151.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045151.1.12",
          "form": "3개만 골라야 돼요",
          "original_form": "3개만 골라야 돼요",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045151.1.13",
          "form": "청경채 대신 고기 넣으게요",
          "original_form": "청경채 대신 고기 넣으게요",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045151.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045151.1.15",
          "form": "양고기요? 아님 소고기?",
          "original_form": "양고기요? 아님 소고기?",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045151.1.16",
          "form": "소고기요",
          "original_form": "소고기요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045151.1.17",
          "form": "전 양고기 판데...",
          "original_form": "전 양고기 판데...",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045151.1.18",
          "form": "전 숙주 배추 건두부 만 있어도 잘 먹어요 ㅋㅋㅋ",
          "original_form": "전 숙주 배추 건두부 만 있어도 잘 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045151.1.19",
          "form": "고기 안 넣어도 괜찮으세요?",
          "original_form": "고기 안 넣어도 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045151.1.20",
          "form": "전 없어도 별 차이 못 느끼겠더라고요 ㅋㅋㅋ",
          "original_form": "전 없어도 별 차이 못 느끼겠더라고요 ㅋㅋㅋ",
