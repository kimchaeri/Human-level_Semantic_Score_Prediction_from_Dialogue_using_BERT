      "utterance": [
        {
          "id": "MDRW2100032615.1.1",
          "form": "마켓칼리 자주 써?",
          "original_form": "마켓칼리 자주 써?",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100032615.1.2",
          "form": "쿠폰을 계속주길래 줄때마다 썻어 ㅋㅋㅋㅋ",
          "original_form": "쿠폰을 계속주길래 줄때마다 썻어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032615.1.3",
          "form": "쿠폰 자주 주나보네 ㅋㅋㅋ",
          "original_form": "쿠폰 자주 주나보네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032615.1.4",
          "form": "신규가입자라서 그런가봨ㅋㅋㅋㅋ",
          "original_form": "신규가입자라서 그런가봨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032615.1.5",
          "form": "맛잇는거많드라",
          "original_form": "맛잇는거많드라",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032615.1.6",
          "form": "오 이번에 신규가입?",
          "original_form": "오 이번에 신규가입?",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032615.1.7",
          "form": "ㅇㅇㅇ요번에 첨가입햇서",
          "original_form": "ㅇㅇㅇ요번에 첨가입햇서",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032615.1.8",
          "form": "ㅋㅋㅋ친구 말로는 밀키트는",
          "original_form": "ㅋㅋㅋ친구 말로는 밀키트는",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032615.1.9",
          "form": "쿠팡보다 마켓컬리가 맛잇대",
          "original_form": "쿠팡보다 마켓컬리가 맛잇대",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032615.1.10",
          "form": "오 유명하군 어쩐지 컬리단독제품있더라구",
          "original_form": "오 유명하군 어쩐지 컬리단독제품있더라구",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032615.1.11",
          "form": "또 맛있는거 있었나여",
          "original_form": "또 맛있는거 있었나여",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032615.1.12",
          "form": "우유식빵맛잇어여",
          "original_form": "우유식빵맛잇어여",
          "speaker_id": "1",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032615.1.13",
          "form": "버터는 이즈니버터 추천핮이다",
          "original_form": "버터는 이즈니버터 추천핮이다",
          "speaker_id": "1",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032615.1.14",
          "form": "컬리에서 만든?",
          "original_form": "컬리에서 만든?",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032615.1.15",
          "form": "노노 버터브랜드인데 컬리에잇어 다른데도팔걸??",
          "original_form": "노노 버터브랜드인데 컬리에잇어 다른데도팔걸??",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032615.1.16",
          "form": "버터종류 다양하게팔도라",
          "original_form": "버터종류 다양하게팔도라",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032615.1.17",
          "form": "아하 식빵 이런거 뭔가 한번 사면",
          "original_form": "아하 식빵 이런거 뭔가 한번 사면",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032615.1.18",
          "form": "주구장창 먹어야하니까 사기 어렵더라...",
          "original_form": "주구장창 먹어야하니까 사기 어렵더라...",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032615.1.19",
          "form": "식빵은 냉동하면대 오래먹을수잇따",
          "original_form": "식빵은 냉동하면대 오래먹을수잇따",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032615.1.20",
          "form": "근데 살찌니까 비추...",
          "original_form": "근데 살찌니까 비추...",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032615.1.21",
          "form": "ㅋㅋㅋㅋㅋ저번에 신세계 지하에",
          "original_form": "ㅋㅋㅋㅋㅋ저번에 신세계 지하에",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032615.1.22",
          "form": "도제식빵? 팝업 스토어 열렸는데",
          "original_form": "도제식빵? 팝업 스토어 열렸는데",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032615.1.23",
          "form": "갓 구운 식빵...너무 탐났지만 참아따ㅠ",
          "original_form": "갓 구운 식빵...너무 탐났지만 참아따ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032615.1.24",
          "form": "오 먼가드렁본거같애",
          "original_form": "오 먼가드렁본거같애",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032615.1.25",
          "form": "와 갓구운식빵 미쳣지",
          "original_form": "와 갓구운식빵 미쳣지",
          "speaker_id": "1",
          "time": "20210905 21:30"
        }
      ]
