      "utterance": [
        {
          "id": "MDRW2100032401.1.1",
          "form": "만일 어떤 것을 하시나요",
          "original_form": "만일 어떤 것을 하시나요",
          "speaker_id": "2",
          "time": "20210905 16:57"
        },
        {
          "id": "MDRW2100032401.1.2",
          "form": "집안일이요? 청소를 합니다",
          "original_form": "집안일이요? 청소를 합니다",
          "speaker_id": "1",
          "time": "20210905 16:57"
        },
        {
          "id": "MDRW2100032401.1.3",
          "form": "청소는 티가 안 나서 쉬운 일은 아닌 거 같은데",
          "original_form": "청소는 티가 안 나서 쉬운 일은 아닌 거 같은데",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.4",
          "form": "청소도 어렵고 저는 요리를 못합니다",
          "original_form": "청소도 어렵고 저는 요리를 못합니다",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.5",
          "form": "각자가 잘하는 걸 하면 효율적인 했네요",
          "original_form": "각자가 잘하는 걸 하면 효율적인 했네요",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.6",
          "form": "각자가 잘하는 걸 하면 효율적이 했",
          "original_form": "각자가 잘하는 걸 하면 효율적이 했",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.7",
          "form": "맞아요 저는 차라리 청소가 낫더라구요",
          "original_form": "맞아요 저는 차라리 청소가 낫더라구요",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.8",
          "form": "혼자 사시나요",
          "original_form": "혼자 사시나요",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.9",
          "form": "아니요 부모님과 삽니다",
          "original_form": "아니요 부모님과 삽니다",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.10",
          "form": "어머니가 요리를 하셨군요",
          "original_form": "어머니가 요리를 하셨군요",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032401.1.11",
          "form": "네 어머니 요리가 맛있습니다",
          "original_form": "네 어머니 요리가 맛있습니다",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032401.1.12",
          "form": "저는 집안일을 좋아해요",
          "original_form": "저는 집안일을 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032401.1.13",
          "form": "일이라고 생각하면 힘들지요",
          "original_form": "일이라고 생각하면 힘들지요",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032401.1.14",
          "form": "가족",
          "original_form": "가족",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032401.1.15",
          "form": "가족과 함께 사는건 감사한일이에요",
          "original_form": "가족과 함께 사는건 감사한일이에요",
