      "utterance": [
        {
          "id": "MDRW2100014419.1.1",
          "form": "반려동물 기르시나여??",
          "original_form": "반려동물 기르시나여??",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014419.1.2",
          "form": "아뇨ㅠ키우고싶은데 낮에 사람이 없어서 못키워요ㅠ",
          "original_form": "아뇨ㅠ키우고싶은데 낮에 사람이 없어서 못키워요ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014419.1.3",
          "form": "아 그러시구나 ㅜㅜ 저는 금붕어만 길러요 ㅋㅋㅋ",
          "original_form": "아 그러시구나 ㅜㅜ 저는 금붕어만 길러요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014419.1.4",
          "form": "저는 옛날에 길렀었는데 생각보다 잘죽더라구요ㅠ",
          "original_form": "저는 옛날에 길렀었는데 생각보다 잘죽더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014419.1.5",
          "form": "엄청 오래살아남는 애들은 또 엄청 오래살아남고ㅜㅋㅋㅋ",
          "original_form": "엄청 오래살아남는 애들은 또 엄청 오래살아남고ㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014419.1.6",
          "form": "맞아요,, 종류에따라 관리법이 천차만별이라 ㅋㅋ",
          "original_form": "맞아요,, 종류에따라 관리법이 천차만별이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014419.1.7",
          "form": "저는 그냥 가장 기르기쉬운 구피 길러요 ㅋㅋㅋ",
          "original_form": "저는 그냥 가장 기르기쉬운 구피 길러요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014419.1.8",
          "form": "우와 구피는 기르기쉬운가요?",
          "original_form": "우와 구피는 기르기쉬운가요?",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014419.1.9",
          "form": "많이들 기르긴하더라구요 예쁘기도예쁘고ㅋㅋㅋ",
          "original_form": "많이들 기르긴하더라구요 예쁘기도예쁘고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:21"
        },
        {
          "id": "MDRW2100014419.1.10",
          "form": "구피 밥만 주면 알아서 잘크는거같아요ㅋㅋ",
          "original_form": "구피 밥만 주면 알아서 잘크는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014419.1.11",
          "form": "알도많이낳고요..",
          "original_form": "알도많이낳고요..",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014419.1.12",
          "form": "헐 그럼 계속 증식하겠네요...?",
          "original_form": "헐 그럼 계속 증식하겠네요...?",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014419.1.13",
          "form": "오래기르다보면 수조가 복작복작하겠는데요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오래기르다보면 수조가 복작복작하겠는데요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014419.1.14",
          "form": "맞아요,, 그래서 필히 분양해야해요,,",
          "original_form": "맞아요,, 그래서 필히 분양해야해요,,",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014419.1.15",
          "form": "감당안된적두많아요 ㅋㅋ",
          "original_form": "감당안된적두많아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014419.1.16",
          "form": "앜 그정도에요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜 그정도에요?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014419.1.17",
          "form": "번식력 엄청나네요ㅋㅋㅋ",
          "original_form": "번식력 엄청나네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014419.1.18",
          "form": "네네 ㅜㅜ맞아요",
          "original_form": "네네 ㅜㅜ맞아요",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014419.1.19",
          "form": "기르는재미?가 있을것같아요ㅋㅋㅋ",
          "original_form": "기르는재미?가 있을것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014419.1.20",
          "form": "보람도있구ㅋㅋㅋ",
          "original_form": "보람도있구ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014419.1.21",
          "form": "분양할때 뿌듯할거같아요",
          "original_form": "분양할때 뿌듯할거같아요",
