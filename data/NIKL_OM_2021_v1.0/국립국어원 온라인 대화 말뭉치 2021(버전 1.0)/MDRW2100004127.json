      "utterance": [
        {
          "id": "MDRW2100004127.1.1",
          "form": "일 하고 계세요?",
          "original_form": "일 하고 계세요?",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004127.1.2",
          "form": "저 취준생이에요ㅋㅋ",
          "original_form": "저 취준생이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:21"
        },
        {
          "id": "MDRW2100004127.1.3",
          "form": "오 취업준비중이시군요!!!",
          "original_form": "오 취업준비중이시군요!!!",
          "speaker_id": "1",
          "time": "20210821 18:33"
        },
        {
          "id": "MDRW2100004127.1.4",
          "form": "네..사실상 백수죠...:)",
          "original_form": "네..사실상 백수죠...:)",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004127.1.5",
          "form": "일..하고싶어요..",
          "original_form": "일..하고싶어요..",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004127.1.6",
          "form": "요새 일자리가 정말 박봉이더라구요 ㅠㅠ",
          "original_form": "요새 일자리가 정말 박봉이더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004127.1.7",
          "form": "하..그쵸",
          "original_form": "하..그쵸",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100004127.1.8",
          "form": "저도할줄아는게없긴하지만..경쟁률미쳤어오ㅓ",
          "original_form": "저도할줄아는게없긴하지만..경쟁률미쳤어오ㅓ",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100004127.1.9",
          "form": "코로나 때문에 더 그렁 거 같아요",
          "original_form": "코로나 때문에 더 그렁 거 같아요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100004127.1.10",
          "form": "하필 코시국에 취준생이 되어버려서",
          "original_form": "하필 코시국에 취준생이 되어버려서",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100004127.1.11",
          "form": "ㅋㅋㅋ주변친구들은 공시마니하더라고요",
          "original_form": "ㅋㅋㅋ주변친구들은 공시마니하더라고요",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100004127.1.12",
          "form": "제 주변에두요",
          "original_form": "제 주변에두요",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100004127.1.13",
          "form": "공뭔 좋지만 전 공부와 연이 없어서 하하",
          "original_form": "공뭔 좋지만 전 공부와 연이 없어서 하하",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100004127.1.14",
          "form": "ㅋㅋㅋ저도 그래서 공시할까하다가 일단취준을하고있어오ㅓ",
          "original_form": "ㅋㅋㅋ저도 그래서 공시할까하다가 일단취준을하고있어오ㅓ",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100004127.1.15",
          "form": "공부너무하기싫어서..ㅎ",
          "original_form": "공부너무하기싫어서..ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100004127.1.16",
          "form": "맞아요.. 흑.. 공부 오래 할 자신도 없고",
          "original_form": "맞아요.. 흑.. 공부 오래 할 자신도 없고",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100004127.1.17",
          "form": "그냥 다 모르겠어요",
          "original_form": "그냥 다 모르겠어요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100004127.1.18",
          "form": "저도요.. 진짜 한치앞도모르겠어요 지금은ㅠ 얼른 자리잡고싶은데..",
          "original_form": "저도요.. 진짜 한치앞도모르겠어요 지금은ㅠ 얼른 자리잡고싶은데..",
