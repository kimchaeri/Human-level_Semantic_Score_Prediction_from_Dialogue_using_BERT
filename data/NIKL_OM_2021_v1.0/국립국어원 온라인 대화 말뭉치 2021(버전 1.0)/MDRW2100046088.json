      "utterance": [
        {
          "id": "MDRW2100046088.1.1",
          "form": "아침 드셨어요??",
          "original_form": "아침 드셨어요??",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046088.1.2",
          "form": "이직요 ㅠㅠ",
          "original_form": "이직요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046088.1.3",
          "form": "빵먹으려했는데 ㅠㅠ",
          "original_form": "빵먹으려했는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046088.1.4",
          "form": "좀 드셔요!!",
          "original_form": "좀 드셔요!!",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046088.1.5",
          "form": "전 오늘도 두유",
          "original_form": "전 오늘도 두유",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046088.1.6",
          "form": "애가 안떨어져요ㅠㅠㅠ",
          "original_form": "애가 안떨어져요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046088.1.7",
          "form": "건강음료네여",
          "original_form": "건강음료네여",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046088.1.8",
          "form": "아이코야",
          "original_form": "아이코야",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046088.1.9",
          "form": "귀찮을때 두유만한게 없어요",
          "original_form": "귀찮을때 두유만한게 없어요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046088.1.10",
          "form": "아 이런.신랑이 모카빵을 먹었나바여ㅠㅠ",
          "original_form": "아 이런.신랑이 모카빵을 먹었나바여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046088.1.11",
          "form": "업네요ㅠㅠㅠㅠㅠ 소보로 먹어야게따ㅠ",
          "original_form": "업네요ㅠㅠㅠㅠㅠ 소보로 먹어야게따ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:17"
        },
        {
          "id": "MDRW2100046088.1.12",
          "form": "소보로도 맛있겠네요",
          "original_form": "소보로도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046088.1.13",
          "form": "우유랑 드시면 좋겠어요ㅋㅋㅋ",
          "original_form": "우유랑 드시면 좋겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046088.1.14",
          "form": "아메랑 먹으려구요^",
          "original_form": "아메랑 먹으려구요^",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046088.1.15",
          "form": "오 아메리카노도 좋네욯ㅎㅎ",
          "original_form": "오 아메리카노도 좋네욯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046088.1.16",
          "form": "네 ㅋㅋ 얼음컵에 더치 부어서 딱!",
          "original_form": "네 ㅋㅋ 얼음컵에 더치 부어서 딱!",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046088.1.17",
          "form": "향이 참 좋을거 같아요",
          "original_form": "향이 참 좋을거 같아요",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046088.1.18",
          "form": "네 요즘은 더치도 일회용 봉지에 나오더라구요",
          "original_form": "네 요즘은 더치도 일회용 봉지에 나오더라구요",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046088.1.19",
          "form": "좋네요 쓸모없는 쓰.레기 없어서요!!",
          "original_form": "좋네요 쓸모없는 쓰.레기 없어서요!!",
