      "utterance": [
        {
          "id": "MDRW2100002990.1.1",
          "form": "ㅇㅇ 지금 쓰는 거 너무 오래돼서 바꿔야해",
          "original_form": "ㅇㅇ 지금 쓰는 거 너무 오래돼서 바꿔야해",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002990.1.2",
          "form": "이마에 대는 게 편할라나",
          "original_form": "이마에 대는 게 편할라나",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002990.1.3",
          "form": "ㅇㅇ 그게 편하긴 한데 온도가 낮게 나와서",
          "original_form": "ㅇㅇ 그게 편하긴 한데 온도가 낮게 나와서",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002990.1.4",
          "form": "그럼 귀체온계로 해?",
          "original_form": "그럼 귀체온계로 해?",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002990.1.5",
          "form": "그게 정확하다고 하더라고 ㅎㅎ",
          "original_form": "그게 정확하다고 하더라고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002990.1.6",
          "form": "귀체온계는 잴 때 좀 불편하던데",
          "original_form": "귀체온계는 잴 때 좀 불편하던데",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002990.1.7",
          "form": "그래도 요즘같은 시국엔 정확해야지",
          "original_form": "그래도 요즘같은 시국엔 정확해야지",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002990.1.8",
          "form": "하긴 열나는데 잘못 재면 안되니깐",
          "original_form": "하긴 열나는데 잘못 재면 안되니깐",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002990.1.9",
          "form": "ㅇㅇ 브라운 꺼 좋으니까 한 번 알아봐요",
          "original_form": "ㅇㅇ 브라운 꺼 좋으니까 한 번 알아봐요",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002990.1.10",
          "form": "ㅇㅇ 알았어 ㅎㅎ",
          "original_form": "ㅇㅇ 알았어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002990.1.11",
          "form": "가격은 아마 10만원 정도 하는 것 같아",
          "original_form": "가격은 아마 10만원 정도 하는 것 같아",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002990.1.12",
          "form": "직구하면 더 저렴할걸",
          "original_form": "직구하면 더 저렴할걸",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002990.1.13",
          "form": "근데 배송이 오래 걸릴껴",
          "original_form": "근데 배송이 오래 걸릴껴",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002990.1.14",
          "form": "부모님 혈압계도 알아봐야 해",
          "original_form": "부모님 혈압계도 알아봐야 해",
