        },
        {
          "id": "MDRW2100025960.1.2",
          "form": "저도 여건이",
          "original_form": "저도 여건이",
          "speaker_id": "1",
          "time": "20210901 19:23"
        },
        {
          "id": "MDRW2100025960.1.3",
          "form": "키우고 싶은 동물 있어요?",
          "original_form": "키우고 싶은 동물 있어요?",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100025960.1.4",
          "form": "강아지오",
          "original_form": "강아지오",
          "speaker_id": "1",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100025960.1.5",
          "form": "강아지 귀엽죠",
          "original_form": "강아지 귀엽죠",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100025960.1.6",
          "form": "귀요미죠",
          "original_form": "귀요미죠",
          "speaker_id": "1",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100025960.1.7",
          "form": "산책하면 자주 보여요",
          "original_form": "산책하면 자주 보여요",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100025960.1.8",
          "form": "케억 힘들죠",
          "original_form": "케억 힘들죠",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100025960.1.9",
          "form": "안고 다니는 사람도 많아요",
          "original_form": "안고 다니는 사람도 많아요",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100025960.1.10",
          "form": "유모차도 있구요",
          "original_form": "유모차도 있구요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100025960.1.11",
          "form": "맞아요ㅋㅋ 소형견들이요",
