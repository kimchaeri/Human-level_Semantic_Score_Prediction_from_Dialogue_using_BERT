        },
        {
          "id": "MDRW2100038221.1.2",
          "form": "엉 유선이 나오네",
          "original_form": "엉 유선이 나오네",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038221.1.3",
          "form": "다른 게스트 나오나보네",
          "original_form": "다른 게스트 나오나보네",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038221.1.4",
          "form": "엉 유선 나온거 봣는데 정말 열정적",
          "original_form": "엉 유선 나온거 봣는데 정말 열정적",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038221.1.5",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038221.1.6",
          "form": "쉬지 않고 계속 뭘 하고 배움",
          "original_form": "쉬지 않고 계속 뭘 하고 배움",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038221.1.7",
          "form": "쉬고 싶어서 해방타운 나온거 아니야",
          "original_form": "쉬고 싶어서 해방타운 나온거 아니야",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038221.1.8",
          "form": "그러니까 다른 게스트랑 좀 틀림",
          "original_form": "그러니까 다른 게스트랑 좀 틀림",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038221.1.9",
          "form": "그러게 다들 쉬던데",
          "original_form": "그러게 다들 쉬던데",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038221.1.10",
          "form": "등산도 가고 뭘 만드고 심지어 오늘은 배우러 가네",
          "original_form": "등산도 가고 뭘 만드고 심지어 오늘은 배우러 가네",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038221.1.11",
          "form": "뭘 배우는데",
          "original_form": "뭘 배우는데",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038221.1.12",
          "form": "용기를 내어서 배우러 온거라는데 모르겠어",
          "original_form": "용기를 내어서 배우러 온거라는데 모르겠어",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038221.1.13",
          "form": "쉬운게 아닌갑다",
          "original_form": "쉬운게 아닌갑다",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038221.1.14",
          "form": "헉 스쿠버다이빙인갑다",
          "original_form": "헉 스쿠버다이빙인갑다",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038221.1.15",
          "form": "윤식당 제방도 한다",
          "original_form": "윤식당 제방도 한다",
