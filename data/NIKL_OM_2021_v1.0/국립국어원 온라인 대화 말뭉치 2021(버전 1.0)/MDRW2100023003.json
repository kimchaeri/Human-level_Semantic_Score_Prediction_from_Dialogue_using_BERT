      "utterance": [
        {
          "id": "MDRW2100023003.1.1",
          "form": "윽 나도 오이소박이 싫어",
          "original_form": "윽 나도 오이소박이 싫어",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023003.1.2",
          "form": "또 나박김치도 싫어",
          "original_form": "또 나박김치도 싫어",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023003.1.3",
          "form": "오리지널이 좋아",
          "original_form": "오리지널이 좋아",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100023003.1.4",
          "form": "진짜? 나는 나박김치 좋아하는데",
          "original_form": "진짜? 나는 나박김치 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023003.1.5",
          "form": "어떤점이 싫어?",
          "original_form": "어떤점이 싫어?",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023003.1.6",
          "form": "이름이 마음에안들어",
          "original_form": "이름이 마음에안들어",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023003.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023003.1.8",
          "form": "나박이 어때서",
          "original_form": "나박이 어때서",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023003.1.9",
          "form": "나박이라니",
          "original_form": "나박이라니",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023003.1.10",
          "form": "넌 와사비 안먹니",
          "original_form": "넌 와사비 안먹니",
          "speaker_id": "1",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023003.1.11",
          "form": "음 스시같은 데에 조금 들어간 건 먹는데",
          "original_form": "음 스시같은 데에 조금 들어간 건 먹는데",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023003.1.12",
          "form": "다른 때는 거의 안 찾아",
          "original_form": "다른 때는 거의 안 찾아",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023003.1.13",
          "form": "넌 와사비 좋아해?",
          "original_form": "넌 와사비 좋아해?",
          "speaker_id": "2",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100023003.1.14",
          "form": "요즘 와사비 간장맛에 빠져서",
          "original_form": "요즘 와사비 간장맛에 빠져서",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023003.1.15",
          "form": "자주먹어",
          "original_form": "자주먹어",
          "speaker_id": "1",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023003.1.16",
          "form": "오오 너 매운거 잘 먹는 편이야?",
          "original_form": "오오 너 매운거 잘 먹는 편이야?",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023003.1.17",
          "form": "매운건 못먹는데",
          "original_form": "매운건 못먹는데",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023003.1.18",
          "form": "와사비 특유의 향이 너무좋더라",
          "original_form": "와사비 특유의 향이 너무좋더라",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100023003.1.19",
          "form": "그럼 괴로워하면서 향이 좋아서 먹는거니?",
          "original_form": "그럼 괴로워하면서 향이 좋아서 먹는거니?",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023003.1.20",
          "form": "와사비 맵지않나",
          "original_form": "와사비 맵지않나",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023003.1.21",
          "form": "응 엄청맵지",
          "original_form": "응 엄청맵지",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023003.1.22",
          "form": "근데 생와사비는 좀 덜맵대",
          "original_form": "근데 생와사비는 좀 덜맵대",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023003.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ매운데 참고 먹는거구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ매운데 참고 먹는거구나",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100023003.1.24",
          "form": "너는 매운거 좋아하치?",
          "original_form": "너는 매운거 좋아하치?",
          "speaker_id": "1",
          "time": "20210829 20:44"
        }
      ]
    }
  ]
