      "utterance": [
        {
          "id": "MDRW2100024472.1.1",
          "form": "오늘은 정말 습해요... 저기압에다가....",
          "original_form": "오늘은 정말 습해요... 저기압에다가....",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024472.1.2",
          "form": "근데 장우산은 무거워서",
          "original_form": "근데 장우산은 무거워서",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024472.1.3",
          "form": "그냥 3단 우산 가져왔어요",
          "original_form": "그냥 3단 우산 가져왔어요",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024472.1.4",
          "form": "날씨 진짜 꾸물꾸물",
          "original_form": "날씨 진짜 꾸물꾸물",
          "speaker_id": "1",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024472.1.5",
          "form": "전 걍 쬐그만 양우산가져옴",
          "original_form": "전 걍 쬐그만 양우산가져옴",
          "speaker_id": "1",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024472.1.6",
          "form": "많이 올것같지는 않아서요",
          "original_form": "많이 올것같지는 않아서요",
          "speaker_id": "1",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024472.1.7",
          "form": "잘 하셨어요!!!",
          "original_form": "잘 하셨어요!!!",
          "speaker_id": "2",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024472.1.8",
          "form": "저는 아까 학교 오는 길에",
          "original_form": "저는 아까 학교 오는 길에",
          "speaker_id": "2",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024472.1.9",
          "form": "빗방울 조금 맞았는데",
          "original_form": "빗방울 조금 맞았는데",
          "speaker_id": "2",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024472.1.10",
          "form": "지금 창밖을 보면 비는 또 안오네요? ",
          "original_form": "지금 창밖을 보면 비는 또 안오네요? ",
          "speaker_id": "2",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100024472.1.11",
          "form": "근데 좀 있다가는 또 올거같고요",
          "original_form": "근데 좀 있다가는 또 올거같고요",
          "speaker_id": "1",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100024472.1.12",
          "form": "이따가 폐수 버리러 가야하는데",
          "original_form": "이따가 폐수 버리러 가야하는데",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100024472.1.13",
          "form": "그 때만큼은 비 안왔으면 좋겠네요 8ㅅ8",
          "original_form": "그 때만큼은 비 안왔으면 좋겠네요 8ㅅ8",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100024472.1.14",
          "form": "엇! 지금 엄청 와요",
          "original_form": "엇! 지금 엄청 와요",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024472.1.15",
          "form": "빗방울이... 굵어요......",
          "original_form": "빗방울이... 굵어요......",
          "speaker_id": "2",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024472.1.16",
          "form": "오늘 우산 작은거 가져왔는데!",
          "original_form": "오늘 우산 작은거 가져왔는데!",
          "speaker_id": "2",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024472.1.17",
          "form": "네, 비오는 소리가 거세네요",
          "original_form": "네, 비오는 소리가 거세네요",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024472.1.18",
          "form": "나도 우산 작은데",
          "original_form": "나도 우산 작은데",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024472.1.19",
          "form": "추워서 긴팔 긴바지를 입어야하는데",
          "original_form": "추워서 긴팔 긴바지를 입어야하는데",
          "speaker_id": "2",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100024472.1.20",
          "form": "습해서 그건 또 그거대로 찝찝해요 ㅋㅋㅋㅋ",
          "original_form": "습해서 그건 또 그거대로 찝찝해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100024472.1.21",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100024472.1.22",
          "form": "맞아요. 저도 길고 좀 달라붙는 옷은 찝찝하더라구요",
          "original_form": "맞아요. 저도 길고 좀 달라붙는 옷은 찝찝하더라구요",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100024472.1.23",
          "form": "비 오는 날이라 그런지",
          "original_form": "비 오는 날이라 그런지",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100024472.1.24",
          "form": "밀가루 음식이 땡기지 않나요? ",
          "original_form": "밀가루 음식이 땡기지 않나요? ",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100024472.1.25",
          "form": "아, 전같은거요?",
          "original_form": "아, 전같은거요?",
          "speaker_id": "1",
          "time": "20210831 13:50"
        },
        {
          "id": "MDRW2100024472.1.26",
          "form": "네,, 아니 비 왜 자꾸 오지!!!!",
          "original_form": "네,, 아니 비 왜 자꾸 오지!!!!",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100024472.1.27",
          "form": "오늘 비가 많이 와서",
          "original_form": "오늘 비가 많이 와서",
          "speaker_id": "2",
          "time": "20210831 15:53"
        },
        {
          "id": "MDRW2100024472.1.28",
          "form": "자꾸 날씨 이야기가 하고싶어져요 ㅋㅋㅋㅋㅋ",
          "original_form": "자꾸 날씨 이야기가 하고싶어져요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:53"
        }
      ]
    }
