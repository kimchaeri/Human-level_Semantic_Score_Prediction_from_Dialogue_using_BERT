      "utterance": [
        {
          "id": "MDRW2100030054.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100030054.1.2",
          "form": "반가워요ㅎㅎ",
          "original_form": "반가워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100030054.1.3",
          "form": "최근에 날씨 급 좋아졌죠ㅎㅎ",
          "original_form": "최근에 날씨 급 좋아졌죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100030054.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210904 06:12"
        },
        {
          "id": "MDRW2100030054.1.5",
          "form": "어제부터 날씨가 좋아서 간만에 해를 즐겼습니다",
          "original_form": "어제부터 날씨가 좋아서 간만에 해를 즐겼습니다",
          "speaker_id": "1",
          "time": "20210904 06:13"
        },
        {
          "id": "MDRW2100030054.1.6",
          "form": "저도 오늘 근처 드라이브나 다녀와 보려구요",
          "original_form": "저도 오늘 근처 드라이브나 다녀와 보려구요",
          "speaker_id": "2",
          "time": "20210904 08:21"
        },
        {
          "id": "MDRW2100030054.1.7",
          "form": "좋은 생각입니다",
          "original_form": "좋은 생각입니다",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030054.1.8",
          "form": "저도 날좋을때 산책을 해볼까 합니다",
          "original_form": "저도 날좋을때 산책을 해볼까 합니다",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030054.1.9",
          "form": "낮에 좀 덥긴 했네요",
          "original_form": "낮에 좀 덥긴 했네요",
          "speaker_id": "2",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100030054.1.10",
          "form": "아직은 늦여름 초가을이라 그런지 낮엔 덥지요",
          "original_form": "아직은 늦여름 초가을이라 그런지 낮엔 덥지요",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100030054.1.11",
          "form": "대신 아침 저녁으로 선선해서 다니가 좋더군요",
          "original_form": "대신 아침 저녁으로 선선해서 다니가 좋더군요",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100030054.1.12",
          "form": "철원 다녀오는 길인데, 어두운데서 별보이길래 한참 보다왔거든요",
          "original_form": "철원 다녀오는 길인데, 어두운데서 별보이길래 한참 보다왔거든요",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100030054.1.13",
          "form": "그러니까 너무 춥네요",
          "original_form": "그러니까 너무 춥네요",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100030054.1.14",
          "form": "철원이면 추웠겠군요",
          "original_form": "철원이면 추웠겠군요",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100030054.1.15",
          "form": "별은 많이 보이던가요?",
          "original_form": "별은 많이 보이던가요?",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030054.1.16",
          "form": "정말 많던데요 태오나서 그렇게 많은건 처음봤어요",
          "original_form": "정말 많던데요 태오나서 그렇게 많은건 처음봤어요",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100030054.1.17",
          "form": "약간 희미하긴 해도 은하수고 보이구요ㅎㅎ",
          "original_form": "약간 희미하긴 해도 은하수고 보이구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100030054.1.18",
          "form": "거긴 빛공해도 없는 곳이라 그렇게 잘 보였군요",
          "original_form": "거긴 빛공해도 없는 곳이라 그렇게 잘 보였군요",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100030054.1.19",
          "form": "저도 나중에 가봐야겠습니다",
          "original_form": "저도 나중에 가봐야겠습니다",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100030054.1.20",
          "form": "어제 새로 느낀건데 달이 없어야? 별이 더 잘보이겠구나 싶더라구요",
          "original_form": "어제 새로 느낀건데 달이 없어야? 별이 더 잘보이겠구나 싶더라구요",
          "speaker_id": "2",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100030054.1.21",
          "form": "달이 안보였고든요 어제는",
          "original_form": "달이 안보였고든요 어제는",
          "speaker_id": "2",
          "time": "20210905 17:14"
        },
        {
          "id": "MDRW2100030054.1.22",
          "form": "달빛이 별빛을 가릴만큼 이군요",
          "original_form": "달빛이 별빛을 가릴만큼 이군요",
          "speaker_id": "1",
          "time": "20210905 17:15"
        },
        {
          "id": "MDRW2100030054.1.23",
          "form": "그건 모르겠어요 실제로 달이 옆에 같이 떠있으면 별이 덜 보일지?",
          "original_form": "그건 모르겠어요 실제로 달이 옆에 같이 떠있으면 별이 덜 보일지?",
          "speaker_id": "2",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100030054.1.24",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100030054.1.25",
          "form": "저도 도시 생활을 오래하다보니",
          "original_form": "저도 도시 생활을 오래하다보니",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100030054.1.26",
          "form": "별본지가 얼마인지 기억도 안납니다",
          "original_form": "별본지가 얼마인지 기억도 안납니다",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100030054.1.27",
          "form": "어제 서울 도착해서 밤하늘 다시 봤는데 똑같은 밤하늘에 어떻게 이렇게 별 개수가 다르지 했어요ㅜㅜ",
          "original_form": "어제 서울 도착해서 밤하늘 다시 봤는데 똑같은 밤하늘에 어떻게 이렇게 별 개수가 다르지 했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 17:37"
        }
      ]
    }
