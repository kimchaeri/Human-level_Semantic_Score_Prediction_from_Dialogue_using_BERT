      "utterance": [
        {
          "id": "MDRW2100040702.1.1",
          "form": "돈 많은 집이라고 하니",
          "original_form": "돈 많은 집이라고 하니",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.2",
          "form": "할말이 없다",
          "original_form": "할말이 없다",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.3",
          "form": "만원이 그리 큰돈이라고 생각을 안 할 테니",
          "original_form": "만원이 그리 큰돈이라고 생각을 안 할 테니",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.4",
          "form": "그렇지ㅋ",
          "original_form": "그렇지ㅋ",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.5",
          "form": "그냥 목마르면 아무데나 가서 물 사마시는거고",
          "original_form": "그냥 목마르면 아무데나 가서 물 사마시는거고",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.6",
          "form": "자연스러운 생활일 텐데",
          "original_form": "자연스러운 생활일 텐데",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.7",
          "form": "우리 같이 맨날 물 챙겨 다니는 사람은",
          "original_form": "우리 같이 맨날 물 챙겨 다니는 사람은",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.8",
          "form": "그래서 나중에 물어보니깐 물은 집에와서",
          "original_form": "그래서 나중에 물어보니깐 물은 집에와서",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.9",
          "form": "마시라고했고",
          "original_form": "마시라고했고",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040702.1.10",
          "form": "편의점 물 사 먹는 거 정말 사치잖아 ㅋㅋ",
          "original_form": "편의점 물 사 먹는 거 정말 사치잖아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.11",
          "form": "아들한테는 편의점충전카드?그거 2천원인가",
          "original_form": "아들한테는 편의점충전카드?그거 2천원인가",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.12",
          "form": "해줬다더라구",
          "original_form": "해줬다더라구",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.13",
          "form": "아 그래 그게 정상이지",
          "original_form": "아 그래 그게 정상이지",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.14",
          "form": "매일쓰면안되고 급할때쓰라고",
          "original_form": "매일쓰면안되고 급할때쓰라고",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.15",
          "form": "했다대",
          "original_form": "했다대",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.16",
          "form": "그래그래 잘했네",
          "original_form": "그래그래 잘했네",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100040702.1.17",
