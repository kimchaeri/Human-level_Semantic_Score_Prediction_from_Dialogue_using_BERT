      "utterance": [
        {
          "id": "MDRW2100043076.1.1",
          "form": "지금은 좀 한가해?",
          "original_form": "지금은 좀 한가해?",
          "speaker_id": "1",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100043076.1.2",
          "form": "지금 막 한번에 몰아쳤어",
          "original_form": "지금 막 한번에 몰아쳤어",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043076.1.3",
          "form": "양쪽에서 세명씩 등장해서 한마디씩 하면 죽을거 같음",
          "original_form": "양쪽에서 세명씩 등장해서 한마디씩 하면 죽을거 같음",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043076.1.4",
          "form": "앞에서 하고 있으면 좀 기다려주면 안되나",
          "original_form": "앞에서 하고 있으면 좀 기다려주면 안되나",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043076.1.5",
          "form": "내 입이 하나랍니다",
          "original_form": "내 입이 하나랍니다",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043076.1.6",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043076.1.7",
          "form": "창구업무의 서러움...",
          "original_form": "창구업무의 서러움...",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043076.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 상자 구매하는 사람 옆에 봉투 구매하는 사람 옆에 상자 바꾸러온 사람과",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 상자 구매하는 사람 옆에 봉투 구매하는 사람 옆에 상자 바꾸러온 사람과",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100043076.1.9",
          "form": "뽁뽁이 달라는 사람이 있으면",
          "original_form": "뽁뽁이 달라는 사람이 있으면",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100043076.1.10",
          "form": "울고싶어짐",
          "original_form": "울고싶어짐",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100043076.1.11",
          "form": "어제까지는 영수증이 안나와서 막 기다리는데",
          "original_form": "어제까지는 영수증이 안나와서 막 기다리는데",
          "speaker_id": "2",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100043076.1.12",
          "form": "내 잘못도 아닌데 괜히 위축되고",
          "original_form": "내 잘못도 아닌데 괜히 위축되고",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.13",
          "form": "아닠",
          "original_form": "아닠",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.14",
          "form": "넘 당황하지마...",
          "original_form": "넘 당황하지마...",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.15",
          "form": "ㅋㅋㅋㅋㅋ 기계탓인데",
          "original_form": "ㅋㅋㅋㅋㅋ 기계탓인데",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.16",
          "form": "늦으면 뭐 어쩔거야 ㅡㅡ",
          "original_form": "늦으면 뭐 어쩔거야 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.17",
          "form": "인턴 목걸이 걸고 있으면 괜히 내잘못같아서",
          "original_form": "인턴 목걸이 걸고 있으면 괜히 내잘못같아서",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.18",
          "form": "ㄴㄴ 인턴목걸이는 오히려",
          "original_form": "ㄴㄴ 인턴목걸이는 오히려",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043076.1.19",
          "form": "얘가 좀 느려도 그러려니 해라",
          "original_form": "얘가 좀 느려도 그러려니 해라",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043076.1.20",
          "form": "의 상징임 ㅋㅋㅋㅋ",
          "original_form": "의 상징임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043076.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043076.1.22",
          "form": "그래서 막 사수?아닌 사수 옆자리 주무관님 부르고",
          "original_form": "그래서 막 사수?아닌 사수 옆자리 주무관님 부르고",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043076.1.23",
          "form": "무해한 눈을 꾸며냄",
          "original_form": "무해한 눈을 꾸며냄",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043076.1.24",
          "form": "주무관님도 안되니까 결국 자기 창구에서 뽑아주심",
          "original_form": "주무관님도 안되니까 결국 자기 창구에서 뽑아주심",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.26",
          "form": "자기 급하다고 막 티내고 말하는 놈이 잇었거든 한놈...",
          "original_form": "자기 급하다고 막 티내고 말하는 놈이 잇었거든 한놈...",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.27",
          "form": "그래 걍 글케 하면 됨 ㅋㅋㅋ",
          "original_form": "그래 걍 글케 하면 됨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.28",
          "form": "아니 나도 급해 그럼 마음이",
          "original_form": "아니 나도 급해 그럼 마음이",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.29",
          "form": "ㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.30",
          "form": "뭐 어떡할거야",
          "original_form": "뭐 어떡할거야",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.31",
          "form": "그럼 자기가 우체국 직원 하던지 ㅋㅋㅋㅋ",
          "original_form": "그럼 자기가 우체국 직원 하던지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.32",
          "form": "기계가 먹통인데 네가 고쳐쥬던가...",
          "original_form": "기계가 먹통인데 네가 고쳐쥬던가...",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.33",
          "form": "마저..",
          "original_form": "마저..",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.34",
          "form": "그럼 네가 영수증 뽑아가던가",
          "original_form": "그럼 네가 영수증 뽑아가던가",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043076.1.35",
          "form": "직접 할 거 아니면 재촉하지 마라ㅡㅡ",
          "original_form": "직접 할 거 아니면 재촉하지 마라ㅡㅡ",
