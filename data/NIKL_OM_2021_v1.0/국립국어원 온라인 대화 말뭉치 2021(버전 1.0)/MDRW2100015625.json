        {
          "id": "MDRW2100015625.1.1",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015625.1.2",
          "form": "와 검색해보니까",
          "original_form": "와 검색해보니까",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015625.1.3",
          "form": "재밌을거같아요",
          "original_form": "재밌을거같아요",
          "speaker_id": "2",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015625.1.4",
          "form": "두껍던데ㅜ 도전해보고싶긴해요",
          "original_form": "두껍던데ㅜ 도전해보고싶긴해요",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015625.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠ 책 읽는건 좋은데 읽을 시간이 잘안나서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠ 책 읽는건 좋은데 읽을 시간이 잘안나서",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015625.1.6",
          "form": "사놓고 못읽을때도 많은 것 같아요 ㅠㅠ",
          "original_form": "사놓고 못읽을때도 많은 것 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015625.1.7",
          "form": "근데 도서관에서 책빌리면 마감이 있으니까 읽게되더라구요",
          "original_form": "근데 도서관에서 책빌리면 마감이 있으니까 읽게되더라구요",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015625.1.8",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015625.1.9",
          "form": "전...그런데 책 사는 걸 좋아해요...",
          "original_form": "전...그런데 책 사는 걸 좋아해요...",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015625.1.10",
          "form": "책장에 꽂아두면 일단 뿌듯해서",
          "original_form": "책장에 꽂아두면 일단 뿌듯해서",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015625.1.11",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015625.1.12",
          "form": "아ㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜ저는 이사다녀야해서ㅜㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜ저는 이사다녀야해서ㅜㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015625.1.13",
          "form": "사고싶은데ㅜㅜ못사요ㅜㅜ",
          "original_form": "사고싶은데ㅜㅜ못사요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015625.1.14",
          "form": "아 ㅠㅠㅠㅠㅠㅠㅠ 이사다니면 좀 그렇죠 ㅠㅠㅠ 또르르...",
          "original_form": "아 ㅠㅠㅠㅠㅠㅠㅠ 이사다니면 좀 그렇죠 ㅠㅠㅠ 또르르...",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015625.1.15",
          "form": "한권두권 사다보면",
          "original_form": "한권두권 사다보면",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015625.1.16",
          "form": "너무 늘어나서...",
          "original_form": "너무 늘어나서...",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015625.1.17",
          "form": "흑흑흑 누워서 뒹굴거리면서 보는거",
          "original_form": "흑흑흑 누워서 뒹굴거리면서 보는거",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015625.1.18",
          "form": "좋아하는데 ㅜㅜ",
          "original_form": "좋아하는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015625.1.19",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015625.1.20",
          "form": "e북 고민해야하구ㅜㅜㅜㅜㅜㅜ",
          "original_form": "e북 고민해야하구ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015625.1.21",
          "form": "전 이북도 이미 보긴하는데",
          "original_form": "전 이북도 이미 보긴하는데",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015625.1.22",
          "form": "좋나요?",
          "original_form": "좋나요?",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015625.1.23",
          "form": "근데 종이책이랑은 또 달라요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 종이책이랑은 또 달라요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015625.1.24",
          "form": "이북으로 사도 종이책으로도 사고 싶은 그런게 있어서 ...",
          "original_form": "이북으로 사도 종이책으로도 사고 싶은 그런게 있어서 ...",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015625.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015625.1.26",
          "form": "그리고 사실 종이책에 비해 드라마틱하게 싼 것도 아니라...",
          "original_form": "그리고 사실 종이책에 비해 드라마틱하게 싼 것도 아니라...",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015625.1.27",
          "form": "솔리님은 출판계의 빛과소금이십니다",
          "original_form": "솔리님은 출판계의 빛과소금이십니다",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015625.1.28",
          "form": "근데 책을 많이...소장하며 볼 수 있는 건 좋아요...",
          "original_form": "근데 책을 많이...소장하며 볼 수 있는 건 좋아요...",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015625.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ... ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015625.1.30",
          "form": "쌓이면 파시나요",
          "original_form": "쌓이면 파시나요",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015625.1.31",
          "form": "아뇨 절대...",
          "original_form": "아뇨 절대...",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015625.1.32",
          "form": "소장하려고 사는거라",
          "original_form": "소장하려고 사는거라",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015625.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015625.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015625.1.35",
          "form": "큰집이시군요",
          "original_form": "큰집이시군요",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015625.1.36",
          "form": "그렇진않아요... 책장이 이미 포화상태라",
          "original_form": "그렇진않아요... 책장이 이미 포화상태라",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015625.1.37",
          "form": "서랍에도 책 넣어두고 있어요 ㅋㅋㅋㅋㅋㅋㅋ",
