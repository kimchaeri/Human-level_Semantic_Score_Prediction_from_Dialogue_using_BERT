      "utterance": [
        {
          "id": "MDRW2100009383.1.1",
          "form": "여행 자주다니세요?",
          "original_form": "여행 자주다니세요?",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009383.1.2",
          "form": "음 지금은 아니여 ㅎ",
          "original_form": "음 지금은 아니여 ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009383.1.3",
          "form": "제주가서 한달살기 해보고 싶어요 요즘은 ㅎ",
          "original_form": "제주가서 한달살기 해보고 싶어요 요즘은 ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009383.1.4",
          "form": "저두요ㅠ",
          "original_form": "저두요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009383.1.5",
          "form": "그냥 아무생각없이 쉬고싶어요ㅠ",
          "original_form": "그냥 아무생각없이 쉬고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009383.1.6",
          "form": "에휴 좀 힐링이 필요한 시기인것 같긴해요ㅠ",
          "original_form": "에휴 좀 힐링이 필요한 시기인것 같긴해요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009383.1.7",
          "form": "저두요 ㅎㅎㅎ",
          "original_form": "저두요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009383.1.8",
          "form": "여유롭게 지내보고 싶어요 ㅎㅎ",
          "original_form": "여유롭게 지내보고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009383.1.9",
          "form": "그니깐요... 너무 삶이 퍽퍽해요ㅠ",
          "original_form": "그니깐요... 너무 삶이 퍽퍽해요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009383.1.10",
          "form": "일하는거는 상관없는데... 그 외적으로 스트레스를 받네요ㅠ",
          "original_form": "일하는거는 상관없는데... 그 외적으로 스트레스를 받네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009383.1.11",
          "form": "제주도 한달살기하면 일은 어떻게 해요?ㄹㄹ",
          "original_form": "제주도 한달살기하면 일은 어떻게 해요?ㄹㄹ",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009383.1.12",
          "form": "ㅎㅎㅎ 그게 문제라 못하죠 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 그게 문제라 못하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009383.1.13",
          "form": "넘 현실적이죠 !!",
          "original_form": "넘 현실적이죠 !!",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009383.1.14",
          "form": "맞아요ㅠ 현실이있으니ㅠ",
          "original_form": "맞아요ㅠ 현실이있으니ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009383.1.15",
          "form": "계속 드는 생각이... 왜이렇게 휴식을 안주는지 모르겠어요ㅠ 나한테만 엄격하네요ㅎ",
          "original_form": "계속 드는 생각이... 왜이렇게 휴식을 안주는지 모르겠어요ㅠ 나한테만 엄격하네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009383.1.16",
          "form": "ㅎㅎ 가끔  확 결단 내리고 히고 싶은거 하시는분들 제일 부러워요 ㅎㅎ",
          "original_form": "ㅎㅎ 가끔  확 결단 내리고 히고 싶은거 하시는분들 제일 부러워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009383.1.17",
          "form": "음 아마도 ~  해야만 하는일이라  더  엄격 한거 같아요",
          "original_form": "음 아마도 ~  해야만 하는일이라  더  엄격 한거 같아요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009383.1.18",
          "form": "나 아니면 이걸 누가 할수 없다는 그런~",
          "original_form": "나 아니면 이걸 누가 할수 없다는 그런~",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009383.1.19",
          "form": "그런것같애요ㅠ 부럽죠~ 그런데 그런분들은 다 계획이 있을꺼에요ㅎ",
          "original_form": "그런것같애요ㅠ 부럽죠~ 그런데 그런분들은 다 계획이 있을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009383.1.20",
          "form": "정말 여행가면 아무것도 안하고 가만히 힐링만하고싶어요ㅎ",
          "original_form": "정말 여행가면 아무것도 안하고 가만히 힐링만하고싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009383.1.21",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009383.1.22",
          "form": "요즘에 캠핑유행하잖아요ㅎ 아무도 없는 산곡 깊은곳에가서 감성을 울리는 음악을 틀어놓고 불멍하고싶어요",
          "original_form": "요즘에 캠핑유행하잖아요ㅎ 아무도 없는 산곡 깊은곳에가서 감성을 울리는 음악을 틀어놓고 불멍하고싶어요",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009383.1.23",
          "form": "이번에 여름휴가 갔다오셨어요?",
          "original_form": "이번에 여름휴가 갔다오셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009383.1.24",
          "form": "저 집에 잠시 ㅎㅎ",
          "original_form": "저 집에 잠시 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009383.1.25",
          "form": "그거빼곤 ㅎㅎ",
          "original_form": "그거빼곤 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009383.1.26",
          "form": "왔다갔다 시간이 더들엇어요 ㅎㅎㅎ",
          "original_form": "왔다갔다 시간이 더들엇어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009383.1.27",
          "form": "왕복 12시간 ㅋ",
          "original_form": "왕복 12시간 ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009383.1.28",
          "form": "에고ㅎㅎㅎㅎ",
          "original_form": "에고ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009383.1.29",
          "form": "엄청 머네요ㅠ 경기도라고 하시않으셨어요?",
          "original_form": "엄청 머네요ㅠ 경기도라고 하시않으셨어요?",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009383.1.30",
          "form": "네네 경기도 전라도~ ㅎ",
          "original_form": "네네 경기도 전라도~ ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009383.1.31",
          "form": "근데 그렇게 멀어요?",
          "original_form": "근데 그렇게 멀어요?",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009383.1.32",
          "form": "장거리 힘등어서 좀 쉬는 편이라 ㅎㅎ",
          "original_form": "장거리 힘등어서 좀 쉬는 편이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009383.1.33",
          "form": "4시간 반이면 갈거에여 안쉬면",
          "original_form": "4시간 반이면 갈거에여 안쉬면",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009383.1.34",
          "form": "왕복 9시간 정도",
          "original_form": "왕복 9시간 정도",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009383.1.35",
          "form": "피로감이 ㅎㅎㅎ",
          "original_form": "피로감이 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009383.1.36",
          "form": "왠만한 해외여행이네요ㅎ",
          "original_form": "왠만한 해외여행이네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009383.1.37",
          "form": "가서 뭐하셨어요?ㅎ 장흥에 관광 명소가 뭐가있었져ㅎㅎ",
          "original_form": "가서 뭐하셨어요?ㅎ 장흥에 관광 명소가 뭐가있었져ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009383.1.38",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009383.1.39",
          "form": "음 그냥 집에만 왔다왓어뇨",
          "original_form": "음 그냥 집에만 왔다왓어뇨",
          "speaker_id": "1",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009383.1.40",
          "form": "원래 8월에 물축제 유명해요 \r\n코로나때문네 안하지만요",
          "original_form": "원래 8월에 물축제 유명해요 \r\n코로나때문네 안하지만요",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009383.1.41",
          "form": "ㅎㅎ 맞네요 물축제가 있었네요ㅎ",
          "original_form": "ㅎㅎ 맞네요 물축제가 있었네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009383.1.42",
          "form": "태극 송크란 축제처럼 ㅎ 물론 거기에 비해 엄청 규모는 작지만요 ㅎㄹ",
          "original_form": "태극 송크란 축제처럼 ㅎ 물론 거기에 비해 엄청 규모는 작지만요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009383.1.43",
          "form": "장흥은 삼합먹으러도 가보고ㅎ 제주도 가기전에 오렌지 페리도 타보고 했었던것 같애요ㅎ",
          "original_form": "장흥은 삼합먹으러도 가보고ㅎ 제주도 가기전에 오렌지 페리도 타보고 했었던것 같애요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009383.1.44",
          "form": "저는 페리 못타봤어요 엉엉",
          "original_form": "저는 페리 못타봤어요 엉엉",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009383.1.45",
          "form": "ㅎㅎ 물축제는 한번도 못해봐서ㅎ 재미있겠네요ㅎ",
          "original_form": "ㅎㅎ 물축제는 한번도 못해봐서ㅎ 재미있겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009383.1.46",
          "form": "왜요?ㅠ",
          "original_form": "왜요?ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009383.1.47",
          "form": "그게 좀 빨리 없어지긴했죠?",
          "original_form": "그게 좀 빨리 없어지긴했죠?",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009383.1.48",
          "form": "저 타려고 햇더니\r\n메르스 + 세월호 타격으로 문닫앗어요",
          "original_form": "저 타려고 햇더니\r\n메르스 + 세월호 타격으로 문닫앗어요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009383.1.49",
          "form": "장흥항이 문을 닫았더라구요",
          "original_form": "장흥항이 문을 닫았더라구요",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009383.1.50",
          "form": "ㅠㅠ 저도 한번 탔는데 빠르긴 빨랐어요ㅎ",
          "original_form": "ㅠㅠ 저도 한번 탔는데 빠르긴 빨랐어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:36"
        }
