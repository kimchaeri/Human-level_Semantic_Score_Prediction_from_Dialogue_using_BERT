      "utterance": [
        {
          "id": "MDRW2100029587.1.1",
          "form": "각나라별",
          "original_form": "각나라별",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.2",
          "form": "아이러브커피나 요리중독이요",
          "original_form": "아이러브커피나 요리중독이요",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.3",
          "form": "요리 만들어서 파는",
          "original_form": "요리 만들어서 파는",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.4",
          "form": "게임잼나요",
          "original_form": "게임잼나요",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.5",
          "form": "요리중독은 진짜 중독이쥬",
          "original_form": "요리중독은 진짜 중독이쥬",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.6",
          "form": "요리중독 존잼",
          "original_form": "요리중독 존잼",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.7",
          "form": "전 고양이 나오는",
          "original_form": "전 고양이 나오는",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.8",
          "form": "정신없이 시간보내기 좋아요",
          "original_form": "정신없이 시간보내기 좋아요",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.9",
          "form": "카페나 힐링겜",
          "original_form": "카페나 힐링겜",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.10",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.11",
          "form": "고양이게임 귀엽네요",
          "original_form": "고양이게임 귀엽네요",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.12",
          "form": "보는걸로 힐링되죠",
          "original_form": "보는걸로 힐링되죠",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.13",
          "form": "게임이름이 뭐죠",
          "original_form": "게임이름이 뭐죠",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029587.1.14",
          "form": "찾아볼게요",
          "original_form": "찾아볼게요",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029587.1.15",
          "form": "나중에 알려주세용",
          "original_form": "나중에 알려주세용",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029587.1.16",
          "form": "고양이자리요",
          "original_form": "고양이자리요",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029587.1.17",
          "form": "힐링입니다",
          "original_form": "힐링입니다",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029587.1.18",
          "form": "동물게임은 힐링이죠",
          "original_form": "동물게임은 힐링이죠",
          "speaker_id": "1",
          "time": "20210903 10:43"
