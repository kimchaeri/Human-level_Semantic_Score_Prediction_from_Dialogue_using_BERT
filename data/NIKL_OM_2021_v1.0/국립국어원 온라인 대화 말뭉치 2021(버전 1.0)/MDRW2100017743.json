      "utterance": [
        {
          "id": "MDRW2100017743.1.1",
          "form": "아니아직ㅋㅋ",
          "original_form": "아니아직ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017743.1.2",
          "form": "로아커 나는 배송됨! ㅋㅋ 이미 뜯어먹음ㅋㅋㅋ",
          "original_form": "로아커 나는 배송됨! ㅋㅋ 이미 뜯어먹음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100017743.1.3",
          "form": "헐ㅜㅜㅜ부럽다ㅜㅜㅜ",
          "original_form": "헐ㅜㅜㅜ부럽다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017743.1.4",
          "form": "나는 대구던데ㅋㅋㅋ",
          "original_form": "나는 대구던데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017743.1.5",
          "form": "맛있어???",
          "original_form": "맛있어???",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017743.1.6",
          "form": "어어 ㅋㅋ오랜만에 먹으니 맛잇더라 나는 빨간색이 제일좋아",
          "original_form": "어어 ㅋㅋ오랜만에 먹으니 맛잇더라 나는 빨간색이 제일좋아",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100017743.1.7",
          "form": "나도나도! 빨간색 맛있징",
          "original_form": "나도나도! 빨간색 맛있징",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100017743.1.8",
          "form": "빨리받고싶다",
          "original_form": "빨리받고싶다",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100017743.1.9",
          "form": "아침엔 받을수있을따",
          "original_form": "아침엔 받을수있을따",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100017743.1.10",
          "form": "나 방금도 간식주문함ㅋㅋㅋㅋㅋ",
          "original_form": "나 방금도 간식주문함ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100017743.1.11",
          "form": "뭐샀어?!",
          "original_form": "뭐샀어?!",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100017743.1.12",
          "form": "파운드케이크!",
          "original_form": "파운드케이크!",
          "speaker_id": "1",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100017743.1.13",
          "form": "아 옛날에 대학병원에서 팔던 파운드케이크 맛있었는데",
          "original_form": "아 옛날에 대학병원에서 팔던 파운드케이크 맛있었는데",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100017743.1.14",
          "form": "지금은 안팔아?",
          "original_form": "지금은 안팔아?",
          "speaker_id": "1",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100017743.1.15",
          "form": "그가게가 그대로있는지 모르겟어ㅋㅋㅋ",
          "original_form": "그가게가 그대로있는지 모르겟어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:57"
        },
        {
          "id": "MDRW2100017743.1.16",
          "form": "대학병원에 그뭐냐 앤티앤스 먹고싶ㅋㅋㅋ",
          "original_form": "대학병원에 그뭐냐 앤티앤스 먹고싶ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100017743.1.17",
          "form": "아 저번에 너가 말한?!",
          "original_form": "아 저번에 너가 말한?!",
          "speaker_id": "2",
          "time": "20210827 00:58"
        },
        {
          "id": "MDRW2100017743.1.18",
          "form": "앤티앤스안먹어봄??",
          "original_form": "앤티앤스안먹어봄??",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100017743.1.19",
          "form": "안먹어봄ㅋㅋ항상 대학병원엘 가면 맘이 급해ㅋㅋ",
          "original_form": "안먹어봄ㅋㅋ항상 대학병원엘 가면 맘이 급해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:02"
        },
        {
          "id": "MDRW2100017743.1.20",
          "form": "앤티앤스 존맛이야",
          "original_form": "앤티앤스 존맛이야",
          "speaker_id": "1",
