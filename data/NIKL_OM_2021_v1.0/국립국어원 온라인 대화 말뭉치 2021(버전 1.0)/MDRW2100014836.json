      "utterance": [
        {
          "id": "MDRW2100014836.1.1",
          "form": "요즘 배달음식 시켜먹은 적 있어?",
          "original_form": "요즘 배달음식 시켜먹은 적 있어?",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014836.1.2",
          "form": "응. 어제 치킨 시켜 먹었어.",
          "original_form": "응. 어제 치킨 시켜 먹었어.",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014836.1.3",
          "form": "어느 브랜드 시켜  먹었어?",
          "original_form": "어느 브랜드 시켜  먹었어?",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014836.1.4",
          "form": "60계 치킨! 닭강정하고 간지치킨 먹었어.",
          "original_form": "60계 치킨! 닭강정하고 간지치킨 먹었어.",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014836.1.5",
          "form": "맛있었어?",
          "original_form": "맛있었어?",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014836.1.6",
          "form": "응. 오랜만에 먹으니까 맛있더라.",
          "original_form": "응. 오랜만에 먹으니까 맛있더라.",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014836.1.7",
          "form": "나도 치킨 먹고 싶다.",
          "original_form": "나도 치킨 먹고 싶다.",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014836.1.8",
          "form": "좋아하는 치킨 있어?",
          "original_form": "좋아하는 치킨 있어?",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014836.1.9",
          "form": "굽네치킨",
          "original_form": "굽네치킨",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014836.1.10",
          "form": "굽네도 맛있지. 어떤 배달음식을 제일 자주 먹어?",
          "original_form": "굽네도 맛있지. 어떤 배달음식을 제일 자주 먹어?",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014836.1.11",
          "form": "피자랑 자장면",
          "original_form": "피자랑 자장면",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014836.1.12",
          "form": "그 중에 제일 좋아하는게 뭐야?",
          "original_form": "그 중에 제일 좋아하는게 뭐야?",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014836.1.13",
          "form": "피자 맛있어. 도미노피자.",
          "original_form": "피자 맛있어. 도미노피자.",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014836.1.14",
          "form": "다음에 같이 먹자.",
          "original_form": "다음에 같이 먹자.",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014836.1.15",
          "form": "그러자.",
          "original_form": "그러자.",
          "speaker_id": "2",
          "time": "20210825 21:03"
        }
      ]
