      "utterance": [
        {
          "id": "MDRW2100018220.1.1",
          "form": "아프셔서요?",
          "original_form": "아프셔서요?",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018220.1.2",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018220.1.3",
          "form": "어디어디요?",
          "original_form": "어디어디요?",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018220.1.4",
          "form": "한의원이랑 치과요 ㅠㅠ",
          "original_form": "한의원이랑 치과요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018220.1.5",
          "form": "한의원은 왜요?",
          "original_form": "한의원은 왜요?",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018220.1.6",
          "form": "오금이 아파서",
          "original_form": "오금이 아파서",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.7",
          "form": "3번째 다니고 있어요 ㅠㅠ",
          "original_form": "3번째 다니고 있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.8",
          "form": "오금이 어디죠?",
          "original_form": "오금이 어디죠?",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.9",
          "form": "슬와요",
          "original_form": "슬와요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.10",
          "form": "무릎 뒤!!",
          "original_form": "무릎 뒤!!",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.11",
          "form": "거기가 오금이군요",
          "original_form": "거기가 오금이군요",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.13",
          "form": "저번에 장구 좀 쳤더니 발목이 아프더니",
          "original_form": "저번에 장구 좀 쳤더니 발목이 아프더니",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.14",
          "form": "이제 오금이 아프네요",
          "original_form": "이제 오금이 아프네요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018220.1.15",
          "form": "발목은 괜찮으세요?",
          "original_form": "발목은 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.16",
          "form": "네 발목은",
          "original_form": "네 발목은",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.17",
          "form": "한의원 한번 가고",
          "original_form": "한의원 한번 가고",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.18",
          "form": "정형외과가서 주사맞고",
          "original_form": "정형외과가서 주사맞고",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.19",
          "form": "나았어요",
          "original_form": "나았어요",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.20",
          "form": "에궁",
          "original_form": "에궁",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.21",
          "form": "아파서 어째요",
          "original_form": "아파서 어째요",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.22",
          "form": "나이먹으니까",
          "original_form": "나이먹으니까",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018220.1.23",
          "form": "점점 아프네요 ㅋㅋ",
          "original_form": "점점 아프네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:17"
        }
