      "utterance": [
        {
          "id": "MDRW2100046527.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211028 19:50"
        },
        {
          "id": "MDRW2100046527.1.2",
          "form": "안녕하세요! 저녁은 드셨나요",
          "original_form": "안녕하세요! 저녁은 드셨나요",
          "speaker_id": "2",
          "time": "20211028 19:51"
        },
        {
          "id": "MDRW2100046527.1.3",
          "form": "네 저녁은 간단히 피자 시켜서 먹었습니대",
          "original_form": "네 저녁은 간단히 피자 시켜서 먹었습니대",
          "speaker_id": "1",
          "time": "20211028 19:51"
        },
        {
          "id": "MDRW2100046527.1.4",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20211028 19:51"
        },
        {
          "id": "MDRW2100046527.1.5",
          "form": "오 저도 피자 좋아하는데",
          "original_form": "오 저도 피자 좋아하는데",
          "speaker_id": "2",
          "time": "20211028 19:53"
        },
        {
          "id": "MDRW2100046527.1.6",
          "form": "어디 피자인가요",
          "original_form": "어디 피자인가요",
          "speaker_id": "2",
          "time": "20211028 19:53"
        },
        {
          "id": "MDRW2100046527.1.7",
          "form": "7번가 피자 먹었는데 아이들이 너무 잘 먹어서 한조각 먹었네요 ㅋㅋ",
          "original_form": "7번가 피자 먹었는데 아이들이 너무 잘 먹어서 한조각 먹었네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211028 19:54"
        },
        {
          "id": "MDRW2100046527.1.8",
          "form": "아하 아이들이 있으셨군요",
          "original_form": "아하 아이들이 있으셨군요",
          "speaker_id": "2",
          "time": "20211028 19:55"
        },
        {
          "id": "MDRW2100046527.1.9",
          "form": "애들은 항상 잘 먹죠 ㅋㅋ",
          "original_form": "애들은 항상 잘 먹죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 19:55"
        },
        {
          "id": "MDRW2100046527.1.10",
          "form": "한조각 밖에 못드셔서 어째요",
          "original_form": "한조각 밖에 못드셔서 어째요",
          "speaker_id": "2",
          "time": "20211028 19:55"
        },
        {
          "id": "MDRW2100046527.1.11",
          "form": "애들 재우고 야식 먹어야죠 ㅋㅋ",
          "original_form": "애들 재우고 야식 먹어야죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211028 19:56"
        },
        {
          "id": "MDRW2100046527.1.12",
          "form": "애들 재우고 먹는 야식이 더 꿀맛입니다",
          "original_form": "애들 재우고 먹는 야식이 더 꿀맛입니다",
          "speaker_id": "1",
          "time": "20211028 19:56"
        },
        {
          "id": "MDRW2100046527.1.13",
          "form": "ㅋㅋㅋㅋ맞습니다 몰래 먹는건 언제나 더 꿀맛이죠",
          "original_form": "ㅋㅋㅋㅋ맞습니다 몰래 먹는건 언제나 더 꿀맛이죠",
          "speaker_id": "2",
          "time": "20211028 19:56"
        },
        {
          "id": "MDRW2100046527.1.14",
          "form": "ㅋㅋㅋ맞습니다! 뱃살이 늘어나지만 야식 포기 못하죠",
          "original_form": "ㅋㅋㅋ맞습니다! 뱃살이 늘어나지만 야식 포기 못하죠",
          "speaker_id": "1",
          "time": "20211028 19:57"
        },
        {
          "id": "MDRW2100046527.1.15",
          "form": "애들은 몇시쯤 재우세요?",
          "original_form": "애들은 몇시쯤 재우세요?",
          "speaker_id": "2",
          "time": "20211028 19:58"
        },
        {
          "id": "MDRW2100046527.1.16",
          "form": "이제 여덟시라 슬슬 야식 준비 하시겠네요",
          "original_form": "이제 여덟시라 슬슬 야식 준비 하시겠네요",
          "speaker_id": "2",
          "time": "20211028 19:59"
        },
        {
          "id": "MDRW2100046527.1.17",
          "form": "8시 30분에 들어가서 마누라가 책 읽어주고 9시에 잠들어요",
          "original_form": "8시 30분에 들어가서 마누라가 책 읽어주고 9시에 잠들어요",
          "speaker_id": "1",
          "time": "20211028 19:59"
        },
        {
          "id": "MDRW2100046527.1.18",
          "form": "그 사이 제가 야식 준비합니다",
          "original_form": "그 사이 제가 야식 준비합니다",
          "speaker_id": "1",
          "time": "20211028 19:59"
        },
        {
          "id": "MDRW2100046527.1.19",
          "form": "오늘 야식은 닭꼬치입니다",
          "original_form": "오늘 야식은 닭꼬치입니다",
          "speaker_id": "1",
          "time": "20211028 19:59"
        },
        {
          "id": "MDRW2100046527.1.20",
          "form": "벌써 기대됩니다",
          "original_form": "벌써 기대됩니다",
          "speaker_id": "1",
          "time": "20211028 20:00"
        },
        {
          "id": "MDRW2100046527.1.21",
          "form": "오오오 닭꼬치 ㅋㅋ",
          "original_form": "오오오 닭꼬치 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 20:00"
        },
        {
          "id": "MDRW2100046527.1.22",
          "form": "음주도 하시나요 !",
          "original_form": "음주도 하시나요 !",
          "speaker_id": "2",
          "time": "20211028 20:00"
        },
        {
          "id": "MDRW2100046527.1.23",
          "form": "당연한 말씀을...",
          "original_form": "당연한 말씀을...",
          "speaker_id": "1",
          "time": "20211028 20:01"
        },
        {
          "id": "MDRW2100046527.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211028 20:01"
        },
        {
          "id": "MDRW2100046527.1.25",
          "form": "ㅋㅋㅋㅋ아 너무 당연할걸 여쭤봤네요",
          "original_form": "ㅋㅋㅋㅋ아 너무 당연할걸 여쭤봤네요",
          "speaker_id": "2",
          "time": "20211028 20:02"
        },
        {
          "id": "MDRW2100046527.1.26",
          "form": "맛난 야식 드시길 바랍니다 ㅋㅋ",
