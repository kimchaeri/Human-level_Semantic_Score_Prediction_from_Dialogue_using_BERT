      "utterance": [
        {
          "id": "MDRW2100045260.1.1",
          "form": "가고싶은 여행지는 없오?",
          "original_form": "가고싶은 여행지는 없오?",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045260.1.2",
          "form": "ㅋㅋㅋ자세하닼ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ자세하닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045260.1.3",
          "form": "ㅋㅋㅋㅋ가서 푸른하늘보고 노을도 볼래",
          "original_form": "ㅋㅋㅋㅋ가서 푸른하늘보고 노을도 볼래",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.4",
          "form": "한국에도 초원있자낭",
          "original_form": "한국에도 초원있자낭",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.5",
          "form": "말도 탈래",
          "original_form": "말도 탈래",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.6",
          "form": "근데 완전 광활한고 푸른 그런 초원이 있나?",
          "original_form": "근데 완전 광활한고 푸른 그런 초원이 있나?",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.7",
          "form": "제주도 딱인데??",
          "original_form": "제주도 딱인데??",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.8",
          "form": "말도 있구",
          "original_form": "말도 있구",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.9",
          "form": "웅장한 자연보러 가고싶어",
          "original_form": "웅장한 자연보러 가고싶어",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.10",
          "form": "아닌가.....?",
          "original_form": "아닌가.....?",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.11",
          "form": "제주도보다 더 큰데로!!",
          "original_form": "제주도보다 더 큰데로!!",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045260.1.12",
          "form": "넌어디가고싶은데?",
          "original_form": "넌어디가고싶은데?",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045260.1.13",
          "form": "흠 아무래도 나는",
          "original_form": "흠 아무래도 나는",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045260.1.14",
          "form": "어디든 해외면 좋은거 같긴 한데 아이슬란드?",
          "original_form": "어디든 해외면 좋은거 같긴 한데 아이슬란드?",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045260.1.15",
          "form": "오로라 보고 싶으",
          "original_form": "오로라 보고 싶으",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045260.1.16",
          "form": "아 나도",
          "original_form": "아 나도",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045260.1.17",
          "form": "홍현희랑 제이쓴 캐나다가서 오로라보고 감동해서 눈물흘렸잖어",
          "original_form": "홍현희랑 제이쓴 캐나다가서 오로라보고 감동해서 눈물흘렸잖어",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045260.1.18",
          "form": "아진짜? 그건 모르는뎈ㅋㅋ",
          "original_form": "아진짜? 그건 모르는뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045260.1.19",
          "form": "아내의맛에 나온거임?",
          "original_form": "아내의맛에 나온거임?",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045260.1.20",
          "form": "ㅋㅋㅋㅋㅋ화면으로 봐도 진짜 멋있긴하더라",
          "original_form": "ㅋㅋㅋㅋㅋ화면으로 봐도 진짜 멋있긴하더라",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045260.1.21",
          "form": "실제로는 대박일듯..",
          "original_form": "실제로는 대박일듯..",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045260.1.22",
          "form": "응ㅋㅋㅋㅋ신혼여행편",
          "original_form": "응ㅋㅋㅋㅋ신혼여행편",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045260.1.23",
          "form": "오오 나 그런거 좋아하는데 나중에 봐야겠당",
          "original_form": "오오 나 그런거 좋아하는데 나중에 봐야겠당",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045260.1.24",
          "form": "티비로 보는 오로라도 이뻐",
          "original_form": "티비로 보는 오로라도 이뻐",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045260.1.25",
          "form": "나는 그 조정석이 간거 보고 가고 싶어 진거거야",
          "original_form": "나는 그 조정석이 간거 보고 가고 싶어 진거거야",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045260.1.26",
          "form": "오로라 보러 아이슬란드로",
          "original_form": "오로라 보러 아이슬란드로",
          "speaker_id": "1",
          "time": "20210908 01:42"
        }
      ]
