      "utterance": [
        {
          "id": "MDRW2100023858.1.1",
          "form": "새로시작한 김선호 신민아 드라마보셨나요?",
          "original_form": "새로시작한 김선호 신민아 드라마보셨나요?",
          "speaker_id": "2",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023858.1.2",
          "form": "어서 얼핏 들엇는뎅... \n재밋나요?",
          "original_form": "어서 얼핏 들엇는뎅... \n재밋나요?",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023858.1.3",
          "form": "저도 보기전이에요!",
          "original_form": "저도 보기전이에요!",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023858.1.4",
          "form": "잼있다고하더라구요!",
          "original_form": "잼있다고하더라구요!",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100023858.1.5",
          "form": "전 좀 쌓이면 몰아볼까싶어요 ㅎ",
          "original_form": "전 좀 쌓이면 몰아볼까싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100023858.1.6",
          "form": "저도 드라마는 한방에 봐야해서... \n실시간은 거의 안보는거 같아여",
          "original_form": "저도 드라마는 한방에 봐야해서... \n실시간은 거의 안보는거 같아여",
          "speaker_id": "1",
          "time": "20210830 19:23"
        },
        {
          "id": "MDRW2100023858.1.7",
          "form": "한방에 보면 좋아요 ㅎㅎ 기다리지않아도되고 ㅋㅋㅋ",
          "original_form": "한방에 보면 좋아요 ㅎㅎ 기다리지않아도되고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:23"
        },
        {
          "id": "MDRW2100023858.1.8",
          "form": "전 펜하안봤는데 시즌 2한참할때",
          "original_form": "전 펜하안봤는데 시즌 2한참할때",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023858.1.9",
          "form": "시즌1부터 쭉봤네요!",
          "original_form": "시즌1부터 쭉봤네요!",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023858.1.10",
          "form": "그거 완전 막장 드라마 아니에여?",
          "original_form": "그거 완전 막장 드라마 아니에여?",
          "speaker_id": "1",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100023858.1.11",
          "form": "마자요;;; 아주 막장이죠ㅠㅠ",
          "original_form": "마자요;;; 아주 막장이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:25"
        },
        {
          "id": "MDRW2100023858.1.12",
          "form": "막장 볼만해여? 전 뭔가 같이 속이 긇어올라서..",
          "original_form": "막장 볼만해여? 전 뭔가 같이 속이 긇어올라서..",
          "speaker_id": "1",
          "time": "20210830 19:28"
        },
        {
          "id": "MDRW2100023858.1.13",
          "form": "별루에요ㅠㅠ ㅋㅋㅋ근데 보던거라 그냥봐요ㅠㅠ",
          "original_form": "별루에요ㅠㅠ ㅋㅋㅋ근데 보던거라 그냥봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023858.1.14",
          "form": "그 계속 보는게 막장의 마약성.. ㅋㅋㅋㅋ",
          "original_form": "그 계속 보는게 막장의 마약성.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100023858.1.15",
          "form": "맞아요ㅠㅠㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023858.1.16",
          "form": "욕하면서 보기 ㅋㅋ",
          "original_form": "욕하면서 보기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023858.1.17",
          "form": "시즌1은 상상이상의 인기였었죠..",
          "original_form": "시즌1은 상상이상의 인기였었죠..",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023858.1.18",
          "form": "관심없는 저도 봐볼까 햇엇을정도..",
          "original_form": "관심없는 저도 봐볼까 햇엇을정도..",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023858.1.19",
          "form": "맞아요 ㅋㅋ얘기많이 들었엇죠 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ얘기많이 들었엇죠 ㅋㅋ",
