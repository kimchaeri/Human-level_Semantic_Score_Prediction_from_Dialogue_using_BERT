        {
          "id": "MDRW2100009628.1.1",
          "form": "과일 수확해서",
          "original_form": "과일 수확해서",
          "speaker_id": "2",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.2",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.3",
          "form": "과실주 담그더라고요",
          "original_form": "과실주 담그더라고요",
          "speaker_id": "2",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.4",
          "form": "그리고 다음해억",
          "original_form": "그리고 다음해억",
          "speaker_id": "2",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.5",
          "form": "수확의 기쁨",
          "original_form": "수확의 기쁨",
          "speaker_id": "1",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.6",
          "form": "새내기 오면",
          "original_form": "새내기 오면",
          "speaker_id": "2",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.7",
          "form": "나눠줘요",
          "original_form": "나눠줘요",
          "speaker_id": "2",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.8",
          "form": "엇.. 그런 용도였다니",
          "original_form": "엇.. 그런 용도였다니",
          "speaker_id": "1",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:55"
        },
        {
          "id": "MDRW2100009628.1.10",
          "form": "집으로 가져갈줄",
          "original_form": "집으로 가져갈줄",
          "speaker_id": "1",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.11",
          "form": "거긴 강권 안 해서 괜찮아요",
          "original_form": "거긴 강권 안 해서 괜찮아요",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.12",
          "form": "물론 자기들끼리도 먹고요",
          "original_form": "물론 자기들끼리도 먹고요",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.13",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.15",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.16",
          "form": "살구주가 그렇게 맛있대요",
          "original_form": "살구주가 그렇게 맛있대요",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.17",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "1",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.18",
          "form": "살구는 교수님이 키우시거든요",
          "original_form": "살구는 교수님이 키우시거든요",
          "speaker_id": "2",
          "time": "20210823 01:56"
        },
        {
          "id": "MDRW2100009628.1.19",
          "form": "그 해에는 그랬대요",
          "original_form": "그 해에는 그랬대요",
