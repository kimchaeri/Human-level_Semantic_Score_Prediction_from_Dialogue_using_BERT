      "utterance": [
        {
          "id": "MDRW2100042197.1.1",
          "form": "앗 민트좋아한대찌",
          "original_form": "앗 민트좋아한대찌",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042197.1.2",
          "form": "그거맛있어나도좋아해",
          "original_form": "그거맛있어나도좋아해",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042197.1.3",
          "form": "마법사의 할로윈도 좋아해?",
          "original_form": "마법사의 할로윈도 좋아해?",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042197.1.4",
          "form": "헷갈리는더",
          "original_form": "헷갈리는더",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042197.1.5",
          "form": "그것도민트지?",
          "original_form": "그것도민트지?",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042197.1.6",
          "form": "응 민트에 진한 초코",
          "original_form": "응 민트에 진한 초코",
          "speaker_id": "2",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042197.1.7",
          "form": "맛있겠다ㅎㅎ",
          "original_form": "맛있겠다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042197.1.8",
          "form": "이거당첨되면먹어봐야지!",
          "original_form": "이거당첨되면먹어봐야지!",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042197.1.9",
          "form": "ㅋㅋㅋㅋㅋ좋아",
          "original_form": "ㅋㅋㅋㅋㅋ좋아",
          "speaker_id": "2",
          "time": "20210907 14:38"
        },
        {
          "id": "MDRW2100042197.1.10",
          "form": "이건 10월쯤에만 팔아서 아쉬워",
          "original_form": "이건 10월쯤에만 팔아서 아쉬워",
          "speaker_id": "2",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100042197.1.11",
          "form": "아구래?",
          "original_form": "아구래?",
          "speaker_id": "1",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100042197.1.12",
          "form": "ㅇㅇ시즌메뉴야",
          "original_form": "ㅇㅇ시즌메뉴야",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042197.1.13",
          "form": "지금많이먹어뒁ㅜ",
          "original_form": "지금많이먹어뒁ㅜ",
          "speaker_id": "1",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042197.1.14",
          "form": "그래야지ㅋㅋ",
          "original_form": "그래야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042197.1.15",
          "form": "넌 베라에서 제일 좋아하는 맛이 뭐야?",
          "original_form": "넌 베라에서 제일 좋아하는 맛이 뭐야?",
          "speaker_id": "2",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042197.1.16",
          "form": "나는 뉴욕치즈케이크!",
          "original_form": "나는 뉴욕치즈케이크!",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100042197.1.17",
          "form": "민초!",
          "original_form": "민초!",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100042197.1.18",
          "form": "그것도 맛있지 치즈블럭 완전 맛있어",
          "original_form": "그것도 맛있지 치즈블럭 완전 맛있어",
          "speaker_id": "2",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100042197.1.19",
          "form": "맞앙ㅎㅎ",
          "original_form": "맞앙ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100042197.1.20",
          "form": "31데이에가면사이즈업해줘",
          "original_form": "31데이에가면사이즈업해줘",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100042197.1.21",
          "form": "맞아 근데 그날 가서 산 적은 없어 꼭 먹고 싶은 때는 월말이 아니라ㅋㅋ",
          "original_form": "맞아 근데 그날 가서 산 적은 없어 꼭 먹고 싶은 때는 월말이 아니라ㅋㅋ",
