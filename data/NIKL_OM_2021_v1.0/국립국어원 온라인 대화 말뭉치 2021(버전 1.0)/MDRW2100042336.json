          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042336.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042336.1.5",
          "form": "뭐드셨어요",
          "original_form": "뭐드셨어요",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042336.1.6",
          "form": "저 완탕이랑 볶음밥이용",
          "original_form": "저 완탕이랑 볶음밥이용",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042336.1.7",
          "form": "와맛있는거네요",
          "original_form": "와맛있는거네요",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042336.1.8",
          "form": "요리하신건가요",
          "original_form": "요리하신건가요",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042336.1.9",
          "form": "아뇨 외식이죠~",
          "original_form": "아뇨 외식이죠~",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042336.1.10",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042336.1.11",
          "form": "저는식당에서ㅎㅎ",
          "original_form": "저는식당에서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042336.1.12",
          "form": "최근에 맛초킹 먹었는데 맛있었어요",
          "original_form": "최근에 맛초킹 먹었는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042336.1.13",
          "form": "맛있죠 저는뿌링클도좋아해요",
          "original_form": "맛있죠 저는뿌링클도좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042336.1.14",
          "form": "어 저 뿌링클은 ㄷㄷ",
          "original_form": "어 저 뿌링클은 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042336.1.15",
          "form": "과자맛",
          "original_form": "과자맛",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042336.1.16",
          "form": "치킨",
          "original_form": "치킨",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042336.1.17",
          "form": "어느순간",
          "original_form": "어느순간",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042336.1.18",
          "form": "맛잏더라고요ㅡ",
          "original_form": "맛잏더라고요ㅡ",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042336.1.19",
