        },
        {
          "id": "MDRW2100017676.1.5",
          "form": "깜짝이야",
          "original_form": "깜짝이야",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.6",
          "form": "여행 마저 얘기해줄게",
          "original_form": "여행 마저 얘기해줄게",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.8",
          "form": "일단 욕조가 크고",
          "original_form": "일단 욕조가 크고",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.9",
          "form": "욕실에 티비가 달려있움",
          "original_form": "욕실에 티비가 달려있움",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.10",
          "form": "??헐",
          "original_form": "??헐",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.11",
          "form": "글고 욕실 한 쪽 벽이 화강암이고",
          "original_form": "글고 욕실 한 쪽 벽이 화강암이고",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.12",
          "form": "그건 좀 신기하다",
          "original_form": "그건 좀 신기하다",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.13",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.14",
          "form": "타일 말고 통 화강암?",
          "original_form": "타일 말고 통 화강암?",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017676.1.15",
          "form": "ㅇㅇ 통화강암",
          "original_form": "ㅇㅇ 통화강암",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017676.1.16",
          "form": "울퉁불퉁한거",
          "original_form": "울퉁불퉁한거",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017676.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017676.1.18",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017676.1.19",
          "form": "아무튼",
          "original_form": "아무튼",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017676.1.20",
          "form": "ㅇㅇ무튼",
          "original_form": "ㅇㅇ무튼",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017676.1.21",
          "form": "호캉스는 좀 좋은것같아",
          "original_form": "호캉스는 좀 좋은것같아",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017676.1.22",
          "form": "난 사실",
          "original_form": "난 사실",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017676.1.23",
          "form": "전에도 몇번 5성급 가봤는데",
          "original_form": "전에도 몇번 5성급 가봤는데",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017676.1.24",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017676.1.25",
          "form": "그때는 딱히",
          "original_form": "그때는 딱히",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017676.1.26",
          "form": "와 이게 바로 자본의 맛...?!",
          "original_form": "와 이게 바로 자본의 맛...?!",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017676.1.27",
          "form": "하는 기분이 안들었거든",
          "original_form": "하는 기분이 안들었거든",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017676.1.28",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017676.1.29",
          "form": "근데 파크하얏트는 진짜 자본의 맛이 느껴졌어",
          "original_form": "근데 파크하얏트는 진짜 자본의 맛이 느껴졌어",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017676.1.30",
          "form": "바닥도 대리석이고",
          "original_form": "바닥도 대리석이고",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017676.1.31",
          "form": "물빠짐구멍은 보통 쇠로 해두는데",
          "original_form": "물빠짐구멍은 보통 쇠로 해두는데",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017676.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.33",
          "form": "얘네는 쇠로 한다음에 그 위에 대리석을 또 덮음",
          "original_form": "얘네는 쇠로 한다음에 그 위에 대리석을 또 덮음",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.34",
          "form": "ㅇㅇ그냥 쇠 아니야?",
          "original_form": "ㅇㅇ그냥 쇠 아니야?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.35",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.36",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.37",
          "form": "그래서 대충 보면 없는거같아",
          "original_form": "그래서 대충 보면 없는거같아",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.38",
          "form": "ㄹㅇ돈냄새나긴 한다...",
          "original_form": "ㄹㅇ돈냄새나긴 한다...",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.39",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.40",
          "form": "아 그리고 직원들이",
          "original_form": "아 그리고 직원들이",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017676.1.41",
