      "utterance": [
        {
          "id": "MDRW2100042500.1.1",
          "form": "아 여자 선생님",
          "original_form": "아 여자 선생님",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042500.1.2",
          "form": "착하고 괜찮아",
          "original_form": "착하고 괜찮아",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042500.1.3",
          "form": "언니 왔어요",
          "original_form": "언니 왔어요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042500.1.4",
          "form": "맛있게 먹어라 어서",
          "original_form": "맛있게 먹어라 어서",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042500.1.5",
          "form": "빨리 씻고 와요",
          "original_form": "빨리 씻고 와요",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042500.1.6",
          "form": "얼른 나가요 언니ㅋㅋㅋ",
          "original_form": "얼른 나가요 언니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100042500.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042500.1.8",
          "form": "오늘도 2분 남기고 왔다",
          "original_form": "오늘도 2분 남기고 왔다",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042500.1.9",
          "form": "대단하다 정말ㅋㅋㅋㅋㅋ",
          "original_form": "대단하다 정말ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042500.1.10",
          "form": "그렇게 맞추기도 힘든",
          "original_form": "그렇게 맞추기도 힘든",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042500.1.11",
          "form": "오늘은 정말 아슬아슬했다",
          "original_form": "오늘은 정말 아슬아슬했다",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042500.1.12",
          "form": "하루하루 바쁘군",
          "original_form": "하루하루 바쁘군",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042500.1.13",
          "form": "언니 오늘 주사 맞으러 안가요",
          "original_form": "언니 오늘 주사 맞으러 안가요",
          "speaker_id": "2",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042500.1.14",
          "form": "출근했다 오는 거라 요?",
          "original_form": "출근했다 오는 거라 요?",
          "speaker_id": "2",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042500.1.15",
          "form": "아 출근했다가 열두시쯤",
          "original_form": "아 출근했다가 열두시쯤",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042500.1.16",
          "form": "퇴근한다",
          "original_form": "퇴근한다",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042500.1.17",
          "form": "출근했다 12시 퇴근하는데 2분 남겨놓고 출근ㅋㅋㅋㅋㅋ",
          "original_form": "출근했다 12시 퇴근하는데 2분 남겨놓고 출근ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.18",
          "form": "멋져 보러",
          "original_form": "멋져 보러",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.20",
          "form": "일하는 시간만 맞추면 되지",
          "original_form": "일하는 시간만 맞추면 되지",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.21",
          "form": "출근하는 것도 디다 ㅋㅋ",
          "original_form": "출근하는 것도 디다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.22",
          "form": "하긴 그건 그래요ㅋㅋㅋ",
          "original_form": "하긴 그건 그래요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.23",
          "form": "대구 넘어가는데 참 많잖아요",
          "original_form": "대구 넘어가는데 참 많잖아요",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.24",
          "form": "차",
          "original_form": "차",
          "speaker_id": "2",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042500.1.25",
          "form": "엉 아침에는 차가 너무 많다",
          "original_form": "엉 아침에는 차가 너무 많다",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042500.1.26",
          "form": "80에서 90 사 이로 가고 싶은데",
          "original_form": "80에서 90 사 이로 가고 싶은데",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042500.1.27",
          "form": "80을 지키는 차들이 너무 많아서",
          "original_form": "80을 지키는 차들이 너무 많아서",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042500.1.28",
          "form": "아침부터 나만 바쁜 느낌",
          "original_form": "아침부터 나만 바쁜 느낌",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042500.1.29",
          "form": "거기 외국한 단속 있어서",
          "original_form": "거기 외국한 단속 있어서",
          "speaker_id": "2",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042500.1.30",
          "form": "구간 단속",
          "original_form": "구간 단속",
          "speaker_id": "2",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042500.1.31",
          "form": "ㅋㅋㅋㅌㅌ",
          "original_form": "ㅋㅋㅋㅌㅌ",
          "speaker_id": "2",
