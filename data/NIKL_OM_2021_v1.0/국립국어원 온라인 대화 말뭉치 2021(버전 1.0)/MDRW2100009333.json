      "utterance": [
        {
          "id": "MDRW2100009333.1.1",
          "form": "부는듯요~",
          "original_form": "부는듯요~",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009333.1.2",
          "form": "올만에 에어컨 안켜고",
          "original_form": "올만에 에어컨 안켜고",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009333.1.3",
          "form": "덥다",
          "original_form": "덥다",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009333.1.4",
          "form": "선풍기 돌립니다~",
          "original_form": "선풍기 돌립니다~",
          "speaker_id": "2",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009333.1.5",
          "form": "올",
          "original_form": "올",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009333.1.6",
          "form": "땀쟁이 왠일이고",
          "original_form": "땀쟁이 왠일이고",
          "speaker_id": "1",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009333.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ땀쟁이 어제오늘 안나가고 계속 에어컨 밑에 있었디..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ땀쟁이 어제오늘 안나가고 계속 에어컨 밑에 있었디..",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009333.1.8",
          "form": "몸이 쎄리하네여~",
          "original_form": "몸이 쎄리하네여~",
          "speaker_id": "2",
          "time": "20210822 22:48"
        },
        {
          "id": "MDRW2100009333.1.9",
          "form": "나가는 순간 비오듯이 흐르긋쥬 ㅋㅋㅋ",
          "original_form": "나가는 순간 비오듯이 흐르긋쥬 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009333.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ콧물날라 조심해라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ콧물날라 조심해라",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009333.1.11",
          "form": "콧물나면 요즘 답없음 ㅋㅋ",
          "original_form": "콧물나면 요즘 답없음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100009333.1.12",
          "form": "비오고 나면 이제 시원하긋네요~",
          "original_form": "비오고 나면 이제 시원하긋네요~",
          "speaker_id": "2",
          "time": "20210822 22:50"
        },
        {
          "id": "MDRW2100009333.1.13",
          "form": "글치 여름도 다 갔구먼",
          "original_form": "글치 여름도 다 갔구먼",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009333.1.14",
          "form": "언넝 추워짐 좋것네요~패딩만 입고다니게 ㅋㅋㅋㅋ",
          "original_form": "언넝 추워짐 좋것네요~패딩만 입고다니게 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009333.1.15",
          "form": "비온다 쪼메",
          "original_form": "비온다 쪼메",
          "speaker_id": "1",
          "time": "20210823 08:30"
        },
        {
          "id": "MDRW2100009333.1.16",
          "form": "글네여~비 부슬부슬오니 더 더운거 같져ㅠ",
          "original_form": "글네여~비 부슬부슬오니 더 더운거 같져ㅠ",
          "speaker_id": "2",
          "time": "20210823 08:45"
        },
        {
          "id": "MDRW2100009333.1.17",
          "form": "둘찌 보내고 왔는데 땀이땀이 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "둘찌 보내고 왔는데 땀이땀이 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:45"
        },
        {
          "id": "MDRW2100009333.1.18",
          "form": "ㅋㅋㅋㅋㅋㄱㄱㄱ 오늘의 땀 시작이가",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱㄱ 오늘의 땀 시작이가",
          "speaker_id": "1",
          "time": "20210823 08:47"
        },
        {
          "id": "MDRW2100009333.1.19",
          "form": "그래 습하네",
          "original_form": "그래 습하네",
          "speaker_id": "1",
          "time": "20210823 08:47"
        },
        {
          "id": "MDRW2100009333.1.20",
          "form": "확 내리지",
          "original_form": "확 내리지",
          "speaker_id": "1",
          "time": "20210823 08:47"
        },
        {
          "id": "MDRW2100009333.1.21",
          "form": "그니깐여~오늘은 에어컨없이는 안되긋어여ㅋㅋㅋㅋ",
          "original_form": "그니깐여~오늘은 에어컨없이는 안되긋어여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:56"
        },
        {
          "id": "MDRW2100009333.1.22",
          "form": "딱 하루네 ㅋㅋㅋㅋㅋ",
          "original_form": "딱 하루네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009333.1.23",
          "form": "ㅋㅋㅋㅋㅋ 어제 선풍기 틀었다드만",
          "original_form": "ㅋㅋㅋㅋㅋ 어제 선풍기 틀었다드만",
          "speaker_id": "1",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009333.1.24",
          "form": "에어컨 트니 춥다",
          "original_form": "에어컨 트니 춥다",
          "speaker_id": "1",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009333.1.25",
          "form": "안틀기는 습하고",
          "original_form": "안틀기는 습하고",
          "speaker_id": "1",
          "time": "20210823 10:14"
        },
        {
          "id": "MDRW2100009333.1.26",
          "form": "에어컨 틀고 있으니 날이 딱 적당하구만옄ㅋㅋㅋㅋㅋ",
          "original_form": "에어컨 틀고 있으니 날이 딱 적당하구만옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100009333.1.27",
          "form": "난 담주에 백신 맞기러했으요~",
          "original_form": "난 담주에 백신 맞기러했으요~",
          "speaker_id": "2",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100009333.1.28",
          "form": "담주에 비가 안와야는데 ㅋㅋㅋㅋㅋ",
          "original_form": "담주에 비가 안와야는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100009333.1.29",
          "form": "뚜벅이 우산도 못들라 ㅋㅋㅋ",
          "original_form": "뚜벅이 우산도 못들라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100009333.1.30",
          "form": "ㅋㅋㅋㅋㄱ 잘했네 원이랑 같이 우산쓰고 갔나??",
          "original_form": "ㅋㅋㅋㅋㄱ 잘했네 원이랑 같이 우산쓰고 갔나??",
          "speaker_id": "1",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009333.1.31",
          "form": "others가나??",
          "original_form": "&others&가나??",
          "speaker_id": "1",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009333.1.32",
          "form": "비쏟아지기 시작하네",
          "original_form": "비쏟아지기 시작하네",
          "speaker_id": "1",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009333.1.33",
          "form": "얍얍~우산쓰고 응응스크르갔다가~",
          "original_form": "얍얍~우산쓰고 응응스크르갔다가~",
          "speaker_id": "2",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009333.1.34",
          "form": "운동햇음 ㅋㅋㅋㅋ",
          "original_form": "운동햇음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009333.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ아침부터 돈썼네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ아침부터 돈썼네",
          "speaker_id": "1",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009333.1.36",
          "form": "잘했다 택시비보다 낫네",
          "original_form": "잘했다 택시비보다 낫네",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.37",
          "form": "피가 되고 살이되니께 ㅋㅋㅋ",
          "original_form": "피가 되고 살이되니께 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.38",
          "form": "살은안되도 되는데 ㅋㅋ",
          "original_form": "살은안되도 되는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.39",
          "form": "ㅋㅋㅋㅋㅋ 그래 살이 중요하지 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그래 살이 중요하지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.40",
          "form": "가을에 바지가 안드갈듯요 ..",
          "original_form": "가을에 바지가 안드갈듯요 ..",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.41",
          "form": "요새 미세먼지 없어서 좋다",
          "original_form": "요새 미세먼지 없어서 좋다",
          "speaker_id": "1",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.42",
          "form": "다 고무바지로 바꿔야함 ㅋㅋㅋ",
          "original_form": "다 고무바지로 바꿔야함 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:35"
        },
        {
          "id": "MDRW2100009333.1.43",
          "form": "지는 비 오는것도 좋아여~해 쨍쨍은 너무 진빠짐 ㅋㅋㅋ",
          "original_form": "지는 비 오는것도 좋아여~해 쨍쨍은 너무 진빠짐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:36"
        },
        {
          "id": "MDRW2100009333.1.44",
          "form": "ㅋㅋㅋㅋ 한창이네",
          "original_form": "ㅋㅋㅋㅋ 한창이네",
          "speaker_id": "1",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009333.1.45",
          "form": "비오는거 좋아하고",
          "original_form": "비오는거 좋아하고",
          "speaker_id": "1",
          "time": "20210823 10:39"
        },
        {
          "id": "MDRW2100009333.1.46",
          "form": "이제 비가 안 와요 해가 반짝 떴어요",
          "original_form": "이제 비가 안 와요 해가 반짝 떴어요",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100009333.1.47",
          "form": "원학교할 때 우산 걱정은 노노",
          "original_form": "원학교할 때 우산 걱정은 노노",
          "speaker_id": "2",
          "time": "20210827 13:15"
        }
      ]
