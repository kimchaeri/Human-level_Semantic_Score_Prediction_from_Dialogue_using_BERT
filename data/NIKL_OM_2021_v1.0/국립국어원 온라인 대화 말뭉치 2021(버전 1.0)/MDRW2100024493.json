      "utterance": [
        {
          "id": "MDRW2100024493.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100024493.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100024493.1.3",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100024493.1.4",
          "form": "불교이긴 한데 자주는 안가요",
          "original_form": "불교이긴 한데 자주는 안가요",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100024493.1.5",
          "form": "아하~ 저는 기독교에요!",
          "original_form": "아하~ 저는 기독교에요!",
          "speaker_id": "2",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100024493.1.6",
          "form": "요즘엔 유튜브에서 하니 좀 편하긴 하네요ㅎㅎ",
          "original_form": "요즘엔 유튜브에서 하니 좀 편하긴 하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100024493.1.7",
          "form": "모태신앙이세요?",
          "original_form": "모태신앙이세요?",
          "speaker_id": "1",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100024493.1.8",
          "form": "교회가면 친한 사람 많이 생겨요?",
          "original_form": "교회가면 친한 사람 많이 생겨요?",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024493.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100024493.1.10",
          "form": "그렇긴하더라구요",
          "original_form": "그렇긴하더라구요",
          "speaker_id": "2",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100024493.1.11",
          "form": "근데 크면서는 좀 뜸해져서ㅠ",
          "original_form": "근데 크면서는 좀 뜸해져서ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100024493.1.12",
          "form": "불교는 어떤가요?",
          "original_form": "불교는 어떤가요?",
          "speaker_id": "2",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100024493.1.13",
          "form": "불교는 거의 혼자다니는 분위기같아요",
          "original_form": "불교는 거의 혼자다니는 분위기같아요",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100024493.1.14",
          "form": "거기서 뭐 같이하는 사람들 보긴했는데...",
          "original_form": "거기서 뭐 같이하는 사람들 보긴했는데...",
          "speaker_id": "1",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100024493.1.15",
          "form": "아하.. 행사라던가 모임은 별로 없는편인가요?",
          "original_form": "아하.. 행사라던가 모임은 별로 없는편인가요?",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024493.1.16",
          "form": "교회가 젤 많은거같아요",
          "original_form": "교회가 젤 많은거같아요",
          "speaker_id": "1",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024493.1.17",
          "form": "절은 그걸 안해도 되는 분위기로 알아요",
          "original_form": "절은 그걸 안해도 되는 분위기로 알아요",
          "speaker_id": "1",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024493.1.18",
          "form": "그건 좋네요",
          "original_form": "그건 좋네요",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024493.1.19",
          "form": "너무 단합을 좋아하는건 좀 안맞더라구요ㅠ",
          "original_form": "너무 단합을 좋아하는건 좀 안맞더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024493.1.20",
          "form": "그러셨군요 저도 그럴거같아요",
          "original_form": "그러셨군요 저도 그럴거같아요",
          "speaker_id": "1",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100024493.1.21",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:09"
        },
        {
          "id": "MDRW2100024493.1.22",
          "form": "운동회? 이런것도 하는데 전 거의 안가오",
          "original_form": "운동회? 이런것도 하는데 전 거의 안가오",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100024493.1.23",
          "form": "운동회라니 민망하겠어요 안친하면...",
          "original_form": "운동회라니 민망하겠어요 안친하면...",
          "speaker_id": "1",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100024493.1.24",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100024493.1.25",
          "form": "친한사람 옆에만 있는데 인싸면 어휴",
          "original_form": "친한사람 옆에만 있는데 인싸면 어휴",
          "speaker_id": "2",
          "time": "20210831 12:15"
        }
      ]
    }
