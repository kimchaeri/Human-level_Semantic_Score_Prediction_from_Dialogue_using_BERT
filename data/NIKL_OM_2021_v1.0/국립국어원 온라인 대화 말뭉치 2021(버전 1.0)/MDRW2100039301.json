      "utterance": [
        {
          "id": "MDRW2100039301.1.1",
          "form": "콘서트 가보셨나요?",
          "original_form": "콘서트 가보셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100039301.1.2",
          "form": "예전엔 많이 갔었는데 너무 오래되서 기억이 안나네요 ㅋㅋ 거의 10년전엔 엄청 다녔었어요",
          "original_form": "예전엔 많이 갔었는데 너무 오래되서 기억이 안나네요 ㅋㅋ 거의 10년전엔 엄청 다녔었어요",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039301.1.3",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100039301.1.4",
          "form": "전 방탄 콘서트 가보고싶어요",
          "original_form": "전 방탄 콘서트 가보고싶어요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100039301.1.5",
          "form": "오 팬이신가요? 저는 노래만 좀 알아요 ㅋㅋ",
          "original_form": "오 팬이신가요? 저는 노래만 좀 알아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100039301.1.6",
          "form": "덕질해요 이나이에ㅠ",
          "original_form": "덕질해요 이나이에ㅠ",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039301.1.7",
          "form": "앗 아미시군요!!",
          "original_form": "앗 아미시군요!!",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039301.1.8",
          "form": "뭐 어때요 덕질도 취미죠 ㅎㅎ",
          "original_form": "뭐 어때요 덕질도 취미죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039301.1.9",
          "form": "절믄이들이 열심히 하더라구요ㅎ",
          "original_form": "절믄이들이 열심히 하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039301.1.10",
          "form": "젊은이 ㅋㅋㅋㅋ 근데 방탄 콘서트 가기 엄청 치열하죠?",
          "original_form": "젊은이 ㅋㅋㅋㅋ 근데 방탄 콘서트 가기 엄청 치열하죠?",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039301.1.11",
          "form": "네 그래서 온라인으로 봐요ㅎ",
          "original_form": "네 그래서 온라인으로 봐요ㅎ",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039301.1.12",
          "form": "아 요새 온라인으로도 콘서트하고 그러죠",
          "original_form": "아 요새 온라인으로도 콘서트하고 그러죠",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039301.1.13",
          "form": "네네 좋더라구요",
          "original_form": "네네 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039301.1.14",
          "form": "온라인으로 보면 집에서 보시나요? ㅎㅎ",
          "original_form": "온라인으로 보면 집에서 보시나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:18"
        },
        {
          "id": "MDRW2100039301.1.15",
