      "utterance": [
        {
          "id": "MDRW2100019040.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019040.1.2",
          "form": "점심 뭐 드셨어용",
          "original_form": "점심 뭐 드셨어용",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019040.1.3",
          "form": "저는오늘 육개장국밥먹었어요",
          "original_form": "저는오늘 육개장국밥먹었어요",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019040.1.4",
          "form": "ㅇ평소에도 잘 드시는 건가요",
          "original_form": "ㅇ평소에도 잘 드시는 건가요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019040.1.5",
          "form": "네 국밥좋아해서 자주먹어용",
          "original_form": "네 국밥좋아해서 자주먹어용",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019040.1.6",
          "form": "매운거 잘 드시나요 전 육개장도 좀 매운 편이라",
          "original_form": "매운거 잘 드시나요 전 육개장도 좀 매운 편이라",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019040.1.7",
          "form": "아 정말요? 저두 맵찔인데 육개장은 한번도 맵다생각한적 없어서요!",
          "original_form": "아 정말요? 저두 맵찔인데 육개장은 한번도 맵다생각한적 없어서요!",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019040.1.8",
          "form": "ㅋㅋㅋㅋㅋ제가 좀 심한 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ제가 좀 심한 거 같아요",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019040.1.9",
          "form": "아하! 그럴수있죠~ 매운거 몸에안좋아요 ㅋㅋ",
          "original_form": "아하! 그럴수있죠~ 매운거 몸에안좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019040.1.10",
          "form": "맞아요 맞아요 ㅜㅜ",
          "original_form": "맞아요 맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019040.1.11",
          "form": "그럼 느끼한음식좋아하세요? 피자나파스타같은",
          "original_form": "그럼 느끼한음식좋아하세요? 피자나파스타같은",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019040.1.12",
          "form": "그런 건 또 그나마 잘 먹는 편인 거 같아요",
          "original_form": "그런 건 또 그나마 잘 먹는 편인 거 같아요",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019040.1.13",
          "form": "아하! 저두 느끼한거좋아해요 ㅎㅎ",
          "original_form": "아하! 저두 느끼한거좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019040.1.14",
          "form": "이게 매운 거 잘 먹는 거랑 좀 상극인 걸까요ㅋㅋ",
          "original_form": "이게 매운 거 잘 먹는 거랑 좀 상극인 걸까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019040.1.15",
          "form": "그런거같아요 ㅋㅋ 근데아무래도 매운거싫어하면 선택지가 줄어들어서 약간 억지로좋아하게되는느낌도 있긴해요 ㅋㅋ",
          "original_form": "그런거같아요 ㅋㅋ 근데아무래도 매운거싫어하면 선택지가 줄어들어서 약간 억지로좋아하게되는느낌도 있긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019040.1.16",
          "form": "마자옄ㅋㅋ",
          "original_form": "마자옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019040.1.17",
          "form": "특히 한국인이라서 더더욱..",
          "original_form": "특히 한국인이라서 더더욱..",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019040.1.18",
          "form": "그쵸 ㅋㅋㅋ 근데사실 매운거못먹어도 세상엔 먹을게 차고넘쳐요~",
          "original_form": "그쵸 ㅋㅋㅋ 근데사실 매운거못먹어도 세상엔 먹을게 차고넘쳐요~",
          "speaker_id": "1",
