        {
          "id": "MDRW2100020274.1.1",
          "form": "그럼 지금은 둘째 계획은 없으신가요??",
          "original_form": "그럼 지금은 둘째 계획은 없으신가요??",
          "speaker_id": "1",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020274.1.2",
          "form": "아뇨ㅎㅎ",
          "original_form": "아뇨ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020274.1.3",
          "form": "아내가 완강하고 ㅡㅎㅎ 곧 이혼을 해요ㅎ",
          "original_form": "아내가 완강하고 ㅡㅎㅎ 곧 이혼을 해요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020274.1.4",
          "form": "그래서ㅎㅎㅎㅎ",
          "original_form": "그래서ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020274.1.5",
          "form": "네..?",
          "original_form": "네..?",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020274.1.6",
          "form": "반전이네요",
          "original_form": "반전이네요",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020274.1.7",
          "form": "다들 그래요ㅎㅎ",
          "original_form": "다들 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020274.1.8",
          "form": "참고살아요ㅎㅎ",
          "original_form": "참고살아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020274.1.9",
          "form": "결혼에 대한 환상보다는ㅎ 현실이라ㅎㅎ",
          "original_form": "결혼에 대한 환상보다는ㅎ 현실이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020274.1.10",
          "form": "아이가 태어나면 자주싸우고 힘들고 그래요 ㅡㅎㅎ",
          "original_form": "아이가 태어나면 자주싸우고 힘들고 그래요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020274.1.11",
          "form": "ㅠㅠ애키우는게 쉽지않죠..",
          "original_form": "ㅠㅠ애키우는게 쉽지않죠..",
          "speaker_id": "1",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020274.1.12",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020274.1.13",
          "form": "그래요ㅎㅎ",
          "original_form": "그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020274.1.14",
          "form": "name1은 여성분?",
          "original_form": "&name1&은 여성분?",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020274.1.15",
          "form": "넵 전 20대 여자에요!!",
          "original_form": "넵 전 20대 여자에요!!",
          "speaker_id": "1",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020274.1.16",
          "form": "근데 벌써 결혼하라고해요? 집에서?",
          "original_form": "근데 벌써 결혼하라고해요? 집에서?",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020274.1.17",
          "form": "아까 손주보고싶다고ㅎㅎ",
          "original_form": "아까 손주보고싶다고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020274.1.18",
          "form": "결혼보다 연애부터 하라고 얘기하시는데",
          "original_form": "결혼보다 연애부터 하라고 얘기하시는데",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020274.1.19",
          "form": "그냥 자기 친구들 손주본거 보면 부러워하더라구요 ㅋㅋㅋㅋ",
          "original_form": "그냥 자기 친구들 손주본거 보면 부러워하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020274.1.20",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020274.1.21",
          "form": "부러워하지만ㅎ",
          "original_form": "부러워하지만ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020274.1.22",
          "form": "그래도 너무빠르네요 ㅣㅎㅎ",
          "original_form": "그래도 너무빠르네요 ㅣㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020274.1.23",
          "form": "결혼을 빨리하고싶으세요?",
          "original_form": "결혼을 빨리하고싶으세요?",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020274.1.24",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020274.1.25",
          "form": "전 원래 결혼하고 애는 안낳고 살고싶었는데",
          "original_form": "전 원래 결혼하고 애는 안낳고 살고싶었는데",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020274.1.26",
          "form": "요즘엔 그냥 결혼안해도될것같아요",
          "original_form": "요즘엔 그냥 결혼안해도될것같아요",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020274.1.27",
          "form": "결혼하면 장점부터 말해드리고 싶지만",
          "original_form": "결혼하면 장점부터 말해드리고 싶지만",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020274.1.28",
          "form": "단점부터 말하자면ㅎㅎ",
          "original_form": "단점부터 말하자면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020274.1.29",
          "form": "집안일 육아 등등 할게많아져요ㅎ",
          "original_form": "집안일 육아 등등 할게많아져요ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020274.1.30",
          "form": "남편이 잘도와준다는 가정하에요ㅎ",
          "original_form": "남편이 잘도와준다는 가정하에요ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020274.1.31",
          "form": "name2은 잘 도와주셧나요",
          "original_form": "&name2&은 잘 도와주셧나요",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020274.1.32",
          "form": "근데 안도와주면 진짜 힘드실꺼에요ㅎ",
          "original_form": "근데 안도와주면 진짜 힘드실꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020274.1.33",
          "form": "전 나름 잘도와주는데ㅎ 그래도 만족을 못하더라구요ㅎ",
          "original_form": "전 나름 잘도와주는데ㅎ 그래도 만족을 못하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020274.1.34",
          "form": "제가 결혼한다면 집안일 잘하는 남자분과 결혼하고 싶었어요",
          "original_form": "제가 결혼한다면 집안일 잘하는 남자분과 결혼하고 싶었어요",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020274.1.35",
          "form": "아무래도 평생을 같이 살아온 가족들이랑도 싸우는데",
          "original_form": "아무래도 평생을 같이 살아온 가족들이랑도 싸우는데",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.36",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.37",
          "form": "남이랑 같이 산다는게 100퍼 만족하는게 쉽지않죠",
          "original_form": "남이랑 같이 산다는게 100퍼 만족하는게 쉽지않죠",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.38",
          "form": "아무래도 그래여",
          "original_form": "아무래도 그래여",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.39",
          "form": "그것땜에 결혼을 망설인것도 있어요 ㅋㅋ",
          "original_form": "그것땜에 결혼을 망설인것도 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.40",
          "form": "서로 맞춰가는데 시간이 걸리고",
          "original_form": "서로 맞춰가는데 시간이 걸리고",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.41",
          "form": "이해하는데두요 ㅡㅎ",
          "original_form": "이해하는데두요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.42",
          "form": "어려운것같애요ㅎ 아직도ㅎ",
          "original_form": "어려운것같애요ㅎ 아직도ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020274.1.43",
          "form": "전 이혼을 하게됫지만ㅎ 그게 누구의 잘못도 아니고ㅎ 서로의 잘못이라ㅎㅎ 근데 어려워요 정말ㅎ",
          "original_form": "전 이혼을 하게됫지만ㅎ 그게 누구의 잘못도 아니고ㅎ 서로의 잘못이라ㅎㅎ 근데 어려워요 정말ㅎ",
