      "utterance": [
        {
          "id": "MDRW2100005568.1.1",
          "form": "우유 커피 아몬드시럽 휘핑크림",
          "original_form": "우유 커피 아몬드시럽 휘핑크림",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.2",
          "form": "응응 ㅋㅋㅋ",
          "original_form": "응응 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.3",
          "form": "생크림으로 했는데",
          "original_form": "생크림으로 했는데",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.4",
          "form": "그냥 넣으면되나?",
          "original_form": "그냥 넣으면되나?",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.5",
          "form": "커피내리고",
          "original_form": "커피내리고",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.6",
          "form": "휘핑크림이",
          "original_form": "휘핑크림이",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.7",
          "form": "더 달달한듯",
          "original_form": "더 달달한듯",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.8",
          "form": "아몬드시럽 쪼메 넣었는데도 달더라",
          "original_form": "아몬드시럽 쪼메 넣었는데도 달더라",
          "speaker_id": "2",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005568.1.9",
          "form": "엉 얼음에 우유붓고",
          "original_form": "엉 얼음에 우유붓고",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005568.1.10",
          "form": "커피넣고",
          "original_form": "커피넣고",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005568.1.11",
          "form": "시럽에 휘핑섞어서 부어주세요",
          "original_form": "시럽에 휘핑섞어서 부어주세요",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005568.1.12",
          "form": "오케이 집에서 해봐야겠다.ㅋ",
          "original_form": "오케이 집에서 해봐야겠다.ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005568.1.13",
          "form": "맛있는 커피를 마시기 위해서ㅋㅋ",
          "original_form": "맛있는 커피를 마시기 위해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005568.1.14",
          "form": "그래 하루한잔 맛있게",
          "original_form": "그래 하루한잔 맛있게",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005568.1.15",
          "form": "아침에 밥은 못먹어도 커피는 내려서 출근함 ㅋㅋㅋ",
          "original_form": "아침에 밥은 못먹어도 커피는 내려서 출근함 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005568.1.16",
          "form": "오 여유있는 느낌인데?ㅋㅋ",
          "original_form": "오 여유있는 느낌인데?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005568.1.17",
          "form": "커피한잔과 시작하는구나ㅋㅋ",
          "original_form": "커피한잔과 시작하는구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005568.1.18",
          "form": "여유는 없고",
          "original_form": "여유는 없고",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005568.1.19",
          "form": "겨우겨우 커피들고 뛰어서 출근하지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "겨우겨우 커피들고 뛰어서 출근하지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005568.1.20",
          "form": "맨날 신호대기 걸림 덜덜 하잖아 ㅋㅋㅋㅋㅋ",
          "original_form": "맨날 신호대기 걸림 덜덜 하잖아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005568.1.21",
          "form": "ㅋㅋㅋㅋㅋ아 웃겨ㅋㅋ아침 안먹고 커피마시면 속쓰리지않나?",
          "original_form": "ㅋㅋㅋㅋㅋ아 웃겨ㅋㅋ아침 안먹고 커피마시면 속쓰리지않나?",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005568.1.22",
          "form": "뭐 아직은 개안네 ㅎㅎ",
          "original_form": "뭐 아직은 개안네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005568.1.23",
          "form": "그래도 요새는 좀 걱정이 되긴해서",
          "original_form": "그래도 요새는 좀 걱정이 되긴해서",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005568.1.24",
          "form": "물한잔 마시고",
          "original_form": "물한잔 마시고",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005568.1.25",
          "form": "ㅋㅋ 왠만하면 밥챙겨먹어",
          "original_form": "ㅋㅋ 왠만하면 밥챙겨먹어",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005568.1.26",
          "form": "커피마신다 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "커피마신다 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005568.1.27",
          "form": "아ㅋㅋㅋㅋㅋ물마시고 커피ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ물마시고 커피ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005568.1.28",
          "form": "빈속은 아니다 이거네ㅋㅋ",
          "original_form": "빈속은 아니다 이거네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005568.1.29",
          "form": "그렇지 ㅋㅋㅋㅋㅋㅋㅁ",
          "original_form": "그렇지 ㅋㅋㅋㅋㅋㅋㅁ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005568.1.30",
          "form": "아침에 일어나서",
          "original_form": "아침에 일어나서",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005568.1.31",
          "form": "물 마시면 좋다길래",
          "original_form": "물 마시면 좋다길래",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005568.1.32",
          "form": "일단 좋은거하고",
          "original_form": "일단 좋은거하고",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005568.1.33",
          "form": "안좋은거하기 ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱ",
          "original_form": "안좋은거하기 ㅋㅋㅋㅋㅋㅋㅋㄱㅋㄱ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005568.1.34",
          "form": "ㅋㅋㅋㅋ맞아 아침에 일어나서 물마시면 좋대ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아 아침에 일어나서 물마시면 좋대ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005568.1.35",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100005568.1.36",
