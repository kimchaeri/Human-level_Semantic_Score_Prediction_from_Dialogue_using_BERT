      "utterance": [
        {
          "id": "MDRW2100018412.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 01:21"
        },
        {
          "id": "MDRW2100018412.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 01:22"
        },
        {
          "id": "MDRW2100018412.1.3",
          "form": "설거지 바로하는 성격 vs 쌓아뒀다하는 성격",
          "original_form": "설거지 바로하는 성격 vs 쌓아뒀다하는 성격",
          "speaker_id": "1",
          "time": "20210827 01:25"
        },
        {
          "id": "MDRW2100018412.1.4",
          "form": "전에는 바로 했는데",
          "original_form": "전에는 바로 했는데",
          "speaker_id": "2",
          "time": "20210827 01:25"
        },
        {
          "id": "MDRW2100018412.1.5",
          "form": "요즘은 쌓아뒀다가 하고 있어요!",
          "original_form": "요즘은 쌓아뒀다가 하고 있어요!",
          "speaker_id": "2",
          "time": "20210827 01:25"
        },
        {
          "id": "MDRW2100018412.1.6",
          "form": "엇 저랑 반대네요 ㅎㅎㅎ!",
          "original_form": "엇 저랑 반대네요 ㅎㅎㅎ!",
          "speaker_id": "1",
          "time": "20210827 01:25"
        },
        {
          "id": "MDRW2100018412.1.7",
          "form": "전 예전엔 쌓아놨는데 요즘엔 바로바로 해요!",
          "original_form": "전 예전엔 쌓아놨는데 요즘엔 바로바로 해요!",
          "speaker_id": "1",
          "time": "20210827 01:25"
        },
        {
          "id": "MDRW2100018412.1.8",
          "form": "오옹 디게 부지런 하시네요!",
          "original_form": "오옹 디게 부지런 하시네요!",
          "speaker_id": "2",
          "time": "20210827 01:26"
        },
        {
          "id": "MDRW2100018412.1.9",
          "form": "요즘에 시간이 좀 많아져서 부지런해졌어요 ㅎㅎㅎ",
          "original_form": "요즘에 시간이 좀 많아져서 부지런해졌어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018412.1.10",
          "form": "맨날 방 닦고 쓸고...",
          "original_form": "맨날 방 닦고 쓸고...",
          "speaker_id": "1",
          "time": "20210827 01:28"
        },
        {
          "id": "MDRW2100018412.1.11",
          "form": "방청소 자주 하세요?",
          "original_form": "방청소 자주 하세요?",
          "speaker_id": "1",
          "time": "20210827 01:29"
        },
        {
          "id": "MDRW2100018412.1.12",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018412.1.13",
          "form": "설거지는 자주 안 하는데",
          "original_form": "설거지는 자주 안 하는데",
          "speaker_id": "2",
          "time": "20210827 01:39"
        },
        {
          "id": "MDRW2100018412.1.14",
          "form": "제 방은 자주 청소해요ㅎㅎ",
          "original_form": "제 방은 자주 청소해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:40"
        },
        {
          "id": "MDRW2100018412.1.15",
          "form": "확실히 청소를 자주 하는게 정신건강에 좋더라구요ㅎㅎㅎ",
          "original_form": "확실히 청소를 자주 하는게 정신건강에 좋더라구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018412.1.16",
          "form": "전 요즘 청소기를 새로사서 자주 하고 있어요",
          "original_form": "전 요즘 청소기를 새로사서 자주 하고 있어요",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018412.1.17",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018412.1.18",
          "form": "근데 가족이랑 안 살고 혼자사시는 걸까요??",
          "original_form": "근데 가족이랑 안 살고 혼자사시는 걸까요??",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018412.1.19",
          "form": "네 전 자취중이에요! name2은요?",
          "original_form": "네 전 자취중이에요! &name2&은요?",
          "speaker_id": "1",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018412.1.20",
          "form": "저는 가족이랑 같이 살고 있어요!!",
          "original_form": "저는 가족이랑 같이 살고 있어요!!",
          "speaker_id": "2",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018412.1.21",
          "form": "사실 집안일의 대부분은 제가 하고 있답니다..ㅎㅎ",
          "original_form": "사실 집안일의 대부분은 제가 하고 있답니다..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018412.1.22",
          "form": "ㅎㅎㅎ 저도.. 가족이랑 같이 살땐 다 제 담당이었어요...",
          "original_form": "ㅎㅎㅎ 저도.. 가족이랑 같이 살땐 다 제 담당이었어요...",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018412.1.23",
          "form": "맨날 빨래널고....",
          "original_form": "맨날 빨래널고....",
          "speaker_id": "1",
          "time": "20210827 01:57"
        },
        {
          "id": "MDRW2100018412.1.24",
          "form": "어머 저랑 똑같으시네요!",
          "original_form": "어머 저랑 똑같으시네요!",
          "speaker_id": "2",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018412.1.25",
          "form": "빨래널고 화장실 청소하고.. 쓰레기 치우고..",
          "original_form": "빨래널고 화장실 청소하고.. 쓰레기 치우고..",
          "speaker_id": "2",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018412.1.26",
          "form": "ㅋㅋㅋㅋㅋㅠ 내가 안하면.. 누가 하나 싶은 마음으로 했어요",
          "original_form": "ㅋㅋㅋㅋㅋㅠ 내가 안하면.. 누가 하나 싶은 마음으로 했어요",
          "speaker_id": "1",
          "time": "20210827 02:01"
        },
        {
          "id": "MDRW2100018412.1.27",
          "form": "그냥 빨리 독립하고 싶어요..ㅎㅎ",
          "original_form": "그냥 빨리 독립하고 싶어요..ㅎㅎ",
