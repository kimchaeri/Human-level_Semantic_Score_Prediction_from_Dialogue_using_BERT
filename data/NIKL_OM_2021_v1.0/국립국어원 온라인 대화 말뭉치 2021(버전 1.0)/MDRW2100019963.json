      "utterance": [
        {
          "id": "MDRW2100019963.1.1",
          "form": "그렇게 정말 건강하게",
          "original_form": "그렇게 정말 건강하게",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019963.1.2",
          "form": "서로 구속하지 않고 집착하지 않고",
          "original_form": "서로 구속하지 않고 집착하지 않고",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019963.1.3",
          "form": "관계를 어떤 말로 정의내리지 않는 사이가 되었어요",
          "original_form": "관계를 어떤 말로 정의내리지 않는 사이가 되었어요",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019963.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019963.1.5",
          "form": "상대방한테 감정 강요하지 않고",
          "original_form": "상대방한테 감정 강요하지 않고",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019963.1.6",
          "form": "약간 거리를 두시나요? 되게 미묘한 관계일거같아요",
          "original_form": "약간 거리를 두시나요? 되게 미묘한 관계일거같아요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.7",
          "form": "아니요! 아무말대잔치하고 놉니다!",
          "original_form": "아니요! 아무말대잔치하고 놉니다!",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.8",
          "form": "그 흔히 말하는",
          "original_form": "그 흔히 말하는",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.9",
          "form": "아 아예 편한 사이가 되셨구나",
          "original_form": "아 아예 편한 사이가 되셨구나",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.10",
          "form": "전애인과의 미묘한 관계 그런 게 없어요",
          "original_form": "전애인과의 미묘한 관계 그런 게 없어요",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.11",
          "form": "마음이 잘 맞으셨네요",
          "original_form": "마음이 잘 맞으셨네요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.12",
          "form": "부럽습니다 ㅜㅜ 건강한 관계",
          "original_form": "부럽습니다 ㅜㅜ 건강한 관계",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019963.1.13",
          "form": "사랑의 정의를 뒤집고 나서 이렇게 된 것 같슴다",
          "original_form": "사랑의 정의를 뒤집고 나서 이렇게 된 것 같슴다",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019963.1.14",
          "form": "사실 사랑과 우정의 차이도",
          "original_form": "사실 사랑과 우정의 차이도",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019963.1.15",
          "form": "그게 몸으로 구현된게 신기해요",
          "original_form": "그게 몸으로 구현된게 신기해요",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019963.1.16",
          "form": "인간이 만든 관념인디",
          "original_form": "인간이 만든 관념인디",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019963.1.17",
          "form": "저는 이성이 감정 못따라가더라고요 ㅋㅋㅋㅋ",
          "original_form": "저는 이성이 감정 못따라가더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019963.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 이리 될 줄은 몰랐어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 이리 될 줄은 몰랐어요",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019963.1.19",
          "form": "그 뭐랄까",
          "original_form": "그 뭐랄까",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019963.1.20",
          "form": "그 과정에서 이런 생각도 해봤던 것 같아요",
          "original_form": "그 과정에서 이런 생각도 해봤던 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019963.1.21",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019963.1.22",
          "form": "내가 상대에 대한 환상을 좋아하고 기대를 했던 건 아닌가",
          "original_form": "내가 상대에 대한 환상을 좋아하고 기대를 했던 건 아닌가",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019963.1.23",
          "form": "진짜 그 사람 그대로의 모습을 좋아했던건가",
          "original_form": "진짜 그 사람 그대로의 모습을 좋아했던건가",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019963.1.24",
          "form": "돌아봤는디 아니더라구요..?",
          "original_form": "돌아봤는디 아니더라구요..?",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019963.1.25",
          "form": "헐 사랑은 환상이었나요",
          "original_form": "헐 사랑은 환상이었나요",
          "speaker_id": "1",
