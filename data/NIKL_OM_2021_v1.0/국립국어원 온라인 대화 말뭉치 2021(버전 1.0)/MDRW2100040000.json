      "utterance": [
        {
          "id": "MDRW2100040000.1.1",
          "form": "저 성시경님 레시피 보고 만들었어요",
          "original_form": "저 성시경님 레시피 보고 만들었어요",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040000.1.2",
          "form": "오 성시경님 레시피가 있군요! \n검색해보고 다음에 만들어봐야겠어요",
          "original_form": "오 성시경님 레시피가 있군요! \n검색해보고 다음에 만들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100040000.1.3",
          "form": "ㅎㅎ 좋아요",
          "original_form": "ㅎㅎ 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100040000.1.4",
          "form": "맛있게 드시길 바래요",
          "original_form": "맛있게 드시길 바래요",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100040000.1.5",
          "form": "네 그럴게요",
          "original_form": "네 그럴게요",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040000.1.6",
          "form": "평소에 요리 많이 하시나요?",
          "original_form": "평소에 요리 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100040000.1.7",
          "form": "가끔 해요",
          "original_form": "가끔 해요",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040000.1.8",
          "form": "잘하는 편은 아니라서요",
          "original_form": "잘하는 편은 아니라서요",
          "speaker_id": "2",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040000.1.9",
          "form": "어떤 요리들을 주로 하시나요?",
          "original_form": "어떤 요리들을 주로 하시나요?",
          "speaker_id": "1",
          "time": "20210904 00:31"
        },
        {
          "id": "MDRW2100040000.1.10",
          "form": "쉬운건 거의 다 하고",
          "original_form": "쉬운건 거의 다 하고",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100040000.1.11",
          "form": "주로 밀키트 조리합니다ㅋㅋ",
          "original_form": "주로 밀키트 조리합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100040000.1.12",
          "form": "밀키트 요즘 넘 잘 나오고 종류 다양한거 같아요",
          "original_form": "밀키트 요즘 넘 잘 나오고 종류 다양한거 같아요",
          "speaker_id": "1",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100040000.1.13",
          "form": "맞아요 너무 많아지니까 오히려 고르기 힘들어요",
          "original_form": "맞아요 너무 많아지니까 오히려 고르기 힘들어요",
          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100040000.1.14",
          "form": "선택의 기회가 다양해져서 좋긴 한데\n대신 고민이 늘어나죠",
          "original_form": "선택의 기회가 다양해져서 좋긴 한데\n대신 고민이 늘어나죠",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100040000.1.15",
          "form": "맞아요 그래서 돌아가면서 사고 있어요",
          "original_form": "맞아요 그래서 돌아가면서 사고 있어요",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100040000.1.16",
          "form": "좋은 방법을 찾으셨네요!",
          "original_form": "좋은 방법을 찾으셨네요!",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100040000.1.17",
          "form": "추천해주시고 싶은 밀키트가 있을까요?",
          "original_form": "추천해주시고 싶은 밀키트가 있을까요?",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100040000.1.18",
          "form": "애슐리 밀키트는 다 맛있었어요",
          "original_form": "애슐리 밀키트는 다 맛있었어요",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100040000.1.19",
          "form": "애슐리 밀키트 맛있다니 참고해야겠어요",
          "original_form": "애슐리 밀키트 맛있다니 참고해야겠어요",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
