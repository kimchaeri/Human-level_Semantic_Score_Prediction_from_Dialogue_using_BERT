        },
        {
          "id": "MDRW2100045446.1.2",
          "form": "더위도 느끼나 봐요",
          "original_form": "더위도 느끼나 봐요",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045446.1.3",
          "form": "저도 그건 처음 보네요",
          "original_form": "저도 그건 처음 보네요",
          "speaker_id": "2",
          "time": "20210908 02:36"
        },
        {
          "id": "MDRW2100045446.1.4",
          "form": "아직 하루도 안 됐대요",
          "original_form": "아직 하루도 안 됐대요",
          "speaker_id": "1",
          "time": "20210908 02:36"
