      "utterance": [
        {
          "id": "MDRW2100020061.1.1",
          "form": "라인은 어려운데",
          "original_form": "라인은 어려운데",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020061.1.2",
          "form": "같은 방벽에 비해서",
          "original_form": "같은 방벽에 비해서",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020061.1.3",
          "form": "오리사는 괜찮더라고요",
          "original_form": "오리사는 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020061.1.4",
          "form": "아 맞아요 총쏠수있어서 그런가?",
          "original_form": "아 맞아요 총쏠수있어서 그런가?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.5",
          "form": "네 총 쏘는 것도",
          "original_form": "네 총 쏘는 것도",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.6",
          "form": "한조나 위도우 가 진짜 어렵지않나요?",
          "original_form": "한조나 위도우 가 진짜 어렵지않나요?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.7",
          "form": "텀 빠르게 잘 쏴지고",
          "original_form": "텀 빠르게 잘 쏴지고",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.8",
          "form": "우클릭으로 끌어당기는 거 하면",
          "original_form": "우클릭으로 끌어당기는 거 하면",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.9",
          "form": "은근 킬 잘 나더라고요",
          "original_form": "은근 킬 잘 나더라고요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.10",
          "form": "한조 위도우 어렵긴 한데 좀 하다보면 에임 느는 거 같아요",
          "original_form": "한조 위도우 어렵긴 한데 좀 하다보면 에임 느는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020061.1.11",
          "form": "오 하다보면",
          "original_form": "오 하다보면",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020061.1.12",
          "form": "느는구나",
          "original_form": "느는구나",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020061.1.13",
          "form": "저는 너무 어렵고",
          "original_form": "저는 너무 어렵고",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020061.1.14",
          "form": "트롤되더라고요 ㅋㅋ",
          "original_form": "트롤되더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020061.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 말만 늘얶지 그래봤자 트롤이에여ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 말만 늘얶지 그래봤자 트롤이에여ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020061.1.16",
          "form": "ㅋㅋㅋ전 루시우도 재밌어요",
          "original_form": "ㅋㅋㅋ전 루시우도 재밌어요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020061.1.17",
          "form": "거점 비비는게 재밌더라고요",
          "original_form": "거점 비비는게 재밌더라고요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020061.1.18",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020061.1.19",
          "form": "그리고 밀어내는 것도",
          "original_form": "그리고 밀어내는 것도",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020061.1.20",
          "form": "은근 쾌감 있고 재밌죠",
          "original_form": "은근 쾌감 있고 재밌죠",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020061.1.21",
          "form": "아 그 우물있는데나 거점전에서 특히 재밌죠",
          "original_form": "아 그 우물있는데나 거점전에서 특히 재밌죠",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020061.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020061.1.23",
          "form": "우물 있는 데ㅋㅋㅋㅋ",
          "original_form": "우물 있는 데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020061.1.24",
          "form": "거기는 파라도 좋아요",
          "original_form": "거기는 파라도 좋아요",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020061.1.25",
          "form": "로드호그나",
          "original_form": "로드호그나",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020061.1.26",
          "form": "파르시 진짜 재밌는데 ㅋㅋㅋ",
          "original_form": "파르시 진짜 재밌는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020061.1.27",
          "form": "오버워치 시네마틱도 가끔 보면 넘 재밌지않나요?",
          "original_form": "오버워치 시네마틱도 가끔 보면 넘 재밌지않나요?",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020061.1.28",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100020061.1.29",
          "form": "파르시 좋ㅁ죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "파르시 좋ㅁ죠ㅋㅋㅋㅋㅋㅋ",
