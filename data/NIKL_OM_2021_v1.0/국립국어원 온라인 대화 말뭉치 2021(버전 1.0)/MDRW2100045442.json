      "utterance": [
        {
          "id": "MDRW2100045442.1.1",
          "form": "아직 고민중이예요",
          "original_form": "아직 고민중이예요",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.2",
          "form": "신청은 하셨어요??",
          "original_form": "신청은 하셨어요??",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.3",
          "form": "네 오늘이요",
          "original_form": "네 오늘이요",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.4",
          "form": "아 어제",
          "original_form": "아 어제",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.5",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.7",
          "form": "제가 오늘이예요",
          "original_form": "제가 오늘이예요",
          "speaker_id": "2",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.9",
          "form": "네 전 렌즈살려구요",
          "original_form": "네 전 렌즈살려구요",
          "speaker_id": "1",
          "time": "20210908 02:33"
        },
        {
          "id": "MDRW2100045442.1.10",
          "form": "아 렌즈",
          "original_form": "아 렌즈",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.11",
          "form": "도수 있는건가요?",
          "original_form": "도수 있는건가요?",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.12",
          "form": "근대 안경도 살까 고민이에뇨",
          "original_form": "근대 안경도 살까 고민이에뇨",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.13",
          "form": "아니면 컬러렌즈인가요?",
          "original_form": "아니면 컬러렌즈인가요?",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.14",
          "form": "네 화장할때 안경 너무 거슬려서",
          "original_form": "네 화장할때 안경 너무 거슬려서",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.15",
          "form": "컬러도 도수있어요",
          "original_form": "컬러도 도수있어요",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.16",
          "form": "25망원이몬 둘다 사도 괜찮을 것 같은데요",
          "original_form": "25망원이몬 둘다 사도 괜찮을 것 같은데요",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.17",
          "form": "아 제가 너무 구시대 사람이라서 ;",
          "original_form": "아 제가 너무 구시대 사람이라서 ;",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.18",
          "form": "안경 맞춤지 얼마안ㄷ서",
          "original_form": "안경 맞춤지 얼마안ㄷ서",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.19",
          "form": "고민중",
          "original_form": "고민중",
          "speaker_id": "1",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 02:34"
        },
        {
          "id": "MDRW2100045442.1.21",
          "form": "렌즈 그러면",
          "original_form": "렌즈 그러면",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045442.1.22",
          "form": "넣을 수 있는지",
          "original_form": "넣을 수 있는지",
          "speaker_id": "2",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045442.1.23",
          "form": "맞춤지 1년안됨",
          "original_form": "맞춤지 1년안됨",
          "speaker_id": "1",
          "time": "20210908 02:35"
        },
        {
          "id": "MDRW2100045442.1.24",
          "form": "시험해보고 정하세요",
          "original_form": "시험해보고 정하세요",
          "speaker_id": "2",
