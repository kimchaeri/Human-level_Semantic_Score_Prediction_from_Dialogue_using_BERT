      "utterance": [
        {
          "id": "MDRW2100004919.1.1",
          "form": "이게무슨짓이야",
          "original_form": "이게무슨짓이야",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.2",
          "form": "그런고얌???",
          "original_form": "그런고얌???",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.3",
          "form": "오늘 날이",
          "original_form": "오늘 날이",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.4",
          "form": "비많이 온다",
          "original_form": "비많이 온다",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.5",
          "form": "오락가락하네",
          "original_form": "오락가락하네",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.6",
          "form": "지금은 비 안오지만",
          "original_form": "지금은 비 안오지만",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.7",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.8",
          "form": "바람소리도 완전 심하고",
          "original_form": "바람소리도 완전 심하고",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004919.1.9",
          "form": "구름도 하루종일 많아",
          "original_form": "구름도 하루종일 많아",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.10",
          "form": "오늘 플디 하려했는데",
          "original_form": "오늘 플디 하려했는데",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.11",
          "form": "뭉게뭉게",
          "original_form": "뭉게뭉게",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.12",
          "form": "바닥 질척해서",
          "original_form": "바닥 질척해서",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.13",
          "form": "못나갔잖아",
          "original_form": "못나갔잖아",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.14",
          "form": "내일은 나갈 수 있을까",
          "original_form": "내일은 나갈 수 있을까",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.15",
          "form": "이제 바람이 차가워",
          "original_form": "이제 바람이 차가워",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.16",
          "form": "담주도 비 계속 온대",
          "original_form": "담주도 비 계속 온대",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004919.1.17",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004919.1.18",
          "form": "내일은 나갈수있을거양",
          "original_form": "내일은 나갈수있을거양",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004919.1.19",
          "form": "가을인가봐",
          "original_form": "가을인가봐",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004919.1.20",
          "form": "아니면 집에서 홈트라도 해야지",
          "original_form": "아니면 집에서 홈트라도 해야지",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004919.1.21",
          "form": "나 가을옷 뽐뿌오잖아 또",
          "original_form": "나 가을옷 뽐뿌오잖아 또",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004919.1.22",
          "form": "오~멋진생각이야",
          "original_form": "오~멋진생각이야",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004919.1.23",
          "form": "가을옷이 또 많이없징",
          "original_form": "가을옷이 또 많이없징",
          "speaker_id": "1",
          "time": "20210821 19:20"
        }
      ]
    }
