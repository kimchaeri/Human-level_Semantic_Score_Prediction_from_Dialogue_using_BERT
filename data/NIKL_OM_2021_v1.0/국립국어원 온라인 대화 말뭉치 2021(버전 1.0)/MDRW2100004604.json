      "utterance": [
        {
          "id": "MDRW2100004604.1.1",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004604.1.2",
          "form": "아뇨 가본적 없어요!",
          "original_form": "아뇨 가본적 없어요!",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004604.1.3",
          "form": "가보고 싶은곳 있어요?",
          "original_form": "가보고 싶은곳 있어요?",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004604.1.4",
          "form": "저는 프랑스 가보고싶어요!",
          "original_form": "저는 프랑스 가보고싶어요!",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004604.1.5",
          "form": "국내여행은 좋아하세요?",
          "original_form": "국내여행은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004604.1.6",
          "form": "국내여행 좋아해요!",
          "original_form": "국내여행 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004604.1.7",
          "form": "지방사람이라 서울 나들이가 제일 재밌었네요",
          "original_form": "지방사람이라 서울 나들이가 제일 재밌었네요",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004604.1.8",
          "form": "서울에서 어디가는거 좋아하세요?",
          "original_form": "서울에서 어디가는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004604.1.9",
          "form": "홍대에 맛있는것도 많고 경복궁 가는것도 좋아해요!",
          "original_form": "홍대에 맛있는것도 많고 경복궁 가는것도 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004604.1.10",
          "form": "밤에 보면 좋은곳 있어요?",
          "original_form": "밤에 보면 좋은곳 있어요?",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004604.1.11",
          "form": "흠.. 경복궁 야경이 괜찮지 않을까요? 잘 모르겠네요",
          "original_form": "흠.. 경복궁 야경이 괜찮지 않을까요? 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004604.1.12",
          "form": "올해나 내년에 여행 계획 있으세요?",
          "original_form": "올해나 내년에 여행 계획 있으세요?",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004604.1.13",
          "form": "코로나 때문에 딱히 없네요",
          "original_form": "코로나 때문에 딱히 없네요",
          "speaker_id": "2",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004604.1.14",
          "form": "경주 한옥마을이나 그런곳도 좋아하세요?",
          "original_form": "경주 한옥마을이나 그런곳도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004604.1.15",
