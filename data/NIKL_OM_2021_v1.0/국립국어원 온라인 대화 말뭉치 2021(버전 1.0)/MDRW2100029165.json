        },
        {
          "id": "MDRW2100029165.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029165.1.3",
          "form": "최근 물건 구매 하셨나요",
          "original_form": "최근 물건 구매 하셨나요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029165.1.4",
          "form": "폰케이스 새로 샀어요",
          "original_form": "폰케이스 새로 샀어요",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029165.1.5",
          "form": "오 좋으셨겠어요",
          "original_form": "오 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029165.1.6",
          "form": "맘에드시나요 ㅎㅎ",
          "original_form": "맘에드시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029165.1.7",
          "form": "민트색인데 예쁘게 잘 나왔더라구요",
          "original_form": "민트색인데 예쁘게 잘 나왔더라구요",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029165.1.8",
          "form": "요즘 구매하신 거 있나요?",
          "original_form": "요즘 구매하신 거 있나요?",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029165.1.9",
          "form": "전 과자요ㅠ",
          "original_form": "전 과자요ㅠ",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029165.1.10",
          "form": "과자를좋아해서요,,ㅎㅎ",
          "original_form": "과자를좋아해서요,,ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029165.1.11",
          "form": "오... 한번에 많이 사시나요?",
          "original_form": "오... 한번에 많이 사시나요?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029165.1.12",
          "form": "네 7개씩 삽니당",
          "original_form": "네 7개씩 삽니당",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029165.1.13",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029165.1.14",
          "form": "전 과자를 많이 사먹진 않아요ㅋㅋ",
          "original_form": "전 과자를 많이 사먹진 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029165.1.15",
          "form": "쇼핑도 그때그때 사는거 같구요",
          "original_form": "쇼핑도 그때그때 사는거 같구요",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029165.1.16",
          "form": "그럼 뭘 주로 사서 드시나요?",
          "original_form": "그럼 뭘 주로 사서 드시나요?",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029165.1.17",
          "form": "보통 배달로 사먹어요",
          "original_form": "보통 배달로 사먹어요",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029165.1.18",
          "form": "냉동식품은 가끔 사긴 하네요",
          "original_form": "냉동식품은 가끔 사긴 하네요",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029165.1.19",
          "form": "오오 맞아요 냉동식품도 간편해서 좋아요",
          "original_form": "오오 맞아요 냉동식품도 간편해서 좋아요",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029165.1.20",
          "form": "요즘 어플로 많이 할인하던데 ㅎㅎ",
          "original_form": "요즘 어플로 많이 할인하던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029165.1.21",
          "form": "할인은 잘 못챙기는 편이라ㅎ",
          "original_form": "할인은 잘 못챙기는 편이라ㅎ",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029165.1.22",
          "form": "오뚜기 피자 맛있어서 잘 사먹어요",
          "original_form": "오뚜기 피자 맛있어서 잘 사먹어요",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029165.1.23",
          "form": "맞아요 요즘 오뚜기 피자 정말 잘나왔어요",
          "original_form": "맞아요 요즘 오뚜기 피자 정말 잘나왔어요",
