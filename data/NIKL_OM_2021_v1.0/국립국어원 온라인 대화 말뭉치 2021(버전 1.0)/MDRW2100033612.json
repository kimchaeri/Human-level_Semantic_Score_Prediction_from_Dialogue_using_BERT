      "utterance": [
        {
          "id": "MDRW2100033612.1.1",
          "form": "제동생은 자기필요할때만 누나라 그랬던거같아요 ㅋ",
          "original_form": "제동생은 자기필요할때만 누나라 그랬던거같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100033612.1.2",
          "form": "제친구는 오빠한테 맨날맞고 ㅎ",
          "original_form": "제친구는 오빠한테 맨날맞고 ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100033612.1.3",
          "form": "싸우고 그렇게 자랐대요",
          "original_form": "싸우고 그렇게 자랐대요",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033612.1.4",
          "form": "나이차이가 얼마 안나나봐요?",
          "original_form": "나이차이가 얼마 안나나봐요?",
          "speaker_id": "2",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033612.1.5",
          "form": "레스링하자하고 동생험하게 다루는 경우가 많은듯",
          "original_form": "레스링하자하고 동생험하게 다루는 경우가 많은듯",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033612.1.6",
          "form": "2살차인가",
          "original_form": "2살차인가",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033612.1.7",
          "form": "아...싸울만하네요 ㅎ",
          "original_form": "아...싸울만하네요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033612.1.8",
          "form": "그래도 크니 엄청 아끼더라구요 밤늦게 들어오면 걱정하고",
          "original_form": "그래도 크니 엄청 아끼더라구요 밤늦게 들어오면 걱정하고",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033612.1.9",
          "form": "나이차이많이 나면 보살핑받던데 ㅎㅎ",
          "original_form": "나이차이많이 나면 보살핑받던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033612.1.10",
          "form": "ㅎㅎㅎ 그래도 핏줄인가봐요",
          "original_form": "ㅎㅎㅎ 그래도 핏줄인가봐요",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033612.1.11",
          "form": "꼭그런건 또아닌듯요ㅎ",
          "original_form": "꼭그런건 또아닌듯요ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033612.1.12",
          "form": "제동생은 요새 공모주할시기에만 연락오는듯요 ㅋㅋ",
          "original_form": "제동생은 요새 공모주할시기에만 연락오는듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033612.1.13",
          "form": "필요할때만 찾는ㅎㅎ",
          "original_form": "필요할때만 찾는ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033612.1.14",
          "form": "얼마넣을껀데? 청약하나?",
          "original_form": "얼마넣을껀데? 청약하나?",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033612.1.15",
          "form": "이런것만 물어요 ㅋ",
          "original_form": "이런것만 물어요 ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033612.1.16",
          "form": "ㅋㅋ 그래도 좋은정보 서로 교환하고 좋은데요",
          "original_form": "ㅋㅋ 그래도 좋은정보 서로 교환하고 좋은데요",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033612.1.17",
          "form": "언니랑 자주 연락하세요?",
          "original_form": "언니랑 자주 연락하세요?",
          "speaker_id": "2",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033612.1.18",
          "form": "자매는 그래도 친하게 잘지내던데요",
          "original_form": "자매는 그래도 친하게 잘지내던데요",
          "speaker_id": "2",
          "time": "20210824 17:10"
        }
