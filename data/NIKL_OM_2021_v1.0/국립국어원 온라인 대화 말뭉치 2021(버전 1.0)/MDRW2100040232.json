      "utterance": [
        {
          "id": "MDRW2100040232.1.1",
          "form": "쇼핑에 이어",
          "original_form": "쇼핑에 이어",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.2",
          "form": "밥 먹었나요",
          "original_form": "밥 먹었나요",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.3",
          "form": "네네 등뼈김치찜!!!!!!",
          "original_form": "네네 등뼈김치찜!!!!!!",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.4",
          "form": "점심어떻게했어요?",
          "original_form": "점심어떻게했어요?",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.5",
          "form": "와따 맛난거 드셨네이",
          "original_form": "와따 맛난거 드셨네이",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.6",
          "form": "그춍!",
          "original_form": "그춍!",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.7",
          "form": "저는 아침에 사온 살러드 먹엇어요",
          "original_form": "저는 아침에 사온 살러드 먹엇어요",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.8",
          "form": "우웩",
          "original_form": "우웩",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.9",
          "form": "아니 샐러드로땡..?",
          "original_form": "아니 샐러드로땡..?",
          "speaker_id": "1",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100040232.1.10",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.11",
          "form": "네 입맛도 없고요",
          "original_form": "네 입맛도 없고요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.12",
          "form": "6시까지일하자나요...",
          "original_form": "6시까지일하자나요...",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.13",
          "form": "저녁에 친구만나요",
          "original_form": "저녁에 친구만나요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.14",
          "form": "그때먹으려고요",
          "original_form": "그때먹으려고요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.15",
          "form": "그거가지구돼요..ㅜㅜ?\r\n하입맛이없다니",
          "original_form": "그거가지구돼요..ㅜㅜ?\r\n하입맛이없다니",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.17",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.18",
          "form": "저녁은외식이네용",
          "original_form": "저녁은외식이네용{emoji:♡}",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.20",
          "form": "뭐먹으러가요?",
          "original_form": "뭐먹으러가요?",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.21",
          "form": "봄이가 혼자있는데ㅠㅠ 약속잡지말걸 ㅠㅠ",
          "original_form": "봄이가 혼자있는데ㅠㅠ 약속잡지말걸 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.22",
          "form": "아마 햄버거 테이크아윳",
          "original_form": "아마 햄버거 테이크아윳",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.23",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.24",
          "form": "3명이라서 어디못갈거같아요",
          "original_form": "3명이라서 어디못갈거같아요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.25",
          "form": "ㅜㅜ하",
          "original_form": "ㅜㅜ하",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.26",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100040232.1.27",
          "form": "봄이한텐미안하지만뚭도바람좀쐬고해야",
          "original_form": "봄이한텐미안하지만뚭도바람좀쐬고해야",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.28",
          "form": "스트레스도날아가고",
          "original_form": "스트레스도날아가고",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.29",
          "form": "하지요",
          "original_form": "하지요",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.30",
          "form": "콧바람숭숭",
          "original_form": "콧바람숭숭",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.31",
          "form": "그것도 그러네요",
          "original_form": "그것도 그러네요",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.33",
          "form": "고마워요 해워니",
          "original_form": "고마워요 해워니",
          "speaker_id": "2",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.34",
          "form": "계속이어해봐용",
          "original_form": "계속이어해봐용",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100040232.1.35",
