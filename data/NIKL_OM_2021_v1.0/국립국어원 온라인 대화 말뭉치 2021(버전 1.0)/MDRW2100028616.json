      "utterance": [
        {
          "id": "MDRW2100028616.1.1",
          "form": "장래희망이 모야?",
          "original_form": "장래희망이 모야?",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028616.1.2",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028616.1.3",
          "form": "확실하게 정해지지는 않았지만",
          "original_form": "확실하게 정해지지는 않았지만",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028616.1.4",
          "form": "심리상담사나 청소년 상담사가 되고싶어 ㅎㅎ",
          "original_form": "심리상담사나 청소년 상담사가 되고싶어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028616.1.5",
          "form": "언니는??",
          "original_form": "언니는??",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028616.1.6",
          "form": "어땡?",
          "original_form": "어땡?",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028616.1.7",
          "form": "난 그냥 시간의 흐름에 맡기구 싶어 ㅎㅎ",
          "original_form": "난 그냥 시간의 흐름에 맡기구 싶어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.8",
          "form": "청소년상담사는 학교에서 일하나?",
          "original_form": "청소년상담사는 학교에서 일하나?",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.9",
          "form": "그렇구나 ㅎㅎ",
          "original_form": "그렇구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.10",
          "form": "그렇지않을까?",
          "original_form": "그렇지않을까?",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.11",
          "form": "학교에서도 일하고",
          "original_form": "학교에서도 일하고",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.12",
          "form": "힘든 청소년들을 도와주기위해서",
          "original_form": "힘든 청소년들을 도와주기위해서",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.13",
          "form": "센터 같은곳에서도 일할꺼같아..",
          "original_form": "센터 같은곳에서도 일할꺼같아..",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.14",
          "form": "장래희망인데 아는게많이업네!",
          "original_form": "장래희망인데 아는게많이업네!",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028616.1.15",
          "form": "원래",
          "original_form": "원래",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.16",
          "form": "청소년은 아직 잘 모르지 ㅎㅎ",
          "original_form": "청소년은 아직 잘 모르지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.17",
          "form": "취업이나",
          "original_form": "취업이나",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.18",
          "form": "급여에대해서 ㅎㅎ",
          "original_form": "급여에대해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.19",
          "form": "언니는",
          "original_form": "언니는",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.20",
          "form": "스펙 많이 쌓았어?",
          "original_form": "스펙 많이 쌓았어?",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.21",
          "form": "하긴 고등학생들은",
          "original_form": "하긴 고등학생들은",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.22",
          "form": "어디 말 해봐",
          "original_form": "어디 말 해봐",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.23",
          "form": "대학입시가 젤 중요할 때지 ㅎㅎ",
          "original_form": "대학입시가 젤 중요할 때지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.24",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.25",
          "form": "취업은 대학생되구나서 천천히 생각해도",
          "original_form": "취업은 대학생되구나서 천천히 생각해도",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.26",
          "form": "괜찮아.",
          "original_form": "괜찮아.",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.27",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.28",
          "form": "그래서 언니는",
          "original_form": "그래서 언니는",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.29",
          "form": "스펙 많이",
          "original_form": "스펙 많이",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.30",
          "form": "쌓았어?",
          "original_form": "쌓았어?",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028616.1.31",
          "form": "그런건 물어보는거아니얌 ㅎㅎ",
          "original_form": "그런건 물어보는거아니얌 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028616.1.32",
          "form": "그렇구나 ㅎㅎ",
          "original_form": "그렇구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028616.1.33",
          "form": "알겠어 ㅎㅎ",
          "original_form": "알겠어 ㅎㅎ",
          "speaker_id": "1",
