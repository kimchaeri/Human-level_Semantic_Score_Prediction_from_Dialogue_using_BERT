      "utterance": [
        {
          "id": "MDRW2100026327.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026327.1.2",
          "form": "요새 게임 하는거 있어요?",
          "original_form": "요새 게임 하는거 있어요?",
          "speaker_id": "1",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026327.1.3",
          "form": "스위치를 사긴했는데",
          "original_form": "스위치를 사긴했는데",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026327.1.4",
          "form": "동숲 공사하다 힘들어서 방치중이에요 ㅋㅋ",
          "original_form": "동숲 공사하다 힘들어서 방치중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026327.1.5",
          "form": "오 동물의 숲 하셨나요?",
          "original_form": "오 동물의 숲 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026327.1.6",
          "form": "무슨 게임 하시나요",
          "original_form": "무슨 게임 하시나요",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026327.1.7",
          "form": "네네 동물만 열심히 모으다 접어버린 ㅋㅋㅋ",
          "original_form": "네네 동물만 열심히 모으다 접어버린 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026327.1.8",
          "form": "저는 하는거는 퍼즐같은거하고 보는거는 이것저것봐요",
          "original_form": "저는 하는거는 퍼즐같은거하고 보는거는 이것저것봐요",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026327.1.9",
          "form": "오 어디서 방송보세요",
          "original_form": "오 어디서 방송보세요",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026327.1.10",
          "form": "유튜브로 진짜 이것저것 보는데",
          "original_form": "유튜브로 진짜 이것저것 보는데",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026327.1.11",
          "form": "기억에 남는거는",
          "original_form": "기억에 남는거는",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026327.1.12",
          "form": "요즘은 메이플 뽑기요!",
          "original_form": "요즘은 메이플 뽑기요!",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026327.1.13",
          "form": "오 저는 트위치로 게임 생방 자주봐요",
          "original_form": "오 저는 트위치로 게임 생방 자주봐요",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026327.1.14",
          "form": "요즘 메이플은 너무많이 바껴서 뭐가 뭔지 모르겠네요 ㅜㅜ",
          "original_form": "요즘 메이플은 너무많이 바껴서 뭐가 뭔지 모르겠네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026327.1.15",
          "form": "오 무슨 게임 보세요?",
          "original_form": "오 무슨 게임 보세요?",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026327.1.16",
          "form": "엄청 옛날에 했었어서 ㅋㅋ",
          "original_form": "엄청 옛날에 했었어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026327.1.17",
          "form": "맞아요ㅠ 예전에 알던 그게임이 아니에요ㅠ",
          "original_form": "맞아요ㅠ 예전에 알던 그게임이 아니에요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026327.1.18",
          "form": "풍월량님 방송 자주봐요",
          "original_form": "풍월량님 방송 자주봐요",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026327.1.19",
          "form": "요즘 프로젝트 좀비 ? 이거 많이들 하시더라구요",
          "original_form": "요즘 프로젝트 좀비 ? 이거 많이들 하시더라구요",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026327.1.20",
          "form": "좀비 피하는 게임 인가요?",
          "original_form": "좀비 피하는 게임 인가요?",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026327.1.21",
          "form": "좀비 세상에서 살아남기 같아요 ㅎㅎ",
          "original_form": "좀비 세상에서 살아남기 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026327.1.22",
          "form": "물건 모아서 생존하기 하더라구요",
          "original_form": "물건 모아서 생존하기 하더라구요",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026327.1.23",
          "form": "퍼즐게임이면 핸드폰 게임인가봐요",
          "original_form": "퍼즐게임이면 핸드폰 게임인가봐요",
          "speaker_id": "2",
          "time": "20210901 21:40"
        }
      ]
    }
