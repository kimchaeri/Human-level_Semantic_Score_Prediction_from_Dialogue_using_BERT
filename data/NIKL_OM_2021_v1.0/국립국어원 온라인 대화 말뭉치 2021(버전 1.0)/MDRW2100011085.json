        {
          "id": "MDRW2100011085.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210824 00:44"
        },
        {
          "id": "MDRW2100011085.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 00:53"
        },
        {
          "id": "MDRW2100011085.1.3",
          "form": "여행 좋아하시나바요",
          "original_form": "여행 좋아하시나바요",
          "speaker_id": "1",
          "time": "20210824 00:53"
        },
        {
          "id": "MDRW2100011085.1.4",
          "form": "네 좋아하죠 ㅎㅎ",
          "original_form": "네 좋아하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100011085.1.5",
          "form": "근데 요즘 코로나때문에 넘 가기 어려워요ㅠㅠ",
          "original_form": "근데 요즘 코로나때문에 넘 가기 어려워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100011085.1.6",
          "form": "맞아요ㅠㅠ 인원제한도 그렇고..",
          "original_form": "맞아요ㅠㅠ 인원제한도 그렇고..",
          "speaker_id": "1",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100011085.1.7",
          "form": "해외 너무 가고싶네요",
          "original_form": "해외 너무 가고싶네요",
          "speaker_id": "1",
          "time": "20210824 01:02"
        },
        {
          "id": "MDRW2100011085.1.8",
          "form": "저 휴학하면 해외 가고싶었는데 해외는 커녕 국내여행도 많이 못가네요...ㅠㅠ 흑흑",
          "original_form": "저 휴학하면 해외 가고싶었는데 해외는 커녕 국내여행도 많이 못가네요...ㅠㅠ 흑흑",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011085.1.9",
          "form": "name1님은 여행 좋아하세요?",
          "original_form": "&name1&님은 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011085.1.10",
          "form": "여행 완전 좋아하죠ㅎㅎ",
          "original_form": "여행 완전 좋아하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:04"
        },
        {
          "id": "MDRW2100011085.1.11",
          "form": "자주가는 여행지 있으세요?",
          "original_form": "자주가는 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210824 01:05"
        },
        {
          "id": "MDRW2100011085.1.12",
          "form": "저는 제주도요!",
          "original_form": "저는 제주도요!",
          "speaker_id": "1",
          "time": "20210824 01:07"
        },
        {
          "id": "MDRW2100011085.1.13",
          "form": "아 제주도 좋져ㅠ",
          "original_form": "아 제주도 좋져ㅠ",
          "speaker_id": "2",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011085.1.14",
          "form": "제주도 가면 꼭 들려야하는곳 있나요?",
          "original_form": "제주도 가면 꼭 들려야하는곳 있나요?",
          "speaker_id": "2",
          "time": "20210824 01:14"
        },
        {
          "id": "MDRW2100011085.1.15",
          "form": "섭지코지 경치 너무좋아요!",
          "original_form": "섭지코지 경치 너무좋아요!",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011085.1.16",
          "form": "오 저는 섭지코지는 안가봤어요",
          "original_form": "오 저는 섭지코지는 안가봤어요",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011085.1.17",
          "form": "담에 가면 가봐야겠네요 ㅎㅎ",
          "original_form": "담에 가면 가봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011085.1.18",
          "form": "넵 꼭 가보세요ㅎㅎ",
          "original_form": "넵 꼭 가보세요ㅎㅎ",
