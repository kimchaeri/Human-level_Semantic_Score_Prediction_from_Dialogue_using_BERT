      "utterance": [
        {
          "id": "MDRW2100021759.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021759.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021759.1.3",
          "form": "최근에 무슨 책 읽으셨어요",
          "original_form": "최근에 무슨 책 읽으셨어요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021759.1.4",
          "form": "알사탕  이요 ㅎㅎ",
          "original_form": "알사탕  이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021759.1.5",
          "form": "어린이책인데  작가님 좋아해서",
          "original_form": "어린이책인데  작가님 좋아해서",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021759.1.6",
          "form": "어린이 책을 더 보게 되네요 ㄹㄹ",
          "original_form": "어린이 책을 더 보게 되네요 ㄹㄹ",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021759.1.7",
          "form": "검색하니 너무 사랑스런 책이네요",
          "original_form": "검색하니 너무 사랑스런 책이네요",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021759.1.8",
          "form": "요샌 이런 힐링하는 책이 좋은것 같아요",
          "original_form": "요샌 이런 힐링하는 책이 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021759.1.9",
          "form": "네 백희나 작가님 책인데  어란이 읽어도 힐링되요 ㅎㅎ",
          "original_form": "네 백희나 작가님 책인데  어란이 읽어도 힐링되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021759.1.10",
          "form": "ㅎㅎ 어른이인 저도 읽어봐야겠어요",
          "original_form": "ㅎㅎ 어른이인 저도 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021759.1.11",
          "form": "코로나블루 떨치게",
          "original_form": "코로나블루 떨치게",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021759.1.12",
          "form": "ㅎㅎ 힐링될거에요",
          "original_form": "ㅎㅎ 힐링될거에요",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021759.1.13",
          "form": "name2님은 어떠 책 좋아하셔요",
          "original_form": "&name2&님은 어떠 책 좋아하셔요",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021759.1.14",
          "form": "추리 미스터리 스릴러ㅎ 추미스 장르 많이 읽어요ㅎㅎ",
          "original_form": "추리 미스터리 스릴러ㅎ 추미스 장르 많이 읽어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021759.1.15",
          "form": "오ㅏ  책 자주 읽으실듯한 느낌!!",
          "original_form": "오ㅏ  책 자주 읽으실듯한 느낌!!",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021759.1.16",
          "form": "좀... 세속적이고 대중적인 소설들 좋아하죠",
          "original_form": "좀... 세속적이고 대중적인 소설들 좋아하죠",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021759.1.17",
          "form": "저는 확실히 나이드니 책을  안보게 되더라구요",
          "original_form": "저는 확실히 나이드니 책을  안보게 되더라구요",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021759.1.18",
          "form": "저도 그래요ㅜ",
          "original_form": "저도 그래요ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021759.1.19",
          "form": "이제 책을 고를 때도 읽기 편한 책들만 찾게 되네요",
          "original_form": "이제 책을 고를 때도 읽기 편한 책들만 찾게 되네요",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021759.1.20",
          "form": "그래서 전 동화가 좋은가봐요 ㄷㄷ",
          "original_form": "그래서 전 동화가 좋은가봐요 ㄷㄷ",
          "speaker_id": "1",
