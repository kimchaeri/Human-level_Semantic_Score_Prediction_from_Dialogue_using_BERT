      "utterance": [
        {
          "id": "MDRW2100043892.1.1",
          "form": "셜록홈즈는 그 자막도 신의한수인 것 같아요",
          "original_form": "셜록홈즈는 그 자막도 신의한수인 것 같아요",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043892.1.2",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043892.1.3",
          "form": "저는 더.빙도 좋게봤어요",
          "original_form": "저는 더.빙도 좋게봤어요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043892.1.4",
          "form": "더빙도 있었군요 .. 그버전은 더 드라마에 집중할 수 있겠네요",
          "original_form": "더빙도 있었군요 .. 그버전은 더 드라마에 집중할 수 있겠네요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043892.1.5",
          "form": "네 ㅋㅋ 케이비에스에서 전에 해줬어요",
          "original_form": "네 ㅋㅋ 케이비에스에서 전에 해줬어요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043892.1.6",
          "form": "퀄 좋더라구요",
          "original_form": "퀄 좋더라구요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043892.1.7",
          "form": "좋네요 ㅋㅋ 가끔 더빙으로도 보는거 좋더라구용",
          "original_form": "좋네요 ㅋㅋ 가끔 더빙으로도 보는거 좋더라구용",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043892.1.8",
          "form": "저도요 ㅋㅋ 더빙 번역이 더 어려다더니",
          "original_form": "저도요 ㅋㅋ 더빙 번역이 더 어려다더니",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043892.1.9",
          "form": "어렵",
          "original_form": "어렵",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043892.1.10",
          "form": "번역도 깔끔해서 더빙있음 가끔 봐요 ㅋㅋ",
          "original_form": "번역도 깔끔해서 더빙있음 가끔 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043892.1.11",
          "form": "맞아요 그 다른 언어인데 싱크 잘 맞는거보면 신기해요 ㅋㅋ",
          "original_form": "맞아요 그 다른 언어인데 싱크 잘 맞는거보면 신기해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043892.1.12",
          "form": "그쵸그쵸 ㅋㅋ",
          "original_form": "그쵸그쵸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043892.1.13",
          "form": "그리구 막 다른 효과음까지 가끔 덧입힌다 하더라구요",
          "original_form": "그리구 막 다른 효과음까지 가끔 덧입힌다 하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043892.1.14",
          "form": "와 그렇군요 ..! 그 요즘 표지판이나 메뉴판도 한국어로 바꾸던데 신기하더라구요",
          "original_form": "와 그렇군요 ..! 그 요즘 표지판이나 메뉴판도 한국어로 바꾸던데 신기하더라구요",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043892.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043892.1.16",
          "form": "진짜 기술이 발달했다 싶어요 ㅋㅋㅋㅋ",
          "original_form": "진짜 기술이 발달했다 싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043892.1.17",
          "form": "한국드라만줄",
          "original_form": "한국드라만줄",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043892.1.18",
          "form": "그러니까요 ㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043892.1.19",
          "form": "진짜 합성티가 하나도 안나더라구요",
          "original_form": "진짜 합성티가 하나도 안나더라구요",
