      "utterance": [
        {
          "id": "MDRW2100041075.1.1",
          "form": "ㅋㅋㅋㅋ 그쵸 무한확장...",
          "original_form": "ㅋㅋㅋㅋ 그쵸 무한확장...",
          "speaker_id": "2",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100041075.1.2",
          "form": "ㅋㅋㅋㅋㅋ 글고 엔시티 간혹 한번에 막 두세팀씩 나와서 혼란스러워요",
          "original_form": "ㅋㅋㅋㅋㅋ 글고 엔시티 간혹 한번에 막 두세팀씩 나와서 혼란스러워요",
          "speaker_id": "1",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100041075.1.3",
          "form": "아 동시컴백을 해요???",
          "original_form": "아 동시컴백을 해요???",
          "speaker_id": "2",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100041075.1.4",
          "form": "팬들이 싫어하겠다 ㅋㅋㅋㅋ",
          "original_form": "팬들이 싫어하겠다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100041075.1.5",
          "form": "ㅋㅋㅋㅋ아 맞아요 팬들 거품물더라구요",
          "original_form": "ㅋㅋㅋㅋ아 맞아요 팬들 거품물더라구요",
          "speaker_id": "1",
          "time": "20210906 13:36"
        },
        {
          "id": "MDRW2100041075.1.6",
          "form": "엔시티 드림이랑 일이칠 같이 나올때도잇구",
          "original_form": "엔시티 드림이랑 일이칠 같이 나올때도잇구",
          "speaker_id": "1",
          "time": "20210906 13:36"
        },
        {
          "id": "MDRW2100041075.1.7",
          "form": "진짜 왜 그런대ㅋㅋㅋㅋ;; 대형이면서 감이 없어요",
          "original_form": "진짜 왜 그런대ㅋㅋㅋㅋ;; 대형이면서 감이 없어요",
          "speaker_id": "2",
          "time": "20210906 14:17"
        },
        {
          "id": "MDRW2100041075.1.8",
          "form": "ㅋㅋㅋㅋㅋ 에셈 욕할때가 젤 재밋어요",
          "original_form": "ㅋㅋㅋㅋㅋ 에셈 욕할때가 젤 재밋어요",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100041075.1.9",
          "form": "ㅌㅋㅌㅋ 에셈 파세요?",
          "original_form": "ㅌㅋㅌㅋ 에셈 파세요?",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041075.1.10",
          "form": "덕질은 안하는데 에셈이 말할거리도많고 욕할거리도 많은거같아요",
          "original_form": "덕질은 안하는데 에셈이 말할거리도많고 욕할거리도 많은거같아요",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041075.1.11",
          "form": "아 ㅋㅋㅋㅋㅋ 전 요즘 덕질은 안해서 회사 자체엔 관심이 없어요",
          "original_form": "아 ㅋㅋㅋㅋㅋ 전 요즘 덕질은 안해서 회사 자체엔 관심이 없어요",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041075.1.12",
          "form": "아하 저 에스엠 주식도 삿거든요 에스파를 믿고 ㅋㅋㅋㅋ 그래서 회사욕할건 많아요",
          "original_form": "아하 저 에스엠 주식도 삿거든요 에스파를 믿고 ㅋㅋㅋㅋ 그래서 회사욕할건 많아요",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
