      "utterance": [
        {
          "id": "MDRW2100018914.1.1",
          "form": "동아리..",
          "original_form": "동아리..",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018914.1.2",
          "form": "저는 보드나 배구 같은 스포츠 하고 싶어요!",
          "original_form": "저는 보드나 배구 같은 스포츠 하고 싶어요!",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018914.1.3",
          "form": "오 운동 좋아하시나봐요!",
          "original_form": "오 운동 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018914.1.4",
          "form": "전 옛날로 돌아간다면 밴드 동아리 들어가고싶어요",
          "original_form": "전 옛날로 돌아간다면 밴드 동아리 들어가고싶어요",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018914.1.5",
          "form": "요즘 취미로 기타 공부하고있어요 ㅋㅋㅋ",
          "original_form": "요즘 취미로 기타 공부하고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018914.1.6",
          "form": "와 기타!!",
          "original_form": "와 기타!!",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018914.1.7",
          "form": "저는 음악 쪽은 영 아니라ㅋㅋㅋ",
          "original_form": "저는 음악 쪽은 영 아니라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018914.1.8",
          "form": "대학교로망같은거 잇으셧나요 ㅎㅎ",
          "original_form": "대학교로망같은거 잇으셧나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018914.1.9",
          "form": "대학 축제요ㅠㅠ",
          "original_form": "대학 축제요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018914.1.10",
          "form": "코로나 때문에 다 취소됐지만...ㅠㅠ",
          "original_form": "코로나 때문에 다 취소됐지만...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018914.1.11",
          "form": "아 역시..ㅠㅠ",
          "original_form": "아 역시..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018914.1.12",
          "form": "가수들도 공연오고",
          "original_form": "가수들도 공연오고",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018914.1.13",
          "form": "학교 주점이",
          "original_form": "학교 주점이",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018914.1.14",
          "form": "재미가있었죠..!",
          "original_form": "재미가있었죠..!",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018914.1.15",
          "form": "주점..!",
          "original_form": "주점..!",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018914.1.16",
          "form": "저희는 이제 주류 반입도 안 된대요...",
          "original_form": "저희는 이제 주류 반입도 안 된대요...",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018914.1.17",
          "form": "근데 지금은 아마 주점이 다 금지됐을거에요!!",
          "original_form": "근데 지금은 아마 주점이 다 금지됐을거에요!!",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018914.1.18",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018914.1.19",
          "form": "맞아요..옛날만큼 재밌는 분위기가",
          "original_form": "맞아요..옛날만큼 재밌는 분위기가",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018914.1.20",
          "form": "나오진않겟네요 ㅋㅋㅋ",
          "original_form": "나오진않겟네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018914.1.21",
          "form": "제가 너무 늙은이 같은 소리를..",
          "original_form": "제가 너무 늙은이 같은 소리를..",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018914.1.22",
          "form": "사고 같은 건 방지할 수 있겠지만 그래도 좀 아쉬워요ㅠ",
          "original_form": "사고 같은 건 방지할 수 있겠지만 그래도 좀 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018914.1.23",
          "form": "아니에요ㅋㅋㅋㅋ",
          "original_form": "아니에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018914.1.24",
          "form": "ㅋㅋㅋㅋ아무튼 다들 백신맞고있으니",
          "original_form": "ㅋㅋㅋㅋ아무튼 다들 백신맞고있으니",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018914.1.25",
          "form": "내년엔 축제를 즐길수 잇을거에요!!",
          "original_form": "내년엔 축제를 즐길수 잇을거에요!!",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018914.1.26",
          "form": "그랬으면 좋겠어요ㅠㅠ",
          "original_form": "그랬으면 좋겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:40"
        }
      ]
