      "utterance": [
        {
          "id": "MDRW2100002490.1.1",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.2",
          "form": "전 봄가을이요",
          "original_form": "전 봄가을이요",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.3",
          "form": "공기좋다는 가정하에...",
          "original_form": "공기좋다는 가정하에...",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.4",
          "form": "name1님!!!!!!",
          "original_form": "&name1&님!!!!!!",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.5",
          "form": "저도 봄가을요ㅋㅋ",
          "original_form": "저도 봄가을요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.6",
          "form": "ㅋㅋㅋ와정말",
          "original_form": "ㅋㅋㅋ와정말",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.7",
          "form": "도플갱어인가요",
          "original_form": "도플갱어인가요",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.8",
          "form": "어릴땐 여름 겨울이 좋았는데",
          "original_form": "어릴땐 여름 겨울이 좋았는데",
          "speaker_id": "2",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002490.1.9",
          "form": "봄가을이 매력 터지더라구요:)",
          "original_form": "봄가을이 매력 터지더라구요:)",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.10",
          "form": "저도 어릴땐 그랬는데",
          "original_form": "저도 어릴땐 그랬는데",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.11",
          "form": "요즘은 여름겨울날씨가",
          "original_form": "요즘은 여름겨울날씨가",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.12",
          "form": "너무극단적이라",
          "original_form": "너무극단적이라",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.13",
          "form": "힘들어요ㅜㅜ",
          "original_form": "힘들어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.14",
          "form": "봄이랑 가을 바람이 \r\n너무 기분 좋아여ㅋㅋㅋㅋ",
          "original_form": "봄이랑 가을 바람이 \r\n너무 기분 좋아여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.15",
          "form": "맞아여ㅋㅋ 극단적임ㅋㅋ",
          "original_form": "맞아여ㅋㅋ 극단적임ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002490.1.16",
          "form": "그래도 이제",
          "original_form": "그래도 이제",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002490.1.17",
          "form": "여름 좀 지나서",
          "original_form": "여름 좀 지나서",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002490.1.18",
          "form": "가을느낌 살짝 나는거같아요",
          "original_form": "가을느낌 살짝 나는거같아요",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002490.1.19",
          "form": "네:) 아침 저녁으로는\r\n제법 선선해졌어요^^",
          "original_form": "네:) 아침 저녁으로는\r\n제법 선선해졌어요^^",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002490.1.20",
          "form": "올 여름 정말 더웠는데\r\n이렇게 지나가네요~",
          "original_form": "올 여름 정말 더웠는데\r\n이렇게 지나가네요~",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002490.1.21",
          "form": "그래도 여름이 길진 않았던거같아요",
          "original_form": "그래도 여름이 길진 않았던거같아요",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002490.1.22",
          "form": "저는 유독 길게 느껴진 여름이였어요",
          "original_form": "저는 유독 길게 느껴진 여름이였어요",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002490.1.23",
          "form": "그러셨나요",
          "original_form": "그러셨나요",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002490.1.24",
          "form": "하긴 너무 더워서",
          "original_form": "하긴 너무 더워서",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002490.1.25",
          "form": "힘들긴했어요ㅜㅜ",
          "original_form": "힘들긴했어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002490.1.26",
          "form": "날은 덥고 마스크는 필수템이고\r\n4단계 격상되고ㅠㅠ",
          "original_form": "날은 덥고 마스크는 필수템이고\r\n4단계 격상되고ㅠㅠ",
