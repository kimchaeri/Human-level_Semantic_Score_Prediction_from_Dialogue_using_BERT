      "utterance": [
        {
          "id": "MDRW2100005662.1.1",
          "form": "빵먹었다 ㅋㅋㅋㅋㅋ",
          "original_form": "빵먹었다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005662.1.2",
          "form": "삼겹살이 맛이없어 빵을 먹음 ㅋㅋㅋㄱ",
          "original_form": "삼겹살이 맛이없어 빵을 먹음 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005662.1.3",
          "form": "아 웃겨 슬프다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 웃겨 슬프다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:41"
        },
        {
          "id": "MDRW2100005662.1.4",
          "form": "나는 소화가안되서",
          "original_form": "나는 소화가안되서",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005662.1.5",
          "form": "앉아있다ㅋ",
          "original_form": "앉아있다ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005662.1.6",
          "form": "저녁을 너무 먹었나봐",
          "original_form": "저녁을 너무 먹었나봐",
          "speaker_id": "1",
          "time": "20210821 23:42"
        },
        {
          "id": "MDRW2100005662.1.7",
          "form": "ㅋㅋㅋㄱㄱ 형님댁에서 포식하고 왔구먼",
          "original_form": "ㅋㅋㅋㄱㄱ 형님댁에서 포식하고 왔구먼",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005662.1.8",
          "form": "부럽군",
          "original_form": "부럽군",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005662.1.9",
          "form": "나도 내일 뭔가 맛있게 먹어봐야겠다 ㅋㅋㅋㅋㅋ",
          "original_form": "나도 내일 뭔가 맛있게 먹어봐야겠다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005662.1.10",
          "form": "응응 진짜 ㅋㅋㅋㅋ두그릇먹음ㅋㅋ",
          "original_form": "응응 진짜 ㅋㅋㅋㅋ두그릇먹음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005662.1.11",
          "form": "내일은 뭐해먹노? ㅠ",
          "original_form": "내일은 뭐해먹노? ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.12",
          "form": "빨래도 해야되고",
          "original_form": "빨래도 해야되고",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.13",
          "form": "할게많다",
          "original_form": "할게많다",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.15",
          "form": "내일 밥이 제일 걱정임",
          "original_form": "내일 밥이 제일 걱정임",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.16",
          "form": "매일 하루하루",
          "original_form": "매일 하루하루",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.17",
          "form": "밥이 제일 큰 숙제다",
          "original_form": "밥이 제일 큰 숙제다",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.18",
          "form": "육아도...청소도...일도...",
          "original_form": "육아도...청소도...일도...",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005662.1.19",
          "form": "밥이 제일 힘듦 ㅠ",
          "original_form": "밥이 제일 힘듦 ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005662.1.20",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005662.1.21",
          "form": "내일 또 뭐하고 시간보내노?",
          "original_form": "내일 또 뭐하고 시간보내노?",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005662.1.22",
          "form": "아이들이랑..",
          "original_form": "아이들이랑..",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005662.1.23",
          "form": "밥도 뭐할까? 고민만한다",
          "original_form": "밥도 뭐할까? 고민만한다",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005662.1.24",
          "form": "어여 자그라",
          "original_form": "어여 자그라",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005662.1.25",
          "form": "내일 또 긴긴 육아가 기다리고 있다",
          "original_form": "내일 또 긴긴 육아가 기다리고 있다",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005662.1.26",
          "form": "응 언니두 어서 자",
          "original_form": "응 언니두 어서 자",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005662.1.27",
          "form": "내일도 홧팅하자",
          "original_form": "내일도 홧팅하자",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005662.1.28",
          "form": "파이링",
          "original_form": "파이링",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005662.1.29",
          "form": "일어났나??",
          "original_form": "일어났나??",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100005662.1.30",
          "form": "일어났어.ㅋㅋ",
          "original_form": "일어났어.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005662.1.31",
          "form": "이제 밥 먹어야되는데 ㅋㅋ",
          "original_form": "이제 밥 먹어야되는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005662.1.32",
          "form": "뭘할지 고민이야",
          "original_form": "뭘할지 고민이야",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005662.1.33",
          "form": "언니 밥먹었어?",
          "original_form": "언니 밥먹었어?",
          "speaker_id": "1",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100005662.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아침부터 밥고민",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 아침부터 밥고민",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005662.1.35",
          "form": "일단 눈 뜨고 있고 아직 침대",
          "original_form": "일단 눈 뜨고 있고 아직 침대",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005662.1.36",
          "form": "오늘은 내가 늦잠자는 날이라",
          "original_form": "오늘은 내가 늦잠자는 날이라",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005662.1.37",
          "form": "신랑이 애들 밥차려줬음",
          "original_form": "신랑이 애들 밥차려줬음",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100005662.1.38",
          "form": "좋다좋다 ㅋㅋ",
          "original_form": "좋다좋다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005662.1.39",
          "form": "부럽다ㅋㅋ",
          "original_form": "부럽다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005662.1.40",
          "form": "나가기가 싫네",
          "original_form": "나가기가 싫네",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005662.1.41",
          "form": "나는 내가 차려주길 기다리는 사람들이 많다",
          "original_form": "나는 내가 차려주길 기다리는 사람들이 많다",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005662.1.42",
          "form": "아효",
          "original_form": "아효",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005662.1.43",
          "form": "문밖으로 나가는 순간부터 일해야되는...",
          "original_form": "문밖으로 나가는 순간부터 일해야되는...",
          "speaker_id": "2",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100005662.1.44",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 10:10"
        }
      ]
    }
