      "utterance": [
        {
          "id": "MDRW2100029403.1.1",
          "form": "좋아하는 계절 있나요?",
          "original_form": "좋아하는 계절 있나요?",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100029403.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100029403.1.3",
          "form": "마침 오는 계절이네요",
          "original_form": "마침 오는 계절이네요",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100029403.1.4",
          "form": "가을 좋아하시는군요",
          "original_form": "가을 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100029403.1.5",
          "form": "저도 가을 좋아해요",
          "original_form": "저도 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029403.1.6",
          "form": "단풍이 예뻐서요",
          "original_form": "단풍이 예뻐서요",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029403.1.7",
          "form": "name2은 어떤 이유가 있을까요?",
          "original_form": "&name2&은 어떤 이유가 있을까요?",
          "speaker_id": "1",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029403.1.8",
          "form": "뭐니뭐니해도",
          "original_form": "뭐니뭐니해도",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029403.1.9",
          "form": "온도죠",
          "original_form": "온도죠",
          "speaker_id": "2",
          "time": "20210903 09:25"
        },
        {
          "id": "MDRW2100029403.1.10",
          "form": "거기에 말씀하신 것처럼",
          "original_form": "거기에 말씀하신 것처럼",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100029403.1.11",
          "form": "단풍도 들고 해서",
          "original_form": "단풍도 들고 해서",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100029403.1.12",
          "form": "분위기도 있고요",
          "original_form": "분위기도 있고요",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
          "id": "MDRW2100029403.1.13",
          "form": "가을분위기 좋죠",
          "original_form": "가을분위기 좋죠",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100029403.1.14",
          "form": "또 먹거리도 풍부하고요",
          "original_form": "또 먹거리도 풍부하고요",
          "speaker_id": "1",
          "time": "20210903 09:31"
        },
        {
          "id": "MDRW2100029403.1.15",
          "form": "가을에 먹을 수 있는 감이 좋아요",
          "original_form": "가을에 먹을 수 있는 감이 좋아요",
          "speaker_id": "1",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100029403.1.16",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029403.1.17",
          "form": "흔히 말하는 오곡백과의 계절...",
          "original_form": "흔히 말하는 오곡백과의 계절...",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029403.1.18",
          "form": "그래서 가을은 색도 풍성하고 먹거리도 풍성해서 기분이 좋아요",
          "original_form": "그래서 가을은 색도 풍성하고 먹거리도 풍성해서 기분이 좋아요",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100029403.1.19",
          "form": "확실히",
          "original_form": "확실히",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029403.1.20",
          "form": "겨울마냥",
          "original_form": "겨울마냥",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029403.1.21",
          "form": "황량하지도 않고",
          "original_form": "황량하지도 않고",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029403.1.22",
          "form": "풍성하면서 알록달록 하니깐요",
          "original_form": "풍성하면서 알록달록 하니깐요",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029403.1.23",
          "form": "풍성...은 아닌가",
          "original_form": "풍성...은 아닌가",
          "speaker_id": "2",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100029403.1.24",
          "form": "싫어하는 계절은 따로 있나요?",
          "original_form": "싫어하는 계절은 따로 있나요?",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029403.1.25",
          "form": "여름이요...",
          "original_form": "여름이요...",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029403.1.26",
          "form": "그러다보니 가을을 더 좋아하는 것일수도 있겠네요",
          "original_form": "그러다보니 가을을 더 좋아하는 것일수도 있겠네요",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029403.1.27",
          "form": "아.. 이해되네요.. 저도 더위타는 체질이라 여름 안좋아해요",
          "original_form": "아.. 이해되네요.. 저도 더위타는 체질이라 여름 안좋아해요",
          "speaker_id": "1",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100029403.1.28",
          "form": "그러다보니",
          "original_form": "그러다보니",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029403.1.29",
          "form": "여름이 좋아 겨울이 좋아",
          "original_form": "여름이 좋아 겨울이 좋아",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029403.1.30",
          "form": "질문에",
          "original_form": "질문에",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029403.1.31",
          "form": "저는 고민도 안하고",
          "original_form": "저는 고민도 안하고",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029403.1.32",
          "form": "겨울이 낫다고 말하죠",
          "original_form": "겨울이 낫다고 말하죠",
          "speaker_id": "2",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100029403.1.33",
          "form": "저도 둘 중에만 골라야 하나면 겨울이에요",
          "original_form": "저도 둘 중에만 골라야 하나면 겨울이에요",
          "speaker_id": "1",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100029403.1.34",
          "form": "추운건 옷 입으면 되는데 더운건 답이 없어요",
          "original_form": "추운건 옷 입으면 되는데 더운건 답이 없어요",
          "speaker_id": "1",
          "time": "20210903 10:20"
        }
      ]
