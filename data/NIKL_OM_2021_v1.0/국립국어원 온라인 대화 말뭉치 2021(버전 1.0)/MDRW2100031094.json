      "utterance": [
        {
          "id": "MDRW2100031094.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031094.1.2",
          "form": "이과에요 문과에요?",
          "original_form": "이과에요 문과에요?",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031094.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031094.1.4",
          "form": "저는 완전 문과입니다ㅎㅎ",
          "original_form": "저는 완전 문과입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031094.1.5",
          "form": "어학과예요",
          "original_form": "어학과예요",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031094.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031094.1.7",
          "form": "오 멋져요 저는 이과에요!",
          "original_form": "오 멋져요 저는 이과에요!",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.8",
          "form": "어학과면 외고...?이신건가요?",
          "original_form": "어학과면 외고...?이신건가요?",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.9",
          "form": "헉 그게 더 멋지네요",
          "original_form": "헉 그게 더 멋지네요",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.10",
          "form": "아 저는 대학생입니다ㅋㅋㅋ",
          "original_form": "아 저는 대학생입니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.11",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.12",
          "form": "고3이신건가요?",
          "original_form": "고3이신건가요?",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.13",
          "form": "ㅋㅋㅋㅋㅋ아니요 저도 대학생이요",
          "original_form": "ㅋㅋㅋㅋㅋ아니요 저도 대학생이요",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031094.1.14",
          "form": "ㅋㅋㅋㅋㅋ순간 어학과라고해서 외곤가 했네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ순간 어학과라고해서 외곤가 했네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031094.1.15",
          "form": "언어 학문이라니 너무 멋져요!!",
          "original_form": "언어 학문이라니 너무 멋져요!!",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031094.1.16",
          "form": "ㅋㅋㅋㅋ아ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031094.1.17",
          "form": "그럼 실험수업같은것때문에 학교도 가보고하시겠어요!",
          "original_form": "그럼 실험수업같은것때문에 학교도 가보고하시겠어요!",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031094.1.18",
          "form": "이게 실험수업만 대면으로 하니까 참 문제에요ㅋㅋㅠㅠ",
          "original_form": "이게 실험수업만 대면으로 하니까 참 문제에요ㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031094.1.19",
          "form": "다른건 비대면일때 실험은 거의 대면이라...",
          "original_form": "다른건 비대면일때 실험은 거의 대면이라...",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031094.1.20",
          "form": "앟ㅋㅋㅋㅋ제친구도 그얘기하더라구요.",
          "original_form": "앟ㅋㅋㅋㅋ제친구도 그얘기하더라구요.",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031094.1.21",
          "form": "그수업만을 위해 가야한다고..",
          "original_form": "그수업만을 위해 가야한다고..",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031094.1.22",
          "form": "그래도 아직 대학교 강의실에서 수업듣는 로망이 있네요ㅜㅜ",
          "original_form": "그래도 아직 대학교 강의실에서 수업듣는 로망이 있네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031094.1.23",
          "form": "아 님도 비대면으로만 계속 다니신거에요?ㅠㅠ",
          "original_form": "아 님도 비대면으로만 계속 다니신거에요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 16:47"
        },
        {
          "id": "MDRW2100031094.1.24",
          "form": "네네ㅜㅜ",
          "original_form": "네네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031094.1.25",
          "form": "동아리활동은 하시나요??",
          "original_form": "동아리활동은 하시나요??",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031094.1.26",
          "form": "저는 지난학기까지 안했어서",
          "original_form": "저는 지난학기까지 안했어서",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031094.1.27",
          "form": "진짜대학사람들 만날일이없었네요ㅋㅋㅋ",
          "original_form": "진짜대학사람들 만날일이없었네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100031094.1.28",
          "form": "비대면이라 그냥 동아리도 활동안해서 대학사람들 만날일이 없어요ㅋㅋㅋ",
          "original_form": "비대면이라 그냥 동아리도 활동안해서 대학사람들 만날일이 없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100031094.1.29",
          "form": "한편으론 편한데 아쉽기도 하고요",
          "original_form": "한편으론 편한데 아쉽기도 하고요",
          "speaker_id": "1",
          "time": "20210904 16:51"
        }
      ]
