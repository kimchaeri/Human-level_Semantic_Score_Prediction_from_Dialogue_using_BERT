      "utterance": [
        {
          "id": "MDRW2100046528.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211028 18:51"
        },
        {
          "id": "MDRW2100046528.1.2",
          "form": "안녕하세요! 저녁은 드셨나요",
          "original_form": "안녕하세요! 저녁은 드셨나요",
          "speaker_id": "2",
          "time": "20211028 19:16"
        },
        {
          "id": "MDRW2100046528.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211028 19:20"
        },
        {
          "id": "MDRW2100046528.1.4",
          "form": "저녁먹고 커피한잔 마셨습니다",
          "original_form": "저녁먹고 커피한잔 마셨습니다",
          "speaker_id": "1",
          "time": "20211028 19:20"
        },
        {
          "id": "MDRW2100046528.1.5",
          "form": "밤인데 커피 괜찮으신가요",
          "original_form": "밤인데 커피 괜찮으신가요",
          "speaker_id": "2",
          "time": "20211028 19:24"
        },
        {
          "id": "MDRW2100046528.1.6",
          "form": "네 저는 누우면 자요",
          "original_form": "네 저는 누우면 자요",
          "speaker_id": "1",
          "time": "20211028 19:26"
        },
        {
          "id": "MDRW2100046528.1.7",
          "form": "ㅋㅋㅋ카페인과 전혀 상관없으시군요",
          "original_form": "ㅋㅋㅋ카페인과 전혀 상관없으시군요",
          "speaker_id": "2",
          "time": "20211028 19:27"
        },
        {
          "id": "MDRW2100046528.1.8",
          "form": "넵ㅋㅋ",
          "original_form": "넵ㅋㅋ",
          "speaker_id": "1",
          "time": "20211028 19:27"
        },
        {
          "id": "MDRW2100046528.1.9",
          "form": "많이 바쁘신가보네요",
          "original_form": "많이 바쁘신가보네요",
          "speaker_id": "1",
          "time": "20211028 19:40"
        },
        {
          "id": "MDRW2100046528.1.10",
          "form": "아유 네 이제 집에 와서요",
          "original_form": "아유 네 이제 집에 와서요",
          "speaker_id": "2",
          "time": "20211028 19:40"
        },
        {
          "id": "MDRW2100046528.1.11",
          "form": "저는 밤에 카페인 먹으면 잠을 못자서 커피는 아침에만 먹는데.. 부럽네요",
          "original_form": "저는 밤에 카페인 먹으면 잠을 못자서 커피는 아침에만 먹는데.. 부럽네요",
          "speaker_id": "2",
          "time": "20211028 19:41"
        },
        {
          "id": "MDRW2100046528.1.12",
          "form": "아..식사 하셔야죠?",
          "original_form": "아..식사 하셔야죠?",
          "speaker_id": "1",
          "time": "20211028 19:43"
        },
        {
          "id": "MDRW2100046528.1.13",
          "form": "얼른 하세요",
          "original_form": "얼른 하세요",
          "speaker_id": "1",
          "time": "20211028 19:43"
        },
        {
          "id": "MDRW2100046528.1.14",
          "form": "아 간단히 라면 먹었습니다 ㅋㅋ",
          "original_form": "아 간단히 라면 먹었습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 19:46"
        },
        {
          "id": "MDRW2100046528.1.15",
          "form": "날씨가 이러니 라면이 갑자기 땡기더라구요",
          "original_form": "날씨가 이러니 라면이 갑자기 땡기더라구요",
          "speaker_id": "2",
          "time": "20211028 19:47"
        },
        {
          "id": "MDRW2100046528.1.16",
          "form": "넵 라면도 맛있죠",
          "original_form": "넵 라면도 맛있죠",
          "speaker_id": "1",
          "time": "20211028 19:49"
        },
        {
          "id": "MDRW2100046528.1.17",
          "form": "뭐 특별히 좋아하는 라면 있으세요?",
          "original_form": "뭐 특별히 좋아하는 라면 있으세요?",
          "speaker_id": "2",
          "time": "20211028 19:50"
        },
        {
          "id": "MDRW2100046528.1.18",
          "form": "전 육개장 사발면 좋아하거든요",
          "original_form": "전 육개장 사발면 좋아하거든요",
          "speaker_id": "2",
          "time": "20211028 19:50"
        },
        {
          "id": "MDRW2100046528.1.19",
          "form": "전 널큰한 너구리요",
          "original_form": "전 널큰한 너구리요",
          "speaker_id": "1",
          "time": "20211028 19:50"
        },
        {
          "id": "MDRW2100046528.1.20",
          "form": "오 너구리 맛있죠 면발도 통통하고",
          "original_form": "오 너구리 맛있죠 면발도 통통하고",
          "speaker_id": "2",
          "time": "20211028 19:50"
        },
        {
          "id": "MDRW2100046528.1.21",
          "form": "네 청양하고 콩나물 추가해서 먹으면 아주 맛있더라구요",
          "original_form": "네 청양하고 콩나물 추가해서 먹으면 아주 맛있더라구요",
          "speaker_id": "1",
          "time": "20211028 19:50"
        }
      ]
