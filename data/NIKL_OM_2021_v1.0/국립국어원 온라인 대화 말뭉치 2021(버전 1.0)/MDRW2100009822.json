      "utterance": [
        {
          "id": "MDRW2100009822.1.1",
          "form": "연애는 얼마나 하고 결혼하신거에요?",
          "original_form": "연애는 얼마나 하고 결혼하신거에요?",
          "speaker_id": "1",
          "time": "20210823 10:58"
        },
        {
          "id": "MDRW2100009822.1.2",
          "form": "저 2년정도?ㅋㅋ",
          "original_form": "저 2년정도?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009822.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009822.1.4",
          "form": "저는 6년 연애하고 결혼했어요 ㅎ",
          "original_form": "저는 6년 연애하고 결혼했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100009822.1.5",
          "form": "우와 오래하셨네요",
          "original_form": "우와 오래하셨네요",
          "speaker_id": "2",
          "time": "20210823 12:56"
        },
        {
          "id": "MDRW2100009822.1.6",
          "form": "네~ 오래 사귀고 결혼한 편인 것 같아요",
          "original_form": "네~ 오래 사귀고 결혼한 편인 것 같아요",
          "speaker_id": "1",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009822.1.7",
          "form": "대단하십니다!",
          "original_form": "대단하십니다!",
          "speaker_id": "2",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100009822.1.8",
          "form": "어쩌다보니 그렇게 됐더라고욬ㅋ",
          "original_form": "어쩌다보니 그렇게 됐더라고욬ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100009822.1.9",
          "form": "보통 같이 출근하고 같이 퇴근하세요?",
          "original_form": "보통 같이 출근하고 같이 퇴근하세요?",
          "speaker_id": "1",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100009822.1.10",
          "form": "네 지금은요~~ 조가 같아서",
          "original_form": "네 지금은요~~ 조가 같아서",
          "speaker_id": "2",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100009822.1.11",
          "form": "이제 곧 달라질겁니다!",
          "original_form": "이제 곧 달라질겁니다!",
          "speaker_id": "2",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100009822.1.12",
          "form": "출퇴근 같이하는게 좋지 않나요?",
          "original_form": "출퇴근 같이하는게 좋지 않나요?",
          "speaker_id": "1",
          "time": "20210823 13:58"
        },
        {
          "id": "MDRW2100009822.1.13",
          "form": "장단점이 ㅎㅎ",
          "original_form": "장단점이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009822.1.14",
          "form": "근데 회사 사정이 있으니ㅎㅎ",
          "original_form": "근데 회사 사정이 있으니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009822.1.15",
          "form": "어쩔수 없어요",
          "original_form": "어쩔수 없어요",
          "speaker_id": "2",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009822.1.16",
          "form": "아 이것도 장단점이 있군욯ㅎ",
          "original_form": "아 이것도 장단점이 있군욯ㅎ",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009822.1.17",
          "form": "네 방학이 없다고 하죠!",
          "original_form": "네 방학이 없다고 하죠!",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100009822.1.18",
          "form": "이상형과 결혼하셨나요?",
          "original_form": "이상형과 결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
