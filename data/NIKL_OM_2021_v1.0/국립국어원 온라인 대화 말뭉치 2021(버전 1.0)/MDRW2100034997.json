      "utterance": [
        {
          "id": "MDRW2100034997.1.1",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100034997.1.2",
          "form": "경주여행가고싶어요",
          "original_form": "경주여행가고싶어요",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100034997.1.3",
          "form": "최근에 직장동료 경주여행 다녀온 사진 봤는데",
          "original_form": "최근에 직장동료 경주여행 다녀온 사진 봤는데",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100034997.1.4",
          "form": "해바라기밭이 너무 예쁘더라고요!!",
          "original_form": "해바라기밭이 너무 예쁘더라고요!!",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100034997.1.5",
          "form": "오 해바라기밭이 있군요",
          "original_form": "오 해바라기밭이 있군요",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100034997.1.6",
          "form": "꽃밭을 잘 조성하고 있네요",
          "original_form": "꽃밭을 잘 조성하고 있네요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100034997.1.7",
          "form": "가고싶당...",
          "original_form": "가고싶당...",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100034997.1.8",
          "form": "좀 더 선선해지면 가고싶어용",
          "original_form": "좀 더 선선해지면 가고싶어용",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100034997.1.9",
          "form": "마자요 경주의 여름은 넘나 더운것",
          "original_form": "마자요 경주의 여름은 넘나 더운것",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100034997.1.10",
          "form": "얼른 시원해졌으면",
          "original_form": "얼른 시원해졌으면{emoji:♡}",
          "speaker_id": "1",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100034997.1.11",
          "form": "오늘도 시원학거같이 생겨놓고 덥네오",
          "original_form": "오늘도 시원학거같이 생겨놓고 덥네오",
          "speaker_id": "2",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100034997.1.12",
          "form": "그러네요 생긴건 시원하게 생겼오",
          "original_form": "그러네요 생긴건 시원하게 생겼오",
          "speaker_id": "1",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100034997.1.13",
          "form": "오늘은 실내동물원을 다냐와쏘여",
          "original_form": "오늘은 실내동물원을 다냐와쏘여",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100034997.1.14",
          "form": "오동물원이있나요",
          "original_form": "오동물원이있나요",
          "speaker_id": "1",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100034997.1.15",
          "form": "몇년전에 생긴거 같더라구여 부산에서 데이트를 별로 안했더니 뭘 몰랐네여",
          "original_form": "몇년전에 생긴거 같더라구여 부산에서 데이트를 별로 안했더니 뭘 몰랐네여",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100034997.1.16",
          "form": "저도요 ㅋㅋㅋ 저기 삼정?",
          "original_form": "저도요 ㅋㅋㅋ 저기 삼정?",
          "speaker_id": "1",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100034997.1.17",
          "form": "저기부산은행 ifc몰인다",
          "original_form": "저기부산은행 ifc몰인다",
          "speaker_id": "2",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100034997.1.18",
          "form": "아 그쪽에있군요",
