      "utterance": [
        {
          "id": "MDRW2100023613.1.1",
          "form": "주원님이랑 고스트 공연봣어요",
          "original_form": "주원님이랑 고스트 공연봣어요",
          "speaker_id": "1",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100023613.1.2",
          "form": "아이비님 시카고 롹시 유명하시던데",
          "original_form": "아이비님 시카고 롹시 유명하시던데",
          "speaker_id": "1",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100023613.1.3",
          "form": "그건못밧어요",
          "original_form": "그건못밧어요",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100023613.1.4",
          "form": "아하! 저도 고스트 봤어요.",
          "original_form": "아하! 저도 고스트 봤어요.",
          "speaker_id": "2",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100023613.1.5",
          "form": "처음 봤을땐 저거 어케한거지 했는데",
          "original_form": "처음 봤을땐 저거 어케한거지 했는데",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100023613.1.6",
          "form": "두번째 보니까 대충 원리를 알겠더라구요 ㅋㅋ",
          "original_form": "두번째 보니까 대충 원리를 알겠더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:59"
        },
        {
          "id": "MDRW2100023613.1.7",
          "form": "와 두번이나보셧군요",
          "original_form": "와 두번이나보셧군요",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023613.1.8",
          "form": "시카고도 보셧나욤?",
          "original_form": "시카고도 보셧나욤?",
          "speaker_id": "1",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023613.1.9",
          "form": "아이비님은 아니지만 네",
          "original_form": "아이비님은 아니지만 네",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023613.1.10",
          "form": "추천인가요?",
          "original_form": "추천인가요?",
          "speaker_id": "1",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023613.1.11",
          "form": "? 네?",
          "original_form": "? 네?",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023613.1.12",
          "form": "김지우님도 호평자자하더라구요",
          "original_form": "김지우님도 호평자자하더라구요",
          "speaker_id": "1",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100023613.1.13",
          "form": "아, 전 최근에 민경아님으로 봤어요",
          "original_form": "아, 전 최근에 민경아님으로 봤어요",
          "speaker_id": "2",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100023613.1.14",
          "form": "시카고 재밋나욤?",
          "original_form": "시카고 재밋나욤?",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100023613.1.15",
          "form": "음... 쇼뮤로 즐기기도 좋고 시사점도 있고 좋은 공연이에요",
          "original_form": "음... 쇼뮤로 즐기기도 좋고 시사점도 있고 좋은 공연이에요",
          "speaker_id": "2",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100023613.1.16",
          "form": "뮤지컬영화도 있는데",
          "original_form": "뮤지컬영화도 있는데",
          "speaker_id": "2",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100023613.1.17",
          "form": "영화로는봣어요 ㅎㅎ",
          "original_form": "영화로는봣어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100023613.1.18",
          "form": "그  영화도 연출이 정말 좋아요.",
          "original_form": "그  영화도 연출이 정말 좋아요.",
          "speaker_id": "2",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100023613.1.19",
          "form": "맞아요유ㅠ",
          "original_form": "맞아요유ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100023613.1.20",
          "form": "스토리는 같으니까요.",
          "original_form": "스토리는 같으니까요.",
          "speaker_id": "2",
          "time": "20210830 13:08"
        },
        {
          "id": "MDRW2100023613.1.21",
          "form": "아하 완전 같군요",
          "original_form": "아하 완전 같군요",
          "speaker_id": "1",
          "time": "20210830 13:08"
        },
        {
          "id": "MDRW2100023613.1.22",
          "form": "제가 쇼뮤취향이 아니라서 저는 막 엄청 좋다까지는 아녔는데",
          "original_form": "제가 쇼뮤취향이 아니라서 저는 막 엄청 좋다까지는 아녔는데",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100023613.1.23",
          "form": "객관적으론 수작이라 생각해요.",
          "original_form": "객관적으론 수작이라 생각해요.",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100023613.1.24",
          "form": "누굴 데려가도 노잼 소리는 안들을 극이죠",
          "original_form": "누굴 데려가도 노잼 소리는 안들을 극이죠",
          "speaker_id": "2",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100023613.1.25",
          "form": "전 그런거좋아해요 !!",
          "original_form": "전 그런거좋아해요 !!",
          "speaker_id": "1",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100023613.1.26",
          "form": "헤드윅 이런거는 찐팬들도 다회차보면 졸릴때 잇다고 하더라구요 ㅋㅋ",
          "original_form": "헤드윅 이런거는 찐팬들도 다회차보면 졸릴때 잇다고 하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100023613.1.27",
          "form": "제친구가 자기도모르게 잠깐 졸앗데요",
          "original_form": "제친구가 자기도모르게 잠깐 졸앗데요",
          "speaker_id": "1",
          "time": "20210830 13:10"
        },
        {
          "id": "MDRW2100023613.1.28",
          "form": "그럴만해요ㅋㅋㅋ",
          "original_form": "그럴만해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:14"
        }
      ]
    }
