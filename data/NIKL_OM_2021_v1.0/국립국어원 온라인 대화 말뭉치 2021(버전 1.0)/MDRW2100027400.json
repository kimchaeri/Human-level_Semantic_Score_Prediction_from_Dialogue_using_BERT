        },
        {
          "id": "MDRW2100027400.1.2",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027400.1.3",
          "form": "집안일이 일이네요",
          "original_form": "집안일이 일이네요",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027400.1.4",
          "form": "독립해서",
          "original_form": "독립해서",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027400.1.5",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027400.1.6",
          "form": "저는 고양이 키워서",
          "original_form": "저는 고양이 키워서",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027400.1.7",
          "form": "청소가 일이네요",
          "original_form": "청소가 일이네요",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027400.1.8",
          "form": "털이 많이 날려서",
          "original_form": "털이 많이 날려서",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027400.1.9",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027400.1.10",
          "form": "전 화장실 청소 나쁘지 않더라구요 ㅋㅋ",
          "original_form": "전 화장실 청소 나쁘지 않더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027400.1.11",
          "form": "깨끗해지는걸 보는게 좋은",
          "original_form": "깨끗해지는걸 보는게 좋은",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027400.1.12",
          "form": "저는 음식물 쓰레기 처리가 힘들어요",
          "original_form": "저는 음식물 쓰레기 처리가 힘들어요",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027400.1.13",
          "form": "냄새가",
          "original_form": "냄새가",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100027400.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027400.1.15",
          "form": "요즘 청소기통 자동 비워주는거 탐나요",
          "original_form": "요즘 청소기통 자동 비워주는거 탐나요",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027400.1.16",
          "form": "그런것도ㅜ있나요",
          "original_form": "그런것도ㅜ있나요",
          "speaker_id": "1",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027400.1.17",
          "form": "그렇데요 비싸서 그렇지...",
          "original_form": "그렇데요 비싸서 그렇지...",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027400.1.18",
          "form": "전 설거지 제일 싫어요",
          "original_form": "전 설거지 제일 싫어요",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027400.1.19",
          "form": "것도 싫죠",
          "original_form": "것도 싫죠",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027400.1.20",
          "form": "식기세척기 사고싶어요",
          "original_form": "식기세척기 사고싶어요",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027400.1.21",
          "form": "집이 좁아서 이사가면 사고싶네요",
          "original_form": "집이 좁아서 이사가면 사고싶네요",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
